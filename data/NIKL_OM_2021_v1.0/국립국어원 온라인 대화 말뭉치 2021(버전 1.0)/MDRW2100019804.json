      "utterance": [
        {
          "id": "MDRW2100019804.1.1",
          "form": "다녀왔다 여행!!",
          "original_form": "다녀왔다 여행!!",
          "speaker_id": "2",
          "time": "20210827 22:07"
        },
        {
          "id": "MDRW2100019804.1.2",
          "form": "여행!!!!! 부럽단",
          "original_form": "여행!!!!! 부럽단",
          "speaker_id": "1",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100019804.1.3",
          "form": "나는 이번에 결혼하는 친구랑 같이 양양 갈려구했는데 8월초에",
          "original_form": "나는 이번에 결혼하는 친구랑 같이 양양 갈려구했는데 8월초에",
          "speaker_id": "1",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100019804.1.4",
          "form": "양양이 그 난리가 나서 못감 ㅠㅜ",
          "original_form": "양양이 그 난리가 나서 못감 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100019804.1.5",
          "form": "아 맞네",
          "original_form": "아 맞네",
          "speaker_id": "2",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100019804.1.6",
          "form": "고성은 상대적으로 많이 조용하더라ㅇㅇ",
          "original_form": "고성은 상대적으로 많이 조용하더라ㅇㅇ",
          "speaker_id": "2",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100019804.1.7",
          "form": "5월에 여수를 다녀와서 다행이야",
          "original_form": "5월에 여수를 다녀와서 다행이야",
          "speaker_id": "1",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019804.1.8",
          "form": "양양이랑 강릉 속초에 다 모였나봐",
          "original_form": "양양이랑 강릉 속초에 다 모였나봐",
          "speaker_id": "1",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019804.1.9",
          "form": "속초는...아우 충격먹었어ㅡㅡ",
          "original_form": "속초는...아우 충격먹었어ㅡㅡ",
          "speaker_id": "2",
          "time": "20210827 22:16"
        },
        {
          "id": "MDRW2100019804.1.10",
          "form": "동생이 속초미널로 막차타고 와서 데리러 갔었는데",
          "original_form": "동생이 속초미널로 막차타고 와서 데리러 갔었는데",
          "speaker_id": "2",
          "time": "20210827 22:17"
        },
        {
          "id": "MDRW2100019804.1.11",
          "form": "밤 11시 도착했거든 동생이",
          "original_form": "밤 11시 도착했거든 동생이",
          "speaker_id": "2",
          "time": "20210827 22:17"
        },
        {
          "id": "MDRW2100019804.1.12",
          "form": "해수욕장 입구에 야시장이 이만하게 열려서는..",
          "original_form": "해수욕장 입구에 야시장이 이만하게 열려서는..",
          "speaker_id": "2",
          "time": "20210827 22:17"
        },
        {
          "id": "MDRW2100019804.1.13",
          "form": "ㅇㅇ 늦게 왔네",
          "original_form": "ㅇㅇ 늦게 왔네",
          "speaker_id": "1",
          "time": "20210827 22:17"
        },
        {
          "id": "MDRW2100019804.1.14",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210827 22:17"
        },
        {
          "id": "MDRW2100019804.1.15",
          "form": "코로나 끝났냐구요ㅠㅠㅠㅠ",
          "original_form": "코로나 끝났냐구요ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 22:18"
        },
        {
          "id": "MDRW2100019804.1.16",
          "form": "대전에 가보고 싶다",
          "original_form": "대전에 가보고 싶다",
          "speaker_id": "1",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019804.1.17",
          "form": "다들 턱스크쓰고 먹고다니고 불꽃놀이하고 폭죽터치고.. 놀랐다그",
          "original_form": "다들 턱스크쓰고 먹고다니고 불꽃놀이하고 폭죽터치고.. 놀랐다그",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019804.1.18",
          "form": "춘천가고싶어ㅠ",
          "original_form": "춘천가고싶어ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019804.1.19",
          "form": "놀라만해",
          "original_form": "놀라만해",
          "speaker_id": "1",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019804.1.20",
          "form": "내년에는 대전도 춘천도 가보자",
          "original_form": "내년에는 대전도 춘천도 가보자",
          "speaker_id": "1",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019804.1.21",
          "form": "갈수있을거야",
          "original_form": "갈수있을거야",
          "speaker_id": "1",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019804.1.22",
          "form": "1차 화이팅",
          "original_form": "1차 화이팅",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019804.1.23",
          "form": "난 2차 화이팅",
          "original_form": "난 2차 화이팅",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019804.1.24",
          "form": "무슨 주제든 기승전코로롱..",
          "original_form": "무슨 주제든 기승전코로롱..",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019804.1.25",
          "form": "여행이니까?",
          "original_form": "여행이니까?",
          "speaker_id": "1",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019804.1.26",
          "form": "여수가서 커다란 손바닥에서 사진찍었는데",
          "original_form": "여수가서 커다란 손바닥에서 사진찍었는데",
          "speaker_id": "1",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019804.1.27",
          "form": "코로나땜시 시간이 정해져있어서 30초광고 찍듯이 찍음ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "코로나땜시 시간이 정해져있어서 30초광고 찍듯이 찍음ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100019804.1.28",
          "form": "그래서 다른거보다 기억에 남네ㅋㅋㅋㅋ",
          "original_form": "그래서 다른거보다 기억에 남네ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100019804.1.29",
          "form": "그것도 추억이라면 추억이겠다ㅋㅋ",
          "original_form": "그것도 추억이라면 추억이겠다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100019804.1.30",
          "form": "이번에 폴라로이드 가져가서 난 거의 폴라위주로 찍었어",
          "original_form": "이번에 폴라로이드 가져가서 난 거의 폴라위주로 찍었어",
          "speaker_id": "2",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100019804.1.31",
          "form": "그리고 내가 잠시 화장실 다녀온 사이 아빠가 자기 폰으로 세명 셀카찍다가",
          "original_form": "그리고 내가 잠시 화장실 다녀온 사이 아빠가 자기 폰으로 세명 셀카찍다가",
          "speaker_id": "2",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100019804.1.32",
          "form": "폰 떨궈서 와장창",
          "original_form": "폰 떨궈서 와장창",
          "speaker_id": "2",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100019804.1.33",
          "form": "그래서 바꿨어..어휴",
          "original_form": "그래서 바꿨어..어휴",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100019804.1.34",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "1",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100019804.1.35",
          "form": "새폰이 가지고 싶으셨나? ㅋㅋㅋ",
          "original_form": "새폰이 가지고 싶으셨나? ㅋㅋㅋ",
