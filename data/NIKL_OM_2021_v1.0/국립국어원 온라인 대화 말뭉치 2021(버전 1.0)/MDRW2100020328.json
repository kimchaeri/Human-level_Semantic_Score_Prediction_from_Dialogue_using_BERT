      "utterance": [
        {
          "id": "MDRW2100020328.1.1",
          "form": "고기도 자주드세요?",
          "original_form": "고기도 자주드세요?",
          "speaker_id": "1",
          "time": "20210828 01:24"
        },
        {
          "id": "MDRW2100020328.1.2",
          "form": "전 고기 먹으러 캠핑나가요 ㅎ",
          "original_form": "전 고기 먹으러 캠핑나가요 ㅎ",
          "speaker_id": "2",
          "time": "20210828 01:24"
        },
        {
          "id": "MDRW2100020328.1.3",
          "form": "자주는 아니고 1주에 2번은 꼭 먹는듯",
          "original_form": "자주는 아니고 1주에 2번은 꼭 먹는듯",
          "speaker_id": "2",
          "time": "20210828 01:25"
        },
        {
          "id": "MDRW2100020328.1.4",
          "form": "우와 캠핑",
          "original_form": "우와 캠핑",
          "speaker_id": "1",
          "time": "20210828 01:25"
        },
        {
          "id": "MDRW2100020328.1.5",
          "form": "흠 치킨 2일 고기 2일 피자 1일 중국음식 1일 하면 ...",
          "original_form": "흠 치킨 2일 고기 2일 피자 1일 중국음식 1일 하면 ...",
          "speaker_id": "2",
          "time": "20210828 01:25"
        },
        {
          "id": "MDRW2100020328.1.6",
          "form": "보통 삼겹살드시나요?",
          "original_form": "보통 삼겹살드시나요?",
          "speaker_id": "1",
          "time": "20210828 01:25"
        },
        {
          "id": "MDRW2100020328.1.7",
          "form": "하루 남네요 ㅜㅡ",
          "original_form": "하루 남네요 ㅜㅡ",
          "speaker_id": "2",
          "time": "20210828 01:25"
        },
        {
          "id": "MDRW2100020328.1.8",
          "form": "넵 ㅎ 삼겹살 자주 먹고 가끔 수육하려고 다릿살이나",
          "original_form": "넵 ㅎ 삼겹살 자주 먹고 가끔 수육하려고 다릿살이나",
          "speaker_id": "2",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100020328.1.9",
          "form": "곱창을 ㅎㅎㅎㅎㅎ",
          "original_form": "곱창을 ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100020328.1.10",
          "form": "삼겹살 와사비에 드셔보셨어요?",
          "original_form": "삼겹살 와사비에 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100020328.1.11",
          "form": "완전 좋아해요",
          "original_form": "완전 좋아해요",
          "speaker_id": "1",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100020328.1.12",
          "form": "ㅎㅎㅎㅎㅎㅎㅎ 역시 아시네요 ㅎ",
          "original_form": "ㅎㅎㅎㅎㅎㅎㅎ 역시 아시네요 ㅎ",
          "speaker_id": "2",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100020328.1.13",
          "form": "소고기랑 와사비 먹는것도 좋아하구요",
          "original_form": "소고기랑 와사비 먹는것도 좋아하구요",
          "speaker_id": "1",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100020328.1.14",
          "form": "ㅎㅎㅎㅎ 좋죠 ㅎㅎㅎㅎㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ 좋죠 ㅎㅎㅎㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 01:27"
        },
        {
          "id": "MDRW2100020328.1.15",
          "form": "삽겹살을 보통 두께가 아니라 4~5 센티 두께로 두툼하게 잘라달라고해서",
          "original_form": "삽겹살을 보통 두께가 아니라 4~5 센티 두께로 두툼하게 잘라달라고해서",
          "speaker_id": "2",
          "time": "20210828 01:27"
        },
        {
          "id": "MDRW2100020328.1.16",
          "form": "잘 구우면 ㅎㅎㅎ 진짜 겉바속촉 ㅎㅎㅎ",
          "original_form": "잘 구우면 ㅎㅎㅎ 진짜 겉바속촉 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 01:28"
        },
        {
          "id": "MDRW2100020328.1.17",
          "form": "거기에 와사비 조금 덜어서 한입하면 ㅎㅎ",
          "original_form": "거기에 와사비 조금 덜어서 한입하면 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 01:28"
        },
        {
          "id": "MDRW2100020328.1.18",
          "form": "너무 좋아요",
          "original_form": "너무 좋아요",
          "speaker_id": "2",
          "time": "20210828 01:28"
        },
        {
          "id": "MDRW2100020328.1.19",
          "form": "와 고기굽기장인의 향기가",
          "original_form": "와 고기굽기장인의 향기가",
          "speaker_id": "1",
          "time": "20210828 01:29"
        },
        {
          "id": "MDRW2100020328.1.20",
          "form": "그정돈 아닌데 ㅎ 동생이 캠핑을 다니는데 따라다니다가",
          "original_form": "그정돈 아닌데 ㅎ 동생이 캠핑을 다니는데 따라다니다가",
          "speaker_id": "2",
          "time": "20210828 01:29"
        },
        {
          "id": "MDRW2100020328.1.21",
          "form": "ㅎㅎ 동생은 먹는건 별로 안좋아하는데 저는 고기에 진심이라",
          "original_form": "ㅎㅎ 동생은 먹는건 별로 안좋아하는데 저는 고기에 진심이라",
          "speaker_id": "2",
          "time": "20210828 01:30"
        },
        {
          "id": "MDRW2100020328.1.22",
          "form": "저도 고기에 진심인데 마음만큼 고기를 잘 굽지는 못해요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 고기에 진심인데 마음만큼 고기를 잘 굽지는 못해요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:30"
        },
        {
          "id": "MDRW2100020328.1.23",
          "form": "원래 고기는 그냥 성격급하거나 가만히 못있는 사람이 굽는거임",
          "original_form": "원래 고기는 그냥 성격급하거나 가만히 못있는 사람이 굽는거임",
          "speaker_id": "2",
          "time": "20210828 01:31"
        },
        {
          "id": "MDRW2100020328.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ저는 성격이 너무 급해서 자꾸 뒤집어서",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저는 성격이 너무 급해서 자꾸 뒤집어서",
          "speaker_id": "1",
          "time": "20210828 01:33"
        },
        {
          "id": "MDRW2100020328.1.25",
          "form": "그게 문제인듯",
          "original_form": "그게 문제인듯",
          "speaker_id": "1",
          "time": "20210828 01:33"
        },
        {
          "id": "MDRW2100020328.1.26",
          "form": "ㅋㅋㅋ 저두 그래요 ㅎ 그래서 두툼하게 먹는게 맛있다는걸 알고서도 한동안 실행을 못했었음 ㅋ",
          "original_form": "ㅋㅋㅋ 저두 그래요 ㅎ 그래서 두툼하게 먹는게 맛있다는걸 알고서도 한동안 실행을 못했었음 ㅋ",
          "speaker_id": "2",
          "time": "20210828 01:33"
        },
        {
          "id": "MDRW2100020328.1.27",
          "form": "육즙을 가둬야하는데...머리로는 아는데 ㅋㅋㅋㅋㅋ",
          "original_form": "육즙을 가둬야하는데...머리로는 아는데 ㅋㅋㅋㅋㅋ",
