      "utterance": [
        {
          "id": "MDRW2100002180.1.1",
          "form": "고양이도 말 알아 듣죠? 고양이를 키워 본 적이 없어서요",
          "original_form": "고양이도 말 알아 듣죠? 고양이를 키워 본 적이 없어서요",
          "speaker_id": "2",
          "time": "20210820 00:19"
        },
        {
          "id": "MDRW2100002180.1.2",
          "form": "성격도 다양하던데",
          "original_form": "성격도 다양하던데",
          "speaker_id": "2",
          "time": "20210820 00:20"
        },
        {
          "id": "MDRW2100002180.1.3",
          "form": "ㅋㅋㅋㅋ네 근데 알아들어도 무시해요",
          "original_form": "ㅋㅋㅋㅋ네 근데 알아들어도 무시해요",
          "speaker_id": "1",
          "time": "20210820 00:21"
        },
        {
          "id": "MDRW2100002180.1.4",
          "form": "그게 또 귀여운포인트 ㅋㅋㅋㅋㅋ",
          "original_form": "그게 또 귀여운포인트 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002180.1.5",
          "form": "고양이 발 귀엽던데",
          "original_form": "고양이 발 귀엽던데",
          "speaker_id": "2",
          "time": "20210820 00:36"
        },
        {
          "id": "MDRW2100002180.1.6",
          "form": "매력적이네",
          "original_form": "매력적이네",
          "speaker_id": "2",
          "time": "20210820 00:36"
        },
        {
          "id": "MDRW2100002180.1.7",
          "form": "네 핑크핑크젤리 누르면 폭신하고말랑해요",
          "original_form": "네 핑크핑크젤리 누르면 폭신하고말랑해요",
          "speaker_id": "1",
          "time": "20210820 00:37"
        },
        {
          "id": "MDRW2100002180.1.8",
          "form": "강아지들은 안을때 은근단단하잖아요",
          "original_form": "강아지들은 안을때 은근단단하잖아요",
          "speaker_id": "1",
          "time": "20210820 00:37"
        },
        {
          "id": "MDRW2100002180.1.9",
          "form": "고양이들은 완전 흐물 말랑하거든여",
          "original_form": "고양이들은 완전 흐물 말랑하거든여",
          "speaker_id": "1",
          "time": "20210820 00:37"
        },
        {
          "id": "MDRW2100002180.1.10",
          "form": "귀여워 ㅋㅋㅋ 고양이도 애교 많아보이더라구요",
          "original_form": "귀여워 ㅋㅋㅋ 고양이도 애교 많아보이더라구요",
          "speaker_id": "2",
          "time": "20210820 00:41"
        },
        {
          "id": "MDRW2100002180.1.11",
          "form": "흐물 말랑이라니",
          "original_form": "흐물 말랑이라니",
          "speaker_id": "2",
          "time": "20210820 00:41"
        },
        {
          "id": "MDRW2100002180.1.12",
          "form": "동물들은 다 귀엽고 사랑스러운것 같아요!",
          "original_form": "동물들은 다 귀엽고 사랑스러운것 같아요!",
          "speaker_id": "2",
          "time": "20210820 00:41"
        },
        {
          "id": "MDRW2100002180.1.13",
          "form": "네 어쩜 그렇게 다들귀여운지ㅜㅜ뽀뽀갈기러가야겠어요",
          "original_form": "네 어쩜 그렇게 다들귀여운지ㅜㅜ뽀뽀갈기러가야겠어요",
          "speaker_id": "1",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100002180.1.14",
          "form": "힐링도 되고 많은 감정을 주는것같아요",
          "original_form": "힐링도 되고 많은 감정을 주는것같아요",
          "speaker_id": "2",
          "time": "20210820 00:49"
        },
        {
          "id": "MDRW2100002180.1.15",
          "form": "말은 지지리도 안 듣지만 ㅋㅋㅋㅋㅋ",
          "original_form": "말은 지지리도 안 듣지만 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:50"
        },
        {
          "id": "MDRW2100002180.1.16",
          "form": "ㅋㅋㅋㅋㅋ맞아요 지지리도안듣지만 귀여우니봐준다",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 지지리도안듣지만 귀여우니봐준다",
          "speaker_id": "1",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002180.1.17",
          "form": "고양이 뭐 잘 먹어? 우리집 개는 닭고기에 환장하거든",
          "original_form": "고양이 뭐 잘 먹어? 우리집 개는 닭고기에 환장하거든",
          "speaker_id": "2",
          "time": "20210820 00:55"
        },
        {
          "id": "MDRW2100002180.1.18",
          "form": "가족들 밥 먹을때 쳐다보는데 부담스러워",
          "original_form": "가족들 밥 먹을때 쳐다보는데 부담스러워",
          "speaker_id": "2",
          "time": "20210820 00:56"
        },
        {
          "id": "MDRW2100002180.1.19",
          "form": "얘도 닭고기에 환장해요 ㅋㅋㅋㅋ근데 그 파는전용 닭가슴살만먹어요",
          "original_form": "얘도 닭고기에 환장해요 ㅋㅋㅋㅋ근데 그 파는전용 닭가슴살만먹어요",
          "speaker_id": "1",
          "time": "20210820 01:01"
        },
        {
          "id": "MDRW2100002180.1.20",
          "form": "좀싼건데 애기때먹던거라 그게좋은가봐여",
          "original_form": "좀싼건데 애기때먹던거라 그게좋은가봐여",
          "speaker_id": "1",
          "time": "20210820 01:02"
        },
        {
          "id": "MDRW2100002180.1.21",
          "form": "귀엽겠다",
          "original_form": "귀엽겠다",
          "speaker_id": "2",
          "time": "20210820 09:45"
        },
        {
          "id": "MDRW2100002180.1.22",
          "form": "우리집 개는 빨리 달라고 보채요",
          "original_form": "우리집 개는 빨리 달라고 보채요",
          "speaker_id": "2",
          "time": "20210820 09:45"
        },
        {
          "id": "MDRW2100002180.1.23",
          "form": "그래도 훈련한거 시키면 귀여워서 빨리 안 줘요 ㅋㅋㅋ",
          "original_form": "그래도 훈련한거 시키면 귀여워서 빨리 안 줘요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 09:45"
        },
        {
          "id": "MDRW2100002180.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ오 훈련 강쥐들 훈련하는거보면 신기해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ오 훈련 강쥐들 훈련하는거보면 신기해요",
          "speaker_id": "1",
          "time": "20210820 11:49"
        },
        {
          "id": "MDRW2100002180.1.25",
          "form": "어떤 훈련되나요?",
          "original_form": "어떤 훈련되나요?",
          "speaker_id": "1",
          "time": "20210820 11:49"
        },
        {
          "id": "MDRW2100002180.1.26",
          "form": "기본 손발이랑 기다려, 손 브이 하면 자기 턱을 브이한 손 위로 걸쳐놔요",
          "original_form": "기본 손발이랑 기다려, 손 브이 하면 자기 턱을 브이한 손 위로 걸쳐놔요",
          "speaker_id": "2",
          "time": "20210820 14:14"
        },
        {
          "id": "MDRW2100002180.1.27",
          "form": "악 넘귀여워요",
          "original_form": "악 넘귀여워요",
          "speaker_id": "1",
          "time": "20210820 14:15"
        }
      ]
    }
