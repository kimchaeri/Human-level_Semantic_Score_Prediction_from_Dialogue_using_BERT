      "utterance": [
        {
          "id": "MDRW2100003641.1.1",
          "form": "고양이는",
          "original_form": "고양이는",
          "speaker_id": "2",
          "time": "20210821 09:37"
        },
        {
          "id": "MDRW2100003641.1.2",
          "form": "영역동물이라",
          "original_form": "영역동물이라",
          "speaker_id": "1",
          "time": "20210821 09:37"
        },
        {
          "id": "MDRW2100003641.1.3",
          "form": "산책 안해줘도 되요?",
          "original_form": "산책 안해줘도 되요?",
          "speaker_id": "2",
          "time": "20210821 09:37"
        },
        {
          "id": "MDRW2100003641.1.4",
          "form": "산책하면 안돼요",
          "original_form": "산책하면 안돼요",
          "speaker_id": "1",
          "time": "20210821 09:37"
        },
        {
          "id": "MDRW2100003641.1.5",
          "form": "안된다구요?",
          "original_form": "안된다구요?",
          "speaker_id": "2",
          "time": "20210821 09:37"
        },
        {
          "id": "MDRW2100003641.1.6",
          "form": "네 안돼요",
          "original_form": "네 안돼요",
          "speaker_id": "1",
          "time": "20210821 09:37"
        },
        {
          "id": "MDRW2100003641.1.7",
          "form": "약간 개는",
          "original_form": "약간 개는",
          "speaker_id": "1",
          "time": "20210821 09:37"
        },
        {
          "id": "MDRW2100003641.1.8",
          "form": "새로운곳을가고 새로운 친구를 만나고",
          "original_form": "새로운곳을가고 새로운 친구를 만나고",
          "speaker_id": "1",
          "time": "20210821 09:37"
        },
        {
          "id": "MDRW2100003641.1.9",
          "form": "그러면서 사회성을 기르고 하는데",
          "original_form": "그러면서 사회성을 기르고 하는데",
          "speaker_id": "1",
          "time": "20210821 09:38"
        },
        {
          "id": "MDRW2100003641.1.10",
          "form": "고양이는 영역동물이라 자기가 활동하는 영역안에 있을 때 안정감을 느껴요",
          "original_form": "고양이는 영역동물이라 자기가 활동하는 영역안에 있을 때 안정감을 느껴요",
          "speaker_id": "1",
          "time": "20210821 09:38"
        },
        {
          "id": "MDRW2100003641.1.11",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210821 09:38"
        },
        {
          "id": "MDRW2100003641.1.12",
          "form": "영역이 늘어나면 그만큼 스트레스를 받는 동물이라",
          "original_form": "영역이 늘어나면 그만큼 스트레스를 받는 동물이라",
          "speaker_id": "1",
          "time": "20210821 09:39"
        },
        {
          "id": "MDRW2100003641.1.13",
          "form": "그럼 강아지가 훨씬 손이 많이가네요",
          "original_form": "그럼 강아지가 훨씬 손이 많이가네요",
          "speaker_id": "2",
          "time": "20210821 09:39"
        },
        {
          "id": "MDRW2100003641.1.14",
          "form": "아무래도 그렇죠",
          "original_form": "아무래도 그렇죠",
          "speaker_id": "1",
          "time": "20210821 09:39"
        },
        {
          "id": "MDRW2100003641.1.15",
          "form": "고양이 목욕은요?",
          "original_form": "고양이 목욕은요?",
          "speaker_id": "2",
          "time": "20210821 09:39"
        },
        {
          "id": "MDRW2100003641.1.16",
          "form": "그래서 반려동물 키우려면 부지런해야해요",
          "original_form": "그래서 반려동물 키우려면 부지런해야해요",
          "speaker_id": "1",
          "time": "20210821 09:39"
        },
        {
          "id": "MDRW2100003641.1.17",
          "form": "고양이 자체가 깨끗해서 사실 냄새안나면 안시켜도 되더라구요",
          "original_form": "고양이 자체가 깨끗해서 사실 냄새안나면 안시켜도 되더라구요",
          "speaker_id": "1",
          "time": "20210821 09:40"
        },
        {
          "id": "MDRW2100003641.1.18",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "2",
          "time": "20210821 09:40"
        },
        {
          "id": "MDRW2100003641.1.19",
          "form": "그루밍?",
          "original_form": "그루밍?",
          "speaker_id": "2",
          "time": "20210821 09:40"
        },
        {
          "id": "MDRW2100003641.1.20",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210821 09:40"
        },
        {
          "id": "MDRW2100003641.1.21",
          "form": "그거 하면 깨끗해지나",
          "original_form": "그거 하면 깨끗해지나",
          "speaker_id": "2",
          "time": "20210821 09:40"
        },
        {
          "id": "MDRW2100003641.1.22",
          "form": "개는 그게 안되잖아요",
          "original_form": "개는 그게 안되잖아요",
          "speaker_id": "1",
          "time": "20210821 09:41"
        },
        {
          "id": "MDRW2100003641.1.23",
          "form": "침에 성분이 있대요",
          "original_form": "침에 성분이 있대요",
          "speaker_id": "1",
          "time": "20210821 09:41"
        },
        {
          "id": "MDRW2100003641.1.24",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210821 09:41"
        },
        {
          "id": "MDRW2100003641.1.25",
          "form": "그래서 길고양이들도 밥챙겨주면 깨끗해져요",
          "original_form": "그래서 길고양이들도 밥챙겨주면 깨끗해져요",
          "speaker_id": "1",
          "time": "20210821 09:41"
        },
        {
          "id": "MDRW2100003641.1.26",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "2",
          "time": "20210821 09:41"
        },
        {
          "id": "MDRW2100003641.1.27",
          "form": "먹이구할 체력을 몸단장하는데 쓰니까",
          "original_form": "먹이구할 체력을 몸단장하는데 쓰니까",
