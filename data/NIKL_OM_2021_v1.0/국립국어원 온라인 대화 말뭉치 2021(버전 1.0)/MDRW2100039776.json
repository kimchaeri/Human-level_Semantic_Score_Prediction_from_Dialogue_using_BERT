      "utterance": [
        {
          "id": "MDRW2100039776.1.1",
          "form": "저 햇반 보통공기 하나 다 못 먹어욧,,,",
          "original_form": "저 햇반 보통공기 하나 다 못 먹어욧,,,",
          "speaker_id": "1",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100039776.1.2",
          "form": "하...",
          "original_form": "하...",
          "speaker_id": "2",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039776.1.3",
          "form": "그러고도 사람인가요",
          "original_form": "그러고도 사람인가요",
          "speaker_id": "2",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039776.1.4",
          "form": "어떻게 그러실수 있죠!!",
          "original_form": "어떻게 그러실수 있죠!!",
          "speaker_id": "2",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039776.1.5",
          "form": "죄송해요 하...",
          "original_form": "죄송해요 하...",
          "speaker_id": "1",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039776.1.6",
          "form": "제가 너무... 여리고.. 연약하고.. 청순하고.. 청초하고... 그렇죠..ㅠㅠ",
          "original_form": "제가 너무... 여리고.. 연약하고.. 청순하고.. 청초하고... 그렇죠..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039776.1.7",
          "form": "하...오늘 밤 굶으세요",
          "original_form": "하...오늘 밤 굶으세요",
          "speaker_id": "2",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100039776.1.8",
          "form": "에프와 고기 압수입니다",
          "original_form": "에프와 고기 압수입니다",
          "speaker_id": "2",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100039776.1.9",
          "form": "싫어요 먹을 거예여!!!!!",
          "original_form": "싫어요 먹을 거예여!!!!!",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039776.1.10",
          "form": "꺄하하하하하ㅏ하하핳ㅎ",
          "original_form": "꺄하하하하하ㅏ하하핳ㅎ",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039776.1.11",
          "form": "그래봐야 햇반 하나 위장...",
          "original_form": "그래봐야 햇반 하나 위장...",
          "speaker_id": "2",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100039776.1.12",
          "form": "훗 안부럽네요",
          "original_form": "훗 안부럽네요",
          "speaker_id": "2",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100039776.1.13",
          "form": "흥",
          "original_form": "흥",
          "speaker_id": "1",
          "time": "20210903 21:56"
        },
        {
          "id": "MDRW2100039776.1.14",
          "form": "name2님은 햇반 얼마나 드시는데여!",
          "original_form": "&name2&님은 햇반 얼마나 드시는데여!",
          "speaker_id": "1",
          "time": "20210903 21:56"
        },
        {
          "id": "MDRW2100039776.1.15",
          "form": "최대 네공기 가능합니다",
          "original_form": "최대 네공기 가능합니다",
          "speaker_id": "2",
          "time": "20210903 21:57"
        },
        {
          "id": "MDRW2100039776.1.16",
          "form": "평소에도 입맛 돌면 기본 두공기는 먹쥬",
          "original_form": "평소에도 입맛 돌면 기본 두공기는 먹쥬",
          "speaker_id": "2",
          "time": "20210903 21:57"
        },
        {
          "id": "MDRW2100039776.1.17",
          "form": "근데 햇반보다 우리집 공기가 훨 큰데요!",
          "original_form": "근데 햇반보다 우리집 공기가 훨 큰데요!",
          "speaker_id": "2",
          "time": "20210903 21:58"
        },
        {
          "id": "MDRW2100039776.1.18",
          "form": "님이 짬뽕 먹을까 짜장 먹을까 고민할 때 전 둘다 먹을수 있단 말이쥬ㅎㅎ",
          "original_form": "님이 짬뽕 먹을까 짜장 먹을까 고민할 때 전 둘다 먹을수 있단 말이쥬ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:58"
        },
        {
          "id": "MDRW2100039776.1.19",
          "form": "어머",
          "original_form": "어머",
          "speaker_id": "1",
          "time": "20210903 22:00"
        },
        {
          "id": "MDRW2100039776.1.20",
          "form": "짬짜면이 있는데 왜 고민을 하나여!",
          "original_form": "짬짜면이 있는데 왜 고민을 하나여!",
          "speaker_id": "1",
          "time": "20210903 22:00"
        },
        {
          "id": "MDRW2100039776.1.21",
          "form": "님이 짬짜면을 먹을때 전 짬짜면에 탕수육을 추가하쥬!!",
          "original_form": "님이 짬짜면을 먹을때 전 짬짜면에 탕수육을 추가하쥬!!",
          "speaker_id": "2",
          "time": "20210903 22:01"
        },
        {
          "id": "MDRW2100039776.1.22",
          "form": "허! 저도 탕수육 먹거든여!",
          "original_form": "허! 저도 탕수육 먹거든여!",
          "speaker_id": "1",
          "time": "20210903 22:05"
        },
        {
          "id": "MDRW2100039776.1.23",
          "form": "먹다남은 탕수육을",
          "original_form": "먹다남은 탕수육을",
          "speaker_id": "1",
          "time": "20210903 22:05"
        },
        {
          "id": "MDRW2100039776.1.24",
          "form": "하루종일 간식처럼 주워먹는 그 기분이 얼마나 좋은지 아시나여!!",
          "original_form": "하루종일 간식처럼 주워먹는 그 기분이 얼마나 좋은지 아시나여!!",
          "speaker_id": "1",
          "time": "20210903 22:05"
        },
        {
          "id": "MDRW2100039776.1.25",
          "form": "어떻게 그게 하루종일 가죠...",
          "original_form": "어떻게 그게 하루종일 가죠...",
          "speaker_id": "2",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039776.1.26",
          "form": "님이 탕수육을 하루종일 먹을때 전 또 새로운 먹거리를 먹고 있겠죠!",
          "original_form": "님이 탕수육을 하루종일 먹을때 전 또 새로운 먹거리를 먹고 있겠죠!",
          "speaker_id": "2",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039776.1.27",
          "form": "전 다른 먹거리보다 탕수육이 조아서 계속 먹구 있는 거라그엽!!",
          "original_form": "전 다른 먹거리보다 탕수육이 조아서 계속 먹구 있는 거라그엽!!",
          "speaker_id": "1",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100039776.1.28",
          "form": "아 탕수육먹구싶다..",
          "original_form": "아 탕수육먹구싶다..",
          "speaker_id": "1",
          "time": "20210903 22:07"
        }
      ]
