      "utterance": [
        {
          "id": "MDRW2100003722.1.1",
          "form": "야구 좋아하세요?",
          "original_form": "야구 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 11:12"
        },
        {
          "id": "MDRW2100003722.1.2",
          "form": "어려워서 잘 몰라요ㅠㅠ",
          "original_form": "어려워서 잘 몰라요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 11:12"
        },
        {
          "id": "MDRW2100003722.1.3",
          "form": "그렇구나 그럼 무슨 운동 좋아하세요?",
          "original_form": "그렇구나 그럼 무슨 운동 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 11:12"
        },
        {
          "id": "MDRW2100003722.1.4",
          "form": "저는 야구만 봐서 그렇게 물어봤네요 ㅎㅎ",
          "original_form": "저는 야구만 봐서 그렇게 물어봤네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 11:12"
        },
        {
          "id": "MDRW2100003722.1.5",
          "form": "운동은 그냥 보는것만...이번에 올림픽 재밌게 봤어요ㅎㅎ",
          "original_form": "운동은 그냥 보는것만...이번에 올림픽 재밌게 봤어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 11:13"
        },
        {
          "id": "MDRW2100003722.1.6",
          "form": "오 올림픽 재밌었죠ㅎㅎ",
          "original_form": "오 올림픽 재밌었죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 11:13"
        },
        {
          "id": "MDRW2100003722.1.7",
          "form": "양궁 선수들 진짜 멋있는 것 같아요!",
          "original_form": "양궁 선수들 진짜 멋있는 것 같아요!",
          "speaker_id": "1",
          "time": "20210821 11:13"
        },
        {
          "id": "MDRW2100003722.1.8",
          "form": "네 스포츠에 관심없었는데 이번에 보고 다들 멋있더라구요",
          "original_form": "네 스포츠에 관심없었는데 이번에 보고 다들 멋있더라구요",
          "speaker_id": "2",
          "time": "20210821 11:14"
        },
        {
          "id": "MDRW2100003722.1.9",
          "form": "맞아요 땀흘리는 모습이 너무 멋있더라구요!",
          "original_form": "맞아요 땀흘리는 모습이 너무 멋있더라구요!",
          "speaker_id": "1",
          "time": "20210821 11:14"
        },
        {
          "id": "MDRW2100003722.1.10",
          "form": "체육은 좋아하세요?",
          "original_form": "체육은 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 11:15"
        },
        {
          "id": "MDRW2100003722.1.11",
          "form": "체육은..항상 패스하는 과목이었어요ㅠㅠ",
          "original_form": "체육은..항상 패스하는 과목이었어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 11:15"
        },
        {
          "id": "MDRW2100003722.1.12",
          "form": "체육 좋아하세요?",
          "original_form": "체육 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 11:15"
        },
        {
          "id": "MDRW2100003722.1.13",
          "form": "아하 저도 그렇게 좋아하는 과목은 아니었는데 ㅎㅎㅎ",
          "original_form": "아하 저도 그렇게 좋아하는 과목은 아니었는데 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 11:15"
        },
        {
          "id": "MDRW2100003722.1.14",
          "form": "요즘은 너무 안해서 헬스라도 하려구요 ㅎㅎ",
          "original_form": "요즘은 너무 안해서 헬스라도 하려구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 11:15"
        },
        {
          "id": "MDRW2100003722.1.15",
          "form": "저도 관심 전혀없었는데 땀흘리는 운동이 멋있고 건강해보여서 시작해보려구요ㅎㅎ",
          "original_form": "저도 관심 전혀없었는데 땀흘리는 운동이 멋있고 건강해보여서 시작해보려구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 11:16"
        },
        {
          "id": "MDRW2100003722.1.16",
          "form": "모두 파이팅 해봐요ㅎㅎㅎ",
          "original_form": "모두 파이팅 해봐요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 11:16"
        },
        {
          "id": "MDRW2100003722.1.17",
          "form": "저는 요새 점핑 다니고 있어요!",
          "original_form": "저는 요새 점핑 다니고 있어요!",
          "speaker_id": "1",
          "time": "20210821 11:17"
        },
        {
          "id": "MDRW2100003722.1.18",
          "form": "재밌을거같아요 재미없으면 운동 하기쉽지않아서ㅎㅎ",
          "original_form": "재밌을거같아요 재미없으면 운동 하기쉽지않아서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 11:17"
        },
        {
          "id": "MDRW2100003722.1.19",
          "form": "맞아요 재밌어서 좋아요 ㅎㅎ",
          "original_form": "맞아요 재밌어서 좋아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 11:17"
        }
      ]
