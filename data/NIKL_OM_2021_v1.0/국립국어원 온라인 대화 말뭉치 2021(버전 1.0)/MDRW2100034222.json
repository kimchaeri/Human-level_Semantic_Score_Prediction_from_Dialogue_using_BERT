      "utterance": [
        {
          "id": "MDRW2100034222.1.1",
          "form": "크루즈 좋았지 ㅎㅎ",
          "original_form": "크루즈 좋았지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100034222.1.2",
          "form": "배 멀미 안했어?",
          "original_form": "배 멀미 안했어?",
          "speaker_id": "1",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100034222.1.3",
          "form": "그래 배가 엄청 커서 괜찮음",
          "original_form": "그래 배가 엄청 커서 괜찮음",
          "speaker_id": "2",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100034222.1.4",
          "form": "신기하당",
          "original_form": "신기하당",
          "speaker_id": "1",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100034222.1.5",
          "form": "안 타봤어?",
          "original_form": "안 타봤어?",
          "speaker_id": "2",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100034222.1.6",
          "form": "안 타봤지 ㅎㅎ 부럽~",
          "original_form": "안 타봤지 ㅎㅎ 부럽~",
          "speaker_id": "1",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100034222.1.7",
          "form": "재밌다 ㅎㅎ",
          "original_form": "재밌다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100034222.1.8",
          "form": "담엔 나도 델꼬 가여 ㅋㅋ",
          "original_form": "담엔 나도 델꼬 가여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100034222.1.9",
          "form": "그래 ㅋㅋ 미국에서 멕시코도 들르고 신기하더라",
          "original_form": "그래 ㅋㅋ 미국에서 멕시코도 들르고 신기하더라",
          "speaker_id": "2",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100034222.1.10",
          "form": "좋다.. 음식도 뷔페로 무제한이지?",
          "original_form": "좋다.. 음식도 뷔페로 무제한이지?",
          "speaker_id": "1",
          "time": "20210827 21:26"
        },
