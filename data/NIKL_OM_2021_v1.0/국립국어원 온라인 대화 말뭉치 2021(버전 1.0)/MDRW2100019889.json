      "utterance": [
        {
          "id": "MDRW2100019889.1.1",
          "form": "name1님은 대학생이신가요?!",
          "original_form": "&name1&님은 대학생이신가요?!",
          "speaker_id": "2",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100019889.1.2",
          "form": "저 후회하는 것 중 하나가",
          "original_form": "저 후회하는 것 중 하나가",
          "speaker_id": "2",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100019889.1.3",
          "form": "학부생일 때 동아리 활동 안한거요 ..!!",
          "original_form": "학부생일 때 동아리 활동 안한거요 ..!!",
          "speaker_id": "2",
          "time": "20210828 01:12"
        },
        {
          "id": "MDRW2100019889.1.4",
          "form": "아니요 직장인이예요 ㅎㅎ",
          "original_form": "아니요 직장인이예요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 01:17"
        },
        {
          "id": "MDRW2100019889.1.5",
          "form": "동아리활동은 저도 못했어요..ㅋㅋ 활동하고 싶은 동아리가 있으셨나요?",
          "original_form": "동아리활동은 저도 못했어요..ㅋㅋ 활동하고 싶은 동아리가 있으셨나요?",
          "speaker_id": "1",
          "time": "20210828 01:18"
        },
        {
          "id": "MDRW2100019889.1.6",
          "form": "사실... 딱히 끌리는 데가 있지도 않았어요!",
          "original_form": "사실... 딱히 끌리는 데가 있지도 않았어요!",
          "speaker_id": "2",
          "time": "20210828 01:18"
        },
        {
          "id": "MDRW2100019889.1.7",
          "form": "근데 봉사활동은 스펙 채운다고",
          "original_form": "근데 봉사활동은 스펙 채운다고",
          "speaker_id": "2",
          "time": "20210828 01:18"
        },
        {
          "id": "MDRW2100019889.1.8",
          "form": "억지로 했던 기억이...",
          "original_form": "억지로 했던 기억이...",
          "speaker_id": "2",
          "time": "20210828 01:18"
        },
        {
          "id": "MDRW2100019889.1.9",
          "form": "ㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100019889.1.10",
          "form": "성격이 외향적이긴 한데",
          "original_form": "성격이 외향적이긴 한데",
          "speaker_id": "2",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100019889.1.11",
          "form": "다들 그런 기억은 있죠..",
          "original_form": "다들 그런 기억은 있죠..",
          "speaker_id": "1",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100019889.1.12",
          "form": "억지로 모이는건 또 스트레스더라구욬ㅋㅋㅋ",
          "original_form": "억지로 모이는건 또 스트레스더라구욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100019889.1.13",
          "form": "네 그건 숙제같으니까요 ㅋㅋㅋ",
          "original_form": "네 그건 숙제같으니까요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100019889.1.14",
          "form": "저는 요양시설 같은 곳에서 어르신 돌봄을 했었는데 말동무하기라서",
          "original_form": "저는 요양시설 같은 곳에서 어르신 돌봄을 했었는데 말동무하기라서",
          "speaker_id": "1",
          "time": "20210828 01:20"
        },
        {
          "id": "MDRW2100019889.1.15",
          "form": "헉 네네!!!",
          "original_form": "헉 네네!!!",
          "speaker_id": "2",
          "time": "20210828 01:21"
        },
        {
          "id": "MDRW2100019889.1.16",
          "form": "정말 어려운 일 하셨네요!!!",
          "original_form": "정말 어려운 일 하셨네요!!!",
          "speaker_id": "2",
          "time": "20210828 01:21"
        },
        {
          "id": "MDRW2100019889.1.17",
          "form": "별거 없이 식사도와드리고 그랬는데 할아버지 한분이 절 편하게 대해주셔서 봉사활동하는동안",
          "original_form": "별거 없이 식사도와드리고 그랬는데 할아버지 한분이 절 편하게 대해주셔서 봉사활동하는동안",
          "speaker_id": "1",
          "time": "20210828 01:21"
        },
        {
          "id": "MDRW2100019889.1.18",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210828 01:21"
        },
        {
          "id": "MDRW2100019889.1.19",
          "form": "조금 편했어요 ㅋㅋㅋㄱ",
          "original_form": "조금 편했어요 ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210828 01:21"
        },
        {
          "id": "MDRW2100019889.1.20",
          "form": "좋은 기억으로 남았을 것 같아요",
          "original_form": "좋은 기억으로 남았을 것 같아요",
          "speaker_id": "2",
          "time": "20210828 01:22"
        },
        {
          "id": "MDRW2100019889.1.21",
          "form": "어르신들 뵙고 오면",
          "original_form": "어르신들 뵙고 오면",
          "speaker_id": "2",
          "time": "20210828 01:22"
        },
        {
          "id": "MDRW2100019889.1.22",
          "form": "묘하게 마음이 아련해져요 ㅠㅠ ㅋㅋㅋㅋㅋ",
          "original_form": "묘하게 마음이 아련해져요 ㅠㅠ ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:22"
        },
        {
          "id": "MDRW2100019889.1.23",
          "form": "너무 적극적으로 대하지 말라고 하셔서 말을 많이 하지도 않았는데 좋아해주셔서",
          "original_form": "너무 적극적으로 대하지 말라고 하셔서 말을 많이 하지도 않았는데 좋아해주셔서",
          "speaker_id": "1",
          "time": "20210828 01:22"
        },
        {
          "id": "MDRW2100019889.1.24",
          "form": "아 그런 매뉴얼이 있나요?",
          "original_form": "아 그런 매뉴얼이 있나요?",
          "speaker_id": "2",
          "time": "20210828 01:23"
        },
        {
          "id": "MDRW2100019889.1.25",
          "form": "저도 가끔 생각하면 아련해요 ㅜ",
          "original_form": "저도 가끔 생각하면 아련해요 ㅜ",
          "speaker_id": "1",
          "time": "20210828 01:23"
        },
        {
          "id": "MDRW2100019889.1.26",
          "form": "그저 옆에 있어주기만 해도",
          "original_form": "그저 옆에 있어주기만 해도",
          "speaker_id": "2",
          "time": "20210828 01:23"
        },
        {
          "id": "MDRW2100019889.1.27",
          "form": "그게 좋으셨나봐요... 8ㅅ8",
          "original_form": "그게 좋으셨나봐요... 8ㅅ8",
          "speaker_id": "2",
          "time": "20210828 01:23"
        },
        {
          "id": "MDRW2100019889.1.28",
          "form": "네 자주 방문하지 못하니까요 ㅠㅜㅜㅜ",
          "original_form": "네 자주 방문하지 못하니까요 ㅠㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 01:23"
        },
        {
          "id": "MDRW2100019889.1.29",
          "form": "맞아요 진짜 아련해요!!!!! 으아아아",
          "original_form": "맞아요 진짜 아련해요!!!!! 으아아아",
          "speaker_id": "2",
          "time": "20210828 01:23"
        },
        {
          "id": "MDRW2100019889.1.30",
          "form": "어차피 사람은 다 늙을텐데",
          "original_form": "어차피 사람은 다 늙을텐데",
          "speaker_id": "2",
          "time": "20210828 01:23"
        },
        {
          "id": "MDRW2100019889.1.31",
          "form": "그러셨던거 같아요",
          "original_form": "그러셨던거 같아요",
          "speaker_id": "1",
          "time": "20210828 01:23"
        },
        {
          "id": "MDRW2100019889.1.32",
          "form": "저는 사회복지사 분들이 정말 대단하다고 생각해요",
          "original_form": "저는 사회복지사 분들이 정말 대단하다고 생각해요",
          "speaker_id": "2",
          "time": "20210828 01:24"
        },
        {
          "id": "MDRW2100019889.1.33",
          "form": "네 그러니 어르신들에게 좋게 좋게 할려구 노력해요",
          "original_form": "네 그러니 어르신들에게 좋게 좋게 할려구 노력해요",
          "speaker_id": "1",
          "time": "20210828 01:24"
        },
        {
          "id": "MDRW2100019889.1.34",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "2",
          "time": "20210828 01:24"
        },
        {
          "id": "MDRW2100019889.1.35",
          "form": "정말 못볼 꼴 많이들 보실텐데....!!!!",
          "original_form": "정말 못볼 꼴 많이들 보실텐데....!!!!",
          "speaker_id": "2",
          "time": "20210828 01:25"
        },
        {
          "id": "MDRW2100019889.1.36",
          "form": "그쵸 저도 사회복지사 분들은 존경해요",
          "original_form": "그쵸 저도 사회복지사 분들은 존경해요",
          "speaker_id": "1",
          "time": "20210828 01:25"
        },
        {
          "id": "MDRW2100019889.1.37",
          "form": "그러니까요 ㅠㅠ",
          "original_form": "그러니까요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 01:25"
        },
        {
          "id": "MDRW2100019889.1.38",
          "form": "게다가 일하신 만큼 대우도 못 받으시는거 같아서 더 대단하다 생각해요..",
          "original_form": "게다가 일하신 만큼 대우도 못 받으시는거 같아서 더 대단하다 생각해요..",
          "speaker_id": "1",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100019889.1.39",
          "form": "헉 맞아요 완전!!!!",
          "original_form": "헉 맞아요 완전!!!!",
          "speaker_id": "2",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100019889.1.40",
          "form": "더 대우받아야하고",
          "original_form": "더 대우받아야하고",
          "speaker_id": "2",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100019889.1.41",
          "form": "인정받아야하는데... 평가절하된 직업군 중 하나라고 생각해요",
          "original_form": "인정받아야하는데... 평가절하된 직업군 중 하나라고 생각해요",
          "speaker_id": "2",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100019889.1.42",
          "form": "저도 그렇게 생각해요",
          "original_form": "저도 그렇게 생각해요",
          "speaker_id": "1",
          "time": "20210828 01:27"
        },
        {
          "id": "MDRW2100019889.1.43",
          "form": "나중에라도 여유가 생기면 다시 봉사활동하고 싶어요",
          "original_form": "나중에라도 여유가 생기면 다시 봉사활동하고 싶어요",
