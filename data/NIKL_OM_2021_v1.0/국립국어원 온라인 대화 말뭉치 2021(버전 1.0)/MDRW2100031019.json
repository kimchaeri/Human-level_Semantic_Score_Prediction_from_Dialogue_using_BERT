      "utterance": [
        {
          "id": "MDRW2100031019.1.1",
          "form": "혹시 전시나 극장영화 자주 보세요??",
          "original_form": "혹시 전시나 극장영화 자주 보세요??",
          "speaker_id": "1",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100031019.1.2",
          "form": "전 전시 가끔 찾아가서 봐요!",
          "original_form": "전 전시 가끔 찾아가서 봐요!",
          "speaker_id": "1",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100031019.1.3",
          "form": "저는 시립 미술관 자주 가봐요",
          "original_form": "저는 시립 미술관 자주 가봐요",
          "speaker_id": "2",
          "time": "20210904 15:39"
        },
        {
          "id": "MDRW2100031019.1.4",
          "form": "코로나땜에 공연전시는 거의 못갔네요",
          "original_form": "코로나땜에 공연전시는 거의 못갔네요",
          "speaker_id": "2",
          "time": "20210904 15:39"
        },
        {
          "id": "MDRW2100031019.1.5",
          "form": "세마 라고 하던가요",
          "original_form": "세마 라고 하던가요",
          "speaker_id": "1",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100031019.1.6",
          "form": "시립미술관 보고싶은거 많이하더라구요!!",
          "original_form": "시립미술관 보고싶은거 많이하더라구요!!",
          "speaker_id": "1",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100031019.1.7",
          "form": "맞아요ㅠㅠ 요새 시국이 시국인지라 잘 못가긴하겠더라구요",
          "original_form": "맞아요ㅠㅠ 요새 시국이 시국인지라 잘 못가긴하겠더라구요",
          "speaker_id": "1",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031019.1.8",
          "form": "그렇죠ㅠㅠ",
          "original_form": "그렇죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031019.1.9",
          "form": "전 저저번달에",
          "original_form": "전 저저번달에",
          "speaker_id": "1",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031019.1.10",
          "form": "저는 아이돌 콘서트 가고파요",
          "original_form": "저는 아이돌 콘서트 가고파요",
          "speaker_id": "2",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031019.1.11",
          "form": "피카소 다녀왔었어용",
          "original_form": "피카소 다녀왔었어용",
          "speaker_id": "1",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031019.1.12",
          "form": "우와우와 멋져요",
          "original_form": "우와우와 멋져요",
          "speaker_id": "2",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031019.1.13",
          "form": "오 좋아하시는 아이돌있으세요?",
          "original_form": "오 좋아하시는 아이돌있으세요?",
          "speaker_id": "1",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031019.1.14",
          "form": "아스트로 좋아합니다",
          "original_form": "아스트로 좋아합니다",
          "speaker_id": "2",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100031019.1.15",
          "form": "차은우 아시죠?",
          "original_form": "차은우 아시죠?",
          "speaker_id": "2",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100031019.1.16",
          "form": "아 알죠 얼굴 천재...",
          "original_form": "아 알죠 얼굴 천재...",
          "speaker_id": "1",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100031019.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100031019.1.18",
          "form": "저희집 누나들도 아이돌 좋아해서",
          "original_form": "저희집 누나들도 아이돌 좋아해서",
          "speaker_id": "1",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100031019.1.19",
          "form": "오 어느 아이돌 좋아하나요",
          "original_form": "오 어느 아이돌 좋아하나요",
          "speaker_id": "2",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100031019.1.20",
          "form": "강제스밍돌려주고있어요",
          "original_form": "강제스밍돌려주고있어요",
          "speaker_id": "1",
          "time": "20210904 15:54"
        },
        {
          "id": "MDRW2100031019.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:54"
        },
        {
          "id": "MDRW2100031019.1.22",
          "form": "뉴이스트 좋아하더라구요",
          "original_form": "뉴이스트 좋아하더라구요",
          "speaker_id": "1",
          "time": "20210904 15:54"
        },
        {
          "id": "MDRW2100031019.1.23",
          "form": "한명은 더보이즈 좋아하구...",
          "original_form": "한명은 더보이즈 좋아하구...",
          "speaker_id": "1",
          "time": "20210904 15:54"
        },
        {
          "id": "MDRW2100031019.1.24",
          "form": "아 둘다알아요",
          "original_form": "아 둘다알아요",
          "speaker_id": "2",
          "time": "20210904 15:56"
        },
        {
          "id": "MDRW2100031019.1.25",
          "form": "아스트로 노래도 들어쥬세요",
          "original_form": "아스트로 노래도 들어쥬세요",
          "speaker_id": "2",
          "time": "20210904 15:56"
        },
        {
          "id": "MDRW2100031019.1.26",
          "form": "ㅋㅋㅋㅋ네 한번 들어보겠습니다아",
          "original_form": "ㅋㅋㅋㅋ네 한번 들어보겠습니다아",
          "speaker_id": "1",
          "time": "20210904 18:14"
        },
        {
          "id": "MDRW2100031019.1.27",
          "form": "코로나전에는",
          "original_form": "코로나전에는",
          "speaker_id": "1",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100031019.1.28",
          "form": "콘서트나 팬싸같은것도 자주가셨나여??",
          "original_form": "콘서트나 팬싸같은것도 자주가셨나여??",
          "speaker_id": "1",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100031019.1.29",
          "form": "아뇨 못가서 한이 맺힌거같아요",
          "original_form": "아뇨 못가서 한이 맺힌거같아요",
          "speaker_id": "2",
          "time": "20210904 18:18"
        },
        {
          "id": "MDRW2100031019.1.30",
          "form": "코로나땜에 더 뮷가니까 ㅋㅋㅋ",
          "original_form": "코로나땜에 더 뮷가니까 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:18"
        },
        {
          "id": "MDRW2100031019.1.31",
          "form": "ㅠㅠㅠㅠㅠㅠㅠ저도 왠지 그맘알거같아요",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠ저도 왠지 그맘알거같아요",
          "speaker_id": "1",
          "time": "20210904 18:19"
        },
        {
          "id": "MDRW2100031019.1.32",
          "form": "제가 좋아하는 밴드도 단콘준비중이었다가",
          "original_form": "제가 좋아하는 밴드도 단콘준비중이었다가",
          "speaker_id": "1",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100031019.1.33",
          "form": "캔슬됐거든요...",
          "original_form": "캔슬됐거든요...",
          "speaker_id": "1",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100031019.1.34",
          "form": "ㅠㅠㅜㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅜㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100031019.1.35",
          "form": "아 무슨 밴드인가요",
          "original_form": "아 무슨 밴드인가요",
          "speaker_id": "2",
          "time": "20210904 18:30"
        },
        {
          "id": "MDRW2100031019.1.36",
          "form": "새소년이라고",
          "original_form": "새소년이라고",
          "speaker_id": "1",
          "time": "20210904 18:41"
        },
        {
          "id": "MDRW2100031019.1.37",
          "form": "인디밴드입니당",
          "original_form": "인디밴드입니당",
          "speaker_id": "1",
          "time": "20210904 18:42"
        },
        {
          "id": "MDRW2100031019.1.38",
          "form": "새소년 저도 한번 찾아 볼게요",
          "original_form": "새소년 저도 한번 찾아 볼게요",
          "speaker_id": "2",
          "time": "20210904 22:04"
        },
        {
          "id": "MDRW2100031019.1.39",
          "form": "쏜애플 아세여?",
          "original_form": "쏜애플 아세여?",
          "speaker_id": "2",
          "time": "20210904 22:04"
        },
        {
          "id": "MDRW2100031019.1.40",
          "form": "쏜애플 들어봣어용",
          "original_form": "쏜애플 들어봣어용",
          "speaker_id": "1",
          "time": "20210905 00:31"
        },
        {
          "id": "MDRW2100031019.1.41",
          "form": "약간 넬느낌나는걸로 기억하고있는뎅",
          "original_form": "약간 넬느낌나는걸로 기억하고있는뎅",
          "speaker_id": "1",
          "time": "20210905 00:32"
        },
        {
          "id": "MDRW2100031019.1.42",
          "form": "오 아마 맞을거에요 인디밴드 조아하는 친구가 쏜애플 좋아하더라고요",
          "original_form": "오 아마 맞을거에요 인디밴드 조아하는 친구가 쏜애플 좋아하더라고요",
          "speaker_id": "2",
          "time": "20210905 08:53"
        },
        {
          "id": "MDRW2100031019.1.43",
          "form": "조금 들어봤는데 좋네여",
          "original_form": "조금 들어봤는데 좋네여",
          "speaker_id": "1",
          "time": "20210905 09:38"
        },
        {
          "id": "MDRW2100031019.1.44",
          "form": "!!",
          "original_form": "!!",
          "speaker_id": "1",
          "time": "20210905 09:38"
        },
        {
          "id": "MDRW2100031019.1.45",
          "form": "ㅎㅎ 저는 이미 백신을 2차까지맞았기때무네...",
          "original_form": "ㅎㅎ 저는 이미 백신을 2차까지맞았기때무네...",
          "speaker_id": "1",
          "time": "20210905 09:39"
        },
        {
          "id": "MDRW2100031019.1.46",
          "form": "9월 25일 좋아하는 밴드보러가여",
          "original_form": "9월 25일 좋아하는 밴드보러가여",
          "speaker_id": "1",
          "time": "20210905 09:39"
        },
        {
          "id": "MDRW2100031019.1.47",
