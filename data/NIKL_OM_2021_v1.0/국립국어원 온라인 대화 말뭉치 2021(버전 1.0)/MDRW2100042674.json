      "utterance": [
        {
          "id": "MDRW2100042674.1.1",
          "form": "name2님 백신 맞으샸나요?",
          "original_form": "&name2&님 백신 맞으샸나요?",
          "speaker_id": "1",
          "time": "20210907 00:59"
        },
        {
          "id": "MDRW2100042674.1.2",
          "form": "네 일차만 맞았어요 맞으셨나요?",
          "original_form": "네 일차만 맞았어요 맞으셨나요?",
          "speaker_id": "2",
          "time": "20210907 00:59"
        },
        {
          "id": "MDRW2100042674.1.3",
          "form": "네 저도 1차요 부작용은 없으셨나요",
          "original_form": "네 저도 1차요 부작용은 없으셨나요",
          "speaker_id": "1",
          "time": "20210907 01:00"
        },
        {
          "id": "MDRW2100042674.1.4",
          "form": "맞은 부위가 좀 부어오르고 다음날까지 뻐근한거 빼고는 괜찮았어요!",
          "original_form": "맞은 부위가 좀 부어오르고 다음날까지 뻐근한거 빼고는 괜찮았어요!",
          "speaker_id": "2",
          "time": "20210907 01:01"
        },
        {
          "id": "MDRW2100042674.1.5",
          "form": "name1님은 괜찮으셨나요??",
          "original_form": "&name1&님은 괜찮으셨나요??",
          "speaker_id": "2",
          "time": "20210907 01:01"
        },
        {
          "id": "MDRW2100042674.1.6",
          "form": "저랑 똑같아요ㅎ 2차가 심하다던데 부작용 없이 넘어가면 좋겠네요",
          "original_form": "저랑 똑같아요ㅎ 2차가 심하다던데 부작용 없이 넘어가면 좋겠네요",
          "speaker_id": "1",
          "time": "20210907 01:03"
        },
        {
          "id": "MDRW2100042674.1.7",
          "form": "맞아요 name1님 이차 잘 맞고 넘어가셨으면 좋겠어요!!",
          "original_form": "맞아요 &name1&님 이차 잘 맞고 넘어가셨으면 좋겠어요!!",
          "speaker_id": "2",
          "time": "20210907 01:06"
        },
        {
          "id": "MDRW2100042674.1.8",
          "form": "name2님도요ㅎㅎ",
          "original_form": "&name2&님도요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 01:06"
        },
        {
          "id": "MDRW2100042674.1.9",
          "form": "name2님 운동하시나요?",
          "original_form": "&name2&님 운동하시나요?",
          "speaker_id": "1",
          "time": "20210907 01:06"
        },
        {
          "id": "MDRW2100042674.1.10",
          "form": "맞기까지가 오래걸려서 그냥 빨리 맞아버리고 싶긴해요",
          "original_form": "맞기까지가 오래걸려서 그냥 빨리 맞아버리고 싶긴해요",
          "speaker_id": "2",
          "time": "20210907 01:07"
        },
        {
          "id": "MDRW2100042674.1.11",
          "form": "불안해서가 아니라 귀찮아서요ㅋㅋㅋㅋ",
          "original_form": "불안해서가 아니라 귀찮아서요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 01:07"
        },
        {
          "id": "MDRW2100042674.1.12",
          "form": "저는 숨쉬기 운동뿐... name1님은 하시나요??",
          "original_form": "저는 숨쉬기 운동뿐... &name1&님은 하시나요??",
          "speaker_id": "2",
          "time": "20210907 01:09"
        },
        {
          "id": "MDRW2100042674.1.13",
          "form": "저는 홈트를 했었어요 코로나 전에는 걸었는데 지금은 안나가려고 하다보니 운동을 안하다가 안될 것 같아서 시작했답니다",
          "original_form": "저는 홈트를 했었어요 코로나 전에는 걸었는데 지금은 안나가려고 하다보니 운동을 안하다가 안될 것 같아서 시작했답니다",
          "speaker_id": "1",
          "time": "20210907 01:20"
        },
        {
          "id": "MDRW2100042674.1.14",
          "form": "맞아요 확실히 걷는양이 줄었어요",
          "original_form": "맞아요 확실히 걷는양이 줄었어요",
          "speaker_id": "2",
          "time": "20210907 01:23"
        },
        {
          "id": "MDRW2100042674.1.15",
          "form": "그래도 운동 챙겨서 하시네요!!",
          "original_form": "그래도 운동 챙겨서 하시네요!!",
          "speaker_id": "2",
          "time": "20210907 01:23"
        },
        {
          "id": "MDRW2100042674.1.16",
          "form": "안하면 안될것 같아서요ㅎㅎname2님 건강검진은 잘 하세요?",
          "original_form": "안하면 안될것 같아서요ㅎㅎ&name2&님 건강검진은 잘 하세요?",
          "speaker_id": "1",
          "time": "20210907 17:03"
        },
        {
          "id": "MDRW2100042674.1.17",
          "form": "아니요...ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니요...ㅋㅋㅋㅋㅋㅋㅋ",
