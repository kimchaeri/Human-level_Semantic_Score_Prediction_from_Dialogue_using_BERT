      "utterance": [
        {
          "id": "MDRW2100013059.1.1",
          "form": "뮤지컬을 그리 비싸게 파니",
          "original_form": "뮤지컬을 그리 비싸게 파니",
          "speaker_id": "2",
          "time": "20210825 11:50"
        },
        {
          "id": "MDRW2100013059.1.2",
          "form": "보는 사람만 계속 보고",
          "original_form": "보는 사람만 계속 보고",
          "speaker_id": "2",
          "time": "20210825 11:50"
        },
        {
          "id": "MDRW2100013059.1.3",
          "form": "그러다 안팔리면",
          "original_form": "그러다 안팔리면",
          "speaker_id": "2",
          "time": "20210825 11:50"
        },
        {
          "id": "MDRW2100013059.1.4",
          "form": "실력 준비 안된 아이돌 데려다가 놓고",
          "original_form": "실력 준비 안된 아이돌 데려다가 놓고",
          "speaker_id": "2",
          "time": "20210825 11:50"
        },
        {
          "id": "MDRW2100013059.1.5",
          "form": "보는사람만봄?",
          "original_form": "보는사람만봄?",
          "speaker_id": "1",
          "time": "20210825 11:50"
        },
        {
          "id": "MDRW2100013059.1.6",
          "form": "ㅂ비싸면한번만보지않아?",
          "original_form": "ㅂ비싸면한번만보지않아?",
          "speaker_id": "1",
          "time": "20210825 11:50"
        },
        {
          "id": "MDRW2100013059.1.7",
          "form": "회전 문 돈다는 표현 있잖아",
          "original_form": "회전 문 돈다는 표현 있잖아",
          "speaker_id": "2",
          "time": "20210825 11:50"
        },
        {
          "id": "MDRW2100013059.1.8",
          "form": "첫관극부터 막관극까지 다 보는 사람들",
          "original_form": "첫관극부터 막관극까지 다 보는 사람들",
          "speaker_id": "2",
          "time": "20210825 11:50"
        },
        {
          "id": "MDRW2100013059.1.9",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100013059.1.10",
          "form": "그건진짜",
          "original_form": "그건진짜",
          "speaker_id": "1",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100013059.1.11",
          "form": "하는사람만하긴하겠네",
          "original_form": "하는사람만하긴하겠네",
          "speaker_id": "1",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100013059.1.12",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100013059.1.13",
          "form": "요즘도뮤지컬에아이돌써?",
          "original_form": "요즘도뮤지컬에아이돌써?",
          "speaker_id": "1",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100013059.1.14",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100013059.1.15",
          "form": "요즘아이돌들진짜노래못하는데",
          "original_form": "요즘아이돌들진짜노래못하는데",
          "speaker_id": "1",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100013059.1.16",
          "form": "더심해졌는데",
          "original_form": "더심해졌는데",
          "speaker_id": "1",
          "time": "20210825 11:51"
        },
        {
          "id": "MDRW2100013059.1.17",
          "form": "그니까 나 광화문 연가에서 한 아이돌이 노래 부르는 것 보고 당황했잖아",
          "original_form": "그니까 나 광화문 연가에서 한 아이돌이 노래 부르는 것 보고 당황했잖아",
          "speaker_id": "2",
          "time": "20210825 11:52"
        },
        {
          "id": "MDRW2100013059.1.18",
          "form": "성량 호흡 이런",
          "original_form": "성량 호흡 이런",
          "speaker_id": "1",
          "time": "20210825 11:52"
        },
        {
          "id": "MDRW2100013059.1.19",
          "form": "기본도안됨",
          "original_form": "기본도안됨",
          "speaker_id": "1",
          "time": "20210825 11:52"
        },
        {
          "id": "MDRW2100013059.1.20",
          "form": "메보인데도이런경우많음",
          "original_form": "메보인데도이런경우많음",
          "speaker_id": "1",
          "time": "20210825 11:52"
        },
        {
          "id": "MDRW2100013059.1.21",
          "form": "그런데어떻게뮤지컬을시키지?",
          "original_form": "그런데어떻게뮤지컬을시키지?",
          "speaker_id": "1",
          "time": "20210825 11:52"
        },
        {
          "id": "MDRW2100013059.1.22",
          "form": "그니까 아니 주연자리를 뮤지컬 배우가 안맞고 왜 하기 싫어하는 아이돌들이 맞냐고",
          "original_form": "그니까 아니 주연자리를 뮤지컬 배우가 안맞고 왜 하기 싫어하는 아이돌들이 맞냐고",
          "speaker_id": "2",
          "time": "20210825 11:53"
        },
        {
          "id": "MDRW2100013059.1.23",
          "form": "아이돌들 스케쥴 바빠서 연습 참여 못하는 경우 많다며",
          "original_form": "아이돌들 스케쥴 바빠서 연습 참여 못하는 경우 많다며",
          "speaker_id": "2",
          "time": "20210825 11:54"
        },
        {
          "id": "MDRW2100013059.1.24",
          "form": "주연까지해?",
          "original_form": "주연까지해?",
          "speaker_id": "1",
          "time": "20210825 11:54"
        },
        {
          "id": "MDRW2100013059.1.25",
          "form": "주연으로 바로 꽃아넣어",
          "original_form": "주연으로 바로 꽃아넣어",
          "speaker_id": "2",
          "time": "20210825 11:54"
        },
        {
          "id": "MDRW2100013059.1.26",
          "form": "원래아이돌주연까지시켜주나?",
          "original_form": "원래아이돌주연까지시켜주나?",
          "speaker_id": "1",
          "time": "20210825 11:54"
        },
        {
          "id": "MDRW2100013059.1.27",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210825 11:54"
        },
        {
          "id": "MDRW2100013059.1.28",
          "form": "그건심한데",
          "original_form": "그건심한데",
          "speaker_id": "1",
          "time": "20210825 11:54"
        },
        {
          "id": "MDRW2100013059.1.29",
          "form": "주연이면비중도",
          "original_form": "주연이면비중도",
          "speaker_id": "1",
          "time": "20210825 11:54"
        },
        {
          "id": "MDRW2100013059.1.30",
          "form": "클거아니야",
          "original_form": "클거아니야",
          "speaker_id": "1",
