      "utterance": [
        {
          "id": "MDRW2100045301.1.1",
          "form": "내일은 무슨 음식 드실예정이에용?",
          "original_form": "내일은 무슨 음식 드실예정이에용?",
          "speaker_id": "2",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045301.1.2",
          "form": "내일 아침이요?",
          "original_form": "내일 아침이요?",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045301.1.3",
          "form": "네네!ㅎㅎ저는 만두먹을거에요",
          "original_form": "네네!ㅎㅎ저는 만두먹을거에요",
          "speaker_id": "2",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045301.1.4",
          "form": "헐 통햇네요",
          "original_form": "헐 통햇네요",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045301.1.5",
          "form": "저 떡만둣국먹을거예요",
          "original_form": "저 떡만둣국먹을거예요",
          "speaker_id": "1",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045301.1.6",
          "form": "앗 진짜루요??!!오!!",
          "original_form": "앗 진짜루요??!!오!!",
          "speaker_id": "2",
          "time": "20210908 01:48"
        },
        {
          "id": "MDRW2100045301.1.7",
          "form": "근데저는귀찮아서 그냥 쩌먹을라구요ㅜㅜ",
          "original_form": "근데저는귀찮아서 그냥 쩌먹을라구요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100045301.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ전 배가고파서 든든하게 먹으려구요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ전 배가고파서 든든하게 먹으려구요",
          "speaker_id": "1",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045301.1.9",
          "form": "과자종류좋아하세요?",
          "original_form": "과자종류좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045301.1.10",
          "form": "너무 좋아해요",
          "original_form": "너무 좋아해요",
          "speaker_id": "1",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045301.1.11",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045301.1.12",
          "form": "저두요ㅋㅋ 밥보다 과자가 좋아요ㅜㅜ",
          "original_form": "저두요ㅋㅋ 밥보다 과자가 좋아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100045301.1.13",
          "form": "무슨과자 좋아하세요?",
          "original_form": "무슨과자 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 01:54"
        },
        {
          "id": "MDRW2100045301.1.14",
          "form": "저는 요즘 쿠키종류 좋은것같아요",
          "original_form": "저는 요즘 쿠키종류 좋은것같아요",
          "speaker_id": "2",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045301.1.15",
          "form": "버터맛나는거!",
          "original_form": "버터맛나는거!",
          "speaker_id": "2",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045301.1.16",
          "form": "버터와플이나 버터링?",
          "original_form": "버터와플이나 버터링?",
          "speaker_id": "1",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100045301.1.17",
          "form": "앗 둘다최근에 먹었어용ㅋㅋ",
          "original_form": "앗 둘다최근에 먹었어용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100045301.1.18",
          "form": "맛있음",
          "original_form": "맛있음",
          "speaker_id": "2",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100045301.1.19",
          "form": "저는 쿠키도좋아하는데 스낵류를 더 좋아해요",
          "original_form": "저는 쿠키도좋아하는데 스낵류를 더 좋아해요",
          "speaker_id": "1",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045301.1.20",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "2",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100045301.1.21",
          "form": "저는 감자칩",
          "original_form": "저는 감자칩",
          "speaker_id": "1",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045301.1.22",
          "form": "오 종류많잖아요! 저저 프링글스 치즈맛인가? 그거 맛있더리구요!",
          "original_form": "오 종류많잖아요! 저저 프링글스 치즈맛인가? 그거 맛있더리구요!",
          "speaker_id": "2",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045301.1.23",
          "form": "프링글스도 좋아해요",
          "original_form": "프링글스도 좋아해요",
          "speaker_id": "1",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045301.1.24",
          "form": "치즈맛도 좋고 양파맛도 좋고 ㅋㅋㅋㅋㅋ",
          "original_form": "치즈맛도 좋고 양파맛도 좋고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:02"
        },
        {
          "id": "MDRW2100045301.1.25",
          "form": "감자칩 조금 안짠거있어요?!",
          "original_form": "감자칩 조금 안짠거있어요?!",
          "speaker_id": "2",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045301.1.26",
          "form": "수미칩이 그나마 덜짠것같아요",
          "original_form": "수미칩이 그나마 덜짠것같아요",
          "speaker_id": "1",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045301.1.27",
          "form": "수미칩이요? 오 그거 먹어봐야겠어요",
          "original_form": "수미칩이요? 오 그거 먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045301.1.28",
          "form": "짠것도 잘먹긴하지만ㅋㅋㅋ",
          "original_form": "짠것도 잘먹긴하지만ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045301.1.29",
          "form": "수미칩 그런데 감자칩이 좀 두꺼운편이에요",
          "original_form": "수미칩 그런데 감자칩이 좀 두꺼운편이에요",
          "speaker_id": "1",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045301.1.30",
          "form": "오 디테일!ㅋㅋ 과자드실때 음료는 뭐드세요?",
          "original_form": "오 디테일!ㅋㅋ 과자드실때 음료는 뭐드세요?",
          "speaker_id": "2",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045301.1.31",
          "form": "우유 좋아해서 우유마시는 편이에요",
          "original_form": "우유 좋아해서 우유마시는 편이에요",
          "speaker_id": "1",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100045301.1.32",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210908 02:09"
        },
        {
          "id": "MDRW2100045301.1.33",
          "form": "오 저는 요구르트랑 우유! 저두 우유좋아해요!",
          "original_form": "오 저는 요구르트랑 우유! 저두 우유좋아해요!",
          "speaker_id": "2",
