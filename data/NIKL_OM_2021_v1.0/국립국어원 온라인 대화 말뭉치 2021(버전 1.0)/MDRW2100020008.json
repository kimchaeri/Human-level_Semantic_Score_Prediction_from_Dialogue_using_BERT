      "utterance": [
        {
          "id": "MDRW2100020008.1.1",
          "form": "토할 거같이 많이먹었어요",
          "original_form": "토할 거같이 많이먹었어요",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100020008.1.2",
          "form": "앜ㄱㅋㅋㅋ ㅇ뭐드셨어요",
          "original_form": "앜ㄱㅋㅋㅋ ㅇ뭐드셨어요",
          "speaker_id": "1",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100020008.1.3",
          "form": "오늘 네끼는 먹었어요",
          "original_form": "오늘 네끼는 먹었어요",
          "speaker_id": "2",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100020008.1.4",
          "form": "네끼요?",
          "original_form": "네끼요?",
          "speaker_id": "1",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100020008.1.5",
          "form": "네ㅜㅜ",
          "original_form": "네ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100020008.1.6",
          "form": "어쩌다가",
          "original_form": "어쩌다가",
          "speaker_id": "1",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100020008.1.7",
          "form": "마카롱참다가 밥을 먹었네요",
          "original_form": "마카롱참다가 밥을 먹었네요",
          "speaker_id": "2",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100020008.1.8",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "1",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100020008.1.9",
          "form": "제가 멍청했죠",
          "original_form": "제가 멍청했죠",
          "speaker_id": "2",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100020008.1.10",
          "form": "그냥 마카롱 사드시지 그랬어요ㅠㅠ",
          "original_form": "그냥 마카롱 사드시지 그랬어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100020008.1.11",
          "form": "그러게요 돈아끼고싶었나봐요",
          "original_form": "그러게요 돈아끼고싶었나봐요",
          "speaker_id": "2",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100020008.1.12",
          "form": "먹고싶은 거 한번 딱 먹는 게 나아요",
          "original_form": "먹고싶은 거 한번 딱 먹는 게 나아요",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100020008.1.13",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100020008.1.14",
          "form": "집에 콜라없어요?",
          "original_form": "집에 콜라없어요?",
          "speaker_id": "1",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020008.1.15",
          "form": "네ㅜㅜ",
          "original_form": "네ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020008.1.16",
          "form": "마트 다녀오세요",
          "original_form": "마트 다녀오세요",
          "speaker_id": "1",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020008.1.17",
          "form": "그럴까봐요",
