      "utterance": [
        {
          "id": "MDRW2100001676.1.1",
          "form": "여행가고싶으다~~~~",
          "original_form": "여행가고싶으다~~~~",
          "speaker_id": "1",
          "time": "20210819 17:57"
        },
        {
          "id": "MDRW2100001676.1.2",
          "form": "나도~~~~~",
          "original_form": "나도~~~~~",
          "speaker_id": "2",
          "time": "20210819 17:58"
        },
        {
          "id": "MDRW2100001676.1.3",
          "form": "어디로???",
          "original_form": "어디로???",
          "speaker_id": "2",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100001676.1.4",
          "form": "해외여행 가고싶어",
          "original_form": "해외여행 가고싶어",
          "speaker_id": "1",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100001676.1.5",
          "form": "아님 남해?",
          "original_form": "아님 남해?",
          "speaker_id": "1",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100001676.1.6",
          "form": "외도 좋지ㅎ",
          "original_form": "외도 좋지ㅎ",
          "speaker_id": "2",
          "time": "20210819 21:12"
        },
        {
          "id": "MDRW2100001676.1.7",
          "form": "외도는 제주도 아니야?",
          "original_form": "외도는 제주도 아니야?",
          "speaker_id": "1",
          "time": "20210819 22:55"
        },
        {
          "id": "MDRW2100001676.1.8",
          "form": "제주도에 있는건 우도",
          "original_form": "제주도에 있는건 우도",
          "speaker_id": "2",
          "time": "20210820 00:27"
        },
        {
          "id": "MDRW2100001676.1.9",
          "form": "외도는 남해",
          "original_form": "외도는 남해",
          "speaker_id": "2",
          "time": "20210820 00:27"
        },
        {
          "id": "MDRW2100001676.1.10",
          "form": "아항 외도 괜찮을거같앙",
          "original_form": "아항 외도 괜찮을거같앙",
          "speaker_id": "1",
          "time": "20210820 00:43"
        },
        {
          "id": "MDRW2100001676.1.11",
          "form": "진짜 코로나 좀 괜찮아지면 제주도 갈까?",
          "original_form": "진짜 코로나 좀 괜찮아지면 제주도 갈까?",
          "speaker_id": "1",
          "time": "20210820 00:43"
        },
        {
          "id": "MDRW2100001676.1.12",
          "form": "좋지~~~~ 일단 가까운곳에 글램핑부터ㅋ",
          "original_form": "좋지~~~~ 일단 가까운곳에 글램핑부터ㅋ",
          "speaker_id": "2",
          "time": "20210820 00:44"
        },
        {
          "id": "MDRW2100001676.1.13",
          "form": "ㅇㅇ 가을엔 글램핑가자!",
          "original_form": "ㅇㅇ 가을엔 글램핑가자!",
          "speaker_id": "1",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100001676.1.14",
          "form": "응~  그러니까 언능 자!!!",
          "original_form": "응~  그러니까 언능 자!!!",
          "speaker_id": "2",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100001676.1.15",
          "form": "ㅋㅋ  그래야지~~~\n돈 마니 벌어서 여행가야지ㅋ",
          "original_form": "ㅋㅋ  그래야지~~~\n돈 마니 벌어서 여행가야지ㅋ",
          "speaker_id": "1",
          "time": "20210820 00:46"
        },
        {
          "id": "MDRW2100001676.1.16",
          "form": "돈벌기전에 피곤해서 쓰러지면 여행도 못가~~~~~",
          "original_form": "돈벌기전에 피곤해서 쓰러지면 여행도 못가~~~~~",
          "speaker_id": "2",
          "time": "20210820 00:47"
        },
        {
          "id": "MDRW2100001676.1.17",
          "form": "응응 지금 껏어\n내일 아주스틸상장이야\n자기두 언능자!",
          "original_form": "응응 지금 껏어\n내일 아주스틸상장이야\n자기두 언능자!",
          "speaker_id": "1",
          "time": "20210820 00:48"
        },
        {
          "id": "MDRW2100001676.1.18",
          "form": "가을이면 10월에 캠핑가나???",
          "original_form": "가을이면 10월에 캠핑가나???",
          "speaker_id": "2",
          "time": "20210820 10:54"
        },
        {
          "id": "MDRW2100001676.1.19",
          "form": "자기 10월 안 더워?",
          "original_form": "자기 10월 안 더워?",
          "speaker_id": "1",
          "time": "20210820 11:11"
        },
        {
          "id": "MDRW2100001676.1.20",
          "form": "안덥지 않을까??? ㅎㅎ",
          "original_form": "안덥지 않을까??? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 11:19"
        },
        {
          "id": "MDRW2100001676.1.21",
          "form": "10월 중순에 울진 캠핑 예약ㅋㅋ",
          "original_form": "10월 중순에 울진 캠핑 예약ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 11:20"
        },
        {
          "id": "MDRW2100001676.1.22",
          "form": "난 거기 안가 자기혼자가ㄲ",
          "original_form": "난 거기 안가 자기혼자가ㄲ",
          "speaker_id": "1",
          "time": "20210820 11:43"
        },
        {
          "id": "MDRW2100001676.1.23",
          "form": "10월말이나 11월에가자",
          "original_form": "10월말이나 11월에가자",
          "speaker_id": "1",
          "time": "20210820 11:43"
        },
        {
          "id": "MDRW2100001676.1.24",
          "form": "약간 쌀쌀할때ㅋ",
          "original_form": "약간 쌀쌀할때ㅋ",
          "speaker_id": "1",
          "time": "20210820 11:44"
        },
        {
          "id": "MDRW2100001676.1.25",
          "form": "시원할때 가잔말이군ㅋ",
          "original_form": "시원할때 가잔말이군ㅋ",
          "speaker_id": "2",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100001676.1.26",
          "form": "모닥불 피우고싶옹ㅋ",
          "original_form": "모닥불 피우고싶옹ㅋ",
          "speaker_id": "1",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100001676.1.27",
          "form": "더울때 피우면 자기 덥자낭",
          "original_form": "더울때 피우면 자기 덥자낭",
          "speaker_id": "1",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100001676.1.28",
          "form": "불멍~~~",
          "original_form": "불멍~~~",
          "speaker_id": "1",
          "time": "20210820 12:49"
        },
        {
          "id": "MDRW2100001676.1.29",
          "form": "덥지~ 그래도 불멍은 좋지ㅎ",
          "original_form": "덥지~ 그래도 불멍은 좋지ㅎ",
          "speaker_id": "2",
          "time": "20210820 13:16"
        },
        {
          "id": "MDRW2100001676.1.30",
          "form": "작년에 11월초에 화담숲 갔을때도 난 더워했어ㅋ",
          "original_form": "작년에 11월초에 화담숲 갔을때도 난 더워했어ㅋ",
          "speaker_id": "2",
          "time": "20210820 15:06"
        },
        {
          "id": "MDRW2100001676.1.31",
          "form": "ㅋㅋ 높은 산에 가믄 춥겠지",
          "original_form": "ㅋㅋ 높은 산에 가믄 춥겠지",
          "speaker_id": "1",
          "time": "20210820 15:07"
        },
        {
          "id": "MDRW2100001676.1.32",
          "form": "갔다 내려오면 덥겠지",
          "original_form": "갔다 내려오면 덥겠지",
          "speaker_id": "2",
          "time": "20210820 15:08"
        },
        {
          "id": "MDRW2100001676.1.33",
          "form": "여행은 서늘할때ㅋㅋ",
          "original_form": "여행은 서늘할때ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:08"
        },
