      "utterance": [
        {
          "id": "MDRW2100016337.1.1",
          "form": "넷플릭스 보시나요 ㅎㅎ",
          "original_form": "넷플릭스 보시나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 12:06"
        },
        {
          "id": "MDRW2100016337.1.2",
          "form": "전 유튜브를 좋아해요~",
          "original_form": "전 유튜브를 좋아해요~",
          "speaker_id": "2",
          "time": "20210826 12:07"
        },
        {
          "id": "MDRW2100016337.1.3",
          "form": "TV는 잘안봐욯ㅋ",
          "original_form": "TV는 잘안봐욯ㅋ",
          "speaker_id": "2",
          "time": "20210826 12:07"
        },
        {
          "id": "MDRW2100016337.1.4",
          "form": "저도 좋아해요~",
          "original_form": "저도 좋아해요~",
          "speaker_id": "1",
          "time": "20210826 12:14"
        },
        {
          "id": "MDRW2100016337.1.5",
          "form": "오 유튜브 즐겨보시는 채널 있으세용?ㅎ",
          "original_form": "오 유튜브 즐겨보시는 채널 있으세용?ㅎ",
          "speaker_id": "2",
          "time": "20210826 12:15"
        },
        {
          "id": "MDRW2100016337.1.6",
          "form": "빠니보틀이요",
          "original_form": "빠니보틀이요",
          "speaker_id": "1",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016337.1.7",
          "form": "오 처음들어요! 구경가야겠어요~",
          "original_form": "오 처음들어요! 구경가야겠어요~",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016337.1.8",
          "form": "전승배라고  개그맨채널인데 빵빵터집니당ㄱㅋ",
          "original_form": "전승배라고  개그맨채널인데 빵빵터집니당ㄱㅋ",
          "speaker_id": "2",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016337.1.9",
          "form": "오 전승배 한번 찾아볼게요",
          "original_form": "오 전승배 한번 찾아볼게요",
          "speaker_id": "1",
          "time": "20210826 12:29"
        },
        {
          "id": "MDRW2100016337.1.10",
          "form": "전 라스베가스 여행기올린거 봤네묨ㅋㅋ",
          "original_form": "전 라스베가스 여행기올린거 봤네묨ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:50"
        },
        {
          "id": "MDRW2100016337.1.11",
          "form": "오 그거 재밌어요ㅎㅇ",
          "original_form": "오 그거 재밌어요ㅎㅇ",
          "speaker_id": "1",
          "time": "20210826 13:24"
        },
        {
          "id": "MDRW2100016337.1.12",
          "form": "한개보고나니까  알고리즘에의해  쭈욱 계속 뜨네요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "한개보고나니까  알고리즘에의해  쭈욱 계속 뜨네요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:25"
        },
        {
          "id": "MDRW2100016337.1.13",
          "form": "음악은 뭘로 들으시나요??ㅎㅎ  멜론같은거요~",
          "original_form": "음악은 뭘로 들으시나요??ㅎㅎ  멜론같은거요~",
          "speaker_id": "2",
          "time": "20210826 13:25"
        },
        {
          "id": "MDRW2100016337.1.14",
          "form": "저 멜론이요!",
          "original_form": "저 멜론이요!",
          "speaker_id": "1",
          "time": "20210826 13:34"
        },
        {
          "id": "MDRW2100016337.1.15",
          "form": "오 멜론으로 들으시는군용!",
          "original_form": "오 멜론으로 들으시는군용!",
          "speaker_id": "2",
          "time": "20210826 13:34"
        },
        {
          "id": "MDRW2100016337.1.16",
          "form": "전유튜브 프리미엄이라  유튜브 뮤직이 덤으로.......어쩔수없이 씁니다ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전유튜브 프리미엄이라  유튜브 뮤직이 덤으로.......어쩔수없이 씁니다ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:34"
        },
        {
          "id": "MDRW2100016337.1.17",
          "form": "엄청불편하거든요",
          "original_form": "엄청불편하거든요",
          "speaker_id": "2",
          "time": "20210826 13:34"
        },
        {
          "id": "MDRW2100016337.1.18",
          "form": "헉 프리미엄 쓰시는구나",
          "original_form": "헉 프리미엄 쓰시는구나",
          "speaker_id": "1",
          "time": "20210826 13:38"
        },
        {
