      "utterance": [
        {
          "id": "MDRW2100033478.1.1",
          "form": "저 요즘 퓨즈서울 운동바지 사고 싶어요",
          "original_form": "저 요즘 퓨즈서울 운동바지 사고 싶어요",
          "speaker_id": "2",
          "time": "20210824 00:12"
        },
        {
          "id": "MDRW2100033478.1.2",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210824 00:12"
        },
        {
          "id": "MDRW2100033478.1.3",
          "form": "레깅스는 입기 어려워서 그런거 찾고 있는데...",
          "original_form": "레깅스는 입기 어려워서 그런거 찾고 있는데...",
          "speaker_id": "2",
          "time": "20210824 00:12"
        },
        {
          "id": "MDRW2100033478.1.4",
          "form": "전 그 정장 자켓",
          "original_form": "전 그 정장 자켓",
          "speaker_id": "1",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100033478.1.5",
          "form": "하 진짜. 아 저도요",
          "original_form": "하 진짜. 아 저도요",
          "speaker_id": "2",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100033478.1.6",
          "form": "ㅠㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100033478.1.7",
          "form": "거기 정장 진짜 잘 뽑는다더라고요?! 근데...",
          "original_form": "거기 정장 진짜 잘 뽑는다더라고요?! 근데...",
          "speaker_id": "2",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100033478.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100033478.1.9",
          "form": "근데.....하 습...",
          "original_form": "근데.....하 습...",
          "speaker_id": "2",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100033478.1.10",
          "form": "글고 그 미국에서",
          "original_form": "글고 그 미국에서",
          "speaker_id": "1",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100033478.1.11",
          "form": "합격하고 면접볼때 입어보자고",
          "original_form": "합격하고 면접볼때 입어보자고",
          "speaker_id": "2",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100033478.1.12",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "2",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100033478.1.13",
          "form": "정장 잘뽑는 브랜드 있었는데 ..A로 시작하는",
          "original_form": "정장 잘뽑는 브랜드 있었는데 ..A로 시작하는",
          "speaker_id": "1",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100033478.1.14",
          "form": "거기 한국에 들어왔음 좋겠어요",
          "original_form": "거기 한국에 들어왔음 좋겠어요",
          "speaker_id": "1",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100033478.1.15",
          "form": "하 저 얼마전에",
          "original_form": "하 저 얼마전에",
          "speaker_id": "1",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100033478.1.16",
          "form": "아니 한달전인가",
          "original_form": "아니 한달전인가",
          "speaker_id": "1",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100033478.1.17",
          "form": "면접보려고 정장을 샀는데",
          "original_form": "면접보려고 정장을 샀는데",
          "speaker_id": "1",
          "time": "20210824 00:14"
        },
        {
          "id": "MDRW2100033478.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 하 그래요? 네네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 하 그래요? 네네",
          "speaker_id": "2",
          "time": "20210824 00:14"
        },
        {
          "id": "MDRW2100033478.1.19",
          "form": "급하게 사느라 긴팔이 없다해서",
          "original_form": "급하게 사느라 긴팔이 없다해서",
          "speaker_id": "1",
          "time": "20210824 00:14"
        },
        {
          "id": "MDRW2100033478.1.20",
          "form": "반팔입고 갔어요",
          "original_form": "반팔입고 갔어요",
          "speaker_id": "1",
          "time": "20210824 00:14"
        },
        {
          "id": "MDRW2100033478.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 00:14"
        },
        {
          "id": "MDRW2100033478.1.22",
          "form": "시원했는데 떨어짐",
          "original_form": "시원했는데 떨어짐",
          "speaker_id": "1",
          "time": "20210824 00:14"
        },
        {
          "id": "MDRW2100033478.1.23",
          "form": "아?",
          "original_form": "아?",
          "speaker_id": "2",
          "time": "20210824 00:14"
        },
        {
          "id": "MDRW2100033478.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 00:14"
        },
        {
          "id": "MDRW2100033478.1.25",
          "form": "글고",
          "original_form": "글고",
          "speaker_id": "1",
          "time": "20210824 00:14"
        },
        {
          "id": "MDRW2100033478.1.26",
          "form": "신발이 운동화밖에 없어서 구두가 없는거에요",
          "original_form": "신발이 운동화밖에 없어서 구두가 없는거에요",
          "speaker_id": "1",
          "time": "20210824 00:14"
        },
        {
          "id": "MDRW2100033478.1.27",
          "form": "아놔 긴팔 상시판매해야하는거 아니냐고요",
          "original_form": "아놔 긴팔 상시판매해야하는거 아니냐고요",
          "speaker_id": "2",
          "time": "20210824 00:14"
        },
        {
          "id": "MDRW2100033478.1.28",
          "form": "아 씁",
          "original_form": "아 씁",
          "speaker_id": "2",
          "time": "20210824 00:15"
        },
