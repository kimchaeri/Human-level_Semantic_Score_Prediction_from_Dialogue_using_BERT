      "utterance": [
        {
          "id": "MDRW2100044904.1.1",
          "form": "반려동물키우시나요",
          "original_form": "반려동물키우시나요",
          "speaker_id": "1",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044904.1.2",
          "form": "아뇨 반려동물은 키워본 적이없어요ㅜ",
          "original_form": "아뇨 반려동물은 키워본 적이없어요ㅜ",
          "speaker_id": "2",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044904.1.3",
          "form": "아하 저는 고양이키우는 집사입니다",
          "original_form": "아하 저는 고양이키우는 집사입니다",
          "speaker_id": "1",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100044904.1.4",
          "form": "코숏 치즈냥이 수컷이에요",
          "original_form": "코숏 치즈냥이 수컷이에요",
          "speaker_id": "1",
          "time": "20210908 00:24"
        },
        {
          "id": "MDRW2100044904.1.5",
          "form": "오ㅋㅋ개냥이인가요",
          "original_form": "오ㅋㅋ개냥이인가요",
          "speaker_id": "2",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044904.1.6",
          "form": "아녀 완전 고양이그자체입니다 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아녀 완전 고양이그자체입니다 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044904.1.7",
          "form": "그래도 기분좋으면 와서애교피울때있어요 ㅋㅋㅋ",
          "original_form": "그래도 기분좋으면 와서애교피울때있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044904.1.8",
          "form": "ㅋㅋㅋ도도 그 자체인가요",
          "original_form": "ㅋㅋㅋ도도 그 자체인가요",
          "speaker_id": "2",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044904.1.9",
          "form": "네에 ㅋㅋㅋㅋㅋㅋ도도보단 아주 폭군이에요 물고",
          "original_form": "네에 ㅋㅋㅋㅋㅋㅋ도도보단 아주 폭군이에요 물고",
          "speaker_id": "1",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044904.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ주인이시니...ㅜㅜ집사는 따를 수밖에요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ주인이시니...ㅜㅜ집사는 따를 수밖에요",
          "speaker_id": "2",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044904.1.11",
          "form": "네 ㅋㅋㅋㅋㅋ받들어모십니다 ㅎㅎ기분안좋거나 그러면 쇼파에 오줌을 갈기셔요",
          "original_form": "네 ㅋㅋㅋㅋㅋ받들어모십니다 ㅎㅎ기분안좋거나 그러면 쇼파에 오줌을 갈기셔요",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044904.1.12",
          "form": "앗.....ㅋㅋㅋㅋㅋ",
          "original_form": "앗.....ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044904.1.13",
          "form": "기분파시군여",
          "original_form": "기분파시군여",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044904.1.14",
          "form": "ㅋㅋㅋㅋㅋ넵 뭔가언짢으면 가구를 긁으시구여",
          "original_form": "ㅋㅋㅋㅋㅋ넵 뭔가언짢으면 가구를 긁으시구여",
