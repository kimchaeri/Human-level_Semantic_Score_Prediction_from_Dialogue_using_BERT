      "utterance": [
        {
          "id": "MDRW2100016937.1.1",
          "form": "비오면 도보 출퇴근 귀찮겟어요",
          "original_form": "비오면 도보 출퇴근 귀찮겟어요",
          "speaker_id": "1",
          "time": "20210826 16:25"
        },
        {
          "id": "MDRW2100016937.1.2",
          "form": "십분정도는 ㅋㅋ 충분히 ㅋㅋ 괜춘해요",
          "original_form": "십분정도는 ㅋㅋ 충분히 ㅋㅋ 괜춘해요",
          "speaker_id": "2",
          "time": "20210826 16:28"
        },
        {
          "id": "MDRW2100016937.1.3",
          "form": "여기는 지금 엄청 날씨 조으네요",
          "original_form": "여기는 지금 엄청 날씨 조으네요",
          "speaker_id": "2",
          "time": "20210826 16:29"
        },
        {
          "id": "MDRW2100016937.1.4",
          "form": "그래요? 여긴 햇살본지 며칠된듯.. \n하루종일 음침하고..",
          "original_form": "그래요? 여긴 햇살본지 며칠된듯.. \n하루종일 음침하고..",
          "speaker_id": "1",
          "time": "20210826 16:30"
        },
        {
          "id": "MDRW2100016937.1.5",
          "form": "어떤 계절 좋아하시나요?",
          "original_form": "어떤 계절 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016937.1.6",
          "form": "여름빼고 다 좋아요",
          "original_form": "여름빼고 다 좋아요",
          "speaker_id": "2",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016937.1.7",
          "form": "저도 땀이 많아서.. 여름 너무 싫어요..",
          "original_form": "저도 땀이 많아서.. 여름 너무 싫어요..",
          "speaker_id": "1",
          "time": "20210826 16:33"
        },
        {
          "id": "MDRW2100016937.1.8",
          "form": "편의점만 갖다와도 샤워해야해서..",
          "original_form": "편의점만 갖다와도 샤워해야해서..",
          "speaker_id": "1",
          "time": "20210826 16:33"
        },
        {
          "id": "MDRW2100016937.1.9",
          "form": "이제 진짜 여름끝인거같아요 ㅋㅋ",
          "original_form": "이제 진짜 여름끝인거같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:34"
        },
        {
          "id": "MDRW2100016937.1.10",
          "form": "쫌만더 참읍시다",
          "original_form": "쫌만더 참읍시다",
          "speaker_id": "2",
          "time": "20210826 16:34"
        },
        {
          "id": "MDRW2100016937.1.11",
          "form": "그래고 9월말까진 반팔이겠죠..",
          "original_form": "그래고 9월말까진 반팔이겠죠..",
          "speaker_id": "1",
          "time": "20210826 16:34"
        },
        {
          "id": "MDRW2100016937.1.12",
          "form": "그렇져.....",
          "original_form": "그렇져.....",
          "speaker_id": "2",
          "time": "20210826 16:35"
        },
        {
          "id": "MDRW2100016937.1.13",
          "form": "얼렁 가을와서 목티입고 싶네요.. ㅎㅎㅎ",
          "original_form": "얼렁 가을와서 목티입고 싶네요.. ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 16:35"
        },
        {
          "id": "MDRW2100016937.1.14",
          "form": "나는 가을 피플..",
          "original_form": "나는 가을 피플..",
          "speaker_id": "1",
          "time": "20210826 16:35"
        },
        {
          "id": "MDRW2100016937.1.15",
          "form": "ㅋㅋ뭘아시네요",
          "original_form": "ㅋㅋ뭘아시네요",
          "speaker_id": "2",
          "time": "20210826 16:37"
        },
        {
          "id": "MDRW2100016937.1.16",
          "form": "전 땀쟁이에서 벗어나고싶어서",
          "original_form": "전 땀쟁이에서 벗어나고싶어서",
          "speaker_id": "2",
          "time": "20210826 16:37"
        },
        {
          "id": "MDRW2100016937.1.17",
          "form": "가을존버",
          "original_form": "가을존버",
          "speaker_id": "2",
          "time": "20210826 16:37"
        },
        {
          "id": "MDRW2100016937.1.18",
          "form": "가을이 옷잊기도 좋고..",
          "original_form": "가을이 옷잊기도 좋고..",
          "speaker_id": "1",
          "time": "20210826 16:38"
        },
        {
          "id": "MDRW2100016937.1.19",
          "form": "코트도 멋지고 ㅎㅎㅎ",
          "original_form": "코트도 멋지고 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 16:38"
        },
        {
          "id": "MDRW2100016937.1.20",
          "form": "아오 오늘은 덥네요",
          "original_form": "아오 오늘은 덥네요",
          "speaker_id": "2",
          "time": "20210826 16:56"
        },
        {
          "id": "MDRW2100016937.1.21",
          "form": "땀쟁이각",
          "original_form": "땀쟁이각",
          "speaker_id": "2",
          "time": "20210826 16:56"
        },
        {
          "id": "MDRW2100016937.1.22",
          "form": "더운데 저녁식사는 하셧나요",
          "original_form": "더운데 저녁식사는 하셧나요",
          "speaker_id": "2",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100016937.1.23",
          "form": "누워계시나요? ㅋㅋㅋ 넘 습해서 에어컨 키고있네요..",
          "original_form": "누워계시나요? ㅋㅋㅋ 넘 습해서 에어컨 키고있네요..",
          "speaker_id": "1",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100016937.1.24",
          "form": "저두요ㅋㅋ 에어컨 필수져",
          "original_form": "저두요ㅋㅋ 에어컨 필수져",
          "speaker_id": "2",
