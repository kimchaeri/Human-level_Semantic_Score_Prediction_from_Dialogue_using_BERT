      "utterance": [
        {
          "id": "MDRW2100005494.1.1",
          "form": "기숙사가 집안일은 안해도 돼서 편한듯",
          "original_form": "기숙사가 집안일은 안해도 돼서 편한듯",
          "speaker_id": "1",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005494.1.2",
          "form": "오 그건 좋네 기숙사는 빨래 정도만 자기 거 하면 되나?",
          "original_form": "오 그건 좋네 기숙사는 빨래 정도만 자기 거 하면 되나?",
          "speaker_id": "2",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005494.1.3",
          "form": "웅 빨래랑 바닥청소 말곤 딱히 없어",
          "original_form": "웅 빨래랑 바닥청소 말곤 딱히 없어",
          "speaker_id": "1",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005494.1.4",
          "form": "ㅇㅎㅇㅎ 기숙사 살면 바닥청소는 뭐.. 룸메랑 날짜 정해서 함??",
          "original_form": "ㅇㅎㅇㅎ 기숙사 살면 바닥청소는 뭐.. 룸메랑 날짜 정해서 함??",
          "speaker_id": "2",
          "time": "20210821 22:43"
        },
        {
          "id": "MDRW2100005494.1.5",
          "form": "걍 워낙 좁아서 눈에 띄면 슥 닦음ㅋㅋ",
          "original_form": "걍 워낙 좁아서 눈에 띄면 슥 닦음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:43"
        },
        {
          "id": "MDRW2100005494.1.6",
          "form": "넓은 집은 청소 힘들겠다",
          "original_form": "넓은 집은 청소 힘들겠다",
          "speaker_id": "1",
          "time": "20210821 22:44"
        },
        {
          "id": "MDRW2100005494.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그러게",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그러게",
          "speaker_id": "2",
          "time": "20210821 22:45"
        },
        {
          "id": "MDRW2100005494.1.8",
          "form": "나는 아무리 돈을 많이 벌어도 청소하기 귀찮아서 좁은 집 살 듯",
          "original_form": "나는 아무리 돈을 많이 벌어도 청소하기 귀찮아서 좁은 집 살 듯",
          "speaker_id": "2",
          "time": "20210821 22:45"
        },
        {
          "id": "MDRW2100005494.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:45"
        },
        {
          "id": "MDRW2100005494.1.10",
          "form": "인정",
          "original_form": "인정",
          "speaker_id": "1",
          "time": "20210821 22:45"
        },
        {
          "id": "MDRW2100005494.1.11",
          "form": "아 바닥 청소는 아닌데",
          "original_form": "아 바닥 청소는 아닌데",
          "speaker_id": "2",
          "time": "20210821 22:46"
        },
        {
          "id": "MDRW2100005494.1.12",
          "form": "우리 집 바닥에 물 흘리면 바닥 타일이 일어나",
          "original_form": "우리 집 바닥에 물 흘리면 바닥 타일이 일어나",
          "speaker_id": "2",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005494.1.13",
          "form": "나중에 이사하게 되면 이거 물어내야 한다함...ㅋ큐큐큐ㅠ",
          "original_form": "나중에 이사하게 되면 이거 물어내야 한다함...ㅋ큐큐큐ㅠ",
          "speaker_id": "2",
          "time": "20210821 22:48"
        },
        {
          "id": "MDRW2100005494.1.14",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210821 22:48"
        },
        {
          "id": "MDRW2100005494.1.15",
          "form": "타일 하나에 2만 5천원...",
          "original_form": "타일 하나에 2만 5천원...",
          "speaker_id": "2",
          "time": "20210821 22:48"
        },
        {
          "id": "MDRW2100005494.1.16",
          "form": "화장실 타일이?",
          "original_form": "화장실 타일이?",
          "speaker_id": "1",
          "time": "20210821 22:48"
        },
        {
          "id": "MDRW2100005494.1.17",
          "form": "아니아니 그냥 그 거실이나 방에 쓰는 타일",
          "original_form": "아니아니 그냥 그 거실이나 방에 쓰는 타일",
          "speaker_id": "2",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005494.1.18",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005494.1.19",
          "form": "집에 있으니까 집안일 안해도 되는건 좋은데",
          "original_form": "집에 있으니까 집안일 안해도 되는건 좋은데",
          "speaker_id": "1",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005494.1.20",
          "form": "야식 맘대로 못먹어서",
          "original_form": "야식 맘대로 못먹어서",
          "speaker_id": "1",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005494.1.21",
          "form": "슬프다",
          "original_form": "슬프다",
          "speaker_id": "1",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005494.1.22",
          "form": "앗 그르게 아쉽네.....",
          "original_form": "앗 그르게 아쉽네.....",
          "speaker_id": "2",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005494.1.23",
          "form": "근데 또 야식 먹고 나면",
          "original_form": "근데 또 야식 먹고 나면",
          "speaker_id": "2",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005494.1.24",
          "form": "먹을 때는 좋은데",
          "original_form": "먹을 때는 좋은데",
          "speaker_id": "2",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005494.1.25",
          "form": "그거 치우는 게 너무 귀찮음",
          "original_form": "그거 치우는 게 너무 귀찮음",
          "speaker_id": "2",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005494.1.26",
          "form": "맞아ㅋㅋ",
          "original_form": "맞아ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005494.1.27",
          "form": "청소는 다 귀찮다 하네",
          "original_form": "청소는 다 귀찮다 하네",
          "speaker_id": "2",
