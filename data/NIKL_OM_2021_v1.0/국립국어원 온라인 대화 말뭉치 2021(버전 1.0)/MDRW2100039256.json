      "utterance": [
        {
          "id": "MDRW2100039256.1.1",
          "form": "오 대박",
          "original_form": "오 대박",
          "speaker_id": "2",
          "time": "20210903 15:13"
        },
        {
          "id": "MDRW2100039256.1.2",
          "form": "나 욕지도 고구마 완전 궁금했어",
          "original_form": "나 욕지도 고구마 완전 궁금했어",
          "speaker_id": "2",
          "time": "20210903 15:13"
        },
        {
          "id": "MDRW2100039256.1.3",
          "form": "십키로 재보니까 더 되더라",
          "original_form": "십키로 재보니까 더 되더라",
          "speaker_id": "1",
          "time": "20210903 15:14"
        },
        {
          "id": "MDRW2100039256.1.4",
          "form": "헐 짱좋네",
          "original_form": "헐 짱좋네",
          "speaker_id": "2",
          "time": "20210903 15:14"
        },
        {
          "id": "MDRW2100039256.1.5",
          "form": "근데 어떻게 쟀어?",
          "original_form": "근데 어떻게 쟀어?",
          "speaker_id": "2",
          "time": "20210903 15:14"
        },
        {
          "id": "MDRW2100039256.1.6",
          "form": "내가 들고 체중계 올라갔다ㅋㅋㅋ",
          "original_form": "내가 들고 체중계 올라갔다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:14"
        },
        {
          "id": "MDRW2100039256.1.7",
          "form": "헐ㅋㅋㅋㅋ안 부서졌나 몰라",
          "original_form": "헐ㅋㅋㅋㅋ안 부서졌나 몰라",
          "speaker_id": "2",
          "time": "20210903 15:14"
        },
        {
          "id": "MDRW2100039256.1.8",
          "form": "멀쩡하거든?",
          "original_form": "멀쩡하거든?",
          "speaker_id": "1",
          "time": "20210903 15:15"
        },
        {
          "id": "MDRW2100039256.1.9",
          "form": "고구마는? 좋아?",
          "original_form": "고구마는? 좋아?",
          "speaker_id": "2",
          "time": "20210903 15:15"
        },
        {
          "id": "MDRW2100039256.1.10",
          "form": "먹기 좋은 사이즈야",
          "original_form": "먹기 좋은 사이즈야",
          "speaker_id": "1",
          "time": "20210903 15:15"
        },
        {
          "id": "MDRW2100039256.1.11",
          "form": "너는 좀 크다고 할지 모르겠다",
          "original_form": "너는 좀 크다고 할지 모르겠다",
          "speaker_id": "1",
          "time": "20210903 15:15"
        },
        {
          "id": "MDRW2100039256.1.12",
          "form": "난 작은 게 좋은데",
          "original_form": "난 작은 게 좋은데",
          "speaker_id": "2",
          "time": "20210903 15:15"
        },
        {
          "id": "MDRW2100039256.1.13",
          "form": "아 근데 고구마 찍힌 자국도 있다",
          "original_form": "아 근데 고구마 찍힌 자국도 있다",
          "speaker_id": "1",
          "time": "20210903 15:15"
        },
        {
          "id": "MDRW2100039256.1.14",
          "form": "아 그거 비싸던데 그런거 보냄?",
          "original_form": "아 그거 비싸던데 그런거 보냄?",
          "speaker_id": "2",
          "time": "20210903 15:16"
        },
        {
          "id": "MDRW2100039256.1.15",
          "form": "그러게말이다",
          "original_form": "그러게말이다",
          "speaker_id": "1",
          "time": "20210903 15:16"
        },
        {
          "id": "MDRW2100039256.1.16",
          "form": "농민들한텐 이런거도 다 자식이라던데 어쩔수없지",
          "original_form": "농민들한텐 이런거도 다 자식이라던데 어쩔수없지",
          "speaker_id": "1",
          "time": "20210903 15:16"
        },
        {
          "id": "MDRW2100039256.1.17",
          "form": "그래도 엄청 비싸던데 좀 그렇네",
          "original_form": "그래도 엄청 비싸던데 좀 그렇네",
          "speaker_id": "2",
          "time": "20210903 15:16"
        },
        {
          "id": "MDRW2100039256.1.18",
          "form": "참아야지 상품가치 없는 게 섞여있",
          "original_form": "참아야지 상품가치 없는 게 섞여있",
          "speaker_id": "1",
          "time": "20210903 15:17"
        },
        {
          "id": "MDRW2100039256.1.19",
          "form": "어서 화날뻔했다",
          "original_form": "어서 화날뻔했다",
          "speaker_id": "1",
          "time": "20210903 15:23"
        },
        {
          "id": "MDRW2100039256.1.20",
          "form": "당연하지 욕지도 고구마 비싸던데",
          "original_form": "당연하지 욕지도 고구마 비싸던데",
          "speaker_id": "2",
          "time": "20210903 15:24"
        }
      ]
