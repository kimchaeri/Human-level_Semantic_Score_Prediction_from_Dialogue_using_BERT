      "utterance": [
        {
          "id": "MDRW2100043538.1.1",
          "form": "안녕하세요! 여행 좋아하세요?",
          "original_form": "안녕하세요! 여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100043538.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043538.1.3",
          "form": "요즘 떠나보고싶긴해요!",
          "original_form": "요즘 떠나보고싶긴해요!",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043538.1.4",
          "form": "특히 해외나 제주도요ㅠㅠ",
          "original_form": "특히 해외나 제주도요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043538.1.5",
          "form": "그쵸ㅜㅠ 저도 해외여행을 더 좋아하는데",
          "original_form": "그쵸ㅜㅠ 저도 해외여행을 더 좋아하는데",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043538.1.6",
          "form": "안 간지 너무 오래돼서 요즘은 제주도 가고 싶어요",
          "original_form": "안 간지 너무 오래돼서 요즘은 제주도 가고 싶어요",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043538.1.7",
          "form": "아 정말요??",
          "original_form": "아 정말요??",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043538.1.8",
          "form": "친구분들이랑 다녀오신건가요?",
          "original_form": "친구분들이랑 다녀오신건가요?",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043538.1.9",
          "form": "저는 여행 가족들이랑 많이 갔어요!",
          "original_form": "저는 여행 가족들이랑 많이 갔어요!",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043538.1.10",
          "form": "친구들이랑은 부산이랑 일본 가봤어요",
          "original_form": "친구들이랑은 부산이랑 일본 가봤어요",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043538.1.11",
          "form": "오오 부산 친구몇분이랑 가신거에요?",
          "original_form": "오오 부산 친구몇분이랑 가신거에요?",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043538.1.12",
          "form": "저포함 여섯 명이서 갔어요!",
          "original_form": "저포함 여섯 명이서 갔어요!",
          "speaker_id": "1",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043538.1.13",
          "form": "대학 동기들이랑요!!",
          "original_form": "대학 동기들이랑요!!",
          "speaker_id": "1",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043538.1.14",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043538.1.15",
          "form": "여럿이 가면 더 좋은 점이 있을까요?",
          "original_form": "여럿이 가면 더 좋은 점이 있을까요?",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043538.1.16",
          "form": "일단 좋아하는 사람들이랑 노는 거 자체가 좋고",
          "original_form": "일단 좋아하는 사람들이랑 노는 거 자체가 좋고",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043538.1.17",
          "form": "역할 분담이 돼서 개인이 맡아야 하는 일이 적어져요",
          "original_form": "역할 분담이 돼서 개인이 맡아야 하는 일이 적어져요",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043538.1.18",
          "form": "그리고 음식 다양하게 시킬 수 있어요ㅋㅋ최고",
          "original_form": "그리고 음식 다양하게 시킬 수 있어요ㅋㅋ최고",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043538.1.19",
          "form": "아 맞네요 음식!!!!",
          "original_form": "아 맞네요 음식!!!!",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043538.1.20",
          "form": "그게 참 좋은 것 같아요",
          "original_form": "그게 참 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043538.1.21",
          "form": "둘이면 시키는게 제한되니ㅠㅠ",
          "original_form": "둘이면 시키는게 제한되니ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043538.1.22",
          "form": "그때 저녁에 작정하고 사오고 시키고 그랬는데",
          "original_form": "그때 저녁에 작정하고 사오고 시키고 그랬는데",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100043538.1.23",
          "form": "뭐 먹었냐면",
          "original_form": "뭐 먹었냐면",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100043538.1.24",
          "form": "다코야끼 닭강정 김밥 주먹밥 회 떡볶이 튀김 과자 피자 음료수 맥주",
          "original_form": "다코야끼 닭강정 김밥 주먹밥 회 떡볶이 튀김 과자 피자 음료수 맥주",
          "speaker_id": "1",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043538.1.25",
          "form": "이랬어요ㅋㅋㅋㅋㅋ",
          "original_form": "이랬어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043538.1.26",
          "form": "와...네명이서요??",
          "original_form": "와...네명이서요??",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043538.1.27",
          "form": "회좋아하시나봐요",
          "original_form": "회좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043538.1.28",
          "form": "아뇨 여섯명이요! ㅋㅋㅋㅋ",
          "original_form": "아뇨 여섯명이요! ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100043538.1.29",
          "form": "회 좋아하기도 하고 부산여행이니까요ㅎㅎ",
          "original_form": "회 좋아하기도 하고 부산여행이니까요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100043538.1.30",
          "form": "맞아요 ㅎㅎㅎㅎ",
          "original_form": "맞아요 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100043538.1.31",
          "form": "부산여행지기억남는곳있어요?",
          "original_form": "부산여행지기억남는곳있어요?",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100043538.1.32",
          "form": "사실 밤에 바닷가 앉아서 광안대교 보고 불꽃놀이하고 그게 최고인거 같아요",
          "original_form": "사실 밤에 바닷가 앉아서 광안대교 보고 불꽃놀이하고 그게 최고인거 같아요",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100043538.1.33",
          "form": "저는 바다를 너무 좋아해서오",
          "original_form": "저는 바다를 너무 좋아해서오",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100043538.1.34",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100043538.1.35",
          "form": "아 정말요???",
          "original_form": "아 정말요???",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100043538.1.36",
          "form": "불꽃놀이해도되나요",
          "original_form": "불꽃놀이해도되나요",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100043538.1.37",
          "form": "옛날에 가서 했는데 요즘은 안되려나요",
          "original_form": "옛날에 가서 했는데 요즘은 안되려나요",
          "speaker_id": "1",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100043538.1.38",
          "form": "아직 많이 할거에요 아마!",
          "original_form": "아직 많이 할거에요 아마!",
          "speaker_id": "1",
          "time": "20210907 21:31"
        }
      ]
    }
