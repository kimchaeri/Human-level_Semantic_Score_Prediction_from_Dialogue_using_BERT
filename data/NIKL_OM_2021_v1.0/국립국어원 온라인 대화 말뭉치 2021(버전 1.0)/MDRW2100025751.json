      "utterance": [
        {
          "id": "MDRW2100025751.1.1",
          "form": "동물의숲에서 좋아하시는 주민이 있으신가요?",
          "original_form": "동물의숲에서 좋아하시는 주민이 있으신가요?",
          "speaker_id": "1",
          "time": "20210901 16:44"
        },
        {
          "id": "MDRW2100025751.1.2",
          "form": "저는 우리섬에있는 애플이요",
          "original_form": "저는 우리섬에있는 애플이요",
          "speaker_id": "2",
          "time": "20210901 16:44"
        },
        {
          "id": "MDRW2100025751.1.3",
          "form": "너무 귀여워요 뒷통수가",
          "original_form": "너무 귀여워요 뒷통수가",
          "speaker_id": "2",
          "time": "20210901 16:44"
        },
        {
          "id": "MDRW2100025751.1.4",
          "form": "아 동물의숲에서 애플 정말 귀엽죠 ㅎㅎ",
          "original_form": "아 동물의숲에서 애플 정말 귀엽죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 16:45"
        },
        {
          "id": "MDRW2100025751.1.5",
          "form": "우님은요?",
          "original_form": "우님은요?",
          "speaker_id": "2",
          "time": "20210901 16:45"
        },
        {
          "id": "MDRW2100025751.1.6",
          "form": "저는 비앙카와 릴리를 좋아해요",
          "original_form": "저는 비앙카와 릴리를 좋아해요",
          "speaker_id": "1",
          "time": "20210901 16:46"
        },
        {
          "id": "MDRW2100025751.1.7",
          "form": "늑대 종류를 좋아해서 커다란 입이 너무 마음에 들어요",
          "original_form": "늑대 종류를 좋아해서 커다란 입이 너무 마음에 들어요",
          "speaker_id": "1",
          "time": "20210901 16:46"
        },
        {
          "id": "MDRW2100025751.1.8",
          "form": "비앙카! 데려오고 싶은데 아직 못만났어요",
          "original_form": "비앙카! 데려오고 싶은데 아직 못만났어요",
          "speaker_id": "2",
          "time": "20210901 16:46"
        },
        {
          "id": "MDRW2100025751.1.9",
          "form": "맞아요 ㅠㅠ 아직 저도 데려오진 못했어요",
          "original_form": "맞아요 ㅠㅠ 아직 저도 데려오진 못했어요",
          "speaker_id": "1",
          "time": "20210901 16:46"
        },
        {
          "id": "MDRW2100025751.1.10",
          "form": "그죠",
          "original_form": "그죠",
          "speaker_id": "2",
          "time": "20210901 16:46"
        },
        {
          "id": "MDRW2100025751.1.11",
          "form": "시베리아 데려왔는데",
          "original_form": "시베리아 데려왔는데",
          "speaker_id": "2",
          "time": "20210901 16:46"
        },
        {
          "id": "MDRW2100025751.1.12",
          "form": "늑대가 참 잘생겼어요",
          "original_form": "늑대가 참 잘생겼어요",
          "speaker_id": "2",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100025751.1.13",
          "form": "헉 늑대 데리고 계시군요 너무 부럽네요 ㅎㅎ",
          "original_form": "헉 늑대 데리고 계시군요 너무 부럽네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100025751.1.14",
          "form": "모델링이 잘된 동물인거같아요",
          "original_form": "모델링이 잘된 동물인거같아요",
          "speaker_id": "2",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100025751.1.15",
          "form": "다른 위시리스트인 주민이 있으신가요?",
          "original_form": "다른 위시리스트인 주민이 있으신가요?",
          "speaker_id": "1",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100025751.1.16",
          "form": "뽀야미랑 쭈니요",
          "original_form": "뽀야미랑 쭈니요",
          "speaker_id": "2",
          "time": "20210901 16:49"
        },
        {
          "id": "MDRW2100025751.1.17",
          "form": "좀 작은 소동물친구들이 좋아요",
          "original_form": "좀 작은 소동물친구들이 좋아요",
          "speaker_id": "2",
          "time": "20210901 16:49"
        },
        {
          "id": "MDRW2100025751.1.18",
          "form": "소동물친구들이 우다다하고 달려오는 모습은 정말 귀여워요",
          "original_form": "소동물친구들이 우다다하고 달려오는 모습은 정말 귀여워요",
          "speaker_id": "1",
          "time": "20210901 16:50"
        },
        {
          "id": "MDRW2100025751.1.19",
          "form": "꼭 데려오셨으면 좋겠네요!",
          "original_form": "꼭 데려오셨으면 좋겠네요!",
          "speaker_id": "1",
          "time": "20210901 16:51"
        }
      ]
