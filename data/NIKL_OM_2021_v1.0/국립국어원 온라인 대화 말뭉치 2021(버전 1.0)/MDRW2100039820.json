      "utterance": [
        {
          "id": "MDRW2100039820.1.1",
          "form": "넷플 켜",
          "original_form": "넷플 켜",
          "speaker_id": "1",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100039820.1.2",
          "form": "이번주 슬기로운 의사생활이나 보자..",
          "original_form": "이번주 슬기로운 의사생활이나 보자..",
          "speaker_id": "1",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100039820.1.3",
          "form": "그거 어제 결방했어",
          "original_form": "그거 어제 결방했어",
          "speaker_id": "2",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100039820.1.4",
          "form": "헐 몰랐어.. 뭐 때문에 결방이었어??",
          "original_form": "헐 몰랐어.. 뭐 때문에 결방이었어??",
          "speaker_id": "1",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100039820.1.5",
          "form": "어제 축구 이라크전 중계 때문에.",
          "original_form": "어제 축구 이라크전 중계 때문에.",
          "speaker_id": "2",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039820.1.6",
          "form": "헐 아쉽다",
          "original_form": "헐 아쉽다",
          "speaker_id": "1",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039820.1.7",
          "form": "그럼 왓챠에서 오피스 볼까",
          "original_form": "그럼 왓챠에서 오피스 볼까",
          "speaker_id": "1",
          "time": "20210903 22:19"
        },
        {
          "id": "MDRW2100039820.1.8",
          "form": "그래 2화 틀었어",
          "original_form": "그래 2화 틀었어",
          "speaker_id": "2",
          "time": "20210903 22:20"
        },
        {
          "id": "MDRW2100039820.1.9",
          "form": "이 에피소드가 인기 진짜 많은 거 알지?",
          "original_form": "이 에피소드가 인기 진짜 많은 거 알지?",
          "speaker_id": "2",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039820.1.10",
          "form": "어ㅋㅋㅋㅋ 그리고 이때부터 메러디스 출연한다",
          "original_form": "어ㅋㅋㅋㅋ 그리고 이때부터 메러디스 출연한다",
          "speaker_id": "1",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039820.1.11",
          "form": "그러네. 마이클 캐릭터도 확실히 이상해지고..",
          "original_form": "그러네. 마이클 캐릭터도 확실히 이상해지고..",
          "speaker_id": "2",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039820.1.12",
          "form": "마이클이 토비 싫어하는 거 너무 웃겨",
          "original_form": "마이클이 토비 싫어하는 거 너무 웃겨",
          "speaker_id": "2",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100039820.1.13",
          "form": "ㅋㅋㅋㅋㅋ 진짜 마이클ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ 진짜 마이클ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100039820.1.14",
          "form": "토비가 오피스 작가 중에 한 명인 거 알아??",
          "original_form": "토비가 오피스 작가 중에 한 명인 거 알아??",
          "speaker_id": "1",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100039820.1.15",
          "form": "진짜? 몰랐어. 원래 배우인 줄.",
          "original_form": "진짜? 몰랐어. 원래 배우인 줄.",
          "speaker_id": "2",
          "time": "20210903 22:23"
        },
        {
          "id": "MDRW2100039820.1.16",
          "form": "원래 작가인데 일회성으로 출연했다가 고정멤버 된 거래ㅋㅋㅋ",
          "original_form": "원래 작가인데 일회성으로 출연했다가 고정멤버 된 거래ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:24"
        },
        {
          "id": "MDRW2100039820.1.17",
          "form": "연가도 잘하는데. 대단하네",
          "original_form": "연가도 잘하는데. 대단하네",
          "speaker_id": "2",
          "time": "20210903 22:24"
        },
        {
          "id": "MDRW2100039820.1.18",
          "form": "연기도 잘한다구.",
          "original_form": "연기도 잘한다구.",
          "speaker_id": "2",
          "time": "20210903 22:24"
        },
        {
          "id": "MDRW2100039820.1.19",
          "form": "맞어ㅋㅋㅋ 토비도 엄청 기운 없는 캐릭터라 신기했는데",
          "original_form": "맞어ㅋㅋㅋ 토비도 엄청 기운 없는 캐릭터라 신기했는데",
          "speaker_id": "1",
          "time": "20210903 22:25"
        },
        {
          "id": "MDRW2100039820.1.20",
          "form": "그게 작가 본인이 만든 캐릭터라니..",
          "original_form": "그게 작가 본인이 만든 캐릭터라니..",
          "speaker_id": "1",
          "time": "20210903 22:25"
        },
        {
          "id": "MDRW2100039820.1.21",
          "form": "오스카는 시즌 초반이랑 후반이랑 크게 달라지지 않았다. 비슷하네",
          "original_form": "오스카는 시즌 초반이랑 후반이랑 크게 달라지지 않았다. 비슷하네",
          "speaker_id": "2",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100039820.1.22",
          "form": "오스카는 오피스에서 그나마 상식적인 캐릭터잖아ㅋㅋㅋ",
          "original_form": "오스카는 오피스에서 그나마 상식적인 캐릭터잖아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:28"
        }
      ]
    }
  ]
