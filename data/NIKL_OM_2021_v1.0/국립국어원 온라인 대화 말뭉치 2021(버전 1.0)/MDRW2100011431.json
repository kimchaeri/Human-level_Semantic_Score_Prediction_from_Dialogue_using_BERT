        },
        {
          "id": "MDRW2100011431.1.3",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 12:41"
        },
        {
          "id": "MDRW2100011431.1.4",
          "form": "좋아는하는데 푹빠지게되서 잘안해요!",
          "original_form": "좋아는하는데 푹빠지게되서 잘안해요!",
          "speaker_id": "2",
          "time": "20210824 12:41"
        },
        {
          "id": "MDRW2100011431.1.5",
          "form": "오 어떤게임 하셨었나요",
          "original_form": "오 어떤게임 하셨었나요",
          "speaker_id": "1",
          "time": "20210824 12:41"
        },
        {
          "id": "MDRW2100011431.1.6",
          "form": "옛날엔 마비노기 메이플스토리?",
          "original_form": "옛날엔 마비노기 메이플스토리?",
          "speaker_id": "2",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100011431.1.7",
          "form": "메이플스토리 송편이벤트 추억이네요ㅎㅎ",
          "original_form": "메이플스토리 송편이벤트 추억이네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100011431.1.8",
          "form": "ㅎㅎ애니팡도 많이햇네요",
          "original_form": "ㅎㅎ애니팡도 많이햇네요",
          "speaker_id": "2",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100011431.1.9",
          "form": "와 애니팡ㅎㅎ",
          "original_form": "와 애니팡ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 12:43"
        },
        {
          "id": "MDRW2100011431.1.10",
          "form": "요즘은 pc방도 못가니 게임하기도 힘드네요",
          "original_form": "요즘은 pc방도 못가니 게임하기도 힘드네요",
          "speaker_id": "1",
          "time": "20210824 12:43"
        },
        {
          "id": "MDRW2100011431.1.11",
          "form": "해보셧나요?",
          "original_form": "해보셧나요?",
          "speaker_id": "2",
          "time": "20210824 12:43"
        },
        {
          "id": "MDRW2100011431.1.12",
          "form": "당연하죠! 예전언 많이했죠ㅎㅎ",
          "original_form": "당연하죠! 예전언 많이했죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 12:44"
        },
        {
          "id": "MDRW2100011431.1.13",
          "form": "맞아요 가기힘들죠",
          "original_form": "맞아요 가기힘들죠",
          "speaker_id": "2",
          "time": "20210824 12:44"
        },
        {
          "id": "MDRW2100011431.1.14",
          "form": "pc방에서 라면이 국룰인데ㅠ",
          "original_form": "pc방에서 라면이 국룰인데ㅠ",
          "speaker_id": "1",
          "time": "20210824 12:46"
        },
