      "utterance": [
        {
          "id": "MDRW2100013758.1.1",
          "form": "안녕하세요ㅎㅎ",
          "original_form": "안녕하세요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100013758.1.2",
          "form": "제가 사는 덴 오늘 비 왔는데 name2님이 사는 덴 어떤가요??",
          "original_form": "제가 사는 덴 오늘 비 왔는데 &name2&님이 사는 덴 어떤가요??",
          "speaker_id": "1",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100013758.1.3",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013758.1.4",
          "form": "저희도 비왔어요ㅠ",
          "original_form": "저희도 비왔어요ㅠ",
          "speaker_id": "2",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013758.1.5",
          "form": "일기예보엔 하루종이 비 온다는데",
          "original_form": "일기예보엔 하루종이 비 온다는데",
          "speaker_id": "2",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013758.1.6",
          "form": "집 안에서 보기엔 지금은 안오는 것같아요",
          "original_form": "집 안에서 보기엔 지금은 안오는 것같아요",
          "speaker_id": "2",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013758.1.7",
          "form": "저희도 아침엔 오다가 지금은 그쳤네요ㅎㅎ",
          "original_form": "저희도 아침엔 오다가 지금은 그쳤네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013758.1.8",
          "form": "태풍이 언제 갈건지ㅠ 비 그만 왔으면 좋겠어요",
          "original_form": "태풍이 언제 갈건지ㅠ 비 그만 왔으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013758.1.9",
          "form": "그러게요...",
          "original_form": "그러게요...",
          "speaker_id": "1",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100013758.1.10",
          "form": "제가 사는 덴 쥬말에 그친다고 하긴 하네요...",
          "original_form": "제가 사는 덴 쥬말에 그친다고 하긴 하네요...",
          "speaker_id": "1",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100013758.1.11",
          "form": "비 그치면 가을 날씨가 될까요?",
          "original_form": "비 그치면 가을 날씨가 될까요?",
          "speaker_id": "2",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100013758.1.12",
          "form": "그러게요... 근데 입추 지나고 확실히 기온이 떨어진 것 같아요",
          "original_form": "그러게요... 근데 입추 지나고 확실히 기온이 떨어진 것 같아요",
          "speaker_id": "1",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100013758.1.13",
          "form": "음 맞는 것같아요",
          "original_form": "음 맞는 것같아요",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100013758.1.14",
          "form": "햇빛이 여전히 뜨겁긴하지만 바람이 시원하게 불어서",
          "original_form": "햇빛이 여전히 뜨겁긴하지만 바람이 시원하게 불어서",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100013758.1.15",
          "form": "그늘가면 시원해요",
          "original_form": "그늘가면 시원해요",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100013758.1.16",
          "form": "맞아요 밤에도 이젠 안 덥고...",
          "original_form": "맞아요 밤에도 이젠 안 덥고...",
          "speaker_id": "1",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100013758.1.17",
          "form": "빨리 가을 옷 입었으면 좋겠어요ㅎㅎ",
          "original_form": "빨리 가을 옷 입었으면 좋겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100013758.1.18",
          "form": "그러게요ㅎㅎ 전 가을 옷이 제일 예쁜 거 같아요ㅎㅎ",
          "original_form": "그러게요ㅎㅎ 전 가을 옷이 제일 예쁜 거 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100013758.1.19",
          "form": "오 저두요 그래서 전 가을 겨울 옷을 좋아해요",
          "original_form": "오 저두요 그래서 전 가을 겨울 옷을 좋아해요",
          "speaker_id": "2",
          "time": "20210825 18:56"
        },
        {
          "id": "MDRW2100013758.1.20",
          "form": "레이어드해 입기도 좋고 가을 겨울 옷이 예쁜 거 같아요ㅎㅎ",
          "original_form": "레이어드해 입기도 좋고 가을 겨울 옷이 예쁜 거 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:22"
        }
