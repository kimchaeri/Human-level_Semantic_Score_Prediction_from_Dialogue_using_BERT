      "utterance": [
        {
          "id": "MDRW2100024643.1.1",
          "form": "왜냐면 불화살 캐릭터이기 때문이야...",
          "original_form": "왜냐면 불화살 캐릭터이기 때문이야...",
          "speaker_id": "1",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100024643.1.2",
          "form": "엠버가 그 성능에 4성인데",
          "original_form": "엠버가 그 성능에 4성인데",
          "speaker_id": "1",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100024643.1.3",
          "form": "요이미야는 5성 찍어야지 ㅋㅋㅋㅋㅋ",
          "original_form": "요이미야는 5성 찍어야지 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100024643.1.4",
          "form": "ㅇㅏ",
          "original_form": "ㅇㅏ",
          "speaker_id": "2",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100024643.1.5",
          "form": "설득당했다",
          "original_form": "설득당했다",
          "speaker_id": "2",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100024643.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100024643.1.7",
          "form": "이제 슬슬",
          "original_form": "이제 슬슬",
          "speaker_id": "2",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100024643.1.8",
          "form": "필드보스 잡아야하나",
          "original_form": "필드보스 잡아야하나",
          "speaker_id": "2",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100024643.1.9",
          "form": "재미없는데.....",
          "original_form": "재미없는데.....",
          "speaker_id": "2",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100024643.1.10",
          "form": "드발린?",
          "original_form": "드발린?",
          "speaker_id": "1",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100024643.1.11",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100024643.1.12",
          "form": "그 그",
          "original_form": "그 그",
          "speaker_id": "2",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100024643.1.13",
          "form": "화염나무같은거",
          "original_form": "화염나무같은거",
          "speaker_id": "2",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100024643.1.14",
          "form": "48렙이고",
          "original_form": "48렙이고",
          "speaker_id": "2",
          "time": "20210831 17:01"
        },
        {
          "id": "MDRW2100024643.1.15",
          "form": "다른 렙업템은",
          "original_form": "다른 렙업템은",
          "speaker_id": "2",
          "time": "20210831 17:01"
        },
        {
          "id": "MDRW2100024643.1.16",
          "form": "싹 모았거든",
          "original_form": "싹 모았거든",
          "speaker_id": "2",
          "time": "20210831 17:01"
        },
        {
          "id": "MDRW2100024643.1.17",
          "form": "ㅇㅏ 근데 돌기 싫다",
          "original_form": "ㅇㅏ 근데 돌기 싫다",
          "speaker_id": "2",
          "time": "20210831 17:01"
        },
        {
          "id": "MDRW2100024643.1.18",
          "form": "차라리 무기업글 재료부터 준비할까",
          "original_form": "차라리 무기업글 재료부터 준비할까",
          "speaker_id": "2",
          "time": "20210831 17:01"
        },
        {
          "id": "MDRW2100024643.1.19",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "1",
          "time": "20210831 17:02"
        },
        {
          "id": "MDRW2100024643.1.20",
          "form": "그것도 좋지",
          "original_form": "그것도 좋지",
          "speaker_id": "1",
          "time": "20210831 17:02"
        },
        {
          "id": "MDRW2100024643.1.21",
          "form": "난....",
          "original_form": "난....",
          "speaker_id": "1",
          "time": "20210831 17:02"
        },
        {
          "id": "MDRW2100024643.1.22",
          "form": "특성 렙업부터 시키려고",
          "original_form": "특성 렙업부터 시키려고",
          "speaker_id": "1",
          "time": "20210831 17:02"
        },
        {
          "id": "MDRW2100024643.1.23",
          "form": "난",
          "original_form": "난",
          "speaker_id": "2",
          "time": "20210831 17:03"
        },
        {
          "id": "MDRW2100024643.1.24",
          "form": "렙업재료도",
          "original_form": "렙업재료도",
          "speaker_id": "2",
          "time": "20210831 17:03"
        },
        {
          "id": "MDRW2100024643.1.25",
          "form": "힘들게 모아서",
          "original_form": "힘들게 모아서",
          "speaker_id": "2",
          "time": "20210831 17:03"
        },
        {
          "id": "MDRW2100024643.1.26",
          "form": "특성렙업에 쓰면",
          "original_form": "특성렙업에 쓰면",
          "speaker_id": "2",
          "time": "20210831 17:03"
        },
        {
          "id": "MDRW2100024643.1.27",
          "form": "또 모아야되잖아......",
          "original_form": "또 모아야되잖아......",
          "speaker_id": "2",
          "time": "20210831 17:03"
        },
        {
          "id": "MDRW2100024643.1.28",
          "form": "ㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 17:03"
        },
        {
          "id": "MDRW2100024643.1.29",
          "form": "차라리",
          "original_form": "차라리",
          "speaker_id": "1",
          "time": "20210831 17:03"
        },
        {
          "id": "MDRW2100024643.1.30",
          "form": "필드보스를 돌아서",
          "original_form": "필드보스를 돌아서",
          "speaker_id": "1",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100024643.1.31",
          "form": "빨리 렙업을 해서",
          "original_form": "빨리 렙업을 해서",
          "speaker_id": "1",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100024643.1.32",
          "form": "재료 다 써버려",
          "original_form": "재료 다 써버려",
          "speaker_id": "1",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100024643.1.33",
          "form": "근데 80->90렙으로 레벨업 해본 입장에서는",
          "original_form": "근데 80->90렙으로 레벨업 해본 입장에서는",
          "speaker_id": "1",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100024643.1.34",
          "form": "돌파하고 풀 렙업 시켜줘도",
          "original_form": "돌파하고 풀 렙업 시켜줘도",
          "speaker_id": "1",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100024643.1.35",
          "form": "성능 거의 차이 안남 ....",
          "original_form": "성능 거의 차이 안남 ....",
          "speaker_id": "1",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100024643.1.36",
          "form": "90렙은 안찍고",
          "original_form": "90렙은 안찍고",
          "speaker_id": "2",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100024643.1.37",
          "form": "80렙 돌파만 하려구",
          "original_form": "80렙 돌파만 하려구",
          "speaker_id": "2",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100024643.1.38",
          "form": "다이루크 90렙 딜 얼마나와?",
          "original_form": "다이루크 90렙 딜 얼마나와?",
          "speaker_id": "2",
          "time": "20210831 17:05"
        },
        {
          "id": "MDRW2100024643.1.39",
          "form": "경험치 책은 다 있어?",
          "original_form": "경험치 책은 다 있어?",
          "speaker_id": "1",
          "time": "20210831 17:05"
        },
        {
          "id": "MDRW2100024643.1.40",
          "form": "몰라",
          "original_form": "몰라",
          "speaker_id": "2",
          "time": "20210831 17:05"
        },
        {
          "id": "MDRW2100024643.1.41",
          "form": "근데 혹시 내가",
          "original_form": "근데 혹시 내가",
          "speaker_id": "2",
          "time": "20210831 17:05"
        },
        {
          "id": "MDRW2100024643.1.42",
          "form": "조만간 5성을",
          "original_form": "조만간 5성을",
          "speaker_id": "2",
          "time": "20210831 17:05"
        },
        {
          "id": "MDRW2100024643.1.43",
          "form": "뽑을수도 잇잔아.......",
          "original_form": "뽑을수도 잇잔아.......",
          "speaker_id": "2",
          "time": "20210831 17:05"
        },
        {
          "id": "MDRW2100024643.1.44",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 17:05"
        },
        {
          "id": "MDRW2100024643.1.45",
          "form": "그니까 애껴둘거야.........",
          "original_form": "그니까 애껴둘거야.........",
          "speaker_id": "2",
          "time": "20210831 17:05"
        },
        {
          "id": "MDRW2100024643.1.46",
          "form": "어디서 타는 냄새가 나는데요",
          "original_form": "어디서 타는 냄새가 나는데요",
          "speaker_id": "1",
          "time": "20210831 17:05"
