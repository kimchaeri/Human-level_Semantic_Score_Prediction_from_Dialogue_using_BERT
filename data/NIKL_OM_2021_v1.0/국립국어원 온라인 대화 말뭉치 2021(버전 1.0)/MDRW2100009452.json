      "utterance": [
        {
          "id": "MDRW2100009452.1.1",
          "form": "애완동물 키우시나여?",
          "original_form": "애완동물 키우시나여?",
          "speaker_id": "1",
          "time": "20210822 23:32"
        },
        {
          "id": "MDRW2100009452.1.2",
          "form": "아니오. 지금은 키우지 않아요.\r\n반려동물 키우시나요?",
          "original_form": "아니오. 지금은 키우지 않아요.\r\n반려동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100009452.1.3",
          "form": ">아니요(오타입니다ㅠ",
          "original_form": ">아니요(오타입니다ㅠ",
          "speaker_id": "2",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100009452.1.4",
          "form": "저도 어릴때 강아지요.. \r\n예전엔 뭐 키우셨었나요?",
          "original_form": "저도 어릴때 강아지요.. \r\n예전엔 뭐 키우셨었나요?",
          "speaker_id": "1",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100009452.1.5",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "1",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009452.1.6",
          "form": "좋아하는 동물은 없나여?",
          "original_form": "좋아하는 동물은 없나여?",
          "speaker_id": "1",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009452.1.7",
          "form": "저는 금붕어 키웠어요!",
          "original_form": "저는 금붕어 키웠어요!",
          "speaker_id": "2",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009452.1.8",
          "form": "지금은 보는건 다 좋아해요",
          "original_form": "지금은 보는건 다 좋아해요",
          "speaker_id": "2",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009452.1.9",
          "form": "어떤 강아지 키우셨나요?",
          "original_form": "어떤 강아지 키우셨나요?",
          "speaker_id": "2",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009452.1.10",
          "form": "하양말틸즈랑.. 요키셔테리어랑.ㅎㅎㅎ",
          "original_form": "하양말틸즈랑.. 요키셔테리어랑.ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009452.1.11",
          "form": "털달린건 다 좋아하는편이에요..",
          "original_form": "털달린건 다 좋아하는편이에요..",
          "speaker_id": "1",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009452.1.12",
          "form": "강아지 유튜브 같은거도 보세요?",
          "original_form": "강아지 유튜브 같은거도 보세요?",
          "speaker_id": "1",
          "time": "20210822 23:44"
        },
        {
          "id": "MDRW2100009452.1.13",
          "form": "와 세상에 너무 귀여웠겠어요ㅎㅎ",
          "original_form": "와 세상에 너무 귀여웠겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:44"
        },
        {
          "id": "MDRW2100009452.1.14",
          "form": "저 강아지 유튜브는 조금 봐요",
          "original_form": "저 강아지 유튜브는 조금 봐요",
          "speaker_id": "2",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100009452.1.15",
          "form": "보시는 유튜브 있으세요?",
          "original_form": "보시는 유튜브 있으세요?",
          "speaker_id": "2",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100009452.1.16",
          "form": "ㅋㅋㅋ 저는 고양이파라서.. ㅎㅎㅎ",
          "original_form": "ㅋㅋㅋ 저는 고양이파라서.. ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:46"
        },
        {
          "id": "MDRW2100009452.1.17",
          "form": "크림히어로즈랑 일본쪽 고양이 봐용.. \r\n금붕어는 대형 수족관이엿나요?",
          "original_form": "크림히어로즈랑 일본쪽 고양이 봐용.. \r\n금붕어는 대형 수족관이엿나요?",
          "speaker_id": "1",
          "time": "20210822 23:47"
        },
        {
          "id": "MDRW2100009452.1.18",
          "form": "저는 고양이는 쫀덕이네랑 양어장 고양이들 봐요!",
          "original_form": "저는 고양이는 쫀덕이네랑 양어장 고양이들 봐요!",
          "speaker_id": "2",
          "time": "20210822 23:47"
        },
        {
          "id": "MDRW2100009452.1.19",
          "form": "금붕어는 작은 수조에서 키웠어요!",
          "original_form": "금붕어는 작은 수조에서 키웠어요!",
          "speaker_id": "2",
          "time": "20210822 23:47"
        },
        {
          "id": "MDRW2100009452.1.20",
          "form": "ㅋㅋㅋ 금붕어도 이름 지어주고 그래요?",
          "original_form": "ㅋㅋㅋ 금붕어도 이름 지어주고 그래요?",
          "speaker_id": "1",
          "time": "20210822 23:48"
        },
        {
          "id": "MDRW2100009452.1.21",
          "form": "지어주긴 했는데 솔직히 말하면 누가 누군지 하나도 구분 못했어요",
          "original_form": "지어주긴 했는데 솔직히 말하면 누가 누군지 하나도 구분 못했어요",
          "speaker_id": "2",
          "time": "20210822 23:48"
        },
        {
          "id": "MDRW2100009452.1.22",
          "form": "그냥 보이는대로 불렀어요ㅎㅎ",
          "original_form": "그냥 보이는대로 불렀어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:48"
        },
        {
          "id": "MDRW2100009452.1.23",
          "form": "ㅋㅋㅋ 아..",
          "original_form": "ㅋㅋㅋ 아..",
          "speaker_id": "1",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009452.1.24",
          "form": "고양이 좋아하시면 키우실 생각도 있으세요?",
          "original_form": "고양이 좋아하시면 키우실 생각도 있으세요?",
          "speaker_id": "2",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009452.1.25",
          "form": "주인도 구분 안되용?",
          "original_form": "주인도 구분 안되용?",
          "speaker_id": "1",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009452.1.26",
          "form": "그냥 하얀거 빨간거만 구분하고 다 똑같아보이더라고요ㅎㅎ",
          "original_form": "그냥 하얀거 빨간거만 구분하고 다 똑같아보이더라고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 23:49"
        }
      ]
    }
