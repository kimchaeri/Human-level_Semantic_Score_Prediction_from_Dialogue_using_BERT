      "utterance": [
        {
          "id": "MDRW2100035523.1.1",
          "form": "음 20대 후반이에요 !!",
          "original_form": "음 20대 후반이에요 !!",
          "speaker_id": "1",
          "time": "20210829 19:37"
        },
        {
          "id": "MDRW2100035523.1.2",
          "form": "와 몇 명 생각나는데 전부 멤버 수가 짝수예요",
          "original_form": "와 몇 명 생각나는데 전부 멤버 수가 짝수예요",
          "speaker_id": "2",
          "time": "20210829 19:37"
        },
        {
          "id": "MDRW2100035523.1.3",
          "form": "아 ㅋㅋㅋ 아니에요 확실히 홀수입니다 !!",
          "original_form": "아 ㅋㅋㅋ 아니에요 확실히 홀수입니다 !!",
          "speaker_id": "1",
          "time": "20210829 19:38"
        },
        {
          "id": "MDRW2100035523.1.4",
          "form": "탈퇴한 멤버 포함 홀수는 아니죠? ㅋㅋㅋㅋ",
          "original_form": "탈퇴한 멤버 포함 홀수는 아니죠? ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:38"
        },
        {
          "id": "MDRW2100035523.1.5",
          "form": "으악 아니에요 ㅋㅋ 탈퇴멤은 없네요 !",
          "original_form": "으악 아니에요 ㅋㅋ 탈퇴멤은 없네요 !",
          "speaker_id": "1",
          "time": "20210829 19:38"
        },
        {
          "id": "MDRW2100035523.1.6",
          "form": "아 죄송합니다ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 죄송합니다ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:39"
        },
        {
          "id": "MDRW2100035523.1.7",
          "form": "힌트 조굼만 더 주세요...",
          "original_form": "힌트 조굼만 더 주세요...",
          "speaker_id": "2",
          "time": "20210829 19:39"
        },
        {
          "id": "MDRW2100035523.1.8",
          "form": "음 멤버 전부 한국인이에요 !",
          "original_form": "음 멤버 전부 한국인이에요 !",
          "speaker_id": "1",
          "time": "20210829 19:41"
        },
        {
          "id": "MDRW2100035523.1.9",
          "form": "앗 혹시 방탄소년단",
          "original_form": "앗 혹시 방탄소년단",
          "speaker_id": "2",
          "time": "20210829 19:41"
        },
        {
          "id": "MDRW2100035523.1.10",
          "form": "오 대박",
          "original_form": "오 대박",
          "speaker_id": "1",
          "time": "20210829 19:42"
        },
        {
          "id": "MDRW2100035523.1.11",
          "form": "정답입니다 ㅋㅋㅋ",
          "original_form": "정답입니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:42"
        },
        {
          "id": "MDRW2100035523.1.12",
          "form": "앗 이런 메이저 아이돌을 힘들게 맞추다니ㅋㅋㅋㅋㅋ",
          "original_form": "앗 이런 메이저 아이돌을 힘들게 맞추다니ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:42"
        },
        {
          "id": "MDRW2100035523.1.13",
          "form": "ㅋㅋㅋㅋ원래 스무고개는 좀 어렵게 생각하게 되는거같아욬ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ원래 스무고개는 좀 어렵게 생각하게 되는거같아욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:44"
        },
        {
          "id": "MDRW2100035523.1.14",
          "form": "저도 맞춰볼까요?",
          "original_form": "저도 맞춰볼까요?",
          "speaker_id": "1",
          "time": "20210829 19:44"
        },
        {
          "id": "MDRW2100035523.1.15",
          "form": "아 좋아요 근데 저 완전 팬은 아니긴 한데 (부정기) 일단 맞춰보세요",
          "original_form": "아 좋아요 근데 저 완전 팬은 아니긴 한데 (부정기) 일단 맞춰보세요",
          "speaker_id": "2",
