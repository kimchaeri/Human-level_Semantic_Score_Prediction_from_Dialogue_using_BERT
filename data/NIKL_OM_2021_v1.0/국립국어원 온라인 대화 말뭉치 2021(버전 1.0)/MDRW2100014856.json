      "utterance": [
        {
          "id": "MDRW2100014856.1.1",
          "form": "어떤 내용인가요?",
          "original_form": "어떤 내용인가요?",
          "speaker_id": "2",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100014856.1.2",
          "form": "약간 제인오스틴 소설 느낌이에요",
          "original_form": "약간 제인오스틴 소설 느낌이에요",
          "speaker_id": "1",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100014856.1.3",
          "form": "연애담인가요?",
          "original_form": "연애담인가요?",
          "speaker_id": "2",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100014856.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100014856.1.5",
          "form": "오만과 편견 좋아했는데 반갑네요",
          "original_form": "오만과 편견 좋아했는데 반갑네요",
          "speaker_id": "2",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100014856.1.6",
          "form": "신분 격차가 있는 남녀주인공이 결국 결혼하고 행복한 가정을 이루어요",
          "original_form": "신분 격차가 있는 남녀주인공이 결국 결혼하고 행복한 가정을 이루어요",
          "speaker_id": "1",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100014856.1.7",
          "form": "궁금하네요",
          "original_form": "궁금하네요",
          "speaker_id": "2",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100014856.1.8",
          "form": "인물묘사가 되게 매력적이고",
          "original_form": "인물묘사가 되게 매력적이고",
          "speaker_id": "1",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100014856.1.9",
          "form": "배경이 되는 영지 풍경을 너무 아름답게 묘사해 두었어요",
          "original_form": "배경이 되는 영지 풍경을 너무 아름답게 묘사해 두었어요",
          "speaker_id": "1",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100014856.1.10",
          "form": "작가가 글솜씨가 있나봐요",
          "original_form": "작가가 글솜씨가 있나봐요",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100014856.1.11",
          "form": "네 필력이 좋아서 빠져들었어요",
          "original_form": "네 필력이 좋아서 빠져들었어요",
          "speaker_id": "1",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100014856.1.12",
          "form": "한번 찾아서 읽어 봐야겠어요",
          "original_form": "한번 찾아서 읽어 봐야겠어요",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100014856.1.13",
          "form": "전 네이버에서 봤어요",
          "original_form": "전 네이버에서 봤어요",
          "speaker_id": "1",
          "time": "20210907 21:05"
        }
