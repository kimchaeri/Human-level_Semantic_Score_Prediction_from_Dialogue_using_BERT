      "utterance": [
        {
          "id": "MDRW2100045672.1.1",
          "form": "ㅋㅋㅋㅋ뭔가 매운 성격이 달라서 그런가봐요",
          "original_form": "ㅋㅋㅋㅋ뭔가 매운 성격이 달라서 그런가봐요",
          "speaker_id": "2",
          "time": "20210908 05:08"
        },
        {
          "id": "MDRW2100045672.1.2",
          "form": "불닭같은 건 뜨거운 매운맛인데",
          "original_form": "불닭같은 건 뜨거운 매운맛인데",
          "speaker_id": "2",
          "time": "20210908 05:08"
        },
        {
          "id": "MDRW2100045672.1.3",
          "form": "와사비는 바늘로 칽 찌르는 것 같아요",
          "original_form": "와사비는 바늘로 칽 찌르는 것 같아요",
          "speaker_id": "2",
          "time": "20210908 05:08"
        },
        {
          "id": "MDRW2100045672.1.4",
          "form": "코를 찌르는 느낌인가요 ㅋㅋㅋㅋ",
          "original_form": "코를 찌르는 느낌인가요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:08"
        },
        {
          "id": "MDRW2100045672.1.5",
          "form": "ㅋㅋㅋㅋㅋ네 맞아요",
          "original_form": "ㅋㅋㅋㅋㅋ네 맞아요",
          "speaker_id": "2",
          "time": "20210908 05:09"
        },
        {
          "id": "MDRW2100045672.1.6",
          "form": "그래도 나름 매력있어서 먹게 되긴 해요ㅋㅋㅋ",
          "original_form": "그래도 나름 매력있어서 먹게 되긴 해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:09"
        },
        {
          "id": "MDRW2100045672.1.7",
          "form": "맞아요 매력잇죠 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 매력잇죠 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:09"
        },
        {
          "id": "MDRW2100045672.1.8",
          "form": "name2님은 제일 좋아하는 음식 잇나요",
          "original_form": "&name2&님은 제일 좋아하는 음식 잇나요",
          "speaker_id": "1",
          "time": "20210908 05:09"
        },
        {
          "id": "MDRW2100045672.1.9",
          "form": "잇다면 맞춰볼까요?",
          "original_form": "잇다면 맞춰볼까요?",
          "speaker_id": "1",
          "time": "20210908 05:10"
        },
        {
          "id": "MDRW2100045672.1.10",
          "form": "좋아요~!!!!",
          "original_form": "좋아요~!!!!",
          "speaker_id": "2",
          "time": "20210908 05:10"
        },
        {
          "id": "MDRW2100045672.1.11",
          "form": "맞춰보시죠",
          "original_form": "맞춰보시죠",
          "speaker_id": "2",
          "time": "20210908 05:10"
        },
        {
          "id": "MDRW2100045672.1.12",
          "form": "고기인가요?",
          "original_form": "고기인가요?",
          "speaker_id": "1",
          "time": "20210908 05:10"
        },
        {
          "id": "MDRW2100045672.1.13",
          "form": "아니요! 고기 아닙니다",
          "original_form": "아니요! 고기 아닙니다",
          "speaker_id": "2",
          "time": "20210908 05:11"
        },
        {
          "id": "MDRW2100045672.1.14",
          "form": "밀가루음식인가요?",
          "original_form": "밀가루음식인가요?",
          "speaker_id": "1",
          "time": "20210908 05:11"
        },
        {
          "id": "MDRW2100045672.1.15",
          "form": "네 밀이 들어가요!",
          "original_form": "네 밀이 들어가요!",
          "speaker_id": "2",
          "time": "20210908 05:11"
        },
        {
          "id": "MDRW2100045672.1.16",
          "form": "주로 배달로 드시나요?",
          "original_form": "주로 배달로 드시나요?",
          "speaker_id": "1",
          "time": "20210908 05:12"
        },
        {
          "id": "MDRW2100045672.1.17",
          "form": "아니요! 집에서 만들어먹어요",
          "original_form": "아니요! 집에서 만들어먹어요",
          "speaker_id": "2",
          "time": "20210908 05:12"
        },
        {
          "id": "MDRW2100045672.1.18",
          "form": "면 요리인가요?",
          "original_form": "면 요리인가요?",
          "speaker_id": "1",
          "time": "20210908 05:12"
        },
        {
          "id": "MDRW2100045672.1.19",
          "form": "네 면 요리예요!",
          "original_form": "네 면 요리예요!",
          "speaker_id": "2",
          "time": "20210908 05:15"
        },
        {
          "id": "MDRW2100045672.1.20",
          "form": "아이고 알림이 안 떴어뇨",
          "original_form": "아이고 알림이 안 떴어뇨",
          "speaker_id": "2",
          "time": "20210908 05:15"
        },
        {
          "id": "MDRW2100045672.1.21",
          "form": "ㅋㅋㅋㅋㅋ국물이 잇나요?",
          "original_form": "ㅋㅋㅋㅋㅋ국물이 잇나요?",
