      "utterance": [
        {
          "id": "MDRW2100032520.1.1",
          "form": "저녁엔 소고기 구워먹었어요",
          "original_form": "저녁엔 소고기 구워먹었어요",
          "speaker_id": "2",
          "time": "20210905 20:12"
        },
        {
          "id": "MDRW2100032520.1.2",
          "form": "오~ 소고기 좋아요",
          "original_form": "오~ 소고기 좋아요",
          "speaker_id": "1",
          "time": "20210905 20:20"
        },
        {
          "id": "MDRW2100032520.1.3",
          "form": "듕이님은 고기를 구워먹는 편인가요 볶아 먹는 편인가요",
          "original_form": "듕이님은 고기를 구워먹는 편인가요 볶아 먹는 편인가요",
          "speaker_id": "1",
          "time": "20210905 20:20"
        },
        {
          "id": "MDRW2100032520.1.4",
          "form": "구워먹는편이에요",
          "original_form": "구워먹는편이에요",
          "speaker_id": "2",
          "time": "20210905 20:59"
        },
        {
          "id": "MDRW2100032520.1.5",
          "form": "어제는 삼겹살 오늘은 소고기",
          "original_form": "어제는 삼겹살 오늘은 소고기",
          "speaker_id": "2",
          "time": "20210905 20:59"
        },
        {
          "id": "MDRW2100032520.1.6",
          "form": "구워먹는 고기를 좋아하는군요",
          "original_form": "구워먹는 고기를 좋아하는군요",
          "speaker_id": "1",
          "time": "20210905 21:09"
        },
        {
          "id": "MDRW2100032520.1.7",
          "form": "저는 탕으로된 고기 좋아해요",
          "original_form": "저는 탕으로된 고기 좋아해요",
          "speaker_id": "1",
          "time": "20210905 21:09"
        },
        {
          "id": "MDRW2100032520.1.8",
          "form": "갈비탕 추어탕 삼계탕 설렁탕 등등",
          "original_form": "갈비탕 추어탕 삼계탕 설렁탕 등등",
          "speaker_id": "1",
          "time": "20210905 21:09"
        },
        {
          "id": "MDRW2100032520.1.9",
          "form": "아 물에빠진 고기를 좋아하시는군여 ㅋㅋ",
          "original_form": "아 물에빠진 고기를 좋아하시는군여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:20"
        },
        {
          "id": "MDRW2100032520.1.10",
          "form": "고기는 다 좋은데 가장 선호하는 요리법이 끓이는 거에요",
          "original_form": "고기는 다 좋은데 가장 선호하는 요리법이 끓이는 거에요",
          "speaker_id": "1",
          "time": "20210905 21:24"
        },
        {
          "id": "MDRW2100032520.1.11",
          "form": "그다음이 볶는 거( 불고기류요)",
          "original_form": "그다음이 볶는 거( 불고기류요)",
          "speaker_id": "1",
          "time": "20210905 21:25"
        },
        {
          "id": "MDRW2100032520.1.12",
          "form": "구워먹는 것도 잘 먹어요..ㅎㅎㅎ",
          "original_form": "구워먹는 것도 잘 먹어요..ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 21:25"
        },
        {
          "id": "MDRW2100032520.1.13",
          "form": "전 구워먹는게 제일 편해서요",
          "original_form": "전 구워먹는게 제일 편해서요",
          "speaker_id": "2",
          "time": "20210905 22:08"
        },
        {
          "id": "MDRW2100032520.1.14",
          "form": "탕은 고기냄새를 잡아줘야 하구",
          "original_form": "탕은 고기냄새를 잡아줘야 하구",
          "speaker_id": "2",
          "time": "20210905 22:08"
        },
        {
          "id": "MDRW2100032520.1.15",
          "form": "볶는것도 양념이 필요하고",
          "original_form": "볶는것도 양념이 필요하고",
          "speaker_id": "2",
          "time": "20210905 22:08"
        },
        {
          "id": "MDRW2100032520.1.16",
          "form": "굽는건 후추와 소금만 있다면....ㅋㅋ",
          "original_form": "굽는건 후추와 소금만 있다면....ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100032520.1.17",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210906 06:44"
        },
        {
          "id": "MDRW2100032520.1.18",
          "form": "듣고보니 구워먹는게 가장 간편하네요",
          "original_form": "듣고보니 구워먹는게 가장 간편하네요",
          "speaker_id": "1",
          "time": "20210906 06:44"
        },
        {
          "id": "MDRW2100032520.1.19",
          "form": "그래도 굽는 실력이 있어서 더 맛있던데",
          "original_form": "그래도 굽는 실력이 있어서 더 맛있던데",
          "speaker_id": "1",
          "time": "20210906 06:44"
        },
        {
          "id": "MDRW2100032520.1.20",
          "form": "고기 잘 굽나요?",
          "original_form": "고기 잘 굽나요?",
          "speaker_id": "1",
          "time": "20210906 06:44"
        },
        {
          "id": "MDRW2100032520.1.21",
          "form": "네  전 잘구워쇼",
          "original_form": "네  전 잘구워쇼",
          "speaker_id": "2",
          "time": "20210906 11:44"
        },
        {
          "id": "MDRW2100032520.1.22",
          "form": "요 ㅎㅎ",
          "original_form": "요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 11:44"
        },
        {
          "id": "MDRW2100032520.1.23",
          "form": "그럼 구워먹는게 좋아할만 하네요",
          "original_form": "그럼 구워먹는게 좋아할만 하네요",
          "speaker_id": "1",
          "time": "20210906 14:52"
        },
        {
          "id": "MDRW2100032520.1.24",
          "form": "맛있게 잘 구우니까요^^",
          "original_form": "맛있게 잘 구우니까요^^",
          "speaker_id": "1",
          "time": "20210906 14:52"
        },
        {
          "id": "MDRW2100032520.1.25",
          "form": "여친은 제가 구워주는거 만족하더라구요 ㅋㅋ",
          "original_form": "여친은 제가 구워주는거 만족하더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:53"
        },
        {
          "id": "MDRW2100032520.1.26",
          "form": "가족들도 그냥 말없이 먹는거 보며ㆍ",
          "original_form": "가족들도 그냥 말없이 먹는거 보며ㆍ",
          "speaker_id": "2",
          "time": "20210906 14:53"
        },
        {
          "id": "MDRW2100032520.1.27",
          "form": "먹을만하게 굽는거 같아요 ㅋㅋ",
          "original_form": "먹을만하게 굽는거 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:53"
        },
        {
          "id": "MDRW2100032520.1.28",
          "form": "큰 재주예요",
          "original_form": "큰 재주예요",
          "speaker_id": "1",
          "time": "20210906 14:59"
        },
        {
          "id": "MDRW2100032520.1.29",
          "form": "고기도 제대로 구워야 맛있는 거라서 아무나 못 굽죠",
          "original_form": "고기도 제대로 구워야 맛있는 거라서 아무나 못 굽죠",
          "speaker_id": "1",
          "time": "20210906 14:59"
        },
        {
          "id": "MDRW2100032520.1.30",
          "form": "하핫 감사합니다",
          "original_form": "하핫 감사합니다",
          "speaker_id": "2",
          "time": "20210906 20:02"
        },
        {
          "id": "MDRW2100032520.1.31",
          "form": "인정해주시다니",
          "original_form": "인정해주시다니",
