      "utterance": [
        {
          "id": "MDRW2100039752.1.1",
          "form": "저희 2차 화이팅 해요 ㅎㅎ",
          "original_form": "저희 2차 화이팅 해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:09"
        },
        {
          "id": "MDRW2100039752.1.2",
          "form": "어떤 날씨 좋아하세요?",
          "original_form": "어떤 날씨 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 21:09"
        },
        {
          "id": "MDRW2100039752.1.3",
          "form": "요즘날씨좋아요",
          "original_form": "요즘날씨좋아요",
          "speaker_id": "1",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100039752.1.4",
          "form": "선선해서",
          "original_form": "선선해서",
          "speaker_id": "1",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100039752.1.5",
          "form": "오 마자요",
          "original_form": "오 마자요",
          "speaker_id": "2",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100039752.1.6",
          "form": "오늘 날씨 너무 좋지 않았나요??",
          "original_form": "오늘 날씨 너무 좋지 않았나요??",
          "speaker_id": "2",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100039752.1.7",
          "form": "구름도 너무 예쁘고..",
          "original_form": "구름도 너무 예쁘고..",
          "speaker_id": "2",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100039752.1.8",
          "form": "일기예보는잘안맞아서좀 그렇지만,,",
          "original_form": "일기예보는잘안맞아서좀 그렇지만,,",
          "speaker_id": "1",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100039752.1.9",
          "form": "ㅋㅋㅋ구라청이라는 말도 있잖아요 ㅋㅋ",
          "original_form": "ㅋㅋㅋ구라청이라는 말도 있잖아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100039752.1.10",
          "form": "그러게요ㅠ",
          "original_form": "그러게요ㅠ",
          "speaker_id": "1",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100039752.1.11",
          "form": "요즘하늘이 정말 이쁘더라고요",
          "original_form": "요즘하늘이 정말 이쁘더라고요",
          "speaker_id": "1",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100039752.1.12",
          "form": "진짜요 ㅠㅠ 깜짝깜짝 놀라요 너무 예뻐서..",
          "original_form": "진짜요 ㅠㅠ 깜짝깜짝 놀라요 너무 예뻐서..",
          "speaker_id": "2",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100039752.1.13",
          "form": "오늘 점심에도 걷는데 진짜 날씨 좋더라구요 ㅎㅎ",
          "original_form": "오늘 점심에도 걷는데 진짜 날씨 좋더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100039752.1.14",
          "form": "이런날이 오래가면좋겠어요",
          "original_form": "이런날이 오래가면좋겠어요",
          "speaker_id": "1",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100039752.1.15",
          "form": "정말요 ㅠㅠ 진짜 날씨 최고예요",
          "original_form": "정말요 ㅠㅠ 진짜 날씨 최고예요",
          "speaker_id": "2",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100039752.1.16",
          "form": "이번 여름 너무 더웠는데",
          "original_form": "이번 여름 너무 더웠는데",
          "speaker_id": "2",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100039752.1.17",
          "form": "이러다갑자기 겨울오겠죠",
          "original_form": "이러다갑자기 겨울오겠죠",
          "speaker_id": "1",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100039752.1.18",
          "form": "허억",
          "original_form": "허억",
          "speaker_id": "2",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100039752.1.19",
          "form": "너무 시간 빠르다 ㅠ",
          "original_form": "너무 시간 빠르다 ㅠ",
          "speaker_id": "2",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100039752.1.20",
          "form": "또엄청추울것같네요",
          "original_form": "또엄청추울것같네요",
          "speaker_id": "1",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100039752.1.21",
          "form": "정말요.. 이번여름 진짜 더웠는데",
          "original_form": "정말요.. 이번여름 진짜 더웠는데",
          "speaker_id": "2",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100039752.1.22",
          "form": "겨울도 역대급으로 춥겠죠..",
          "original_form": "겨울도 역대급으로 춥겠죠..",
          "speaker_id": "2",
          "time": "20210903 21:15"
        },
        {
          "id": "MDRW2100039752.1.23",
          "form": "롱패딩으로 견뎌야죠뭐!",
          "original_form": "롱패딩으로 견뎌야죠뭐!",
          "speaker_id": "1",
          "time": "20210903 21:16"
        }
      ]
    }
