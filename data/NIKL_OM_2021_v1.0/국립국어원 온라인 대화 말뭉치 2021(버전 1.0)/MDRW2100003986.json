        },
        {
          "id": "MDRW2100003986.1.3",
          "form": "????",
          "original_form": "????",
          "speaker_id": "2",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100003986.1.4",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100003986.1.5",
          "form": "어이 아리마셍",
          "original_form": "어이 아리마셍",
          "speaker_id": "2",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100003986.1.6",
          "form": "뭐야 그게ㅋㅋㅋㅋ",
          "original_form": "뭐야 그게ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100003986.1.7",
          "form": "피티 받고싶다...",
          "original_form": "피티 받고싶다...",
          "speaker_id": "1",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100003986.1.8",
          "form": "ㅠ 나 퇴근 루트상",
          "original_form": "ㅠ 나 퇴근 루트상",
          "speaker_id": "2",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100003986.1.9",
          "form": "3호선 라인에 덕천역에서 거제역 사이에",
          "original_form": "3호선 라인에 덕천역에서 거제역 사이에",
          "speaker_id": "2",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100003986.1.10",
          "form": "피티샵 찾고 있는데 안보인다",
          "original_form": "피티샵 찾고 있는데 안보인다",
          "speaker_id": "2",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100003986.1.11",
          "form": "사상에",
          "original_form": "사상에",
          "speaker_id": "1",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100003986.1.12",
          "form": "사상은 위치 좀 그런가?",
          "original_form": "사상은 위치 좀 그런가?",
          "speaker_id": "1",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100003986.1.13",
          "form": "사상 명지 경대 일케 3개 찾았거든..",
          "original_form": "사상 명지 경대 일케 3개 찾았거든..",
          "speaker_id": "1",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100003986.1.14",
          "form": "다 멀다..",
          "original_form": "다 멀다..",
          "speaker_id": "1",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100003986.1.15",
          "form": "엉 사상에 피티 싼데 있긴한데",
          "original_form": "엉 사상에 피티 싼데 있긴한데",
          "speaker_id": "2",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100003986.1.16",
          "form": "그럼 내가 거제역 내려서",
          "original_form": "그럼 내가 거제역 내려서",
          "speaker_id": "2",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100003986.1.17",
          "form": "덕천역 가서 사상역 가야함..",
          "original_form": "덕천역 가서 사상역 가야함..",
          "speaker_id": "2",
          "time": "20210821 14:32"
        },
