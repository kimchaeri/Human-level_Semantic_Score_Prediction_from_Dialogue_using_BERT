      "utterance": [
        {
          "id": "MDRW2100027813.1.1",
          "form": "난 요즘 운동이 절실해",
          "original_form": "난 요즘 운동이 절실해",
          "speaker_id": "1",
          "time": "20210902 15:24"
        },
        {
          "id": "MDRW2100027813.1.2",
          "form": "나 요즘 허벅지 운동 했다가 말고 했다 말고 반복중임 너무 힘들어",
          "original_form": "나 요즘 허벅지 운동 했다가 말고 했다 말고 반복중임 너무 힘들어",
          "speaker_id": "2",
          "time": "20210902 16:05"
        },
        {
          "id": "MDRW2100027813.1.3",
          "form": "진짜 다시 몸의 근육 0로 돌아가서",
          "original_form": "진짜 다시 몸의 근육 0로 돌아가서",
          "speaker_id": "1",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100027813.1.4",
          "form": "그 코어 0인 사람도 할 수 있는 운동부터 해야할듯",
          "original_form": "그 코어 0인 사람도 할 수 있는 운동부터 해야할듯",
          "speaker_id": "1",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100027813.1.5",
          "form": "유산소랑 근력운동 섞어서 하라는데 그럴 체력이 안된다",
          "original_form": "유산소랑 근력운동 섞어서 하라는데 그럴 체력이 안된다",
          "speaker_id": "2",
          "time": "20210902 16:49"
        },
        {
          "id": "MDRW2100027813.1.6",
          "form": "그러니까 그건 어떻게 하는겨",
          "original_form": "그러니까 그건 어떻게 하는겨",
          "speaker_id": "1",
          "time": "20210902 16:53"
        },
        {
          "id": "MDRW2100027813.1.7",
          "form": "나는 당최 이해할 수 없음",
          "original_form": "나는 당최 이해할 수 없음",
          "speaker_id": "1",
          "time": "20210902 16:53"
        },
        {
          "id": "MDRW2100027813.1.8",
          "form": "그냥 허벅지 근력운동 하나만 해도 기운 쏙 빠지는데",
          "original_form": "그냥 허벅지 근력운동 하나만 해도 기운 쏙 빠지는데",
          "speaker_id": "2",
          "time": "20210902 17:14"
        },
        {
          "id": "MDRW2100027813.1.9",
          "form": "땅끄부부 진짜 좋은데 내가 열심히 안하는듯",
          "original_form": "땅끄부부 진짜 좋은데 내가 열심히 안하는듯",
          "speaker_id": "2",
          "time": "20210902 17:15"
        },
        {
          "id": "MDRW2100027813.1.10",
          "form": "나 다이어트 할 때 진짜 열심히했어",
          "original_form": "나 다이어트 할 때 진짜 열심히했어",
          "speaker_id": "1",
          "time": "20210902 17:18"
        },
        {
          "id": "MDRW2100027813.1.11",
          "form": "칼소폭 맞나?",
          "original_form": "칼소폭 맞나?",
          "speaker_id": "1",
          "time": "20210902 17:18"
        },
        {
          "id": "MDRW2100027813.1.12",
          "form": "그거랑 소미핏님 골반교정 스트레칭",
          "original_form": "그거랑 소미핏님 골반교정 스트레칭",
          "speaker_id": "1",
          "time": "20210902 17:18"
        },
        {
          "id": "MDRW2100027813.1.13",
          "form": "나는 허벅지 빼는거",
          "original_form": "나는 허벅지 빼는거",
          "speaker_id": "2",
          "time": "20210902 17:33"
        },
        {
          "id": "MDRW2100027813.1.14",
          "form": "위주로 해봤어",
          "original_form": "위주로 해봤어",
          "speaker_id": "2",
          "time": "20210902 17:33"
        },
        {
          "id": "MDRW2100027813.1.15",
          "form": "칼소폭은 안해봄",
          "original_form": "칼소폭은 안해봄",
          "speaker_id": "2",
          "time": "20210902 17:33"
        },
        {
          "id": "MDRW2100027813.1.16",
          "form": "난 체력이 우선이고 근력운동하기 전에 몸 가볍게 만들려고",
          "original_form": "난 체력이 우선이고 근력운동하기 전에 몸 가볍게 만들려고",
          "speaker_id": "1",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100027813.1.17",
          "form": "칼소폭 열심히했어",
          "original_form": "칼소폭 열심히했어",
          "speaker_id": "1",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100027813.1.18",
          "form": "첨에 1세트만하다가 나중엔 2세트",
          "original_form": "첨에 1세트만하다가 나중엔 2세트",
          "speaker_id": "1",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100027813.1.19",
          "form": "나는 그냥 매일 1번밖에 안함",
          "original_form": "나는 그냥 매일 1번밖에 안함",
          "speaker_id": "2",
          "time": "20210902 17:41"
        },
        {
          "id": "MDRW2100027813.1.20",
          "form": "이러고 빠지냐 하는 마음만 있음",
          "original_form": "이러고 빠지냐 하는 마음만 있음",
          "speaker_id": "2",
          "time": "20210902 17:41"
        },
        {
          "id": "MDRW2100027813.1.21",
          "form": "그래도 근육유지는 하겠지",
          "original_form": "그래도 근육유지는 하겠지",
          "speaker_id": "1",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100027813.1.22",
          "form": "나 이번에 완전 바닥쳤잖아",
          "original_form": "나 이번에 완전 바닥쳤잖아",
          "speaker_id": "1",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100027813.1.23",
          "form": "지금 나도 글러먹은거같아 내일 백신맞으면 운동 또못해",
          "original_form": "지금 나도 글러먹은거같아 내일 백신맞으면 운동 또못해",
          "speaker_id": "2",
          "time": "20210902 17:50"
        },
        {
          "id": "MDRW2100027813.1.24",
          "form": "오늘로 2일 쉬었는데 절망적",
          "original_form": "오늘로 2일 쉬었는데 절망적",
          "speaker_id": "2",
          "time": "20210902 17:50"
        },
        {
          "id": "MDRW2100027813.1.25",
          "form": "내일이야?",
          "original_form": "내일이야?",
          "speaker_id": "1",
          "time": "20210902 17:57"
        },
        {
          "id": "MDRW2100027813.1.26",
          "form": "아냐 그래도 지금까지 운동한게 중요한거지",
          "original_form": "아냐 그래도 지금까지 운동한게 중요한거지",
          "speaker_id": "1",
          "time": "20210902 17:57"
        },
        {
          "id": "MDRW2100027813.1.27",
          "form": "내일 2시에!",
          "original_form": "내일 2시에!",
          "speaker_id": "2",
          "time": "20210902 17:57"
        }
      ]
