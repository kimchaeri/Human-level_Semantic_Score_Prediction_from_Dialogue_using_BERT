      "utterance": [
        {
          "id": "MDRW2100023629.1.1",
          "form": "name2은 일하시는거 어떠세요?",
          "original_form": "&name2&은 일하시는거 어떠세요?",
          "speaker_id": "1",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100023629.1.2",
          "form": "근무환경이 좋아서 만족하면서 잘다니구잇어욤",
          "original_form": "근무환경이 좋아서 만족하면서 잘다니구잇어욤",
          "speaker_id": "2",
          "time": "20210830 13:22"
        },
        {
          "id": "MDRW2100023629.1.3",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 13:22"
        },
        {
          "id": "MDRW2100023629.1.4",
          "form": "부럽네요 전 직장마다 업무폭탄을 맡고있어서요",
          "original_form": "부럽네요 전 직장마다 업무폭탄을 맡고있어서요",
          "speaker_id": "1",
          "time": "20210830 13:24"
        },
        {
          "id": "MDRW2100023629.1.5",
          "form": "아이고ㅜㅜ 일복이 많으시네요",
          "original_form": "아이고ㅜㅜ 일복이 많으시네요",
          "speaker_id": "2",
          "time": "20210830 13:24"
        },
        {
          "id": "MDRW2100023629.1.6",
          "form": "적게일하고 마니벌어야하는데 흑흑",
          "original_form": "적게일하고 마니벌어야하는데 흑흑",
          "speaker_id": "2",
          "time": "20210830 13:24"
        },
        {
          "id": "MDRW2100023629.1.7",
          "form": "그니까요 일많이하고 적게 벌고 있으니",
          "original_form": "그니까요 일많이하고 적게 벌고 있으니",
          "speaker_id": "1",
          "time": "20210830 13:25"
        },
        {
          "id": "MDRW2100023629.1.8",
          "form": "근무환경이 좋다고하시니 부럽네요ㅜㅜ",
          "original_form": "근무환경이 좋다고하시니 부럽네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 13:26"
        },
        {
          "id": "MDRW2100023629.1.9",
          "form": "네 일한지도 오래됏고",
          "original_form": "네 일한지도 오래됏고",
          "speaker_id": "2",
          "time": "20210830 13:26"
        },
        {
          "id": "MDRW2100023629.1.10",
          "form": "성격도 스트레스안받는 스타일이에요",
          "original_form": "성격도 스트레스안받는 스타일이에요",
          "speaker_id": "2",
          "time": "20210830 13:26"
        },
        {
          "id": "MDRW2100023629.1.11",
          "form": "영혼없이 기계처럼 함다 ㅋㅋㅋㅋ",
          "original_form": "영혼없이 기계처럼 함다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:26"
        },
        {
          "id": "MDRW2100023629.1.12",
          "form": "저도 한직장에서 오래일하고싶네요\r\nㅋㅋ 영혼없이일해도 다닐만하시니 다니실테니 부럽습니다ㅋㅋ",
          "original_form": "저도 한직장에서 오래일하고싶네요\r\nㅋㅋ 영혼없이일해도 다닐만하시니 다니실테니 부럽습니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:27"
        },
        {
          "id": "MDRW2100023629.1.13",
          "form": "성격도 부럽네요",
          "original_form": "성격도 부럽네요",
          "speaker_id": "1",
          "time": "20210830 13:27"
        },
        {
          "id": "MDRW2100023629.1.14",
          "form": "능력이되니까 일을 몰아서하는게아닐까요",
          "original_form": "능력이되니까 일을 몰아서하는게아닐까요",
          "speaker_id": "2",
          "time": "20210830 13:29"
        },
        {
          "id": "MDRW2100023629.1.15",
          "form": "인재라서",
          "original_form": "인재라서",
          "speaker_id": "2",
          "time": "20210830 13:29"
        },
        {
          "id": "MDRW2100023629.1.16",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 13:29"
        },
        {
          "id": "MDRW2100023629.1.17",
          "form": "일복이많은ㅜㅜ",
          "original_form": "일복이많은ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 13:29"
        },
        {
          "id": "MDRW2100023629.1.18",
          "form": "그건 아니예요 알아서 찾아서 일해서 그런거같아요\r\n문제가보이니 찾아 일하는 제자신이 싫어요ㅋㅋ",
          "original_form": "그건 아니예요 알아서 찾아서 일해서 그런거같아요\r\n문제가보이니 찾아 일하는 제자신이 싫어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:34"
        },
        {
          "id": "MDRW2100023629.1.19",
          "form": "앗ㅜㅜ 성실하기까지",
          "original_form": "앗ㅜㅜ 성실하기까지",
          "speaker_id": "2",
          "time": "20210830 13:34"
        },
        {
          "id": "MDRW2100023629.1.20",
          "form": "전고인물이라서ㅋㅋㅋㅋㅋ",
          "original_form": "전고인물이라서ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:34"
        },
        {
          "id": "MDRW2100023629.1.21",
          "form": "저도 고인물이 소원이예요 고인물되는것도 쉽지 않아요",
          "original_form": "저도 고인물이 소원이예요 고인물되는것도 쉽지 않아요",
          "speaker_id": "1",
          "time": "20210830 14:06"
        },
        {
          "id": "MDRW2100023629.1.22",
          "form": "힘내세요ㅠㅠ 스트레스 많으시것어요ㅜㅜ",
          "original_form": "힘내세요ㅠㅠ 스트레스 많으시것어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 14:11"
        },
        {
          "id": "MDRW2100023629.1.23",
          "form": "그래서 오늘 퇴사해요ㅋㅋ",
          "original_form": "그래서 오늘 퇴사해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:09"
        },
        {
          "id": "MDRW2100023629.1.24",
          "form": "너무 급이네요",
          "original_form": "너무 급이네요",
          "speaker_id": "1",
          "time": "20210830 15:09"
        },
        {
          "id": "MDRW2100023629.1.25",
          "form": "아이고 고생 많으셨어요 진짜진짜루ㅜㅜㅜ",
