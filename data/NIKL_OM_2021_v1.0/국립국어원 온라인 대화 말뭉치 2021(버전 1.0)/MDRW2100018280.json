      "utterance": [
        {
          "id": "MDRW2100018280.1.1",
          "form": "누구요?",
          "original_form": "누구요?",
          "speaker_id": "2",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100018280.1.2",
          "form": "저 역사학자랑",
          "original_form": "저 역사학자랑",
          "speaker_id": "1",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100018280.1.3",
          "form": "곽재식이요 ㅋㅋㅋㅋ",
          "original_form": "곽재식이요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100018280.1.4",
          "form": "귀엽나요? ㅎㅎㅎㅎ",
          "original_form": "귀엽나요? ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100018280.1.5",
          "form": "네 저 곽재식 넘 귀여워서",
          "original_form": "네 저 곽재식 넘 귀여워서",
          "speaker_id": "1",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100018280.1.6",
          "form": "좋아요 ㅋㅋㅋ",
          "original_form": "좋아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100018280.1.7",
          "form": "그 괴심파괴하시는분?",
          "original_form": "그 괴심파괴하시는분?",
          "speaker_id": "2",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100018280.1.8",
          "form": "네 ㅋㅋㅋ 그 분 당혹사에도 나와요",
          "original_form": "네 ㅋㅋㅋ 그 분 당혹사에도 나와요",
          "speaker_id": "1",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100018280.1.9",
          "form": "그 다빈치노트? 거기도 나오시던데 ㅎㅎ",
          "original_form": "그 다빈치노트? 거기도 나오시던데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100018280.1.10",
          "form": "아 맞아요 ㅋㅋㅋ",
          "original_form": "아 맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100018280.1.11",
          "form": "약간 그런데 특화됐나봐요 ㅋㅋㅋㅋ",
          "original_form": "약간 그런데 특화됐나봐요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100018280.1.12",
          "form": "귀엽다고는 생각 못해봤는데..",
          "original_form": "귀엽다고는 생각 못해봤는데..",
          "speaker_id": "2",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100018280.1.13",
          "form": "ㅋㅋㅋ 제 친구들도 그래요",
          "original_form": "ㅋㅋㅋ 제 친구들도 그래요",
          "speaker_id": "1",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100018280.1.14",
          "form": "제가 좋아하는 스타일이에요",
          "original_form": "제가 좋아하는 스타일이에요",
          "speaker_id": "1",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100018280.1.15",
          "form": "아 그런 취향이시군요 ㅎㅎ",
          "original_form": "아 그런 취향이시군요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:34"
        },
        {
          "id": "MDRW2100018280.1.16",
          "form": "네 ㅋㅋㅋ 넘 귀여우신 ㅎㅎ",
          "original_form": "네 ㅋㅋㅋ 넘 귀여우신 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:34"
        },
        {
          "id": "MDRW2100018280.1.17",
          "form": "심야괴담회 이번편 재밌지 않아요?",
          "original_form": "심야괴담회 이번편 재밌지 않아요?",
          "speaker_id": "2",
          "time": "20210826 23:34"
        },
        {
          "id": "MDRW2100018280.1.18",
          "form": "네 ㅋㅋ 재연에 힘 좀 썼네요",
          "original_form": "네 ㅋㅋ 재연에 힘 좀 썼네요",
          "speaker_id": "1",
          "time": "20210826 23:34"
        },
        {
          "id": "MDRW2100018280.1.19",
          "form": "네 방금꺼 ㅋㅋㅋ",
          "original_form": "네 방금꺼 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:34"
        },
        {
          "id": "MDRW2100018280.1.20",
          "form": "네 저 이진혁도 얘기 잘 하는 것 같아요",
          "original_form": "네 저 이진혁도 얘기 잘 하는 것 같아요",
          "speaker_id": "1",
          "time": "20210826 23:34"
        },
        {
          "id": "MDRW2100018280.1.21",
          "form": "전 지금 공포유튜브 듣고 있어요",
