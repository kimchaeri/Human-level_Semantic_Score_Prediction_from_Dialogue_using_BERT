      "utterance": [
        {
          "id": "MDRW2100019289.1.1",
          "form": "저녁은 뭘 드실 예정인가요?",
          "original_form": "저녁은 뭘 드실 예정인가요?",
          "speaker_id": "1",
          "time": "20210827 16:58"
        },
        {
          "id": "MDRW2100019289.1.2",
          "form": "저녁은 샐러드요",
          "original_form": "저녁은 샐러드요",
          "speaker_id": "2",
          "time": "20210827 17:14"
        },
        {
          "id": "MDRW2100019289.1.3",
          "form": "다이어트",
          "original_form": "다이어트",
          "speaker_id": "2",
          "time": "20210827 17:14"
        },
        {
          "id": "MDRW2100019289.1.4",
          "form": "그런데 아내랑 데이트가 있어서 밖에서 먹을 것 같기도 하네요",
          "original_form": "그런데 아내랑 데이트가 있어서 밖에서 먹을 것 같기도 하네요",
          "speaker_id": "2",
          "time": "20210827 17:14"
        },
        {
          "id": "MDRW2100019289.1.5",
          "form": "카페 자주 다니세요?",
          "original_form": "카페 자주 다니세요?",
          "speaker_id": "2",
          "time": "20210827 17:15"
        },
        {
          "id": "MDRW2100019289.1.6",
          "form": "금요일이라 특식 먹고싶은날이네요!",
          "original_form": "금요일이라 특식 먹고싶은날이네요!",
          "speaker_id": "1",
          "time": "20210827 17:15"
        },
        {
          "id": "MDRW2100019289.1.7",
          "form": "요즘은 개인 카페로 다녀요. 프랜차이즈보단 사람이 적은것 같아서요.",
          "original_form": "요즘은 개인 카페로 다녀요. 프랜차이즈보단 사람이 적은것 같아서요.",
          "speaker_id": "1",
          "time": "20210827 17:15"
        },
        {
          "id": "MDRW2100019289.1.8",
          "form": "커피 좋아하시나요?",
          "original_form": "커피 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 17:15"
        },
        {
          "id": "MDRW2100019289.1.9",
          "form": "하루에 한잔씩 마시죠",
          "original_form": "하루에 한잔씩 마시죠",
          "speaker_id": "2",
          "time": "20210827 17:31"
        },
        {
          "id": "MDRW2100019289.1.10",
          "form": "저는 바다 근처에 살아서",
          "original_form": "저는 바다 근처에 살아서",
          "speaker_id": "2",
          "time": "20210827 17:31"
        },
        {
          "id": "MDRW2100019289.1.11",
          "form": "주변에 예쁜카페가 많은데",
          "original_form": "주변에 예쁜카페가 많은데",
          "speaker_id": "2",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019289.1.12",
          "form": "그래도 스타벅스를 제일 자주 가요",
          "original_form": "그래도 스타벅스를 제일 자주 가요",
          "speaker_id": "2",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019289.1.13",
          "form": "오션뷰 카페가 많은가봐요!",
          "original_form": "오션뷰 카페가 많은가봐요!",
          "speaker_id": "1",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019289.1.14",
          "form": "아메리카노를 드시나요?",
          "original_form": "아메리카노를 드시나요?",
          "speaker_id": "1",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100019289.1.15",
          "form": "네네 아메리카노 마셔요",
          "original_form": "네네 아메리카노 마셔요",
          "speaker_id": "2",
          "time": "20210827 17:34"
        },
        {
          "id": "MDRW2100019289.1.16",
          "form": "단 음료는 안좋아하시나요?",
          "original_form": "단 음료는 안좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 17:36"
        },
        {
          "id": "MDRW2100019289.1.17",
          "form": "좋아하는데 이제 단거는 줄이려고요",
          "original_form": "좋아하는데 이제 단거는 줄이려고요",
          "speaker_id": "2",
          "time": "20210827 17:42"
        },
        {
          "id": "MDRW2100019289.1.18",
          "form": "맛집은 자주 다니세요",
          "original_form": "맛집은 자주 다니세요",
          "speaker_id": "2",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100019289.1.19",
          "form": "네! 좋아합니다 name2님도 자주 다니시나요?",
          "original_form": "네! 좋아합니다 &name2&님도 자주 다니시나요?",
          "speaker_id": "1",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019289.1.20",
          "form": "가끔 외식하러가면 찾아보가 가는 편이에요",
          "original_form": "가끔 외식하러가면 찾아보가 가는 편이에요",
          "speaker_id": "2",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019289.1.21",
          "form": "그런데 너무 웨이팅길면 쉽게 포기해요",
          "original_form": "그런데 너무 웨이팅길면 쉽게 포기해요",
          "speaker_id": "2",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100019289.1.22",
          "form": "저두요! 몇시간 줄서서 기다리고 그런건 못하겠어요",
          "original_form": "저두요! 몇시간 줄서서 기다리고 그런건 못하겠어요",
          "speaker_id": "1",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100019289.1.23",
          "form": "네 그럴땐 그냥 주변에 다른 식당으로 가요",
          "original_form": "네 그럴땐 그냥 주변에 다른 식당으로 가요",
          "speaker_id": "2",
          "time": "20210827 17:46"
        },
        {
          "id": "MDRW2100019289.1.24",
          "form": "가장 인상깊었던 식당은?",
          "original_form": "가장 인상깊었던 식당은?",
          "speaker_id": "2",
          "time": "20210827 17:46"
        },
        {
          "id": "MDRW2100019289.1.25",
          "form": "음 최근에 주인분 내외가 하시는 중식당에 갔는데 시간도 하루 4시간 정도 하시고 좀 불친절한데 사람이 엄청 많더라구요. 맛있으니 불친절해도 인기가 많은가봐요 ㅋㅋ",
          "original_form": "음 최근에 주인분 내외가 하시는 중식당에 갔는데 시간도 하루 4시간 정도 하시고 좀 불친절한데 사람이 엄청 많더라구요. 맛있으니 불친절해도 인기가 많은가봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:51"
        }
      ]
