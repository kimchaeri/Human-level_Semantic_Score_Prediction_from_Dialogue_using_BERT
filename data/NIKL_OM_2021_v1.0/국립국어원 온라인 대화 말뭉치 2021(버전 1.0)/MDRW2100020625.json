      "utterance": [
        {
          "id": "MDRW2100020625.1.1",
          "form": "여행 가고 싶은 곳 있으신가요?",
          "original_form": "여행 가고 싶은 곳 있으신가요?",
          "speaker_id": "1",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020625.1.2",
          "form": "대만여행 가보고 싶어요",
          "original_form": "대만여행 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020625.1.3",
          "form": "우와 대만 좋죠. 거기는 뭐가 유명한가요?",
          "original_form": "우와 대만 좋죠. 거기는 뭐가 유명한가요?",
          "speaker_id": "1",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020625.1.4",
          "form": "온천이요. 바캉스 여행으로 인기 있더라구요.",
          "original_form": "온천이요. 바캉스 여행으로 인기 있더라구요.",
          "speaker_id": "2",
          "time": "20210828 10:38"
        },
        {
          "id": "MDRW2100020625.1.5",
          "form": "우와 온천이 유명하구나.. 대만은 동성결혼 합법화 된게 이슈가 됐어서 기억나네요",
          "original_form": "우와 온천이 유명하구나.. 대만은 동성결혼 합법화 된게 이슈가 됐어서 기억나네요",
          "speaker_id": "1",
          "time": "20210828 10:40"
        },
        {
          "id": "MDRW2100020625.1.6",
          "form": "한번 가 보고 싶네요 대만",
          "original_form": "한번 가 보고 싶네요 대만",
          "speaker_id": "1",
          "time": "20210828 10:40"
        },
        {
          "id": "MDRW2100020625.1.7",
          "form": "저는 포루투갈 가 보고 싶네요",
          "original_form": "저는 포루투갈 가 보고 싶네요",
          "speaker_id": "1",
          "time": "20210828 10:40"
        },
        {
          "id": "MDRW2100020625.1.8",
          "form": "좋을거 같아요",
          "original_form": "좋을거 같아요",
          "speaker_id": "2",
          "time": "20210828 10:40"
        },
        {
          "id": "MDRW2100020625.1.9",
          "form": "가면 뭐 해보고 싶어요?",
          "original_form": "가면 뭐 해보고 싶어요?",
          "speaker_id": "2",
          "time": "20210828 10:40"
        },
        {
          "id": "MDRW2100020625.1.10",
          "form": "사실 저 나라를 잘 몰라요..ㅋㅋ 여행 유튜버들이 추천하는 곳이라 가고 싶었어요. 가면 에그타르트 먹어보고 싶네요!\n경치 좋은 곳도 구경하고",
          "original_form": "사실 저 나라를 잘 몰라요..ㅋㅋ 여행 유튜버들이 추천하는 곳이라 가고 싶었어요. 가면 에그타르트 먹어보고 싶네요!\n경치 좋은 곳도 구경하고",
          "speaker_id": "1",
          "time": "20210828 10:42"
        },
        {
          "id": "MDRW2100020625.1.11",
          "form": "ㅋㅋㅋ다 비슷한 거 같아요",
          "original_form": "ㅋㅋㅋ다 비슷한 거 같아요",
          "speaker_id": "2",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020625.1.12",
          "form": "일단 어딘가 가고 싶어요",
          "original_form": "일단 어딘가 가고 싶어요",
          "speaker_id": "2",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020625.1.13",
          "form": "그쵸? ㅋㅋ 맛있는거 먹고 좋은 곳 구경하고",
          "original_form": "그쵸? ㅋㅋ 맛있는거 먹고 좋은 곳 구경하고",
          "speaker_id": "1",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020625.1.14",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "1",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020625.1.15",
          "form": "국내 여행 다니셨나요? 기억에 남는 장소라던지",
          "original_form": "국내 여행 다니셨나요? 기억에 남는 장소라던지",
          "speaker_id": "1",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020625.1.16",
          "form": "강릉이요",
          "original_form": "강릉이요",
          "speaker_id": "2",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100020625.1.17",
          "form": "안목해변 좋더라구요",
          "original_form": "안목해변 좋더라구요",
          "speaker_id": "2",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100020625.1.18",
          "form": "아 이 분 뭘 아시는 분이네요",
          "original_form": "아 이 분 뭘 아시는 분이네요",
          "speaker_id": "1",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020625.1.19",
          "form": "저도 바다 중에 제일 좋아하는 곳이 강릉이에요",
          "original_form": "저도 바다 중에 제일 좋아하는 곳이 강릉이에요",
          "speaker_id": "1",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020625.1.20",
          "form": "강릉 진짜 탁 트이고 이쁜 것 같아요 바다가",
          "original_form": "강릉 진짜 탁 트이고 이쁜 것 같아요 바다가",
          "speaker_id": "1",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020625.1.21",
          "form": "맞아요. 시간대별로 다르게 이쁘고요",
          "original_form": "맞아요. 시간대별로 다르게 이쁘고요",
          "speaker_id": "2",
          "time": "20210828 10:47"
        },
        {
          "id": "MDRW2100020625.1.22",
          "form": "들어가는건 못해서 가면 해변산책만 자주 해요ㅋㅋ",
          "original_form": "들어가는건 못해서 가면 해변산책만 자주 해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:49"
        },
        {
          "id": "MDRW2100020625.1.23",
          "form": "아 너무 좋죠.. 해 질 때 그렇게 이쁜 곳은 처음 봤어요",
          "original_form": "아 너무 좋죠.. 해 질 때 그렇게 이쁜 곳은 처음 봤어요",
          "speaker_id": "1",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020625.1.24",
          "form": "안목해변도 좋은데 사람 많지 않나요?",
          "original_form": "안목해변도 좋은데 사람 많지 않나요?",
          "speaker_id": "1",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020625.1.25",
          "form": "경포대 쪽에서 좀 끝으로 가시면 사람 진짜 없어요!",
          "original_form": "경포대 쪽에서 좀 끝으로 가시면 사람 진짜 없어요!",
          "speaker_id": "1",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020625.1.26",
          "form": "다음에 가게 되면 거기로 가봐야겠어요ㅋㅋ",
          "original_form": "다음에 가게 되면 거기로 가봐야겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:57"
        },
        {
          "id": "MDRW2100020625.1.27",
          "form": "안목해변은 비수기때도 사람 꽤 있더라구요",
          "original_form": "안목해변은 비수기때도 사람 꽤 있더라구요",
          "speaker_id": "2",
          "time": "20210828 10:57"
        },
        {
          "id": "MDRW2100020625.1.28",
          "form": "네 진짜 추천합니다. 바다를 찍을 때 사람 머리가 안나온다구요!!",
          "original_form": "네 진짜 추천합니다. 바다를 찍을 때 사람 머리가 안나온다구요!!",
          "speaker_id": "1",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020625.1.29",
          "form": "그쵸 워낙 유명해서 항상 있더라고요 그 쪽은",
          "original_form": "그쵸 워낙 유명해서 항상 있더라고요 그 쪽은",
          "speaker_id": "1",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020625.1.30",
          "form": "강릉 시장에서 순대국밥 먹었었는데 맛있더라고요",
          "original_form": "강릉 시장에서 순대국밥 먹었었는데 맛있더라고요",
          "speaker_id": "1",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020625.1.31",
          "form": "보통 뭐 드시나요 강릉오시면?",
          "original_form": "보통 뭐 드시나요 강릉오시면?",
          "speaker_id": "1",
          "time": "20210828 11:02"
        },
        {
          "id": "MDRW2100020625.1.32",
          "form": "해변근처에서 해물 주로 먹어요.",
          "original_form": "해변근처에서 해물 주로 먹어요.",
          "speaker_id": "2",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100020625.1.33",
          "form": "아니면 순두부? 유명하잖아요",
          "original_form": "아니면 순두부? 유명하잖아요",
          "speaker_id": "2",
          "time": "20210828 11:06"
        }
      ]
    }
  ]
