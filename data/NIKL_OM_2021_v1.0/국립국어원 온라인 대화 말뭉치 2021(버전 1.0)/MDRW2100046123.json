        {
          "id": "MDRW2100046123.1.1",
          "form": "치료를 해야 하지 않을까 ?",
          "original_form": "치료를 해야 하지 않을까 ?",
          "speaker_id": "2",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100046123.1.2",
          "form": "어떻게 하지ㅠ?",
          "original_form": "어떻게 하지ㅠ?",
          "speaker_id": "1",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100046123.1.3",
          "form": "피부과를 가볼까 ?",
          "original_form": "피부과를 가볼까 ?",
          "speaker_id": "2",
          "time": "20210908 10:35"
        },
        {
          "id": "MDRW2100046123.1.4",
          "form": "피부과? 피부과 가면 돈 많이 들지 않으려나ㅠㅠ",
          "original_form": "피부과? 피부과 가면 돈 많이 들지 않으려나ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:36"
        },
        {
          "id": "MDRW2100046123.1.5",
          "form": "돈 많이 들수도 있지",
          "original_form": "돈 많이 들수도 있지",
          "speaker_id": "2",
          "time": "20210908 10:39"
        },
        {
          "id": "MDRW2100046123.1.6",
          "form": "ㅠㅠ 그럼 뭔가 집에서 해결 하고 싶긴해",
          "original_form": "ㅠㅠ 그럼 뭔가 집에서 해결 하고 싶긴해",
          "speaker_id": "1",
          "time": "20210908 10:39"
        },
        {
          "id": "MDRW2100046123.1.7",
          "form": "좋은 약같은거로 어떻게 안되려나",
          "original_form": "좋은 약같은거로 어떻게 안되려나",
          "speaker_id": "1",
          "time": "20210908 10:39"
        },
        {
          "id": "MDRW2100046123.1.8",
          "form": "글쎄 모르겠네",
          "original_form": "글쎄 모르겠네",
          "speaker_id": "2",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046123.1.9",
          "form": "어렵다 진짜",
          "original_form": "어렵다 진짜",
          "speaker_id": "1",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046123.1.10",
          "form": "나요새 혓바늘도 나구ㅠㅠ",
          "original_form": "나요새 혓바늘도 나구ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046123.1.11",
          "form": "건강이 정말 말이아니야",
          "original_form": "건강이 정말 말이아니야",
          "speaker_id": "1",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100046123.1.12",
          "form": "고농축 비타민 먹어",
          "original_form": "고농축 비타민 먹어",
          "speaker_id": "2",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100046123.1.13",
          "form": "앗 그래야겠당 비타민 기억해둬야지",
          "original_form": "앗 그래야겠당 비타민 기억해둬야지",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100046123.1.14",
          "form": "꼭 챙겨먹어",
          "original_form": "꼭 챙겨먹어",
          "speaker_id": "2",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100046123.1.15",
          "form": "알겠어!!!",
          "original_form": "알겠어!!!",
          "speaker_id": "1",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100046123.1.16",
          "form": "엄마도 비타민 같은거 챙겨먹어",
          "original_form": "엄마도 비타민 같은거 챙겨먹어",
