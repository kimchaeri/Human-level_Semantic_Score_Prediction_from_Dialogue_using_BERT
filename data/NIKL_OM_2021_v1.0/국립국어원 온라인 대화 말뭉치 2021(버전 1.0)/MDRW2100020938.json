        },
        {
          "id": "MDRW2100020938.1.2",
          "form": "안녕하세요~ 공연 좋아하세요?",
          "original_form": "안녕하세요~ 공연 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100020938.1.3",
          "form": "공연좋죠",
          "original_form": "공연좋죠",
          "speaker_id": "1",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020938.1.4",
          "form": "name2님은 공연좋아하세요?",
          "original_form": "&name2&님은 공연좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020938.1.5",
          "form": "저도 좋아해요",
          "original_form": "저도 좋아해요",
          "speaker_id": "2",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100020938.1.6",
          "form": "어떤 공연 좋아하세요?",
          "original_form": "어떤 공연 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100020938.1.7",
          "form": "저는 뮤지컬이요",
          "original_form": "저는 뮤지컬이요",
          "speaker_id": "1",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100020938.1.8",
          "form": "공연을 많이 안 봐서ㅋㅋㅋㅋㅋㅋ",
          "original_form": "공연을 많이 안 봐서ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100020938.1.9",
          "form": "근데 잔잔한거는 별로라",
          "original_form": "근데 잔잔한거는 별로라",
          "speaker_id": "1",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100020938.1.10",
          "form": "시끌벅적한게 좋더라구요",
          "original_form": "시끌벅적한게 좋더라구요",
          "speaker_id": "1",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100020938.1.11",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100020938.1.12",
          "form": "저는 콘서트 좋아해요 ㅎㅎ",
          "original_form": "저는 콘서트 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100020938.1.13",
          "form": "저도 조용한거 안좋아해요!",
          "original_form": "저도 조용한거 안좋아해요!",
          "speaker_id": "2",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100020938.1.14",
          "form": "오 누구 콘서트 가보셨어요?",
          "original_form": "오 누구 콘서트 가보셨어요?",
          "speaker_id": "1",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100020938.1.15",
          "form": "저 방탄 콘서트 가봤어요!",
          "original_form": "저 방탄 콘서트 가봤어요!",
          "speaker_id": "2",
          "time": "20210828 14:37"
        },
        {
          "id": "MDRW2100020938.1.16",
          "form": "티켓팅 완전 빡세지않아요??",
          "original_form": "티켓팅 완전 빡세지않아요??",
          "speaker_id": "1",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020938.1.17",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "1",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020938.1.18",
          "form": "빡셌는데 ㅋㅋ 방탄이 티켓팅 실력을 키워주더라구요 ..",
          "original_form": "빡셌는데 ㅋㅋ 방탄이 티켓팅 실력을 키워주더라구요 ..",
          "speaker_id": "2",
          "time": "20210828 14:40"
        },
        {
          "id": "MDRW2100020938.1.19",
          "form": "앜ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100020938.1.20",
          "form": "콘서트 어때요??",
          "original_form": "콘서트 어때요??",
          "speaker_id": "1",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100020938.1.21",
          "form": "저는 콘서트는 안 가봐서",
          "original_form": "저는 콘서트는 안 가봐서",
          "speaker_id": "1",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100020938.1.22",
          "form": "너무 재밌을거같아요",
          "original_form": "너무 재밌을거같아요",
          "speaker_id": "1",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100020938.1.23",
          "form": "요즘은 못가지만 ㅜㅜ 엄청 재밌어요 !",
          "original_form": "요즘은 못가지만 ㅜㅜ 엄청 재밌어요 !",
          "speaker_id": "2",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100020938.1.24",
          "form": "꼭 라이브 하는 가수로 가보셨으면 ㅎㅎ 좋겠어요",
          "original_form": "꼭 라이브 하는 가수로 가보셨으면 ㅎㅎ 좋겠어요",
          "speaker_id": "2",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100020938.1.25",
          "form": "저는 아이유 콘서트 가보고싶더라구요",
          "original_form": "저는 아이유 콘서트 가보고싶더라구요",
          "speaker_id": "1",
          "time": "20210828 14:49"
        },
        {
          "id": "MDRW2100020938.1.26",
          "form": "영상으로만 봤는데 너무 잘불러서",
          "original_form": "영상으로만 봤는데 너무 잘불러서",
          "speaker_id": "1",
          "time": "20210828 14:49"
        },
        {
          "id": "MDRW2100020938.1.27",
          "form": "직접들어보고싶더라구요",
          "original_form": "직접들어보고싶더라구요",
          "speaker_id": "1",
          "time": "20210828 14:49"
        }
      ]
