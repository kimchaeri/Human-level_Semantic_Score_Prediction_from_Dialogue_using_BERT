      "utterance": [
        {
          "id": "MDRW2100006402.1.1",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100006402.1.2",
          "form": "아니\n애초에",
          "original_form": "아니\n애초에",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100006402.1.3",
          "form": "읽어야만해",
          "original_form": "읽어야만해",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100006402.1.4",
          "form": "아니 오ㅑㅋㅋㅋㅋ",
          "original_form": "아니 오ㅑㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100006402.1.5",
          "form": "이거 집착이라구요 ㅋㅋㅋ",
          "original_form": "이거 집착이라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100006402.1.6",
          "form": "머라는거야 ㅋㅋㅋ 난 거기다 얹어서 지금 다른거도 읽어재끼고 있는걸",
          "original_form": "머라는거야 ㅋㅋㅋ 난 거기다 얹어서 지금 다른거도 읽어재끼고 있는걸",
          "speaker_id": "1",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100006402.1.7",
          "form": "아유 대단하신분이야 증말",
          "original_form": "아유 대단하신분이야 증말",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100006402.1.8",
          "form": "어휴 제가 뭘요",
          "original_form": "어휴 제가 뭘요",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100006402.1.9",
          "form": "실제 볼륨은 니가 더 많이 읽을걸?",
          "original_form": "실제 볼륨은 니가 더 많이 읽을걸?",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100006402.1.10",
          "form": "아닐걸..?",
          "original_form": "아닐걸..?",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100006402.1.11",
          "form": "무슨 그런소리를 해요",
          "original_form": "무슨 그런소리를 해요",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100006402.1.12",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100006402.1.13",
          "form": "진짠데..",
          "original_form": "진짠데..",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100006402.1.14",
          "form": "니가 더 읽을걸?",
          "original_form": "니가 더 읽을걸?",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100006402.1.15",
          "form": "너 읽는 속도는 내가 어떻게 해도 못 따라가는걸..",
          "original_form": "너 읽는 속도는 내가 어떻게 해도 못 따라가는걸..",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100006402.1.16",
          "form": "같은 시간을 써도 더 많이 읽으시는걸ㅋㅋㅋ",
          "original_form": "같은 시간을 써도 더 많이 읽으시는걸ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100006402.1.17",
          "form": "너도 느리지 않잖앜ㅋㅋㅋ",
          "original_form": "너도 느리지 않잖앜ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100006402.1.18",
          "form": "나는 내용이 너보다 더 얕게 읽을 뿐이야",
          "original_form": "나는 내용이 너보다 더 얕게 읽을 뿐이야",
          "speaker_id": "1",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100006402.1.19",
          "form": "에이 ㅋㅋㅋㅋ",
          "original_form": "에이 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100006402.1.20",
          "form": "진짠데.. 얘가 안믿네 ㅋㅋㅋ",
          "original_form": "진짠데.. 얘가 안믿네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100006402.1.21",
          "form": "아냐 그래도 일단 많이 읽는게 더 나아보여 ㅋㅋㅋㅋ",
          "original_form": "아냐 그래도 일단 많이 읽는게 더 나아보여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100006402.1.22",
          "form": "양보단 질 아닌가요",
          "original_form": "양보단 질 아닌가요",
          "speaker_id": "1",
          "time": "20210908 00:11"
