        },
        {
          "id": "MDRW2100024301.1.3",
          "form": "요즘 약간 지루해요",
          "original_form": "요즘 약간 지루해요",
          "speaker_id": "2",
          "time": "20210831 00:24"
        },
        {
          "id": "MDRW2100024301.1.4",
          "form": "zzzz 저도 그냥저냥 보는중 ㅋㅋ",
          "original_form": "zzzz 저도 그냥저냥 보는중 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:25"
        },
        {
          "id": "MDRW2100024301.1.5",
          "form": "어떤 캐릭이 젤 좋아요?",
          "original_form": "어떤 캐릭이 젤 좋아요?",
          "speaker_id": "1",
          "time": "20210831 00:25"
        },
        {
          "id": "MDRW2100024301.1.6",
          "form": "그나마 이익준?",
          "original_form": "그나마 이익준?",
          "speaker_id": "2",
          "time": "20210831 00:26"
        },
        {
          "id": "MDRW2100024301.1.7",
          "form": "조정석 배우 연기하는거 좋아해서요ㅋㅋㅋ",
          "original_form": "조정석 배우 연기하는거 좋아해서요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:26"
        },
        {
          "id": "MDRW2100024301.1.8",
          "form": "ㅋㅋㅋㅋㅋ 조정석은 진짜 만능 인것 같아요",
          "original_form": "ㅋㅋㅋㅋㅋ 조정석은 진짜 만능 인것 같아요",
          "speaker_id": "1",
          "time": "20210831 00:26"
        },
        {
          "id": "MDRW2100024301.1.9",
          "form": "조정석 나온 드라마 중에",
          "original_form": "조정석 나온 드라마 중에",
          "speaker_id": "2",
          "time": "20210831 00:27"
        },
        {
          "id": "MDRW2100024301.1.10",
          "form": "질투의 화신 진짜 좋아해요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "질투의 화신 진짜 좋아해요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:28"
        },
        {
          "id": "MDRW2100024301.1.11",
          "form": "그냥 조정석이 그 자체일거같은",
          "original_form": "그냥 조정석이 그 자체일거같은",
          "speaker_id": "2",
          "time": "20210831 00:28"
        },
        {
          "id": "MDRW2100024301.1.12",
          "form": "헐.....저 그거 블루레이샀음",
          "original_form": "헐.....저 그거 블루레이샀음",
          "speaker_id": "1",
          "time": "20210831 00:28"
        },
        {
          "id": "MDRW2100024301.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:28"
        },
        {
          "id": "MDRW2100024301.1.14",
          "form": "진짜 첨으로 담화 할때까지 복습한 드라마...",
          "original_form": "진짜 첨으로 담화 할때까지 복습한 드라마...",
          "speaker_id": "1",
          "time": "20210831 00:29"
        },
        {
          "id": "MDRW2100024301.1.15",
          "form": "헐 저도요",
          "original_form": "헐 저도요",
          "speaker_id": "2",
          "time": "20210831 00:31"
        },
        {
          "id": "MDRW2100024301.1.16",
          "form": "블레 샀어요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "블레 샀어요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:31"
        },
        {
          "id": "MDRW2100024301.1.17",
          "form": "근데 블플없어서 못봤어요ㅋㅋㅋㅋㅋ먼지만 쌓이는 중",
          "original_form": "근데 블플없어서 못봤어요ㅋㅋㅋㅋㅋ먼지만 쌓이는 중",
          "speaker_id": "2",
          "time": "20210831 00:31"
        },
        {
          "id": "MDRW2100024301.1.18",
          "form": "헐 ㅋㅋㅋㅋㅋㅋㅋ 통했네요 !!",
          "original_form": "헐 ㅋㅋㅋㅋㅋㅋㅋ 통했네요 !!",
          "speaker_id": "1",
          "time": "20210831 00:32"
        },
        {
          "id": "MDRW2100024301.1.19",
          "form": "ㅋㅋㅋㅋ 첨으로 블레카페 가입하고 ....ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋ 첨으로 블레카페 가입하고 ....ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 00:32"
        },
        {
          "id": "MDRW2100024301.1.20",
          "form": "조만간 함 다시 돌려야겟네요 근데귀찮아서 블레로 안보고 넷플같은걸로볼듯 ㅋㅋㅋㅋ",
          "original_form": "조만간 함 다시 돌려야겟네요 근데귀찮아서 블레로 안보고 넷플같은걸로볼듯 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:33"
        },
        {
          "id": "MDRW2100024301.1.21",
          "form": "블플 있으세요?ㅜㅜㅜㅜ",
