      "utterance": [
        {
          "id": "MDRW2100027117.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100027117.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100027117.1.3",
          "form": "키워본적도없어요",
          "original_form": "키워본적도없어요",
          "speaker_id": "1",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100027117.1.4",
          "form": "그러면 키워보고 싶은 친구는 있었나요?",
          "original_form": "그러면 키워보고 싶은 친구는 있었나요?",
          "speaker_id": "2",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100027117.1.5",
          "form": "글쎄요. 사실 동물 별로안좋아해요ㅠ",
          "original_form": "글쎄요. 사실 동물 별로안좋아해요ㅠ",
          "speaker_id": "1",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100027117.1.6",
          "form": "아 동물을 안좋아하시는군요",
          "original_form": "아 동물을 안좋아하시는군요",
          "speaker_id": "2",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100027117.1.7",
          "form": "왜 싫어하는지 여쭤봐도 될까요?",
          "original_form": "왜 싫어하는지 여쭤봐도 될까요?",
          "speaker_id": "2",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100027117.1.8",
          "form": "글쎄요...이게 참 어려워요",
          "original_form": "글쎄요...이게 참 어려워요",
          "speaker_id": "1",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100027117.1.9",
          "form": "왜냐면 대게 고양이 강아지 귀엽다고 리액션하는데",
          "original_form": "왜냐면 대게 고양이 강아지 귀엽다고 리액션하는데",
          "speaker_id": "1",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100027117.1.10",
          "form": "저는 진짜...뭐라고해야할지",
          "original_form": "저는 진짜...뭐라고해야할지",
          "speaker_id": "1",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100027117.1.11",
          "form": "name1에게 안예쁘게 보일수도 있는거죠",
          "original_form": "&name1&에게 안예쁘게 보일수도 있는거죠",
          "speaker_id": "2",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100027117.1.12",
          "form": "근데 진짜 동물안좋아하는사람 찾기힘들어요",
          "original_form": "근데 진짜 동물안좋아하는사람 찾기힘들어요",
          "speaker_id": "1",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100027117.1.13",
          "form": "어릴적에도 접할기회도 없었고",
          "original_form": "어릴적에도 접할기회도 없었고",
          "speaker_id": "1",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100027117.1.14",
          "form": "무서워했어요",
          "original_form": "무서워했어요",
          "speaker_id": "1",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100027117.1.15",
          "form": "토키랑 햄스터도 귀엽다기보다 무서워요",
          "original_form": "토키랑 햄스터도 귀엽다기보다 무서워요",
          "speaker_id": "1",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100027117.1.16",
          "form": "털있는동물은무서워요",
          "original_form": "털있는동물은무서워요",
          "speaker_id": "1",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100027117.1.17",
          "form": "징그럽다고해야하나ㅠ 닭도여",
          "original_form": "징그럽다고해야하나ㅠ 닭도여",
          "speaker_id": "1",
          "time": "20210902 22:55"
        },
        {
          "id": "MDRW2100027117.1.18",
          "form": "전부 싫어하시는군요ㅠ",
          "original_form": "전부 싫어하시는군요ㅠ",
          "speaker_id": "2",
          "time": "20210902 22:56"
        },
        {
          "id": "MDRW2100027117.1.19",
          "form": "그럼 물고기는요?",
          "original_form": "그럼 물고기는요?",
          "speaker_id": "2",
          "time": "20210902 22:56"
        },
        {
          "id": "MDRW2100027117.1.20",
          "form": "ㅋㅋㅋ구피 키워는봤어요",
          "original_form": "ㅋㅋㅋ구피 키워는봤어요",
          "speaker_id": "1",
          "time": "20210902 22:56"
        },
        {
          "id": "MDRW2100027117.1.21",
          "form": "차라리 곤충은 안무서워요",
          "original_form": "차라리 곤충은 안무서워요",
          "speaker_id": "1",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100027117.1.22",
          "form": "벌레도 안징그러워요",
          "original_form": "벌레도 안징그러워요",
          "speaker_id": "1",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100027117.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100027117.1.24",
          "form": "그래서 저는 오히려 벌레무서워하는 사람들이 신기해요",
          "original_form": "그래서 저는 오히려 벌레무서워하는 사람들이 신기해요",
          "speaker_id": "1",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100027117.1.25",
          "form": "와 신기하네요",
          "original_form": "와 신기하네요",
          "speaker_id": "2",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100027117.1.26",
          "form": "저랑 정반대세요",
          "original_form": "저랑 정반대세요",
          "speaker_id": "2",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100027117.1.27",
          "form": "저는 곤충을 못잡아서 고생이에요",
          "original_form": "저는 곤충을 못잡아서 고생이에요",
          "speaker_id": "2",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100027117.1.28",
          "form": "아 진짜요? 서로 신기한 상황이네요",
          "original_form": "아 진짜요? 서로 신기한 상황이네요",
          "speaker_id": "1",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100027117.1.29",
          "form": "저는 심지어 자다가 벌레가 보여요",
          "original_form": "저는 심지어 자다가 벌레가 보여요",
          "speaker_id": "1",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100027117.1.30",
          "form": "근데 잡기 귀찮을때",
          "original_form": "근데 잡기 귀찮을때",
          "speaker_id": "1",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100027117.1.31",
