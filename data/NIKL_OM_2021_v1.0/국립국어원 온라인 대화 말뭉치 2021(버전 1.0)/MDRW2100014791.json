      "utterance": [
        {
          "id": "MDRW2100014791.1.1",
          "form": "아 저번에 전시회보러갔을때",
          "original_form": "아 저번에 전시회보러갔을때",
          "speaker_id": "1",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014791.1.2",
          "form": "친구가 휴학을 한다거 그러던데",
          "original_form": "친구가 휴학을 한다거 그러던데",
          "speaker_id": "1",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014791.1.3",
          "form": "왜??",
          "original_form": "왜??",
          "speaker_id": "2",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014791.1.4",
          "form": "그래서 내가 휴학하면 졸업일이 밀려서",
          "original_form": "그래서 내가 휴학하면 졸업일이 밀려서",
          "speaker_id": "1",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014791.1.5",
          "form": "휴학 안하는게 더 낫지않냐",
          "original_form": "휴학 안하는게 더 낫지않냐",
          "speaker_id": "1",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014791.1.6",
          "form": "그랬더니",
          "original_form": "그랬더니",
          "speaker_id": "1",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014791.1.7",
          "form": "휴학하고 일단 작은기업 지원해보면서",
          "original_form": "휴학하고 일단 작은기업 지원해보면서",
          "speaker_id": "1",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014791.1.8",
          "form": "좀더 기술도 쌓고 그러고 싶나봐",
          "original_form": "좀더 기술도 쌓고 그러고 싶나봐",
          "speaker_id": "1",
          "time": "20210825 20:45"
        },
        {
          "id": "MDRW2100014791.1.9",
          "form": "말듣고보니 괜찮은거같아서",
          "original_form": "말듣고보니 괜찮은거같아서",
          "speaker_id": "1",
          "time": "20210825 20:45"
        },
        {
          "id": "MDRW2100014791.1.10",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210825 20:45"
        },
        {
          "id": "MDRW2100014791.1.11",
          "form": "그러라고 했어",
          "original_form": "그러라고 했어",
          "speaker_id": "1",
          "time": "20210825 20:45"
        },
        {
          "id": "MDRW2100014791.1.12",
          "form": "약간 인턴같은거",
          "original_form": "약간 인턴같은거",
          "speaker_id": "2",
          "time": "20210825 20:45"
        },
        {
          "id": "MDRW2100014791.1.13",
          "form": "그치그치",
          "original_form": "그치그치",
          "speaker_id": "1",
          "time": "20210825 20:45"
        },
        {
          "id": "MDRW2100014791.1.14",
          "form": "하는건가?",
          "original_form": "하는건가?",
          "speaker_id": "2",
          "time": "20210825 20:45"
        },
        {
          "id": "MDRW2100014791.1.15",
          "form": "근데 인턴도 이제 1년남으면 넣을수있는곳이",
          "original_form": "근데 인턴도 이제 1년남으면 넣을수있는곳이",
          "speaker_id": "1",
          "time": "20210825 20:45"
        },
        {
          "id": "MDRW2100014791.1.16",
          "form": "막 많지 않았던 기억이 있어서",
          "original_form": "막 많지 않았던 기억이 있어서",
          "speaker_id": "1",
          "time": "20210825 20:45"
        },
        {
          "id": "MDRW2100014791.1.17",
          "form": "아쉽지않나 했는데",
          "original_form": "아쉽지않나 했는데",
          "speaker_id": "1",
          "time": "20210825 20:45"
        },
        {
          "id": "MDRW2100014791.1.18",
          "form": "몇학기 남기고 휴학한건데?",
          "original_form": "몇학기 남기고 휴학한건데?",
          "speaker_id": "2",
          "time": "20210825 20:45"
        },
        {
          "id": "MDRW2100014791.1.19",
          "form": "아무래도 그친구가 그동안 대기업에만 지원해봐서",
          "original_form": "아무래도 그친구가 그동안 대기업에만 지원해봐서",
          "speaker_id": "1",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014791.1.20",
          "form": "이제 한학기남았어",
          "original_form": "이제 한학기남았어",
          "speaker_id": "1",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014791.1.21",
          "form": "그러면",
          "original_form": "그러면",
          "speaker_id": "2",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014791.1.22",
          "form": "좋은 경험이 될 수 있겠네",
          "original_form": "좋은 경험이 될 수 있겠네",
          "speaker_id": "2",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014791.1.23",
          "form": "아무튼 그래서 그동안 너무 많은 실패의 경험으로 인해",
          "original_form": "아무튼 그래서 그동안 너무 많은 실패의 경험으로 인해",
          "speaker_id": "1",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014791.1.24",
          "form": "아무래도 휴학생일때는",
          "original_form": "아무래도 휴학생일때는",
          "speaker_id": "2",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014791.1.25",
          "form": "조금 눈을 낮추려고 하는 것 같다",
          "original_form": "조금 눈을 낮추려고 하는 것 같다",
          "speaker_id": "1",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014791.1.26",
          "form": "졸업한거보단 마음이 편하니까",
          "original_form": "졸업한거보단 마음이 편하니까",
          "speaker_id": "2",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014791.1.27",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014791.1.28",
          "form": "안타깝네...",
          "original_form": "안타깝네...",
          "speaker_id": "2",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014791.1.29",
          "form": "졸업하면 아예 공백기잖아",
          "original_form": "졸업하면 아예 공백기잖아",
          "speaker_id": "1",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014791.1.30",
          "form": "휴학동안 인턴 잘 해서",
          "original_form": "휴학동안 인턴 잘 해서",
          "speaker_id": "2",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014791.1.31",
          "form": "우리 ACM팀끼리",
          "original_form": "우리 ACM팀끼리",
          "speaker_id": "1",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014791.1.32",
          "form": "자신감을 얻었으면 좋겠다",
          "original_form": "자신감을 얻었으면 좋겠다",
          "speaker_id": "2",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014791.1.33",
          "form": "얘기할때도 그얘기 많이함",
          "original_form": "얘기할때도 그얘기 많이함",
          "speaker_id": "1",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014791.1.34",
          "form": "졸업하면 아예 공백기니까",
          "original_form": "졸업하면 아예 공백기니까",
          "speaker_id": "1",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014791.1.35",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014791.1.36",
          "form": "이게 백수라는 타이틀을",
          "original_form": "이게 백수라는 타이틀을",
          "speaker_id": "2",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014791.1.37",
          "form": "신경 안쓸수가 없잖아?",
          "original_form": "신경 안쓸수가 없잖아?",
          "speaker_id": "2",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014791.1.38",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014791.1.39",
          "form": "인턴이 진짜",
          "original_form": "인턴이 진짜",
          "speaker_id": "2",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014791.1.40",
          "form": "중요한 경험인거같아",
          "original_form": "중요한 경험인거같아",
          "speaker_id": "2",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014791.1.41",
          "form": "나도 복학하면 계속",
          "original_form": "나도 복학하면 계속",
          "speaker_id": "2",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014791.1.42",
          "form": "있는대로 지원해보려고",
          "original_form": "있는대로 지원해보려고",
          "speaker_id": "2",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014791.1.43",
          "form": "그게 최고인거같아",
          "original_form": "그게 최고인거같아",
          "speaker_id": "1",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014791.1.44",
          "form": "나도 있는대로 다 넣었다가",
          "original_form": "나도 있는대로 다 넣었다가",
          "speaker_id": "1",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014791.1.45",
          "form": "하나 얻어걸린케이스잖아",
          "original_form": "하나 얻어걸린케이스잖아",
          "speaker_id": "1",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014791.1.46",
          "form": "인턴으로 성공한 케이스지",
          "original_form": "인턴으로 성공한 케이스지",
          "speaker_id": "2",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014791.1.47",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014791.1.48",
          "form": "그거 해보고 물량공세에는 답없다고",
          "original_form": "그거 해보고 물량공세에는 답없다고",
          "speaker_id": "1",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014791.1.49",
          "form": "이게 왜 나쁜말이야",
          "original_form": "이게 왜 나쁜말이야",
          "speaker_id": "1",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014791.1.50",
          "form": "확실히 신입으로 들어가는거보단",
          "original_form": "확실히 신입으로 들어가는거보단",
          "speaker_id": "2",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014791.1.51",
          "form": "물량공세가 답이다",
          "original_form": "물량공세가 답이다",
          "speaker_id": "1",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014791.1.52",
          "form": "이건 좋은말이지",
          "original_form": "이건 좋은말이지",
          "speaker_id": "1",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014791.1.53",
          "form": "인턴이 수월한거같아",
          "original_form": "인턴이 수월한거같아",
          "speaker_id": "2",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014791.1.54",
          "form": "나쁜말이야 저거?",
          "original_form": "나쁜말이야 저거?",
          "speaker_id": "2",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014791.1.55",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014791.1.56",
          "form": "답은 인턴에 있네",
          "original_form": "답은 인턴에 있네",
          "speaker_id": "2",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014791.1.57",
          "form": "답은 신입사원에 없어",
          "original_form": "답은 신입사원에 없어",
          "speaker_id": "2",
          "time": "20210825 20:50"
        }
      ]
    }
