      "utterance": [
        {
          "id": "MDRW2100002516.1.1",
          "form": "요즘 인터넷 커뮤니티하는사람이",
          "original_form": "요즘 인터넷 커뮤니티하는사람이",
          "speaker_id": "1",
          "time": "20210820 13:03"
        },
        {
          "id": "MDRW2100002516.1.2",
          "form": "많아진거같아요..",
          "original_form": "많아진거같아요..",
          "speaker_id": "1",
          "time": "20210820 13:03"
        },
        {
          "id": "MDRW2100002516.1.3",
          "form": "논란도많고",
          "original_form": "논란도많고",
          "speaker_id": "1",
          "time": "20210820 13:03"
        },
        {
          "id": "MDRW2100002516.1.4",
          "form": "커뮤니티하세요?",
          "original_form": "커뮤니티하세요?",
          "speaker_id": "2",
          "time": "20210820 13:04"
        },
        {
          "id": "MDRW2100002516.1.5",
          "form": "네~~ 저는 하고있어요",
          "original_form": "네~~ 저는 하고있어요",
          "speaker_id": "1",
          "time": "20210820 13:25"
        },
        {
          "id": "MDRW2100002516.1.6",
          "form": "name2님도 하세요??",
          "original_form": "&name2&님도 하세요??",
          "speaker_id": "1",
          "time": "20210820 13:25"
        },
        {
          "id": "MDRW2100002516.1.7",
          "form": "저는 딱히 하고있는 곳은 없어요. 인스타 이것도 커뮤니티에 속하나요?",
          "original_form": "저는 딱히 하고있는 곳은 없어요. 인스타 이것도 커뮤니티에 속하나요?",
          "speaker_id": "2",
          "time": "20210820 13:25"
        },
        {
          "id": "MDRW2100002516.1.8",
          "form": "SNS도...? 일종에 커뮤니티겠죵?!",
          "original_form": "SNS도...? 일종에 커뮤니티겠죵?!",
          "speaker_id": "1",
          "time": "20210820 13:26"
        },
        {
          "id": "MDRW2100002516.1.9",
          "form": "남들이랑 소통하는 소통의 장이니까?!",
          "original_form": "남들이랑 소통하는 소통의 장이니까?!",
          "speaker_id": "1",
          "time": "20210820 13:26"
        },
        {
          "id": "MDRW2100002516.1.10",
          "form": "그럼 저는 인스타그램을 해요!",
          "original_form": "그럼 저는 인스타그램을 해요!",
          "speaker_id": "2",
          "time": "20210820 13:27"
        },
        {
          "id": "MDRW2100002516.1.11",
          "form": "name1님은 커뮤니티 활동을 하면서 어떤 것을 소통하시나요?",
          "original_form": "&name1&님은 커뮤니티 활동을 하면서 어떤 것을 소통하시나요?",
          "speaker_id": "2",
          "time": "20210820 13:27"
        },
        {
          "id": "MDRW2100002516.1.12",
          "form": "저는 보통 일상이나 회사욕이나 ㅋㅋ",
          "original_form": "저는 보통 일상이나 회사욕이나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 13:28"
        },
        {
          "id": "MDRW2100002516.1.13",
          "form": "전 트위터해요!!",
          "original_form": "전 트위터해요!!",
          "speaker_id": "1",
          "time": "20210820 13:29"
        },
        {
          "id": "MDRW2100002516.1.14",
          "form": "커뮤니티마다 분위기가 있다던데 트위터는 어때요?",
          "original_form": "커뮤니티마다 분위기가 있다던데 트위터는 어때요?",
          "speaker_id": "2",
          "time": "20210820 13:33"
        },
        {
          "id": "MDRW2100002516.1.15",
          "form": "가끔 캡쳐올라오는거 보는데 뭔가 되게 맞는 말 대잔치던데",
          "original_form": "가끔 캡쳐올라오는거 보는데 뭔가 되게 맞는 말 대잔치던데",
          "speaker_id": "2",
          "time": "20210820 13:34"
        },
        {
          "id": "MDRW2100002516.1.16",
          "form": "맞아요ㅋㅋㅋㅋㅋ트위터는 엄청 자유로워요",
          "original_form": "맞아요ㅋㅋㅋㅋㅋ트위터는 엄청 자유로워요",
          "speaker_id": "1",
          "time": "20210820 13:36"
        },
        {
          "id": "MDRW2100002516.1.17",
          "form": "사람도많고",
          "original_form": "사람도많고",
          "speaker_id": "1",
          "time": "20210820 13:36"
        },
        {
          "id": "MDRW2100002516.1.18",
          "form": "되게 궁금해지네요 트위터는 어떤식으로 활동해요? 그냥 글 올리고 공감하고?",
          "original_form": "되게 궁금해지네요 트위터는 어떤식으로 활동해요? 그냥 글 올리고 공감하고?",
          "speaker_id": "2",
          "time": "20210820 13:37"
        },
        {
          "id": "MDRW2100002516.1.19",
          "form": "글 올리면",
          "original_form": "글 올리면",
          "speaker_id": "1",
          "time": "20210820 13:38"
        },
        {
          "id": "MDRW2100002516.1.20",
          "form": "스크랩같은 리트윗이란 기능이있고",
          "original_form": "스크랩같은 리트윗이란 기능이있고",
          "speaker_id": "1",
          "time": "20210820 13:38"
        },
        {
          "id": "MDRW2100002516.1.21",
          "form": "공감한다는 마음이라는 버튼이 있어요!!!",
          "original_form": "공감한다는 마음이라는 버튼이 있어요!!!",
          "speaker_id": "1",
          "time": "20210820 13:38"
        },
        {
          "id": "MDRW2100002516.1.22",
          "form": "오 리트윗이 스크랩이군요",
          "original_form": "오 리트윗이 스크랩이군요",
          "speaker_id": "2",
          "time": "20210820 13:38"
        },
        {
          "id": "MDRW2100002516.1.23",
          "form": "인스타 리그램같은거?!",
          "original_form": "인스타 리그램같은거?!",
          "speaker_id": "1",
          "time": "20210820 13:38"
        },
        {
          "id": "MDRW2100002516.1.24",
          "form": "뭔가 리트윗 하는데 그게 뭔지 몰라 궁금했거든요",
          "original_form": "뭔가 리트윗 하는데 그게 뭔지 몰라 궁금했거든요",
          "speaker_id": "2",
          "time": "20210820 13:39"
        },
        {
          "id": "MDRW2100002516.1.25",
          "form": "인스타랑 비슷하구나",
          "original_form": "인스타랑 비슷하구나",
          "speaker_id": "2",
          "time": "20210820 13:39"
        },
        {
          "id": "MDRW2100002516.1.26",
