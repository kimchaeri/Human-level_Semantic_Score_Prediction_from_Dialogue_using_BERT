      "utterance": [
        {
          "id": "MDRW2100027802.1.1",
          "form": "여행 가고 싶어",
          "original_form": "여행 가고 싶어",
          "speaker_id": "1",
          "time": "20210902 15:13"
        },
        {
          "id": "MDRW2100027802.1.2",
          "form": "제주도",
          "original_form": "제주도",
          "speaker_id": "1",
          "time": "20210902 15:13"
        },
        {
          "id": "MDRW2100027802.1.3",
          "form": "재주도 좋지...",
          "original_form": "재주도 좋지...",
          "speaker_id": "2",
          "time": "20210902 15:14"
        },
        {
          "id": "MDRW2100027802.1.4",
          "form": "뭐래? 제주도",
          "original_form": "뭐래? 제주도",
          "speaker_id": "2",
          "time": "20210902 15:14"
        },
        {
          "id": "MDRW2100027802.1.5",
          "form": "이 시국만 아녔어도 여행 가는데",
          "original_form": "이 시국만 아녔어도 여행 가는데",
          "speaker_id": "2",
          "time": "20210902 15:14"
        },
        {
          "id": "MDRW2100027802.1.6",
          "form": "내말이",
          "original_form": "내말이",
          "speaker_id": "1",
          "time": "20210902 15:14"
        },
        {
          "id": "MDRW2100027802.1.7",
          "form": "바다보고 패러글라이딩 하고 싶어.",
          "original_form": "바다보고 패러글라이딩 하고 싶어.",
          "speaker_id": "1",
          "time": "20210902 15:15"
        },
        {
          "id": "MDRW2100027802.1.8",
          "form": "진심 패러글라이딩 못한 거 너무 아쉬운",
          "original_form": "진심 패러글라이딩 못한 거 너무 아쉬운",
          "speaker_id": "2",
          "time": "20210902 15:15"
        },
        {
          "id": "MDRW2100027802.1.9",
          "form": "언제 하러간담",
          "original_form": "언제 하러간담",
          "speaker_id": "2",
          "time": "20210902 15:15"
        },
        {
          "id": "MDRW2100027802.1.10",
          "form": "눈물 줄줄",
          "original_form": "눈물 줄줄",
          "speaker_id": "1",
          "time": "20210902 15:16"
        },
        {
          "id": "MDRW2100027802.1.11",
          "form": "졸업... 하고... 졸업여행 가자",
          "original_form": "졸업... 하고... 졸업여행 가자",
          "speaker_id": "1",
          "time": "20210902 15:16"
        },
        {
          "id": "MDRW2100027802.1.12",
          "form": "졸업 너무 먼 미래 아니냐고",
          "original_form": "졸업 너무 먼 미래 아니냐고",
          "speaker_id": "2",
          "time": "20210902 15:16"
        },
        {
          "id": "MDRW2100027802.1.13",
          "form": "졸업 금방이다",
          "original_form": "졸업 금방이다",
          "speaker_id": "1",
          "time": "20210902 15:17"
        },
        {
          "id": "MDRW2100027802.1.14",
          "form": "중간에 당일치기라도 패러글라이딩 하고 싶은데",
          "original_form": "중간에 당일치기라도 패러글라이딩 하고 싶은데",
          "speaker_id": "2",
          "time": "20210902 15:17"
        },
        {
          "id": "MDRW2100027802.1.15",
          "form": "...너 사람을 꼬실 줄",
          "original_form": "...너 사람을 꼬실 줄",
          "speaker_id": "1",
          "time": "20210902 15:17"
        },
        {
          "id": "MDRW2100027802.1.16",
          "form": "번지점프도...",
          "original_form": "번지점프도...",
          "speaker_id": "2",
          "time": "20210902 15:17"
        },
        {
          "id": "MDRW2100027802.1.17",
          "form": "#갈때데리고가",
          "original_form": "#갈때데리고가",
          "speaker_id": "1",
          "time": "20210902 15:17"
        },
        {
          "id": "MDRW2100027802.1.18",
          "form": "ㅠㅠ제발 코시국 멈춰",
          "original_form": "ㅠㅠ제발 코시국 멈춰",
          "speaker_id": "2",
          "time": "20210902 15:17"
        },
        {
          "id": "MDRW2100027802.1.19",
          "form": "가면 여름에 가야 하나",
          "original_form": "가면 여름에 가야 하나",
          "speaker_id": "1",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027802.1.20",
          "form": "가을?",
          "original_form": "가을?",
          "speaker_id": "1",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027802.1.21",
          "form": "여름이나 가을?",
          "original_form": "여름이나 가을?",
          "speaker_id": "2",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027802.1.22",
          "form": "올해는 글렀고...",
          "original_form": "올해는 글렀고...",
          "speaker_id": "2",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027802.1.23",
          "form": "가을에도 좀 추워지면 번지점프는 안 하던데...",
          "original_form": "가을에도 좀 추워지면 번지점프는 안 하던데...",
          "speaker_id": "2",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027802.1.24",
          "form": "내년 9월 초가 괜찮으려나",
          "original_form": "내년 9월 초가 괜찮으려나",
          "speaker_id": "1",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027802.1.25",
          "form": "아님 8월 말",
          "original_form": "아님 8월 말",
          "speaker_id": "1",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100027802.1.26",
          "form": "8월이 낫지",
          "original_form": "8월이 낫지",
          "speaker_id": "2",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100027802.1.27",
          "form": "9월은... 학교 갈 수도 있고...",
          "original_form": "9월은... 학교 갈 수도 있고...",
          "speaker_id": "2",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100027802.1.28",
          "form": "좋아...",
          "original_form": "좋아...",
          "speaker_id": "1",
          "time": "20210902 15:19"
        }
      ]
    }
  ]
