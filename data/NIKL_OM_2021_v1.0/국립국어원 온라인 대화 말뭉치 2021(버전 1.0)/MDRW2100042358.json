      "utterance": [
        {
          "id": "MDRW2100042358.1.1",
          "form": "라이덴 성.능은",
          "original_form": "라이덴 성.능은",
          "speaker_id": "1",
          "time": "20210906 21:52"
        },
        {
          "id": "MDRW2100042358.1.2",
          "form": "2돌부터 진가를 발휘한다고 하긴 하는데....",
          "original_form": "2돌부터 진가를 발휘한다고 하긴 하는데....",
          "speaker_id": "1",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042358.1.3",
          "form": "(이때부터 주변 몹의 원소저항력을 무시하고 공격한다고 함",
          "original_form": "(이때부터 주변 몹의 원소저항력을 무시하고 공격한다고 함",
          "speaker_id": "1",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042358.1.4",
          "form": "걍... 나름 일곱 신 중 하나니까",
          "original_form": "걍... 나름 일곱 신 중 하나니까",
          "speaker_id": "1",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042358.1.5",
          "form": "명함만 가질까 싶어서....",
          "original_form": "명함만 가질까 싶어서....",
          "speaker_id": "1",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042358.1.6",
          "form": "아 미안",
          "original_form": "아 미안",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042358.1.7",
          "form": "나 잠깐",
          "original_form": "나 잠깐",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100042358.1.8",
          "form": "전기박쥐 사냥하느라",
          "original_form": "전기박쥐 사냥하느라",
          "speaker_id": "2",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042358.1.9",
          "form": "답을 못했어",
          "original_form": "답을 못했어",
          "speaker_id": "2",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042358.1.10",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042358.1.11",
          "form": "근데 서브딜러로도",
          "original_form": "근데 서브딜러로도",
          "speaker_id": "2",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042358.1.12",
          "form": "나쁘지 않댔으니까",
          "original_form": "나쁘지 않댔으니까",
          "speaker_id": "2",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042358.1.13",
          "form": "라이덴 궁 깔고 다이루크 쓰면",
          "original_form": "라이덴 궁 깔고 다이루크 쓰면",
          "speaker_id": "2",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042358.1.14",
          "form": "혼자 과부하 터뜨릴듯",
          "original_form": "혼자 과부하 터뜨릴듯",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042358.1.15",
          "form": "이제 라이덴 70렙인데",
          "original_form": "이제 라이덴 70렙인데",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042358.1.16",
          "form": "쓸만 할까...?",
          "original_form": "쓸만 할까...?",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042358.1.17",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100042358.1.18",
          "form": "섭딜이면 딜이 보통",
          "original_form": "섭딜이면 딜이 보통",
          "speaker_id": "1",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100042358.1.19",
          "form": "한손검 수준으로 나온단 얘긴데",
          "original_form": "한손검 수준으로 나온단 얘긴데",
          "speaker_id": "1",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100042358.1.20",
          "form": "얜 장병기니까 좀 낫지 안ㄹ을까...?",
          "original_form": "얜 장병기니까 좀 낫지 안ㄹ을까...?",
          "speaker_id": "1",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100042358.1.21",
          "form": "라이덴 무기 뭐쥐어줄까 찾아보는중인데",
          "original_form": "라이덴 무기 뭐쥐어줄까 찾아보는중인데",
          "speaker_id": "2",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100042358.1.22",
          "form": "얜 스킬 구조가",
          "original_form": "얜 스킬 구조가",
          "speaker_id": "2",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100042358.1.23",
          "form": "디게 특이하다",
          "original_form": "디게 특이하다",
          "speaker_id": "2",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100042358.1.24",
          "form": "궁 평타 공격이 기본이라",
          "original_form": "궁 평타 공격이 기본이라",
          "speaker_id": "2",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100042358.1.25",
          "form": "일반평타는 특성 찍을 필요도 없대",
          "original_form": "일반평타는 특성 찍을 필요도 없대",
          "speaker_id": "2",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100042358.1.26",
          "form": "보통 섭딜이 그래 ㅇㅇ",
          "original_form": "보통 섭딜이 그래 ㅇㅇ",
          "speaker_id": "1",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042358.1.27",
          "form": "주력딜은 888",
          "original_form": "주력딜은 888",
          "speaker_id": "1",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042358.1.28",
          "form": "섭딜은 188",
          "original_form": "섭딜은 188",
          "speaker_id": "1",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042358.1.29",
          "form": "카즈하가 좀 특이하게",
          "original_form": "카즈하가 좀 특이하게",
          "speaker_id": "1",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042358.1.30",
          "form": "스킬딜이 평딜이랑 연계되어있어서",
          "original_form": "스킬딜이 평딜이랑 연계되어있어서",
          "speaker_id": "1",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042358.1.31",
          "form": "888 권장람",
          "original_form": "888 권장람",
          "speaker_id": "1",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042358.1.32",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042358.1.33",
          "form": "추천 무기가 그 어창? 그 물고기작살이랑",
          "original_form": "추천 무기가 그 어창? 그 물고기작살이랑",
          "speaker_id": "2",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042358.1.34",
          "form": "페보니우스 장창이길래",
          "original_form": "페보니우스 장창이길래",
          "speaker_id": "2",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042358.1.35",
          "form": "페보니우스 장창 쥐어줌",
          "original_form": "페보니우스 장창 쥐어줌",
          "speaker_id": "2",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042358.1.36",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "1",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042358.1.37",
          "form": "가챠템이야?",
          "original_form": "가챠템이야?",
          "speaker_id": "1",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042358.1.38",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042358.1.39",
          "form": "페보니우스 기사단 템이지만",
          "original_form": "페보니우스 기사단 템이지만",
          "speaker_id": "2",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042358.1.40",
          "form": "라이덴한테 들려주기...",
          "original_form": "라이덴한테 들려주기...",
          "speaker_id": "2",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042358.1.41",
          "form": "그럼 왠지 나도 잇을거같은데 함 봐야겟다",
          "original_form": "그럼 왠지 나도 잇을거같은데 함 봐야겟다",
          "speaker_id": "1",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042358.1.42",
          "form": "페보니우스 기사단에",
          "original_form": "페보니우스 기사단에",
          "speaker_id": "1",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042358.1.43",
          "form": "애초에 장병기캐가",
          "original_form": "애초에 장병기캐가",
          "speaker_id": "1",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042358.1.44",
          "form": "없잖앜",
          "original_form": "없잖앜",
          "speaker_id": "1",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042358.1.45",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ왜 준비한거야",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ왜 준비한거야",
          "speaker_id": "2",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042358.1.46",
          "form": "근데 4성 추천은 그 작살이고",
          "original_form": "근데 4성 추천은 그 작살이고",
          "speaker_id": "2",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042358.1.47",
          "form": "페보니우스 장창은 대체제래",
          "original_form": "페보니우스 장창은 대체제래",
          "speaker_id": "2",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100042358.1.48",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ ㅎ ㅏ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ ㅎ ㅏ",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042358.1.49",
          "form": "낚시도 해야하는데",
          "original_form": "낚시도 해야하는데",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042358.1.50",
          "form": "낚시 스팟 찾기 너무 힘들어",
          "original_form": "낚시 스팟 찾기 너무 힘들어",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042358.1.51",
          "form": "나 낚시는",
          "original_form": "나 낚시는",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042358.1.52",
          "form": "시작도 안했어",
          "original_form": "시작도 안했어",
          "speaker_id": "2",
          "time": "20210906 22:19"
        }
      ]
    }
  ]
