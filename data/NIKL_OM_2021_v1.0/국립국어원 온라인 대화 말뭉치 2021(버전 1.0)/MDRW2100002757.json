      "utterance": [
        {
          "id": "MDRW2100002757.1.1",
          "form": "챙겨드시는 건강식품 있으신가요",
          "original_form": "챙겨드시는 건강식품 있으신가요",
          "speaker_id": "1",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002757.1.2",
          "form": "비타민, 두유(무첨가) 먹고있어요~",
          "original_form": "비타민, 두유(무첨가) 먹고있어요~",
          "speaker_id": "2",
          "time": "20210820 17:00"
        },
        {
          "id": "MDRW2100002757.1.3",
          "form": "저는 알약이싫어서 구미타입으로 먹어요",
          "original_form": "저는 알약이싫어서 구미타입으로 먹어요",
          "speaker_id": "1",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002757.1.4",
          "form": "저도 어쩔수없는건 알약먹는데 비타민은 츄?타입으로먹어요ㅎㅎ",
          "original_form": "저도 어쩔수없는건 알약먹는데 비타민은 츄?타입으로먹어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002757.1.5",
          "form": "어떤제품이에요?제가먹는건 센트룸구미에요",
          "original_form": "어떤제품이에요?제가먹는건 센트룸구미에요",
          "speaker_id": "1",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002757.1.6",
          "form": "얼라이브먹다가 메이크미 먹고있어요 솔가도 크기작아서먹기편해요",
          "original_form": "얼라이브먹다가 메이크미 먹고있어요 솔가도 크기작아서먹기편해요",
          "speaker_id": "2",
          "time": "20210820 17:25"
        },
        {
          "id": "MDRW2100002757.1.7",
          "form": "메이크미 처음듣네요",
          "original_form": "메이크미 처음듣네요",
          "speaker_id": "1",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100002757.1.8",
          "form": "종류가참 많죠",
          "original_form": "종류가참 많죠",
          "speaker_id": "1",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100002757.1.9",
          "form": "저도 처음 들어본 브랜드데 츄타입 비타민이라 사봤어요~",
          "original_form": "저도 처음 들어본 브랜드데 츄타입 비타민이라 사봤어요~",
          "speaker_id": "2",
          "time": "20210820 17:35"
        },
        {
          "id": "MDRW2100002757.1.10",
          "form": "맛은 괜찮나요?",
          "original_form": "맛은 괜찮나요?",
          "speaker_id": "1",
          "time": "20210820 17:56"
        },
        {
          "id": "MDRW2100002757.1.11",
          "form": "시지않고 괜찮아요~",
          "original_form": "시지않고 괜찮아요~",
          "speaker_id": "2",
          "time": "20210820 17:58"
        },
        {
          "id": "MDRW2100002757.1.12",
          "form": "저도 다음에 먹어볼게요",
          "original_form": "저도 다음에 먹어볼게요",
          "speaker_id": "1",
          "time": "20210820 18:02"
        },
        {
          "id": "MDRW2100002757.1.13",
          "form": "정보많이주셔서 감사해요",
          "original_form": "정보많이주셔서 감사해요",
          "speaker_id": "1",
          "time": "20210820 18:02"
        },
        {
          "id": "MDRW2100002757.1.14",
          "form": "네~ 약간 어린이비타민 비슷한데 그거보단 시지않아서 좋아요",
          "original_form": "네~ 약간 어린이비타민 비슷한데 그거보단 시지않아서 좋아요",
          "speaker_id": "2",
          "time": "20210820 18:11"
        },
        {
          "id": "MDRW2100002757.1.15",
          "form": "제가 좋아하는 스타일이네요ㅎ",
          "original_form": "제가 좋아하는 스타일이네요ㅎ",
          "speaker_id": "1",
          "time": "20210820 18:12"
        },
        {
          "id": "MDRW2100002757.1.16",
          "form": "ㅎㅎ , 그리고 알약 좀 작게 나왔으면 좋겠어요  먹기편하게요~",
          "original_form": "ㅎㅎ , 그리고 알약 좀 작게 나왔으면 좋겠어요  먹기편하게요~",
          "speaker_id": "2",
          "time": "20210820 18:16"
        },
        {
          "id": "MDRW2100002757.1.17",
          "form": "맞아요 오메가3같은건 목에걸려서 힘들어요ㅠ",
          "original_form": "맞아요 오메가3같은건 목에걸려서 힘들어요ㅠ",
          "speaker_id": "1",
          "time": "20210820 18:28"
        },
        {
          "id": "MDRW2100002757.1.18",
          "form": "그나마 캡슐은 편한데 알약,젤로된거는 더 그러터라고요~",
          "original_form": "그나마 캡슐은 편한데 알약,젤로된거는 더 그러터라고요~",
          "speaker_id": "2",
          "time": "20210820 18:29"
        },
        {
          "id": "MDRW2100002757.1.19",
          "form": "혹 운동도 하세요?",
          "original_form": "혹 운동도 하세요?",
          "speaker_id": "2",
          "time": "20210820 18:29"
        },
        {
          "id": "MDRW2100002757.1.20",
          "form": "네 만보걷기하다가 요즘은 좀쉬어요",
          "original_form": "네 만보걷기하다가 요즘은 좀쉬어요",
          "speaker_id": "1",
          "time": "20210820 18:29"
        },
        {
          "id": "MDRW2100002757.1.21",
          "form": "와~ 만보걷기하시면 밖에서 하세요?",
          "original_form": "와~ 만보걷기하시면 밖에서 하세요?",
          "speaker_id": "2",
          "time": "20210820 18:40"
        },
        {
          "id": "MDRW2100002757.1.22",
          "form": "네 낮은 뜨거워서 밤에 걸어요",
          "original_form": "네 낮은 뜨거워서 밤에 걸어요",
          "speaker_id": "1",
          "time": "20210820 18:40"
        },
        {
          "id": "MDRW2100002757.1.23",
          "form": "낮에 왠만하면 나가기도 싫어져서 가끔 걷기나가요ㅎ",
          "original_form": "낮에 왠만하면 나가기도 싫어져서 가끔 걷기나가요ㅎ",
          "speaker_id": "2",
          "time": "20210820 18:43"
        },
        {
          "id": "MDRW2100002757.1.24",
          "form": "그리고 의외로 저녁때도 사람많더라고요",
          "original_form": "그리고 의외로 저녁때도 사람많더라고요",
          "speaker_id": "2",
          "time": "20210820 18:43"
        },
        {
          "id": "MDRW2100002757.1.25",
          "form": "맞아요ㅎ 밤11시에도 많이들 걸으러나오시더라구요",
          "original_form": "맞아요ㅎ 밤11시에도 많이들 걸으러나오시더라구요",
          "speaker_id": "1",
          "time": "20210820 18:44"
        },
        {
          "id": "MDRW2100002757.1.26",
          "form": "오~ 전7~8시쯤 나가는데 밤늦게도 많네요",
          "original_form": "오~ 전7~8시쯤 나가는데 밤늦게도 많네요",
          "speaker_id": "2",
          "time": "20210820 18:47"
        },
        {
          "id": "MDRW2100002757.1.27",
          "form": "네ㅎㅎ그래서 덜무서워요",
          "original_form": "네ㅎㅎ그래서 덜무서워요",
          "speaker_id": "1",
          "time": "20210820 19:03"
        },
        {
          "id": "MDRW2100002757.1.28",
          "form": "아예 없는것보단 낫죠~",
          "original_form": "아예 없는것보단 낫죠~",
          "speaker_id": "2",
          "time": "20210820 19:17"
        },
        {
          "id": "MDRW2100002757.1.29",
          "form": "네 산책로가 잘 되어있어서 걷기운동하기 좋아요",
          "original_form": "네 산책로가 잘 되어있어서 걷기운동하기 좋아요",
          "speaker_id": "1",
          "time": "20210820 19:18"
        }
      ]
