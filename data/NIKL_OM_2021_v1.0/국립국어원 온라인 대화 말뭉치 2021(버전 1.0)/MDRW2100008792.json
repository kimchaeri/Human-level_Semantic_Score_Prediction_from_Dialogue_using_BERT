      "utterance": [
        {
          "id": "MDRW2100008792.1.1",
          "form": "요즘 건강하다고 느끼시나요?",
          "original_form": "요즘 건강하다고 느끼시나요?",
          "speaker_id": "2",
          "time": "20210822 18:21"
        },
        {
          "id": "MDRW2100008792.1.2",
          "form": "저는 여기저기 아픈것같아요ㅜㅜ",
          "original_form": "저는 여기저기 아픈것같아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 18:22"
        },
        {
          "id": "MDRW2100008792.1.3",
          "form": "저도 그래서 운동 시작하려구요..",
          "original_form": "저도 그래서 운동 시작하려구요..",
          "speaker_id": "1",
          "time": "20210822 18:22"
        },
        {
          "id": "MDRW2100008792.1.4",
          "form": "예전같지않네요",
          "original_form": "예전같지않네요",
          "speaker_id": "1",
          "time": "20210822 18:22"
        },
        {
          "id": "MDRW2100008792.1.5",
          "form": "맞아요ㅜㅜ 진짜 운동 해야될것같아요",
          "original_form": "맞아요ㅜㅜ 진짜 운동 해야될것같아요",
          "speaker_id": "2",
          "time": "20210822 18:23"
        },
        {
          "id": "MDRW2100008792.1.6",
          "form": "체력이 너무 안좋아졌어요",
          "original_form": "체력이 너무 안좋아졌어요",
          "speaker_id": "2",
          "time": "20210822 18:23"
        },
        {
          "id": "MDRW2100008792.1.7",
          "form": "아무래도 코로나라 집에 있는시간이 많아져서 그런것같아요",
          "original_form": "아무래도 코로나라 집에 있는시간이 많아져서 그런것같아요",
          "speaker_id": "2",
          "time": "20210822 18:23"
        },
        {
          "id": "MDRW2100008792.1.8",
          "form": "저도 그래요..그냥 느낌으로 알게 되는게 더 무서운ㅜㅜ진짜 운동해야겠다고 느껴요",
          "original_form": "저도 그래요..그냥 느낌으로 알게 되는게 더 무서운ㅜㅜ진짜 운동해야겠다고 느껴요",
          "speaker_id": "1",
          "time": "20210822 18:24"
        },
        {
          "id": "MDRW2100008792.1.9",
          "form": "계단오르는것도 금방 숨차더라구요",
          "original_form": "계단오르는것도 금방 숨차더라구요",
          "speaker_id": "2",
          "time": "20210822 18:24"
        },
        {
          "id": "MDRW2100008792.1.10",
          "form": "그리고 식습관도 한몫 하는것같아요",
          "original_form": "그리고 식습관도 한몫 하는것같아요",
          "speaker_id": "2",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008792.1.11",
          "form": "맞아요 하루가 다른느낌? 이제 정말 단거 짠거 안먹어야하는데..",
          "original_form": "맞아요 하루가 다른느낌? 이제 정말 단거 짠거 안먹어야하는데..",
          "speaker_id": "1",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008792.1.12",
          "form": "저는 액상과당 중독자 같아요 ㅋㅋ",
          "original_form": "저는 액상과당 중독자 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:27"
        },
        {
          "id": "MDRW2100008792.1.13",
          "form": "아이스크림에 콜라에..  진짜 끊어야 하는데ㅜㅜ",
          "original_form": "아이스크림에 콜라에..  진짜 끊어야 하는데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 18:28"
        },
        {
          "id": "MDRW2100008792.1.14",
          "form": "저는 마카롱...ㅠ 끊어야하는데",
          "original_form": "저는 마카롱...ㅠ 끊어야하는데",
          "speaker_id": "1",
          "time": "20210822 18:29"
        },
        {
          "id": "MDRW2100008792.1.15",
          "form": "저도 마카롱 진짜 많이 먹었어요 ㅋㅋ",
          "original_form": "저도 마카롱 진짜 많이 먹었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:30"
        },
        {
          "id": "MDRW2100008792.1.16",
          "form": "요즘 대체당식품이 잘나와서",
          "original_form": "요즘 대체당식품이 잘나와서",
