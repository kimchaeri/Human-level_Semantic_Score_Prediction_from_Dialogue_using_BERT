      "utterance": [
        {
          "id": "MDRW2100047600.1.1",
          "form": "주말에 맛난거 드셨나요",
          "original_form": "주말에 맛난거 드셨나요",
          "speaker_id": "2",
          "time": "20210829 06:25"
        },
        {
          "id": "MDRW2100047600.1.2",
          "form": "일찍 일어나셨네요!",
          "original_form": "일찍 일어나셨네요!",
          "speaker_id": "1",
          "time": "20210829 11:06"
        },
        {
          "id": "MDRW2100047600.1.3",
          "form": "중국집서 배달시켜 먹었어요ㅎ",
          "original_form": "중국집서 배달시켜 먹었어요ㅎ",
          "speaker_id": "1",
          "time": "20210829 11:07"
        },
        {
          "id": "MDRW2100047600.1.4",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210829 11:07"
        },
        {
          "id": "MDRW2100047600.1.5",
          "form": "오 저는 집밥 먹고 있어요 중국집 어떤메뉴 드셨어요",
          "original_form": "오 저는 집밥 먹고 있어요 중국집 어떤메뉴 드셨어요",
          "speaker_id": "2",
          "time": "20210829 11:41"
        },
        {
          "id": "MDRW2100047600.1.6",
          "form": "짬뽕 군만두 탕수육이요!",
          "original_form": "짬뽕 군만두 탕수육이요!",
          "speaker_id": "1",
          "time": "20210829 11:42"
        },
        {
          "id": "MDRW2100047600.1.7",
          "form": "메인반찬은 뭔가요",
          "original_form": "메인반찬은 뭔가요",
          "speaker_id": "1",
          "time": "20210829 11:43"
        },
        {
          "id": "MDRW2100047600.1.8",
          "form": "어제는 쌀국수..된장국 먹었네요",
          "original_form": "어제는 쌀국수..된장국 먹었네요",
          "speaker_id": "2",
          "time": "20210829 11:44"
        },
        {
          "id": "MDRW2100047600.1.9",
          "form": "된장국 맛있죠ㅎㅎ",
          "original_form": "된장국 맛있죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 11:45"
        },
        {
          "id": "MDRW2100047600.1.10",
          "form": "바깥밥 많이 먹다보면 집밥이 소중한것 같아요",
          "original_form": "바깥밥 많이 먹다보면 집밥이 소중한것 같아요",
          "speaker_id": "1",
          "time": "20210829 11:45"
        },
        {
          "id": "MDRW2100047600.1.11",
          "form": "네 기본적인 음식들이 생각날때가 있죠^^",
          "original_form": "네 기본적인 음식들이 생각날때가 있죠^^",
          "speaker_id": "2",
          "time": "20210829 11:47"
        },
        {
          "id": "MDRW2100047600.1.12",
          "form": "회사 근처에 백반집이 있음 좋겠어요",
          "original_form": "회사 근처에 백반집이 있음 좋겠어요",
          "speaker_id": "1",
          "time": "20210829 11:48"
        },
        {
          "id": "MDRW2100047600.1.13",
          "form": "회사가 이사를 가니 음식점들이 줄어서ㅜ",
          "original_form": "회사가 이사를 가니 음식점들이 줄어서ㅜ",
          "speaker_id": "1",
          "time": "20210829 11:48"
        },
        {
          "id": "MDRW2100047600.1.14",
          "form": "백반집이 가성비 참 좋죠^^",
          "original_form": "백반집이 가성비 참 좋죠^^",
          "speaker_id": "2",
          "time": "20210829 11:49"
        },
        {
          "id": "MDRW2100047600.1.15",
          "form": "ㅎㅎ그리고 전에 잘 가던 백반집은 밑반찬이 괜찮았거든요",
          "original_form": "ㅎㅎ그리고 전에 잘 가던 백반집은 밑반찬이 괜찮았거든요",
          "speaker_id": "1",
          "time": "20210829 11:50"
        },
        {
          "id": "MDRW2100047600.1.16",
          "form": "잡채는 기본에ㅎㅎ 제육 찌개 국도 기본세팅이라",
          "original_form": "잡채는 기본에ㅎㅎ 제육 찌개 국도 기본세팅이라",
          "speaker_id": "1",
          "time": "20210829 11:51"
        },
        {
          "id": "MDRW2100047600.1.17",
          "form": "와 집에선 잡채 한번 하려면 손이 많이 가는데여 ㅠ",
          "original_form": "와 집에선 잡채 한번 하려면 손이 많이 가는데여 ㅠ",
          "speaker_id": "2",
          "time": "20210829 12:46"
        },
        {
          "id": "MDRW2100047600.1.18",
          "form": "그니까요ㅜ 세팅도 매일 바뀌어서 자주 갔는데",
          "original_form": "그니까요ㅜ 세팅도 매일 바뀌어서 자주 갔는데",
          "speaker_id": "1",
          "time": "20210829 12:51"
        },
        {
          "id": "MDRW2100047600.1.19",
          "form": "회사 이사한 곳은....백반집도 잘 없고 그나마 있는곳도 너무 빈약해요",
          "original_form": "회사 이사한 곳은....백반집도 잘 없고 그나마 있는곳도 너무 빈약해요",
          "speaker_id": "1",
          "time": "20210829 12:51"
        }
      ]
