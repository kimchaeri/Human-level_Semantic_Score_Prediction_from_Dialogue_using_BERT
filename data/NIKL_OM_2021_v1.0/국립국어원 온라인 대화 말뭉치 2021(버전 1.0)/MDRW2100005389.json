      "utterance": [
        {
          "id": "MDRW2100005389.1.1",
          "form": "아 반찬도 잘 안드세요?",
          "original_form": "아 반찬도 잘 안드세요?",
          "speaker_id": "1",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005389.1.2",
          "form": "반찬 한 두가지?",
          "original_form": "반찬 한 두가지?",
          "speaker_id": "2",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005389.1.3",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005389.1.4",
          "form": "좋아하는 반찬 뭐 있어요?",
          "original_form": "좋아하는 반찬 뭐 있어요?",
          "speaker_id": "1",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005389.1.5",
          "form": "저는 섞어먹거나 비벼먹거나 차라리 한꺼번에 먹는걸 좋아해요",
          "original_form": "저는 섞어먹거나 비벼먹거나 차라리 한꺼번에 먹는걸 좋아해요",
          "speaker_id": "2",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005389.1.6",
          "form": "계란 종류 좋아해요",
          "original_form": "계란 종류 좋아해요",
          "speaker_id": "2",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005389.1.7",
          "form": "장조림 후라이 이런거요?",
          "original_form": "장조림 후라이 이런거요?",
          "speaker_id": "1",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005389.1.8",
          "form": "계란은 항상 옳죠",
          "original_form": "계란은 항상 옳죠",
          "speaker_id": "2",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005389.1.9",
          "form": "장조림은 별로..",
          "original_form": "장조림은 별로..",
          "speaker_id": "2",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005389.1.10",
          "form": "짠 걸 안좋아해요",
          "original_form": "짠 걸 안좋아해요",
          "speaker_id": "2",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005389.1.11",
          "form": "아앗..",
          "original_form": "아앗..",
          "speaker_id": "1",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005389.1.12",
          "form": "그럼 프라이도 간 안하고 먹나요?",
          "original_form": "그럼 프라이도 간 안하고 먹나요?",
          "speaker_id": "1",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005389.1.13",
          "form": "그래서 짠거 싫어함+여러 가지 먹는게 귀찮음 이래서 반찬 ㅇ잘 안먹어요..",
          "original_form": "그래서 짠거 싫어함+여러 가지 먹는게 귀찮음 이래서 반찬 ㅇ잘 안먹어요..",
          "speaker_id": "2",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005389.1.14",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005389.1.15",
          "form": "저는 소금안쳐요 ㅋㅋㅋㅋ",
          "original_form": "저는 소금안쳐요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005389.1.16",
          "form": "헉 소금을 안치신다니",
          "original_form": "헉 소금을 안치신다니",
          "speaker_id": "1",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005389.1.17",
          "form": "삶은 계란도 소금 안찍어먹구",
          "original_form": "삶은 계란도 소금 안찍어먹구",
          "speaker_id": "2",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005389.1.18",
          "form": "약간 재료 본연의 맛을 즐기시는 편인가봐요",
          "original_form": "약간 재료 본연의 맛을 즐기시는 편인가봐요",
          "speaker_id": "1",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005389.1.19",
          "form": "아빠가 짠걸 싫어하다보니 집에서 싱겁게 먹다가",
          "original_form": "아빠가 짠걸 싫어하다보니 집에서 싱겁게 먹다가",
          "speaker_id": "2",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005389.1.20",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005389.1.21",
          "form": "그리고 자극적인것도 영...",
          "original_form": "그리고 자극적인것도 영...",
          "speaker_id": "2",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005389.1.22",
          "form": "헛 저랑 완전 반대시네요",
          "original_form": "헛 저랑 완전 반대시네요",
          "speaker_id": "1",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005389.1.23",
          "form": "카레도 좋아하긴 한데 너무 많이 섞어 먹음 강황의 그 자극적임이 좀 그시기해요",
          "original_form": "카레도 좋아하긴 한데 너무 많이 섞어 먹음 강황의 그 자극적임이 좀 그시기해요",
          "speaker_id": "2",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005389.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:12"
        }
      ]
    }
