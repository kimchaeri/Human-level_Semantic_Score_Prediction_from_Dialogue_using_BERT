      "utterance": [
        {
          "id": "MDRW2100030086.1.1",
          "form": "직장인이시죠?",
          "original_form": "직장인이시죠?",
          "speaker_id": "1",
          "time": "20210903 21:17"
        },
        {
          "id": "MDRW2100030086.1.2",
          "form": "넵넵 사무직이요!",
          "original_form": "넵넵 사무직이요!",
          "speaker_id": "2",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100030086.1.3",
          "form": "요즘 바쁘세요?",
          "original_form": "요즘 바쁘세요?",
          "speaker_id": "1",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100030086.1.4",
          "form": "월 중순부터 말까지 바빴어요",
          "original_form": "월 중순부터 말까지 바빴어요",
          "speaker_id": "2",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100030086.1.5",
          "form": "지금은 괜찮아요?",
          "original_form": "지금은 괜찮아요?",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100030086.1.6",
          "form": "네 월초는 좀 괜찮아요",
          "original_form": "네 월초는 좀 괜찮아요",
          "speaker_id": "2",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100030086.1.7",
          "form": "직장다니세요?",
          "original_form": "직장다니세요?",
          "speaker_id": "2",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100030086.1.8",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100030086.1.9",
          "form": "저도 직장인",
          "original_form": "저도 직장인",
          "speaker_id": "1",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100030086.1.10",
          "form": "바쁘세요?ㅜㅠ",
          "original_form": "바쁘세요?ㅜㅠ",
          "speaker_id": "2",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100030086.1.11",
          "form": "한참 바쁘다가",
          "original_form": "한참 바쁘다가",
          "speaker_id": "1",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100030086.1.12",
          "form": "6월중순부터 좀 괜찮아졌어요",
          "original_form": "6월중순부터 좀 괜찮아졌어요",
          "speaker_id": "1",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100030086.1.13",
          "form": "연단위로 스케쥴 돌아가시나봐요",
          "original_form": "연단위로 스케쥴 돌아가시나봐요",
          "speaker_id": "2",
          "time": "20210903 21:22"
        },
        {
          "id": "MDRW2100030086.1.14",
          "form": "네 프로젝트땜에",
          "original_form": "네 프로젝트땜에",
          "speaker_id": "1",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100030086.1.15",
          "form": "보통 그래요",
          "original_form": "보통 그래요",
          "speaker_id": "1",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100030086.1.16",
          "form": "바쁘시면",
          "original_form": "바쁘시면",
          "speaker_id": "1",
          "time": "20210903 21:24"
        },
        {
          "id": "MDRW2100030086.1.17",
          "form": "야근도 자주하세요?",
          "original_form": "야근도 자주하세요?",
          "speaker_id": "1",
          "time": "20210903 21:24"
        },
        {
          "id": "MDRW2100030086.1.18",
          "form": "거의 칼퇴해서ㅋㅋ 급한일 가끔 있음 기꺼이 야근해요ㅋㅋ",
          "original_form": "거의 칼퇴해서ㅋㅋ 급한일 가끔 있음 기꺼이 야근해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100030086.1.19",
          "form": "아 칼퇴하셔야죠ㅋㅋㅋ",
          "original_form": "아 칼퇴하셔야죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100030086.1.20",
          "form": "좋은회사네요",
          "original_form": "좋은회사네요",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100030086.1.21",
          "form": "아...저만...하는 경우가 많아요ㅋㅋ 개발자들은 야근...ㅜ",
          "original_form": "아...저만...하는 경우가 많아요ㅋㅋ 개발자들은 야근...ㅜ",
          "speaker_id": "2",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100030086.1.22",
          "form": "아 개발자들하고 일하세요?",
          "original_form": "아 개발자들하고 일하세요?",
          "speaker_id": "1",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100030086.1.23",
          "form": "넵ㅋㅋ전 그냥 관리부서라...ㅜ",
          "original_form": "넵ㅋㅋ전 그냥 관리부서라...ㅜ",
          "speaker_id": "2",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100030086.1.24",
          "form": "그래도 야근을 기꺼이 하시다니",
          "original_form": "그래도 야근을 기꺼이 하시다니",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030086.1.25",
          "form": "우리 관리부서 여직원은",
          "original_form": "우리 관리부서 여직원은",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100030086.1.26",
          "form": "야근 아예 안하는데 ㅋㅋ",
          "original_form": "야근 아예 안하는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100030086.1.27",
          "form": "ㅋㅋ양심이 찔리기도하고ㅎㅎ",
          "original_form": "ㅋㅋ양심이 찔리기도하고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:49"
        },
        {
          "id": "MDRW2100030086.1.28",
          "form": "ㄱㅏ끔 일이 진짜 많을때는 칼퇴했던때를 되새기며 이정도는 해준다 심정으로 해요",
          "original_form": "ㄱㅏ끔 일이 진짜 많을때는 칼퇴했던때를 되새기며 이정도는 해준다 심정으로 해요",
          "speaker_id": "2",
          "time": "20210903 22:50"
        },
        {
          "id": "MDRW2100030086.1.29",
          "form": "우리 여직원은 땡 하면 바로가요",
          "original_form": "우리 여직원은 땡 하면 바로가요",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030086.1.30",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030086.1.31",
          "form": "ㅋㅋ 어떤 직무세요???",
          "original_form": "ㅋㅋ 어떤 직무세요???",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030086.1.32",
          "form": "저도 칼요라고... 칼퇴요정...",
          "original_form": "저도 칼요라고... 칼퇴요정...",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030086.1.33",
          "form": "전 설계사에요.도면 그리고 그런일 하는",
          "original_form": "전 설계사에요.도면 그리고 그런일 하는",
          "speaker_id": "1",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030086.1.34",
          "form": "와 너무 멋져요!??",
          "original_form": "와 너무 멋져요!??",
          "speaker_id": "2",
          "time": "20210903 23:32"
        }
      ]
    }
