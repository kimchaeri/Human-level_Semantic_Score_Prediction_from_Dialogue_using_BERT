      "utterance": [
        {
          "id": "MDRW2100016268.1.1",
          "form": "쿠킹덤 계속 하는데 관성으로 하고있는느낌",
          "original_form": "쿠킹덤 계속 하는데 관성으로 하고있는느낌",
          "speaker_id": "2",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100016268.1.2",
          "form": "근데 가챠게임 중에서 명함따기 제일 쉽기도하고 게임이 가벼워서(=할게없어서) 계속하고있는듯",
          "original_form": "근데 가챠게임 중에서 명함따기 제일 쉽기도하고 게임이 가벼워서(=할게없어서) 계속하고있는듯",
          "speaker_id": "2",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100016268.1.3",
          "form": "ㅋㅋㅋ할거없어서 하는것만 하지",
          "original_form": "ㅋㅋㅋ할거없어서 하는것만 하지",
          "speaker_id": "1",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016268.1.4",
          "form": "지우지도 못하고 계속함",
          "original_form": "지우지도 못하고 계속함",
          "speaker_id": "1",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016268.1.5",
          "form": "ㅋㅋㅋㅋ게임이 그렇지",
          "original_form": "ㅋㅋㅋㅋ게임이 그렇지",
          "speaker_id": "2",
          "time": "20210826 15:46"
        },
        {
          "id": "MDRW2100016268.1.6",
          "form": "그 검사 만든 게임사에서",
          "original_form": "그 검사 만든 게임사에서",
          "speaker_id": "2",
          "time": "20210826 15:46"
        },
        {
          "id": "MDRW2100016268.1.7",
          "form": "괜찮더라",
          "original_form": "괜찮더라",
          "speaker_id": "2",
          "time": "20210826 15:47"
        },
        {
          "id": "MDRW2100016268.1.8",
          "form": "도깨비 라는 게임 신작 티저 봤는데",
          "original_form": "도깨비 라는 게임 신작 티저 봤는데",
          "speaker_id": "2",
          "time": "20210826 15:47"
        },
        {
          "id": "MDRW2100016268.1.9",
          "form": "뭔데 첨 들음",
          "original_form": "뭔데 첨 들음",
          "speaker_id": "1",
          "time": "20210826 15:47"
        },
        {
          "id": "MDRW2100016268.1.10",
          "form": "시간나면 한번 봐봐 한국풍 오픈월드 수집형 게임임ㅇㅇ",
          "original_form": "시간나면 한번 봐봐 한국풍 오픈월드 수집형 게임임ㅇㅇ",
          "speaker_id": "2",
          "time": "20210826 15:47"
        },
        {
          "id": "MDRW2100016268.1.11",
          "form": "그래? 오픈된거임?",
          "original_form": "그래? 오픈된거임?",
          "speaker_id": "1",
          "time": "20210826 15:48"
        },
        {
          "id": "MDRW2100016268.1.12",
          "form": "ㄴㄴ 출시는 붉은사막이라는 게임 출시 후에 한다던데",
          "original_form": "ㄴㄴ 출시는 붉은사막이라는 게임 출시 후에 한다던데",
          "speaker_id": "2",
          "time": "20210826 15:48"
        },
        {
          "id": "MDRW2100016268.1.13",
          "form": "이번 2차 pv가 꽤 괜찮아",
          "original_form": "이번 2차 pv가 꽤 괜찮아",
          "speaker_id": "2",
          "time": "20210826 15:48"
        },
        {
          "id": "MDRW2100016268.1.14",
          "form": "함 찾아봐야겠네",
          "original_form": "함 찾아봐야겠네",
          "speaker_id": "1",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100016268.1.15",
          "form": "캐릭터 귀엽다",
          "original_form": "캐릭터 귀엽다",
          "speaker_id": "1",
          "time": "20210826 15:50"
        },
        {
          "id": "MDRW2100016268.1.16",
          "form": "2차 pv가 실제 플레이 영상 같다던데",
          "original_form": "2차 pv가 실제 플레이 영상 같다던데",
          "speaker_id": "2",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016268.1.17",
          "form": "게임 플레이 할때의 프레임드랍이 살아있다곸ㅋㅋ",
          "original_form": "게임 플레이 할때의 프레임드랍이 살아있다곸ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016268.1.18",
          "form": "퇴근하고 함 봐야겠다ㅋㅋㅋ",
          "original_form": "퇴근하고 함 봐야겠다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:52"
        },
        {
          "id": "MDRW2100016268.1.19",
          "form": "출시일은 안나온거라고?",
          "original_form": "출시일은 안나온거라고?",
          "speaker_id": "1",
          "time": "20210826 15:53"
        },
        {
          "id": "MDRW2100016268.1.20",
          "form": "ㅇㅇ 지금 개발 3년 넘어가는거같은데",
          "original_form": "ㅇㅇ 지금 개발 3년 넘어가는거같은데",
          "speaker_id": "2",
          "time": "20210826 15:53"
        },
        {
          "id": "MDRW2100016268.1.21",
          "form": "원래 모바일로가려다가 콘솔로 간보고있다던데",
          "original_form": "원래 모바일로가려다가 콘솔로 간보고있다던데",
