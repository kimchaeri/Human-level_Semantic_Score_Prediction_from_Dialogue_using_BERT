      "utterance": [
        {
          "id": "MDRW2100044876.1.1",
          "form": "저는 친구 결혼식 선물을 사려하는데",
          "original_form": "저는 친구 결혼식 선물을 사려하는데",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044876.1.2",
          "form": "뭐가좋을까요",
          "original_form": "뭐가좋을까요",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044876.1.3",
          "form": "아 예산을 어느 정도로 생각하세요?",
          "original_form": "아 예산을 어느 정도로 생각하세요?",
          "speaker_id": "2",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044876.1.4",
          "form": "축의는 따로 하면 한 10- 20정도요!",
          "original_form": "축의는 따로 하면 한 10- 20정도요!",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044876.1.5",
          "form": "소형가전을 많이 한다던데요",
          "original_form": "소형가전을 많이 한다던데요",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044876.1.6",
          "form": "네 소형가전 많이하죠",
          "original_form": "네 소형가전 많이하죠",
          "speaker_id": "2",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044876.1.7",
          "form": "커피머신이나",
          "original_form": "커피머신이나",
          "speaker_id": "2",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044876.1.8",
          "form": "전 친구 몇명이 모아서 선물했어요 보통",
          "original_form": "전 친구 몇명이 모아서 선물했어요 보통",
          "speaker_id": "2",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044876.1.9",
          "form": "저는 저번에 에어프라이기 해줬거든요",
          "original_form": "저는 저번에 에어프라이기 해줬거든요",
          "speaker_id": "1",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044876.1.10",
          "form": "커피머신 좋네요!",
          "original_form": "커피머신 좋네요!",
          "speaker_id": "1",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044876.1.11",
          "form": "커피머신 좋더라고요",
          "original_form": "커피머신 좋더라고요",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044876.1.12",
          "form": "한번 찾아봐야겠어요",
          "original_form": "한번 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044876.1.13",
          "form": "네스프레소를 많이 하나요?",
          "original_form": "네스프레소를 많이 하나요?",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044876.1.14",
          "form": "네네 찾아보셔요",
          "original_form": "네네 찾아보셔요",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044876.1.15",
          "form": "네스프레소나 일리",
          "original_form": "네스프레소나 일리",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044876.1.16",
          "form": "많이 하는 듯요",
          "original_form": "많이 하는 듯요",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044876.1.17",
          "form": "아 일리도 있군오",
          "original_form": "아 일리도 있군오",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044876.1.18",
          "form": "네 일리도 좋다더라고요",
          "original_form": "네 일리도 좋다더라고요",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044876.1.19",
          "form": "친구가 제발 집주소를 알려줬음 좋겠어요ㅋㅋ",
          "original_form": "친구가 제발 집주소를 알려줬음 좋겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044876.1.20",
          "form": "주고싶은데 안받겠다고 안알려줘요ㅋㅋ",
          "original_form": "주고싶은데 안받겠다고 안알려줘요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044876.1.21",
          "form": "아...그럼 카톡 선물하기에 있나 찾아보세요",
          "original_form": "아...그럼 카톡 선물하기에 있나 찾아보세요",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044876.1.22",
          "form": "카톡 선물하기는 받는 사람이 주소 넣는거닌ㆍ",
          "original_form": "카톡 선물하기는 받는 사람이 주소 넣는거닌ㆍ",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044876.1.23",
          "form": "어머나 name2님 지니어스시네요ㅋㅋ",
          "original_form": "어머나 &name2&님 지니어스시네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044876.1.24",
          "form": "주소 몰라도 보낼 수 있어요ㅋㅋ",
          "original_form": "주소 몰라도 보낼 수 있어요ㅋㅋ",
