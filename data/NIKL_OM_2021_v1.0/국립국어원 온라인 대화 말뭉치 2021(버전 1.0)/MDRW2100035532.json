      "utterance": [
        {
          "id": "MDRW2100035532.1.1",
          "form": "아뇨 투자금이 70만원이 아니라",
          "original_form": "아뇨 투자금이 70만원이 아니라",
          "speaker_id": "2",
          "time": "20210829 19:56"
        },
        {
          "id": "MDRW2100035532.1.2",
          "form": "70만원대일때 라고하더라고요",
          "original_form": "70만원대일때 라고하더라고요",
          "speaker_id": "2",
          "time": "20210829 19:56"
        },
        {
          "id": "MDRW2100035532.1.3",
          "form": "코인이 종류가 많잖아요 어떤건 지금 몇천만원한다던데 맞죠?",
          "original_form": "코인이 종류가 많잖아요 어떤건 지금 몇천만원한다던데 맞죠?",
          "speaker_id": "2",
          "time": "20210829 19:57"
        },
        {
          "id": "MDRW2100035532.1.4",
          "form": "개당 그런게 있을거에요",
          "original_form": "개당 그런게 있을거에요",
          "speaker_id": "1",
          "time": "20210829 19:57"
        },
        {
          "id": "MDRW2100035532.1.5",
          "form": "맨 첨에 나왔을땐 그걸로 피자 한판 시켜먹을 돈이었는데,,",
          "original_form": "맨 첨에 나왔을땐 그걸로 피자 한판 시켜먹을 돈이었는데,,",
          "speaker_id": "1",
          "time": "20210829 19:58"
        },
        {
          "id": "MDRW2100035532.1.6",
          "form": "네 개당 70 짜리가 지금 몇백이래요ㅋㅋ",
          "original_form": "네 개당 70 짜리가 지금 몇백이래요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:58"
        },
        {
          "id": "MDRW2100035532.1.7",
          "form": "제가 과거로 돌아간다면 미국에서 코인으로 피자를 팔겠어요",
          "original_form": "제가 과거로 돌아간다면 미국에서 코인으로 피자를 팔겠어요",
          "speaker_id": "1",
          "time": "20210829 19:58"
        },
        {
          "id": "MDRW2100035532.1.8",
          "form": "코인받고 피자를 팔겠어요",
          "original_form": "코인받고 피자를 팔겠어요",
          "speaker_id": "1",
          "time": "20210829 19:58"
        },
        {
          "id": "MDRW2100035532.1.9",
          "form": "ㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210829 19:58"
        },
        {
          "id": "MDRW2100035532.1.10",
          "form": "저도 예적금은 안하겠습니다",
          "original_form": "저도 예적금은 안하겠습니다",
          "speaker_id": "2",
          "time": "20210829 20:00"
        },
        {
          "id": "MDRW2100035532.1.11",
          "form": "지금도 있긴하지만요",
          "original_form": "지금도 있긴하지만요",
          "speaker_id": "2",
          "time": "20210829 20:00"
        },
        {
          "id": "MDRW2100035532.1.12",
          "form": "오우 코인 좀 사셨나요",
          "original_form": "오우 코인 좀 사셨나요",
          "speaker_id": "1",
          "time": "20210829 20:01"
        },
        {
          "id": "MDRW2100035532.1.13",
          "form": "전 아직 쫄보라 ,,,",
          "original_form": "전 아직 쫄보라 ,,,",
          "speaker_id": "1",
          "time": "20210829 20:01"
        },
        {
          "id": "MDRW2100035532.1.14",
          "form": "아뇨아뇨 있는건 예적금...저는 지금 주식펀드상품만 사요",
          "original_form": "아뇨아뇨 있는건 예적금...저는 지금 주식펀드상품만 사요",
          "speaker_id": "2",
          "time": "20210829 20:01"
        },
        {
          "id": "MDRW2100035532.1.15",
          "form": "제일사고싶은건 집이지만요ㅋㅋ",
          "original_form": "제일사고싶은건 집이지만요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:03"
        },
        {
          "id": "MDRW2100035532.1.16",
          "form": "인간의 가장 기본적인 욕망/ 잘먹고 잘자고 이걸 하려면 필요한게 참 많아요 ㅋㅋ",
          "original_form": "인간의 가장 기본적인 욕망/ 잘먹고 잘자고 이걸 하려면 필요한게 참 많아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:04"
        },
        {
          "id": "MDRW2100035532.1.17",
          "form": "잘 자려면 집이 좀 좋아야겠고 아님 집이 있어야겠고,,,",
          "original_form": "잘 자려면 집이 좀 좋아야겠고 아님 집이 있어야겠고,,,",
          "speaker_id": "1",
          "time": "20210829 20:04"
        },
        {
          "id": "MDRW2100035532.1.18",
          "form": "ㅋㅋ그니까요",
          "original_form": "ㅋㅋ그니까요",
          "speaker_id": "2",
          "time": "20210829 20:05"
        },
        {
          "id": "MDRW2100035532.1.19",
          "form": "의식주 중에 예전엔 식이 중요했는데 요즘은 주가 중요하네요",
          "original_form": "의식주 중에 예전엔 식이 중요했는데 요즘은 주가 중요하네요",
          "speaker_id": "2",
          "time": "20210829 20:05"
        },
        {
          "id": "MDRW2100035532.1.20",
          "form": "집이 이렇게 오를 줄 몰랐어요",
          "original_form": "집이 이렇게 오를 줄 몰랐어요",
          "speaker_id": "1",
          "time": "20210829 20:06"
        },
        {
          "id": "MDRW2100035532.1.21",
          "form": "저 사는 지역은 어떻게 제가 사볼라하면",
          "original_form": "저 사는 지역은 어떻게 제가 사볼라하면",
          "speaker_id": "2",
          "time": "20210829 20:06"
        },
        {
          "id": "MDRW2100035532.1.22",
          "form": "21세기에 지어진 집은 안된다더군요ㅋㅋ",
          "original_form": "21세기에 지어진 집은 안된다더군요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:07"
        },
        {
          "id": "MDRW2100035532.1.23",
          "form": "1990년대 집 정도나 겨우가능해요",
          "original_form": "1990년대 집 정도나 겨우가능해요",
          "speaker_id": "2",
          "time": "20210829 20:07"
        },
        {
          "id": "MDRW2100035532.1.24",
          "form": "것두 재개발이 될것 같음 조은데요",
          "original_form": "것두 재개발이 될것 같음 조은데요",
          "speaker_id": "1",
          "time": "20210829 20:08"
        },
        {
          "id": "MDRW2100035532.1.25",
          "form": "ㅋㅋㅋ그런정보를 제가 알리가요",
          "original_form": "ㅋㅋㅋ그런정보를 제가 알리가요",
          "speaker_id": "2",
          "time": "20210829 20:08"
        },
        {
          "id": "MDRW2100035532.1.26",
          "form": "눈 딱감고 지르는게 중요하다고 하는데요",
          "original_form": "눈 딱감고 지르는게 중요하다고 하는데요",
          "speaker_id": "1",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100035532.1.27",
          "form": "이게 부모님이 딱 질러라 말하고 조언해주는 집이랑 혼자 고민하는 집의 차이가 큰 것 같아요",
          "original_form": "이게 부모님이 딱 질러라 말하고 조언해주는 집이랑 혼자 고민하는 집의 차이가 큰 것 같아요",
          "speaker_id": "1",
          "time": "20210829 20:17"
        }
      ]
