      "utterance": [
        {
          "id": "MDRW2100038345.1.1",
          "form": "점심은 뭐 드셨어요?",
          "original_form": "점심은 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210902 14:17"
        },
        {
          "id": "MDRW2100038345.1.2",
          "form": "저~ 찜닭이랑 콩나물국이요",
          "original_form": "저~ 찜닭이랑 콩나물국이요",
          "speaker_id": "1",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100038345.1.3",
          "form": "맛있는 거 드셨네요ㅋㅋ",
          "original_form": "맛있는 거 드셨네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:24"
        },
        {
          "id": "MDRW2100038345.1.4",
          "form": "전 그냥 백반ㅋㅋ",
          "original_form": "전 그냥 백반ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:24"
        },
        {
          "id": "MDRW2100038345.1.5",
          "form": "그쵸ㅎ 오늘 점심은 맘에 들었어요",
          "original_form": "그쵸ㅎ 오늘 점심은 맘에 들었어요",
          "speaker_id": "1",
          "time": "20210902 19:31"
        },
        {
          "id": "MDRW2100038345.1.6",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210902 19:31"
        },
        {
          "id": "MDRW2100038345.1.7",
          "form": "네 순두부찌개 먹었어요",
          "original_form": "네 순두부찌개 먹었어요",
          "speaker_id": "2",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100038345.1.8",
          "form": "오~ 맛있었겠네요",
          "original_form": "오~ 맛있었겠네요",
          "speaker_id": "1",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100038345.1.9",
          "form": "저는 밥먹고 만두구워먹었어요",
          "original_form": "저는 밥먹고 만두구워먹었어요",
          "speaker_id": "1",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100038345.1.10",
          "form": "비비고 만두요?",
          "original_form": "비비고 만두요?",
          "speaker_id": "2",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100038345.1.11",
          "form": "네!  맞아요ㅋㅋㄱ 드디어 구워먹었어요",
          "original_form": "네!  맞아요ㅋㅋㄱ 드디어 구워먹었어요",
          "speaker_id": "1",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100038345.1.12",
          "form": "너~~~무 맛있더라고요ㅠ",
          "original_form": "너~~~무 맛있더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100038345.1.13",
          "form": "ㅋㅋㅋ 군만두 생각하니 침 고이네요",
          "original_form": "ㅋㅋㅋ 군만두 생각하니 침 고이네요",
          "speaker_id": "2",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100038345.1.14",
          "form": "ㅋㅋ군만두는 증말 맛있는거같아요",
          "original_form": "ㅋㅋ군만두는 증말 맛있는거같아요",
          "speaker_id": "1",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100038345.1.15",
          "form": "살찌기 딱 좋지만요",
          "original_form": "살찌기 딱 좋지만요",
          "speaker_id": "1",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100038345.1.16",
          "form": "ㅋㅋㅋ 기름의 맛",
          "original_form": "ㅋㅋㅋ 기름의 맛",
          "speaker_id": "2",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100038345.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100038345.1.18",
          "form": "기름. 촉촉해야 더 맛잇는거같아요",
          "original_form": "기름. 촉촉해야 더 맛잇는거같아요",
