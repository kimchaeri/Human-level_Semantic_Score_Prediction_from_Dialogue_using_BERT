      "utterance": [
        {
          "id": "MDRW2100000991.1.1",
          "form": "제일 좋아하는 음료수가 뭐야?",
          "original_form": "제일 좋아하는 음료수가 뭐야?",
          "speaker_id": "2",
          "time": "20210820 11:22"
        },
        {
          "id": "MDRW2100000991.1.2",
          "form": "나는 자몽을 좋아해서",
          "original_form": "나는 자몽을 좋아해서",
          "speaker_id": "1",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100000991.1.3",
          "form": "자몽허니블랙티 좋아해",
          "original_form": "자몽허니블랙티 좋아해",
          "speaker_id": "1",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100000991.1.4",
          "form": "너는 무슨 음료 좋아해?",
          "original_form": "너는 무슨 음료 좋아해?",
          "speaker_id": "1",
          "time": "20210820 11:23"
        },
        {
          "id": "MDRW2100000991.1.5",
          "form": "난 음료를 많이 안마셔서 콜라? 정도",
          "original_form": "난 음료를 많이 안마셔서 콜라? 정도",
          "speaker_id": "2",
          "time": "20210820 11:24"
        },
        {
          "id": "MDRW2100000991.1.6",
          "form": "어떤 콜라 좋아해?",
          "original_form": "어떤 콜라 좋아해?",
          "speaker_id": "1",
          "time": "20210820 11:24"
        },
        {
          "id": "MDRW2100000991.1.7",
          "form": "코카 콜라 많이 마시는 편이야",
          "original_form": "코카 콜라 많이 마시는 편이야",
          "speaker_id": "2",
          "time": "20210820 11:25"
        },
        {
          "id": "MDRW2100000991.1.8",
          "form": "나는 펩시 마시는데",
          "original_form": "나는 펩시 마시는데",
          "speaker_id": "1",
          "time": "20210820 11:26"
        },
        {
          "id": "MDRW2100000991.1.9",
          "form": "아니 펩시보단 코카지",
          "original_form": "아니 펩시보단 코카지",
          "speaker_id": "2",
          "time": "20210820 11:26"
        },
        {
          "id": "MDRW2100000991.1.10",
          "form": "펩시가 달달해서 더 맛있어",
          "original_form": "펩시가 달달해서 더 맛있어",
          "speaker_id": "1",
          "time": "20210820 11:26"
        },
        {
          "id": "MDRW2100000991.1.11",
          "form": "뭘 모르네~ 코카만의 탄산 탁 쏘는 느낌!",
          "original_form": "뭘 모르네~ 코카만의 탄산 탁 쏘는 느낌!",
          "speaker_id": "2",
          "time": "20210820 11:26"
        },
        {
          "id": "MDRW2100000991.1.12",
          "form": "코카콜라 탄산 너무 세서 목아픔",
          "original_form": "코카콜라 탄산 너무 세서 목아픔",
          "speaker_id": "1",
          "time": "20210820 11:27"
        },
        {
          "id": "MDRW2100000991.1.13",
          "form": "그럴수도 있겠다",
          "original_form": "그럴수도 있겠다",
          "speaker_id": "2",
          "time": "20210820 11:27"
        },
        {
          "id": "MDRW2100000991.1.14",
          "form": "난 단걸 잘안먹어서 그런듯",
          "original_form": "난 단걸 잘안먹어서 그런듯",
          "speaker_id": "2",
          "time": "20210820 11:27"
        },
        {
          "id": "MDRW2100000991.1.15",
          "form": "달달한거 잘 안먹어?",
          "original_form": "달달한거 잘 안먹어?",
          "speaker_id": "1",
          "time": "20210820 11:27"
        },
        {
          "id": "MDRW2100000991.1.16",
          "form": "응 당땡길때 아니면 단건 안좋아해",
          "original_form": "응 당땡길때 아니면 단건 안좋아해",
          "speaker_id": "2",
          "time": "20210820 11:28"
        }
      ]
