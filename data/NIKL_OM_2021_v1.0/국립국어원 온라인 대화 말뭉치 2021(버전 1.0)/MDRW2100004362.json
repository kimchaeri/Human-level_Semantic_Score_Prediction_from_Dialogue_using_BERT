      "utterance": [
        {
          "id": "MDRW2100004362.1.1",
          "form": "안녕하세요 저는 취업준비생이에요",
          "original_form": "안녕하세요 저는 취업준비생이에요",
          "speaker_id": "1",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004362.1.2",
          "form": "안녕하세요 저는 아직 대학생이에요",
          "original_form": "안녕하세요 저는 아직 대학생이에요",
          "speaker_id": "2",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004362.1.3",
          "form": "어떤 쪽으로 취업 준비하시나요",
          "original_form": "어떤 쪽으로 취업 준비하시나요",
          "speaker_id": "2",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004362.1.4",
          "form": "저는 회계쪽으로 준비중이에요",
          "original_form": "저는 회계쪽으로 준비중이에요",
          "speaker_id": "1",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004362.1.5",
          "form": "혹시 전공이 어떻게 되세요?",
          "original_form": "혹시 전공이 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004362.1.6",
          "form": "저는 간호학과 재학 중이에요",
          "original_form": "저는 간호학과 재학 중이에요",
          "speaker_id": "2",
          "time": "20210821 16:10"
        },
        {
          "id": "MDRW2100004362.1.7",
          "form": "간호학과 취업률 높잖아요 부러워요",
          "original_form": "간호학과 취업률 높잖아요 부러워요",
          "speaker_id": "1",
          "time": "20210821 16:11"
        },
        {
          "id": "MDRW2100004362.1.8",
          "form": "간호학과 선택한 게 졸업 후 취업률 때문이었어요",
          "original_form": "간호학과 선택한 게 졸업 후 취업률 때문이었어요",
          "speaker_id": "2",
          "time": "20210821 16:12"
        },
        {
          "id": "MDRW2100004362.1.9",
          "form": "후회되는 것 같긴 하지만...",
          "original_form": "후회되는 것 같긴 하지만...",
          "speaker_id": "2",
          "time": "20210821 16:12"
        },
        {
          "id": "MDRW2100004362.1.10",
          "form": "그러면 좋아하시는 일은 뭐예요?",
          "original_form": "그러면 좋아하시는 일은 뭐예요?",
          "speaker_id": "1",
          "time": "20210821 16:12"
        },
        {
          "id": "MDRW2100004362.1.11",
          "form": "직업적으로 좋아하는 분야는 생각해 본 게 없어요",
          "original_form": "직업적으로 좋아하는 분야는 생각해 본 게 없어요",
          "speaker_id": "2",
          "time": "20210821 16:12"
        },
        {
          "id": "MDRW2100004362.1.12",
          "form": "꿈이 없다고 해야 하나",
          "original_form": "꿈이 없다고 해야 하나",
          "speaker_id": "2",
          "time": "20210821 16:12"
        },
        {
          "id": "MDRW2100004362.1.13",
          "form": "저두 마찬가지예요 여태 해온게 회계라서 이 직종을 선택했어요,,",
          "original_form": "저두 마찬가지예요 여태 해온게 회계라서 이 직종을 선택했어요,,",
          "speaker_id": "1",
          "time": "20210821 16:13"
        },
        {
          "id": "MDRW2100004362.1.14",
          "form": "회계쪽은 아예 모르는데 어떤 스펙이 필요한가요",
          "original_form": "회계쪽은 아예 모르는데 어떤 스펙이 필요한가요",
          "speaker_id": "2",
          "time": "20210821 16:14"
        },
        {
          "id": "MDRW2100004362.1.15",
          "form": "회계는 회계나 세무 자격증이 필요해요 급수는 높을수록 좋구요",
          "original_form": "회계는 회계나 세무 자격증이 필요해요 급수는 높을수록 좋구요",
          "speaker_id": "1",
          "time": "20210821 16:15"
        },
        {
          "id": "MDRW2100004362.1.16",
          "form": "엑셀이나 컴활은 기본이에요!",
          "original_form": "엑셀이나 컴활은 기본이에요!",
          "speaker_id": "1",
          "time": "20210821 16:15"
        },
        {
          "id": "MDRW2100004362.1.17",
          "form": "아하 간호는 엑셀 컴활은 보통 잘 안 따거든요",
          "original_form": "아하 간호는 엑셀 컴활은 보통 잘 안 따거든요",
          "speaker_id": "2",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004362.1.18",
          "form": "상위병원 가려면 토익 학점 정도?",
          "original_form": "상위병원 가려면 토익 학점 정도?",
          "speaker_id": "2",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004362.1.19",
          "form": "간호는 어떤 자격증이 필요한가요?",
          "original_form": "간호는 어떤 자격증이 필요한가요?",
          "speaker_id": "1",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004362.1.20",
          "form": "오 토익..",
          "original_form": "오 토익..",
          "speaker_id": "1",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004362.1.21",
          "form": "자격증은 졸업하면서 취득하는 간호사 국가고시 면허증만 필요한 경우가 많아요",
          "original_form": "자격증은 졸업하면서 취득하는 간호사 국가고시 면허증만 필요한 경우가 많아요",
          "speaker_id": "2",
          "time": "20210821 16:17"
        },
        {
          "id": "MDRW2100004362.1.22",
          "form": "졸업이 많이 힘든가요?",
          "original_form": "졸업이 많이 힘든가요?",
          "speaker_id": "1",
          "time": "20210821 16:17"
        },
        {
          "id": "MDRW2100004362.1.23",
          "form": "국가고시 합격률이 90퍼센트 이상이라는 거 보면 열심히 하는 만큼 결과가 나오는 거 같아요",
          "original_form": "국가고시 합격률이 90퍼센트 이상이라는 거 보면 열심히 하는 만큼 결과가 나오는 거 같아요",
          "speaker_id": "2",
          "time": "20210821 16:18"
        },
        {
          "id": "MDRW2100004362.1.24",
          "form": "합격률이 높네요! 혹시 아르바이트는 안하세요?",
          "original_form": "합격률이 높네요! 혹시 아르바이트는 안하세요?",
          "speaker_id": "1",
          "time": "20210821 16:19"
        },
        {
          "id": "MDRW2100004362.1.25",
          "form": "아르바이트 해보고 싶다는 생각만 해봤어요!",
          "original_form": "아르바이트 해보고 싶다는 생각만 해봤어요!",
          "speaker_id": "2",
          "time": "20210821 16:21"
        },
        {
          "id": "MDRW2100004362.1.26",
          "form": "핑계지만 실습과 학업과 일을 병행하기가 힘들 것 같아서",
          "original_form": "핑계지만 실습과 학업과 일을 병행하기가 힘들 것 같아서",
          "speaker_id": "2",
          "time": "20210821 16:21"
        },
        {
          "id": "MDRW2100004362.1.27",
          "form": "그냥 아르바이트 하는 친구들 얘기나 많이 들어보고 있어요",
          "original_form": "그냥 아르바이트 하는 친구들 얘기나 많이 들어보고 있어요",
          "speaker_id": "2",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004362.1.28",
          "form": "맞아요 공부와 일을 하는건 힘든 것 같아요ㅠㅠ",
          "original_form": "맞아요 공부와 일을 하는건 힘든 것 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 16:33"
        }
      ]
