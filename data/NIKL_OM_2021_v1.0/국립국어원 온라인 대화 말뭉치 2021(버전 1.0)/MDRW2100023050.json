      "utterance": [
        {
          "id": "MDRW2100023050.1.1",
          "form": "우리 아까 라면 먹을 때 같이 먹지",
          "original_form": "우리 아까 라면 먹을 때 같이 먹지",
          "speaker_id": "2",
          "time": "20210829 21:08"
        },
        {
          "id": "MDRW2100023050.1.2",
          "form": "그 때는 배불렀는데.. ㅠ",
          "original_form": "그 때는 배불렀는데.. ㅠ",
          "speaker_id": "1",
          "time": "20210829 21:09"
        },
        {
          "id": "MDRW2100023050.1.3",
          "form": "뭐라도 챙겨 먹어",
          "original_form": "뭐라도 챙겨 먹어",
          "speaker_id": "2",
          "time": "20210829 21:09"
        },
        {
          "id": "MDRW2100023050.1.4",
          "form": "갑자기 매콤한 쫄면이 먹고 싶네 ㅎㅎ",
          "original_form": "갑자기 매콤한 쫄면이 먹고 싶네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:09"
        },
        {
          "id": "MDRW2100023050.1.5",
          "form": "이런.. 집에 쫄면은 없는데",
          "original_form": "이런.. 집에 쫄면은 없는데",
          "speaker_id": "2",
          "time": "20210829 21:09"
        },
        {
          "id": "MDRW2100023050.1.6",
          "form": "시장 안에 있는 쫄면집 맛있는데",
          "original_form": "시장 안에 있는 쫄면집 맛있는데",
          "speaker_id": "1",
          "time": "20210829 21:09"
        },
        {
          "id": "MDRW2100023050.1.7",
          "form": "거긴 진짜 기본야채만 들어가는 데도 맛있어",
          "original_form": "거긴 진짜 기본야채만 들어가는 데도 맛있어",
          "speaker_id": "2",
          "time": "20210829 21:10"
        },
        {
          "id": "MDRW2100023050.1.8",
          "form": "양념장이 맛있어서 그런가봐 ㅎㅎ",
          "original_form": "양념장이 맛있어서 그런가봐 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:10"
        },
        {
          "id": "MDRW2100023050.1.9",
          "form": "콩나물도 아삭하고.. 근데 그 집은 쫄면 말고도 다 맛있더라",
          "original_form": "콩나물도 아삭하고.. 근데 그 집은 쫄면 말고도 다 맛있더라",
          "speaker_id": "2",
          "time": "20210829 21:10"
        },
        {
          "id": "MDRW2100023050.1.10",
          "form": "글치? 쫄면하고 나오는 국물마저 맛있음 ㅎㅎ",
          "original_form": "글치? 쫄면하고 나오는 국물마저 맛있음 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:11"
        },
        {
          "id": "MDRW2100023050.1.11",
          "form": "국물하고 김치 맛있는 집이 진정 맛집이지",
          "original_form": "국물하고 김치 맛있는 집이 진정 맛집이지",
          "speaker_id": "2",
          "time": "20210829 21:11"
        },
        {
          "id": "MDRW2100023050.1.12",
          "form": "낼 가서 사다 먹을까?",
          "original_form": "낼 가서 사다 먹을까?",
          "speaker_id": "1",
          "time": "20210829 21:11"
        },
        {
          "id": "MDRW2100023050.1.13",
          "form": "먹고 싶으면 먹어야지 ㅎㅎ",
          "original_form": "먹고 싶으면 먹어야지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:11"
        },
        {
          "id": "MDRW2100023050.1.14",
          "form": "시장 안까지 들어가기 좀 귀찮긴 한데",
          "original_form": "시장 안까지 들어가기 좀 귀찮긴 한데",
          "speaker_id": "1",
          "time": "20210829 21:11"
        },
        {
          "id": "MDRW2100023050.1.15",
          "form": "거기 쫄면이랑 김밥도 진짜 맛있는데",
          "original_form": "거기 쫄면이랑 김밥도 진짜 맛있는데",
          "speaker_id": "1",
          "time": "20210829 21:13"
        }
