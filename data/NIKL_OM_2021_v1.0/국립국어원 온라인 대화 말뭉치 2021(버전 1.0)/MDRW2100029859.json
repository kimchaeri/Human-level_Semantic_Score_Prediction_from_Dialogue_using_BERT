      "utterance": [
        {
          "id": "MDRW2100029859.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210903 15:30"
        },
        {
          "id": "MDRW2100029859.1.2",
          "form": "보는건 좋아하는데 키우진 못해요ㅠㅠ 여건마련이 쉽지않더라구요",
          "original_form": "보는건 좋아하는데 키우진 못해요ㅠㅠ 여건마련이 쉽지않더라구요",
          "speaker_id": "2",
          "time": "20210903 15:56"
        },
        {
          "id": "MDRW2100029859.1.3",
          "form": "전 구피 키워요",
          "original_form": "전 구피 키워요",
          "speaker_id": "1",
          "time": "20210903 16:43"
        },
        {
          "id": "MDRW2100029859.1.4",
          "form": "아 물고기키우시는 군요",
          "original_form": "아 물고기키우시는 군요",
          "speaker_id": "2",
          "time": "20210903 16:45"
        },
        {
          "id": "MDRW2100029859.1.5",
          "form": "어떤기회로 키우게되셨어요?",
          "original_form": "어떤기회로 키우게되셨어요?",
          "speaker_id": "2",
          "time": "20210903 16:45"
        },
        {
          "id": "MDRW2100029859.1.6",
          "form": "선물받아서요",
          "original_form": "선물받아서요",
          "speaker_id": "1",
          "time": "20210903 16:49"
        },
        {
          "id": "MDRW2100029859.1.7",
          "form": "아하 물고기 선물이라! 흔치 않네요",
          "original_form": "아하 물고기 선물이라! 흔치 않네요",
          "speaker_id": "2",
          "time": "20210903 16:50"
        },
        {
          "id": "MDRW2100029859.1.8",
          "form": "네 아무래도 ㅋㅋ",
          "original_form": "네 아무래도 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100029859.1.9",
          "form": "구피 돌보기는 어렵나요?",
          "original_form": "구피 돌보기는 어렵나요?",
          "speaker_id": "2",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100029859.1.10",
          "form": "물관리 힘들죠~",
          "original_form": "물관리 힘들죠~",
          "speaker_id": "1",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100029859.1.11",
          "form": "그쵸 쉽지않을거같어요",
          "original_form": "그쵸 쉽지않을거같어요",
          "speaker_id": "2",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100029859.1.12",
          "form": "이끼가 잘 생겨서 관리가 어려워요",
          "original_form": "이끼가 잘 생겨서 관리가 어려워요",
          "speaker_id": "1",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100029859.1.13",
          "form": "배식은 어떻개하세요?",
          "original_form": "배식은 어떻개하세요?",
          "speaker_id": "2",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100029859.1.14",
          "form": "걍주면 알아서 먹던대요",
          "original_form": "걍주면 알아서 먹던대요",
          "speaker_id": "1",
          "time": "20210903 17:45"
        }
