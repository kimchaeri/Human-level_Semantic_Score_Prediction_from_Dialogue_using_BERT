      "utterance": [
        {
          "id": "MDRW2100005483.1.1",
          "form": "가장 못생긴 동물도 보여주네 ㅋㅋ",
          "original_form": "가장 못생긴 동물도 보여주네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005483.1.2",
          "form": "코주부 긴코 원숭이",
          "original_form": "코주부 긴코 원숭이",
          "speaker_id": "2",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005483.1.3",
          "form": "그때 못생긴 개 콘테스트 1 위 보여줄거 같은데",
          "original_form": "그때 못생긴 개 콘테스트 1 위 보여줄거 같은데",
          "speaker_id": "1",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005483.1.4",
          "form": "ㅋㅋㅋ 했던거 특집으로 보여주고 하네",
          "original_form": "ㅋㅋㅋ 했던거 특집으로 보여주고 하네",
          "speaker_id": "2",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005483.1.5",
          "form": "또 뭐 보여 주는데",
          "original_form": "또 뭐 보여 주는데",
          "speaker_id": "1",
          "time": "20210821 22:39"
        },
        {
          "id": "MDRW2100005483.1.6",
          "form": "올해 북극에서 바다코끼리가 빙하위에서 잠들어서 영국까지 흘러왔단다",
          "original_form": "올해 북극에서 바다코끼리가 빙하위에서 잠들어서 영국까지 흘러왔단다",
          "speaker_id": "2",
          "time": "20210821 22:39"
        },
        {
          "id": "MDRW2100005483.1.7",
          "form": "아 그게 가능하네ㅋ",
          "original_form": "아 그게 가능하네ㅋ",
          "speaker_id": "1",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005483.1.8",
          "form": "북극으로 안가고 6월에 스페인에서도 발견",
          "original_form": "북극으로 안가고 6월에 스페인에서도 발견",
          "speaker_id": "2",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005483.1.9",
          "form": "빙하도 녹고 있다던데 그런 동물 많을듯",
          "original_form": "빙하도 녹고 있다던데 그런 동물 많을듯",
          "speaker_id": "1",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005483.1.10",
          "form": "혼자 잇으면 외로울껀데",
          "original_form": "혼자 잇으면 외로울껀데",
          "speaker_id": "2",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005483.1.11",
          "form": "동물원에 보내질거 같다",
          "original_form": "동물원에 보내질거 같다",
          "speaker_id": "1",
          "time": "20210821 22:43"
        },
        {
          "id": "MDRW2100005483.1.12",
          "form": "동물원이든 북극이든 보내야할듯 온도가 올라간 바다에 있으면 위험하니",
          "original_form": "동물원이든 북극이든 보내야할듯 온도가 올라간 바다에 있으면 위험하니",
          "speaker_id": "2",
          "time": "20210821 22:44"
        },
        {
          "id": "MDRW2100005483.1.13",
          "form": "지구온난화로 동물들도 불쌍하네",
          "original_form": "지구온난화로 동물들도 불쌍하네",
          "speaker_id": "1",
          "time": "20210821 22:45"
        },
        {
          "id": "MDRW2100005483.1.14",
          "form": "그러게 거기다가 쓰 레 기 때문에 고생도 하고",
          "original_form": "그러게 거기다가 쓰 레 기 때문에 고생도 하고",
          "speaker_id": "2",
          "time": "20210821 22:45"
        },
        {
          "id": "MDRW2100005483.1.15",
          "form": "다 인간들이 문제다",
          "original_form": "다 인간들이 문제다",
          "speaker_id": "1",
          "time": "20210821 22:46"
        }
      ]
