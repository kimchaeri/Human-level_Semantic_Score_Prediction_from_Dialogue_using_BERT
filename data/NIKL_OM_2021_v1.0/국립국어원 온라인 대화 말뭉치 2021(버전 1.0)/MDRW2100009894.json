      "utterance": [
        {
          "id": "MDRW2100009894.1.1",
          "form": "아니요 동물 키우세요",
          "original_form": "아니요 동물 키우세요",
          "speaker_id": "2",
          "time": "20210823 11:25"
        },
        {
          "id": "MDRW2100009894.1.2",
          "form": "저도 아직",
          "original_form": "저도 아직",
          "speaker_id": "1",
          "time": "20210823 11:38"
        },
        {
          "id": "MDRW2100009894.1.3",
          "form": "키우고 싶은 동물은 있으세요",
          "original_form": "키우고 싶은 동물은 있으세요",
          "speaker_id": "2",
          "time": "20210823 11:38"
        },
        {
          "id": "MDRW2100009894.1.4",
          "form": "강아지요",
          "original_form": "강아지요",
          "speaker_id": "1",
          "time": "20210823 14:20"
        },
        {
          "id": "MDRW2100009894.1.5",
          "form": "저도 푸들이 귀엽더라고요",
          "original_form": "저도 푸들이 귀엽더라고요",
          "speaker_id": "2",
          "time": "20210823 14:25"
        },
        {
          "id": "MDRW2100009894.1.6",
          "form": "악 귀요미",
          "original_form": "악 귀요미",
          "speaker_id": "1",
          "time": "20210823 14:26"
        },
        {
          "id": "MDRW2100009894.1.7",
          "form": "고양이는 털때문에",
          "original_form": "고양이는 털때문에",
          "speaker_id": "1",
          "time": "20210901 07:26"
        },
        {
          "id": "MDRW2100009894.1.8",
          "form": "장모인 애들이 더 털이 안빠진대요",
          "original_form": "장모인 애들이 더 털이 안빠진대요",
          "speaker_id": "2",
          "time": "20210901 07:37"
        },
        {
          "id": "MDRW2100009894.1.9",
          "form": "아 그런가요",
          "original_form": "아 그런가요",
          "speaker_id": "1",
          "time": "20210901 07:52"
        },
        {
          "id": "MDRW2100009894.1.10",
          "form": "네 단모는 강아지든 고양이든 털이 공중에 흩날린대요 ㅋㅋ",
          "original_form": "네 단모는 강아지든 고양이든 털이 공중에 흩날린대요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 07:58"
        },
        {
          "id": "MDRW2100009894.1.11",
          "form": "흰털이 대박이죠",
          "original_form": "흰털이 대박이죠",
          "speaker_id": "1",
          "time": "20210901 08:02"
        },
        {
          "id": "MDRW2100009894.1.12",
          "form": "흰강아지가 그래도 귀신쫓아준다고하더라고요 ㅋ",
          "original_form": "흰강아지가 그래도 귀신쫓아준다고하더라고요 ㅋ",
          "speaker_id": "2",
          "time": "20210901 09:35"
        },
        {
          "id": "MDRW2100009894.1.13",
          "form": "삽살개처럼?",
          "original_form": "삽살개처럼?",
          "speaker_id": "2",
          "time": "20210901 09:35"
        },
        {
          "id": "MDRW2100009894.1.14",
          "form": "그런가요",
          "original_form": "그런가요",
          "speaker_id": "1",
          "time": "20210901 09:36"
        },
        {
          "id": "MDRW2100009894.1.15",
          "form": "유투브 구독하시는거 있으세요?",
          "original_form": "유투브 구독하시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210901 09:38"
        },
        {
          "id": "MDRW2100009894.1.16",
          "form": "요새랜선집사도 많은데",
          "original_form": "요새랜선집사도 많은데",
          "speaker_id": "2",
          "time": "20210901 09:38"
        },
        {
          "id": "MDRW2100009894.1.17",
          "form": "구독보다는 찾아보죠",
          "original_form": "구독보다는 찾아보죠",
          "speaker_id": "1",
          "time": "20210901 09:57"
        }
      ]
