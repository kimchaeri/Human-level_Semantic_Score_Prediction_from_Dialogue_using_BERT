      "utterance": [
        {
          "id": "MDRW2100029207.1.1",
          "form": "백신 맞으셨어요?",
          "original_form": "백신 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100029207.1.2",
          "form": "백신 맞으셨어요?",
          "original_form": "백신 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029207.1.3",
          "form": "예약했는데 조금 늦게 해서 아직 1차도 못맞았네요 ㅎㅎ",
          "original_form": "예약했는데 조금 늦게 해서 아직 1차도 못맞았네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029207.1.4",
          "form": "아 저는 일차 맞았어요",
          "original_form": "아 저는 일차 맞았어요",
          "speaker_id": "1",
          "time": "20210903 00:20"
        },
        {
          "id": "MDRW2100029207.1.5",
          "form": "오 맞으시고 어떠셨나요 천차만별인 것 같더라고요",
          "original_form": "오 맞으시고 어떠셨나요 천차만별인 것 같더라고요",
          "speaker_id": "2",
          "time": "20210903 00:20"
        },
        {
          "id": "MDRW2100029207.1.6",
          "form": "팔 좀 뻐근한거빼고 괜찮았어요 ㅎ",
          "original_form": "팔 좀 뻐근한거빼고 괜찮았어요 ㅎ",
          "speaker_id": "1",
          "time": "20210903 00:21"
        },
        {
          "id": "MDRW2100029207.1.7",
          "form": "백신 어떤거 맞으실 예정이세요?",
          "original_form": "백신 어떤거 맞으실 예정이세요?",
          "speaker_id": "1",
          "time": "20210903 00:21"
        },
        {
          "id": "MDRW2100029207.1.8",
          "form": "다행이네요!",
          "original_form": "다행이네요!",
          "speaker_id": "2",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029207.1.9",
          "form": "저 예약할 땐 모더나나 화이자라고 하던데 확정됐는진 모르겠네요 ㅋㅋ",
          "original_form": "저 예약할 땐 모더나나 화이자라고 하던데 확정됐는진 모르겠네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029207.1.10",
          "form": "그쵸 저도 걱정했는데 괜찮더라구요",
          "original_form": "그쵸 저도 걱정했는데 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210903 00:22"
        },
        {
          "id": "MDRW2100029207.1.11",
          "form": "아마도 화이자일것 같긴해요",
          "original_form": "아마도 화이자일것 같긴해요",
          "speaker_id": "1",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029207.1.12",
          "form": "모더나 수급 어렵다하더라구요",
          "original_form": "모더나 수급 어렵다하더라구요",
          "speaker_id": "1",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029207.1.13",
          "form": "아 어쩐지 화이자 얘기만 많이 들리던데 그렇군요",
          "original_form": "아 어쩐지 화이자 얘기만 많이 들리던데 그렇군요",
          "speaker_id": "2",
          "time": "20210903 00:23"
        },
        {
          "id": "MDRW2100029207.1.14",
          "form": "저도 화이자였어요 ㅎㅎ",
          "original_form": "저도 화이자였어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100029207.1.15",
          "form": "나이가 됐으면 그냥 아스트라제네카를 잔여로 잡아서 맞았겠는데",
          "original_form": "나이가 됐으면 그냥 아스트라제네카를 잔여로 잡아서 맞았겠는데",
          "speaker_id": "2",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100029207.1.16",
          "form": "아 저도 나이때문에",
          "original_form": "아 저도 나이때문에",
          "speaker_id": "1",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100029207.1.17",
          "form": "아스트라 안되더라구요",
          "original_form": "아스트라 안되더라구요",
          "speaker_id": "1",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100029207.1.18",
          "form": "군필도 아니라 얀센도 안되고 ㅎㅎ",
          "original_form": "군필도 아니라 얀센도 안되고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 00:25"
        },
        {
          "id": "MDRW2100029207.1.19",
          "form": "초기에 잔여백신 접종 시작할 때 탐냈는데...",
          "original_form": "초기에 잔여백신 접종 시작할 때 탐냈는데...",
          "speaker_id": "2",
          "time": "20210903 00:25"
        },
        {
          "id": "MDRW2100029207.1.20",
          "form": "요즘도 아스트라제네카는 잔여 있어서 ㅋㅋ",
          "original_form": "요즘도 아스트라제네카는 잔여 있어서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:25"
        },
        {
          "id": "MDRW2100029207.1.21",
          "form": "초기엔 완전 순삭이였죠",
          "original_form": "초기엔 완전 순삭이였죠",
          "speaker_id": "1",
          "time": "20210903 00:25"
        }
      ]
    }
