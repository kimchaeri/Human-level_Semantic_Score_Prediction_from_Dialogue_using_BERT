        },
        {
          "id": "MDRW2100006601.1.2",
          "form": "네 운동하시나요",
          "original_form": "네 운동하시나요",
          "speaker_id": "2",
          "time": "20210908 03:06"
        },
        {
          "id": "MDRW2100006601.1.3",
          "form": "ㅋㅋ네 가끔 홈트",
          "original_form": "ㅋㅋ네 가끔 홈트",
          "speaker_id": "1",
          "time": "20210908 03:07"
        },
        {
          "id": "MDRW2100006601.1.4",
          "form": "운동하세오?",
          "original_form": "운동하세오?",
          "speaker_id": "1",
          "time": "20210908 03:07"
        },
        {
          "id": "MDRW2100006601.1.5",
          "form": "네 저는 헬스요",
          "original_form": "네 저는 헬스요",
          "speaker_id": "2",
          "time": "20210908 03:07"
        },
        {
          "id": "MDRW2100006601.1.6",
          "form": "아!",
          "original_form": "아!",
          "speaker_id": "1",
          "time": "20210908 03:08"
        },
        {
          "id": "MDRW2100006601.1.7",
          "form": "매일가세요?",
          "original_form": "매일가세요?",
          "speaker_id": "1",
          "time": "20210908 03:09"
        },
        {
          "id": "MDRW2100006601.1.8",
          "form": "마음은 그런데ㅜ애들 재우다 같이 잠들우요",
          "original_form": "마음은 그런데ㅜ애들 재우다 같이 잠들우요",
          "speaker_id": "2",
          "time": "20210908 03:10"
        },
        {
          "id": "MDRW2100006601.1.9",
          "form": "ㅜㅋㅋ",
          "original_form": "ㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:10"
        },
        {
          "id": "MDRW2100006601.1.10",
          "form": "좋은 아버님",
          "original_form": "좋은 아버님",
          "speaker_id": "1",
          "time": "20210908 03:10"
        },
        {
          "id": "MDRW2100006601.1.11",
          "form": "절로 근육생기시길!!!",
          "original_form": "절로 근육생기시길!!!",
          "speaker_id": "1",
          "time": "20210908 03:10"
        },
        {
          "id": "MDRW2100006601.1.12",
          "form": "ㅎㅎ 응원감사합니다 홈트는 꾸준히 하시나요",
          "original_form": "ㅎㅎ 응원감사합니다 홈트는 꾸준히 하시나요",
          "speaker_id": "2",
          "time": "20210908 03:11"
        },
        {
          "id": "MDRW2100006601.1.13",
          "form": "주 1~3회ㅎㅎ",
          "original_form": "주 1~3회ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 03:11"
        },
        {
          "id": "MDRW2100006601.1.14",
          "form": "넘 적죠ㅜ",
          "original_form": "넘 적죠ㅜ",
          "speaker_id": "1",
          "time": "20210908 03:12"
        },
        {
          "id": "MDRW2100006601.1.15",
          "form": "그래도 하고 있다는게 중요하지요",
          "original_form": "그래도 하고 있다는게 중요하지요",
          "speaker_id": "2",
          "time": "20210908 03:12"
        },
        {
          "id": "MDRW2100006601.1.16",
          "form": "ㅋㅋ 헬스 피티는 안하시죠?",
          "original_form": "ㅋㅋ 헬스 피티는 안하시죠?",
