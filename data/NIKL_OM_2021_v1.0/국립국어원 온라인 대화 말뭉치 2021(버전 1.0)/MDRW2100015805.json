      "utterance": [
        {
          "id": "MDRW2100015805.1.1",
          "form": "반려동물 있으세요?",
          "original_form": "반려동물 있으세요?",
          "speaker_id": "2",
          "time": "20210826 01:21"
        },
        {
          "id": "MDRW2100015805.1.2",
          "form": "아니요 name2님은요",
          "original_form": "아니요 &name2&님은요",
          "speaker_id": "1",
          "time": "20210826 01:22"
        },
        {
          "id": "MDRW2100015805.1.3",
          "form": "저두여ㅋㅋ키우고는 싶은데",
          "original_form": "저두여ㅋㅋ키우고는 싶은데",
          "speaker_id": "2",
          "time": "20210826 01:23"
        },
        {
          "id": "MDRW2100015805.1.4",
          "form": "이별생각하면ㅜㅜ",
          "original_form": "이별생각하면ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 01:23"
        },
        {
          "id": "MDRW2100015805.1.5",
          "form": "저도요ㅜ",
          "original_form": "저도요ㅜ",
          "speaker_id": "1",
          "time": "20210826 01:24"
        },
        {
          "id": "MDRW2100015805.1.6",
          "form": "한번도 키운적 없으신가요?",
          "original_form": "한번도 키운적 없으신가요?",
          "speaker_id": "1",
          "time": "20210826 01:25"
        },
        {
          "id": "MDRW2100015805.1.7",
          "form": "네 지금은 물고기 키우고 있어요ㅋㅋ",
          "original_form": "네 지금은 물고기 키우고 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:25"
        },
        {
          "id": "MDRW2100015805.1.8",
          "form": "물고기 이름이 뭔가요?",
          "original_form": "물고기 이름이 뭔가요?",
          "speaker_id": "1",
          "time": "20210826 01:27"
        },
        {
          "id": "MDRW2100015805.1.9",
          "form": "name3이요ㅋㅋ",
          "original_form": "&name3&이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:27"
        },
        {
          "id": "MDRW2100015805.1.10",
          "form": "인디언복어라서ㅋㅋ",
          "original_form": "인디언복어라서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:28"
        },
        {
          "id": "MDRW2100015805.1.11",
          "form": "복복이",
          "original_form": "복복이",
          "speaker_id": "2",
          "time": "20210826 01:28"
        },
        {
          "id": "MDRW2100015805.1.12",
          "form": "오!!",
          "original_form": "오!!",
          "speaker_id": "1",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100015805.1.13",
          "form": "인디언 복어 처음들어봐쇼",
          "original_form": "인디언 복어 처음들어봐쇼",
          "speaker_id": "1",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100015805.1.14",
          "form": "봐요",
          "original_form": "봐요",
          "speaker_id": "1",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100015805.1.15",
          "form": "엄청 귀여워요ㅋㅋ",
          "original_form": "엄청 귀여워요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100015805.1.16",
          "form": "눈이 동글동글해서",
          "original_form": "눈이 동글동글해서",
          "speaker_id": "2",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100015805.1.17",
          "form": "가까이가면 막 쳐다봐요",
          "original_form": "가까이가면 막 쳐다봐요",
          "speaker_id": "2",
          "time": "20210826 01:30"
        },
        {
          "id": "MDRW2100015805.1.18",
          "form": "지금 검색해봤는데",
          "original_form": "지금 검색해봤는데",
          "speaker_id": "1",
          "time": "20210826 01:30"
        },
        {
          "id": "MDRW2100015805.1.19",
          "form": "엄청 귀엽네요ㅋㅋ",
          "original_form": "엄청 귀엽네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:30"
        },
        {
          "id": "MDRW2100015805.1.20",
          "form": "귀엽긴한데 다른 친구들을 괴롭혀요ㅜㅜ",
          "original_form": "귀엽긴한데 다른 친구들을 괴롭혀요ㅜㅜ",
