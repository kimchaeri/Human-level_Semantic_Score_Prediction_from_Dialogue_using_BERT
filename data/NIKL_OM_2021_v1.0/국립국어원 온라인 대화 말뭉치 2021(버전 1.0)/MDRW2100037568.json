      "utterance": [
        {
          "id": "MDRW2100037568.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210901 11:57"
        },
        {
          "id": "MDRW2100037568.1.2",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "2",
          "time": "20210901 12:07"
        },
        {
          "id": "MDRW2100037568.1.3",
          "form": "좋아하는 영화 있으세요?",
          "original_form": "좋아하는 영화 있으세요?",
          "speaker_id": "1",
          "time": "20210901 13:13"
        },
        {
          "id": "MDRW2100037568.1.4",
          "form": "가끔 보는 편이라ㅎ",
          "original_form": "가끔 보는 편이라ㅎ",
          "speaker_id": "2",
          "time": "20210901 13:31"
        },
        {
          "id": "MDRW2100037568.1.5",
          "form": "세 얼간이는 재밌어서 많이 봤어요",
          "original_form": "세 얼간이는 재밌어서 많이 봤어요",
          "speaker_id": "2",
          "time": "20210901 13:32"
        },
        {
          "id": "MDRW2100037568.1.6",
          "form": "저도 어렸을 때 여러 번 봤어요 학교에서 선생님들이 자주 보여주시더라고요",
          "original_form": "저도 어렸을 때 여러 번 봤어요 학교에서 선생님들이 자주 보여주시더라고요",
          "speaker_id": "1",
          "time": "20210901 14:01"
        },
        {
          "id": "MDRW2100037568.1.7",
          "form": "코믹한 장면도 많고 노래도 재밌고요",
          "original_form": "코믹한 장면도 많고 노래도 재밌고요",
          "speaker_id": "2",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100037568.1.8",
          "form": "좋아하시는 영화 있어요?",
          "original_form": "좋아하시는 영화 있어요?",
          "speaker_id": "2",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100037568.1.9",
          "form": "전 독립영화 좋아해요",
          "original_form": "전 독립영화 좋아해요",
          "speaker_id": "1",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100037568.1.10",
          "form": "재밌게 본 거 있나요?",
          "original_form": "재밌게 본 거 있나요?",
          "speaker_id": "2",
          "time": "20210901 14:05"
        },
        {
          "id": "MDRW2100037568.1.11",
          "form": "야구소녀랑 은희에게요",
          "original_form": "야구소녀랑 은희에게요",
          "speaker_id": "1",
          "time": "20210901 14:05"
        },
        {
          "id": "MDRW2100037568.1.12",
          "form": "김희애님 나오는거요?",
          "original_form": "김희애님 나오는거요?",
          "speaker_id": "2",
          "time": "20210901 14:09"
        },
        {
          "id": "MDRW2100037568.1.13",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210901 16:44"
        },
        {
          "id": "MDRW2100037568.1.14",
          "form": "예전에 추천받아서 본 적 있어요.",
          "original_form": "예전에 추천받아서 본 적 있어요.",
          "speaker_id": "2",
          "time": "20210901 16:48"
        },
        {
          "id": "MDRW2100037568.1.15",
          "form": "좀 먹먹해지기도 하고 재밌게 봤네요",
          "original_form": "좀 먹먹해지기도 하고 재밌게 봤네요",
          "speaker_id": "2",
          "time": "20210901 16:49"
        },
        {
          "id": "MDRW2100037568.1.16",
          "form": "그쵸 마지막 나레이션까지 버릴 부분이 없어요",
          "original_form": "그쵸 마지막 나레이션까지 버릴 부분이 없어요",
          "speaker_id": "1",
          "time": "20210901 16:54"
        }
