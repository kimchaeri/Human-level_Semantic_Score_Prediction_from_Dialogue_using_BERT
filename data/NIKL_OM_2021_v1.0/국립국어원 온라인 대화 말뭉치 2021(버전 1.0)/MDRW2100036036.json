      "utterance": [
        {
          "id": "MDRW2100036036.1.1",
          "form": "점심 드셧나용",
          "original_form": "점심 드셧나용",
          "speaker_id": "2",
          "time": "20210830 14:45"
        },
        {
          "id": "MDRW2100036036.1.2",
          "form": "오늘은 점심뭐드셨나요",
          "original_form": "오늘은 점심뭐드셨나요",
          "speaker_id": "1",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100036036.1.3",
          "form": "삼계탕 먹었습니덩",
          "original_form": "삼계탕 먹었습니덩",
          "speaker_id": "2",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100036036.1.4",
          "form": "전 두유먹고말았슴니다",
          "original_form": "전 두유먹고말았슴니다",
          "speaker_id": "1",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100036036.1.5",
          "form": "삼계탕 오오",
          "original_form": "삼계탕 오오",
          "speaker_id": "1",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100036036.1.6",
          "form": "영양사님이 늦여름 몸보신 시켜주시네용",
          "original_form": "영양사님이 늦여름 몸보신 시켜주시네용",
          "speaker_id": "2",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100036036.1.7",
          "form": "거기닭잘주네요",
          "original_form": "거기닭잘주네요",
          "speaker_id": "1",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100036036.1.8",
          "form": "더운 여름 돌아다니려묜 든든히 드셔야죵",
          "original_form": "더운 여름 돌아다니려묜 든든히 드셔야죵",
          "speaker_id": "2",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100036036.1.9",
          "form": "닭이 제일 저렴한걸까요",
          "original_form": "닭이 제일 저렴한걸까요",
          "speaker_id": "2",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100036036.1.10",
          "form": "소고기는 줄 수 없으니까",
          "original_form": "소고기는 줄 수 없으니까",
          "speaker_id": "2",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100036036.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:47"
        },
        {
          "id": "MDRW2100036036.1.12",
          "form": "두유 초코맛이에용?",
          "original_form": "두유 초코맛이에용?",
          "speaker_id": "2",
          "time": "20210830 14:47"
        },
        {
          "id": "MDRW2100036036.1.13",
          "form": "소고기는줄수없으니까ㅋㅋㅋㄱ현실적이네요",
          "original_form": "소고기는줄수없으니까ㅋㅋㅋㄱ현실적이네요",
          "speaker_id": "1",
          "time": "20210830 14:47"
        },
        {
          "id": "MDRW2100036036.1.14",
          "form": "고단백맛이예용",
          "original_form": "고단백맛이예용",
          "speaker_id": "1",
          "time": "20210830 14:47"
        },
        {
          "id": "MDRW2100036036.1.15",
          "form": "이제썬업과두유질려가요",
          "original_form": "이제썬업과두유질려가요",
          "speaker_id": "1",
          "time": "20210830 14:48"
        },
        {
          "id": "MDRW2100036036.1.16",
          "form": "더안사려구요",
          "original_form": "더안사려구요",
          "speaker_id": "1",
          "time": "20210830 14:48"
        },
        {
          "id": "MDRW2100036036.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:50"
        },
        {
          "id": "MDRW2100036036.1.18",
          "form": "이제 인생 할당량 끝났군영",
          "original_form": "이제 인생 할당량 끝났군영",
          "speaker_id": "2",
          "time": "20210830 14:50"
        },
        {
          "id": "MDRW2100036036.1.19",
          "form": "ㅋㄲㄱㅋ아 맞다 저인생할당량있엇죠",
          "original_form": "ㅋㄲㄱㅋ아 맞다 저인생할당량있엇죠",
          "speaker_id": "1",
          "time": "20210830 14:52"
        },
        {
          "id": "MDRW2100036036.1.20",
          "form": "네 이제한계인거같아요",
          "original_form": "네 이제한계인거같아요",
          "speaker_id": "1",
          "time": "20210830 14:52"
        },
        {
          "id": "MDRW2100036036.1.21",
          "form": "보내주려구요",
          "original_form": "보내주려구요",
          "speaker_id": "1",
          "time": "20210830 14:52"
        },
        {
          "id": "MDRW2100036036.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:56"
        },
        {
          "id": "MDRW2100036036.1.23",
          "form": "그동안 수고했어 두유랑 썬업~~",
          "original_form": "그동안 수고했어 두유랑 썬업~~",
          "speaker_id": "2",
          "time": "20210830 14:56"
        },
        {
          "id": "MDRW2100036036.1.24",
          "form": "건강하게 잘 살아야한다~",
          "original_form": "건강하게 잘 살아야한다~",
          "speaker_id": "2",
          "time": "20210830 14:56"
        },
        {
          "id": "MDRW2100036036.1.25",
          "form": "ㅋㄲㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㄲㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100036036.1.26",
          "form": "진짜오래된친구긴했네요",
          "original_form": "진짜오래된친구긴했네요",
          "speaker_id": "1",
          "time": "20210830 14:58"
        },
        {
          "id": "MDRW2100036036.1.27",
          "form": "인사해줄만해요",
          "original_form": "인사해줄만해요",
          "speaker_id": "1",
          "time": "20210830 14:58"
        },
        {
          "id": "MDRW2100036036.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 15:03"
        },
        {
          "id": "MDRW2100036036.1.29",
          "form": "이제 다음 음료는 무엇이 될까용",
          "original_form": "이제 다음 음료는 무엇이 될까용",
          "speaker_id": "2",
          "time": "20210830 15:15"
        },
        {
          "id": "MDRW2100036036.1.30",
          "form": "ㅋㅋㅋㅋㅋ그러게요다음음료는무엇이돤까요",
