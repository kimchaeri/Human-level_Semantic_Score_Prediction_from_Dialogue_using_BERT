      "utterance": [
        {
          "id": "MDRW2100032976.1.1",
          "form": "어젯밤엔 비가 새차게 내렸어요",
          "original_form": "어젯밤엔 비가 새차게 내렸어요",
          "speaker_id": "1",
          "time": "20210906 11:33"
        },
        {
          "id": "MDRW2100032976.1.2",
          "form": "이곳은 날씨가 맑아요",
          "original_form": "이곳은 날씨가 맑아요",
          "speaker_id": "2",
          "time": "20210906 11:33"
        },
        {
          "id": "MDRW2100032976.1.3",
          "form": "요즘은 비가 너무 자주와요",
          "original_form": "요즘은 비가 너무 자주와요",
          "speaker_id": "1",
          "time": "20210906 11:33"
        },
        {
          "id": "MDRW2100032976.1.4",
          "form": "가을에는 비가 많이 내린다고 해요",
          "original_form": "가을에는 비가 많이 내린다고 해요",
          "speaker_id": "2",
          "time": "20210906 11:33"
        },
        {
          "id": "MDRW2100032976.1.5",
          "form": "여름에 안오더니 가을엔 비랑 태풍이 오려나봅니다",
          "original_form": "여름에 안오더니 가을엔 비랑 태풍이 오려나봅니다",
          "speaker_id": "1",
          "time": "20210906 11:34"
        },
        {
          "id": "MDRW2100032976.1.6",
          "form": "비가 오늘 날씨가 싫은가요",
          "original_form": "비가 오늘 날씨가 싫은가요",
          "speaker_id": "2",
          "time": "20210906 11:34"
        },
        {
          "id": "MDRW2100032976.1.7",
          "form": "적당히 오면 좋은데 밤에 자다가 깼어요",
          "original_form": "적당히 오면 좋은데 밤에 자다가 깼어요",
          "speaker_id": "1",
          "time": "20210906 11:34"
        },
        {
          "id": "MDRW2100032976.1.8",
          "form": "오늘은 날씨가 맑다고 해요",
          "original_form": "오늘은 날씨가 맑다고 해요",
          "speaker_id": "2",
          "time": "20210906 11:34"
        },
        {
          "id": "MDRW2100032976.1.9",
          "form": "선선하고 맑은 날씨에는 산책가기에 좋지요",
          "original_form": "선선하고 맑은 날씨에는 산책가기에 좋지요",
          "speaker_id": "1",
          "time": "20210906 11:34"
        },
        {
          "id": "MDRW2100032976.1.10",
          "form": "저녁에는 산책 가려고요",
          "original_form": "저녁에는 산책 가려고요",
          "speaker_id": "2",
          "time": "20210906 11:35"
        },
        {
          "id": "MDRW2100032976.1.11",
          "form": "이제는 쌀쌀해서 긴팔을 입어야해요",
          "original_form": "이제는 쌀쌀해서 긴팔을 입어야해요",
          "speaker_id": "1",
          "time": "20210906 11:35"
        },
        {
          "id": "MDRW2100032976.1.12",
          "form": "여름이 뭐를 넣어야겠어요",
          "original_form": "여름이 뭐를 넣어야겠어요",
          "speaker_id": "2",
          "time": "20210906 11:35"
        },
        {
          "id": "MDRW2100032976.1.13",
          "form": "따뜻한 스웨터도 구입해야겠네요",
          "original_form": "따뜻한 스웨터도 구입해야겠네요",
          "speaker_id": "1",
          "time": "20210906 11:35"
        },
        {
          "id": "MDRW2100032976.1.14",
          "form": "가을에는 드라이브 가기에 좋은 날씨에요",
          "original_form": "가을에는 드라이브 가기에 좋은 날씨에요",
