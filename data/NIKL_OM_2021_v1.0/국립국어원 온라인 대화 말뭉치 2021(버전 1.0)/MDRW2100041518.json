      "utterance": [
        {
          "id": "MDRW2100041518.1.1",
          "form": "건강을 위해 먹는 영양제 있나요?",
          "original_form": "건강을 위해 먹는 영양제 있나요?",
          "speaker_id": "1",
          "time": "20210906 08:38"
        },
        {
          "id": "MDRW2100041518.1.2",
          "form": "비타민b랑 유산균이랑 \r\n있는데 안챙겨먹어요ㅠㅋㅋ",
          "original_form": "비타민b랑 유산균이랑 \r\n있는데 안챙겨먹어요ㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 10:02"
        },
        {
          "id": "MDRW2100041518.1.3",
          "form": "챙겨먹어야죠ㅠ",
          "original_form": "챙겨먹어야죠ㅠ",
          "speaker_id": "1",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100041518.1.4",
          "form": "코시국엔 면역력 잘 챙겨야 해요",
          "original_form": "코시국엔 면역력 잘 챙겨야 해요",
          "speaker_id": "1",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100041518.1.5",
          "form": "뭐 챙겨드시고 계세요?",
          "original_form": "뭐 챙겨드시고 계세요?",
          "speaker_id": "2",
          "time": "20210906 17:11"
        },
        {
          "id": "MDRW2100041518.1.6",
          "form": "저는 일단 뼈건강을 위해 비타민 디랑 칼슘은 전부터 먹었구요",
          "original_form": "저는 일단 뼈건강을 위해 비타민 디랑 칼슘은 전부터 먹었구요",
          "speaker_id": "1",
          "time": "20210906 19:39"
        },
        {
          "id": "MDRW2100041518.1.7",
          "form": "요즘에 감기 예방을 위해 비타민 씨를 먹기 시작했어요",
          "original_form": "요즘에 감기 예방을 위해 비타민 씨를 먹기 시작했어요",
          "speaker_id": "1",
          "time": "20210906 19:39"
        },
        {
          "id": "MDRW2100041518.1.8",
          "form": "그리고 숙면에 도움이 된다고 해서 마그네슘도 시켰어요",
          "original_form": "그리고 숙면에 도움이 된다고 해서 마그네슘도 시켰어요",
          "speaker_id": "1",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100041518.1.9",
          "form": "오! 많이 챙겨드시네요",
          "original_form": "오! 많이 챙겨드시네요",
          "speaker_id": "2",
          "time": "20210906 19:43"
        },
        {
          "id": "MDRW2100041518.1.10",
          "form": "주로 언제 드시나요?",
          "original_form": "주로 언제 드시나요?",
          "speaker_id": "2",
          "time": "20210906 19:44"
        },
        {
          "id": "MDRW2100041518.1.11",
          "form": "대부분 자기 전에 먹고",
          "original_form": "대부분 자기 전에 먹고",
          "speaker_id": "1",
          "time": "20210906 19:47"
        },
        {
          "id": "MDRW2100041518.1.12",
          "form": "비타민씨는 점심때 먹어요",
          "original_form": "비타민씨는 점심때 먹어요",
          "speaker_id": "1",
          "time": "20210906 19:47"
        },
        {
          "id": "MDRW2100041518.1.13",
          "form": "알람같은거 해두세요?  :)",
          "original_form": "알람같은거 해두세요?  :)",
          "speaker_id": "2",
          "time": "20210906 19:49"
        },
        {
          "id": "MDRW2100041518.1.14",
          "form": "저는 왜케 안먹어질까요ㅜㅜ",
          "original_form": "저는 왜케 안먹어질까요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 19:49"
        },
        {
          "id": "MDRW2100041518.1.15",
          "form": "아니요 사실 딸이 챙겨줘요 ㅎㅎㅎㅎ",
          "original_form": "아니요 사실 딸이 챙겨줘요 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 19:51"
        },
        {
          "id": "MDRW2100041518.1.16",
          "form": "저도 자꾸 잊어버리는데 옆에서 딸이 먹으라고 알려줘요",
          "original_form": "저도 자꾸 잊어버리는데 옆에서 딸이 먹으라고 알려줘요",
          "speaker_id": "1",
          "time": "20210906 19:51"
        },
        {
          "id": "MDRW2100041518.1.17",
          "form": "와 ",
          "original_form": "와 ",
          "speaker_id": "2",
          "time": "20210906 19:52"
        },
        {
          "id": "MDRW2100041518.1.18",
          "form": "딸이 그런건 정말 잘 챙겨요",
          "original_form": "딸이 그런건 정말 잘 챙겨요",
          "speaker_id": "1",
          "time": "20210906 19:54"
        },
        {
          "id": "MDRW2100041518.1.19",
          "form": "신기하게 엄마 아빠 뭐 해야하는지 비서처럼 잘 알려줘서 편해요",
          "original_form": "신기하게 엄마 아빠 뭐 해야하는지 비서처럼 잘 알려줘서 편해요",
          "speaker_id": "1",
          "time": "20210906 19:55"
        },
        {
          "id": "MDRW2100041518.1.20",
          "form": "저는 아들들만 있어서\r\n제 건강은 제가 챙겨야겠네영ㅋ",
          "original_form": "저는 아들들만 있어서\r\n제 건강은 제가 챙겨야겠네영ㅋ",
          "speaker_id": "2",
          "time": "20210906 20:11"
        },
        {
          "id": "MDRW2100041518.1.21",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210906 20:15"
        },
        {
          "id": "MDRW2100041518.1.22",
          "form": "근데 진짜 영양제는 잘 챙겨먹어야 효과를 본다네요",
          "original_form": "근데 진짜 영양제는 잘 챙겨먹어야 효과를 본다네요",
          "speaker_id": "1",
          "time": "20210906 20:16"
        },
        {
          "id": "MDRW2100041518.1.23",
          "form": "어차피 비싼 영양제 샀으니까 잘 챙겨드세요",
          "original_form": "어차피 비싼 영양제 샀으니까 잘 챙겨드세요",
          "speaker_id": "1",
          "time": "20210906 20:16"
        }
      ]
    }
