      "utterance": [
        {
          "id": "MDRW2100009595.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210823 01:16"
        },
        {
          "id": "MDRW2100009595.1.2",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "2",
          "time": "20210823 01:17"
        },
        {
          "id": "MDRW2100009595.1.3",
          "form": "좋아하는 음식 있으신가용",
          "original_form": "좋아하는 음식 있으신가용",
          "speaker_id": "1",
          "time": "20210823 01:17"
        },
        {
          "id": "MDRW2100009595.1.4",
          "form": "저 피자 좋아해요",
          "original_form": "저 피자 좋아해요",
          "speaker_id": "2",
          "time": "20210823 01:17"
        },
        {
          "id": "MDRW2100009595.1.5",
          "form": "좋아하는 음식 있나요",
          "original_form": "좋아하는 음식 있나요",
          "speaker_id": "2",
          "time": "20210823 01:18"
        },
        {
          "id": "MDRW2100009595.1.6",
          "form": "저도 피자에 고구마무스 진짜 좋아해여",
          "original_form": "저도 피자에 고구마무스 진짜 좋아해여",
          "speaker_id": "1",
          "time": "20210823 01:18"
        },
        {
          "id": "MDRW2100009595.1.7",
          "form": "저는 치킨이랑 면요리 조아해요",
          "original_form": "저는 치킨이랑 면요리 조아해요",
          "speaker_id": "1",
          "time": "20210823 01:18"
        },
        {
          "id": "MDRW2100009595.1.8",
          "form": "고구마무스는 진리죠",
          "original_form": "고구마무스는 진리죠",
          "speaker_id": "2",
          "time": "20210823 01:19"
        },
        {
          "id": "MDRW2100009595.1.9",
          "form": "치킨은 후라이드 좋아하시나요 양념좋아하시나요",
          "original_form": "치킨은 후라이드 좋아하시나요 양념좋아하시나요",
          "speaker_id": "2",
          "time": "20210823 01:19"
        },
        {
          "id": "MDRW2100009595.1.10",
          "form": "후라이드여!! 양념은 달달해서 금방 물리더라구요ㅜ",
          "original_form": "후라이드여!! 양념은 달달해서 금방 물리더라구요ㅜ",
          "speaker_id": "1",
          "time": "20210823 01:19"
        },
        {
          "id": "MDRW2100009595.1.11",
          "form": "요즘은 푸라닭 자주먹어요",
          "original_form": "요즘은 푸라닭 자주먹어요",
          "speaker_id": "1",
          "time": "20210823 01:20"
        },
        {
          "id": "MDRW2100009595.1.12",
          "form": "역시 치킨의 근본은 후라이드에요 그쵸",
          "original_form": "역시 치킨의 근본은 후라이드에요 그쵸",
          "speaker_id": "2",
          "time": "20210823 01:20"
        },
        {
          "id": "MDRW2100009595.1.13",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 01:20"
        },
        {
          "id": "MDRW2100009595.1.14",
          "form": "푸라닭 어때요 저는 집근처에 푸라닭이 없어서",
          "original_form": "푸라닭 어때요 저는 집근처에 푸라닭이 없어서",
          "speaker_id": "2",
          "time": "20210823 01:20"
        },
        {
          "id": "MDRW2100009595.1.15",
          "form": "저는 투움바 블랙알리오 고.추마요 먹어봤는데 다 맛있었어요",
          "original_form": "저는 투움바 블랙알리오 고.추마요 먹어봤는데 다 맛있었어요",
          "speaker_id": "1",
          "time": "20210823 01:22"
        },
        {
          "id": "MDRW2100009595.1.16",
          "form": "순살이 부드러워서 좋았어용",
          "original_form": "순살이 부드러워서 좋았어용",
          "speaker_id": "1",
          "time": "20210823 01:22"
        },
        {
          "id": "MDRW2100009595.1.17",
          "form": "기회가 되면 한 번 먹어봐야겠어요",
          "original_form": "기회가 되면 한 번 먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210823 01:23"
        },
        {
          "id": "MDRW2100009595.1.18",
          "form": "블락알리오 이름만들어도 맛있는 느낌",
          "original_form": "블락알리오 이름만들어도 맛있는 느낌",
          "speaker_id": "2",
          "time": "20210823 01:23"
        },
        {
          "id": "MDRW2100009595.1.19",
          "form": "맞아요 맛있었어요 ㅜ.ㅜ 이거얘기하니깐 또먹고싶어지네영",
          "original_form": "맞아요 맛있었어요 ㅜ.ㅜ 이거얘기하니깐 또먹고싶어지네영",
          "speaker_id": "1",
          "time": "20210823 01:25"
        }
      ]
