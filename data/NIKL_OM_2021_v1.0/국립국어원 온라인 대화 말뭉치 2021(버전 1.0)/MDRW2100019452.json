      "utterance": [
        {
          "id": "MDRW2100019452.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210827 19:26"
        },
        {
          "id": "MDRW2100019452.1.2",
          "form": "최근에 영화나 공연 관람 해봤어요?",
          "original_form": "최근에 영화나 공연 관람 해봤어요?",
          "speaker_id": "1",
          "time": "20210827 19:26"
        },
        {
          "id": "MDRW2100019452.1.3",
          "form": "안녕하세요 저 최근에 루카 봤어요 아세요?",
          "original_form": "안녕하세요 저 최근에 루카 봤어요 아세요?",
          "speaker_id": "2",
          "time": "20210827 19:27"
        },
        {
          "id": "MDRW2100019452.1.4",
          "form": "루카는 애니매이션이였던 것 같은데.. 맞나요?",
          "original_form": "루카는 애니매이션이였던 것 같은데.. 맞나요?",
          "speaker_id": "1",
          "time": "20210827 19:27"
        },
        {
          "id": "MDRW2100019452.1.5",
          "form": "네 맞아요! 애니메이션 별론가요?ㅎㅎ",
          "original_form": "네 맞아요! 애니메이션 별론가요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:28"
        },
        {
          "id": "MDRW2100019452.1.6",
          "form": "저 애니매이션 별로 안좋아해요 ㅠㅠ",
          "original_form": "저 애니매이션 별로 안좋아해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 19:28"
        },
        {
          "id": "MDRW2100019452.1.7",
          "form": "그래도 디즈니에 나오는 건 종종 챙겨봐요~",
          "original_form": "그래도 디즈니에 나오는 건 종종 챙겨봐요~",
          "speaker_id": "1",
          "time": "20210827 19:28"
        },
        {
          "id": "MDRW2100019452.1.8",
          "form": "그러시구나 그럴수있죠~ name1님은 영화 최근에 보신거 있어요?",
          "original_form": "그러시구나 그럴수있죠~ &name1&님은 영화 최근에 보신거 있어요?",
          "speaker_id": "2",
          "time": "20210827 19:29"
        },
        {
          "id": "MDRW2100019452.1.9",
          "form": "저 최근에 한국영화 두가지 봤어요!",
          "original_form": "저 최근에 한국영화 두가지 봤어요!",
          "speaker_id": "1",
          "time": "20210827 19:29"
        },
        {
          "id": "MDRW2100019452.1.10",
          "form": "뭐뭐 보셨어요?",
          "original_form": "뭐뭐 보셨어요?",
          "speaker_id": "2",
          "time": "20210827 19:30"
        },
        {
          "id": "MDRW2100019452.1.11",
          "form": "인질, 씽크홀 이렇게 두가지 봤어요!",
          "original_form": "인질, 씽크홀 이렇게 두가지 봤어요!",
          "speaker_id": "1",
          "time": "20210827 19:32"
        },
        {
          "id": "MDRW2100019452.1.12",
          "form": "어느 배우가 나오는 지 혹시 아세요?",
          "original_form": "어느 배우가 나오는 지 혹시 아세요?",
          "speaker_id": "1",
          "time": "20210827 19:32"
        },
        {
          "id": "MDRW2100019452.1.13",
          "form": "인질 황정민배우님 맞나요?",
          "original_form": "인질 황정민배우님 맞나요?",
          "speaker_id": "2",
          "time": "20210827 19:32"
        },
        {
          "id": "MDRW2100019452.1.14",
          "form": "씽크홀은 차승원님이랑 김성균님",
          "original_form": "씽크홀은 차승원님이랑 김성균님",
          "speaker_id": "2",
          "time": "20210827 19:33"
        },
        {
          "id": "MDRW2100019452.1.15",
          "form": "맞아요?",
          "original_form": "맞아요?",
          "speaker_id": "2",
          "time": "20210827 19:33"
        },
        {
          "id": "MDRW2100019452.1.16",
          "form": "배우 호칭을 일부러 뺀건아니고 아리송해서 쓰다보니까 빠졌네요 ㅋㅋㅋ",
          "original_form": "배우 호칭을 일부러 뺀건아니고 아리송해서 쓰다보니까 빠졌네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:33"
        },
        {
          "id": "MDRW2100019452.1.17",
          "form": "헐 맞아요 대박!",
          "original_form": "헐 맞아요 대박!",
          "speaker_id": "1",
          "time": "20210827 19:34"
        },
        {
          "id": "MDRW2100019452.1.18",
          "form": "완전 잘 알고 계시네요~ 전 누구 나오는 지도 모르고 봤어요.",
          "original_form": "완전 잘 알고 계시네요~ 전 누구 나오는 지도 모르고 봤어요.",
          "speaker_id": "1",
          "time": "20210827 19:34"
        },
        {
          "id": "MDRW2100019452.1.19",
          "form": "요새 영화관에서 할인행사 엄청 하더라구요!",
          "original_form": "요새 영화관에서 할인행사 엄청 하더라구요!",
          "speaker_id": "1",
          "time": "20210827 19:35"
        },
        {
          "id": "MDRW2100019452.1.20",
          "form": "그래요? 저 보고 싶어서 예매하려고 했는데 비싸서 그냥 말았거든요 ㅠ",
          "original_form": "그래요? 저 보고 싶어서 예매하려고 했는데 비싸서 그냥 말았거든요 ㅠ",
          "speaker_id": "2",
          "time": "20210827 19:36"
        },
        {
          "id": "MDRW2100019452.1.21",
          "form": "어디서  할인해요?",
          "original_form": "어디서  할인해요?",
          "speaker_id": "2",
          "time": "20210827 19:36"
        },
        {
          "id": "MDRW2100019452.1.22",
          "form": "근데 보통 선착순으로 쿠폰 뿌려서요.. ㅜㅜ",
          "original_form": "근데 보통 선착순으로 쿠폰 뿌려서요.. ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 19:37"
        },
        {
          "id": "MDRW2100019452.1.23",
          "form": "그리고 씨제이 어플 가니까 씨지비 7천원 쿠폰 들어있었어요~",
          "original_form": "그리고 씨제이 어플 가니까 씨지비 7천원 쿠폰 들어있었어요~",
          "speaker_id": "1",
          "time": "20210827 19:37"
        },
        {
          "id": "MDRW2100019452.1.24",
          "form": "아!",
          "original_form": "아!",
          "speaker_id": "2",
          "time": "20210827 19:38"
        },
        {
          "id": "MDRW2100019452.1.25",
          "form": "ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 19:38"
        },
        {
          "id": "MDRW2100019452.1.26",
          "form": "코로나19 상황이라서 요새 영화관에 사람도 없더라구요",
          "original_form": "코로나19 상황이라서 요새 영화관에 사람도 없더라구요",
          "speaker_id": "1",
          "time": "20210827 19:38"
        },
        {
          "id": "MDRW2100019452.1.27",
          "form": "아쉽네요",
          "original_form": "아쉽네요",
          "speaker_id": "2",
          "time": "20210827 19:38"
        },
        {
          "id": "MDRW2100019452.1.28",
          "form": "그렇구나...",
          "original_form": "그렇구나...",
          "speaker_id": "2",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100019452.1.29",
          "form": "인질이랑 싱크홀중에는 뭐가 더 재밌었어요?",
          "original_form": "인질이랑 싱크홀중에는 뭐가 더 재밌었어요?",
          "speaker_id": "2",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100019452.1.30",
          "form": "저는 싱크홀이 더 재밌었어요.",
          "original_form": "저는 싱크홀이 더 재밌었어요.",
          "speaker_id": "1",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019452.1.31",
          "form": "인질은 약간 개연성이 없는 듯한 느낌을 받아서 ㅜㅜ",
          "original_form": "인질은 약간 개연성이 없는 듯한 느낌을 받아서 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019452.1.32",
          "form": "그래도 황정민 배우님이 연기를 잘해주셔서 그건 좋았어요!",
          "original_form": "그래도 황정민 배우님이 연기를 잘해주셔서 그건 좋았어요!",
          "speaker_id": "1",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019452.1.33",
          "form": "역시 믿고보는 배우",
          "original_form": "역시 믿고보는 배우",
          "speaker_id": "2",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019452.1.34",
          "form": "싱크홀은 어때요?",
          "original_form": "싱크홀은 어때요?",
          "speaker_id": "2",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019452.1.35",
          "form": "막 나중에",
          "original_form": "막 나중에",
          "speaker_id": "2",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100019452.1.36",
          "form": "눈물나난요?",
          "original_form": "눈물나난요?",
          "speaker_id": "2",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100019452.1.37",
          "form": "싱크홀은 감동스토리는 아니었던 것 같은데...ㅋㅋ",
          "original_form": "싱크홀은 감동스토리는 아니었던 것 같은데...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100019452.1.38",
          "form": "그냥 재난블록버스터로 킬링타임용이에요~",
          "original_form": "그냥 재난블록버스터로 킬링타임용이에요~",
          "speaker_id": "1",
          "time": "20210827 19:41"
        }
      ]
