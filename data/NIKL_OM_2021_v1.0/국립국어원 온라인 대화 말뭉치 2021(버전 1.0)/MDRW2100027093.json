      "utterance": [
        {
          "id": "MDRW2100027093.1.1",
          "form": "반려동물 키우세요??",
          "original_form": "반려동물 키우세요??",
          "speaker_id": "1",
          "time": "20210902 09:23"
        },
        {
          "id": "MDRW2100027093.1.2",
          "form": "아뇨 ㅎㅎ name1은요?",
          "original_form": "아뇨 ㅎㅎ &name1&은요?",
          "speaker_id": "2",
          "time": "20210902 09:51"
        },
        {
          "id": "MDRW2100027093.1.3",
          "form": "저도 없어요...ㅎㅎㅎ",
          "original_form": "저도 없어요...ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 10:04"
        },
        {
          "id": "MDRW2100027093.1.4",
          "form": "동물은 좋아하세요??",
          "original_form": "동물은 좋아하세요??",
          "speaker_id": "1",
          "time": "20210902 10:04"
        },
        {
          "id": "MDRW2100027093.1.5",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210902 10:08"
        },
        {
          "id": "MDRW2100027093.1.6",
          "form": "애기는 좋아합니다ㅋㅋㅋ",
          "original_form": "애기는 좋아합니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:08"
        },
        {
          "id": "MDRW2100027093.1.7",
          "form": "애기 너무 귀엽죠ㅎㅎㅎ",
          "original_form": "애기 너무 귀엽죠ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 10:21"
        },
        {
          "id": "MDRW2100027093.1.8",
          "form": "동물 알러지 있나요??",
          "original_form": "동물 알러지 있나요??",
          "speaker_id": "1",
          "time": "20210902 10:22"
        },
        {
          "id": "MDRW2100027093.1.9",
          "form": "아뇨 그건 아닌데ㅋㅋ 동물<애기라서",
          "original_form": "아뇨 그건 아닌데ㅋㅋ 동물<애기라서",
          "speaker_id": "2",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027093.1.10",
          "form": "이제 애기 키워야 할 나이라 ㅋㅋ",
          "original_form": "이제 애기 키워야 할 나이라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027093.1.11",
          "form": "아 맞아 애기랑 동물 같이 키우기는 그렇지!",
          "original_form": "아 맞아 애기랑 동물 같이 키우기는 그렇지!",
          "speaker_id": "1",
          "time": "20210902 10:52"
        },
        {
          "id": "MDRW2100027093.1.12",
          "form": "네ㅋㅋ 나중에 크면 좀 생각해봐야겠으요",
          "original_form": "네ㅋㅋ 나중에 크면 좀 생각해봐야겠으요",
          "speaker_id": "2",
          "time": "20210902 11:06"
        },
        {
          "id": "MDRW2100027093.1.13",
          "form": "맞아요 어느정도 컸을때 키우면 좋은거 같더라구요~",
          "original_form": "맞아요 어느정도 컸을때 키우면 좋은거 같더라구요~",
          "speaker_id": "1",
          "time": "20210902 11:35"
        },
        {
          "id": "MDRW2100027093.1.14",
          "form": "넵 ㅋㅋ 그렇게 해야지요",
          "original_form": "넵 ㅋㅋ 그렇게 해야지요",
          "speaker_id": "2",
          "time": "20210902 12:54"
        },
        {
          "id": "MDRW2100027093.1.15",
          "form": "털때문에 지금은 좀 ㅎㅎ",
          "original_form": "털때문에 지금은 좀 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 12:54"
        },
        {
          "id": "MDRW2100027093.1.16",
          "form": "강아지가 좋으세요 고양이가 좋으세요??",
          "original_form": "강아지가 좋으세요 고양이가 좋으세요??",
          "speaker_id": "1",
          "time": "20210902 13:01"
        },
        {
          "id": "MDRW2100027093.1.17",
          "form": "전 강아지요",
          "original_form": "전 강아지요",
          "speaker_id": "2",
          "time": "20210902 13:04"
        },
        {
          "id": "MDRW2100027093.1.18",
          "form": "고양이는 날 아래로 보는거 같아요",
          "original_form": "고양이는 날 아래로 보는거 같아요",
          "speaker_id": "2",
          "time": "20210902 13:04"
        },
        {
          "id": "MDRW2100027093.1.19",
          "form": "ㅋㅋㅋㅋ저도 고양이보다 강아지 더 좋아하는거같아요",
          "original_form": "ㅋㅋㅋㅋ저도 고양이보다 강아지 더 좋아하는거같아요",
