      "utterance": [
        {
          "id": "MDRW2100031365.1.1",
          "form": "요즘 가을이라 회가 땡겨요",
          "original_form": "요즘 가을이라 회가 땡겨요",
          "speaker_id": "1",
          "time": "20210904 20:48"
        },
        {
          "id": "MDRW2100031365.1.2",
          "form": "회 좋아하세요?",
          "original_form": "회 좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 20:48"
        },
        {
          "id": "MDRW2100031365.1.3",
          "form": "네 엄청이요",
          "original_form": "네 엄청이요",
          "speaker_id": "1",
          "time": "20210904 20:48"
        },
        {
          "id": "MDRW2100031365.1.4",
          "form": "저도 좋아해요",
          "original_form": "저도 좋아해요",
          "speaker_id": "2",
          "time": "20210904 20:48"
        },
        {
          "id": "MDRW2100031365.1.5",
          "form": "무슨 회 좋아하세요?",
          "original_form": "무슨 회 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100031365.1.6",
          "form": "가을에는 전어가 맛있어요",
          "original_form": "가을에는 전어가 맛있어요",
          "speaker_id": "2",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100031365.1.7",
          "form": "아 뼈째 오독오독 씹어먹는 그맛",
          "original_form": "아 뼈째 오독오독 씹어먹는 그맛",
          "speaker_id": "1",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100031365.1.8",
          "form": "드실줄 아시네요 ㅎㅎ",
          "original_form": "드실줄 아시네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100031365.1.9",
          "form": "초장에 야채랑 버무려서 먹으면 꿀맛이죠",
          "original_form": "초장에 야채랑 버무려서 먹으면 꿀맛이죠",
          "speaker_id": "1",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100031365.1.10",
          "form": "이시기에는 확실히 전어가 제일 고소하고 맛있는 거 같아요",
          "original_form": "이시기에는 확실히 전어가 제일 고소하고 맛있는 거 같아요",
          "speaker_id": "2",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100031365.1.11",
          "form": "아 내일이나 언제 배달시켜먹어야겠어요ㅠㅠ 너무 먹고싶네요",
          "original_form": "아 내일이나 언제 배달시켜먹어야겠어요ㅠㅠ 너무 먹고싶네요",
          "speaker_id": "1",
          "time": "20210904 20:49"
        },
        {
          "id": "MDRW2100031365.1.12",
          "form": "요즘 회도 배달이 되나봐요",
          "original_form": "요즘 회도 배달이 되나봐요",
          "speaker_id": "2",
          "time": "20210904 20:50"
        },
        {
          "id": "MDRW2100031365.1.13",
          "form": "그럼요 요즘 배달 안되는게 없어요",
          "original_form": "그럼요 요즘 배달 안되는게 없어요",
          "speaker_id": "1",
