      "utterance": [
        {
          "id": "MDRW2100038300.1.1",
          "form": "흐흐",
          "original_form": "흐흐",
          "speaker_id": "1",
          "time": "20210902 09:08"
        },
        {
          "id": "MDRW2100038300.1.2",
          "form": "오늘 수강신청 하나 하고...",
          "original_form": "오늘 수강신청 하나 하고...",
          "speaker_id": "1",
          "time": "20210902 09:08"
        },
        {
          "id": "MDRW2100038300.1.3",
          "form": "아 맞다!",
          "original_form": "아 맞다!",
          "speaker_id": "2",
          "time": "20210902 09:08"
        },
        {
          "id": "MDRW2100038300.1.4",
          "form": "네넨 언니 오늘 실험도 하실건가요??",
          "original_form": "네넨 언니 오늘 실험도 하실건가요??",
          "speaker_id": "2",
          "time": "20210902 09:08"
        },
        {
          "id": "MDRW2100038300.1.5",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210902 09:08"
        },
        {
          "id": "MDRW2100038300.1.6",
          "form": "일단 오늘 오전에 교수님한테",
          "original_form": "일단 오늘 오전에 교수님한테",
          "speaker_id": "1",
          "time": "20210902 09:09"
        },
        {
          "id": "MDRW2100038300.1.7",
          "form": "이렇게 실험하려고 하는데 괜찮을지",
          "original_form": "이렇게 실험하려고 하는데 괜찮을지",
          "speaker_id": "1",
          "time": "20210902 09:09"
        },
        {
          "id": "MDRW2100038300.1.8",
          "form": "종이에 적어둔거 보여드리려구요",
          "original_form": "종이에 적어둔거 보여드리려구요",
          "speaker_id": "1",
          "time": "20210902 09:09"
        },
        {
          "id": "MDRW2100038300.1.9",
          "form": "그 김에 개별 과제 연구 싸인도 받는게 좋으려나...",
          "original_form": "그 김에 개별 과제 연구 싸인도 받는게 좋으려나...",
          "speaker_id": "1",
          "time": "20210902 09:09"
        },
        {
          "id": "MDRW2100038300.1.10",
          "form": "저도!! 그거랑 출입권한도 싸인받게요!",
          "original_form": "저도!! 그거랑 출입권한도 싸인받게요!",
          "speaker_id": "2",
          "time": "20210902 09:32"
        },
        {
          "id": "MDRW2100038300.1.11",
          "form": "오오 좋네요!!",
          "original_form": "오오 좋네요!!",
          "speaker_id": "1",
          "time": "20210902 09:33"
        },
        {
          "id": "MDRW2100038300.1.12",
          "form": "저는 그럼 가자마자",
          "original_form": "저는 그럼 가자마자",
          "speaker_id": "1",
          "time": "20210902 09:33"
        },
        {
          "id": "MDRW2100038300.1.13",
          "form": "수강신청을 하고... 인쇄를 해야겠어요!",
          "original_form": "수강신청을 하고... 인쇄를 해야겠어요!",
          "speaker_id": "1",
          "time": "20210902 09:33"
        },
        {
          "id": "MDRW2100038300.1.14",
          "form": "저도요 ㅎㅎㅎ",
          "original_form": "저도요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100038300.1.15",
          "form": "저는 아직 2호선이랍니다~",
          "original_form": "저는 아직 2호선이랍니다~",
          "speaker_id": "1",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100038300.1.16",
          "form": "을지로역...",
          "original_form": "을지로역...",
          "speaker_id": "1",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100038300.1.17",
          "form": "벌써 목요일이에요!",
          "original_form": "벌써 목요일이에요!",
          "speaker_id": "2",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100038300.1.18",
          "form": "그러게요... 시간이 진짜 빨라요",
          "original_form": "그러게요... 시간이 진짜 빨라요",
          "speaker_id": "1",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100038300.1.19",
          "form": "오 ! ㄱ거의 다오셨네요!!",
          "original_form": "오 ! ㄱ거의 다오셨네요!!",
          "speaker_id": "2",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100038300.1.20",
          "form": "다음주에는 랩미팅 안했으면 좋겠는데!!!",
          "original_form": "다음주에는 랩미팅 안했으면 좋겠는데!!!",
          "speaker_id": "1",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100038300.1.21",
          "form": "헤헤 네네",
          "original_form": "헤헤 네네",
          "speaker_id": "1",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100038300.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ저도요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저도요",
          "speaker_id": "2",
          "time": "20210902 09:35"
        },
        {
          "id": "MDRW2100038300.1.23",
          "form": "랩미팅 한달에 한번... 했으면좋겠어요",
          "original_form": "랩미팅 한달에 한번... 했으면좋겠어요",
          "speaker_id": "2",
          "time": "20210902 09:35"
        },
        {
          "id": "MDRW2100038300.1.24",
          "form": "그쵸.... 그러면 그만큼 뭔가 잔뜩 준비해야할 것 같긴 하지만 ㅜㅠㅠㅠㅠㅠ 흐아아아",
          "original_form": "그쵸.... 그러면 그만큼 뭔가 잔뜩 준비해야할 것 같긴 하지만 ㅜㅠㅠㅠㅠㅠ 흐아아아",
