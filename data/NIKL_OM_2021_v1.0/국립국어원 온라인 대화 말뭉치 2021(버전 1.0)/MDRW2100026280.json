          "speaker_id": "1",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100026280.1.5",
          "form": "어제 너무너무너무 많이와서",
          "original_form": "어제 너무너무너무 많이와서",
          "speaker_id": "1",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100026280.1.6",
          "form": "오..",
          "original_form": "오..",
          "speaker_id": "2",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100026280.1.7",
          "form": "박에안나감",
          "original_form": "박에안나감",
          "speaker_id": "1",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100026280.1.8",
          "form": "오늘은?",
          "original_form": "오늘은?",
          "speaker_id": "2",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100026280.1.9",
          "form": "오늘도 박에안나감",
          "original_form": "오늘도 박에안나감",
          "speaker_id": "1",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100026280.1.10",
          "form": "날씨는?",
          "original_form": "날씨는?",
          "speaker_id": "2",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026280.1.11",
          "form": "비가 안옴",
          "original_form": "비가 안옴",
          "speaker_id": "1",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026280.1.12",
          "form": "그렇군.",
          "original_form": "그렇군.",
          "speaker_id": "2",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026280.1.13",
          "form": "날시 기억이 안나요",
          "original_form": "날시 기억이 안나요",
          "speaker_id": "1",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026280.1.14",
          "form": "덥나요",
          "original_form": "덥나요",
          "speaker_id": "2",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026280.1.15",
          "form": "여긴 오늘은 안 더운데",
          "original_form": "여긴 오늘은 안 더운데",
          "speaker_id": "2",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026280.1.16",
          "form": "습해졌다",
          "original_form": "습해졌다",
          "speaker_id": "2",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026280.1.17",
          "form": "덥지 않아요",
          "original_form": "덥지 않아요",
          "speaker_id": "1",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026280.1.18",
          "form": "하루종일 이불덮고 잠",
          "original_form": "하루종일 이불덮고 잠",
          "speaker_id": "1",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026280.1.19",
          "form": "낮에도",
          "original_form": "낮에도",
          "speaker_id": "1",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026280.1.20",
          "form": "솜이불 덮고 자기 가능이었음",
