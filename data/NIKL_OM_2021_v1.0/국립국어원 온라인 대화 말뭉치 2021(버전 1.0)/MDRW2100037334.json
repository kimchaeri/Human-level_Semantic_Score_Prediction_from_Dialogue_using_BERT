      "utterance": [
        {
          "id": "MDRW2100037334.1.1",
          "form": "코로나 백신 접종 하셨어요?",
          "original_form": "코로나 백신 접종 하셨어요?",
          "speaker_id": "1",
          "time": "20210831 22:41"
        },
        {
          "id": "MDRW2100037334.1.2",
          "form": "아직이요 접종 하셨나요?",
          "original_form": "아직이요 접종 하셨나요?",
          "speaker_id": "2",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100037334.1.3",
          "form": "저도 아직이요. 예약해놨는데 아직도 2주 남았어요",
          "original_form": "저도 아직이요. 예약해놨는데 아직도 2주 남았어요",
          "speaker_id": "1",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100037334.1.4",
          "form": "그래도 빠르게 예약하셨나봐요",
          "original_form": "그래도 빠르게 예약하셨나봐요",
          "speaker_id": "2",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100037334.1.5",
          "form": "생일 날짜 맞춰 들어갔는데 생각보다 너무 쉽게 됐어요ㅎㅎ 17분쯤 기억나서 들어갔는데도",
          "original_form": "생일 날짜 맞춰 들어갔는데 생각보다 너무 쉽게 됐어요ㅎㅎ 17분쯤 기억나서 들어갔는데도",
          "speaker_id": "1",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100037334.1.6",
          "form": "제일 빠른 게 한달후더라구요",
          "original_form": "제일 빠른 게 한달후더라구요",
          "speaker_id": "1",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100037334.1.7",
          "form": "그렇군요 그럼 지금까지 격리 해보신 적 있으신가요?",
          "original_form": "그렇군요 그럼 지금까지 격리 해보신 적 있으신가요?",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037334.1.8",
          "form": "아뇨. 님은 혹시 해보셨어요?",
          "original_form": "아뇨. 님은 혹시 해보셨어요?",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100037334.1.9",
          "form": "저도 아직은 다행히 없네요",
          "original_form": "저도 아직은 다행히 없네요",
          "speaker_id": "2",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100037334.1.10",
          "form": "운 좋게 검사도 한 번도 안 해봤어요",
          "original_form": "운 좋게 검사도 한 번도 안 해봤어요",
          "speaker_id": "1",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100037334.1.11",
          "form": "저도요ㅋㅋㅋ 참 다행인거같아요 그죠??",
          "original_form": "저도요ㅋㅋㅋ 참 다행인거같아요 그죠??",
          "speaker_id": "2",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100037334.1.12",
          "form": "ㅋㅋ 그러게요. 코 찔리는 느낌 평생 모르고 싶어요",
          "original_form": "ㅋㅋ 그러게요. 코 찔리는 느낌 평생 모르고 싶어요",
          "speaker_id": "1",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100037334.1.13",
          "form": "맞아요 소문으로만 들어도 무시무시하더라고요",
          "original_form": "맞아요 소문으로만 들어도 무시무시하더라고요",
          "speaker_id": "2",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100037334.1.14",
          "form": "주사 맞고 안 아팠음 좋겠어요ㅠ",
          "original_form": "주사 맞고 안 아팠음 좋겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210831 22:57"
        }
      ]
