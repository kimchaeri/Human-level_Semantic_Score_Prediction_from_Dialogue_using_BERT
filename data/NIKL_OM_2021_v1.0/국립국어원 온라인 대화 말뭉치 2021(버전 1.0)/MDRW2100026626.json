      "utterance": [
        {
          "id": "MDRW2100026626.1.1",
          "form": "안녕~ 가족이랑 같이 살아?",
          "original_form": "안녕~ 가족이랑 같이 살아?",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100026626.1.2",
          "form": "안녕 가족이랑 같이살고있어",
          "original_form": "안녕 가족이랑 같이살고있어",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026626.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026626.1.4",
          "form": "응 나두! 엄마랑 살아!",
          "original_form": "응 나두! 엄마랑 살아!",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026626.1.5",
          "form": "둘이서 살아서 2인 가족이야~",
          "original_form": "둘이서 살아서 2인 가족이야~",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026626.1.6",
          "form": "아하 외동이야??",
          "original_form": "아하 외동이야??",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026626.1.7",
          "form": "나는 막내야",
          "original_form": "나는 막내야",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026626.1.8",
          "form": "형제 셋인데 둘은 독립했어",
          "original_form": "형제 셋인데 둘은 독립했어",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026626.1.9",
          "form": "오 나두 막내얌",
          "original_form": "오 나두 막내얌",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026626.1.10",
          "form": "싱기방기",
          "original_form": "싱기방기",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026626.1.11",
          "form": "오 독립했구나 .. 우린 아직 독립할 생각 없어보여ㅋㅋ..",
          "original_form": "오 독립했구나 .. 우린 아직 독립할 생각 없어보여ㅋㅋ..",
          "speaker_id": "2",
          "time": "20210902 00:05"
        },
        {
          "id": "MDRW2100026626.1.12",
          "form": "집안일인 분담해서 하고있어?",
          "original_form": "집안일인 분담해서 하고있어?",
          "speaker_id": "2",
          "time": "20210902 00:05"
        },
        {
          "id": "MDRW2100026626.1.13",
          "form": "응 내가 요리랑 설거지!",
          "original_form": "응 내가 요리랑 설거지!",
          "speaker_id": "1",
          "time": "20210902 10:02"
        },
        {
          "id": "MDRW2100026626.1.14",
          "form": "장보는 건 서로 먹고 싶은거 알아서 봐왕",
          "original_form": "장보는 건 서로 먹고 싶은거 알아서 봐왕",
          "speaker_id": "1",
          "time": "20210902 10:02"
        },
        {
          "id": "MDRW2100026626.1.15",
          "form": "아하 요리는 하는구나 요리 잘해??",
          "original_form": "아하 요리는 하는구나 요리 잘해??",
          "speaker_id": "2",
          "time": "20210902 12:32"
        },
        {
          "id": "MDRW2100026626.1.16",
          "form": "요리 그냥 그래!ㅋㅋㅋㅋ",
          "original_form": "요리 그냥 그래!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100026626.1.17",
          "form": "그래도 다들 맛있댕ㅋㅋㅋ",
          "original_form": "그래도 다들 맛있댕ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100026626.1.18",
          "form": "그러면 잘하는거지ㅎㅎ",
          "original_form": "그러면 잘하는거지ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 15:03"
        },
        {
          "id": "MDRW2100026626.1.19",
          "form": "크크 고마워",
          "original_form": "크크 고마워",
          "speaker_id": "1",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100026626.1.20",
          "form": "너는 잘하는 음식 있어?",
          "original_form": "너는 잘하는 음식 있어?",
          "speaker_id": "1",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100026626.1.21",
          "form": "나는 파스타 자주해먹어서 파스타는 평타이상은 하는 것 같긴해",
          "original_form": "나는 파스타 자주해먹어서 파스타는 평타이상은 하는 것 같긴해",
          "speaker_id": "2",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100026626.1.22",
          "form": "근데 난 집에서 거의 차려주는거 먹기만해서.. 잘은 안하구ㅠㅠ",
          "original_form": "근데 난 집에서 거의 차려주는거 먹기만해서.. 잘은 안하구ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100026626.1.23",
          "form": "우와 파스타 대박이다",
          "original_form": "우와 파스타 대박이다",
          "speaker_id": "1",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100026626.1.24",
          "form": "요리 잘하는거 아냐?!",
          "original_form": "요리 잘하는거 아냐?!",
          "speaker_id": "1",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100026626.1.25",
          "form": "아냐 그냥 시판용소스나 치킨스톡 넣으면 맛있어 ..ㅋㅋ",
          "original_form": "아냐 그냥 시판용소스나 치킨스톡 넣으면 맛있어 ..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:07"
        }
      ]
    }
