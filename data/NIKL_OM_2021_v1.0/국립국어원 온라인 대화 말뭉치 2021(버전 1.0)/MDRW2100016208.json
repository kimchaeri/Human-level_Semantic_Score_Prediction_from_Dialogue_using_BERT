      "utterance": [
        {
          "id": "MDRW2100016208.1.1",
          "form": "오늘 점심은 떡볶이",
          "original_form": "오늘 점심은 떡볶이",
          "speaker_id": "2",
          "time": "20210826 11:58"
        },
        {
          "id": "MDRW2100016208.1.2",
          "form": "배달의 민족 주문 함",
          "original_form": "배달의 민족 주문 함",
          "speaker_id": "2",
          "time": "20210826 11:58"
        },
        {
          "id": "MDRW2100016208.1.3",
          "form": "오오 좋네요ㅋㅋ",
          "original_form": "오오 좋네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:00"
        },
        {
          "id": "MDRW2100016208.1.4",
          "form": "저는 된찌에 두부야채전ㅋ",
          "original_form": "저는 된찌에 두부야채전ㅋ",
          "speaker_id": "1",
          "time": "20210826 12:01"
        },
        {
          "id": "MDRW2100016208.1.5",
          "form": "커피먹고싶다",
          "original_form": "커피먹고싶다",
          "speaker_id": "2",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100016208.1.6",
          "form": "맛있는거",
          "original_form": "맛있는거",
          "speaker_id": "2",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100016208.1.7",
          "form": "ㅋㅋ 나두요.ㅋ",
          "original_form": "ㅋㅋ 나두요.ㅋ",
          "speaker_id": "1",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016208.1.8",
          "form": "이제 점심먹음ㅋㅋㅋ",
          "original_form": "이제 점심먹음ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016208.1.9",
          "form": "커피한잔 좋쥬ㅋ",
          "original_form": "커피한잔 좋쥬ㅋ",
          "speaker_id": "1",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016208.1.10",
          "form": "이제주사맞으러간다",
          "original_form": "이제주사맞으러간다",
          "speaker_id": "2",
          "time": "20210826 13:36"
        },
        {
          "id": "MDRW2100016208.1.11",
          "form": "일진우째됐노?",
          "original_form": "일진우째됐노?",
          "speaker_id": "2",
          "time": "20210826 15:31"
        },
        {
          "id": "MDRW2100016208.1.12",
          "form": "무슨주사요? 백신2차,",
          "original_form": "무슨주사요? 백신2차,",
          "speaker_id": "1",
          "time": "20210826 15:32"
        },
        {
          "id": "MDRW2100016208.1.13",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210826 15:32"
        },
        {
          "id": "MDRW2100016208.1.14",
          "form": "일진2주됐어요.  언니는요?",
          "original_form": "일진2주됐어요.  언니는요?",
          "speaker_id": "1",
          "time": "20210826 15:36"
        },
        {
          "id": "MDRW2100016208.1.15",
          "form": "나도ㅋ",
          "original_form": "나도ㅋ",
          "speaker_id": "2",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100016208.1.16",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100016208.1.17",
          "form": "2주",
          "original_form": "2주",
          "speaker_id": "2",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100016208.1.18",
          "form": "백신2차",
          "original_form": "백신2차",
          "speaker_id": "2",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100016208.1.19",
          "form": "화이자",
          "original_form": "화이자",
          "speaker_id": "2",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100016208.1.20",
          "form": "오 축하축하ㅋㅋ",
          "original_form": "오 축하축하ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100016208.1.21",
          "form": "좋구만요",
          "original_form": "좋구만요",
          "speaker_id": "1",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100016208.1.22",
          "form": "아하 백신2차 홧팅홧팅",
          "original_form": "아하 백신2차 홧팅홧팅",
          "speaker_id": "1",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100016208.1.23",
          "form": "당분간 무리하지말아야 겠네유",
          "original_form": "당분간 무리하지말아야 겠네유",
          "speaker_id": "1",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100016208.1.24",
          "form": "오늘은 배달이다",
          "original_form": "오늘은 배달이다",
          "speaker_id": "2",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100016208.1.25",
          "form": "저녁도ㅋ",
          "original_form": "저녁도ㅋ",
          "speaker_id": "2",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100016208.1.26",
          "form": "부엌파업",
          "original_form": "부엌파업",
          "speaker_id": "2",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100016208.1.27",
          "form": "맞아요. 배달좋죠ㅋㅋ",
          "original_form": "맞아요. 배달좋죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100016208.1.28",
          "form": "저도 아까찜닭먹고",
          "original_form": "저도 아까찜닭먹고",
          "speaker_id": "1",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100016208.1.29",
          "form": "오늘저녁은 된찌먹으려구요ㅋ",
          "original_form": "오늘저녁은 된찌먹으려구요ㅋ",
          "speaker_id": "1",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100016208.1.30",
          "form": "된장찌게ㅋ",
          "original_form": "된장찌게ㅋ",
          "speaker_id": "1",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100016208.1.31",
          "form": "팔아파서 못하겠다 밥",
          "original_form": "팔아파서 못하겠다 밥",
          "speaker_id": "2",
          "time": "20210826 15:47"
        },
        {
          "id": "MDRW2100016208.1.32",
          "form": "당연하죠 ㅠㅠ 며칠동안 아플텐데 ㅠㅠ",
          "original_form": "당연하죠 ㅠㅠ 며칠동안 아플텐데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 15:48"
        },
        {
          "id": "MDRW2100016208.1.33",
          "form": "ㅎㅎ 당당하게 시켜먹자고 말함ㅋㅋ",
          "original_form": "ㅎㅎ 당당하게 시켜먹자고 말함ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100016208.1.34",
          "form": "당연하죠!! 시켜먹어도됨. ㅋㅋㅋ시켜먹어야됨.ㅋㅋㅋ",
          "original_form": "당연하죠!! 시켜먹어도됨. ㅋㅋㅋ시켜먹어야됨.ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100016208.1.35",
          "form": "진미김밥ㅋㅋ",
          "original_form": "진미김밥ㅋㅋ",
          "speaker_id": "1",
