      "utterance": [
        {
          "id": "MDRW2100019428.1.1",
          "form": "제 레어치즈요",
          "original_form": "제 레어치즈요",
          "speaker_id": "2",
          "time": "20210827 19:10"
        },
        {
          "id": "MDRW2100019428.1.2",
          "form": "오 저도요",
          "original_form": "오 저도요",
          "speaker_id": "1",
          "time": "20210827 19:10"
        },
        {
          "id": "MDRW2100019428.1.3",
          "form": "그게 제일 맛있어요",
          "original_form": "그게 제일 맛있어요",
          "speaker_id": "1",
          "time": "20210827 19:10"
        },
        {
          "id": "MDRW2100019428.1.4",
          "form": "그 딸기?무스들어간거",
          "original_form": "그 딸기?무스들어간거",
          "speaker_id": "2",
          "time": "20210827 19:10"
        },
        {
          "id": "MDRW2100019428.1.5",
          "form": "그게넘좋더라구요",
          "original_form": "그게넘좋더라구요",
          "speaker_id": "2",
          "time": "20210827 19:10"
        },
        {
          "id": "MDRW2100019428.1.6",
          "form": "그쵸??",
          "original_form": "그쵸??",
          "speaker_id": "2",
          "time": "20210827 19:10"
        },
        {
          "id": "MDRW2100019428.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 19:10"
        },
        {
          "id": "MDRW2100019428.1.8",
          "form": "그냥 치케도",
          "original_form": "그냥 치케도",
          "speaker_id": "1",
          "time": "20210827 19:10"
        },
        {
          "id": "MDRW2100019428.1.9",
          "form": "다들이맛을몰라",
          "original_form": "다들이맛을몰라",
          "speaker_id": "2",
          "time": "20210827 19:10"
        },
        {
          "id": "MDRW2100019428.1.10",
          "form": "맛있는데 딸기라니....",
          "original_form": "맛있는데 딸기라니....",
          "speaker_id": "1",
          "time": "20210827 19:10"
        },
        {
          "id": "MDRW2100019428.1.11",
          "form": "아이스박스는 넘 ㅇㅅㅇ..인데",
          "original_form": "아이스박스는 넘 ㅇㅅㅇ..인데",
          "speaker_id": "2",
          "time": "20210827 19:11"
        },
        {
          "id": "MDRW2100019428.1.12",
          "form": "ㅋㅋㅋㅋㅋ 맞아여 ㅋㅋㅋ 다 초코만 먹더라구여 티라미수랑",
          "original_form": "ㅋㅋㅋㅋㅋ 맞아여 ㅋㅋㅋ 다 초코만 먹더라구여 티라미수랑",
          "speaker_id": "1",
          "time": "20210827 19:11"
        },
        {
          "id": "MDRW2100019428.1.13",
          "form": "헉 저도요 !!",
          "original_form": "헉 저도요 !!",
          "speaker_id": "1",
          "time": "20210827 19:11"
        },
        {
          "id": "MDRW2100019428.1.14",
          "form": "ㅋㅋㅋㅋㄲㅋㅋㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㄲㅋㅋㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 19:11"
        },
        {
          "id": "MDRW2100019428.1.15",
          "form": "역시맛잘알",
          "original_form": "역시맛잘알",
          "speaker_id": "2",
          "time": "20210827 19:11"
        },
        {
          "id": "MDRW2100019428.1.16",
          "form": "그거 오레오래서 기대했는데 흠...",
          "original_form": "그거 오레오래서 기대했는데 흠...",
          "speaker_id": "1",
          "time": "20210827 19:11"
        },
        {
          "id": "MDRW2100019428.1.17",
          "form": "쩝쩝박사죠?",
          "original_form": "쩝쩝박사죠?",
          "speaker_id": "2",
          "time": "20210827 19:11"
        },
        {
          "id": "MDRW2100019428.1.18",
          "form": "인정합니다",
          "original_form": "인정합니다",
          "speaker_id": "1",
          "time": "20210827 19:11"
        },
        {
          "id": "MDRW2100019428.1.19",
          "form": "ㅋㅋㅋㄱ걍ㅇㅅㅇ...이거왜?싶은 아이스박스",
          "original_form": "ㅋㅋㅋㄱ걍ㅇㅅㅇ...이거왜?싶은 아이스박스",
          "speaker_id": "2",
          "time": "20210827 19:11"
        },
        {
          "id": "MDRW2100019428.1.20",
          "form": "음료는 스트로베리피치프라페나 로열밀크티쉐이크먹어요ㅎ",
          "original_form": "음료는 스트로베리피치프라페나 로열밀크티쉐이크먹어요ㅎ",
          "speaker_id": "2",
          "time": "20210827 19:11"
        },
        {
          "id": "MDRW2100019428.1.21",
          "form": "맞아요 기대이하였어여 ㅠ",
          "original_form": "맞아요 기대이하였어여 ㅠ",
