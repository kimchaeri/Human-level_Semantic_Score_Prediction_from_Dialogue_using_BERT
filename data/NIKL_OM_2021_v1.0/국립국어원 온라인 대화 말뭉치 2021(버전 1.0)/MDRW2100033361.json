        },
        {
          "id": "MDRW2100033361.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100033361.1.3",
          "form": "이번 방역 대책이 어떤게 잇나요?",
          "original_form": "이번 방역 대책이 어떤게 잇나요?",
          "speaker_id": "2",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100033361.1.4",
          "form": "저도 정확히는 모르겠는데 백신 맞은 사람에 한해서",
          "original_form": "저도 정확히는 모르겠는데 백신 맞은 사람에 한해서",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100033361.1.5",
          "form": "약간 특혜가 있는거 같더라구여!",
          "original_form": "약간 특혜가 있는거 같더라구여!",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100033361.1.6",
          "form": "백신 맞으셨어요",
          "original_form": "백신 맞으셨어요",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100033361.1.7",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100033361.1.8",
          "form": "오 특혜",
          "original_form": "오 특혜",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100033361.1.9",
          "form": "네네 맞았어요",
          "original_form": "네네 맞았어요",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100033361.1.10",
          "form": "아 그 백신접종자 포함 6명인가? 뭐 그거아닌가요??",
          "original_form": "아 그 백신접종자 포함 6명인가? 뭐 그거아닌가요??",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100033361.1.11",
          "form": "네 맞아요!! 그거!",
          "original_form": "네 맞아요!! 그거!",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100033361.1.12",
          "form": "2차까지 다 맞으셨엉ㅅ?",
          "original_form": "2차까지 다 맞으셨엉ㅅ?",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100033361.1.13",
          "form": "오 아니요 저번주에 1차맞앗어요!",
          "original_form": "오 아니요 저번주에 1차맞앗어요!",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100033361.1.14",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100033361.1.15",
          "form": "전 27일에 1차 접종해요 ㅠㅠ",
          "original_form": "전 27일에 1차 접종해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100033361.1.16",
          "form": "그럼 추석에 어디 가실 계획 있으신가요?",
          "original_form": "그럼 추석에 어디 가실 계획 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100033361.1.17",
          "form": "아 할머니집 갈고같아요 ㅠㅠ",
          "original_form": "아 할머니집 갈고같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100033361.1.18",
          "form": "근데 1차백신자도 그 방역대책에 포함인가요?",
          "original_form": "근데 1차백신자도 그 방역대책에 포함인가요?",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100033361.1.19",
          "form": "아마 2차까지 맞고+14일 지나서?",
          "original_form": "아마 2차까지 맞고+14일 지나서?",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100033361.1.20",
          "form": "인거 같긴해요..ㅎㅎ..",
          "original_form": "인거 같긴해요..ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100033361.1.21",
          "form": "근데 뭔가 이번엔 그래도 고향 간다는 사람들이 꽤 더ㅣ더라구여!",
          "original_form": "근데 뭔가 이번엔 그래도 고향 간다는 사람들이 꽤 더ㅣ더라구여!",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100033361.1.22",
          "form": "아아 그렇군요! 백신접종자가많아서그런가봐요",
          "original_form": "아아 그렇군요! 백신접종자가많아서그런가봐요",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100033361.1.23",
          "form": "맞아요! 보면 주변에 저빼고 다 1차는 맞았더라구요..",
          "original_form": "맞아요! 보면 주변에 저빼고 다 1차는 맞았더라구요..",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100033361.1.24",
          "form": "마자요..작년엔 명절에 안갔어서",
          "original_form": "마자요..작년엔 명절에 안갔어서",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100033361.1.25",
          "form": "이번에가야될거같긴한데 고민이네요",
          "original_form": "이번에가야될거같긴한데 고민이네요",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100033361.1.26",
          "form": "저두 한 2년째 안가고있네여..",
          "original_form": "저두 한 2년째 안가고있네여..",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100033361.1.27",
          "form": "어디로 가세요?",
          "original_form": "어디로 가세요?",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100033361.1.28",
          "form": "김천이에요!",
          "original_form": "김천이에요!",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100033361.1.29",
          "form": "휴 작냔에 조심스러눠서 못닷는데",
          "original_form": "휴 작냔에 조심스러눠서 못닷는데",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100033361.1.30",
          "form": "남들다 간다고 오라고 그랫던 기억나네요 ㅎ..",
          "original_form": "남들다 간다고 오라고 그랫던 기억나네요 ㅎ..",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100033361.1.31",
          "form": "오우 가는데 시간이 꽤 걸릴거같아요!",
          "original_form": "오우 가는데 시간이 꽤 걸릴거같아요!",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100033361.1.32",
          "form": "저는 경상도살아서 가까우ㅑ요 ㅋㅋㅋㅋ",
          "original_form": "저는 경상도살아서 가까우ㅑ요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:25"
        }
      ]
