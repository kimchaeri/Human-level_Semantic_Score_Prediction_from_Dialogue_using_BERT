      "utterance": [
        {
          "id": "MDRW2100041233.1.1",
          "form": "저는 딱한번 구매 해봣어요",
          "original_form": "저는 딱한번 구매 해봣어요",
          "speaker_id": "2",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100041233.1.2",
          "form": "화장품이요 !!",
          "original_form": "화장품이요 !!",
          "speaker_id": "2",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100041233.1.3",
          "form": "아 ㅋㅋ 화장품 구매하셨군요 ㅋㅋ",
          "original_form": "아 ㅋㅋ 화장품 구매하셨군요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100041233.1.4",
          "form": "저는 당근 이용잘 하거든요 ㅋㅋ",
          "original_form": "저는 당근 이용잘 하거든요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100041233.1.5",
          "form": "팔기도 하고, 사기도 하구요",
          "original_form": "팔기도 하고, 사기도 하구요",
          "speaker_id": "1",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100041233.1.6",
          "form": "오오 당근 직거래 하세여?",
          "original_form": "오오 당근 직거래 하세여?",
          "speaker_id": "2",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100041233.1.7",
          "form": "네네 저는 무조건 직거래요 ㅋ",
          "original_form": "네네 저는 무조건 직거래요 ㅋ",
          "speaker_id": "1",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100041233.1.8",
          "form": "택배거래는 좀 불안해서요",
          "original_form": "택배거래는 좀 불안해서요",
          "speaker_id": "1",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100041233.1.9",
          "form": "오 만나면 안어색 하세요??",
          "original_form": "오 만나면 안어색 하세요??",
          "speaker_id": "2",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100041233.1.10",
          "form": "그냥 인사만 하고 서로 물건 설명하고 돈 받고 그래요 ㅋㅋㅋ",
          "original_form": "그냥 인사만 하고 서로 물건 설명하고 돈 받고 그래요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100041233.1.11",
          "form": "근데 돈 송금받을때 기다리는 시간이 조금 뻘쭘할때도 있긴 해요 ㅋㅋ",
          "original_form": "근데 돈 송금받을때 기다리는 시간이 조금 뻘쭘할때도 있긴 해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100041233.1.12",
          "form": "오오 그렇게 하면 진짜 서로 안전거래 하겠어요 !!",
          "original_form": "오오 그렇게 하면 진짜 서로 안전거래 하겠어요 !!",
          "speaker_id": "2",
          "time": "20210905 20:49"
        },
        {
          "id": "MDRW2100041233.1.13",
          "form": "주로 어떤거 파세요?",
          "original_form": "주로 어떤거 파세요?",
          "speaker_id": "2",
          "time": "20210905 20:49"
        },
        {
          "id": "MDRW2100041233.1.14",
          "form": "저는 책이랑 한복, 옷종류, 매트, 커피 이런거 팔아봤어요 ㅋ",
          "original_form": "저는 책이랑 한복, 옷종류, 매트, 커피 이런거 팔아봤어요 ㅋ",
          "speaker_id": "1",
          "time": "20210905 20:50"
        },
        {
          "id": "MDRW2100041233.1.15",
          "form": "최근에 아이 한복팔았네요 ㅋㅋ 추석이 다가와서 그런지 빨리 팔렸어요",
          "original_form": "최근에 아이 한복팔았네요 ㅋㅋ 추석이 다가와서 그런지 빨리 팔렸어요",
          "speaker_id": "1",
          "time": "20210905 20:50"
        },
        {
          "id": "MDRW2100041233.1.16",
          "form": "와 아이는 금방금방 크니까 !!",
          "original_form": "와 아이는 금방금방 크니까 !!",
          "speaker_id": "2",
          "time": "20210905 20:51"
        },
        {
          "id": "MDRW2100041233.1.17",
          "form": "매트는 무겁지 않으세요??",
          "original_form": "매트는 무겁지 않으세요??",
          "speaker_id": "2",
          "time": "20210905 20:51"
        },
        {
          "id": "MDRW2100041233.1.18",
          "form": "와 정말 매트 너무 무거웠어요",
          "original_form": "와 정말 매트 너무 무거웠어요",
          "speaker_id": "1",
          "time": "20210905 20:51"
        },
        {
          "id": "MDRW2100041233.1.19",
          "form": "제가 갖다드렸거든요",
          "original_form": "제가 갖다드렸거든요",
          "speaker_id": "1",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100041233.1.20",
          "form": "와 ㅋㅋㅋㅋ 포기하고 싶더라구요 ㅋ",
          "original_form": "와 ㅋㅋㅋㅋ 포기하고 싶더라구요 ㅋ",
          "speaker_id": "1",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100041233.1.21",
          "form": "와 ㅜㅜ 혼자 들고 가셧어요?,",
          "original_form": "와 ㅜㅜ 혼자 들고 가셧어요?,",
          "speaker_id": "2",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100041233.1.22",
          "form": "네 ㅠㅠ 그와중에 아들래미도 데리고갔더니 ㅋㅋㅋ",
          "original_form": "네 ㅠㅠ 그와중에 아들래미도 데리고갔더니 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:53"
        },
        {
          "id": "MDRW2100041233.1.23",
          "form": "그와중에 또 비가...ㅋㅋㅋ난장판이었어요",
          "original_form": "그와중에 또 비가...ㅋㅋㅋ난장판이었어요",
          "speaker_id": "1",
          "time": "20210905 20:53"
        },
        {
          "id": "MDRW2100041233.1.24",
          "form": "와 파느라 고생하셧어요 ㅠㅠ",
          "original_form": "와 파느라 고생하셧어요 ㅠㅠ",
          "speaker_id": "2",
