      "utterance": [
        {
          "id": "MDRW2100019201.1.1",
          "form": "요즘 무슨 게임해?",
          "original_form": "요즘 무슨 게임해?",
          "speaker_id": "1",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019201.1.2",
          "form": "난 어제 소닉게임을했어",
          "original_form": "난 어제 소닉게임을했어",
          "speaker_id": "2",
          "time": "20210827 16:38"
        },
        {
          "id": "MDRW2100019201.1.3",
          "form": "소닉 캐릭터?",
          "original_form": "소닉 캐릭터?",
          "speaker_id": "1",
          "time": "20210827 19:25"
        },
        {
          "id": "MDRW2100019201.1.4",
          "form": "맞아 그 파란색",
          "original_form": "맞아 그 파란색",
          "speaker_id": "2",
          "time": "20210827 19:25"
        },
        {
          "id": "MDRW2100019201.1.5",
          "form": "와 추억이네",
          "original_form": "와 추억이네",
          "speaker_id": "1",
          "time": "20210827 19:25"
        },
        {
          "id": "MDRW2100019201.1.6",
          "form": "재밌더라구",
          "original_form": "재밌더라구",
          "speaker_id": "2",
          "time": "20210827 19:25"
        },
        {
          "id": "MDRW2100019201.1.7",
          "form": "다음에 나도 해볼래",
          "original_form": "다음에 나도 해볼래",
          "speaker_id": "1",
          "time": "20210827 19:26"
        },
        {
          "id": "MDRW2100019201.1.8",
          "form": "같이 게임하면 재밌을거 같아",
          "original_form": "같이 게임하면 재밌을거 같아",
          "speaker_id": "2",
          "time": "20210827 19:26"
        },
        {
          "id": "MDRW2100019201.1.9",
          "form": "테일이란 캐릭터로 게임해보고 싶어",
          "original_form": "테일이란 캐릭터로 게임해보고 싶어",
          "speaker_id": "1",
          "time": "20210827 19:28"
        },
        {
          "id": "MDRW2100019201.1.10",
          "form": "그 캐릭터는 하늘을 날 수 있어",
          "original_form": "그 캐릭터는 하늘을 날 수 있어",
          "speaker_id": "2",
          "time": "20210827 19:28"
        },
        {
          "id": "MDRW2100019201.1.11",
          "form": "게임을하면 스트레스가 풀려",
          "original_form": "게임을하면 스트레스가 풀려",
          "speaker_id": "1",
          "time": "20210827 19:28"
        },
        {
          "id": "MDRW2100019201.1.12",
          "form": "나는 오히려 쌓이기도 하더라",
          "original_form": "나는 오히려 쌓이기도 하더라",
          "speaker_id": "2",
          "time": "20210827 19:28"
        },
        {
          "id": "MDRW2100019201.1.13",
          "form": "팀전게임은 그럴때가 있지",
          "original_form": "팀전게임은 그럴때가 있지",
          "speaker_id": "1",
          "time": "20210827 19:28"
        },
        {
          "id": "MDRW2100019201.1.14",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
