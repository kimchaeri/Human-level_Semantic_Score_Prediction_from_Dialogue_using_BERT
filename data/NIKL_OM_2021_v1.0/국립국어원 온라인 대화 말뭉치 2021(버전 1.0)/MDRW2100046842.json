      "utterance": [
        {
          "id": "MDRW2100046842.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211105 17:47"
        },
        {
          "id": "MDRW2100046842.1.2",
          "form": "안녕하세요! 불금인데 저녁시간 어떻게 보낼 계획이신가요",
          "original_form": "안녕하세요! 불금인데 저녁시간 어떻게 보낼 계획이신가요",
          "speaker_id": "2",
          "time": "20211105 17:47"
        },
        {
          "id": "MDRW2100046842.1.3",
          "form": "이제집에가서쉬어야죠",
          "original_form": "이제집에가서쉬어야죠",
          "speaker_id": "1",
          "time": "20211105 17:47"
        },
        {
          "id": "MDRW2100046842.1.4",
          "form": "내일부터주말이라 넷플릭봐야죠",
          "original_form": "내일부터주말이라 넷플릭봐야죠",
          "speaker_id": "1",
          "time": "20211105 17:48"
        },
        {
          "id": "MDRW2100046842.1.5",
          "form": "오 저랑 같으시군요!",
          "original_form": "오 저랑 같으시군요!",
          "speaker_id": "2",
          "time": "20211105 17:48"
        },
        {
          "id": "MDRW2100046842.1.6",
          "form": "저는 마이네임 보려구요",
          "original_form": "저는 마이네임 보려구요",
          "speaker_id": "2",
          "time": "20211105 17:48"
        },
        {
          "id": "MDRW2100046842.1.7",
          "form": "오저두요",
          "original_form": "오저두요",
          "speaker_id": "1",
          "time": "20211105 17:48"
        },
        {
          "id": "MDRW2100046842.1.8",
          "form": "ㅋㅋ배우들이 너무 멋있게 나오더라구요",
          "original_form": "ㅋㅋ배우들이 너무 멋있게 나오더라구요",
          "speaker_id": "2",
          "time": "20211105 17:48"
        },
        {
          "id": "MDRW2100046842.1.9",
          "form": "오징어게임은다보고 이제그거남아서주말을기다리고있었네요",
          "original_form": "오징어게임은다보고 이제그거남아서주말을기다리고있었네요",
          "speaker_id": "1",
          "time": "20211105 17:48"
        },
        {
          "id": "MDRW2100046842.1.10",
          "form": "이미스토리를알지만 박휘순멋집니다",
          "original_form": "이미스토리를알지만 박휘순멋집니다",
          "speaker_id": "1",
          "time": "20211105 17:49"
        },
        {
          "id": "MDRW2100046842.1.11",
          "form": "이미스포당했네요",
          "original_form": "이미스포당했네요",
          "speaker_id": "1",
          "time": "20211105 17:49"
        },
        {
          "id": "MDRW2100046842.1.12",
          "form": "맞아요... 저도 오겜이랑 디피 다 보고 ㅋㅋ",
          "original_form": "맞아요... 저도 오겜이랑 디피 다 보고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211105 17:49"
        },
        {
          "id": "MDRW2100046842.1.13",
          "form": "마이네임 내용도 다 아는데 박휘순이 멋있어서 보려구요",
          "original_form": "마이네임 내용도 다 아는데 박휘순이 멋있어서 보려구요",
          "speaker_id": "2",
          "time": "20211105 17:49"
        },
        {
          "id": "MDRW2100046842.1.14",
          "form": "디피는아직이네요",
          "original_form": "디피는아직이네요",
          "speaker_id": "1",
          "time": "20211105 17:49"
        },
        {
          "id": "MDRW2100046842.1.15",
          "form": "디피도 재밌어요",
          "original_form": "디피도 재밌어요",
          "speaker_id": "2",
          "time": "20211105 17:49"
        },
        {
          "id": "MDRW2100046842.1.16",
          "form": "배우들연기가진짜좋다고하네요",
          "original_form": "배우들연기가진짜좋다고하네요",
          "speaker_id": "1",
          "time": "20211105 17:50"
        },
        {
          "id": "MDRW2100046842.1.17",
          "form": "정말.. 넷플에 올라오는건 뭐하나 뺄게 없더라구요",
          "original_form": "정말.. 넷플에 올라오는건 뭐하나 뺄게 없더라구요",
          "speaker_id": "2",
          "time": "20211105 17:50"
        },
        {
          "id": "MDRW2100046842.1.18",
          "form": "다 재밌어서",
          "original_form": "다 재밌어서",
          "speaker_id": "2",
          "time": "20211105 17:50"
        }
