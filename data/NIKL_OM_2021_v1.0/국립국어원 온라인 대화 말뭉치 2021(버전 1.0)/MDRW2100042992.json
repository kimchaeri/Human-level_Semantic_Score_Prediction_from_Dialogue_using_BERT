        {
          "id": "MDRW2100042992.1.1",
          "form": "여행 좋아해?",
          "original_form": "여행 좋아해?",
          "speaker_id": "1",
          "time": "20210907 12:39"
        },
        {
          "id": "MDRW2100042992.1.2",
          "form": "좋죠! ㅋㅋ",
          "original_form": "좋죠! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:41"
        },
        {
          "id": "MDRW2100042992.1.3",
          "form": "와 너무 해외에 가고파요ㅠㅠ",
          "original_form": "와 너무 해외에 가고파요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 12:52"
        },
        {
          "id": "MDRW2100042992.1.4",
          "form": "저 유럽 너무 가고싶네요",
          "original_form": "저 유럽 너무 가고싶네요",
          "speaker_id": "2",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100042992.1.5",
          "form": "저도요 북유럽 동유럽 서유럽 다 퓨ㅠ",
          "original_form": "저도요 북유럽 동유럽 서유럽 다 퓨ㅠ",
          "speaker_id": "1",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100042992.1.6",
          "form": "유럽이 최고",
          "original_form": "유럽이 최고",
          "speaker_id": "1",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100042992.1.7",
          "form": "한번도 안가봤어요",
          "original_form": "한번도 안가봤어요",
          "speaker_id": "2",
          "time": "20210907 13:22"
        },
        {
          "id": "MDRW2100042992.1.8",
          "form": "저도요ㅠㅠ 일본이랑 베트남 끝",
          "original_form": "저도요ㅠㅠ 일본이랑 베트남 끝",
          "speaker_id": "1",
          "time": "20210907 13:22"
        },
        {
          "id": "MDRW2100042992.1.9",
          "form": "전 캐나다랑 홍콩 중국",
          "original_form": "전 캐나다랑 홍콩 중국",
          "speaker_id": "2",
          "time": "20210907 14:09"
        },
        {
          "id": "MDRW2100042992.1.10",
          "form": "갔어요",
          "original_form": "갔어요",
          "speaker_id": "2",
          "time": "20210907 14:09"
        },
        {
          "id": "MDRW2100042992.1.11",
          "form": "유와 캐나다 어때요ㅠㅠㅠ 꿈의 여행지",
          "original_form": "유와 캐나다 어때요ㅠㅠㅠ 꿈의 여행지",
          "speaker_id": "1",
          "time": "20210907 14:43"
        },
        {
          "id": "MDRW2100042992.1.12",
          "form": "ㅋㅋ좋았던거같아요ㅋㅋ근데 중국인이 너무 많아요",
          "original_form": "ㅋㅋ좋았던거같아요ㅋㅋ근데 중국인이 너무 많아요",
          "speaker_id": "2",
          "time": "20210907 14:46"
        },
        {
          "id": "MDRW2100042992.1.13",
          "form": "어딜가나 중국인이 많네요 ㅋㅋ",
          "original_form": "어딜가나 중국인이 많네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:48"
        },
        {
          "id": "MDRW2100042992.1.14",
          "form": "지긋지긋ㅋㅋ",
          "original_form": "지긋지긋ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:59"
        },
        {
          "id": "MDRW2100042992.1.15",
          "form": "완전 여기가 캐나단지 중국인지ㅋㅋ",
