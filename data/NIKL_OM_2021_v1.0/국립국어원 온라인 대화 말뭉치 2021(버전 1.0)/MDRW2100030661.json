      "utterance": [
        {
          "id": "MDRW2100030661.1.1",
          "form": "저는 차도 좋아하고 홍차도 좋아하고 녹차도 좋아해요",
          "original_form": "저는 차도 좋아하고 홍차도 좋아하고 녹차도 좋아해요",
          "speaker_id": "2",
          "time": "20210904 11:06"
        },
        {
          "id": "MDRW2100030661.1.2",
          "form": "음료수도 좋아해요",
          "original_form": "음료수도 좋아해요",
          "speaker_id": "2",
          "time": "20210904 11:06"
        },
        {
          "id": "MDRW2100030661.1.3",
          "form": "홍초도 좋아하시나요",
          "original_form": "홍초도 좋아하시나요",
          "speaker_id": "1",
          "time": "20210904 11:06"
        },
        {
          "id": "MDRW2100030661.1.4",
          "form": "홍천은 예전에 못 먹었는데 지금은 없어서 못 먹습니다",
          "original_form": "홍천은 예전에 못 먹었는데 지금은 없어서 못 먹습니다",
          "speaker_id": "2",
          "time": "20210904 11:06"
        },
        {
          "id": "MDRW2100030661.1.5",
          "form": "쟁여 놓고 먹습니다",
          "original_form": "쟁여 놓고 먹습니다",
          "speaker_id": "1",
          "time": "20210904 11:06"
        },
        {
          "id": "MDRW2100030661.1.6",
          "form": "1신 걸 잘 못 먹었었는데 나이가 들면서 조금씩 먹어 지네요",
          "original_form": "1신 걸 잘 못 먹었었는데 나이가 들면서 조금씩 먹어 지네요",
          "speaker_id": "2",
          "time": "20210904 11:06"
        },
        {
          "id": "MDRW2100030661.1.7",
          "form": "이마트에서 하나 사면 하나 더 줍니다",
          "original_form": "이마트에서 하나 사면 하나 더 줍니다",
          "speaker_id": "1",
          "time": "20210904 11:06"
        },
        {
          "id": "MDRW2100030661.1.8",
          "form": "연하게 타서 먹는 거 좋아합니다 홍초 나 매실액",
          "original_form": "연하게 타서 먹는 거 좋아합니다 홍초 나 매실액",
          "speaker_id": "2",
          "time": "20210904 11:07"
        },
        {
          "id": "MDRW2100030661.1.9",
          "form": "쌀때 사놔야죠",
          "original_form": "쌀때 사놔야죠",
          "speaker_id": "1",
          "time": "20210904 11:07"
        },
        {
          "id": "MDRW2100030661.1.10",
          "form": "원뿔 원 아주 좋은 행사 이지요",
          "original_form": "원뿔 원 아주 좋은 행사 이지요",
          "speaker_id": "2",
          "time": "20210904 11:07"
        },
        {
          "id": "MDRW2100030661.1.11",
          "form": "홍초도 다양한 맛이 있어요",
          "original_form": "홍초도 다양한 맛이 있어요",
          "speaker_id": "1",
          "time": "20210904 11:07"
        },
        {
          "id": "MDRW2100030661.1.12",
          "form": "홍차는 가향홍차 좋아합니다",
          "original_form": "홍차는 가향홍차 좋아합니다",
          "speaker_id": "2",
          "time": "20210904 11:07"
        },
        {
          "id": "MDRW2100030661.1.13",
          "form": "그냥 혼자도 맛있긴 하지만 달달한 향이 나는 홍차를 좋아해요",
          "original_form": "그냥 혼자도 맛있긴 하지만 달달한 향이 나는 홍차를 좋아해요",
          "speaker_id": "2",
          "time": "20210904 11:07"
        },
        {
          "id": "MDRW2100030661.1.14",
          "form": "홍차를 잘 알지 못합니다",
          "original_form": "홍차를 잘 알지 못합니다",
          "speaker_id": "1",
          "time": "20210904 11:07"
        },
        {
          "id": "MDRW2100030661.1.15",
          "form": "밀크티 만들어 먹으면 좋맛탱",
          "original_form": "밀크티 만들어 먹으면 좋맛탱",
          "speaker_id": "2",
          "time": "20210904 11:07"
        },
        {
          "id": "MDRW2100030661.1.16",
          "form": "얼그레이 아쌈",
          "original_form": "얼그레이 아쌈",
          "speaker_id": "1",
          "time": "20210904 11:07"
        },
        {
          "id": "MDRW2100030661.1.17",
          "form": "그 정도 만아네요 만화 주인공 이니까요",
          "original_form": "그 정도 만아네요 만화 주인공 이니까요",
          "speaker_id": "1",
          "time": "20210904 11:08"
        },
        {
          "id": "MDRW2100030661.1.18",
          "form": "웨딩 임페리얼 일하고 있는데 비싸지만 맛있어요",
          "original_form": "웨딩 임페리얼 일하고 있는데 비싸지만 맛있어요",
          "speaker_id": "2",
          "time": "20210904 11:08"
        }
