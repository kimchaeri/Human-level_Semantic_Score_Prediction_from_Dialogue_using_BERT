      "utterance": [
        {
          "id": "MDRW2100020191.1.1",
          "form": "여행계획 있으신가요??",
          "original_form": "여행계획 있으신가요??",
          "speaker_id": "1",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020191.1.2",
          "form": "없어요 ㅜㅜ 있으세요?",
          "original_form": "없어요 ㅜㅜ 있으세요?",
          "speaker_id": "2",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020191.1.3",
          "form": "넵 저는 다담주에",
          "original_form": "넵 저는 다담주에",
          "speaker_id": "1",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020191.1.4",
          "form": "강릉가기로 했어요!!",
          "original_form": "강릉가기로 했어요!!",
          "speaker_id": "1",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020191.1.5",
          "form": "최근에 여행가신적없으신가요??",
          "original_form": "최근에 여행가신적없으신가요??",
          "speaker_id": "1",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020191.1.6",
          "form": "오 재밌겠어요!",
          "original_form": "오 재밌겠어요!",
          "speaker_id": "2",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020191.1.7",
          "form": "네 저는 오래됐어요 ㅜㅜ 호캉스만 갔어요",
          "original_form": "네 저는 오래됐어요 ㅜㅜ 호캉스만 갔어요",
          "speaker_id": "2",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020191.1.8",
          "form": "어느 호텔로 가셨나요!!",
          "original_form": "어느 호텔로 가셨나요!!",
          "speaker_id": "1",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020191.1.9",
          "form": "저도 호캉스해보고싶어요",
          "original_form": "저도 호캉스해보고싶어요",
          "speaker_id": "1",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020191.1.10",
          "form": "저는 그 시청역에 있는덴데",
          "original_form": "저는 그 시청역에 있는덴데",
          "speaker_id": "2",
          "time": "20210828 00:33"
        },
        {
          "id": "MDRW2100020191.1.11",
          "form": "이름을 잘 모르겠네요ㅜ",
          "original_form": "이름을 잘 모르겠네요ㅜ",
          "speaker_id": "2",
          "time": "20210828 00:33"
        },
        {
          "id": "MDRW2100020191.1.12",
          "form": "수영장있는 호텔이였나요??",
          "original_form": "수영장있는 호텔이였나요??",
          "speaker_id": "1",
          "time": "20210828 00:34"
        },
        {
          "id": "MDRW2100020191.1.13",
          "form": "아뇨 그냥 아담하고 깔끔한데였어요 ㅋㅋ",
          "original_form": "아뇨 그냥 아담하고 깔끔한데였어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:34"
        },
        {
          "id": "MDRW2100020191.1.14",
          "form": "아하! 호캉스 해본 후기는 어떤가요??",
          "original_form": "아하! 호캉스 해본 후기는 어떤가요??",
          "speaker_id": "1",
          "time": "20210828 00:37"
        },
        {
          "id": "MDRW2100020191.1.15",
          "form": "편하고 좋더라구요 돌아다닐거없이 ㅋㅋㅋ",
          "original_form": "편하고 좋더라구요 돌아다닐거없이 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100020191.1.16",
          "form": "오 역시..",
          "original_form": "오 역시..",
          "speaker_id": "1",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100020191.1.17",
          "form": "저도 다음에 저질체력인 친구랑 호캉스 한번 가보고싶네요 ㅎㅎ",
          "original_form": "저도 다음에 저질체력인 친구랑 호캉스 한번 가보고싶네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 00:39"
        },
        {
          "id": "MDRW2100020191.1.18",
          "form": "앜ㅋㅋㅋ 딱이네요",
          "original_form": "앜ㅋㅋㅋ 딱이네요",
          "speaker_id": "2",
          "time": "20210828 00:39"
        },
        {
          "id": "MDRW2100020191.1.19",
          "form": "돌아다니기 싫어하는 사람한테 안성맞춤이겠어요 ㅋㅋㅋ",
          "original_form": "돌아다니기 싫어하는 사람한테 안성맞춤이겠어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:39"
        },
        {
          "id": "MDRW2100020191.1.20",
          "form": "맞아요 ㅋㅋㅋ저랑 제친구돌아다니는거 별로 안좋아하거든요 ㅎㅎ",
          "original_form": "맞아요 ㅋㅋㅋ저랑 제친구돌아다니는거 별로 안좋아하거든요 ㅎㅎ",
