      "utterance": [
        {
          "id": "MDRW2100019035.1.1",
          "form": "탕수육도 시켜드셨나요",
          "original_form": "탕수육도 시켜드셨나요",
          "speaker_id": "1",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100019035.1.2",
          "form": "네 가족들도 있어서 탕수육도 같이 시켰어요",
          "original_form": "네 가족들도 있어서 탕수육도 같이 시켰어요",
          "speaker_id": "2",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100019035.1.3",
          "form": "선생님은 점심 늦게 드시네요",
          "original_form": "선생님은 점심 늦게 드시네요",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019035.1.4",
          "form": "탕수육 부먹이세요 찍먹이세요",
          "original_form": "탕수육 부먹이세요 찍먹이세요",
          "speaker_id": "1",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019035.1.5",
          "form": "아 저는 오늘 좀 일이있어서",
          "original_form": "아 저는 오늘 좀 일이있어서",
          "speaker_id": "1",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019035.1.6",
          "form": "식사가 늦었습니다ㅎ",
          "original_form": "식사가 늦었습니다ㅎ",
          "speaker_id": "1",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019035.1.7",
          "form": "아하 그러셨군요",
          "original_form": "아하 그러셨군요",
          "speaker_id": "2",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100019035.1.8",
          "form": "저는 찍먹파입니다 선생님은요?",
          "original_form": "저는 찍먹파입니다 선생님은요?",
          "speaker_id": "2",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100019035.1.9",
          "form": "저는 막먹파에요ㅎ 사실 가리지않아요",
          "original_form": "저는 막먹파에요ㅎ 사실 가리지않아요",
          "speaker_id": "1",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100019035.1.10",
          "form": "탕수육은 다 맛있으니까요",
          "original_form": "탕수육은 다 맛있으니까요",
          "speaker_id": "1",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019035.1.11",
          "form": "혹시 제가 어려우실까요.. 호칭 선생님이라 안하셔도 되는데.. 어...... 혹시 그게 편하신거라면",
          "original_form": "혹시 제가 어려우실까요.. 호칭 선생님이라 안하셔도 되는데.. 어...... 혹시 그게 편하신거라면",
          "speaker_id": "1",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019035.1.12",
          "form": "그렇게 부르셔도 상관은 없어요",
          "original_form": "그렇게 부르셔도 상관은 없어요",
          "speaker_id": "1",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019035.1.13",
          "form": "탕수육은 찹쌀 좋아하시나요 기본 좋아하시나요",
          "original_form": "탕수육은 찹쌀 좋아하시나요 기본 좋아하시나요",
          "speaker_id": "1",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019035.1.14",
          "form": "탕수육은 어떻게 먹든 맛있긴 하죠",
          "original_form": "탕수육은 어떻게 먹든 맛있긴 하죠",
          "speaker_id": "2",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019035.1.15",
          "form": "아 그게 편해서 선생님이라고 부르긴 했는데 재미님이라고 부르겠습니다",
          "original_form": "아 그게 편해서 선생님이라고 부르긴 했는데 재미님이라고 부르겠습니다",
          "speaker_id": "2",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019035.1.16",
          "form": "저는 기본 좋아해요 name1님은요?",
          "original_form": "저는 기본 좋아해요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019035.1.17",
          "form": "저도 기본이 좋아요",
          "original_form": "저도 기본이 좋아요",
          "speaker_id": "1",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019035.1.18",
          "form": "찹쌀은 뭔가.. 음 맛은 있는데 고기보단 밀가루 먹는 느낌이라",
          "original_form": "찹쌀은 뭔가.. 음 맛은 있는데 고기보단 밀가루 먹는 느낌이라",
          "speaker_id": "1",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019035.1.19",
          "form": "그쵸 찹쌀 쫀쫀함이라고 해야하나요.. 그게 탕수육 먹을 땐 별로더라구요",
          "original_form": "그쵸 찹쌀 쫀쫀함이라고 해야하나요.. 그게 탕수육 먹을 땐 별로더라구요",
          "speaker_id": "2",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019035.1.20",
          "form": "한식/양식/중식/일식 중 뭐를 제일 좋아하시나요?",
          "original_form": "한식/양식/중식/일식 중 뭐를 제일 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019035.1.21",
          "form": "저는 한식좋아해요",
          "original_form": "저는 한식좋아해요",
          "speaker_id": "1",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019035.1.22",
          "form": "뭔가 돌고 돌아도 다시 한식으로 돌아오는 기적",
          "original_form": "뭔가 돌고 돌아도 다시 한식으로 돌아오는 기적",
          "speaker_id": "1",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019035.1.23",
          "form": "name2님은 어떠신가요",
          "original_form": "&name2&님은 어떠신가요",
          "speaker_id": "1",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019035.1.24",
          "form": "저는 일식이요 초밥 좋아하거든요",
          "original_form": "저는 일식이요 초밥 좋아하거든요",
          "speaker_id": "2",
          "time": "20210827 15:00"
        },
        {
          "id": "MDRW2100019035.1.25",
          "form": "어떤 초밥 가장 좋아하시나요",
          "original_form": "어떤 초밥 가장 좋아하시나요",
          "speaker_id": "1",
          "time": "20210827 15:01"
        },
        {
          "id": "MDRW2100019035.1.26",
          "form": "근데 최근에 일본 방사능 오염수 방출 소식 듣고 먹기를 꺼려하고 있습니다..",
          "original_form": "근데 최근에 일본 방사능 오염수 방출 소식 듣고 먹기를 꺼려하고 있습니다..",
          "speaker_id": "2",
          "time": "20210827 15:01"
        },
        {
          "id": "MDRW2100019035.1.27",
          "form": "저 연어초밥이요",
          "original_form": "저 연어초밥이요",
          "speaker_id": "2",
          "time": "20210827 15:01"
        },
        {
          "id": "MDRW2100019035.1.28",
          "form": "아무래노 방사능 걱정이 크긴하죠",
          "original_form": "아무래노 방사능 걱정이 크긴하죠",
          "speaker_id": "1",
          "time": "20210827 15:01"
        }
      ]
