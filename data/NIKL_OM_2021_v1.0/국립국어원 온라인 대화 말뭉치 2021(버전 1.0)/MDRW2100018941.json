        },
        {
          "id": "MDRW2100018941.1.2",
          "form": "와 맛있었겠어요",
          "original_form": "와 맛있었겠어요",
          "speaker_id": "1",
          "time": "20210827 14:19"
        },
        {
          "id": "MDRW2100018941.1.3",
