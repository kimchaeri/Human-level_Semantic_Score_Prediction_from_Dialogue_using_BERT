          "speaker_id": "2",
          "time": "20210827 14:21"
        },
        {
          "id": "MDRW2100018941.1.5",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "1",
          "time": "20210827 14:22"
        },
        {
          "id": "MDRW2100018941.1.6",
          "form": "오랜머ㅡㅏㄴ에",
          "original_form": "오랜머ㅡㅏㄴ에",
          "speaker_id": "1",
          "time": "20210827 14:22"
        },
        {
          "id": "MDRW2100018941.1.7",
          "form": "점심짜장면때렸숨돠",
          "original_form": "점심짜장면때렸숨돠",
          "speaker_id": "1",
          "time": "20210827 14:23"
        },
        {
          "id": "MDRW2100018941.1.8",
          "form": "오 맛있는거 드셧군여",
          "original_form": "오 맛있는거 드셧군여",
          "speaker_id": "2",
          "time": "20210827 14:36"
        },
        {
          "id": "MDRW2100018941.1.9",
          "form": "곱배기 드시나요",
          "original_form": "곱배기 드시나요",
          "speaker_id": "2",
          "time": "20210827 14:36"
        },
        {
          "id": "MDRW2100018941.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㄱㅈㄲ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㄱㅈㄲ",
          "speaker_id": "1",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100018941.1.11",
          "form": "모르는사람이 곱배기드시나요한줄알고",
          "original_form": "모르는사람이 곱배기드시나요한줄알고",
          "speaker_id": "1",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100018941.1.12",
          "form": "내배를어째알지싶었어요",
          "original_form": "내배를어째알지싶었어요",
          "speaker_id": "1",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100018941.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100018941.1.14",
          "form": "보통먹고 탕슉먹었어요",
          "original_form": "보통먹고 탕슉먹었어요",
          "speaker_id": "1",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100018941.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100018941.1.16",
          "form": "영혼을 담아 대화합시다 예",
          "original_form": "영혼을 담아 대화합시다 예",
          "speaker_id": "2",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100018941.1.17",
          "form": "ㅋㅋㅋㅋㅋ영혼은 담는데 기억에 없어요..ㅋㅋㅋㄱ하..",
          "original_form": "ㅋㅋㅋㅋㅋ영혼은 담는데 기억에 없어요..ㅋㅋㅋㄱ하..",
          "speaker_id": "1",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100018941.1.18",
          "form": "오늘중식먹어놓고",
          "original_form": "오늘중식먹어놓고",
          "speaker_id": "1",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100018941.1.19",
          "form": "내일돼서 어제뭐먹엇냐하면기억못하겠죠..",
          "original_form": "내일돼서 어제뭐먹엇냐하면기억못하겠죠..",
          "speaker_id": "1",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100018941.1.20",
          "form": "ㅋㅋㅋㅋㅋ식단 일기를 열심히 써야겠군요",
          "original_form": "ㅋㅋㅋㅋㅋ식단 일기를 열심히 써야겠군요",
          "speaker_id": "2",
          "time": "20210827 15:03"
        },
        {
          "id": "MDRW2100018941.1.21",
          "form": "그냥일기도안쓰는데 식단일기..",
          "original_form": "그냥일기도안쓰는데 식단일기..",
          "speaker_id": "1",
          "time": "20210827 15:03"
        },
        {
          "id": "MDRW2100018941.1.22",
          "form": "ㅋㅋㅈㅋㅋㅋ",
          "original_form": "ㅋㅋㅈㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:04"
        },
        {
          "id": "MDRW2100018941.1.23",
          "form": "흐어",
          "original_form": "흐어",
          "speaker_id": "1",
          "time": "20210827 15:04"
        },
        {
          "id": "MDRW2100018941.1.24",
          "form": "뭐맛있는거없을까여",
          "original_form": "뭐맛있는거없을까여",
          "speaker_id": "1",
          "time": "20210827 15:04"
        },
        {
          "id": "MDRW2100018941.1.25",
          "form": "맛있는거 생각해내기가 정말 어렵다요",
          "original_form": "맛있는거 생각해내기가 정말 어렵다요",
          "speaker_id": "2",
          "time": "20210827 15:06"
        },
        {
          "id": "MDRW2100018941.1.26",
          "form": "색다른거먹고시포요",
          "original_form": "색다른거먹고시포요",
          "speaker_id": "1",
          "time": "20210827 15:07"
        },
        {
          "id": "MDRW2100018941.1.27",
          "form": "흑흑..",
          "original_form": "흑흑..",
          "speaker_id": "1",
          "time": "20210827 15:07"
        },
        {
          "id": "MDRW2100018941.1.28",
          "form": "맞아요 요즘엔 식당을 의도적으로 많이 검색해보고 있어요",
          "original_form": "맞아요 요즘엔 식당을 의도적으로 많이 검색해보고 있어요",
          "speaker_id": "2",
          "time": "20210827 15:08"
        },
        {
          "id": "MDRW2100018941.1.29",
          "form": "막도전해보고싶은용기는없는데",
          "original_form": "막도전해보고싶은용기는없는데",
          "speaker_id": "1",
          "time": "20210827 15:16"
        },
        {
          "id": "MDRW2100018941.1.30",
          "form": "색다른거먹고싶은",
          "original_form": "색다른거먹고싶은",
          "speaker_id": "1",
          "time": "20210827 15:16"
        },
