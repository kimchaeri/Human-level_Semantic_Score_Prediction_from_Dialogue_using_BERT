        },
        {
          "id": "MDRW2100028851.1.2",
          "form": "아프리카보다 트위치가 더 나은 거 같아요",
          "original_form": "아프리카보다 트위치가 더 나은 거 같아요",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028851.1.3",
          "form": "그쵸 저도 거기서 보다가",
          "original_form": "그쵸 저도 거기서 보다가",
          "speaker_id": "1",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100028851.1.4",
          "form": "게임비에이들 많이가길래 트위치로 간 ㅋㅋ",
          "original_form": "게임비에이들 많이가길래 트위치로 간 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100028851.1.5",
          "form": "비제이들",
          "original_form": "비제이들",
          "speaker_id": "1",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100028851.1.6",
          "form": "그니까요 원래 아프리카였는데",
          "original_form": "그니까요 원래 아프리카였는데",
          "speaker_id": "2",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100028851.1.7",
          "form": "너무 논란이 많긴 했어요",
          "original_form": "너무 논란이 많긴 했어요",
          "speaker_id": "2",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100028851.1.8",
          "form": "있을때  잘했어야지~",
          "original_form": "있을때  잘했어야지~",
          "speaker_id": "1",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100028851.1.9",
          "form": "ㅋㅋㅋㅋㅌ에휴 아프리카",
          "original_form": "ㅋㅋㅋㅋㅌ에휴 아프리카",
          "speaker_id": "2",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100028851.1.10",
          "form": "마썸카 아시나요",
          "original_form": "마썸카 아시나요",
          "speaker_id": "1",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100028851.1.11",
          "form": "예전에 왁굳이형이 하지 않았나요..?",
          "original_form": "예전에 왁굳이형이 하지 않았나요..?",
          "speaker_id": "2",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100028851.1.12",
          "form": "재밌다고 이야기 들었어요",
          "original_form": "재밌다고 이야기 들었어요",
          "speaker_id": "2",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100028851.1.13",
          "form": "오오 풍월량 버전도 재밌어요",
