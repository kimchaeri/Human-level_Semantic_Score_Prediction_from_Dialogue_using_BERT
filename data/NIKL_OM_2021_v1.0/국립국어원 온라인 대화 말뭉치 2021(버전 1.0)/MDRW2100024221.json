      "utterance": [
        {
          "id": "MDRW2100024221.1.1",
          "form": "저도 싫을것 같아여ㅠㅠ",
          "original_form": "저도 싫을것 같아여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024221.1.2",
          "form": "저도 무서운데 ㅠㅠ 멀쩡한 사람들도 사망기사 나오니 ㅠㅠ",
          "original_form": "저도 무서운데 ㅠㅠ 멀쩡한 사람들도 사망기사 나오니 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100024221.1.3",
          "form": "아직 저희애는 10세라 해당이 없지만 ㅠㅠ",
          "original_form": "아직 저희애는 10세라 해당이 없지만 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100024221.1.4",
          "form": "봉사 다녀보셨나요??",
          "original_form": "봉사 다녀보셨나요??",
          "speaker_id": "1",
          "time": "20210830 23:47"
        },
        {
          "id": "MDRW2100024221.1.5",
          "form": "대학생 때 교육봉사하고",
          "original_form": "대학생 때 교육봉사하고",
          "speaker_id": "2",
          "time": "20210831 07:32"
        },
        {
          "id": "MDRW2100024221.1.6",
          "form": "방학 때 단기적으로 몇번 해봤어요",
          "original_form": "방학 때 단기적으로 몇번 해봤어요",
          "speaker_id": "2",
          "time": "20210831 07:33"
        },
        {
          "id": "MDRW2100024221.1.7",
          "form": "아 저도요 ㅎㅎ 요즘은 코로나때문에 일반봉사활동은 많이 못 갈것 같네여ㅠ\n`",
          "original_form": "아 저도요 ㅎㅎ 요즘은 코로나때문에 일반봉사활동은 많이 못 갈것 같네여ㅠ\n`",
          "speaker_id": "1",
          "time": "20210831 08:17"
        },
        {
          "id": "MDRW2100024221.1.8",
          "form": "네 특히 대면봉사는 힘들죠ㅠ",
          "original_form": "네 특히 대면봉사는 힘들죠ㅠ",
          "speaker_id": "2",
          "time": "20210831 08:22"
        },
        {
          "id": "MDRW2100024221.1.9",
          "form": "맞아요 어떤봉사가 기억이 제일 나세요??",
          "original_form": "맞아요 어떤봉사가 기억이 제일 나세요??",
          "speaker_id": "1",
          "time": "20210831 08:24"
        },
        {
          "id": "MDRW2100024221.1.10",
          "form": "저는 대학생때 시각장애인들 위해서 책 타이핑하는 봉사했는데",
          "original_form": "저는 대학생때 시각장애인들 위해서 책 타이핑하는 봉사했는데",
          "speaker_id": "2",
          "time": "20210831 08:34"
        },
        {
          "id": "MDRW2100024221.1.11",
          "form": "불현듯 그 생각이 나네요",
          "original_form": "불현듯 그 생각이 나네요",
          "speaker_id": "2",
          "time": "20210831 08:34"
        },
        {
          "id": "MDRW2100024221.1.12",
          "form": "그건 요즘 시국에도 할 수 있을 것 같은데",
          "original_form": "그건 요즘 시국에도 할 수 있을 것 같은데",
          "speaker_id": "2",
          "time": "20210831 08:34"
        },
        {
          "id": "MDRW2100024221.1.13",
          "form": "아 그러개요 그런거 가능하겟네요",
          "original_form": "아 그러개요 그런거 가능하겟네요",
          "speaker_id": "1",
          "time": "20210831 08:39"
        },
        {
          "id": "MDRW2100024221.1.14",
          "form": "네 한번 찾아봐야겠어요",
          "original_form": "네 한번 찾아봐야겠어요",
          "speaker_id": "2",
          "time": "20210831 08:54"
        },
        {
          "id": "MDRW2100024221.1.15",
          "form": "요즘도 봉사하는지",
          "original_form": "요즘도 봉사하는지",
          "speaker_id": "2",
          "time": "20210831 08:54"
        },
        {
          "id": "MDRW2100024221.1.16",
          "form": "집에서도 할 수 있으니 좋은것 같네요",
          "original_form": "집에서도 할 수 있으니 좋은것 같네요",
          "speaker_id": "1",
          "time": "20210831 08:56"
        },
        {
          "id": "MDRW2100024221.1.17",
          "form": "네 타이핑하는 게 좀 피곤하긴 했지만",
          "original_form": "네 타이핑하는 게 좀 피곤하긴 했지만",
          "speaker_id": "2",
          "time": "20210831 13:29"
        },
        {
          "id": "MDRW2100024221.1.18",
          "form": "지금 시국에도 할 수 있는 봉사라",
          "original_form": "지금 시국에도 할 수 있는 봉사라",
          "speaker_id": "2",
          "time": "20210831 13:29"
        },
        {
          "id": "MDRW2100024221.1.19",
          "form": "재밌을것 같네요~ ㅎㅎ",
          "original_form": "재밌을것 같네요~ ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 13:49"
        },
        {
          "id": "MDRW2100024221.1.20",
          "form": "네네 재미도 보람도 있는 봉사",
          "original_form": "네네 재미도 보람도 있는 봉사",
          "speaker_id": "2",
          "time": "20210831 13:54"
        },
        {
          "id": "MDRW2100024221.1.21",
          "form": "그니까요ㅎㅎㅎ",
          "original_form": "그니까요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 13:55"
        }
      ]
