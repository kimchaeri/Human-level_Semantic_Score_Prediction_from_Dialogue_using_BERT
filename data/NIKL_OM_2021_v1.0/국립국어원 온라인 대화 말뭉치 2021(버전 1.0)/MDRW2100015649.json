      "utterance": [
        {
          "id": "MDRW2100015649.1.1",
          "form": "네. 많이하게되더라고요",
          "original_form": "네. 많이하게되더라고요",
          "speaker_id": "2",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015649.1.2",
          "form": "요리 좋아하세요?",
          "original_form": "요리 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015649.1.3",
          "form": "사먹는것보다 저렴하고 \r\n맛이 만족스러워서 열심히 만들어먹고잏어요",
          "original_form": "사먹는것보다 저렴하고 \r\n맛이 만족스러워서 열심히 만들어먹고잏어요",
          "speaker_id": "2",
          "time": "20210826 01:42"
        },
        {
          "id": "MDRW2100015649.1.4",
          "form": "그렇군요. 저는 요리하는거보다 남이 만들어준 음식이 더 좋네요",
          "original_form": "그렇군요. 저는 요리하는거보다 남이 만들어준 음식이 더 좋네요",
          "speaker_id": "1",
          "time": "20210826 01:43"
        },
        {
          "id": "MDRW2100015649.1.5",
          "form": "오 그렇군요.\r\n스스로 요리는 안하시나요",
          "original_form": "오 그렇군요.\r\n스스로 요리는 안하시나요",
          "speaker_id": "2",
          "time": "20210826 02:36"
        },
        {
          "id": "MDRW2100015649.1.6",
          "form": "자주는 아니지만 해요",
          "original_form": "자주는 아니지만 해요",
          "speaker_id": "1",
          "time": "20210826 02:37"
        },
        {
          "id": "MDRW2100015649.1.7",
          "form": "무슨요리위주로하시는지 궁금해요",
          "original_form": "무슨요리위주로하시는지 궁금해요",
          "speaker_id": "2",
          "time": "20210826 02:37"
        },
        {
          "id": "MDRW2100015649.1.8",
          "form": "주로 양식을 해요",
          "original_form": "주로 양식을 해요",
          "speaker_id": "1",
          "time": "20210826 02:38"
        },
        {
          "id": "MDRW2100015649.1.9",
          "form": "양식 요리를 주로 합니다",
          "original_form": "양식 요리를 주로 합니다",
          "speaker_id": "1",
          "time": "20210826 02:42"
        },
        {
          "id": "MDRW2100015649.1.10",
          "form": "오 특히  자신있는 요리가 뭐죠?",
          "original_form": "오 특히  자신있는 요리가 뭐죠?",
          "speaker_id": "2",
          "time": "20210826 13:03"
        },
        {
          "id": "MDRW2100015649.1.11",
          "form": "스파게티요",
          "original_form": "스파게티요",
          "speaker_id": "1",
          "time": "20210826 13:03"
        },
        {
          "id": "MDRW2100015649.1.12",
          "form": "저는 경양식 소스 잘만들어요.",
          "original_form": "저는 경양식 소스 잘만들어요.",
          "speaker_id": "2",
          "time": "20210826 13:03"
        },
        {
          "id": "MDRW2100015649.1.13",
          "form": "경양식 소스가 뭐죠?",
          "original_form": "경양식 소스가 뭐죠?",
          "speaker_id": "1",
          "time": "20210826 13:04"
        },
        {
          "id": "MDRW2100015649.1.14",
          "form": "어릴적 부모님이랑 먹던 레스토랑에서파는 돈까스소스맛이요",
          "original_form": "어릴적 부모님이랑 먹던 레스토랑에서파는 돈까스소스맛이요",
          "speaker_id": "2",
          "time": "20210826 13:04"
        },
        {
          "id": "MDRW2100015649.1.15",
          "form": "추억의 맛인가 보네요",
          "original_form": "추억의 맛인가 보네요",
          "speaker_id": "1",
          "time": "20210826 13:04"
        }
