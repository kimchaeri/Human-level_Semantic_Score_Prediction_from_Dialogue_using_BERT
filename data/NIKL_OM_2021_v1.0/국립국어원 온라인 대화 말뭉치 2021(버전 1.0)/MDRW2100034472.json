        },
        {
          "id": "MDRW2100034472.1.2",
          "form": "1시밖에 안되었는데 벌써",
          "original_form": "1시밖에 안되었는데 벌써",
          "speaker_id": "1",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100034472.1.3",
          "form": "아점으로 먹었지",
          "original_form": "아점으로 먹었지",
          "speaker_id": "2",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100034472.1.4",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100034472.1.5",
          "form": "넌",
          "original_form": "넌",
          "speaker_id": "2",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100034472.1.6",
          "form": "난 아직  먹어야한다",
          "original_form": "난 아직  먹어야한다",
          "speaker_id": "1",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100034472.1.7",
          "form": "뭐 먹을려고 하는데",
          "original_form": "뭐 먹을려고 하는데",
          "speaker_id": "2",
          "time": "20210828 13:09"
        },
        {
          "id": "MDRW2100034472.1.8",
          "form": "어제 빵집 가서 빵 사온게 남아서 먹어야한다",
          "original_form": "어제 빵집 가서 빵 사온게 남아서 먹어야한다",
          "speaker_id": "1",
          "time": "20210828 13:09"
        },
        {
          "id": "MDRW2100034472.1.9",
          "form": "그거갖고 밥 되겠나",
          "original_form": "그거갖고 밥 되겠나",
          "speaker_id": "2",
          "time": "20210828 13:09"
        },
        {
          "id": "MDRW2100034472.1.10",
          "form": "우유랑 같이 먹으면 될꺼 같다",
          "original_form": "우유랑 같이 먹으면 될꺼 같다",
          "speaker_id": "1",
          "time": "20210828 13:09"
        },
        {
          "id": "MDRW2100034472.1.11",
          "form": "엉 배고프겠다 빨리 먹어라",
          "original_form": "엉 배고프겠다 빨리 먹어라",
          "speaker_id": "2",
          "time": "20210828 13:10"
        },
        {
          "id": "MDRW2100034472.1.12",
          "form": "토스트 비슷한거라 전자렌지 데워 먹어야한다",
          "original_form": "토스트 비슷한거라 전자렌지 데워 먹어야한다",
          "speaker_id": "1",
          "time": "20210828 13:10"
        },
        {
          "id": "MDRW2100034472.1.13",
          "form": "너무 많이 데우지 말고 살짝 데워라",
          "original_form": "너무 많이 데우지 말고 살짝 데워라",
          "speaker_id": "2",
          "time": "20210828 13:11"
        },
        {
          "id": "MDRW2100034472.1.14",
          "form": "응 한 30초만 데울려고",
          "original_form": "응 한 30초만 데울려고",
          "speaker_id": "1",
          "time": "20210828 13:11"
        },
        {
          "id": "MDRW2100034472.1.15",
          "form": "파스타 만큼 간단하게 먹기 좋은게 없는거 같아",
