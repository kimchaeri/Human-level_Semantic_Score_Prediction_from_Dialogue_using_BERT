      "utterance": [
        {
          "id": "MDRW2100006230.1.1",
          "form": "누님 저 공원에서 곰표맥주 고고함",
          "original_form": "누님 저 공원에서 곰표맥주 고고함",
          "speaker_id": "2",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100006230.1.2",
          "form": "아닠 주당에겐 너무 순한맛 아닌가요",
          "original_form": "아닠 주당에겐 너무 순한맛 아닌가요",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100006230.1.3",
          "form": "잠깐",
          "original_form": "잠깐",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100006230.1.4",
          "form": "소주 두병 마시고 또?",
          "original_form": "소주 두병 마시고 또?",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100006230.1.5",
          "form": "ㄷㄷ내일 괜찮으세요",
          "original_form": "ㄷㄷ내일 괜찮으세요",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100006230.1.6",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100006230.1.7",
          "form": "땅바닥에 앉아서",
          "original_form": "땅바닥에 앉아서",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100006230.1.8",
          "form": "고고중",
          "original_form": "고고중",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100006230.1.9",
          "form": "바닷가 엎입니다",
          "original_form": "바닷가 엎입니다",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100006230.1.10",
          "form": "아이고ㄷㄷ 안주는요!",
          "original_form": "아이고ㄷㄷ 안주는요!",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100006230.1.11",
          "form": "소주 먹고 맥주 마심 속 더 버려욧!",
          "original_form": "소주 먹고 맥주 마심 속 더 버려욧!",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100006230.1.12",
          "form": "그 맥주가 다 소주처럼 독해져서ㅜ",
          "original_form": "그 맥주가 다 소주처럼 독해져서ㅜ",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100006230.1.13",
          "form": "피할수없습니다",
          "original_form": "피할수없습니다",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100006230.1.14",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100006230.1.15",
          "form": "아닠  상대방이 님보다 더한 주당인가요",
          "original_form": "아닠  상대방이 님보다 더한 주당인가요",
          "speaker_id": "1",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100006230.1.16",
          "form": "미치인놈입니다",
          "original_form": "미치인놈입니다",
          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100006230.1.17",
          "form": "동생인데",
          "original_form": "동생인데",
          "speaker_id": "2",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100006230.1.18",
          "form": "용서치 않아요",
          "original_form": "용서치 않아요",
          "speaker_id": "2",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100006230.1.19",
          "form": "누나",
          "original_form": "누나",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100006230.1.20",
          "form": "어디 회사애요?",
          "original_form": "어디 회사애요?",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100006230.1.21",
          "form": "난 전기연구원인거",
          "original_form": "난 전기연구원인거",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100006230.1.22",
          "form": "아는데",
          "original_form": "아는데",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100006230.1.23",
          "form": "누나 어디?",
          "original_form": "누나 어디?",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100006230.1.24",
          "form": "아니 직종 알믄 다 알았쥬!",
          "original_form": "아니 직종 알믄 다 알았쥬!",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100006230.1.25",
          "form": "아 사실 중요한건  아니지",
          "original_form": "아 사실 중요한건  아니지",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100006230.1.26",
          "form": "진짜 안주는 안 먹남요ㅜ",
          "original_form": "진짜 안주는 안 먹남요ㅜ",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100006230.1.27",
          "form": "그냥 궁금 ㅎㅎ",
          "original_form": "그냥 궁금 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100006230.1.28",
          "form": "닭꼬지",
          "original_form": "닭꼬지",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100006230.1.29",
          "form": "아니 닭꼬치를 이시간에 파는데가 있었나요",
          "original_form": "아니 닭꼬치를 이시간에 파는데가 있었나요",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100006230.1.30",
          "form": "푸드트럭있서써요",
          "original_form": "푸드트럭있서써요",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100006230.1.31",
          "form": "좋은 동네네요",
          "original_form": "좋은 동네네요",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100006230.1.32",
          "form": "누나동네는",
          "original_form": "누나동네는",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100006230.1.33",
          "form": "바닷가 근처서 닭꼬치먹으며 맥주라니ㅜㅜㅜ",
          "original_form": "바닷가 근처서 닭꼬치먹으며 맥주라니ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100006230.1.34",
          "form": "24시간아님?",
          "original_form": "24시간아님?",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100006230.1.35",
          "form": "서울이자냐",
          "original_form": "서울이자냐",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100006230.1.36",
          "form": "바닷가라서 너무 좋아요",
          "original_form": "바닷가라서 너무 좋아요",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100006230.1.37",
          "form": "거리두기이후론 많이 줄어서ㅜ",
          "original_form": "거리두기이후론 많이 줄어서ㅜ",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100006230.1.38",
          "form": "누나",
          "original_form": "누나",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100006230.1.39",
          "form": "기자 해서 취재다니고 하면",
          "original_form": "기자 해서 취재다니고 하면",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100006230.1.40",
          "form": "지방안옴?",
          "original_form": "지방안옴?",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100006230.1.41",
          "form": "아니ㅋㅋ 가기야 하죠",
          "original_form": "아니ㅋㅋ 가기야 하죠",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100006230.1.42",
          "form": "근데 잘 안 가죠ㅎ",
          "original_form": "근데 잘 안 가죠ㅎ",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100006230.1.43",
          "form": "주재기자도 있고",
          "original_form": "주재기자도 있고",
          "speaker_id": "1",
          "time": "20210907 22:32"
        }
      ]
    }
  ]
