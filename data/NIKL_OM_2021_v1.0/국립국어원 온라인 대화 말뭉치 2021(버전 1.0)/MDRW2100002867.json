      "utterance": [
        {
          "id": "MDRW2100002867.1.1",
          "form": "저녁 어떤거 드신다고 하셨죠?",
          "original_form": "저녁 어떤거 드신다고 하셨죠?",
          "speaker_id": "2",
          "time": "20210820 16:08"
        },
        {
          "id": "MDRW2100002867.1.2",
          "form": "집밥이요ㅎ 동그랑땡굽고",
          "original_form": "집밥이요ㅎ 동그랑땡굽고",
          "speaker_id": "1",
          "time": "20210820 16:09"
        },
        {
          "id": "MDRW2100002867.1.3",
          "form": "찌개나국을 하나 끓여야할텐데.. ㅋㅋㅋㄱ",
          "original_form": "찌개나국을 하나 끓여야할텐데.. ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210820 16:09"
        },
        {
          "id": "MDRW2100002867.1.4",
          "form": "아~~동그랑땡!!맛나겠어요",
          "original_form": "아~~동그랑땡!!맛나겠어요",
          "speaker_id": "2",
          "time": "20210820 16:09"
        },
        {
          "id": "MDRW2100002867.1.5",
          "form": "미역국?",
          "original_form": "미역국?",
          "speaker_id": "2",
          "time": "20210820 16:09"
        },
        {
          "id": "MDRW2100002867.1.6",
          "form": "아~ 고기가없어요.ㅠ 냉장고좀 나중에 뒤적뒤적 해봐야겠어요ㅠ",
          "original_form": "아~ 고기가없어요.ㅠ 냉장고좀 나중에 뒤적뒤적 해봐야겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210820 16:11"
        },
        {
          "id": "MDRW2100002867.1.7",
          "form": "애들 있어서 맑은 국을 주로 하실것 같아요",
          "original_form": "애들 있어서 맑은 국을 주로 하실것 같아요",
          "speaker_id": "2",
          "time": "20210820 16:12"
        },
        {
          "id": "MDRW2100002867.1.8",
          "form": "근데 첫째는 매운것도 잘먹고ㅎㅎ 둘째는국을 싫어라해서ㅠㅠ",
          "original_form": "근데 첫째는 매운것도 잘먹고ㅎㅎ 둘째는국을 싫어라해서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 16:12"
        },
        {
          "id": "MDRW2100002867.1.9",
          "form": "아~~그럼 아무 국이나 해도 되겠어요",
          "original_form": "아~~그럼 아무 국이나 해도 되겠어요",
          "speaker_id": "2",
          "time": "20210820 16:13"
        },
        {
          "id": "MDRW2100002867.1.10",
          "form": "네ㅋㅋ 냉장고에서 시들어가는 콩나물을 살려줘야 하는데",
          "original_form": "네ㅋㅋ 냉장고에서 시들어가는 콩나물을 살려줘야 하는데",
          "speaker_id": "1",
          "time": "20210820 16:14"
        },
        {
          "id": "MDRW2100002867.1.11",
          "form": "그럼 콩나물국?",
          "original_form": "그럼 콩나물국?",
          "speaker_id": "2",
          "time": "20210820 16:14"
        },
        {
          "id": "MDRW2100002867.1.12",
          "form": "그래야겠어요 오징어넣고ㅎㅎ",
          "original_form": "그래야겠어요 오징어넣고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002867.1.13",
          "form": "굿굿 ~~맛나겠어요",
          "original_form": "굿굿 ~~맛나겠어요",
          "speaker_id": "2",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002867.1.14",
          "form": "콩나물 다듬어서 국 끓이시나요?",
          "original_form": "콩나물 다듬어서 국 끓이시나요?",
          "speaker_id": "2",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002867.1.15",
