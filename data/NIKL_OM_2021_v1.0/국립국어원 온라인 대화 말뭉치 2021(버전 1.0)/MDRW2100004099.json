      "utterance": [
        {
          "id": "MDRW2100004099.1.1",
          "form": "이번에 휴가 어디로 여행 다녀왔어??",
          "original_form": "이번에 휴가 어디로 여행 다녀왔어??",
          "speaker_id": "1",
          "time": "20210821 14:55"
        },
        {
          "id": "MDRW2100004099.1.2",
          "form": "나는 좀 오래됐지만 제주도 갔다왔어",
          "original_form": "나는 좀 오래됐지만 제주도 갔다왔어",
          "speaker_id": "2",
          "time": "20210821 15:01"
        },
        {
          "id": "MDRW2100004099.1.3",
          "form": "제주도 좋았겟다",
          "original_form": "제주도 좋았겟다",
          "speaker_id": "1",
          "time": "20210821 15:02"
        },
        {
          "id": "MDRW2100004099.1.4",
          "form": "친구랑 다녀왓어?",
          "original_form": "친구랑 다녀왓어?",
          "speaker_id": "1",
          "time": "20210821 15:02"
        },
        {
          "id": "MDRW2100004099.1.5",
          "form": "응 둘이 갔다왔어 짧게!",
          "original_form": "응 둘이 갔다왔어 짧게!",
          "speaker_id": "2",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100004099.1.6",
          "form": "큰 계획 없이 버스타고 다녔는데 좋더라고",
          "original_form": "큰 계획 없이 버스타고 다녔는데 좋더라고",
          "speaker_id": "2",
          "time": "20210821 15:05"
        },
        {
          "id": "MDRW2100004099.1.7",
          "form": "우아 버스 타고 다녔구나",
          "original_form": "우아 버스 타고 다녔구나",
          "speaker_id": "1",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004099.1.8",
          "form": "바다도 다녀왓어?",
          "original_form": "바다도 다녀왓어?",
          "speaker_id": "1",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004099.1.9",
          "form": "응 거의 바다위주로 다닌거같애 바다는 매일 봐도 좋더라 내가 물 좋아해서",
          "original_form": "응 거의 바다위주로 다닌거같애 바다는 매일 봐도 좋더라 내가 물 좋아해서",
          "speaker_id": "2",
          "time": "20210821 15:08"
        },
        {
          "id": "MDRW2100004099.1.10",
          "form": "나도 코로나 풀리면 제주도 갈까 생각중이야!!",
          "original_form": "나도 코로나 풀리면 제주도 갈까 생각중이야!!",
          "speaker_id": "1",
          "time": "20210821 15:10"
        },
        {
          "id": "MDRW2100004099.1.11",
          "form": "흑돼지가 맛있더라구",
          "original_form": "흑돼지가 맛있더라구",
          "speaker_id": "1",
          "time": "20210821 15:10"
        },
        {
          "id": "MDRW2100004099.1.12",
          "form": "아하 나는 고기는 안 먹지만 뭘 먹어도 여행가서 먹으면 좀 다른 거 같애서 좋더라구 그게 여행의 맛이겠지만",
          "original_form": "아하 나는 고기는 안 먹지만 뭘 먹어도 여행가서 먹으면 좀 다른 거 같애서 좋더라구 그게 여행의 맛이겠지만",
          "speaker_id": "2",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004099.1.13",
          "form": "그치그치!!",
          "original_form": "그치그치!!",
          "speaker_id": "1",
          "time": "20210821 18:02"
        },
        {
          "id": "MDRW2100004099.1.14",
          "form": "다른나라도 다녀봤어??",
          "original_form": "다른나라도 다녀봤어??",
          "speaker_id": "1",
          "time": "20210821 18:02"
        },
        {
          "id": "MDRW2100004099.1.15",
          "form": "해외는 예전에 일본만!",
          "original_form": "해외는 예전에 일본만!",
          "speaker_id": "2",
          "time": "20210821 18:02"
        },
        {
          "id": "MDRW2100004099.1.16",
          "form": "그리고 진짜 어릴 때 중국?",
          "original_form": "그리고 진짜 어릴 때 중국?",
          "speaker_id": "2",
          "time": "20210821 18:02"
        },
        {
          "id": "MDRW2100004099.1.17",
          "form": "일본 가봤어?",
          "original_form": "일본 가봤어?",
          "speaker_id": "2",
          "time": "20210821 18:02"
        },
        {
          "id": "MDRW2100004099.1.18",
          "form": "나는 아직 한번도 안가봣어 ㅠㅜ",
          "original_form": "나는 아직 한번도 안가봣어 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004099.1.19",
          "form": "유럽쪽은 가보고 싶지않아??",
          "original_form": "유럽쪽은 가보고 싶지않아??",
          "speaker_id": "1",
          "time": "20210821 18:03"
        },
        {
          "id": "MDRW2100004099.1.20",
          "form": "아하ㅜㅜ 웅 유럽쪽도 가보고 싶지.. 항상 영국이 꼭 가고 싶었어 나는~",
          "original_form": "아하ㅜㅜ 웅 유럽쪽도 가보고 싶지.. 항상 영국이 꼭 가고 싶었어 나는~",
          "speaker_id": "2",
          "time": "20210821 18:04"
        },
        {
          "id": "MDRW2100004099.1.21",
          "form": "이번에 돈 모아서 코로나 풀리면 가게 !!",
          "original_form": "이번에 돈 모아서 코로나 풀리면 가게 !!",
