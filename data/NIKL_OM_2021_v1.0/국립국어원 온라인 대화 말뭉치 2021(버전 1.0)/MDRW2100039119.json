      "utterance": [
        {
          "id": "MDRW2100039119.1.1",
          "form": "저 알파카 인형 샀어요!",
          "original_form": "저 알파카 인형 샀어요!",
          "speaker_id": "2",
          "time": "20210903 10:44"
        },
        {
          "id": "MDRW2100039119.1.2",
          "form": "겨울에 안고 자려구요",
          "original_form": "겨울에 안고 자려구요",
          "speaker_id": "2",
          "time": "20210903 10:44"
        },
        {
          "id": "MDRW2100039119.1.3",
          "form": "우와알파카인형 너무귀엽겠어요",
          "original_form": "우와알파카인형 너무귀엽겠어요",
          "speaker_id": "1",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100039119.1.4",
          "form": "완전 귀여워요오",
          "original_form": "완전 귀여워요오",
          "speaker_id": "2",
          "time": "20210903 10:54"
        },
        {
          "id": "MDRW2100039119.1.5",
          "form": "가을옷 하나 더사고싶네용",
          "original_form": "가을옷 하나 더사고싶네용",
          "speaker_id": "1",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100039119.1.6",
          "form": "어떤 옷으로요?",
          "original_form": "어떤 옷으로요?",
          "speaker_id": "2",
          "time": "20210903 11:05"
        },
        {
          "id": "MDRW2100039119.1.7",
          "form": "니트원피스랑 하얀바지사고파요",
          "original_form": "니트원피스랑 하얀바지사고파요",
          "speaker_id": "1",
          "time": "20210903 14:49"
        },
        {
          "id": "MDRW2100039119.1.8",
          "form": "와 하얀바지는 깨끗하게 못 입을까봐 엄두도 잘 안내는데",
          "original_form": "와 하얀바지는 깨끗하게 못 입을까봐 엄두도 잘 안내는데",
          "speaker_id": "2",
          "time": "20210903 15:07"
        },
        {
          "id": "MDRW2100039119.1.9",
          "form": "저도 그런데 너무 이뻐요ㅠㅠㅋㅋㅋㅋ",
          "original_form": "저도 그런데 너무 이뻐요ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:13"
        },
        {
          "id": "MDRW2100039119.1.10",
          "form": "ㅋㅋ 만약 구매하시면",
          "original_form": "ㅋㅋ 만약 구매하시면",
          "speaker_id": "2",
          "time": "20210903 15:13"
        },
        {
          "id": "MDRW2100039119.1.11",
          "form": "조심해서 입으세요",
          "original_form": "조심해서 입으세요",
          "speaker_id": "2",
          "time": "20210903 15:13"
        },
        {
          "id": "MDRW2100039119.1.12",
          "form": "흰바지는 매번 사면 버리게되던데 또 사고파요 ㅋㅋ",
          "original_form": "흰바지는 매번 사면 버리게되던데 또 사고파요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:14"
        },
        {
          "id": "MDRW2100039119.1.13",
          "form": "흰바지 중독인가요ㄷㄷ",
          "original_form": "흰바지 중독인가요ㄷㄷ",
          "speaker_id": "2",
          "time": "20210903 15:14"
        },
        {
          "id": "MDRW2100039119.1.14",
          "form": "그런가봐요 ㅋㅋㅋ",
          "original_form": "그런가봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:14"
        },
        {
          "id": "MDRW2100039119.1.15",
          "form": "패션 좋아해요",
          "original_form": "패션 좋아해요",
          "speaker_id": "1",
          "time": "20210903 15:14"
        },
        {
          "id": "MDRW2100039119.1.16",
          "form": "패션쪽에 관심이 많으셨구나",
