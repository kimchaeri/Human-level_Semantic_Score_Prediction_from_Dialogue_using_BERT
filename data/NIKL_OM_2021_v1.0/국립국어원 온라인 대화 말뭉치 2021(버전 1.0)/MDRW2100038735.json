      "utterance": [
        {
          "id": "MDRW2100038735.1.1",
          "form": "나으 해방 일지 라는 드라마도 있네",
          "original_form": "나으 해방 일지 라는 드라마도 있네",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100038735.1.2",
          "form": "드라마 정말 많네요",
          "original_form": "드라마 정말 많네요",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100038735.1.3",
          "form": "여긴 주인공이 누구예요?",
          "original_form": "여긴 주인공이 누구예요?",
          "speaker_id": "1",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100038735.1.4",
          "form": "이건 이제 내년 방송예정",
          "original_form": "이건 이제 내년 방송예정",
          "speaker_id": "2",
          "time": "20210902 21:12"
        },
        {
          "id": "MDRW2100038735.1.5",
          "form": "이민기 김지원이 주연이네요",
          "original_form": "이민기 김지원이 주연이네요",
          "speaker_id": "1",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100038735.1.6",
          "form": "나의 아저씨 작가네요",
          "original_form": "나의 아저씨 작가네요",
          "speaker_id": "1",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100038735.1.7",
          "form": "여주는 김지원이다",
          "original_form": "여주는 김지원이다",
          "speaker_id": "2",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100038735.1.8",
          "form": "이민기는 별로 맘에 안들어요",
          "original_form": "이민기는 별로 맘에 안들어요",
          "speaker_id": "1",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100038735.1.9",
          "form": "노답 인생에서 탈출을 원하는 세남매와 미스터리 외지인",
          "original_form": "노답 인생에서 탈출을 원하는 세남매와 미스터리 외지인",
          "speaker_id": "2",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100038735.1.10",
          "form": "미스터리 외지인은 누구죠?",
          "original_form": "미스터리 외지인은 누구죠?",
          "speaker_id": "1",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100038735.1.11",
          "form": "출연진이 좋은듯",
          "original_form": "출연진이 좋은듯",
          "speaker_id": "2",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100038735.1.12",
          "form": "무엇보다 작가가 맘에 들어 보고 싶네요",
          "original_form": "무엇보다 작가가 맘에 들어 보고 싶네요",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100038735.1.13",
          "form": "손석구가 미스터리 외지인",
          "original_form": "손석구가 미스터리 외지인",
          "speaker_id": "2",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100038735.1.14",
          "form": "아 손석구 연기 잘 하던데",
          "original_form": "아 손석구 연기 잘 하던데",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100038735.1.15",
          "form": "세남매의 아버지가 천호진",
          "original_form": "세남매의 아버지가 천호진",
          "speaker_id": "2",
