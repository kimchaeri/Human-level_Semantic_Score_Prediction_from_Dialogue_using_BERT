        {
          "id": "MDRW2100031210.1.1",
          "form": "지코 갈 거예요",
          "original_form": "지코 갈 거예요",
          "speaker_id": "1",
          "time": "20210904 18:14"
        },
        {
          "id": "MDRW2100031210.1.2",
          "form": "집에 머무르 시죠",
          "original_form": "집에 머무르 시죠",
          "speaker_id": "2",
          "time": "20210904 18:14"
        },
        {
          "id": "MDRW2100031210.1.3",
          "form": "이번에 읽어야 할 책이 많아요",
          "original_form": "이번에 읽어야 할 책이 많아요",
          "speaker_id": "1",
          "time": "20210904 18:14"
        },
        {
          "id": "MDRW2100031210.1.4",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "2",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100031210.1.5",
          "form": "내 그래서 집에만 있으려구요",
          "original_form": "내 그래서 집에만 있으려구요",
          "speaker_id": "1",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100031210.1.6",
          "form": "책만 보기에는 시간이 여유롭지 났나요",
          "original_form": "책만 보기에는 시간이 여유롭지 났나요",
          "speaker_id": "2",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100031210.1.7",
          "form": "아니에요 부족해요",
          "original_form": "아니에요 부족해요",
          "speaker_id": "1",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100031210.1.8",
          "form": "휴일이 더 필요하시군요",
          "original_form": "휴일이 더 필요하시군요",
          "speaker_id": "2",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100031210.1.9",
          "form": "내한한 달 정도 필요해요",
          "original_form": "내한한 달 정도 필요해요",
          "speaker_id": "1",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100031210.1.10",
          "form": "한 달은 너무 많은 거 아니에요",
          "original_form": "한 달은 너무 많은 거 아니에요",
          "speaker_id": "2",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100031210.1.11",
          "form": "그냥 그렇지 않아요",
          "original_form": "그냥 그렇지 않아요",
