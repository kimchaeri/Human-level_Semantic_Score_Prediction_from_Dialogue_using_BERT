      "utterance": [
        {
          "id": "MDRW2100041194.1.1",
          "form": "최대한 빨리 맞고 넘어가께 ㅋㅋㄱ",
          "original_form": "최대한 빨리 맞고 넘어가께 ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100041194.1.2",
          "form": "벌써 신나네 ㅋㅋ",
          "original_form": "벌써 신나네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100041194.1.3",
          "form": "간만에 언니보는구먼",
          "original_form": "간만에 언니보는구먼",
          "speaker_id": "2",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100041194.1.4",
          "form": "힐데스도 할말 많을텐데 ㅋㅋㄱㄱㄱㅋㅋㅋㅋㅋ",
          "original_form": "힐데스도 할말 많을텐데 ㅋㅋㄱㄱㄱㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100041194.1.5",
          "form": "웅웅 ㅋㅋㅋ",
          "original_form": "웅웅 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100041194.1.6",
          "form": "그러겤",
          "original_form": "그러겤",
          "speaker_id": "1",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100041194.1.7",
          "form": "다 모이지못해 아쉽네",
          "original_form": "다 모이지못해 아쉽네",
          "speaker_id": "2",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100041194.1.8",
          "form": "나는 한번씩 보니깐 ㅋㅋㅋ 근데 자주는 못보지",
          "original_form": "나는 한번씩 보니깐 ㅋㅋㅋ 근데 자주는 못보지",
          "speaker_id": "1",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100041194.1.9",
          "form": "다들 비슷하다마",
          "original_form": "다들 비슷하다마",
          "speaker_id": "1",
          "time": "20210905 19:54"
        },
        {
          "id": "MDRW2100041194.1.10",
          "form": "그러게 ㅠㅠ 코로나만 아니면 다 모이는데",
          "original_form": "그러게 ㅠㅠ 코로나만 아니면 다 모이는데",
          "speaker_id": "1",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100041194.1.11",
          "form": "다 모이려면 2차 접종 다 맞고 모일수있는거 아이가??ㅋㅋ",
          "original_form": "다 모이려면 2차 접종 다 맞고 모일수있는거 아이가??ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100041194.1.12",
          "form": "엉 그렇지",
          "original_form": "엉 그렇지",
          "speaker_id": "2",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100041194.1.13",
          "form": "2차까지 맞고가면 수에 포함안되니",
          "original_form": "2차까지 맞고가면 수에 포함안되니",
          "speaker_id": "2",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100041194.1.14",
          "form": "쉽지 않구먼 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "쉽지 않구먼 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100041194.1.15",
          "form": "그러니깐 ㅋㅋㅋ",
          "original_form": "그러니깐 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100041194.1.16",
          "form": "지금 언니들은 다 2차까지 맞아시으니",
          "original_form": "지금 언니들은 다 2차까지 맞아시으니",
          "speaker_id": "2",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100041194.1.17",
          "form": "포함안된다",
          "original_form": "포함안된다",
          "speaker_id": "2",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100041194.1.18",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100041194.1.19",
          "form": "나가도 괜찮은데",
          "original_form": "나가도 괜찮은데",
          "speaker_id": "2",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100041194.1.20",
          "form": "언니는 언제지? 2차 접종?",
          "original_form": "언니는 언제지? 2차 접종?",
          "speaker_id": "1",
          "time": "20210905 19:57"
        },
        {
          "id": "MDRW2100041194.1.21",
          "form": "사람들이 많으니 못가지 ㅠ ㅠ",
          "original_form": "사람들이 많으니 못가지 ㅠ ㅠ",
          "speaker_id": "2",
          "time": "20210905 19:57"
        },
        {
          "id": "MDRW2100041194.1.22",
          "form": "나 화요일 이라니까",
          "original_form": "나 화요일 이라니까",
          "speaker_id": "2",
          "time": "20210905 19:57"
        },
        {
          "id": "MDRW2100041194.1.23",
          "form": "그렇지..시선들이...",
          "original_form": "그렇지..시선들이...",
          "speaker_id": "1",
          "time": "20210905 19:57"
        },
        {
          "id": "MDRW2100041194.1.24",
          "form": "정신차려라 이사람아",
          "original_form": "정신차려라 이사람아",
          "speaker_id": "2",
          "time": "20210905 19:57"
        },
        {
          "id": "MDRW2100041194.1.25",
          "form": "아 맞다 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 맞다 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:57"
        },
        {
          "id": "MDRW2100041194.1.26",
          "form": "화욜날 접종하고 니 만나는거잖아",
          "original_form": "화욜날 접종하고 니 만나는거잖아",
          "speaker_id": "2",
          "time": "20210905 19:57"
        },
        {
          "id": "MDRW2100041194.1.27",
          "form": "정신줄 아직 놓지말라고 ㅋㅋㅋㅋ",
          "original_form": "정신줄 아직 놓지말라고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:57"
        },
        {
          "id": "MDRW2100041194.1.28",
          "form": "뭐 그럴수있지 푸핫 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "뭐 그럴수있지 푸핫 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:57"
        }
      ]
    }
