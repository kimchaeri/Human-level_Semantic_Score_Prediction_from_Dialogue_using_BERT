      "utterance": [
        {
          "id": "MDRW2100016352.1.1",
          "form": "자녀 학교 스트레스이신가요",
          "original_form": "자녀 학교 스트레스이신가요",
          "speaker_id": "2",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100016352.1.2",
          "form": "학교가 온라인이라ㅠ 다 제몫이네요",
          "original_form": "학교가 온라인이라ㅠ 다 제몫이네요",
          "speaker_id": "1",
          "time": "20210826 13:36"
        },
        {
          "id": "MDRW2100016352.1.3",
          "form": "아 그거 힘드실법하죠ㅠ",
          "original_form": "아 그거 힘드실법하죠ㅠ",
          "speaker_id": "2",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100016352.1.4",
          "form": "닥달중입니다ㅠ",
          "original_form": "닥달중입니다ㅠ",
          "speaker_id": "1",
          "time": "20210826 15:47"
        },
        {
          "id": "MDRW2100016352.1.5",
          "form": "과제도 봐줘야하고ㅠ",
          "original_form": "과제도 봐줘야하고ㅠ",
          "speaker_id": "2",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100016352.1.6",
          "form": "하... 넘나 힘드네요",
          "original_form": "하... 넘나 힘드네요",
          "speaker_id": "1",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100016352.1.7",
          "form": "어머니의 무게ㅠㅠ",
          "original_form": "어머니의 무게ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100016352.1.8",
          "form": "힘내시옵서",
          "original_form": "힘내시옵서",
          "speaker_id": "2",
          "time": "20210826 20:16"
        },
        {
          "id": "MDRW2100016352.1.9",
          "form": "차라리 내가 공부하는게 나은것같아요ㅠ",
          "original_form": "차라리 내가 공부하는게 나은것같아요ㅠ",
          "speaker_id": "1",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100016352.1.10",
          "form": "그정도인가요?ㅜ",
          "original_form": "그정도인가요?ㅜ",
          "speaker_id": "2",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100016352.1.11",
          "form": "네 천불이ㅠ",
          "original_form": "네 천불이ㅠ",
          "speaker_id": "1",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100016352.1.12",
          "form": "ㅋㅋ코로나가 얼른 끝나야....",
          "original_form": "ㅋㅋ코로나가 얼른 끝나야....",
          "speaker_id": "2",
          "time": "20210826 23:17"
        },
        {
