      "utterance": [
        {
          "id": "MDRW2100045033.1.1",
          "form": "학창시절 추억 있으세요?",
          "original_form": "학창시절 추억 있으세요?",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045033.1.2",
          "form": "그저 논 기억밖에 ㅋㅋㅋ",
          "original_form": "그저 논 기억밖에 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045033.1.3",
          "form": "뭐 하고 노셨나요?",
          "original_form": "뭐 하고 노셨나요?",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045033.1.4",
          "form": "그저 먹고 놀고",
          "original_form": "그저 먹고 놀고",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045033.1.5",
          "form": "뭘그렇게 했는지.",
          "original_form": "뭘그렇게 했는지.",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045033.1.6",
          "form": "딱히 어딜 가거나 한것 같지는 않은데 친구집 가서 놀고 그랬던것 같아요",
          "original_form": "딱히 어딜 가거나 한것 같지는 않은데 친구집 가서 놀고 그랬던것 같아요",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045033.1.7",
          "form": "재미있었겠는데요",
          "original_form": "재미있었겠는데요",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045033.1.8",
          "form": "그땐 좋았져ㅠㅠㅠ 그립네여ㅠ",
          "original_form": "그땐 좋았져ㅠㅠㅠ 그립네여ㅠ",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045033.1.9",
          "form": "그러게요 그땐 그때가 좋은지 몰랐어요",
          "original_form": "그러게요 그땐 그때가 좋은지 몰랐어요",
          "speaker_id": "1",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045033.1.10",
          "form": "그러게요 어른들도 그러잖아여 그때가 제일 좋을떄라고 그때는 진짜 몰라여퓨",
          "original_form": "그러게요 어른들도 그러잖아여 그때가 제일 좋을떄라고 그때는 진짜 몰라여퓨",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045033.1.11",
          "form": "그러게요.. 다시 돌아가면 저도 잘 놀 수 있는데요",
          "original_form": "그러게요.. 다시 돌아가면 저도 잘 놀 수 있는데요",
          "speaker_id": "1",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045033.1.12",
          "form": "맞아여 더 열심히 놀걸",
          "original_form": "맞아여 더 열심히 놀걸",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045033.1.13",
          "form": "뭐든 더 열심히 할 수 있을것 같죠?",
          "original_form": "뭐든 더 열심히 할 수 있을것 같죠?",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045033.1.14",
          "form": "맞아여 지나고 나서 후회..ㅋ ㅋ 공부도 좀 만 더 열심히 해볼걸 싶고",
          "original_form": "맞아여 지나고 나서 후회..ㅋ ㅋ 공부도 좀 만 더 열심히 해볼걸 싶고",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045033.1.15",
          "form": "좀 미리미리 했으면 잘했을텐데요",
          "original_form": "좀 미리미리 했으면 잘했을텐데요",
