      "utterance": [
        {
          "id": "MDRW2100041222.1.1",
          "form": "name2님은 가리는 것 없이 잘 드시나요?",
          "original_form": "&name2&님은 가리는 것 없이 잘 드시나요?",
          "speaker_id": "1",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100041222.1.2",
          "form": "저 진짜 안가려요ㅋㅋ",
          "original_form": "저 진짜 안가려요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100041222.1.3",
          "form": "뭐 가리시나요?",
          "original_form": "뭐 가리시나요?",
          "speaker_id": "2",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100041222.1.4",
          "form": "저도 진짜 웬만한 것 다 잘 먹어요",
          "original_form": "저도 진짜 웬만한 것 다 잘 먹어요",
          "speaker_id": "1",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100041222.1.5",
          "form": "그래서 여행다니기도 좋고",
          "original_form": "그래서 여행다니기도 좋고",
          "speaker_id": "1",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100041222.1.6",
          "form": "아 멍게는 못먹어요ㅜ",
          "original_form": "아 멍게는 못먹어요ㅜ",
          "speaker_id": "1",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100041222.1.7",
          "form": "아 멍게 정도는!!! ㅋㅋ 굴은 드시나요?",
          "original_form": "아 멍게 정도는!!! ㅋㅋ 굴은 드시나요?",
          "speaker_id": "2",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100041222.1.8",
          "form": "굴은 먹어요",
          "original_form": "굴은 먹어요",
          "speaker_id": "1",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100041222.1.9",
          "form": "멍게는 어렸을 때 색 보고 예뻐서 먹었다가 배신감을 느꼈던 기억이 있어서ㅋㅋ",
          "original_form": "멍게는 어렸을 때 색 보고 예뻐서 먹었다가 배신감을 느꼈던 기억이 있어서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100041222.1.10",
          "form": "근데 물회나 비빔밥에 조금 들어있는 정도는 먹습니다",
          "original_form": "근데 물회나 비빔밥에 조금 들어있는 정도는 먹습니다",
          "speaker_id": "1",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100041222.1.11",
          "form": "ㅋㅋ 멍게가 그 주황색 맞죠?",
          "original_form": "ㅋㅋ 멍게가 그 주황색 맞죠?",
          "speaker_id": "2",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100041222.1.12",
          "form": "네네 주황색",
          "original_form": "네네 주황색",
          "speaker_id": "1",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100041222.1.13",
          "form": "마치 홍시같이 생겨서",
          "original_form": "마치 홍시같이 생겨서",
          "speaker_id": "1",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100041222.1.14",
          "form": "그냥 별생각없이 먹었던거같아요ㅋㅋ",
          "original_form": "그냥 별생각없이 먹었던거같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100041222.1.15",
          "form": "해산물 좋아하세요? 육류좋아하세요?",
          "original_form": "해산물 좋아하세요? 육류좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100041222.1.16",
          "form": "다 좋아해요~",
          "original_form": "다 좋아해요~",
          "speaker_id": "1",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100041222.1.17",
          "form": "그때그때 당기는 게 있는듯요",
          "original_form": "그때그때 당기는 게 있는듯요",
          "speaker_id": "1",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100041222.1.18",
          "form": "ㅋㅋ 전 무조건 육고기였는데 나이먹으니 속이 부대끼는 느낌",
          "original_form": "ㅋㅋ 전 무조건 육고기였는데 나이먹으니 속이 부대끼는 느낌",
          "speaker_id": "2",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100041222.1.19",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "1",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100041222.1.20",
          "form": "고기 많이 먹음 좀 더부룩할 때가 있죠ㅜ",
          "original_form": "고기 많이 먹음 좀 더부룩할 때가 있죠ㅜ",
          "speaker_id": "1",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100041222.1.21",
          "form": "어떤 고기 좋아하세요?",
          "original_form": "어떤 고기 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100041222.1.22",
          "form": "소식을 해야하는데 안해서ㅋㅋ",
          "original_form": "소식을 해야하는데 안해서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:49"
        },
        {
          "id": "MDRW2100041222.1.23",
          "form": "전 소 돼지 닭 오리 다ㅋㅋ",
          "original_form": "전 소 돼지 닭 오리 다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:49"
        },
        {
          "id": "MDRW2100041222.1.24",
          "form": "각각 요리에 따라!!",
          "original_form": "각각 요리에 따라!!",
          "speaker_id": "2",
          "time": "20210905 20:49"
        },
        {
          "id": "MDRW2100041222.1.25",
          "form": "오 어떤 식으로 조리하는 걸 선호하시나요",
          "original_form": "오 어떤 식으로 조리하는 걸 선호하시나요",
          "speaker_id": "1",
          "time": "20210905 20:50"
        },
        {
          "id": "MDRW2100041222.1.26",
          "form": "소는 국으로 돼지는 굽고 닭은 치킨 오리는 주물럭!",
          "original_form": "소는 국으로 돼지는 굽고 닭은 치킨 오리는 주물럭!",
          "speaker_id": "2",
          "time": "20210905 20:51"
        },
        {
          "id": "MDRW2100041222.1.27",
          "form": "ㅎㅎ각각의 고기마다 선호하는 조리법이 있으시군요",
          "original_form": "ㅎㅎ각각의 고기마다 선호하는 조리법이 있으시군요",
          "speaker_id": "1",
          "time": "20210905 20:51"
        },
        {
          "id": "MDRW2100041222.1.28",
          "form": "전문적이신데요?",
          "original_form": "전문적이신데요?",
          "speaker_id": "1",
          "time": "20210905 20:51"
        },
        {
          "id": "MDRW2100041222.1.29",
          "form": "ㅋㅋ 많이 먹다보니ㅋㅋ 사실 굽는게 가장 건강엔 나쁜거같아서 자제하려고요",
          "original_form": "ㅋㅋ 많이 먹다보니ㅋㅋ 사실 굽는게 가장 건강엔 나쁜거같아서 자제하려고요",
          "speaker_id": "2",
          "time": "20210905 20:55"
        },
        {
          "id": "MDRW2100041222.1.30",
          "form": "네 그래도 구이가 당길때가 있어요",
          "original_form": "네 그래도 구이가 당길때가 있어요",
          "speaker_id": "1",
          "time": "20210905 20:55"
        },
        {
          "id": "MDRW2100041222.1.31",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100041222.1.32",
          "form": "오늘은 뭐드셨어요?",
          "original_form": "오늘은 뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100041222.1.33",
          "form": "오늘은 점심에 주꾸미볶음이요",
          "original_form": "오늘은 점심에 주꾸미볶음이요",
          "speaker_id": "1",
          "time": "20210905 20:59"
        }
      ]
    }
