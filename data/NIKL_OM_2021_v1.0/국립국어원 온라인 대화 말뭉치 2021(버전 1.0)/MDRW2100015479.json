          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015479.1.5",
          "form": "깊은뜻이있군요..",
          "original_form": "깊은뜻이있군요..",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015479.1.6",
          "form": "한탕은접어라.",
          "original_form": "한탕은접어라.",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015479.1.7",
          "form": "하지만 월급으론 절대 부자가 될수없겠다",
          "original_form": "하지만 월급으론 절대 부자가 될수없겠다",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015479.1.8",
          "form": "맨날 생각합니다",
          "original_form": "맨날 생각합니다",
          "speaker_id": "2",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100015479.1.9",
          "form": "그건 맞아요.",
          "original_form": "그건 맞아요.",
          "speaker_id": "1",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015479.1.10",
