        {
          "id": "MDRW2100021550.1.2",
          "form": "연애 중이에요?",
          "original_form": "연애 중이에요?",
          "speaker_id": "1",
          "time": "20210828 20:16"
        },
        {
          "id": "MDRW2100021550.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100021550.1.4",
          "form": "아뇨 name1은요?",
          "original_form": "아뇨 &name1&은요?",
          "speaker_id": "2",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100021550.1.5",
          "form": "ㅎㅎ저도 솔로네요",
          "original_form": "ㅎㅎ저도 솔로네요",
          "speaker_id": "1",
          "time": "20210828 20:29"
        },
        {
          "id": "MDRW2100021550.1.6",
          "form": "연애해본 경험은 있으시죠?",
          "original_form": "연애해본 경험은 있으시죠?",
          "speaker_id": "1",
          "time": "20210828 20:29"
        },
        {
          "id": "MDRW2100021550.1.7",
          "form": "네 name1은요?",
          "original_form": "네 &name1&은요?",
          "speaker_id": "2",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100021550.1.8",
          "form": "저도 있는데 어떻게 사겼는지 기억이 잘 안나요..",
          "original_form": "저도 있는데 어떻게 사겼는지 기억이 잘 안나요..",
          "speaker_id": "1",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100021550.1.9",
          "form": "누구랑 사겼는 지도 잘 모르겠네요 ㅋㅋ",
          "original_form": "누구랑 사겼는 지도 잘 모르겠네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100021550.1.10",
          "form": "ㅎㅎ 자만추선호하세요 소개팅 선호하세요",
          "original_form": "ㅎㅎ 자만추선호하세요 소개팅 선호하세요",
          "speaker_id": "2",
          "time": "20210828 21:55"
        },
        {
          "id": "MDRW2100021550.1.11",
          "form": "자만추가 뭐에요?",
          "original_form": "자만추가 뭐에요?",
          "speaker_id": "1",
          "time": "20210828 21:58"
        },
        {
          "id": "MDRW2100021550.1.12",
          "form": "자기만남추구?",
          "original_form": "자기만남추구?",
          "speaker_id": "1",
          "time": "20210828 21:58"
        },
        {
          "id": "MDRW2100021550.1.13",
          "form": "자연만남추구인가?ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "자연만남추구인가?ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:58"
        },
        {
          "id": "MDRW2100021550.1.14",
          "form": "자연스러운만남을추구",
          "original_form": "자연스러운만남을추구",
          "speaker_id": "2",
          "time": "20210828 22:04"
        },
        {
          "id": "MDRW2100021550.1.15",
          "form": "네 얼추맞아요 ㅋㅋㅋ",
          "original_form": "네 얼추맞아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:04"
        },
        {
          "id": "MDRW2100021550.1.16",
          "form": "음 저는 자만추요!!!",
          "original_form": "음 저는 자만추요!!!",
          "speaker_id": "1",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100021550.1.17",
          "form": "그런데 코로나라서 자만추가 불가능하네요 ㅠㅠ!!",
          "original_form": "그런데 코로나라서 자만추가 불가능하네요 ㅠㅠ!!",
          "speaker_id": "1",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100021550.1.18",
          "form": "그쳐 지금은 그래서 휴식기가 길어졌네요",
          "original_form": "그쳐 지금은 그래서 휴식기가 길어졌네요",
          "speaker_id": "2",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100021550.1.19",
          "form": "이상형은 어떻게되세요",
          "original_form": "이상형은 어떻게되세요",
          "speaker_id": "2",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100021550.1.20",
          "form": "그 질문이 제일 어렵더라구요 ㅎㅎㅎ",
          "original_form": "그 질문이 제일 어렵더라구요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:08"
        },
        {
          "id": "MDRW2100021550.1.21",
          "form": "첫 눈에 반하게 해주는 사람이 이상형이에요",
          "original_form": "첫 눈에 반하게 해주는 사람이 이상형이에요",
          "speaker_id": "1",
          "time": "20210828 22:08"
        },
        {
          "id": "MDRW2100021550.1.22",
          "form": "그럼 보통 첫눈에 반하게하는 외적인 공통점같은게 있었나요",
          "original_form": "그럼 보통 첫눈에 반하게하는 외적인 공통점같은게 있었나요",
          "speaker_id": "2",
          "time": "20210828 22:10"
        },
        {
          "id": "MDRW2100021550.1.23",
          "form": "전 웃는게 잘생긴사람이여",
          "original_form": "전 웃는게 잘생긴사람이여",
