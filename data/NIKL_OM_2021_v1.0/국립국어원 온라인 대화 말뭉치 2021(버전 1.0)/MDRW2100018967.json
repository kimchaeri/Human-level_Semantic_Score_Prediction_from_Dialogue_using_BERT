      "utterance": [
        {
          "id": "MDRW2100018967.1.1",
          "form": "저녁 머머금?",
          "original_form": "저녁 머머금?",
          "speaker_id": "2",
          "time": "20210827 17:26"
        },
        {
          "id": "MDRW2100018967.1.2",
          "form": "편의점 가서 삼김이라도 먹을까여",
          "original_form": "편의점 가서 삼김이라도 먹을까여",
          "speaker_id": "1",
          "time": "20210827 17:26"
        },
        {
          "id": "MDRW2100018967.1.3",
          "form": "나 어제는 안먹었고",
          "original_form": "나 어제는 안먹었고",
          "speaker_id": "1",
          "time": "20210827 17:26"
        },
        {
          "id": "MDRW2100018967.1.4",
          "form": "오 쿠폰이 있지",
          "original_form": "오 쿠폰이 있지",
          "speaker_id": "2",
          "time": "20210827 17:26"
        },
        {
          "id": "MDRW2100018967.1.5",
          "form": "오늘도 안먹으려는데",
          "original_form": "오늘도 안먹으려는데",
          "speaker_id": "1",
          "time": "20210827 17:26"
        },
        {
          "id": "MDRW2100018967.1.6",
          "form": "안먹음 간식먹고싶지아너?",
          "original_form": "안먹음 간식먹고싶지아너?",
          "speaker_id": "2",
          "time": "20210827 17:26"
        },
        {
          "id": "MDRW2100018967.1.7",
          "form": "와 대박 개짜증나네여",
          "original_form": "와 대박 개짜증나네여",
          "speaker_id": "1",
          "time": "20210827 17:26"
        },
        {
          "id": "MDRW2100018967.1.8",
          "form": "어제는 참았고",
          "original_form": "어제는 참았고",
          "speaker_id": "1",
          "time": "20210827 17:26"
        },
        {
          "id": "MDRW2100018967.1.9",
          "form": "인성 나빠져",
          "original_form": "인성 나빠져",
          "speaker_id": "2",
          "time": "20210827 17:27"
        },
        {
          "id": "MDRW2100018967.1.10",
          "form": "안대",
          "original_form": "안대",
          "speaker_id": "2",
          "time": "20210827 17:27"
        },
        {
          "id": "MDRW2100018967.1.11",
          "form": "오늘은 허허ㅜ",
          "original_form": "오늘은 허허ㅜ",
          "speaker_id": "1",
          "time": "20210827 17:27"
        },
        {
          "id": "MDRW2100018967.1.12",
          "form": "간단하게 묵지",
          "original_form": "간단하게 묵지",
          "speaker_id": "2",
          "time": "20210827 17:27"
        },
        {
          "id": "MDRW2100018967.1.13",
          "form": "근데쌀국수넘 땡긴다",
          "original_form": "근데쌀국수넘 땡긴다",
          "speaker_id": "1",
          "time": "20210827 17:27"
        },
        {
          "id": "MDRW2100018967.1.14",
          "form": "그집이 네이버페이되던가",
          "original_form": "그집이 네이버페이되던가",
          "speaker_id": "1",
          "time": "20210827 17:27"
        },
        {
          "id": "MDRW2100018967.1.15",
          "form": "카드없어서?",
          "original_form": "카드없어서?",
          "speaker_id": "2",
          "time": "20210827 17:27"
        },
        {
          "id": "MDRW2100018967.1.16",
          "form": "계좌이처안됨?",
          "original_form": "계좌이처안됨?",
          "speaker_id": "2",
          "time": "20210827 17:27"
        },
        {
          "id": "MDRW2100018967.1.17",
          "form": "물어봐야함",
          "original_form": "물어봐야함",
          "speaker_id": "1",
          "time": "20210827 17:28"
        },
        {
          "id": "MDRW2100018967.1.18",
          "form": "계좌이체되는지",
          "original_form": "계좌이체되는지",
          "speaker_id": "1",
          "time": "20210827 17:28"
        },
        {
          "id": "MDRW2100018967.1.19",
          "form": "사실 안사먹으려고",
          "original_form": "사실 안사먹으려고",
          "speaker_id": "1",
          "time": "20210827 17:28"
        },
        {
          "id": "MDRW2100018967.1.20",
          "form": "현금도 안들고 다니는데",
          "original_form": "현금도 안들고 다니는데",
          "speaker_id": "1",
          "time": "20210827 17:28"
        },
        {
          "id": "MDRW2100018967.1.21",
          "form": "통장없는 사장이 어데잇노",
          "original_form": "통장없는 사장이 어데잇노",
          "speaker_id": "2",
          "time": "20210827 17:28"
        },
        {
          "id": "MDRW2100018967.1.22",
          "form": "죄다 페이하고",
          "original_form": "죄다 페이하고",
          "speaker_id": "1",
          "time": "20210827 17:28"
        },
        {
          "id": "MDRW2100018967.1.23",
          "form": "아 프랜차이즈같아서 받는감",
          "original_form": "아 프랜차이즈같아서 받는감",
          "speaker_id": "1",
          "time": "20210827 17:28"
        },
        {
          "id": "MDRW2100018967.1.24",
          "form": "싶었음",
          "original_form": "싶었음",
          "speaker_id": "1",
          "time": "20210827 17:28"
        },
        {
          "id": "MDRW2100018967.1.25",
          "form": "나도 머먹지",
          "original_form": "나도 머먹지",
          "speaker_id": "2",
          "time": "20210827 17:29"
        },
        {
          "id": "MDRW2100018967.1.26",
          "form": "좀 머거랑",
          "original_form": "좀 머거랑",
          "speaker_id": "2",
          "time": "20210827 17:29"
        },
        {
          "id": "MDRW2100018967.1.27",
          "form": "일단 6시반 수업이 끝낭",
          "original_form": "일단 6시반 수업이 끝낭",
          "speaker_id": "1",
          "time": "20210827 17:29"
        },
        {
          "id": "MDRW2100018967.1.28",
          "form": "나는 떡볶이무러옴",
          "original_form": "나는 떡볶이무러옴",
          "speaker_id": "2",
          "time": "20210827 17:39"
        },
        {
          "id": "MDRW2100018967.1.29",
          "form": "나는 쌀국수 먹으러옴",
          "original_form": "나는 쌀국수 먹으러옴",
          "speaker_id": "1",
          "time": "20210827 18:41"
        },
        {
          "id": "MDRW2100018967.1.30",
          "form": "네이버페이된다더니",
          "original_form": "네이버페이된다더니",
          "speaker_id": "1",
          "time": "20210827 18:41"
        },
        {
          "id": "MDRW2100018967.1.31",
          "form": "걍 뭐가복잡해서 계좌이체함",
          "original_form": "걍 뭐가복잡해서 계좌이체함",
          "speaker_id": "1",
          "time": "20210827 18:41"
        },
        {
          "id": "MDRW2100018967.1.32",
          "form": "계좌이체가되네",
          "original_form": "계좌이체가되네",
          "speaker_id": "2",
          "time": "20210827 19:03"
        },
        {
          "id": "MDRW2100018967.1.33",
          "form": "그게낫지머",
          "original_form": "그게낫지머",
          "speaker_id": "2",
          "time": "20210827 19:03"
        }
      ]
    }
