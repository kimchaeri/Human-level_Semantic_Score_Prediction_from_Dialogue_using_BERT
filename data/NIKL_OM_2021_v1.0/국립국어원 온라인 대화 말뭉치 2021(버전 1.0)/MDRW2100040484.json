      "utterance": [
        {
          "id": "MDRW2100040484.1.1",
          "form": "저는 집에있는 곰탕먹었어요",
          "original_form": "저는 집에있는 곰탕먹었어요",
          "speaker_id": "2",
          "time": "20210904 21:50"
        },
        {
          "id": "MDRW2100040484.1.2",
          "form": "인스턴트라서 먹고나도 그냥 그렇네요ㅎㅎ",
          "original_form": "인스턴트라서 먹고나도 그냥 그렇네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 21:50"
        },
        {
          "id": "MDRW2100040484.1.3",
          "form": "ㅋㅋㅋ맞아요 인스턴트 ㅠㅠ 괜히 후식만 땡기고",
          "original_form": "ㅋㅋㅋ맞아요 인스턴트 ㅠㅠ 괜히 후식만 땡기고",
          "speaker_id": "1",
          "time": "20210904 21:53"
        },
        {
          "id": "MDRW2100040484.1.4",
          "form": "name1님은 뭐드셨나요",
          "original_form": "&name1&님은 뭐드셨나요",
          "speaker_id": "2",
          "time": "20210904 21:58"
        },
        {
          "id": "MDRW2100040484.1.5",
          "form": "저는 군고구마랑... 옥수수랑...감자전이랑...계란찜이랑....",
          "original_form": "저는 군고구마랑... 옥수수랑...감자전이랑...계란찜이랑....",
          "speaker_id": "1",
          "time": "20210904 22:00"
        },
        {
          "id": "MDRW2100040484.1.6",
          "form": "엄마표 이것저것이요 ㅋㅋㅋㅋㅋ",
          "original_form": "엄마표 이것저것이요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:00"
        },
        {
          "id": "MDRW2100040484.1.7",
          "form": "와ㅋㅋ 좋네요",
          "original_form": "와ㅋㅋ 좋네요",
          "speaker_id": "2",
          "time": "20210904 22:09"
        },
        {
          "id": "MDRW2100040484.1.8",
          "form": "배달은 자주 안드세요?",
          "original_form": "배달은 자주 안드세요?",
          "speaker_id": "2",
          "time": "20210904 22:09"
        },
        {
          "id": "MDRW2100040484.1.9",
          "form": "저 주중에는 혼자 자취해서",
          "original_form": "저 주중에는 혼자 자취해서",
          "speaker_id": "1",
          "time": "20210904 22:11"
        },
        {
          "id": "MDRW2100040484.1.10",
          "form": "그때 가끔 시켜먹어요",
          "original_form": "그때 가끔 시켜먹어요",
          "speaker_id": "1",
          "time": "20210904 22:11"
        },
        {
          "id": "MDRW2100040484.1.11",
          "form": "처음 자취할때는 진짜 매일 시켰는데 요즘 좀 자제합니다 ㅜ ㅠ",
          "original_form": "처음 자취할때는 진짜 매일 시켰는데 요즘 좀 자제합니다 ㅜ ㅠ",
          "speaker_id": "1",
          "time": "20210904 22:12"
        },
        {
          "id": "MDRW2100040484.1.12",
          "form": "ㅋㅋㅋ혼자서 시키면 많이 부담스럽죠ㅠㅠ",
          "original_form": "ㅋㅋㅋ혼자서 시키면 많이 부담스럽죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 22:17"
        },
        {
          "id": "MDRW2100040484.1.13",
          "form": "그럼 반찬도 만들어서 드세요?",
          "original_form": "그럼 반찬도 만들어서 드세요?",
          "speaker_id": "2",
          "time": "20210904 22:17"
        },
        {
          "id": "MDRW2100040484.1.14",
          "form": "아니요 그냥... 배달을 줄이고선 냉동음식을 먹어요ㅠㅠㅠ",
          "original_form": "아니요 그냥... 배달을 줄이고선 냉동음식을 먹어요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 22:19"
        },
        {
          "id": "MDRW2100040484.1.15",
          "form": "ㅠㅠ그쵸 냉동음식은 우리의 친구...",
          "original_form": "ㅠㅠ그쵸 냉동음식은 우리의 친구...",
          "speaker_id": "2",
          "time": "20210904 22:36"
        },
        {
          "id": "MDRW2100040484.1.16",
          "form": "그래도 주말에는 집밥 드셔서 다행이에요",
          "original_form": "그래도 주말에는 집밥 드셔서 다행이에요",
          "speaker_id": "2",
          "time": "20210904 22:36"
        },
        {
          "id": "MDRW2100040484.1.17",
          "form": "그쵸 냉동실은 항상 두둑히 채워둬야죠 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그쵸 냉동실은 항상 두둑히 채워둬야죠 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100040484.1.18",
          "form": "전자렌지 만세..!",
          "original_form": "전자렌지 만세..!",
          "speaker_id": "1",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100040484.1.19",
          "form": "ㅋㅋㅋ이번에도 만두랑 이것저것 시켰는데",
          "original_form": "ㅋㅋㅋ이번에도 만두랑 이것저것 시켰는데",
          "speaker_id": "2",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100040484.1.20",
          "form": "얼른빨리오면 좋겠어요",
          "original_form": "얼른빨리오면 좋겠어요",
          "speaker_id": "2",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100040484.1.21",
          "form": "오오오 만두가 진짜 ㅋㅋㅋㅋㅋ최고 먹기 편하고 좋죠",
          "original_form": "오오오 만두가 진짜 ㅋㅋㅋㅋㅋ최고 먹기 편하고 좋죠",
          "speaker_id": "1",
          "time": "20210904 23:03"
        },
        {
          "id": "MDRW2100040484.1.22",
          "form": "저도 만두는 늘 구비하고 있어요!! ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 만두는 늘 구비하고 있어요!! ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:03"
        },
        {
          "id": "MDRW2100040484.1.23",
          "form": "저두요ㅎㅎ",
          "original_form": "저두요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:10"
        },
        {
          "id": "MDRW2100040484.1.24",
          "form": "얇은피만두 맛있더라구요",
          "original_form": "얇은피만두 맛있더라구요",
