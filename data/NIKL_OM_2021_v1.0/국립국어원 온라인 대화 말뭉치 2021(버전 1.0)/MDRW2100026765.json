      "utterance": [
        {
          "id": "MDRW2100026765.1.1",
          "form": "부산경남에서 먹는술이 있어요",
          "original_form": "부산경남에서 먹는술이 있어요",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026765.1.2",
          "form": "아 좋은데이ㅎ",
          "original_form": "아 좋은데이ㅎ",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026765.1.3",
          "form": "브랜드가 좋은데이 소주입니다",
          "original_form": "브랜드가 좋은데이 소주입니다",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026765.1.4",
          "form": "알죠알죠ㅎ",
          "original_form": "알죠알죠ㅎ",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026765.1.5",
          "form": "오 아세요?",
          "original_form": "오 아세요?",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026765.1.6",
          "form": "드셔봄?",
          "original_form": "드셔봄?",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026765.1.7",
          "form": "그럼요ㅎ",
          "original_form": "그럼요ㅎ",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026765.1.8",
          "form": "근데 저 술알못이라ㅎㅎ",
          "original_form": "근데 저 술알못이라ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026765.1.9",
          "form": "잘 접하기 힘드실텐데",
          "original_form": "잘 접하기 힘드실텐데",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026765.1.10",
          "form": "소주는 소주려니 해서요ㅎ",
          "original_form": "소주는 소주려니 해서요ㅎ",
          "speaker_id": "1",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026765.1.11",
          "form": "음 ㅎㅎ",
          "original_form": "음 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026765.1.12",
          "form": "요새는 소주도 종류별로 두는곳이 꽤 있어요",
          "original_form": "요새는 소주도 종류별로 두는곳이 꽤 있어요",
          "speaker_id": "1",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026765.1.13",
          "form": "한라산두 많이 보이고",
          "original_form": "한라산두 많이 보이고",
          "speaker_id": "1",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026765.1.14",
          "form": "판매다각화인가요",
          "original_form": "판매다각화인가요",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026765.1.15",
          "form": "한라산 ㅎㄷㄷ",
          "original_form": "한라산 ㅎㄷㄷ",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026765.1.16",
          "form": "2만원씩 하던거 아니엇서요?",
          "original_form": "2만원씩 하던거 아니엇서요?",
          "speaker_id": "2",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026765.1.17",
          "form": "ㅎㅎ 나란히 두고 마시면 다 개성이 있더라구요",
          "original_form": "ㅎㅎ 나란히 두고 마시면 다 개성이 있더라구요",
          "speaker_id": "1",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026765.1.18",
          "form": "한라산은 진짜 추억이 있는술입니다",
          "original_form": "한라산은 진짜 추억이 있는술입니다",
          "speaker_id": "2",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026765.1.19",
          "form": "넹? 한라산이요?",
          "original_form": "넹? 한라산이요?",
          "speaker_id": "1",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026765.1.20",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026765.1.21",
          "form": "비싸도 오육천원 안 넘는데...",
          "original_form": "비싸도 오육천원 안 넘는데...",
          "speaker_id": "1",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026765.1.22",
          "form": "2만원은 무슨 양심리스 가게쥬",
          "original_form": "2만원은 무슨 양심리스 가게쥬",
          "speaker_id": "1",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100026765.1.23",
          "form": "우리때 한라산 구하기 힘들어서 좀 비싸게 파는집있엇서요",
          "original_form": "우리때 한라산 구하기 힘들어서 좀 비싸게 파는집있엇서요",
          "speaker_id": "2",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100026765.1.24",
          "form": "우리 어릴때요 ㅋㅋ",
          "original_form": "우리 어릴때요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100026765.1.25",
          "form": "님 중1쯤",
          "original_form": "님 중1쯤",
          "speaker_id": "2",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100026765.1.26",
          "form": "아ㅎㅇ여기도 한라산 6000원에 팔다가 슬쩍 떨어뜨리더라구요",
          "original_form": "아ㅎㅇ여기도 한라산 6000원에 팔다가 슬쩍 떨어뜨리더라구요",
          "speaker_id": "1",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100026765.1.27",
          "form": "나쁘사람들",
          "original_form": "나쁘사람들",
          "speaker_id": "2",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100026765.1.28",
          "form": "한라산 소주는 제주도 가서 마셔야되는뎅",
          "original_form": "한라산 소주는 제주도 가서 마셔야되는뎅",
          "speaker_id": "2",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026765.1.29",
          "form": "무슨 추억인가요?",
          "original_form": "무슨 추억인가요?",
          "speaker_id": "1",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026765.1.30",
          "form": "무슨 추억인가요?",
          "original_form": "무슨 추억인가요?",
          "speaker_id": "1",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026765.1.31",
          "form": "제주도 좋아하시나요?",
          "original_form": "제주도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026765.1.32",
          "form": "맛있는 동네는 다 좋아하죠ㅎㅎ",
          "original_form": "맛있는 동네는 다 좋아하죠ㅎㅎ",
          "speaker_id": "1",
