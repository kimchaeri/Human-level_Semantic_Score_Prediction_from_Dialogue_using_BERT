      "utterance": [
        {
          "id": "MDRW2100016021.1.1",
          "form": "난 오늘 뿌염을 하겠다 ㅋㅋㅋㅋㅋ",
          "original_form": "난 오늘 뿌염을 하겠다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 08:27"
        },
        {
          "id": "MDRW2100016021.1.2",
          "form": "오오오 뿌염하러 가나요 ㅋㅋ",
          "original_form": "오오오 뿌염하러 가나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:39"
        },
        {
          "id": "MDRW2100016021.1.3",
          "form": "사촌언니 미용실로?",
          "original_form": "사촌언니 미용실로?",
          "speaker_id": "1",
          "time": "20210826 08:39"
        },
        {
          "id": "MDRW2100016021.1.4",
          "form": "응~멀다멀어 ㅋㅋㅋㅋ",
          "original_form": "응~멀다멀어 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 08:39"
        },
        {
          "id": "MDRW2100016021.1.5",
          "form": "ㅋㅋㅋ어디라고 했지?",
          "original_form": "ㅋㅋㅋ어디라고 했지?",
          "speaker_id": "1",
          "time": "20210826 08:40"
        },
        {
          "id": "MDRW2100016021.1.6",
          "form": "address쪽이라고 했나?",
          "original_form": "&address&쪽이라고 했나?",
          "speaker_id": "1",
          "time": "20210826 08:40"
        },
        {
          "id": "MDRW2100016021.1.7",
          "form": "address ㅋㅋㅋㅋ넘 구석에 있어가",
          "original_form": "&address& ㅋㅋㅋㅋ넘 구석에 있어가",
          "speaker_id": "2",
          "time": "20210826 08:40"
        },
        {
          "id": "MDRW2100016021.1.8",
          "form": "손님이 전부 가족이다 ㅋ",
          "original_form": "손님이 전부 가족이다 ㅋ",
          "speaker_id": "2",
          "time": "20210826 08:40"
        },
        {
          "id": "MDRW2100016021.1.9",
          "form": "뿌염하거 사람되서 나타나꿈 ㅋㅋㅋㅋ",
          "original_form": "뿌염하거 사람되서 나타나꿈 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 08:41"
        },
        {
          "id": "MDRW2100016021.1.10",
          "form": "아 웃겨 ㅋㅋㅋ 손님이 다 가족 ㅋㅋㅋ",
          "original_form": "아 웃겨 ㅋㅋㅋ 손님이 다 가족 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:43"
        },
        {
          "id": "MDRW2100016021.1.11",
          "form": "그래 뿌염해서 나타나거라 ㅋㅋㅋ",
          "original_form": "그래 뿌염해서 나타나거라 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:43"
        },
        {
          "id": "MDRW2100016021.1.12",
          "form": "나도 뿌염해야되는데 ㅠㅠ ㅋㅋㅋㅋ",
          "original_form": "나도 뿌염해야되는데 ㅠㅠ ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:43"
        },
        {
          "id": "MDRW2100016021.1.13",
          "form": "우웅^^뭐든 내 몸에 돈들이는건 기부니가 좋아^^",
          "original_form": "우웅^^뭐든 내 몸에 돈들이는건 기부니가 좋아^^",
          "speaker_id": "2",
          "time": "20210826 08:44"
        },
        {
          "id": "MDRW2100016021.1.14",
          "form": "그러니깐은 ㅋㅋㅋㅋ",
          "original_form": "그러니깐은 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:44"
        },
        {
          "id": "MDRW2100016021.1.15",
          "form": "예전에 적금보험인가?? 애들꺼 들었나??",
          "original_form": "예전에 적금보험인가?? 애들꺼 들었나??",
          "speaker_id": "2",
          "time": "20210826 08:44"
        },
        {
          "id": "MDRW2100016021.1.16",
          "form": "조여사꺼~",
          "original_form": "조여사꺼~",
          "speaker_id": "2",
          "time": "20210826 08:44"
        },
        {
          "id": "MDRW2100016021.1.17",
          "form": "나도 어제 기분 괜찮았어",
          "original_form": "나도 어제 기분 괜찮았어",
          "speaker_id": "1",
          "time": "20210826 08:44"
        },
        {
          "id": "MDRW2100016021.1.18",
          "form": "아니아니 안들었어.",
          "original_form": "아니아니 안들었어.",
          "speaker_id": "1",
          "time": "20210826 08:44"
        },
        {
          "id": "MDRW2100016021.1.19",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210826 08:45"
        },
        {
          "id": "MDRW2100016021.1.20",
          "form": "적금할까 적금 보험을 들까 싶어가~",
          "original_form": "적금할까 적금 보험을 들까 싶어가~",
          "speaker_id": "2",
          "time": "20210826 08:45"
        },
        {
          "id": "MDRW2100016021.1.21",
          "form": "나는 일단 청약넣어주고있고",
          "original_form": "나는 일단 청약넣어주고있고",
          "speaker_id": "1",
          "time": "20210826 08:45"
        },
        {
          "id": "MDRW2100016021.1.22",
          "form": "오~난도 두찌꺼 통장 만들어줘야굿다 ㅋㅋㅋㅋ",
          "original_form": "오~난도 두찌꺼 통장 만들어줘야굿다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 08:46"
        },
        {
          "id": "MDRW2100016021.1.23",
          "form": "이제 ㅋㅋㅋㅋ",
          "original_form": "이제 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 08:46"
        },
        {
          "id": "MDRW2100016021.1.24",
          "form": "적금보험은 고민하다가 안들어줬던것같아",
          "original_form": "적금보험은 고민하다가 안들어줬던것같아",
          "speaker_id": "1",
          "time": "20210826 08:46"
        },
        {
          "id": "MDRW2100016021.1.25",
          "form": "작금보험은 보험기능도 있다카던데~",
