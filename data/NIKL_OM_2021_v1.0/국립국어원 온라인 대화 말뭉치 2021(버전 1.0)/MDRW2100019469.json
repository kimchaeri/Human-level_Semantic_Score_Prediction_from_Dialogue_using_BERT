      "utterance": [
        {
          "id": "MDRW2100019469.1.1",
          "form": "페코리코 로마노 들어봤어요",
          "original_form": "페코리코 로마노 들어봤어요",
          "speaker_id": "1",
          "time": "20210827 19:34"
        },
        {
          "id": "MDRW2100019469.1.2",
          "form": "페코리노 로마노가 꼬릿꼬릿하지만 요리에 들어가면 아주 맛있거든요",
          "original_form": "페코리노 로마노가 꼬릿꼬릿하지만 요리에 들어가면 아주 맛있거든요",
          "speaker_id": "2",
          "time": "20210827 19:34"
        },
        {
          "id": "MDRW2100019469.1.3",
          "form": "맞아요 그 무슨 파스타 있지않나요",
          "original_form": "맞아요 그 무슨 파스타 있지않나요",
          "speaker_id": "1",
          "time": "20210827 19:35"
        },
        {
          "id": "MDRW2100019469.1.4",
          "form": "치즈만 넣는",
          "original_form": "치즈만 넣는",
          "speaker_id": "1",
          "time": "20210827 19:35"
        },
        {
          "id": "MDRW2100019469.1.5",
          "form": "까르보나라",
          "original_form": "까르보나라",
          "speaker_id": "2",
          "time": "20210827 19:35"
        },
        {
          "id": "MDRW2100019469.1.6",
          "form": "오리지널 방식은 크림스파게티가 아니기 때문에",
          "original_form": "오리지널 방식은 크림스파게티가 아니기 때문에",
          "speaker_id": "2",
          "time": "20210827 19:35"
        },
        {
          "id": "MDRW2100019469.1.7",
          "form": "음 .. 달걀 노른자가 소스예요",
          "original_form": "음 .. 달걀 노른자가 소스예요",
          "speaker_id": "2",
          "time": "20210827 19:35"
        },
        {
          "id": "MDRW2100019469.1.8",
          "form": "오 그것도 해봤엇어요 맛있었는데",
          "original_form": "오 그것도 해봤엇어요 맛있었는데",
          "speaker_id": "1",
          "time": "20210827 19:36"
        },
        {
          "id": "MDRW2100019469.1.9",
          "form": "덜 익은 달걀 먹으면 병난대서 끊었어요 ㅋㅋㅋㅋ",
          "original_form": "덜 익은 달걀 먹으면 병난대서 끊었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:36"
        },
        {
          "id": "MDRW2100019469.1.10",
          "form": "아악 ㅋㅋ",
          "original_form": "아악 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:36"
        },
        {
          "id": "MDRW2100019469.1.11",
          "form": "아 그 치즈만 들어가는 파스타 찾았어요",
          "original_form": "아 그 치즈만 들어가는 파스타 찾았어요",
          "speaker_id": "1",
          "time": "20210827 19:37"
        },
        {
          "id": "MDRW2100019469.1.12",
          "form": "카치오 에 페페라고",
          "original_form": "카치오 에 페페라고",
          "speaker_id": "1",
          "time": "20210827 19:37"
        },
        {
          "id": "MDRW2100019469.1.13",
          "form": "이거 하느라 치즈 다졌었어요 ㅋㅋㅋ 이거 할때 페코리노 로마노만 원래 넣는다 햇는데 잘 모르겟네요",
          "original_form": "이거 하느라 치즈 다졌었어요 ㅋㅋㅋ 이거 할때 페코리노 로마노만 원래 넣는다 햇는데 잘 모르겟네요",
          "speaker_id": "1",
          "time": "20210827 19:37"
        },
        {
          "id": "MDRW2100019469.1.14",
          "form": "우오앙 해보겠어요",
          "original_form": "우오앙 해보겠어요",
          "speaker_id": "2",
          "time": "20210827 19:38"
        },
        {
          "id": "MDRW2100019469.1.15",
          "form": "맛있겠다 ㅜㅜ",
          "original_form": "맛있겠다 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 19:38"
        },
        {
          "id": "MDRW2100019469.1.16",
          "form": "통후추를 팬에 구워서 빻으면",
          "original_form": "통후추를 팬에 구워서 빻으면",
          "speaker_id": "2",
          "time": "20210827 19:38"
        },
        {
          "id": "MDRW2100019469.1.17",
          "form": "치즈만 잘 갈면...!!",
          "original_form": "치즈만 잘 갈면...!!",
          "speaker_id": "1",
          "time": "20210827 19:38"
        },
        {
          "id": "MDRW2100019469.1.18",
          "form": "풍미가 엄청나게 좋아져요",
          "original_form": "풍미가 엄청나게 좋아져요",
          "speaker_id": "2",
          "time": "20210827 19:38"
        },
        {
          "id": "MDRW2100019469.1.19",
          "form": "오 대박 그거 시도할 생각도 못해봤어요",
          "original_form": "오 대박 그거 시도할 생각도 못해봤어요",
          "speaker_id": "1",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100019469.1.20",
          "form": "미뇨넷 페퍼라고",
          "original_form": "미뇨넷 페퍼라고",
          "speaker_id": "2",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100019469.1.21",
          "form": "후추 빻을 때 후추 다 날아가지 않나요?",
          "original_form": "후추 빻을 때 후추 다 날아가지 않나요?",
          "speaker_id": "1",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100019469.1.22",
          "form": "통통 날아가더라구욬ㅋㅋ",
          "original_form": "통통 날아가더라구욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100019469.1.23",
          "form": "으음 ㅋㅋㅋㅋㅋ",
          "original_form": "으음 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100019469.1.24",
          "form": "뭉근하게 조지면",
          "original_form": "뭉근하게 조지면",
          "speaker_id": "2",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100019469.1.25",
          "form": "거의 안 튀어요 ㅎㅎ",
          "original_form": "거의 안 튀어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100019469.1.26",
          "form": "막자사발에 넣고 방망이로 뭉개듯이",
          "original_form": "막자사발에 넣고 방망이로 뭉개듯이",
          "speaker_id": "2",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019469.1.27",
          "form": "ㅋㅋㅋㅋ뭉근하게..",
          "original_form": "ㅋㅋㅋㅋ뭉근하게..",
