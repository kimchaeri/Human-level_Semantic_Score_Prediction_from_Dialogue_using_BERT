      "utterance": [
        {
          "id": "MDRW2100044461.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044461.1.2",
          "form": "요즘 날씨가 많이 서늘해졌죠?",
          "original_form": "요즘 날씨가 많이 서늘해졌죠?",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044461.1.3",
          "form": "안녕하세요 맞아요 요새 날씨 엄청 시원하죠",
          "original_form": "안녕하세요 맞아요 요새 날씨 엄청 시원하죠",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044461.1.4",
          "form": "네 너무좋네요",
          "original_form": "네 너무좋네요",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044461.1.5",
          "form": "금방추워질까 걱정이에요",
          "original_form": "금방추워질까 걱정이에요",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044461.1.6",
          "form": "맞아요 가을 금방 가고 겨울 올 거 같아요ㅠ",
          "original_form": "맞아요 가을 금방 가고 겨울 올 거 같아요ㅠ",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044461.1.7",
          "form": "겨울옷 꺼내야하는데",
          "original_form": "겨울옷 꺼내야하는데",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044461.1.8",
          "form": "엄두가 안나네요",
          "original_form": "엄두가 안나네요",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044461.1.9",
          "form": "아 마자요ㅠ",
          "original_form": "아 마자요ㅠ",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044461.1.10",
          "form": "한번 또 다 세탁하고 해야하는데",
          "original_form": "한번 또 다 세탁하고 해야하는데",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044461.1.11",
          "form": "그쳐 한두개가아닌데",
          "original_form": "그쳐 한두개가아닌데",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044461.1.12",
          "form": "전 여름이불은 넣고 가을꺼 꺼냈어요ㅎㅎ",
          "original_form": "전 여름이불은 넣고 가을꺼 꺼냈어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044461.1.13",
          "form": "저도 가을이불꺼낸지 좀 됐네요",
          "original_form": "저도 가을이불꺼낸지 좀 됐네요",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044461.1.14",
          "form": "요새는 아침에 일어나면 춥더라고요",
          "original_form": "요새는 아침에 일어나면 춥더라고요",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044461.1.15",
          "form": "근데 아직 선풍기는 안넣어요ㅋㅋ",
          "original_form": "근데 아직 선풍기는 안넣어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044461.1.16",
          "form": "머리말리려고요..",
          "original_form": "머리말리려고요..",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044461.1.17",
          "form": "아 저도요 선풍기로 머리 말리는게 최고죠!ㅋㅋ",
          "original_form": "아 저도요 선풍기로 머리 말리는게 최고죠!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044461.1.18",
          "form": "가끔 낮에 더울때는 필요하더라구요",
          "original_form": "가끔 낮에 더울때는 필요하더라구요",
          "speaker_id": "2",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044461.1.19",
          "form": "ㅋㅋ 드라이기는 팔아파요",
          "original_form": "ㅋㅋ 드라이기는 팔아파요",
          "speaker_id": "1",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044461.1.20",
          "form": "맞아요 가끔 해가 들면 아직은 잠깐 더워요",
          "original_form": "맞아요 가끔 해가 들면 아직은 잠깐 더워요",
          "speaker_id": "1",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044461.1.21",
          "form": "마자요 팔아프고 힘들죠ㅠ",
          "original_form": "마자요 팔아프고 힘들죠ㅠ",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044461.1.22",
          "form": "그쵸 낮에는 덥더라구요",
          "original_form": "그쵸 낮에는 덥더라구요",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044461.1.23",
          "form": "이제 여름옷도 넣어야하는데 큰일이고요ㅋㅋ",
          "original_form": "이제 여름옷도 넣어야하는데 큰일이고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044461.1.24",
          "form": "마자요..ㅠ 한참 걸리겠네요ㅠ",
          "original_form": "마자요..ㅠ 한참 걸리겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210907 23:45"
        }
      ]
    }
