      "utterance": [
        {
          "id": "MDRW2100028948.1.1",
          "form": "전 항상 도시락싸요ㅎㅎ",
          "original_form": "전 항상 도시락싸요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 07:40"
        },
        {
          "id": "MDRW2100028948.1.2",
          "form": "ㄷㅏ이어트에 도움이 된다 생각중인데ㅋㅋ",
          "original_form": "ㄷㅏ이어트에 도움이 된다 생각중인데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 07:40"
        },
        {
          "id": "MDRW2100028948.1.3",
          "form": "도시락 싸신다구여?",
          "original_form": "도시락 싸신다구여?",
          "speaker_id": "1",
          "time": "20210903 07:55"
        },
        {
          "id": "MDRW2100028948.1.4",
          "form": "부지런하시네요",
          "original_form": "부지런하시네요",
          "speaker_id": "1",
          "time": "20210903 07:55"
        },
        {
          "id": "MDRW2100028948.1.5",
          "form": "그냥 라면이나 냉동볶음밥 빵 닭가슴살 요렇게 돌려막기해요 ㅋㅋ",
          "original_form": "그냥 라면이나 냉동볶음밥 빵 닭가슴살 요렇게 돌려막기해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 08:10"
        },
        {
          "id": "MDRW2100028948.1.6",
          "form": "그래도 쉬운일이 아닌데",
          "original_form": "그래도 쉬운일이 아닌데",
          "speaker_id": "1",
          "time": "20210903 08:11"
        },
        {
          "id": "MDRW2100028948.1.7",
          "form": "코로나시국엔 장점이 된거같아요 식당에서 드시나요?",
          "original_form": "코로나시국엔 장점이 된거같아요 식당에서 드시나요?",
          "speaker_id": "2",
          "time": "20210903 08:12"
        },
        {
          "id": "MDRW2100028948.1.8",
          "form": "네 전 식당요ㅜㅜ",
          "original_form": "네 전 식당요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 09:06"
        },
        {
          "id": "MDRW2100028948.1.9",
          "form": "아ㅜㅠ 좀 불편하시죠? 그래도 맛난거 드시니까ㅜㅠ 구내식당이에요?",
          "original_form": "아ㅜㅠ 좀 불편하시죠? 그래도 맛난거 드시니까ㅜㅠ 구내식당이에요?",
          "speaker_id": "2",
          "time": "20210903 09:13"
        },
        {
          "id": "MDRW2100028948.1.10",
          "form": "아뇨 그냥 밖에 식당요",
          "original_form": "아뇨 그냥 밖에 식당요",
          "speaker_id": "1",
          "time": "20210903 14:44"
        },
        {
          "id": "MDRW2100028948.1.11",
          "form": "요즘은 좀 그래요",
          "original_form": "요즘은 좀 그래요",
          "speaker_id": "1",
          "time": "20210903 14:44"
        },
        {
          "id": "MDRW2100028948.1.12",
          "form": "점심시간에 사람들 쏟아져나와서",
          "original_form": "점심시간에 사람들 쏟아져나와서",
          "speaker_id": "1",
          "time": "20210903 14:44"
        },
        {
          "id": "MDRW2100028948.1.13",
          "form": "그래도 왠만한 곳들은 다 칸막이 되어있긴하더라고요 다행이죠ㅡㅡ",
          "original_form": "그래도 왠만한 곳들은 다 칸막이 되어있긴하더라고요 다행이죠ㅡㅡ",
          "speaker_id": "2",
          "time": "20210903 15:07"
        },
        {
          "id": "MDRW2100028948.1.14",
          "form": "그렇긴한데 사람 너무 많으면 불안해요",
          "original_form": "그렇긴한데 사람 너무 많으면 불안해요",
          "speaker_id": "1",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100028948.1.15",
          "form": "맞아요 맛집들은 간격 너무 좁고요ㅜ",
          "original_form": "맞아요 맛집들은 간격 너무 좁고요ㅜ",
          "speaker_id": "2",
          "time": "20210903 20:30"
        },
        {
          "id": "MDRW2100028948.1.16",
          "form": "오늘 퇴근하는데 재난문자에서",
          "original_form": "오늘 퇴근하는데 재난문자에서",
          "speaker_id": "1",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100028948.1.17",
          "form": "음식점 방문객 및 포장해간사람에 배달시켜먹은 사람까지",
          "original_form": "음식점 방문객 및 포장해간사람에 배달시켜먹은 사람까지",
          "speaker_id": "1",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100028948.1.18",
          "form": "다 코로나검사 받으라고 왔더라구요",
          "original_form": "다 코로나검사 받으라고 왔더라구요",
          "speaker_id": "1",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100028948.1.19",
          "form": "무서웠어요",
          "original_form": "무서웠어요",
          "speaker_id": "1",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100028948.1.20",
          "form": "조리하는사람이 걸린거 같은데",
          "original_form": "조리하는사람이 걸린거 같은데",
          "speaker_id": "1",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100028948.1.21",
          "form": "가셨던 곳에서요???ㅜㅠ",
          "original_form": "가셨던 곳에서요???ㅜㅠ",
          "speaker_id": "2",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100028948.1.22",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100028948.1.23",
          "form": "갔던곳은 아닌데",
          "original_form": "갔던곳은 아닌데",
          "speaker_id": "1",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100028948.1.24",
          "form": "아 진짜 다행이에요ㅜ",
          "original_form": "아 진짜 다행이에요ㅜ",
          "speaker_id": "2",
          "time": "20210903 20:57"
        },
        {
          "id": "MDRW2100028948.1.25",
          "form": "그런문자보면 뭔가 무섭",
          "original_form": "그런문자보면 뭔가 무섭",
          "speaker_id": "1",
          "time": "20210903 20:57"
        }
      ]
    }
