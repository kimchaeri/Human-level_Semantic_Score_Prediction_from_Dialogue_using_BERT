      "utterance": [
        {
          "id": "MDRW2100040267.1.1",
          "form": "연구실 선배가 해준 이야기인데",
          "original_form": "연구실 선배가 해준 이야기인데",
          "speaker_id": "1",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100040267.1.2",
          "form": "데이트 장소 어떤곳 선호하세요?",
          "original_form": "데이트 장소 어떤곳 선호하세요?",
          "speaker_id": "2",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100040267.1.3",
          "form": "앗 데이트 안한지 너무 오래되었는데 ㅋㅋㅋㅋ ㅠㅠㅠ",
          "original_form": "앗 데이트 안한지 너무 오래되었는데 ㅋㅋㅋㅋ ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100040267.1.4",
          "form": "저는 음... 루프탑 카페나...",
          "original_form": "저는 음... 루프탑 카페나...",
          "speaker_id": "1",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100040267.1.5",
          "form": "사실 어디든 좋아요!!!",
          "original_form": "사실 어디든 좋아요!!!",
          "speaker_id": "1",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100040267.1.6",
          "form": "실내도 좋고 실외도 좋고~!",
          "original_form": "실내도 좋고 실외도 좋고~!",
          "speaker_id": "1",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100040267.1.7",
          "form": "보통 인스타같은거 보고 찾아가시는거예요?",
          "original_form": "보통 인스타같은거 보고 찾아가시는거예요?",
          "speaker_id": "2",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100040267.1.8",
          "form": "네 그러기도 하고 친구 추천으로 가기도 했고..?",
          "original_form": "네 그러기도 하고 친구 추천으로 가기도 했고..?",
          "speaker_id": "1",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100040267.1.9",
          "form": "요즘은 친구들이랑 거의 데이트처럼 노니까",
          "original_form": "요즘은 친구들이랑 거의 데이트처럼 노니까",
          "speaker_id": "1",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100040267.1.10",
          "form": "네네 약속장소 정할 때",
          "original_form": "네네 약속장소 정할 때",
          "speaker_id": "1",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100040267.1.11",
          "form": "인스타, 블로그 이런 데 찾아봐요!",
          "original_form": "인스타, 블로그 이런 데 찾아봐요!",
          "speaker_id": "1",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100040267.1.12",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100040267.1.13",
          "form": "맞아요 가는곳이 비슷비슷해져요",
          "original_form": "맞아요 가는곳이 비슷비슷해져요",
          "speaker_id": "2",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100040267.1.14",
          "form": "저는 그냥 의견을 안내요ㅋㅋ",
          "original_form": "저는 그냥 의견을 안내요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100040267.1.15",
          "form": "상대쪽 원하는방향으로",
          "original_form": "상대쪽 원하는방향으로",
          "speaker_id": "2",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100040267.1.16",
          "form": "앗 그러시구나 !!!!",
          "original_form": "앗 그러시구나 !!!!",
          "speaker_id": "1",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100040267.1.17",
          "form": "상대방한테 맞춰주시는군요",
          "original_form": "상대방한테 맞춰주시는군요",
          "speaker_id": "1",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100040267.1.18",
          "form": "그럼 리드하는 애인을 만나셔야겠어요!",
          "original_form": "그럼 리드하는 애인을 만나셔야겠어요!",
          "speaker_id": "1",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100040267.1.19",
          "form": "네 그런것도 있고 제가 찾기 귀찮은것도 있어요",
          "original_form": "네 그런것도 있고 제가 찾기 귀찮은것도 있어요",
          "speaker_id": "2",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100040267.1.20",
          "form": "그래서 차라리 이런건 상대가 너무 배려해주면 힘들어요ㅋㅋ",
          "original_form": "그래서 차라리 이런건 상대가 너무 배려해주면 힘들어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:21"
        },
        {
          "id": "MDRW2100040267.1.21",
          "form": "아 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:21"
        },
        {
          "id": "MDRW2100040267.1.22",
          "form": "역시 이런 것도 다 궁합인가봐요~~!",
          "original_form": "역시 이런 것도 다 궁합인가봐요~~!",
          "speaker_id": "1",
          "time": "20210904 15:21"
        },
        {
          "id": "MDRW2100040267.1.23",
          "form": "저는 이럴 때도 있고 저럴 때도 있어서,,,",
          "original_form": "저는 이럴 때도 있고 저럴 때도 있어서,,,",
          "speaker_id": "1",
          "time": "20210904 15:21"
        },
        {
          "id": "MDRW2100040267.1.24",
          "form": "맞아요 근데 중요한건 저도 양보안해요ㅋㅋ",
          "original_form": "맞아요 근데 중요한건 저도 양보안해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:21"
        },
        {
          "id": "MDRW2100040267.1.25",
          "form": "밥같은거야 다 맛있으니 상관없고ㅋㅋ",
          "original_form": "밥같은거야 다 맛있으니 상관없고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:22"
        },
        {
          "id": "MDRW2100040267.1.26",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "1",
          "time": "20210904 15:22"
        },
        {
          "id": "MDRW2100040267.1.27",
          "form": "저는 그.....",
          "original_form": "저는 그.....",
          "speaker_id": "1",
          "time": "20210904 15:23"
        },
        {
          "id": "MDRW2100040267.1.28",
          "form": "연애할 때 너무 가성비 따지려는 사람은 안맞더라구요",
          "original_form": "연애할 때 너무 가성비 따지려는 사람은 안맞더라구요",
          "speaker_id": "1",
          "time": "20210904 15:23"
        },
        {
          "id": "MDRW2100040267.1.29",
          "form": "아 최악이에요 그거ㅋㅋ",
          "original_form": "아 최악이에요 그거ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040267.1.30",
          "form": "절대만나면 안돼요",
          "original_form": "절대만나면 안돼요",
          "speaker_id": "2",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040267.1.31",
          "form": "저 사실 전에 만나던 남자가",
          "original_form": "저 사실 전에 만나던 남자가",
          "speaker_id": "1",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040267.1.32",
          "form": "자기 선배들이랑 놀 때는",
          "original_form": "자기 선배들이랑 놀 때는",
          "speaker_id": "1",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040267.1.33",
          "form": "형들 비위 잘 맞추면서",
          "original_form": "형들 비위 잘 맞추면서",
          "speaker_id": "1",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040267.1.34",
          "form": "저한테는,, 카카오프렌즈 인형,, 이딴걸",
          "original_form": "저한테는,, 카카오프렌즈 인형,, 이딴걸",
          "speaker_id": "1",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040267.1.35",
          "form": "크리스마스 선물이랍시고 ㅋㅋㅋㅋ",
          "original_form": "크리스마스 선물이랍시고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040267.1.36",
          "form": "ㅋㅋㅋ아 최악입니다 그거",
          "original_form": "ㅋㅋㅋ아 최악입니다 그거",
          "speaker_id": "2",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100040267.1.37",
          "form": "아 갑자기",
          "original_form": "아 갑자기",
