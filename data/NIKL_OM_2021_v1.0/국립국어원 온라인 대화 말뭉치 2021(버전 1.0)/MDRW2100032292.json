      "utterance": [
        {
          "id": "MDRW2100032292.1.1",
          "form": "요새 유튜브 뭐 보고 잇음?",
          "original_form": "요새 유튜브 뭐 보고 잇음?",
          "speaker_id": "1",
          "time": "20210905 14:59"
        },
        {
          "id": "MDRW2100032292.1.2",
          "form": "나는 스트리머들 영상",
          "original_form": "나는 스트리머들 영상",
          "speaker_id": "2",
          "time": "20210905 15:17"
        },
        {
          "id": "MDRW2100032292.1.3",
          "form": "하루종일 보고있지..",
          "original_form": "하루종일 보고있지..",
          "speaker_id": "2",
          "time": "20210905 15:17"
        },
        {
          "id": "MDRW2100032292.1.4",
          "form": "한번 빠져버렸더니 나올 수가 없어",
          "original_form": "한번 빠져버렸더니 나올 수가 없어",
          "speaker_id": "2",
          "time": "20210905 15:17"
        },
        {
          "id": "MDRW2100032292.1.5",
          "form": "너는 노래를 제일 많이 듣겠네",
          "original_form": "너는 노래를 제일 많이 듣겠네",
          "speaker_id": "2",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100032292.1.6",
          "form": "이제는 어플로 들어야할텐데",
          "original_form": "이제는 어플로 들어야할텐데",
          "speaker_id": "2",
          "time": "20210905 15:20"
        },
        {
          "id": "MDRW2100032292.1.7",
          "form": "그렇고만 나도 옛날에 많이 봣지",
          "original_form": "그렇고만 나도 옛날에 많이 봣지",
          "speaker_id": "1",
          "time": "20210905 15:21"
        },
        {
          "id": "MDRW2100032292.1.8",
          "form": "요샌 어플로 듣지 음원없는건 유튜브로 듣지만",
          "original_form": "요샌 어플로 듣지 음원없는건 유튜브로 듣지만",
          "speaker_id": "1",
          "time": "20210905 15:21"
        },
        {
          "id": "MDRW2100032292.1.9",
          "form": "나는 아까 영화볼까 싶어서 넷플릭스 한 바퀴 돌았는데 여전히 볼만한게 눈에 안들어왔어",
          "original_form": "나는 아까 영화볼까 싶어서 넷플릭스 한 바퀴 돌았는데 여전히 볼만한게 눈에 안들어왔어",
          "speaker_id": "2",
          "time": "20210905 15:21"
        },
        {
          "id": "MDRW2100032292.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ너도 킹덤봐",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ너도 킹덤봐",
          "speaker_id": "1",
          "time": "20210905 15:22"
        },
        {
          "id": "MDRW2100032292.1.11",
          "form": "그건 좀 그래",
          "original_form": "그건 좀 그래",
          "speaker_id": "2",
          "time": "20210905 15:22"
        },
        {
          "id": "MDRW2100032292.1.12",
          "form": "난 재밌게 봣어",
          "original_form": "난 재밌게 봣어",
          "speaker_id": "1",
          "time": "20210905 15:22"
        },
        {
          "id": "MDRW2100032292.1.13",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210905 15:22"
        },
        {
          "id": "MDRW2100032292.1.14",
          "form": "약간 재난물 느낌이잖아",
          "original_form": "약간 재난물 느낌이잖아",
          "speaker_id": "2",
          "time": "20210905 15:22"
        },
        {
          "id": "MDRW2100032292.1.15",
          "form": "난 재난물 별로 안좋아해",
          "original_form": "난 재난물 별로 안좋아해",
          "speaker_id": "2",
          "time": "20210905 15:22"
        },
        {
          "id": "MDRW2100032292.1.16",
          "form": "글쿠만",
          "original_form": "글쿠만",
          "speaker_id": "1",
          "time": "20210905 15:22"
        },
        {
          "id": "MDRW2100032292.1.17",
          "form": "좀비도 엄청 싫어했었는데 부산행 보고나서 그만큼 싫지는 않더라고",
          "original_form": "좀비도 엄청 싫어했었는데 부산행 보고나서 그만큼 싫지는 않더라고",
          "speaker_id": "2",
          "time": "20210905 15:22"
        },
        {
          "id": "MDRW2100032292.1.18",
          "form": "그럼뭐잇디",
          "original_form": "그럼뭐잇디",
          "speaker_id": "1",
          "time": "20210905 15:22"
        },
        {
          "id": "MDRW2100032292.1.19",
          "form": "아직 스위트홈도 안봤는데",
          "original_form": "아직 스위트홈도 안봤는데",
          "speaker_id": "2",
          "time": "20210905 15:23"
        },
        {
          "id": "MDRW2100032292.1.20",
          "form": "ㅋㅋㅋㅋㅋ부산행재밋지",
          "original_form": "ㅋㅋㅋㅋㅋ부산행재밋지",
          "speaker_id": "1",
          "time": "20210905 15:23"
        },
        {
          "id": "MDRW2100032292.1.21",
          "form": "나도 웹툰만 봣어",
          "original_form": "나도 웹툰만 봣어",
          "speaker_id": "1",
          "time": "20210905 15:23"
        },
        {
          "id": "MDRW2100032292.1.22",
          "form": "엑시트같은 영화를 더 알게되면 좋겠다",
          "original_form": "엑시트같은 영화를 더 알게되면 좋겠다",
          "speaker_id": "2",
          "time": "20210905 15:23"
        },
        {
          "id": "MDRW2100032292.1.23",
          "form": "뭔가 많이 바뀐거같음",
          "original_form": "뭔가 많이 바뀐거같음",
          "speaker_id": "1",
          "time": "20210905 15:23"
        },
        {
          "id": "MDRW2100032292.1.24",
          "form": "부산행은 난 영화만 봤지만 영화가 레전드인듯",
          "original_form": "부산행은 난 영화만 봤지만 영화가 레전드인듯",
          "speaker_id": "2",
          "time": "20210905 15:23"
        },
        {
          "id": "MDRW2100032292.1.25",
          "form": "부산행은 영화만 잇는거아냐?",
          "original_form": "부산행은 영화만 잇는거아냐?",
          "speaker_id": "1",
          "time": "20210905 15:23"
        },
        {
          "id": "MDRW2100032292.1.26",
          "form": "좋아하는 배우도 나와서 더 좋았나",
          "original_form": "좋아하는 배우도 나와서 더 좋았나",
          "speaker_id": "2",
          "time": "20210905 15:23"
        },
        {
          "id": "MDRW2100032292.1.27",
          "form": "난 스위트홈 웹툰말한거엿어",
          "original_form": "난 스위트홈 웹툰말한거엿어",
          "speaker_id": "1",
          "time": "20210905 15:24"
        },
        {
          "id": "MDRW2100032292.1.28",
          "form": "아 스위트홈 웹툰 봤다고?",
          "original_form": "아 스위트홈 웹툰 봤다고?",
          "speaker_id": "2",
          "time": "20210905 15:24"
        },
        {
          "id": "MDRW2100032292.1.29",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210905 15:24"
        },
        {
          "id": "MDRW2100032292.1.30",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210905 15:24"
        },
        {
          "id": "MDRW2100032292.1.31",
          "form": "재밋엇지",
          "original_form": "재밋엇지",
          "speaker_id": "1",
          "time": "20210905 15:24"
        },
        {
          "id": "MDRW2100032292.1.32",
          "form": "그새 개물개물하지만",
          "original_form": "그새 개물개물하지만",
          "speaker_id": "1",
          "time": "20210905 15:24"
        },
        {
          "id": "MDRW2100032292.1.33",
          "form": "가뭉가물",
          "original_form": "가뭉가물",
          "speaker_id": "1",
          "time": "20210905 15:24"
        },
        {
          "id": "MDRW2100032292.1.34",
          "form": "으응..",
          "original_form": "으응..",
          "speaker_id": "2",
          "time": "20210905 15:24"
        },
        {
          "id": "MDRW2100032292.1.35",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 15:24"
        },
        {
          "id": "MDRW2100032292.1.36",
          "form": "나는",
          "original_form": "나는",
          "speaker_id": "2",
          "time": "20210905 15:25"
        },
        {
          "id": "MDRW2100032292.1.37",
          "form": "낭만닥터를 재밌게 봐서",
          "original_form": "낭만닥터를 재밌게 봐서",
          "speaker_id": "2",
          "time": "20210905 15:25"
        },
        {
          "id": "MDRW2100032292.1.38",
          "form": "정주행하고싶더라",
          "original_form": "정주행하고싶더라",
          "speaker_id": "2",
          "time": "20210905 15:25"
        },
        {
          "id": "MDRW2100032292.1.39",
          "form": "그때도 한두번 본거같은데",
          "original_form": "그때도 한두번 본거같은데",
          "speaker_id": "2",
          "time": "20210905 15:25"
        },
        {
          "id": "MDRW2100032292.1.40",
          "form": "시즌 1, 2 둘 다 좋아하는 배우들 나와서 더 좋았던거같기도해",
          "original_form": "시즌 1, 2 둘 다 좋아하는 배우들 나와서 더 좋았던거같기도해",
          "speaker_id": "2",
          "time": "20210905 15:26"
        },
        {
          "id": "MDRW2100032292.1.41",
          "form": "그럼 또 보면 되지",
          "original_form": "그럼 또 보면 되지",
          "speaker_id": "1",
          "time": "20210905 15:26"
        },
        {
          "id": "MDRW2100032292.1.42",
          "form": "그때쯤에는 드라마를 진짜 많이 챙겨봤었는데",
          "original_form": "그때쯤에는 드라마를 진짜 많이 챙겨봤었는데",
          "speaker_id": "2",
          "time": "20210905 15:26"
        },
        {
          "id": "MDRW2100032292.1.43",
          "form": "낭만닥터가 그 김사부얏나?",
          "original_form": "낭만닥터가 그 김사부얏나?",
          "speaker_id": "1",
          "time": "20210905 15:26"
        },
        {
          "id": "MDRW2100032292.1.44",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210905 15:26"
        },
        {
          "id": "MDRW2100032292.1.45",
          "form": "맞구만",
          "original_form": "맞구만",
          "speaker_id": "1",
          "time": "20210905 15:26"
        },
        {
          "id": "MDRW2100032292.1.46",
          "form": "그럼 나더 몇번본적잇어",
          "original_form": "그럼 나더 몇번본적잇어",
          "speaker_id": "1",
          "time": "20210905 15:26"
        },
        {
          "id": "MDRW2100032292.1.47",
          "form": "서현진도 좋아하고",
          "original_form": "서현진도 좋아하고",
          "speaker_id": "2",
          "time": "20210905 15:26"
        },
        {
          "id": "MDRW2100032292.1.48",
          "form": "글쿠만",
          "original_form": "글쿠만",
          "speaker_id": "1",
          "time": "20210905 15:28"
        },
        {
          "id": "MDRW2100032292.1.49",
          "form": "이성경도 좋아해",
          "original_form": "이성경도 좋아해",
          "speaker_id": "2",
          "time": "20210905 15:28"
        },
        {
          "id": "MDRW2100032292.1.50",
          "form": "다른 배우들도 좋았어",
          "original_form": "다른 배우들도 좋았어",
          "speaker_id": "2",
          "time": "20210905 15:28"
        },
        {
          "id": "MDRW2100032292.1.51",
          "form": "재밌었다",
          "original_form": "재밌었다",
          "speaker_id": "2",
          "time": "20210905 15:28"
        },
        {
          "id": "MDRW2100032292.1.52",
          "form": "난 배우는 잘몸ㄹ러",
          "original_form": "난 배우는 잘몸ㄹ러",
          "speaker_id": "1",
          "time": "20210905 15:28"
        },
        {
          "id": "MDRW2100032292.1.53",
          "form": "근대 저사람들은 아는듯",
          "original_form": "근대 저사람들은 아는듯",
          "speaker_id": "1",
          "time": "20210905 15:28"
        },
        {
          "id": "MDRW2100032292.1.54",
          "form": "내가 재밌다고 추천하는 드라마는 잘 없거든",
          "original_form": "내가 재밌다고 추천하는 드라마는 잘 없거든",
          "speaker_id": "2",
          "time": "20210905 15:28"
        },
        {
          "id": "MDRW2100032292.1.55",
          "form": "끝까지 다 보는게 드물어서",
          "original_form": "끝까지 다 보는게 드물어서",
          "speaker_id": "2",
          "time": "20210905 15:28"
        },
        {
          "id": "MDRW2100032292.1.56",
          "form": "또 있었을 수는 있는데",
          "original_form": "또 있었을 수는 있는데",
          "speaker_id": "2",
          "time": "20210905 15:29"
        },
        {
          "id": "MDRW2100032292.1.57",
          "form": "일단 기억나는거는 일드 언내추럴이랑 낭만닥터 김사부 두개네",
          "original_form": "일단 기억나는거는 일드 언내추럴이랑 낭만닥터 김사부 두개네",
          "speaker_id": "2",
          "time": "20210905 15:29"
        },
        {
          "id": "MDRW2100032292.1.58",
          "form": "언내추럴 추천해줬을때도 넌 안봤지?",
          "original_form": "언내추럴 추천해줬을때도 넌 안봤지?",
          "speaker_id": "2",
          "time": "20210905 15:29"
        },
        {
          "id": "MDRW2100032292.1.59",
          "form": "못본거제..",
          "original_form": "못본거제..",
          "speaker_id": "1",
          "time": "20210905 15:31"
        },
        {
          "id": "MDRW2100032292.1.60",
          "form": "ㄴ0ㅅ플에 업서",
          "original_form": "ㄴ0ㅅ플에 업서",
          "speaker_id": "1",
          "time": "20210905 15:31"
        },
        {
          "id": "MDRW2100032292.1.61",
          "form": "왓챠에 잇대",
          "original_form": "왓챠에 잇대",
          "speaker_id": "1",
          "time": "20210905 15:31"
        },
        {
          "id": "MDRW2100032292.1.62",
          "form": "우리 언젠가는 다같이 왓챠로 옮겨보자",
          "original_form": "우리 언젠가는 다같이 왓챠로 옮겨보자",
          "speaker_id": "2",
          "time": "20210905 15:32"
        },
        {
          "id": "MDRW2100032292.1.63",
          "form": "넷플도 뭔가 끊기 아까운데 그럴게 효율적으로 보질않는거같아",
          "original_form": "넷플도 뭔가 끊기 아까운데 그럴게 효율적으로 보질않는거같아",
          "speaker_id": "2",
          "time": "20210905 15:32"
        },
