      "utterance": [
        {
          "id": "MDRW2100045231.1.1",
          "form": "엘리자벳이",
          "original_form": "엘리자벳이",
          "speaker_id": "2",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045231.1.2",
          "form": "페르젠 나오는거지여?",
          "original_form": "페르젠 나오는거지여?",
          "speaker_id": "2",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045231.1.3",
          "form": "아이돌들 자주 들어가는",
          "original_form": "아이돌들 자주 들어가는",
          "speaker_id": "2",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045231.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045231.1.5",
          "form": "아니그건 마리앙투아네트",
          "original_form": "아니그건 마리앙투아네트",
          "speaker_id": "1",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045231.1.6",
          "form": "엘리자벳은",
          "original_form": "엘리자벳은",
          "speaker_id": "1",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045231.1.7",
          "form": "아마 알기로 돌이 들어가봐야",
          "original_form": "아마 알기로 돌이 들어가봐야",
          "speaker_id": "1",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045231.1.8",
          "form": "샤?",
          "original_form": "샤?",
          "speaker_id": "1",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045231.1.9",
          "form": "박효신도 한거같은데",
          "original_form": "박효신도 한거같은데",
          "speaker_id": "1",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045231.1.10",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045231.1.11",
          "form": "아 맞네 마리앙이구나",
          "original_form": "아 맞네 마리앙이구나",
          "speaker_id": "2",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045231.1.12",
          "form": "나는 나만의 것이 엘리자벳 넘버인가...?",
          "original_form": "나는 나만의 것이 엘리자벳 넘버인가...?",
          "speaker_id": "2",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045231.1.13",
          "form": "마리앙ㅇ느",
          "original_form": "마리앙ㅇ느",
          "speaker_id": "1",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045231.1.14",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045231.1.15",
          "form": "머릿속에서 다 섞여잇넼ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "머릿속에서 다 섞여잇넼ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045231.1.16",
          "form": "그거 엘리자벳은",
          "original_form": "그거 엘리자벳은",
          "speaker_id": "1",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045231.1.17",
          "form": "죽음이유명했음",
          "original_form": "죽음이유명했음",
          "speaker_id": "1",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045231.1.18",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045231.1.19",
          "form": "샤토드라고",
          "original_form": "샤토드라고",
          "speaker_id": "2",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045231.1.20",
          "form": "지겹게 들어본즛",
          "original_form": "지겹게 들어본즛",
          "speaker_id": "2",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045231.1.21",
          "form": "듯",
          "original_form": "듯",
          "speaker_id": "2",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045231.1.22",
          "form": "유툽프로모션도 빡세게했을것",
          "original_form": "유툽프로모션도 빡세게했을것",
          "speaker_id": "1",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045231.1.23",
          "form": "류님외에 인정못해",
          "original_form": "류님외에 인정못해",
          "speaker_id": "1",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045231.1.24",
          "form": "류토드지",
          "original_form": "류토드지",
          "speaker_id": "1",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045231.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045231.1.26",
          "form": "넘버를 그길밖에 모르는데 뭔가",
          "original_form": "넘버를 그길밖에 모르는데 뭔가",
          "speaker_id": "2",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045231.1.27",
          "form": "후... 샤는...",
          "original_form": "후... 샤는...",
          "speaker_id": "1",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045231.1.28",
          "form": "아니야",
          "original_form": "아니야",
          "speaker_id": "1",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045231.1.29",
          "form": "토드 다들 한번씩 해보고 싶어하는 배역같던데",
          "original_form": "토드 다들 한번씩 해보고 싶어하는 배역같던데",
          "speaker_id": "2",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045231.1.30",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045231.1.31",
          "form": "샤는 저도.. 인정 x..",
          "original_form": "샤는 저도.. 인정 x..",
          "speaker_id": "2",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045231.1.32",
          "form": "어려운역이라",
          "original_form": "어려운역이라",
          "speaker_id": "1",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045231.1.33",
          "form": "후",
          "original_form": "후",
          "speaker_id": "1",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045231.1.34",
          "form": "영상으로",
          "original_form": "영상으로",
          "speaker_id": "1",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045231.1.35",
          "form": "박제좀해주라고.",
          "original_form": "박제좀해주라고.",
          "speaker_id": "1",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045231.1.36",
          "form": "류님 고급인간 연기 정말 잘해서",
          "original_form": "류님 고급인간 연기 정말 잘해서",
          "speaker_id": "1",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045231.1.37",
          "form": "토드할때 ㅣㄴ짜 고급진 토드..",
          "original_form": "토드할때 ㅣㄴ짜 고급진 토드..",
          "speaker_id": "1",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045231.1.38",
          "form": "프레스콜 영상같은거 없나...?",
          "original_form": "프레스콜 영상같은거 없나...?",
          "speaker_id": "2",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045231.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ노랜남아있어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ노랜남아있어요",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045231.1.40",
          "form": "후...",
          "original_form": "후...",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045231.1.41",
