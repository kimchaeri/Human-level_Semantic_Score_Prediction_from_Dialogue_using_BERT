        },
        {
          "id": "MDRW2100022763.1.3",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210829 17:45"
        },
        {
          "id": "MDRW2100022763.1.4",
          "form": "펫샵도 펫샵인데",
          "original_form": "펫샵도 펫샵인데",
          "speaker_id": "2",
          "time": "20210829 17:45"
        },
        {
          "id": "MDRW2100022763.1.5",
          "form": "준비 안된 사람이",
          "original_form": "준비 안된 사람이",
          "speaker_id": "2",
          "time": "20210829 17:45"
        },
        {
          "id": "MDRW2100022763.1.6",
          "form": "동물을 카우는게 문제인듯",
          "original_form": "동물을 카우는게 문제인듯",
          "speaker_id": "2",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100022763.1.7",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100022763.1.8",
          "form": "걍 물건하나 사오듯이",
          "original_form": "걍 물건하나 사오듯이",
          "speaker_id": "1",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100022763.1.9",
          "form": "귀엽고좋은면만 상상하면서",
          "original_form": "귀엽고좋은면만 상상하면서",
          "speaker_id": "1",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100022763.1.10",
          "form": "맞아맞라",
          "original_form": "맞아맞라",
          "speaker_id": "2",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100022763.1.11",
          "form": "ㅋㅋㅋㅋㅋ ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100022763.1.12",
          "form": "어렸을때",
          "original_form": "어렸을때",
          "speaker_id": "2",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100022763.1.13",
          "form": "동화책 봐도",
          "original_form": "동화책 봐도",
          "speaker_id": "2",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100022763.1.14",
          "form": "막 크라스마스 선물로 동물 사오고",
          "original_form": "막 크라스마스 선물로 동물 사오고",
          "speaker_id": "2",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100022763.1.15",
          "form": "맞음맞음",
          "original_form": "맞음맞음",
          "speaker_id": "1",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100022763.1.16",
          "form": "선물상자 안에 강아지나 고양이 들어있고",
          "original_form": "선물상자 안에 강아지나 고양이 들어있고",
          "speaker_id": "2",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100022763.1.17",
          "form": "이런것만 나오제",
          "original_form": "이런것만 나오제",
          "speaker_id": "2",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100022763.1.18",
          "form": "옛날드라마들도 그런내용 진짜많았음ㅋㅋㅋㅋ",
          "original_form": "옛날드라마들도 그런내용 진짜많았음ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:47"
        },
        {
          "id": "MDRW2100022763.1.19",
          "form": "강아지를 데려오기 위해 준비하는 그런 책들은",
          "original_form": "강아지를 데려오기 위해 준비하는 그런 책들은",
          "speaker_id": "2",
          "time": "20210829 17:47"
        },
        {
          "id": "MDRW2100022763.1.20",
          "form": "진짜 보미도 못했다",
          "original_form": "진짜 보미도 못했다",
          "speaker_id": "2",
          "time": "20210829 17:47"
        },
        {
          "id": "MDRW2100022763.1.21",
          "form": "ㅇㅈㅇㅈㅇㅈㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅈㅇㅈㅇㅈㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:47"
        },
        {
          "id": "MDRW2100022763.1.22",
          "form": "갑자기 남주가 여주한테 강아지 턱",
          "original_form": "갑자기 남주가 여주한테 강아지 턱",
          "speaker_id": "2",
          "time": "20210829 17:47"
        },
        {
          "id": "MDRW2100022763.1.23",
          "form": "으악!!!!!!!",
          "original_form": "으악!!!!!!!",
          "speaker_id": "1",
          "time": "20210829 17:47"
        },
        {
          "id": "MDRW2100022763.1.24",
          "form": "심지어 드라마 특성상 여주는 돈도 없는데",
          "original_form": "심지어 드라마 특성상 여주는 돈도 없는데",
          "speaker_id": "2",
          "time": "20210829 17:47"
        },
        {
          "id": "MDRW2100022763.1.25",
          "form": "품종견 키우면서",
          "original_form": "품종견 키우면서",
          "speaker_id": "2",
          "time": "20210829 17:47"
        },
        {
          "id": "MDRW2100022763.1.26",
          "form": "드라마 잘되면 그 품종도 수요 늘고",
          "original_form": "드라마 잘되면 그 품종도 수요 늘고",
          "speaker_id": "2",
          "time": "20210829 17:48"
        },
        {
          "id": "MDRW2100022763.1.27",
          "form": "진짜 끔찍하다",
          "original_form": "진짜 끔찍하다",
          "speaker_id": "2",
          "time": "20210829 17:48"
        },
        {
          "id": "MDRW2100022763.1.28",
          "form": "으...",
          "original_form": "으...",
          "speaker_id": "1",
          "time": "20210829 17:48"
        },
        {
          "id": "MDRW2100022763.1.29",
          "form": "+남주가 여주닮은 동물 사와서",
          "original_form": "+남주가 여주닮은 동물 사와서",
          "speaker_id": "1",
          "time": "20210829 17:48"
        },
        {
          "id": "MDRW2100022763.1.30",
          "form": "여주이름 붙이기 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "여주이름 붙이기 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:48"
        },
