      "utterance": [
        {
          "id": "MDRW2100033964.1.1",
          "form": "어떤거 쇼핑하셨러요?",
          "original_form": "어떤거 쇼핑하셨러요?",
          "speaker_id": "1",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100033964.1.2",
          "form": "전 요새 식자재요 ㅋㅋ",
          "original_form": "전 요새 식자재요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100033964.1.3",
          "form": "식자재요? 오",
          "original_form": "식자재요? 오",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100033964.1.4",
          "form": "저는 운동화를 샀답니다ㅋㅋㅋ",
          "original_form": "저는 운동화를 샀답니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100033964.1.5",
          "form": "오 저도 운동화",
          "original_form": "오 저도 운동화",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100033964.1.6",
          "form": "두켤레요",
          "original_form": "두켤레요",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100033964.1.7",
          "form": "남편 딸꺼요 ㅎㅎ",
          "original_form": "남편 딸꺼요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100033964.1.8",
          "form": "네 주로 안나가서 요새 식자재요 복숭아랑",
          "original_form": "네 주로 안나가서 요새 식자재요 복숭아랑",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100033964.1.9",
          "form": "냉동피자",
          "original_form": "냉동피자",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100033964.1.10",
          "form": "야채 배달 시켰어요",
          "original_form": "야채 배달 시켰어요",
          "speaker_id": "1",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100033964.1.11",
          "form": "복숭아 넘 성공했는데 ㅎㅎ이제 끝이라 안나오네요",
          "original_form": "복숭아 넘 성공했는데 ㅎㅎ이제 끝이라 안나오네요",
          "speaker_id": "1",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100033964.1.12",
          "form": "운동화 무슨색 사셨어요?",
          "original_form": "운동화 무슨색 사셨어요?",
          "speaker_id": "1",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100033964.1.13",
          "form": "냉동피자좋네요ㅋㅋㅋ 복숭아너무맛있었는데",
          "original_form": "냉동피자좋네요ㅋㅋㅋ 복숭아너무맛있었는데",
          "speaker_id": "2",
          "time": "20210825 17:38"
        },
        {
          "id": "MDRW2100033964.1.14",
          "form": "철지나가는중이라 아쉬워요",
          "original_form": "철지나가는중이라 아쉬워요",
          "speaker_id": "2",
          "time": "20210825 17:38"
        },
        {
          "id": "MDRW2100033964.1.15",
          "form": "저 흰색이요ㅋㅋㅋ 베이지인가",
          "original_form": "저 흰색이요ㅋㅋㅋ 베이지인가",
          "speaker_id": "2",
          "time": "20210825 17:38"
        },
        {
          "id": "MDRW2100033964.1.16",
          "form": "아디다스 오즈위고요!",
          "original_form": "아디다스 오즈위고요!",
          "speaker_id": "2",
          "time": "20210825 17:38"
        },
        {
          "id": "MDRW2100033964.1.17",
          "form": "네 철지나서 이젠 못먹죠 ㅜㅜ",
          "original_form": "네 철지나서 이젠 못먹죠 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100033964.1.18",
          "form": "오 흰색요 멋쟁이시네요 ㅎㅎ",
          "original_form": "오 흰색요 멋쟁이시네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:40"
        },
        {
          "id": "MDRW2100033964.1.19",
          "form": "전 검정요",
          "original_form": "전 검정요",
          "speaker_id": "1",
          "time": "20210825 17:40"
        },
        {
          "id": "MDRW2100033964.1.20",
          "form": "전 휜색 자신이 없어요",
          "original_form": "전 휜색 자신이 없어요",
          "speaker_id": "1",
          "time": "20210825 17:40"
        },
        {
          "id": "MDRW2100033964.1.21",
          "form": "흰색",
          "original_form": "흰색",
          "speaker_id": "1",
          "time": "20210825 17:40"
        },
        {
          "id": "MDRW2100033964.1.22",
          "form": "왜죠?ㅠㅠ",
          "original_form": "왜죠?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100033964.1.23",
          "form": "때탈까봐인가요ㅣ",
          "original_form": "때탈까봐인가요ㅣ",
          "speaker_id": "2",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100033964.1.24",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100033964.1.25",
          "form": "저 때탈까봐요",
          "original_form": "저 때탈까봐요",
          "speaker_id": "1",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100033964.1.26",
          "form": "관리가 좀 서툴러요 ㅜㅜ",
          "original_form": "관리가 좀 서툴러요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100033964.1.27",
          "form": "관리 잘하세요?",
          "original_form": "관리 잘하세요?",
          "speaker_id": "1",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100033964.1.28",
          "form": "전 전용세제로 쓱쓱해도 더러워서 하나사고 포기요",
          "original_form": "전 전용세제로 쓱쓱해도 더러워서 하나사고 포기요",
          "speaker_id": "1",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100033964.1.29",
          "form": "ㅋㅋㅋ사실저는",
          "original_form": "ㅋㅋㅋ사실저는",
          "speaker_id": "2",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100033964.1.30",
          "form": "그냥 막신어요",
          "original_form": "그냥 막신어요",
          "speaker_id": "2",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100033964.1.31",
          "form": "관리랄거없이",
          "original_form": "관리랄거없이",
          "speaker_id": "2",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100033964.1.32",
          "form": "오 통크시군요",
          "original_form": "오 통크시군요",
          "speaker_id": "1",
          "time": "20210825 18:04"
        },
        {
          "id": "MDRW2100033964.1.33",
          "form": "대범하게 막",
          "original_form": "대범하게 막",
          "speaker_id": "1",
          "time": "20210825 18:04"
        },
        {
          "id": "MDRW2100033964.1.34",
          "form": "전 소심하게 신는데도 ㅋㅋ더러워져요",
          "original_form": "전 소심하게 신는데도 ㅋㅋ더러워져요",
          "speaker_id": "1",
          "time": "20210825 18:04"
        },
        {
          "id": "MDRW2100033964.1.35",
          "form": "가방하나 샀어요",
          "original_form": "가방하나 샀어요",
          "speaker_id": "1",
          "time": "20210825 18:41"
        },
        {
          "id": "MDRW2100033964.1.36",
          "form": "아들껀데 h패션 아울렛 쿠폰 받아서 싸게 샀어요 ㅎㅎ",
          "original_form": "아들껀데 h패션 아울렛 쿠폰 받아서 싸게 샀어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:42"
        },
        {
          "id": "MDRW2100033964.1.37",
          "form": "오 오즈위고 검색해보니이뿌네요 ㅎㅎ",
          "original_form": "오 오즈위고 검색해보니이뿌네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100033964.1.38",
          "form": "저희딸도 사고 싶어 할것 같아요 ㅎㅎ",
          "original_form": "저희딸도 사고 싶어 할것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100033964.1.39",
          "form": "오 가방좋네요ㅋㅋㅋㅋ",
          "original_form": "오 가방좋네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100033964.1.40",
          "form": "네네 오즈위고 인기잇는템이랍니다ㅋㅋㅋ엄청요!",
          "original_form": "네네 오즈위고 인기잇는템이랍니다ㅋㅋㅋ엄청요!",
          "speaker_id": "2",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100033964.1.41",
          "form": "그런거 같아요",
          "original_form": "그런거 같아요",
          "speaker_id": "1",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100033964.1.42",
          "form": "딸사줘야 겠어요",
          "original_form": "딸사줘야 겠어요",
          "speaker_id": "1",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100033964.1.43",
          "form": "그럼 인싸 되나요 ㅎㅎ",
          "original_form": "그럼 인싸 되나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100033964.1.44",
          "form": "ㅋㅋㅋ존예랍니다",
          "original_form": "ㅋㅋㅋ존예랍니다",
          "speaker_id": "2",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100033964.1.45",
          "form": "신어보고사는거추천이요!",
          "original_form": "신어보고사는거추천이요!",
          "speaker_id": "2",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100033964.1.46",
          "form": "네 abc 가볼께요 ㅎㅎ",
          "original_form": "네 abc 가볼께요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100033964.1.47",
          "form": "이제 샌들 벗고 운동화 사야하는데 좋은정보네요 ㅎㅎ",
          "original_form": "이제 샌들 벗고 운동화 사야하는데 좋은정보네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100033964.1.48",
          "form": "네네ㅋㅋㅋ 아",
          "original_form": "네네ㅋㅋㅋ 아",
