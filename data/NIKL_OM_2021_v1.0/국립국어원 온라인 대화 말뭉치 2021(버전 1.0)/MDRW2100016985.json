      "utterance": [
        {
          "id": "MDRW2100016985.1.1",
          "form": "당근 하세요?",
          "original_form": "당근 하세요?",
          "speaker_id": "2",
          "time": "20210826 16:42"
        },
        {
          "id": "MDRW2100016985.1.2",
          "form": "네 자주합니다",
          "original_form": "네 자주합니다",
          "speaker_id": "1",
          "time": "20210826 16:43"
        },
        {
          "id": "MDRW2100016985.1.3",
          "form": "주로 어떤 거래 하세요",
          "original_form": "주로 어떤 거래 하세요",
          "speaker_id": "2",
          "time": "20210826 16:44"
        },
        {
          "id": "MDRW2100016985.1.4",
          "form": "옷 신발등등 합니다ㅎㅎ",
          "original_form": "옷 신발등등 합니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 16:46"
        },
        {
          "id": "MDRW2100016985.1.5",
          "form": "부지런하시네요",
          "original_form": "부지런하시네요",
          "speaker_id": "2",
          "time": "20210826 16:46"
        },
        {
          "id": "MDRW2100016985.1.6",
          "form": "전 주로 문구용품이나 사무용품 팔았어요",
          "original_form": "전 주로 문구용품이나 사무용품 팔았어요",
          "speaker_id": "2",
          "time": "20210826 16:46"
        },
        {
          "id": "MDRW2100016985.1.7",
          "form": "오 잘 팔리셨나요",
          "original_form": "오 잘 팔리셨나요",
          "speaker_id": "1",
          "time": "20210826 16:47"
        },
        {
          "id": "MDRW2100016985.1.8",
          "form": "주변에 학생들이 종종 사갔어요",
          "original_form": "주변에 학생들이 종종 사갔어요",
          "speaker_id": "2",
          "time": "20210826 16:48"
        },
        {
          "id": "MDRW2100016985.1.9",
          "form": "근데 한번 바람 맞아서 상처입고 안해요 흑흑",
          "original_form": "근데 한번 바람 맞아서 상처입고 안해요 흑흑",
          "speaker_id": "2",
          "time": "20210826 16:49"
        },
        {
          "id": "MDRW2100016985.1.10",
          "form": "헉 사기당하셧나요",
          "original_form": "헉 사기당하셧나요",
          "speaker_id": "1",
          "time": "20210826 17:53"
        },
        {
          "id": "MDRW2100016985.1.11",
          "form": "사러오기로 해놓고 안오더라구여ㅋㅋㅋ",
          "original_form": "사러오기로 해놓고 안오더라구여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100016985.1.12",
          "form": "아 너무시러요",
          "original_form": "아 너무시러요",
          "speaker_id": "1",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100016985.1.13",
          "form": "근데 마침 팔기 아쉬웠던 물건이었는데 운명인가 싶어서 그냥 긍정적으로 생각했어요",
          "original_form": "근데 마침 팔기 아쉬웠던 물건이었는데 운명인가 싶어서 그냥 긍정적으로 생각했어요",
          "speaker_id": "2",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100016985.1.14",
          "form": "간직해야되나보다 하고",
          "original_form": "간직해야되나보다 하고",
          "speaker_id": "2",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100016985.1.15",
          "form": "오히려 다행이네요ㅠ",
          "original_form": "오히려 다행이네요ㅠ",
