      "utterance": [
        {
          "id": "MDRW2100004893.1.1",
          "form": "그러게 다들 놀래네",
          "original_form": "그러게 다들 놀래네",
          "speaker_id": "2",
          "time": "20210821 19:09"
        },
        {
          "id": "MDRW2100004893.1.2",
          "form": "깔끔하다 정말 포즈도 그렇고 결과도",
          "original_form": "깔끔하다 정말 포즈도 그렇고 결과도",
          "speaker_id": "1",
          "time": "20210821 19:09"
        },
        {
          "id": "MDRW2100004893.1.3",
          "form": "실력자다",
          "original_form": "실력자다",
          "speaker_id": "2",
          "time": "20210821 19:09"
        },
        {
          "id": "MDRW2100004893.1.4",
          "form": "응 현주 저분도 선영 잘 친다고 뭐라고 하네",
          "original_form": "응 현주 저분도 선영 잘 친다고 뭐라고 하네",
          "speaker_id": "1",
          "time": "20210821 19:09"
        },
        {
          "id": "MDRW2100004893.1.5",
          "form": "선수가 보기에도 그러면 진짜 잘 치는거임",
          "original_form": "선수가 보기에도 그러면 진짜 잘 치는거임",
          "speaker_id": "2",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004893.1.6",
          "form": "맞아 쉽지 않을껀데",
          "original_form": "맞아 쉽지 않을껀데",
          "speaker_id": "1",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004893.1.7",
          "form": "결정 적일때 광고들어가네",
          "original_form": "결정 적일때 광고들어가네",
          "speaker_id": "2",
          "time": "20210821 19:10"
        },
        {
          "id": "MDRW2100004893.1.8",
          "form": "ㅋㅋ 그러게 난 이거 보고 놀토 볼꺼야",
          "original_form": "ㅋㅋ 그러게 난 이거 보고 놀토 볼꺼야",
          "speaker_id": "1",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004893.1.9",
          "form": "시작한다",
          "original_form": "시작한다",
          "speaker_id": "2",
          "time": "20210821 19:11"
        },
        {
          "id": "MDRW2100004893.1.10",
          "form": "홀컴에 들어갔나가 나옴",
          "original_form": "홀컴에 들어갔나가 나옴",
          "speaker_id": "1",
          "time": "20210821 19:12"
        },
        {
          "id": "MDRW2100004893.1.11",
          "form": "안타깝다 좋은 기회였는데",
          "original_form": "안타깝다 좋은 기회였는데",
          "speaker_id": "2",
          "time": "20210821 19:12"
        },
        {
          "id": "MDRW2100004893.1.12",
          "form": "그러게 승엽이분도 잘하는데 상대편이 너무 잘히니 부담되나보다",
          "original_form": "그러게 승엽이분도 잘하는데 상대편이 너무 잘히니 부담되나보다",
          "speaker_id": "1",
          "time": "20210821 19:13"
        },
        {
          "id": "MDRW2100004893.1.13",
          "form": "에고 다시 홀컵 들어갔다 나옴",
          "original_form": "에고 다시 홀컵 들어갔다 나옴",
          "speaker_id": "2",
          "time": "20210821 19:13"
        },
        {
          "id": "MDRW2100004893.1.14",
          "form": "일진이 안좋은 날인갑네",
          "original_form": "일진이 안좋은 날인갑네",
          "speaker_id": "1",
