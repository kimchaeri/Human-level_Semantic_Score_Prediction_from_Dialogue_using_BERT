      "utterance": [
        {
          "id": "MDRW2100003488.1.1",
          "form": "뭐샀는데?",
          "original_form": "뭐샀는데?",
          "speaker_id": "1",
          "time": "20210820 23:42"
        },
        {
          "id": "MDRW2100003488.1.2",
          "form": "누나 선물은 안사고",
          "original_form": "누나 선물은 안사고",
          "speaker_id": "1",
          "time": "20210820 23:42"
        },
        {
          "id": "MDRW2100003488.1.3",
          "form": "안샀다",
          "original_form": "안샀다",
          "speaker_id": "2",
          "time": "20210820 23:42"
        },
        {
          "id": "MDRW2100003488.1.4",
          "form": "바람 현질좀했다",
          "original_form": "바람 현질좀했다",
          "speaker_id": "2",
          "time": "20210820 23:42"
        },
        {
          "id": "MDRW2100003488.1.5",
          "form": "다",
          "original_form": "다",
          "speaker_id": "2",
          "time": "20210820 23:42"
        },
        {
          "id": "MDRW2100003488.1.6",
          "form": "불만있으면 카톡 account",
          "original_form": "불만있으면 카톡 &account&",
          "speaker_id": "2",
          "time": "20210820 23:42"
        },
        {
          "id": "MDRW2100003488.1.7",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210820 23:42"
        },
        {
          "id": "MDRW2100003488.1.8",
          "form": "게임에 돈을 쓰다니",
          "original_form": "게임에 돈을 쓰다니",
          "speaker_id": "1",
          "time": "20210820 23:42"
        },
        {
          "id": "MDRW2100003488.1.9",
          "form": "중독자가 다되었구나",
          "original_form": "중독자가 다되었구나",
          "speaker_id": "1",
          "time": "20210820 23:42"
        },
        {
          "id": "MDRW2100003488.1.10",
          "form": "보물변신 기댓값 200만원인데",
          "original_form": "보물변신 기댓값 200만원인데",
          "speaker_id": "2",
          "time": "20210820 23:43"
        },
        {
          "id": "MDRW2100003488.1.11",
          "form": "지금 2주동안 보물변신만",
          "original_form": "지금 2주동안 보물변신만",
          "speaker_id": "2",
          "time": "20210820 23:43"
        },
        {
          "id": "MDRW2100003488.1.12",
          "form": "4개 얻었따",
          "original_form": "4개 얻었따",
          "speaker_id": "2",
          "time": "20210820 23:43"
        },
        {
          "id": "MDRW2100003488.1.13",
          "form": "꽁짜로",
          "original_form": "꽁짜로",
          "speaker_id": "2",
          "time": "20210820 23:43"
        },
        {
          "id": "MDRW2100003488.1.14",
          "form": "쩔제?",
          "original_form": "쩔제?",
          "speaker_id": "2",
          "time": "20210820 23:43"
        },
        {
          "id": "MDRW2100003488.1.15",
          "form": "나는야 럭키가이",
          "original_form": "나는야 럭키가이",
          "speaker_id": "2",
          "time": "20210820 23:43"
        },
        {
          "id": "MDRW2100003488.1.16",
          "form": "심각하다.. 어쩌면 좋누..",
          "original_form": "심각하다.. 어쩌면 좋누..",
          "speaker_id": "1",
          "time": "20210820 23:43"
        },
        {
          "id": "MDRW2100003488.1.17",
          "form": "다 부질없는.. 허구이거늘,.,",
          "original_form": "다 부질없는.. 허구이거늘,.,",
          "speaker_id": "1",
          "time": "20210820 23:43"
        },
        {
          "id": "MDRW2100003488.1.18",
          "form": "허상이거늘..",
          "original_form": "허상이거늘..",
          "speaker_id": "1",
          "time": "20210820 23:43"
        },
        {
          "id": "MDRW2100003488.1.19",
          "form": "그런소리 마셈",
          "original_form": "그런소리 마셈",
          "speaker_id": "2",
          "time": "20210820 23:44"
        },
        {
          "id": "MDRW2100003488.1.20",
          "form": "게임은 인생의 추억이며",
          "original_form": "게임은 인생의 추억이며",
          "speaker_id": "2",
          "time": "20210820 23:44"
        },
        {
          "id": "MDRW2100003488.1.21",
          "form": "쯧쯧",
          "original_form": "쯧쯧",
          "speaker_id": "1",
          "time": "20210820 23:44"
        },
        {
          "id": "MDRW2100003488.1.22",
          "form": "인생의 유일한 낙이니라",
          "original_form": "인생의 유일한 낙이니라",
          "speaker_id": "2",
          "time": "20210820 23:44"
        },
        {
          "id": "MDRW2100003488.1.23",
          "form": "어쩌다 이렇게 된건지..",
          "original_form": "어쩌다 이렇게 된건지..",
          "speaker_id": "1",
          "time": "20210820 23:44"
        },
        {
          "id": "MDRW2100003488.1.24",
          "form": "안타깝도다",
          "original_form": "안타깝도다",
          "speaker_id": "1",
          "time": "20210820 23:44"
        },
        {
          "id": "MDRW2100003488.1.25",
          "form": "회사 그만두고",
          "original_form": "회사 그만두고",
          "speaker_id": "2",
          "time": "20210820 23:44"
        },
        {
          "id": "MDRW2100003488.1.26",
          "form": "1년동안 게임만해도 괜찮을듯 인정?",
          "original_form": "1년동안 게임만해도 괜찮을듯 인정?",
          "speaker_id": "2",
          "time": "20210820 23:44"
        },
        {
          "id": "MDRW2100003488.1.27",
          "form": "게임하면 돈이 나오느냐",
          "original_form": "게임하면 돈이 나오느냐",
          "speaker_id": "1",
          "time": "20210820 23:44"
        },
        {
          "id": "MDRW2100003488.1.28",
          "form": "쯧쯧",
          "original_form": "쯧쯧",
          "speaker_id": "1",
          "time": "20210820 23:44"
        },
        {
          "id": "MDRW2100003488.1.29",
          "form": "님은 쿠키런이나 얼른하셈",
          "original_form": "님은 쿠키런이나 얼른하셈",
          "speaker_id": "2",
          "time": "20210820 23:44"
        },
        {
          "id": "MDRW2100003488.1.30",
          "form": "나는 쿠키런 질린다",
          "original_form": "나는 쿠키런 질린다",
          "speaker_id": "2",
          "time": "20210820 23:44"
        },
        {
          "id": "MDRW2100003488.1.31",
          "form": "난 의미있는데에만 돈쓴다",
          "original_form": "난 의미있는데에만 돈쓴다",
          "speaker_id": "1",
          "time": "20210820 23:45"
        },
        {
          "id": "MDRW2100003488.1.32",
          "form": "우리집 너무 물건이 많다고 생각이들지않나?",
          "original_form": "우리집 너무 물건이 많다고 생각이들지않나?",
          "speaker_id": "1",
          "time": "20210821 00:05"
        },
        {
          "id": "MDRW2100003488.1.33",
          "form": "중고로 좀 팔아보는게 어떻니",
          "original_form": "중고로 좀 팔아보는게 어떻니",
          "speaker_id": "1",
          "time": "20210821 00:05"
        }
      ]
    }
