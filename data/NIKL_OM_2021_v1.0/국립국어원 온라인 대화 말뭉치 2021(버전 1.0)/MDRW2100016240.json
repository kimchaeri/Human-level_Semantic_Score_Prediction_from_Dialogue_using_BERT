      "utterance": [
        {
          "id": "MDRW2100016240.1.1",
          "form": "요즘 곰표옷이랑 곰표 팝콘 같은거 많이 보이더라구요. 신선한거 같아요.",
          "original_form": "요즘 곰표옷이랑 곰표 팝콘 같은거 많이 보이더라구요. 신선한거 같아요.",
          "speaker_id": "1",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016240.1.2",
          "form": "요즘에 그런 콜라보 많이 하는 거 같더라고요#",
          "original_form": "요즘에 그런 콜라보 많이 하는 거 같더라고요#",
          "speaker_id": "2",
          "time": "20210826 12:09"
        },
        {
          "id": "MDRW2100016240.1.3",
          "form": "말표 초콜릿도 본거 같아요",
          "original_form": "말표 초콜릿도 본거 같아요",
          "speaker_id": "1",
          "time": "20210826 12:20"
        },
        {
          "id": "MDRW2100016240.1.4",
          "form": "진로 두꺼비 덩치큰게 귀여운거 같아요",
          "original_form": "진로 두꺼비 덩치큰게 귀여운거 같아요",
          "speaker_id": "1",
          "time": "20210826 12:20"
        },
        {
          "id": "MDRW2100016240.1.5",
          "form": "ㅎㅎ그거 실제 모형 봤는데",
          "original_form": "ㅎㅎ그거 실제 모형 봤는데",
          "speaker_id": "2",
          "time": "20210826 12:57"
        },
        {
          "id": "MDRW2100016240.1.6",
          "form": "커도 귀엽더라고요~",
          "original_form": "커도 귀엽더라고요~",
          "speaker_id": "2",
          "time": "20210826 12:57"
        },
        {
          "id": "MDRW2100016240.1.7",
          "form": "실제 모형을 보셨어요? 우와 사람만 ㅎㅐ요?",
          "original_form": "실제 모형을 보셨어요? 우와 사람만 ㅎㅐ요?",
          "speaker_id": "1",
          "time": "20210826 13:07"
        },
        {
          "id": "MDRW2100016240.1.8",
          "form": "네~ 거의 사람만해요~~",
          "original_form": "네~ 거의 사람만해요~~",
          "speaker_id": "2",
          "time": "20210826 13:19"
        },
        {
          "id": "MDRW2100016240.1.9",
          "form": "타임스퀘어에 있더라고요~",
          "original_form": "타임스퀘어에 있더라고요~",
          "speaker_id": "2",
          "time": "20210826 13:19"
        },
        {
          "id": "MDRW2100016240.1.10",
          "form": "배 한 번 쓰담해보고 싶어요 ㅎ",
          "original_form": "배 한 번 쓰담해보고 싶어요 ㅎ",
          "speaker_id": "1",
          "time": "20210826 13:21"
        },
        {
          "id": "MDRW2100016240.1.11",
          "form": "막 말랑말랑한 재질은 아니었어서 쓰다듬었을 때 별 느낌은 없었는데",
          "original_form": "막 말랑말랑한 재질은 아니었어서 쓰다듬었을 때 별 느낌은 없었는데",
          "speaker_id": "2",
          "time": "20210826 13:23"
        },
        {
          "id": "MDRW2100016240.1.12",
          "form": "그래도 귀여웠어요~",
          "original_form": "그래도 귀여웠어요~",
          "speaker_id": "2",
          "time": "20210826 13:23"
        },
        {
          "id": "MDRW2100016240.1.13",
          "form": "플라스틱 이겠죠 ㅋ",
          "original_form": "플라스틱 이겠죠 ㅋ",
          "speaker_id": "1",
          "time": "20210826 13:26"
        },
        {
          "id": "MDRW2100016240.1.14",
          "form": "ㅎㅎ 잘만들어놨더라고요~~",
          "original_form": "ㅎㅎ 잘만들어놨더라고요~~",
          "speaker_id": "2",
          "time": "20210826 13:27"
        },
        {
          "id": "MDRW2100016240.1.15",
          "form": "애들 옷 휠라도 샌드위치랑 콜라보 한거 같더라구요ㅡ",
          "original_form": "애들 옷 휠라도 샌드위치랑 콜라보 한거 같더라구요ㅡ",
          "speaker_id": "1",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100016240.1.16",
          "form": "샌드위치요??ㅎㅎ",
          "original_form": "샌드위치요??ㅎㅎ",
