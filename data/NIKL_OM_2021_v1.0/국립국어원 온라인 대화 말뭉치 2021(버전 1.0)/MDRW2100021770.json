      "utterance": [
        {
          "id": "MDRW2100021770.1.1",
          "form": "해산물 종류는 다 좋아해요",
          "original_form": "해산물 종류는 다 좋아해요",
          "speaker_id": "1",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021770.1.2",
          "form": "회도 좋고 멍게 해삼 개불 다 잘 먹어요",
          "original_form": "회도 좋고 멍게 해삼 개불 다 잘 먹어요",
          "speaker_id": "1",
          "time": "20210828 22:19"
        },
        {
          "id": "MDRW2100021770.1.3",
          "form": "name2님은 회 드시나요",
          "original_form": "&name2&님은 회 드시나요",
          "speaker_id": "1",
          "time": "20210828 22:19"
        },
        {
          "id": "MDRW2100021770.1.4",
          "form": "못먹어요",
          "original_form": "못먹어요",
          "speaker_id": "2",
          "time": "20210828 22:21"
        },
        {
          "id": "MDRW2100021770.1.5",
          "form": "없어서...",
          "original_form": "없어서...",
          "speaker_id": "2",
          "time": "20210828 22:21"
        },
        {
          "id": "MDRW2100021770.1.6",
          "form": "너무 좋아합니다ㅋㅋㅋㅋ",
          "original_form": "너무 좋아합니다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:21"
        },
        {
          "id": "MDRW2100021770.1.7",
          "form": "ㅎㅎ 가을 전어는 드셨나요",
          "original_form": "ㅎㅎ 가을 전어는 드셨나요",
          "speaker_id": "1",
          "time": "20210828 22:21"
        },
        {
          "id": "MDRW2100021770.1.8",
          "form": "육고기보다 해산물을 좋아해요",
          "original_form": "육고기보다 해산물을 좋아해요",
          "speaker_id": "2",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021770.1.9",
          "form": "전어는 아직요",
          "original_form": "전어는 아직요",
          "speaker_id": "2",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021770.1.10",
          "form": "전어 너무 맛있죠",
          "original_form": "전어 너무 맛있죠",
          "speaker_id": "2",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021770.1.11",
          "form": "벌써 물이 완전 올랐어요",
          "original_form": "벌써 물이 완전 올랐어요",
          "speaker_id": "1",
          "time": "20210828 22:25"
        },
        {
          "id": "MDRW2100021770.1.12",
          "form": "시간이 더 가기 전에 빨리 먹어야겠어요",
          "original_form": "시간이 더 가기 전에 빨리 먹어야겠어요",
          "speaker_id": "2",
          "time": "20210828 22:27"
        },
        {
          "id": "MDRW2100021770.1.13",
          "form": "전어회도 좋아하세요?",
          "original_form": "전어회도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 22:27"
        },
        {
          "id": "MDRW2100021770.1.14",
          "form": "네네 세꼬시류를 특히 좋아해요",
          "original_form": "네네 세꼬시류를 특히 좋아해요",
          "speaker_id": "1",
          "time": "20210828 22:31"
        },
        {
          "id": "MDRW2100021770.1.15",
          "form": "뼈 더 억세지기 전에 드세요!",
          "original_form": "뼈 더 억세지기 전에 드세요!",
          "speaker_id": "1",
          "time": "20210828 22:32"
        },
        {
          "id": "MDRW2100021770.1.16",
          "form": "그래야죠",
          "original_form": "그래야죠",
          "speaker_id": "2",
          "time": "20210828 22:32"
        },
        {
          "id": "MDRW2100021770.1.17",
          "form": "무슨 회를 가장 좋아하세요?",
          "original_form": "무슨 회를 가장 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 22:33"
        },
        {
          "id": "MDRW2100021770.1.18",
          "form": "전 고등어요",
          "original_form": "전 고등어요",
          "speaker_id": "2",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100021770.1.19",
          "form": "근데 사실 맨날 바뀌네요ㅋㅋㅋ",
          "original_form": "근데 사실 맨날 바뀌네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100021770.1.20",
          "form": "헉 고등어회 잘하기 어려운데",
          "original_form": "헉 고등어회 잘하기 어려운데",
          "speaker_id": "1",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100021770.1.21",
          "form": "고등어회 추천맛집 있나요",
          "original_form": "고등어회 추천맛집 있나요",
          "speaker_id": "1",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100021770.1.22",
          "form": "음 잘하는 집은 잘 모르겠고 아무래도 고등어회가 바닷가에서만 먹을 수 있잖아요",
          "original_form": "음 잘하는 집은 잘 모르겠고 아무래도 고등어회가 바닷가에서만 먹을 수 있잖아요",
          "speaker_id": "2",
          "time": "20210828 22:38"
        },
        {
          "id": "MDRW2100021770.1.23",
          "form": "낚시 따라가서 먹는 편이에요...ㅎㅎ",
          "original_form": "낚시 따라가서 먹는 편이에요...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 22:38"
        },
        {
          "id": "MDRW2100021770.1.24",
          "form": "헉 정말 본격적이네요ㄷㄷ",
          "original_form": "헉 정말 본격적이네요ㄷㄷ",
          "speaker_id": "1",
          "time": "20210828 22:38"
        }
      ]
