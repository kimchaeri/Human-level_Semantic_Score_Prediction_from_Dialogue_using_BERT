      "utterance": [
        {
          "id": "MDRW2100026002.1.1",
          "form": "오 봉사활동 하시나요?",
          "original_form": "오 봉사활동 하시나요?",
          "speaker_id": "1",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100026002.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210901 20:00"
        },
        {
          "id": "MDRW2100026002.1.3",
          "form": "혹시 봉사활동하세요?",
          "original_form": "혹시 봉사활동하세요?",
          "speaker_id": "2",
          "time": "20210901 20:00"
        },
        {
          "id": "MDRW2100026002.1.4",
          "form": "아뇨 주제바꿔야겠네여 ㅎㅎ",
          "original_form": "아뇨 주제바꿔야겠네여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 20:02"
        },
        {
          "id": "MDRW2100026002.1.5",
          "form": "ㅋㅋ아니에요",
          "original_form": "ㅋㅋ아니에요",
          "speaker_id": "2",
          "time": "20210901 20:02"
        },
        {
          "id": "MDRW2100026002.1.6",
          "form": "그냥 아는 봉사활동 얘기를 해요",
          "original_form": "그냥 아는 봉사활동 얘기를 해요",
          "speaker_id": "2",
          "time": "20210901 20:03"
        },
        {
          "id": "MDRW2100026002.1.7",
          "form": "봉사단체들 주변에 많으니ㅋㅋㅋ",
          "original_form": "봉사단체들 주변에 많으니ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:03"
        },
        {
          "id": "MDRW2100026002.1.8",
          "form": "아 이전에 활동한적 있어요",
          "original_form": "아 이전에 활동한적 있어요",
          "speaker_id": "1",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100026002.1.9",
          "form": "독거노인분들 반찬 전달해드리는 단체에서",
          "original_form": "독거노인분들 반찬 전달해드리는 단체에서",
          "speaker_id": "1",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100026002.1.10",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100026002.1.11",
          "form": "뜻깊은 일 하셨네요",
          "original_form": "뜻깊은 일 하셨네요",
          "speaker_id": "2",
          "time": "20210901 20:04"
        },
        {
          "id": "MDRW2100026002.1.12",
          "form": "근데 반찬보다 라면주면 더 좋아하신다는",
          "original_form": "근데 반찬보다 라면주면 더 좋아하신다는",
          "speaker_id": "1",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100026002.1.13",
          "form": "^^",
          "original_form": "^^",
          "speaker_id": "1",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100026002.1.14",
          "form": "아닛",
          "original_form": "아닛",
          "speaker_id": "2",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100026002.1.15",
          "form": "노숙인분들도 소주를 더 좋아한다더라구요",
          "original_form": "노숙인분들도 소주를 더 좋아한다더라구요",
          "speaker_id": "2",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100026002.1.16",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "2",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100026002.1.17",
          "form": "저도 옛날에 학교다닐때",
          "original_form": "저도 옛날에 학교다닐때",
          "speaker_id": "2",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100026002.1.18",
          "form": "비슷한 활동한적있었어요",
          "original_form": "비슷한 활동한적있었어요",
          "speaker_id": "2",
          "time": "20210901 20:06"
        },
        {
          "id": "MDRW2100026002.1.19",
          "form": "집집마다 김치랑 밑반찬 전달해드리는",
          "original_form": "집집마다 김치랑 밑반찬 전달해드리는",
          "speaker_id": "2",
          "time": "20210901 20:06"
        },
        {
          "id": "MDRW2100026002.1.20",
          "form": "오 그러셨군요",
          "original_form": "오 그러셨군요",
          "speaker_id": "1",
          "time": "20210901 20:07"
        },
        {
          "id": "MDRW2100026002.1.21",
          "form": "힘들지만 나름 보람있었어요",
          "original_form": "힘들지만 나름 보람있었어요",
          "speaker_id": "2",
          "time": "20210901 20:08"
        },
        {
          "id": "MDRW2100026002.1.22",
          "form": "할머니들이 엄청 반겨주셨거든요",
          "original_form": "할머니들이 엄청 반겨주셨거든요",
          "speaker_id": "2",
          "time": "20210901 20:08"
        },
        {
          "id": "MDRW2100026002.1.23",
          "form": "봉사하며 살아야하는데",
          "original_form": "봉사하며 살아야하는데",
          "speaker_id": "2",
          "time": "20210901 20:08"
        },
        {
          "id": "MDRW2100026002.1.24",
          "form": "아이 하루하루 먹고 살기 바쁨디",
          "original_form": "아이 하루하루 먹고 살기 바쁨디",
          "speaker_id": "1",
          "time": "20210901 20:10"
        },
        {
          "id": "MDRW2100026002.1.25",
          "form": "가능할 때 봉사하는거죠",
          "original_form": "가능할 때 봉사하는거죠",
          "speaker_id": "1",
          "time": "20210901 20:10"
        },
        {
          "id": "MDRW2100026002.1.26",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "2",
          "time": "20210901 20:11"
        },
        {
          "id": "MDRW2100026002.1.27",
          "form": "아주 소액이지만 기부라도 꾸준히",
          "original_form": "아주 소액이지만 기부라도 꾸준히",
          "speaker_id": "2",
          "time": "20210901 20:11"
        },
        {
          "id": "MDRW2100026002.1.28",
          "form": "해보려구요",
          "original_form": "해보려구요",
          "speaker_id": "2",
          "time": "20210901 20:11"
        },
        {
          "id": "MDRW2100026002.1.29",
