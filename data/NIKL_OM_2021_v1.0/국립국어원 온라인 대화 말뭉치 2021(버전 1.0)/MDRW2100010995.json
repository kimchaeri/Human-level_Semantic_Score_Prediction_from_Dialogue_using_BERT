      "utterance": [
        {
          "id": "MDRW2100010995.1.1",
          "form": "나는 가을이 좋아",
          "original_form": "나는 가을이 좋아",
          "speaker_id": "2",
          "time": "20210823 23:35"
        },
        {
          "id": "MDRW2100010995.1.2",
          "form": "봄에는 황사가 있어",
          "original_form": "봄에는 황사가 있어",
          "speaker_id": "2",
          "time": "20210823 23:35"
        },
        {
          "id": "MDRW2100010995.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210823 23:35"
        },
        {
          "id": "MDRW2100010995.1.4",
          "form": "나도 가을이 좋아",
          "original_form": "나도 가을이 좋아",
          "speaker_id": "1",
          "time": "20210823 23:35"
        },
        {
          "id": "MDRW2100010995.1.5",
          "form": "왜냐면 내 생일이 가을에있오",
          "original_form": "왜냐면 내 생일이 가을에있오",
          "speaker_id": "1",
          "time": "20210823 23:35"
        },
        {
          "id": "MDRW2100010995.1.6",
          "form": "근데 봄가을은 간절기라 옷차림이 애매해",
          "original_form": "근데 봄가을은 간절기라 옷차림이 애매해",
          "speaker_id": "2",
          "time": "20210823 23:35"
        },
        {
          "id": "MDRW2100010995.1.7",
          "form": "어머 나도 그래.",
          "original_form": "어머 나도 그래.",
          "speaker_id": "2",
          "time": "20210823 23:35"
        },
        {
          "id": "MDRW2100010995.1.8",
          "form": "맞아 근데 가을옷이 예쁘긴해",
          "original_form": "맞아 근데 가을옷이 예쁘긴해",
          "speaker_id": "1",
          "time": "20210823 23:35"
        },
        {
          "id": "MDRW2100010995.1.9",
          "form": "짧게밖에 못입지만",
          "original_form": "짧게밖에 못입지만",
          "speaker_id": "1",
          "time": "20210823 23:35"
        },
        {
          "id": "MDRW2100010995.1.10",
          "form": "나는 추위를 좀 타거든. 코트를 입고싶은데 몇번 못입어",
          "original_form": "나는 추위를 좀 타거든. 코트를 입고싶은데 몇번 못입어",
          "speaker_id": "2",
          "time": "20210823 23:35"
        },
        {
          "id": "MDRW2100010995.1.11",
          "form": "코트는 얇고 예뻐",
          "original_form": "코트는 얇고 예뻐",
          "speaker_id": "1",
          "time": "20210823 23:35"
        },
        {
          "id": "MDRW2100010995.1.12",
          "form": "가을옷 장만은 다 한거야?",
          "original_form": "가을옷 장만은 다 한거야?",
          "speaker_id": "2",
          "time": "20210823 23:35"
        },
        {
          "id": "MDRW2100010995.1.13",
          "form": "두꺼우면 무거워ㅠ",
          "original_form": "두꺼우면 무거워ㅠ",
          "speaker_id": "2",
          "time": "20210823 23:36"
        },
        {
          "id": "MDRW2100010995.1.14",
          "form": "아니 아마 작년옷을 재탕할 것 같아",
          "original_form": "아니 아마 작년옷을 재탕할 것 같아",
          "speaker_id": "1",
          "time": "20210823 23:36"
        },
        {
          "id": "MDRW2100010995.1.15",
          "form": "나도. 돈이 없어.",
          "original_form": "나도. 돈이 없어.",
          "speaker_id": "2",
          "time": "20210823 23:36"
        },
        {
          "id": "MDRW2100010995.1.16",
          "form": "돈이 별로 없거든",
          "original_form": "돈이 별로 없거든",
          "speaker_id": "1",
          "time": "20210823 23:36"
        },
        {
          "id": "MDRW2100010995.1.17",
          "form": "봄옷도 재탕하고",
          "original_form": "봄옷도 재탕하고",
          "speaker_id": "1",
          "time": "20210823 23:36"
        },
        {
          "id": "MDRW2100010995.1.18",
          "form": "그래도 봄가을은 같은 옷을 입을 슈 있어 다행이야",
          "original_form": "그래도 봄가을은 같은 옷을 입을 슈 있어 다행이야",
          "speaker_id": "1",
          "time": "20210823 23:36"
        },
        {
          "id": "MDRW2100010995.1.19",
          "form": "똗같구나. 그래 두해정도는 재탕해도 돼",
          "original_form": "똗같구나. 그래 두해정도는 재탕해도 돼",
          "speaker_id": "2",
          "time": "20210823 23:36"
        },
        {
          "id": "MDRW2100010995.1.20",
          "form": "아껴야 여행가지.",
          "original_form": "아껴야 여행가지.",
          "speaker_id": "2",
          "time": "20210823 23:36"
        },
        {
          "id": "MDRW2100010995.1.21",
          "form": "점점 쌀쌀해지니까 새로운 트렌치코트가 사고싶긴해",
          "original_form": "점점 쌀쌀해지니까 새로운 트렌치코트가 사고싶긴해",
          "speaker_id": "1",
          "time": "20210823 23:36"
        },
        {
          "id": "MDRW2100010995.1.22",
          "form": "자꾸 나의 소비욕을 자극하는구나.",
          "original_form": "자꾸 나의 소비욕을 자극하는구나.",
          "speaker_id": "2",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100010995.1.23",
          "form": "오늘 비오는거 봤오?",
          "original_form": "오늘 비오는거 봤오?",
          "speaker_id": "1",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100010995.1.24",
          "form": "엄청 살벌하더라",
          "original_form": "엄청 살벌하더라",
          "speaker_id": "1",
          "time": "20210824 23:54"
        }
      ]
    }
