      "utterance": [
        {
          "id": "MDRW2100037016.1.1",
          "form": "스페인은 뭘먹어도 맛있더라구용 ㅠ",
          "original_form": "스페인은 뭘먹어도 맛있더라구용 ㅠ",
          "speaker_id": "2",
          "time": "20210831 17:10"
        },
        {
          "id": "MDRW2100037016.1.2",
          "form": "스페인..!",
          "original_form": "스페인..!",
          "speaker_id": "1",
          "time": "20210831 17:10"
        },
        {
          "id": "MDRW2100037016.1.3",
          "form": "크으 미슐랭의나라",
          "original_form": "크으 미슐랭의나라",
          "speaker_id": "1",
          "time": "20210831 17:10"
        },
        {
          "id": "MDRW2100037016.1.4",
          "form": "한국사람  입에 잘맞는듯",
          "original_form": "한국사람  입에 잘맞는듯",
          "speaker_id": "2",
          "time": "20210831 17:10"
        },
        {
          "id": "MDRW2100037016.1.5",
          "form": "뭐가젤맛있어요?",
          "original_form": "뭐가젤맛있어요?",
          "speaker_id": "1",
          "time": "20210831 17:10"
        },
        {
          "id": "MDRW2100037016.1.6",
          "form": "그냥 스몰디쉬들이 다 맛있던뎅..",
          "original_form": "그냥 스몰디쉬들이 다 맛있던뎅..",
          "speaker_id": "2",
          "time": "20210831 17:10"
        },
        {
          "id": "MDRW2100037016.1.7",
          "form": "가장기억에남는건",
          "original_form": "가장기억에남는건",
          "speaker_id": "2",
          "time": "20210831 17:10"
        },
        {
          "id": "MDRW2100037016.1.8",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210831 17:11"
        },
        {
          "id": "MDRW2100037016.1.9",
          "form": "이베리코 돼지랑",
          "original_form": "이베리코 돼지랑",
          "speaker_id": "2",
          "time": "20210831 17:11"
        },
        {
          "id": "MDRW2100037016.1.10",
          "form": "대구 크로켓 ?",
          "original_form": "대구 크로켓 ?",
          "speaker_id": "2",
          "time": "20210831 17:11"
        },
        {
          "id": "MDRW2100037016.1.11",
          "form": "오오 한국에서랑 맛이 좀 틀리던가여?",
          "original_form": "오오 한국에서랑 맛이 좀 틀리던가여?",
          "speaker_id": "1",
          "time": "20210831 17:11"
        },
        {
          "id": "MDRW2100037016.1.12",
          "form": "움...돼지 진짜 소고기같았어요 ㅋㅋ",
          "original_form": "움...돼지 진짜 소고기같았어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:12"
        },
        {
          "id": "MDRW2100037016.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ오오 스페인가면 맛집투어쫙하고싶어요ㅜ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ오오 스페인가면 맛집투어쫙하고싶어요ㅜ",
          "speaker_id": "1",
          "time": "20210831 17:13"
        },
        {
          "id": "MDRW2100037016.1.14",
          "form": "여행하면서 소매치기 위험은없었나요",
          "original_form": "여행하면서 소매치기 위험은없었나요",
          "speaker_id": "1",
          "time": "20210831 17:13"
        },
        {
          "id": "MDRW2100037016.1.15",
          "form": "음..전 그냥 잘 주시하고다녀서",
          "original_form": "음..전 그냥 잘 주시하고다녀서",
          "speaker_id": "2",
          "time": "20210831 17:17"
        },
        {
          "id": "MDRW2100037016.1.16",
          "form": "그런적은 없었어용..운좋게도",
          "original_form": "그런적은 없었어용..운좋게도",
          "speaker_id": "2",
          "time": "20210831 17:17"
        },
        {
          "id": "MDRW2100037016.1.17",
          "form": "유럽쪽에 소매치기많다고들어서",
          "original_form": "유럽쪽에 소매치기많다고들어서",
          "speaker_id": "1",
          "time": "20210831 17:17"
        },
        {
          "id": "MDRW2100037016.1.18",
          "form": "어떤가했어욯ㅎ 맞아요 좀 주시하고다니면 괜찮은것같야요",
          "original_form": "어떤가했어욯ㅎ 맞아요 좀 주시하고다니면 괜찮은것같야요",
          "speaker_id": "1",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100037016.1.19",
          "form": "넹.... 경계심을 늦추지않고 특히불안한데서 가방 앞으로 꽉 잡고다니곸ㅋㅋㅋㅋ",
          "original_form": "넹.... 경계심을 늦추지않고 특히불안한데서 가방 앞으로 꽉 잡고다니곸ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100037016.1.20",
          "form": "ㅋㅋㅋㅋ맞아요 가방꼭손에쥐고 앞으로",
          "original_form": "ㅋㅋㅋㅋ맞아요 가방꼭손에쥐고 앞으로",
          "speaker_id": "1",
          "time": "20210831 17:20"
        },
        {
          "id": "MDRW2100037016.1.21",
          "form": "그리고 전 좀 추레하게여행하다보니..저도 소매치기는 한번도안당해봤어요",
          "original_form": "그리고 전 좀 추레하게여행하다보니..저도 소매치기는 한번도안당해봤어요",
          "speaker_id": "1",
          "time": "20210831 17:20"
        },
        {
          "id": "MDRW2100037016.1.22",
          "form": "아 ㅋㅋㅋㅋ 타겟에서 제거",
          "original_form": "아 ㅋㅋㅋㅋ 타겟에서 제거",
          "speaker_id": "2",
          "time": "20210831 17:22"
        }
      ]
