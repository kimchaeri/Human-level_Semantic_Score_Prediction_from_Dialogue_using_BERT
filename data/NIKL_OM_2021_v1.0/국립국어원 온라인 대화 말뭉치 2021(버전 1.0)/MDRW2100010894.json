      "utterance": [
        {
          "id": "MDRW2100010894.1.1",
          "form": "스포츠보는 거 좋아하세요?",
          "original_form": "스포츠보는 거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 22:41"
        },
        {
          "id": "MDRW2100010894.1.2",
          "form": "앗 챙겨보지는 않지만 최근에 손흥민 선수 원더골 영상 돌렵봤어욬ㅋㅋㅋㅋ",
          "original_form": "앗 챙겨보지는 않지만 최근에 손흥민 선수 원더골 영상 돌렵봤어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:42"
        },
        {
          "id": "MDRW2100010894.1.3",
          "form": "혹시 스포츠 즐겨보시나요? 어떤거 좋아하세요?",
          "original_form": "혹시 스포츠 즐겨보시나요? 어떤거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 22:42"
        },
        {
          "id": "MDRW2100010894.1.4",
          "form": "오 돌려볼 정도면 축구 좋아하시나봐요?",
          "original_form": "오 돌려볼 정도면 축구 좋아하시나봐요?",
          "speaker_id": "1",
          "time": "20210823 22:43"
        },
        {
          "id": "MDRW2100010894.1.5",
          "form": "전 딱히 챙겨보는 거 없었는데",
          "original_form": "전 딱히 챙겨보는 거 없었는데",
          "speaker_id": "1",
          "time": "20210823 22:43"
        },
        {
          "id": "MDRW2100010894.1.6",
          "form": "올림픽 이후에 넘 재밌어서 여자배구 봐요",
          "original_form": "올림픽 이후에 넘 재밌어서 여자배구 봐요",
          "speaker_id": "1",
          "time": "20210823 22:44"
        },
        {
          "id": "MDRW2100010894.1.7",
          "form": "아녀 원더골이 너무 멋있어서",
          "original_form": "아녀 원더골이 너무 멋있어서",
          "speaker_id": "2",
          "time": "20210823 22:45"
        },
        {
          "id": "MDRW2100010894.1.8",
          "form": "희열이..",
          "original_form": "희열이..",
          "speaker_id": "2",
          "time": "20210823 22:45"
        },
        {
          "id": "MDRW2100010894.1.9",
          "form": "헉 여배 너무 잼죠",
          "original_form": "헉 여배 너무 잼죠",
          "speaker_id": "2",
          "time": "20210823 22:45"
        },
        {
          "id": "MDRW2100010894.1.10",
          "form": "역전승 그것도 돌려보ㅓ요",
          "original_form": "역전승 그것도 돌려보ㅓ요",
          "speaker_id": "2",
          "time": "20210823 22:45"
        },
        {
          "id": "MDRW2100010894.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ짜릿한 건 돌려보는 편이죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ짜릿한 건 돌려보는 편이죠",
          "speaker_id": "2",
          "time": "20210823 22:46"
        },
        {
          "id": "MDRW2100010894.1.12",
          "form": "아 뭔지 알 것 같아여",
          "original_form": "아 뭔지 알 것 같아여",
          "speaker_id": "1",
          "time": "20210823 22:46"
        },
        {
          "id": "MDRW2100010894.1.13",
          "form": "짜릿한 느낌!!!",
          "original_form": "짜릿한 느낌!!!",
          "speaker_id": "1",
          "time": "20210823 22:46"
        },
        {
          "id": "MDRW2100010894.1.14",
          "form": "일상생활에선 느낄 수 없어서 그럴까요 ^_ㅠ",
          "original_form": "일상생활에선 느낄 수 없어서 그럴까요 ^_ㅠ",
          "speaker_id": "1",
          "time": "20210823 22:46"
        },
        {
          "id": "MDRW2100010894.1.15",
          "form": "앗...그게 맞는 것 같아욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앗...그게 맞는 것 같아욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100010894.1.16",
          "form": "역전할 일이 없다보니..",
          "original_form": "역전할 일이 없다보니..",
          "speaker_id": "2",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100010894.1.17",
          "form": "직관 해보신적은 있으세요??",
          "original_form": "직관 해보신적은 있으세요??",
          "speaker_id": "2",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100010894.1.18",
          "form": "야구는 직관해봐도 배구는 없어요ㅠ",
          "original_form": "야구는 직관해봐도 배구는 없어요ㅠ",
          "speaker_id": "1",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100010894.1.19",
          "form": "헉 야구!!",
          "original_form": "헉 야구!!",
          "speaker_id": "2",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100010894.1.20",
          "form": "오늘부터 여자배구 리그 시작했는데 이놈의 코로나!!!",
          "original_form": "오늘부터 여자배구 리그 시작했는데 이놈의 코로나!!!",
          "speaker_id": "1",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100010894.1.21",
          "form": "어떤 팀 응원하세요?",
          "original_form": "어떤 팀 응원하세요?",
          "speaker_id": "2",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100010894.1.22",
          "form": "저는 아빠따라 기아 응원하러 간 적 있어요",
          "original_form": "저는 아빠따라 기아 응원하러 간 적 있어요",
          "speaker_id": "2",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100010894.1.23",
          "form": "배구는 기업은행 응원하구 야구는 롯데 응원하러 갔었어요 ㅋㅋㅋ",
          "original_form": "배구는 기업은행 응원하구 야구는 롯데 응원하러 갔었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100010894.1.24",
          "form": "오 아버님이 기아팬이신가봐요",
          "original_form": "오 아버님이 기아팬이신가봐요",
          "speaker_id": "1",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100010894.1.25",
          "form": "네네 완전",
          "original_form": "네네 완전",
          "speaker_id": "2",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100010894.1.26",
          "form": "코로나때문에 못가시지만",
          "original_form": "코로나때문에 못가시지만",
          "speaker_id": "2",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100010894.1.27",
          "form": "휴가내고 야구보러가시더라고요",
          "original_form": "휴가내고 야구보러가시더라고요",
          "speaker_id": "2",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100010894.1.28",
          "form": "와 진짜 좋아하시나봐요",
          "original_form": "와 진짜 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210823 22:52"
        },
        {
          "id": "MDRW2100010894.1.29",
          "form": "야구팬들은 화가 많으시던데...아버님도 그러신가요...?",
          "original_form": "야구팬들은 화가 많으시던데...아버님도 그러신가요...?",
          "speaker_id": "1",
          "time": "20210823 22:52"
        },
        {
          "id": "MDRW2100010894.1.30",
          "form": "엇 네",
          "original_form": "엇 네",
          "speaker_id": "2",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100010894.1.31",
          "form": "야구볼때만 분출하시는 것 같아요",
          "original_form": "야구볼때만 분출하시는 것 같아요",
          "speaker_id": "2",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100010894.1.32",
