      "utterance": [
        {
          "id": "MDRW2100043018.1.1",
          "form": "비엄청와",
          "original_form": "비엄청와",
          "speaker_id": "1",
          "time": "20210907 13:10"
        },
        {
          "id": "MDRW2100043018.1.2",
          "form": "거기도 와? 여기도 엄청온다",
          "original_form": "거기도 와? 여기도 엄청온다",
          "speaker_id": "2",
          "time": "20210907 13:10"
        },
        {
          "id": "MDRW2100043018.1.3",
          "form": "하루종일 오게생겼다",
          "original_form": "하루종일 오게생겼다",
          "speaker_id": "1",
          "time": "20210907 13:10"
        },
        {
          "id": "MDRW2100043018.1.4",
          "form": "그니까 오늘 장보려고했는데...",
          "original_form": "그니까 오늘 장보려고했는데...",
          "speaker_id": "2",
          "time": "20210907 13:10"
        },
        {
          "id": "MDRW2100043018.1.5",
          "form": "까비...",
          "original_form": "까비...",
          "speaker_id": "2",
          "time": "20210907 13:10"
        },
        {
          "id": "MDRW2100043018.1.6",
          "form": "비올때나가면 빨래가 2배",
          "original_form": "비올때나가면 빨래가 2배",
          "speaker_id": "1",
          "time": "20210907 13:11"
        },
        {
          "id": "MDRW2100043018.1.7",
          "form": "빨래도 2배 건조시간도 2배",
          "original_form": "빨래도 2배 건조시간도 2배",
          "speaker_id": "2",
          "time": "20210907 13:11"
        },
        {
          "id": "MDRW2100043018.1.8",
          "form": "맞아 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:11"
        },
        {
          "id": "MDRW2100043018.1.9",
          "form": "거기다 냄새남...",
          "original_form": "거기다 냄새남...",
          "speaker_id": "1",
          "time": "20210907 13:11"
        },
        {
          "id": "MDRW2100043018.1.10",
          "form": "인 진짜 너무 쿰쿰한 냄새나",
          "original_form": "인 진짜 너무 쿰쿰한 냄새나",
          "speaker_id": "2",
          "time": "20210907 13:12"
        },
        {
          "id": "MDRW2100043018.1.11",
          "form": "실내건조 세제 써봤어?",
          "original_form": "실내건조 세제 써봤어?",
          "speaker_id": "1",
          "time": "20210907 13:12"
        },
        {
          "id": "MDRW2100043018.1.12",
          "form": "헐 그런것도 있어?",
          "original_form": "헐 그런것도 있어?",
          "speaker_id": "2",
          "time": "20210907 13:12"
        },
        {
          "id": "MDRW2100043018.1.13",
          "form": "다우니꺼 써봤는데 좋아",
          "original_form": "다우니꺼 써봤는데 좋아",
          "speaker_id": "1",
          "time": "20210907 13:12"
        },
        {
          "id": "MDRW2100043018.1.14",
          "form": "대박 내년여름에 사봐야겠다",
          "original_form": "대박 내년여름에 사봐야겠다",
          "speaker_id": "2",
          "time": "20210907 13:13"
        },
        {
          "id": "MDRW2100043018.1.15",
          "form": "하긴 겨울애는 쓸일 잘 없겠다",
          "original_form": "하긴 겨울애는 쓸일 잘 없겠다",
          "speaker_id": "1",
