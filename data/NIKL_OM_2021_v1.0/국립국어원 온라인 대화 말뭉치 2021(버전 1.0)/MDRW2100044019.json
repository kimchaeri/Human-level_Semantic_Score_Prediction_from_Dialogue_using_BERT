      "utterance": [
        {
          "id": "MDRW2100044019.1.1",
          "form": "ㅋㅋㅋ어른의 사치래",
          "original_form": "ㅋㅋㅋ어른의 사치래",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044019.1.2",
          "form": "요즘 냉동 칵테일새우같은거 잘 나오던데",
          "original_form": "요즘 냉동 칵테일새우같은거 잘 나오던데",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044019.1.3",
          "form": "응 근데 이상하게 잘안사져",
          "original_form": "응 근데 이상하게 잘안사져",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044019.1.4",
          "form": "시켜두면 두루두루 활용해서 먹기 좋대",
          "original_form": "시켜두면 두루두루 활용해서 먹기 좋대",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044019.1.5",
          "form": "나도 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "나도 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044019.1.6",
          "form": "말은 이렇게 하는데 사기 귀찮아",
          "original_form": "말은 이렇게 하는데 사기 귀찮아",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044019.1.7",
          "form": "맞아 왜지 어째서지",
          "original_form": "맞아 왜지 어째서지",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044019.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그니까말여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그니까말여",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044019.1.9",
          "form": "그리고 냉동실에 자리가 없어",
          "original_form": "그리고 냉동실에 자리가 없어",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044019.1.10",
          "form": "볶음밥에 새우 왕창 넣는게 꿈이야",
          "original_form": "볶음밥에 새우 왕창 넣는게 꿈이야",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044019.1.11",
          "form": "냉동실은 이상하게 꽉차",
          "original_form": "냉동실은 이상하게 꽉차",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044019.1.12",
          "form": "맛나겄다...",
          "original_form": "맛나겄다...",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044019.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아 뭐가 맨날 많이 차있어 냉동실은",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아 뭐가 맨날 많이 차있어 냉동실은",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044019.1.14",
          "form": "식재료 다 먹지도 않는데 뭐가 많음",
          "original_form": "식재료 다 먹지도 않는데 뭐가 많음",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044019.1.15",
          "form": "멸치가 제일많아",
          "original_form": "멸치가 제일많아",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100044019.1.16",
          "form": "ㅋㅋㅋㅋㅋ멸치랑",
          "original_form": "ㅋㅋㅋㅋㅋ멸치랑",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044019.1.17",
          "form": "이상한 시래기같은 풀때기가",
          "original_form": "이상한 시래기같은 풀때기가",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044019.1.18",
          "form": "꽝꽝 얼어서 한가득 들어있어",
          "original_form": "꽝꽝 얼어서 한가득 들어있어",
          "speaker_id": "2",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044019.1.19",
          "form": "맞아 꼭 생각이랑",
          "original_form": "맞아 꼭 생각이랑",
          "speaker_id": "1",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044019.1.20",
          "form": "생강",
          "original_form": "생강",
          "speaker_id": "1",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044019.1.21",
          "form": "ㅇㅈㅇㅈ",
          "original_form": "ㅇㅈㅇㅈ",
          "speaker_id": "2",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044019.1.22",
          "form": "먹다남은 무언가가",
          "original_form": "먹다남은 무언가가",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044019.1.23",
          "form": "ㅌㅋㅋㅋㅋ먹다 남은 무언가래ㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋ먹다 남은 무언가래ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044019.1.24",
          "form": "시래기하니까",
          "original_form": "시래기하니까",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044019.1.25",
          "form": "시래기국 먹고싶다",
          "original_form": "시래기국 먹고싶다",
          "speaker_id": "1",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044019.1.26",
          "form": "너네집도",
          "original_form": "너네집도",
          "speaker_id": "2",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044019.1.27",
          "form": "시래기국 배춧국 아욱국 이런 국 다",
          "original_form": "시래기국 배춧국 아욱국 이런 국 다",
          "speaker_id": "2",
          "time": "20210907 21:22"
        },
        {
          "id": "MDRW2100044019.1.28",
          "form": "된장 넣고 끓이니?",
          "original_form": "된장 넣고 끓이니?",
