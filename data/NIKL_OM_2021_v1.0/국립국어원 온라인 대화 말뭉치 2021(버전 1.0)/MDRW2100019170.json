        {
          "id": "MDRW2100019170.1.28",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210827 18:32"
        },
        {
          "id": "MDRW2100019170.1.29",
          "form": "갤러리만 정리해도 훨 낫긴 해",
          "original_form": "갤러리만 정리해도 훨 낫긴 해",
          "speaker_id": "1",
          "time": "20210827 18:32"
        },
        {
          "id": "MDRW2100019170.1.30",
          "form": "일단 용량 정리하고 게임 설치한다음에",
          "original_form": "일단 용량 정리하고 게임 설치한다음에",
