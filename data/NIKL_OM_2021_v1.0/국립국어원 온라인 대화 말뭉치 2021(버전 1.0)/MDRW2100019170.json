      "utterance": [
        {
          "id": "MDRW2100019170.1.1",
          "form": "게임 하다가 모르는거 있으면 편하게 물어봐",
          "original_form": "게임 하다가 모르는거 있으면 편하게 물어봐",
          "speaker_id": "1",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100019170.1.2",
          "form": "참고로 용량 10기가야...",
          "original_form": "참고로 용량 10기가야...",
          "speaker_id": "1",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100019170.1.3",
          "form": "다운받는 것만 하루종일 걸릴거 같은데.. 내 연약한 공공 와이파이(?",
          "original_form": "다운받는 것만 하루종일 걸릴거 같은데.. 내 연약한 공공 와이파이(?",
          "speaker_id": "2",
          "time": "20210827 16:45"
        },
        {
          "id": "MDRW2100019170.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ 그냥 다운로드 눌러놓고 휴대폰 덮어둬..",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그냥 다운로드 눌러놓고 휴대폰 덮어둬..",
          "speaker_id": "1",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100019170.1.5",
          "form": "원신이 게임중에서도 용량이 큰편이라...",
          "original_form": "원신이 게임중에서도 용량이 큰편이라...",
          "speaker_id": "1",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019170.1.6",
          "form": "아직도 안됐어...;;;;",
          "original_form": "아직도 안됐어...;;;;",
          "speaker_id": "2",
          "time": "20210827 18:11"
        },
        {
          "id": "MDRW2100019170.1.7",
          "form": "아직도 게임 다운로드 중이야?",
          "original_form": "아직도 게임 다운로드 중이야?",
          "speaker_id": "1",
          "time": "20210827 18:12"
        },
        {
          "id": "MDRW2100019170.1.8",
          "form": "와이파이 안되는거 아냐?",
          "original_form": "와이파이 안되는거 아냐?",
          "speaker_id": "1",
          "time": "20210827 18:12"
        },
        {
          "id": "MDRW2100019170.1.9",
          "form": "와이파이는 되는데",
          "original_form": "와이파이는 되는데",
          "speaker_id": "2",
          "time": "20210827 18:12"
        },
        {
          "id": "MDRW2100019170.1.10",
          "form": "게임 용량 럽프듀도 많다 했는데 얘네 비할게 아니었네",
          "original_form": "게임 용량 럽프듀도 많다 했는데 얘네 비할게 아니었네",
          "speaker_id": "2",
          "time": "20210827 18:12"
        },
        {
          "id": "MDRW2100019170.1.11",
          "form": "눈물만큼 받아지는 느낌이야",
          "original_form": "눈물만큼 받아지는 느낌이야",
          "speaker_id": "2",
          "time": "20210827 18:12"
        },
        {
          "id": "MDRW2100019170.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:13"
        },
        {
          "id": "MDRW2100019170.1.13",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210827 18:13"
        },
        {
          "id": "MDRW2100019170.1.14",
          "form": "럽프듀는 3기가인가 그랫잖아",
          "original_form": "럽프듀는 3기가인가 그랫잖아",
          "speaker_id": "1",
          "time": "20210827 18:13"
        },
        {
          "id": "MDRW2100019170.1.15",
          "form": "얜 그 3.5배...",
          "original_form": "얜 그 3.5배...",
          "speaker_id": "1",
          "time": "20210827 18:13"
        },
        {
          "id": "MDRW2100019170.1.16",
          "form": "그래서... 지금도 답답해... 나 그때 언제냐",
          "original_form": "그래서... 지금도 답답해... 나 그때 언제냐",
          "speaker_id": "2",
          "time": "20210827 18:13"
        },
        {
          "id": "MDRW2100019170.1.17",
          "form": "용량 눈물만할때",
          "original_form": "용량 눈물만할때",
          "speaker_id": "2",
          "time": "20210827 18:13"
        },
        {
          "id": "MDRW2100019170.1.18",
          "form": "럽프듀 돌린다고 다 지웠는데",
          "original_form": "럽프듀 돌린다고 다 지웠는데",
          "speaker_id": "2",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100019170.1.19",
          "form": "원신하려면......(절래",
          "original_form": "원신하려면......(절래",
          "speaker_id": "2",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100019170.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100019170.1.21",
          "form": "폰 용량 얼마 안남았어?",
          "original_form": "폰 용량 얼마 안남았어?",
          "speaker_id": "1",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100019170.1.22",
          "form": "요즘 겜들이 다 용량이 크긴 해",
          "original_form": "요즘 겜들이 다 용량이 크긴 해",
          "speaker_id": "1",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100019170.1.23",
          "form": "지금 너 안됨 그래서 몇개 지우긴 했어",
          "original_form": "지금 너 안됨 그래서 몇개 지우긴 했어",
          "speaker_id": "2",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100019170.1.24",
          "form": "나 다 쓰는 것들만 있는데(아님",
          "original_form": "나 다 쓰는 것들만 있는데(아님",
          "speaker_id": "2",
          "time": "20210827 18:15"
        },
        {
          "id": "MDRW2100019170.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:17"
        },
        {
          "id": "MDRW2100019170.1.26",
          "form": "노트10이엇어?",
          "original_form": "노트10이엇어?",
          "speaker_id": "1",
          "time": "20210827 18:17"
        },
        {
          "id": "MDRW2100019170.1.27",
          "form": "노트9이야! 나름... 많이 안깔았다고 생각했는데 생각난 김에 좀 지워볼까..",
          "original_form": "노트9이야! 나름... 많이 안깔았다고 생각했는데 생각난 김에 좀 지워볼까..",
          "speaker_id": "2",
