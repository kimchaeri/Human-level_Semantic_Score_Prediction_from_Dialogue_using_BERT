      "utterance": [
        {
          "id": "MDRW2100024654.1.1",
          "form": "곰표 맥주 드셔보셨나유",
          "original_form": "곰표 맥주 드셔보셨나유",
          "speaker_id": "1",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100024654.1.2",
          "form": "아직 못 먹어봤어요 맥주를 안좋아해서요",
          "original_form": "아직 못 먹어봤어요 맥주를 안좋아해서요",
          "speaker_id": "2",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100024654.1.3",
          "form": "아하 그럼 콜라보 제품 뭐써보셧나요",
          "original_form": "아하 그럼 콜라보 제품 뭐써보셧나요",
          "speaker_id": "1",
          "time": "20210831 16:43"
        },
        {
          "id": "MDRW2100024654.1.4",
          "form": "저는 카카오랑 삼성 콜라보한 빔프로젝터를",
          "original_form": "저는 카카오랑 삼성 콜라보한 빔프로젝터를",
          "speaker_id": "1",
          "time": "20210831 16:43"
        },
        {
          "id": "MDRW2100024654.1.5",
          "form": "사고파요",
          "original_form": "사고파요",
          "speaker_id": "1",
          "time": "20210831 16:43"
        },
        {
          "id": "MDRW2100024654.1.6",
          "form": "진로 두꺼비는 봤어요",
          "original_form": "진로 두꺼비는 봤어요",
          "speaker_id": "2",
          "time": "20210831 16:44"
        },
        {
          "id": "MDRW2100024654.1.7",
          "form": "음 진로는 캐릭터 아닌가요",
          "original_form": "음 진로는 캐릭터 아닌가요",
          "speaker_id": "1",
          "time": "20210831 16:45"
        },
        {
          "id": "MDRW2100024654.1.8",
          "form": "넹 개구리요",
          "original_form": "넹 개구리요",
          "speaker_id": "2",
          "time": "20210831 16:45"
        },
        {
          "id": "MDRW2100024654.1.9",
          "form": "저는 라이언 캐릭터 좋아해서 콜라보한 거에 손이가요",
          "original_form": "저는 라이언 캐릭터 좋아해서 콜라보한 거에 손이가요",
          "speaker_id": "1",
          "time": "20210831 16:46"
        },
        {
          "id": "MDRW2100024654.1.10",
          "form": "저도 그 피치 좋아해요 캐릭터중",
          "original_form": "저도 그 피치 좋아해요 캐릭터중",
          "speaker_id": "2",
          "time": "20210831 16:46"
        },
        {
          "id": "MDRW2100024654.1.11",
          "form": "아하 두꺼비 아니었나요? 지금까지 두꺼비인줄 알았어요",
          "original_form": "아하 두꺼비 아니었나요? 지금까지 두꺼비인줄 알았어요",
          "speaker_id": "1",
          "time": "20210831 16:46"
        },
        {
          "id": "MDRW2100024654.1.12",
          "form": "아 두꺼비 같네요ㅎ",
          "original_form": "아 두꺼비 같네요ㅎ",
          "speaker_id": "2",
          "time": "20210831 16:47"
        },
        {
          "id": "MDRW2100024654.1.13",
          "form": "피치보단 라이언이죠!귀염 최고는 라이언입니다..",
          "original_form": "피치보단 라이언이죠!귀염 최고는 라이언입니다..",
          "speaker_id": "1",
          "time": "20210831 16:47"
        },
        {
          "id": "MDRW2100024654.1.14",
          "form": "피치는 뒷모습이 예뻐요",
          "original_form": "피치는 뒷모습이 예뻐요",
          "speaker_id": "2",
          "time": "20210831 16:48"
        },
        {
