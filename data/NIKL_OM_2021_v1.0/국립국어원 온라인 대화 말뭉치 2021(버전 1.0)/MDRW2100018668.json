      "utterance": [
        {
          "id": "MDRW2100018668.1.1",
          "form": "진짜 웰메이드극이에요 역적",
          "original_form": "진짜 웰메이드극이에요 역적",
          "speaker_id": "2",
          "time": "20210827 10:24"
        },
        {
          "id": "MDRW2100018668.1.2",
          "form": "세번봤어요 ㅋㅋㅋ",
          "original_form": "세번봤어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:24"
        },
        {
          "id": "MDRW2100018668.1.3",
          "form": "이걸 왜 본방으로 안봤지 싶더라고요!!!",
          "original_form": "이걸 왜 본방으로 안봤지 싶더라고요!!!",
          "speaker_id": "1",
          "time": "20210827 10:24"
        },
        {
          "id": "MDRW2100018668.1.4",
          "form": "본방도달리고 ㅋㅋㅋ",
          "original_form": "본방도달리고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:24"
        },
        {
          "id": "MDRW2100018668.1.5",
          "form": "서사장난아니죠 ㅋㅋ",
          "original_form": "서사장난아니죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:24"
        },
        {
          "id": "MDRW2100018668.1.6",
          "form": "이하늬님도 대박이고",
          "original_form": "이하늬님도 대박이고",
          "speaker_id": "1",
          "time": "20210827 10:25"
        },
        {
          "id": "MDRW2100018668.1.7",
          "form": "길동이....!!!! 완전 미쳐써요",
          "original_form": "길동이....!!!! 완전 미쳐써요",
          "speaker_id": "1",
          "time": "20210827 10:25"
        },
        {
          "id": "MDRW2100018668.1.8",
          "form": "김상중님도 포스 장난아니고ㅋㅋㅋㅋ몰입확되고",
          "original_form": "김상중님도 포스 장난아니고ㅋㅋㅋㅋ몰입확되고",
          "speaker_id": "1",
          "time": "20210827 10:25"
        },
        {
          "id": "MDRW2100018668.1.9",
          "form": "윤균상 역적으로 처음 알았는데 역적에서 연기도 좋고 캐릭터자체가 너무좋았어요",
          "original_form": "윤균상 역적으로 처음 알았는데 역적에서 연기도 좋고 캐릭터자체가 너무좋았어요",
          "speaker_id": "2",
          "time": "20210827 10:25"
        },
        {
          "id": "MDRW2100018668.1.10",
          "form": "근데 여러번볼수록 저는 가령이가 자꾸 마음이 쓰여서 ㅋㅋㅋㅋ",
          "original_form": "근데 여러번볼수록 저는 가령이가 자꾸 마음이 쓰여서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:26"
        },
        {
          "id": "MDRW2100018668.1.11",
          "form": "갑자기 역적뽕 차올라서",
          "original_form": "갑자기 역적뽕 차올라서",
          "speaker_id": "1",
          "time": "20210827 10:26"
        },
        {
          "id": "MDRW2100018668.1.12",
          "form": "홍연 좀 들어야겠어요ㅋㅋㅋㅋ",
          "original_form": "홍연 좀 들어야겠어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:26"
        },
        {
          "id": "MDRW2100018668.1.13",
          "form": "ㅋㅋㅋㅋㅋ 괜히 듣다보면 막 온갖 장면 다 떠오르고 그래요 ㅋㅋㅋ 전사실 지금 채수빈이 부른 역적 오슽 듣는중이에요 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 괜히 듣다보면 막 온갖 장면 다 떠오르고 그래요 ㅋㅋㅋ 전사실 지금 채수빈이 부른 역적 오슽 듣는중이에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:27"
        },
        {
          "id": "MDRW2100018668.1.14",
          "form": "그것도 있어요?? 드라마볼때 몰랐는데 찾아봐야겠어요",
          "original_form": "그것도 있어요?? 드라마볼때 몰랐는데 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100018668.1.15",
          "form": "저 최근에 완전 재밌는 드라마 발견했잖아요....",
          "original_form": "저 최근에 완전 재밌는 드라마 발견했잖아요....",
          "speaker_id": "1",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100018668.1.16",
          "form": "이하늬가 부른것도 있어요! ㅋㅋ",
          "original_form": "이하늬가 부른것도 있어요! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:29"
        },
        {
          "id": "MDRW2100018668.1.17",
          "form": "뭐요뭐요",
          "original_form": "뭐요뭐요",
          "speaker_id": "2",
          "time": "20210827 10:29"
        },
        {
          "id": "MDRW2100018668.1.18",
          "form": "웨이브 오리지널인데 모범택시요!",
          "original_form": "웨이브 오리지널인데 모범택시요!",
          "speaker_id": "1",
          "time": "20210827 10:29"
        },
        {
          "id": "MDRW2100018668.1.19",
          "form": "헐 저그거 보려고 딱 대기중이에욬ㅋㅋㅋ",
          "original_form": "헐 저그거 보려고 딱 대기중이에욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:29"
        },
        {
          "id": "MDRW2100018668.1.20",
          "form": "출연진 논란잇는거만 알고 재미없어보여서 안 봤는데....",
          "original_form": "출연진 논란잇는거만 알고 재미없어보여서 안 봤는데....",
          "speaker_id": "1",
          "time": "20210827 10:29"
        },
        {
          "id": "MDRW2100018668.1.21",
          "form": "왓쳐 하차하면 그거볼거에요 ㅋㅋㅋ",
          "original_form": "왓쳐 하차하면 그거볼거에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:29"
        },
        {
          "id": "MDRW2100018668.1.22",
          "form": "대박... 이건 진짜 밤새보고 얘도 이틀만에 클리어ㅋㅋㅋㅋㅋㅋ",
          "original_form": "대박... 이건 진짜 밤새보고 얘도 이틀만에 클리어ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:29"
        },
        {
          "id": "MDRW2100018668.1.23",
          "form": "진짜 재밌어요 코미디요소도 많고 강추합니다!",
          "original_form": "진짜 재밌어요 코미디요소도 많고 강추합니다!",
          "speaker_id": "1",
          "time": "20210827 10:30"
        },
        {
          "id": "MDRW2100018668.1.24",
          "form": "저도 모범택시 처음에 논란때문에 안보고있었는데 평좋더라구요",
          "original_form": "저도 모범택시 처음에 논란때문에 안보고있었는데 평좋더라구요",
          "speaker_id": "2",
          "time": "20210827 10:30"
        }
      ]
    }
