      "utterance": [
        {
          "id": "MDRW2100023362.1.1",
          "form": "ㅎㅎ 이제부터 경제공부해여죠ㄹ",
          "original_form": "ㅎㅎ 이제부터 경제공부해여죠ㄹ",
          "speaker_id": "2",
          "time": "20210830 00:47"
        },
        {
          "id": "MDRW2100023362.1.2",
          "form": "이게더 어울릴거 같음!!",
          "original_form": "이게더 어울릴거 같음!!",
          "speaker_id": "1",
          "time": "20210830 00:47"
        },
        {
          "id": "MDRW2100023362.1.3",
          "form": "돈모으려면",
          "original_form": "돈모으려면",
          "speaker_id": "1",
          "time": "20210830 00:47"
        },
        {
          "id": "MDRW2100023362.1.4",
          "form": "솔직히 알았는데 그냥 냅뒀어요ㅎ",
          "original_form": "솔직히 알았는데 그냥 냅뒀어요ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:47"
        },
        {
          "id": "MDRW2100023362.1.5",
          "form": "강데 저축이 답이긴 해요 ㅎㅎ",
          "original_form": "강데 저축이 답이긴 해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 00:47"
        },
        {
          "id": "MDRW2100023362.1.6",
          "form": "맞아요ㅎ",
          "original_form": "맞아요ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:47"
        },
        {
          "id": "MDRW2100023362.1.7",
          "form": "정말 제테크도 많이 공부했고",
          "original_form": "정말 제테크도 많이 공부했고",
          "speaker_id": "2",
          "time": "20210830 00:47"
        },
        {
          "id": "MDRW2100023362.1.8",
          "form": "그동안에 투자도 기본적으로 잘했어요ㅎ",
          "original_form": "그동안에 투자도 기본적으로 잘했어요ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:48"
        },
        {
          "id": "MDRW2100023362.1.9",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "1",
          "time": "20210830 00:48"
        },
        {
          "id": "MDRW2100023362.1.10",
          "form": "근데 결혼하고 애기가 태어나고부터는",
          "original_form": "근데 결혼하고 애기가 태어나고부터는",
          "speaker_id": "2",
          "time": "20210830 00:48"
        },
        {
          "id": "MDRW2100023362.1.11",
          "form": "아에 아내에게 맡겼죠",
          "original_form": "아에 아내에게 맡겼죠",
          "speaker_id": "2",
          "time": "20210830 00:48"
        },
        {
          "id": "MDRW2100023362.1.12",
          "form": "대화가 통하지않아서 일방통행이였기에ㅎ 하고싶은대로 하게 해줬어요 ㅡㅎ",
          "original_form": "대화가 통하지않아서 일방통행이였기에ㅎ 하고싶은대로 하게 해줬어요 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210830 00:48"
        },
        {
          "id": "MDRW2100023362.1.13",
          "form": "음 근데 돈은 잘하는 사람에게 맡기는게 답이에요 ㅎ",
          "original_form": "음 근데 돈은 잘하는 사람에게 맡기는게 답이에요 ㅎ",
          "speaker_id": "1",
          "time": "20210830 00:48"
        },
        {
          "id": "MDRW2100023362.1.14",
          "form": "근데 결과가 이렇네여ㅎㅎ",
          "original_form": "근데 결과가 이렇네여ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 00:48"
        },
        {
          "id": "MDRW2100023362.1.15",
          "form": "정말 많은 돈을 벌었고ㅎ 수입도 많았는데ㅎㅎ",
          "original_form": "정말 많은 돈을 벌었고ㅎ 수입도 많았는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 00:49"
        },
        {
          "id": "MDRW2100023362.1.16",
          "form": "너무아쉬워요ㅠ",
          "original_form": "너무아쉬워요ㅠ",
          "speaker_id": "2",
          "time": "20210830 00:49"
        },
        {
          "id": "MDRW2100023362.1.17",
          "form": "저희아버지가 저한테 준돈만 1억이넘어가더라구요ㅎㅎ",
          "original_form": "저희아버지가 저한테 준돈만 1억이넘어가더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 00:49"
        },
        {
          "id": "MDRW2100023362.1.18",
          "form": "사람각자 마다 다르겠지만",
          "original_form": "사람각자 마다 다르겠지만",
          "speaker_id": "1",
          "time": "20210830 00:49"
        },
        {
          "id": "MDRW2100023362.1.19",
          "form": "글죠글죠ㅎㅎ",
          "original_form": "글죠글죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 00:50"
        },
        {
          "id": "MDRW2100023362.1.20",
          "form": "저는  고정비용 빼고 쓰는돈이",
          "original_form": "저는  고정비용 빼고 쓰는돈이",
          "speaker_id": "1",
          "time": "20210830 00:50"
        },
        {
          "id": "MDRW2100023362.1.21",
          "form": "월 80에사 거의 해결중이에요",
          "original_form": "월 80에사 거의 해결중이에요",
          "speaker_id": "1",
          "time": "20210830 00:50"
        },
        {
          "id": "MDRW2100023362.1.22",
          "form": "우왐ㅠ 부럽습니다ㅠ",
          "original_form": "우왐ㅠ 부럽습니다ㅠ",
          "speaker_id": "2",
          "time": "20210830 00:50"
        },
        {
          "id": "MDRW2100023362.1.23",
          "form": "식비 옷 생핑품 다 포함요",
          "original_form": "식비 옷 생핑품 다 포함요",
          "speaker_id": "1",
          "time": "20210830 00:51"
        },
        {
          "id": "MDRW2100023362.1.24",
          "form": "생활비80으로 생활이된느구나",
          "original_form": "생활비80으로 생활이된느구나",
          "speaker_id": "2",
          "time": "20210830 00:51"
        },
        {
          "id": "MDRW2100023362.1.25",
          "form": "그니깐요",
          "original_form": "그니깐요",
          "speaker_id": "2",
          "time": "20210830 00:51"
        },
        {
          "id": "MDRW2100023362.1.26",
          "form": "이번에 계산하니깐 고정비 빼고 생활비가 150~200",
          "original_form": "이번에 계산하니깐 고정비 빼고 생활비가 150~200",
          "speaker_id": "2",
          "time": "20210830 00:51"
        },
        {
          "id": "MDRW2100023362.1.27",
          "form": "이러니 돈을 못모으고 계속 나가는거에요ㅎ",
          "original_form": "이러니 돈을 못모으고 계속 나가는거에요ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:51"
        },
        {
          "id": "MDRW2100023362.1.28",
          "form": "음 근데 이게 누가 잘했다곤 할순 없지만",
          "original_form": "음 근데 이게 누가 잘했다곤 할순 없지만",
          "speaker_id": "1",
          "time": "20210830 00:52"
        },
        {
          "id": "MDRW2100023362.1.29",
          "form": "보통은  그테두리안에서 쓰는데 그게 아쉽네요",
          "original_form": "보통은  그테두리안에서 쓰는데 그게 아쉽네요",
          "speaker_id": "1",
          "time": "20210830 00:52"
        }
      ]
    }
  ]
