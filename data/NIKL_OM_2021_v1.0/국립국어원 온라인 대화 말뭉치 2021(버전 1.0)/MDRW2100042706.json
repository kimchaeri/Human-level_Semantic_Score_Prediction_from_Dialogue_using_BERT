      "utterance": [
        {
          "id": "MDRW2100042706.1.1",
          "form": "야 너 왜 입맛이 없냐",
          "original_form": "야 너 왜 입맛이 없냐",
          "speaker_id": "2",
          "time": "20210907 14:51"
        },
        {
          "id": "MDRW2100042706.1.2",
          "form": "어제 고기 많이 먹어서 그런가봐",
          "original_form": "어제 고기 많이 먹어서 그런가봐",
          "speaker_id": "1",
          "time": "20210907 14:51"
        },
        {
          "id": "MDRW2100042706.1.3",
          "form": "먹어봤자 얼마나 먹는다고",
          "original_form": "먹어봤자 얼마나 먹는다고",
          "speaker_id": "2",
          "time": "20210907 14:51"
        },
        {
          "id": "MDRW2100042706.1.4",
          "form": "몰라 입맛이 없댜..",
          "original_form": "몰라 입맛이 없댜..",
          "speaker_id": "1",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100042706.1.5",
          "form": "떡 맛있는데 하나만 먹어봐",
          "original_form": "떡 맛있는데 하나만 먹어봐",
          "speaker_id": "2",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100042706.1.6",
          "form": "안 땡김ㅠㅜ",
          "original_form": "안 땡김ㅠㅜ",
          "speaker_id": "1",
          "time": "20210907 14:53"
        },
        {
          "id": "MDRW2100042706.1.7",
          "form": "체했냐?",
          "original_form": "체했냐?",
          "speaker_id": "2",
          "time": "20210907 14:53"
        },
        {
          "id": "MDRW2100042706.1.8",
          "form": "막 배가 부른건 아닌데",
          "original_form": "막 배가 부른건 아닌데",
          "speaker_id": "1",
          "time": "20210907 14:53"
        },
        {
          "id": "MDRW2100042706.1.9",
          "form": "배가 고프지도 않음",
          "original_form": "배가 고프지도 않음",
          "speaker_id": "1",
          "time": "20210907 14:53"
        },
        {
          "id": "MDRW2100042706.1.10",
          "form": "왜 그러지",
          "original_form": "왜 그러지",
          "speaker_id": "2",
          "time": "20210907 14:53"
        },
        {
          "id": "MDRW2100042706.1.11",
          "form": "점심도 생각없었는데 억지로 먹었다",
          "original_form": "점심도 생각없었는데 억지로 먹었다",
          "speaker_id": "1",
          "time": "20210907 14:53"
        },
        {
          "id": "MDRW2100042706.1.12",
          "form": "달고기 이제 먹기싫은가봐",
          "original_form": "달고기 이제 먹기싫은가봐",
          "speaker_id": "2",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100042706.1.13",
          "form": "아니야 난 맛있는데 입맛이 없어서 그럼",
          "original_form": "아니야 난 맛있는데 입맛이 없어서 그럼",
          "speaker_id": "1",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100042706.1.14",
          "form": "아저씨는 질린다더라",
          "original_form": "아저씨는 질린다더라",
          "speaker_id": "2",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100042706.1.15",
          "form": "그거 생선가스치고 살진짜 통통한건데",
          "original_form": "그거 생선가스치고 살진짜 통통한건데",
          "speaker_id": "1",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100042706.1.16",
          "form": "난 좋음",
          "original_form": "난 좋음",
          "speaker_id": "1",
          "time": "20210907 14:55"
        }
