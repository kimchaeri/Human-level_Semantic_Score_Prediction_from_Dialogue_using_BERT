      "utterance": [
        {
          "id": "MDRW2100001937.1.1",
          "form": "왓따껌에서 그왓따인가여?",
          "original_form": "왓따껌에서 그왓따인가여?",
          "speaker_id": "1",
          "time": "20210819 22:22"
        },
        {
          "id": "MDRW2100001937.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210819 22:23"
        },
        {
          "id": "MDRW2100001937.1.3",
          "form": "ㅋㅋ  맞아요",
          "original_form": "ㅋㅋ  맞아요",
          "speaker_id": "2",
          "time": "20210819 22:23"
        },
        {
          "id": "MDRW2100001937.1.4",
          "form": "왓따껌에 그 왓따",
          "original_form": "왓따껌에 그 왓따",
          "speaker_id": "2",
          "time": "20210819 22:24"
        },
        {
          "id": "MDRW2100001937.1.5",
          "form": "공연 좋아하세요?",
          "original_form": "공연 좋아하세요?",
          "speaker_id": "1",
          "time": "20210819 22:24"
        },
        {
          "id": "MDRW2100001937.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210819 22:25"
        },
        {
          "id": "MDRW2100001937.1.7",
          "form": "연극 좋아해요",
          "original_form": "연극 좋아해요",
          "speaker_id": "2",
          "time": "20210819 22:25"
        },
        {
          "id": "MDRW2100001937.1.8",
          "form": "사진전이나 미술관 가는것도 좋아해요",
          "original_form": "사진전이나 미술관 가는것도 좋아해요",
          "speaker_id": "2",
          "time": "20210819 22:25"
        },
        {
          "id": "MDRW2100001937.1.9",
          "form": "서울에가면ㅋㅋ 인사동에 꼭 가요ㅎ",
          "original_form": "서울에가면ㅋㅋ 인사동에 꼭 가요ㅎ",
          "speaker_id": "2",
          "time": "20210819 22:26"
        },
        {
          "id": "MDRW2100001937.1.10",
          "form": "무료 전시회가 꽤 많더라구요",
          "original_form": "무료 전시회가 꽤 많더라구요",
          "speaker_id": "2",
          "time": "20210819 22:26"
        },
        {
          "id": "MDRW2100001937.1.11",
          "form": "저는 뮤지컬 콘서트 연극보러 가끔 가요",
          "original_form": "저는 뮤지컬 콘서트 연극보러 가끔 가요",
          "speaker_id": "1",
          "time": "20210819 22:26"
        },
        {
          "id": "MDRW2100001937.1.12",
          "form": "요즘 코로나 때문에..",
          "original_form": "요즘 코로나 때문에..",
          "speaker_id": "1",
          "time": "20210819 22:26"
        },
        {
          "id": "MDRW2100001937.1.13",
          "form": "그쵸..",
          "original_form": "그쵸..",
          "speaker_id": "2",
          "time": "20210819 22:26"
        },
        {
          "id": "MDRW2100001937.1.14",
          "form": "마스크 쓰고 그래도 볼건 봅니다",
          "original_form": "마스크 쓰고 그래도 볼건 봅니다",
          "speaker_id": "1",
          "time": "20210819 22:27"
        },
        {
          "id": "MDRW2100001937.1.15",
          "form": "생각보다 많이들 보러 오더라구요",
          "original_form": "생각보다 많이들 보러 오더라구요",
          "speaker_id": "1",
          "time": "20210819 22:27"
        },
        {
          "id": "MDRW2100001937.1.16",
          "form": "ㅋㅋㅋ 안간지 오래된거 같아요ㅠ",
          "original_form": "ㅋㅋㅋ 안간지 오래된거 같아요ㅠ",
          "speaker_id": "2",
          "time": "20210819 22:27"
        },
        {
          "id": "MDRW2100001937.1.17",
          "form": "콘서트도 가고 싶고ㅎㅎ 연극도 보고 싶은데",
          "original_form": "콘서트도 가고 싶고ㅎㅎ 연극도 보고 싶은데",
          "speaker_id": "2",
          "time": "20210819 22:27"
        },
        {
          "id": "MDRW2100001937.1.18",
          "form": "처음엔 눈치보면서 갔다가",
          "original_form": "처음엔 눈치보면서 갔다가",
          "speaker_id": "1",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100001937.1.19",
          "form": "자가격리 한 이후로",
          "original_form": "자가격리 한 이후로",
          "speaker_id": "2",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100001937.1.20",
          "form": "더 못 가겠더라구요",
          "original_form": "더 못 가겠더라구요",
          "speaker_id": "2",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100001937.1.21",
          "form": "많은분들이 온거 보고 거리두기하며 보고있어요",
          "original_form": "많은분들이 온거 보고 거리두기하며 보고있어요",
          "speaker_id": "1",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100001937.1.22",
          "form": "아 자가격리ㅠㅠ",
          "original_form": "아 자가격리ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100001937.1.23",
          "form": "저도.. 몰래 몰래다니다가",
          "original_form": "저도.. 몰래 몰래다니다가",
          "speaker_id": "2",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100001937.1.24",
          "form": "걸렸지뭐예요ㅎㅎ",
          "original_form": "걸렸지뭐예요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001937.1.25",
          "form": "힘드셨겠다 지금은 말할수있지만요",
          "original_form": "힘드셨겠다 지금은 말할수있지만요",
          "speaker_id": "1",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001937.1.26",
          "form": "코로나 끝나면 유리상자 콘서트는  꼭 가보려구요",
          "original_form": "코로나 끝나면 유리상자 콘서트는  꼭 가보려구요",
          "speaker_id": "2",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001937.1.27",
          "form": "우연히 한번 가봤는데 생각보다 너무 좋았어요ㅎㅎ",
          "original_form": "우연히 한번 가봤는데 생각보다 너무 좋았어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001937.1.28",
          "form": "온라인으로도 소통해주는 스타들도 있어서 좋아요",
          "original_form": "온라인으로도 소통해주는 스타들도 있어서 좋아요",
          "speaker_id": "1",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100001937.1.29",
          "form": "공연장에서 그느낌은",
          "original_form": "공연장에서 그느낌은",
          "speaker_id": "1",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100001937.1.30",
          "form": "가봐야 느껴지죠",
          "original_form": "가봐야 느껴지죠",
          "speaker_id": "1",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100001937.1.31",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100001937.1.32",
          "form": "저는 친한언니가 젝키완전팬이라",
          "original_form": "저는 친한언니가 젝키완전팬이라",
          "speaker_id": "2",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100001937.1.33",
          "form": "저는 에쵸티팬이였는데",
          "original_form": "저는 에쵸티팬이였는데",
          "speaker_id": "2",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100001937.1.34",
          "form": "따라갔었죠ㅋㅋㅋ",
          "original_form": "따라갔었죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100001937.1.35",
          "form": "완전 재미있었어요",
          "original_form": "완전 재미있었어요",
          "speaker_id": "2",
          "time": "20210819 22:32"
        },
        {
          "id": "MDRW2100001937.1.36",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210819 22:32"
        },
        {
          "id": "MDRW2100001937.1.37",
          "form": "저도 학창시절에 좋아했던",
          "original_form": "저도 학창시절에 좋아했던",
          "speaker_id": "1",
          "time": "20210819 22:33"
        },
        {
          "id": "MDRW2100001937.1.38",
          "form": "이름들이네요",
          "original_form": "이름들이네요",
          "speaker_id": "1",
          "time": "20210819 22:33"
        },
        {
          "id": "MDRW2100001937.1.39",
          "form": "저랑 같은 시대를 사셨군요ㅎ",
          "original_form": "저랑 같은 시대를 사셨군요ㅎ",
          "speaker_id": "2",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100001937.1.40",
          "form": "지금은 그느낌 안들지만 가끔 나오는거보니 저랑 같더라구여",
          "original_form": "지금은 그느낌 안들지만 가끔 나오는거보니 저랑 같더라구여",
          "speaker_id": "1",
          "time": "20210819 22:56"
        },
        {
          "id": "MDRW2100001937.1.41",
          "form": "같이 늘거가는구나ㅋㅋㅋ",
          "original_form": "같이 늘거가는구나ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:56"
        },
        {
          "id": "MDRW2100001937.1.42",
          "form": "ㅋㅋㅋㅋ  왜요? 저 칠현이오빠랑 토니오빠 완전 사랑했는데요?",
          "original_form": "ㅋㅋㅋㅋ  왜요? 저 칠현이오빠랑 토니오빠 완전 사랑했는데요?",
          "speaker_id": "2",
          "time": "20210819 22:57"
        },
        {
          "id": "MDRW2100001937.1.43",
          "form": "알고보니 옵빠가 아닌 친구였고 동생이었고ㅋㅋㅋ",
          "original_form": "알고보니 옵빠가 아닌 친구였고 동생이었고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:59"
        },
        {
          "id": "MDRW2100001937.1.44",
          "form": "구래도 학창시절 멋지면 다 오빠였어요",
          "original_form": "구래도 학창시절 멋지면 다 오빠였어요",
          "speaker_id": "1",
          "time": "20210819 23:00"
        },
        {
          "id": "MDRW2100001937.1.45",
          "form": "맞아요 송강 오빠라고 부르고 싶다ㅋㅋ",
          "original_form": "맞아요 송강 오빠라고 부르고 싶다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:01"
        },
        {
          "id": "MDRW2100001937.1.46",
          "form": "앗 요즘 좋아하는분인가여?",
          "original_form": "앗 요즘 좋아하는분인가여?",
          "speaker_id": "1",
          "time": "20210819 23:02"
        },
        {
          "id": "MDRW2100001937.1.47",
          "form": "아뇨ㅎㅎ 저는 솔직히 요즘배우보다는",
          "original_form": "아뇨ㅎㅎ 저는 솔직히 요즘배우보다는",
          "speaker_id": "2",
          "time": "20210819 23:03"
        },
        {
          "id": "MDRW2100001937.1.48",
          "form": "지성이나 조정석을 좋아합니다ㅎ",
          "original_form": "지성이나 조정석을 좋아합니다ㅎ",
          "speaker_id": "2",
          "time": "20210819 23:03"
        },
        {
          "id": "MDRW2100001937.1.49",
          "form": "앗 둘다 좋아하는 배우들이에요",
          "original_form": "앗 둘다 좋아하는 배우들이에요",
