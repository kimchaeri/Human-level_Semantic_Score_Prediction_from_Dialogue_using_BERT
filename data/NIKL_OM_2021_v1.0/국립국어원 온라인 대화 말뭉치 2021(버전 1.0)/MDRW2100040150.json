      "utterance": [
        {
          "id": "MDRW2100040150.1.1",
          "form": "아잠 먹었어요ㅋㅋㅋㅋㅋ",
          "original_form": "아잠 먹었어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 12:33"
        },
        {
          "id": "MDRW2100040150.1.2",
          "form": "치즈김밥이랑 참치김밥!",
          "original_form": "치즈김밥이랑 참치김밥!",
          "speaker_id": "2",
          "time": "20210904 12:33"
        },
        {
          "id": "MDRW2100040150.1.3",
          "form": "아점ㅋㅋ이요 뭐드셨어요",
          "original_form": "아점ㅋㅋ이요 뭐드셨어요",
          "speaker_id": "2",
          "time": "20210904 12:33"
        },
        {
          "id": "MDRW2100040150.1.4",
          "form": "저흰 오늘 짜장면  시켜먹었어요 ^^;",
          "original_form": "저흰 오늘 짜장면  시켜먹었어요 ^^;",
          "speaker_id": "1",
          "time": "20210904 22:26"
        },
        {
          "id": "MDRW2100040150.1.5",
          "form": "오오 저는 철원에 칼국수 먹고왔어욬ㅋㅋㅋㅋㅋ",
          "original_form": "오오 저는 철원에 칼국수 먹고왔어욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:27"
        },
        {
          "id": "MDRW2100040150.1.6",
          "form": "둘 다 면류네요",
          "original_form": "둘 다 면류네요",
          "speaker_id": "2",
          "time": "20210904 22:27"
        },
        {
          "id": "MDRW2100040150.1.7",
          "form": "와~~칼국수~~ 맛있으셨나요^^",
          "original_form": "와~~칼국수~~ 맛있으셨나요^^",
          "speaker_id": "1",
          "time": "20210904 22:40"
        },
        {
          "id": "MDRW2100040150.1.8",
          "form": "전 사실 콩국수먹었어요 ㅋㅋㅋ",
          "original_form": "전 사실 콩국수먹었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:41"
        },
        {
          "id": "MDRW2100040150.1.9",
          "form": "남편은 간짜장매운맛 애들은 짜장 저는 콩국수용ㅋ",
          "original_form": "남편은 간짜장매운맛 애들은 짜장 저는 콩국수용ㅋ",
          "speaker_id": "1",
          "time": "20210904 22:41"
        },
        {
          "id": "MDRW2100040150.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ부페처럼 여러개 시키니까 좋네요ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ부페처럼 여러개 시키니까 좋네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 12:58"
        },
        {
          "id": "MDRW2100040150.1.11",
          "form": "저는 비빔칼국수랑 고기칼국수 먹었는데 둘 다 별미네요",
          "original_form": "저는 비빔칼국수랑 고기칼국수 먹었는데 둘 다 별미네요",
          "speaker_id": "2",
          "time": "20210905 12:58"
        },
        {
          "id": "MDRW2100040150.1.12",
          "form": "우와~ 맛나게 드셨네요",
          "original_form": "우와~ 맛나게 드셨네요",
          "speaker_id": "1",
          "time": "20210905 13:14"
        },
        {
          "id": "MDRW2100040150.1.13",
          "form": "전 오늘 애기들 잔치국수 삶아줬어요 ㅋㅋㅋ",
          "original_form": "전 오늘 애기들 잔치국수 삶아줬어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:14"
        },
        {
          "id": "MDRW2100040150.1.14",
          "form": "면파티네요 ㅎㅎ",
          "original_form": "면파티네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 13:14"
        },
        {
          "id": "MDRW2100040150.1.15",
          "form": "오 면 좋아하시네용ㅋㅋㅋ",
          "original_form": "오 면 좋아하시네용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 16:35"
        },
        {
          "id": "MDRW2100040150.1.16",
          "form": "평이담백뼈칼국수 여긴데 완전 맛집이에요면 좋아하시구 언젠가 철원 가신다면 추천추천",
          "original_form": "평이담백뼈칼국수 여긴데 완전 맛집이에요면 좋아하시구 언젠가 철원 가신다면 추천추천",
          "speaker_id": "2",
          "time": "20210905 16:35"
        },
        {
          "id": "MDRW2100040150.1.17",
          "form": "혹시 철원점인가요?ㅋㅋ  집에서 쉬지않고 자차 다이렉트로 6시간넘게 걸리네요 ㅋㅋ",
          "original_form": "혹시 철원점인가요?ㅋㅋ  집에서 쉬지않고 자차 다이렉트로 6시간넘게 걸리네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:00"
        },
        {
          "id": "MDRW2100040150.1.18",
          "form": "근데 서울 마포에 본점이있네요ㅎㅎ",
          "original_form": "근데 서울 마포에 본점이있네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 17:00"
        },
        {
          "id": "MDRW2100040150.1.19",
          "form": "맛있겠어요~~ 언젠가 가서 먹어보는 날이 오겠죠 ㅋㅋ",
          "original_form": "맛있겠어요~~ 언젠가 가서 먹어보는 날이 오겠죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:00"
        },
        {
          "id": "MDRW2100040150.1.20",
          "form": "저녁은 뭘 드시나요 ㅎㅎ",
          "original_form": "저녁은 뭘 드시나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 17:01"
        },
        {
          "id": "MDRW2100040150.1.21",
          "form": "오 서울에 본점 있는지는 몰랐어요ㅋㅋㅋㅋㅋ추천합니다ㅋㅋㅋㅋ",
          "original_form": "오 서울에 본점 있는지는 몰랐어요ㅋㅋㅋㅋㅋ추천합니다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100040150.1.22",
          "form": "만두는 양이 너무 작고 그냥 적당히 맛있지만 칼국수 굿굿",
          "original_form": "만두는 양이 너무 작고 그냥 적당히 맛있지만 칼국수 굿굿",
          "speaker_id": "2",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100040150.1.23",
          "form": "저녁은 불고기 예정이에요",
          "original_form": "저녁은 불고기 예정이에요",
          "speaker_id": "2",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100040150.1.24",
          "form": "name2님~~~~~ 너무늦게 돌아왔네요",
          "original_form": "&name2&님~~~~~ 너무늦게 돌아왔네요",
          "speaker_id": "1",
          "time": "20210906 16:31"
        },
        {
          "id": "MDRW2100040150.1.25",
          "form": "오늘도 식사는 잘하셨나용",
          "original_form": "오늘도 식사는 잘하셨나용",
          "speaker_id": "1",
          "time": "20210906 16:31"
        },
        {
          "id": "MDRW2100040150.1.26",
          "form": "저는 아점으로 냉동실 애슐리 볶음밥을먹고",
          "original_form": "저는 아점으로 냉동실 애슐리 볶음밥을먹고",
          "speaker_id": "1",
          "time": "20210906 16:31"
        },
        {
          "id": "MDRW2100040150.1.27",
          "form": "돌아서니 배가고프네요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "돌아서니 배가고프네요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:31"
        },
        {
          "id": "MDRW2100040150.1.28",
          "form": "~ 저녁을 뭘해먹어야하나요~~~",
          "original_form": "~ 저녁을 뭘해먹어야하나요~~~",
          "speaker_id": "1",
          "time": "20210906 16:31"
        },
        {
          "id": "MDRW2100040150.1.29",
          "form": "어우 저는 쭈꾸미 한 상 식당에서 먹고 푹 잤더니",
          "original_form": "어우 저는 쭈꾸미 한 상 식당에서 먹고 푹 잤더니",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100040150.1.30",
          "form": "ㅋㅋㅋ점심땨 먹은게 아직 소화 중이에요ㅋㅋ",
          "original_form": "ㅋㅋㅋ점심땨 먹은게 아직 소화 중이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100040150.1.31",
          "form": "우와~~~~~~",
          "original_form": "우와~~~~~~",
          "speaker_id": "1",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100040150.1.32",
          "form": "쭈꾸미ㅜㅜ",
          "original_form": "쭈꾸미ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100040150.1.33",
          "form": "완전먹고싶어요 완전!",
          "original_form": "완전먹고싶어요 완전!",
          "speaker_id": "1",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100040150.1.34",
          "form": "맛있으셨겠어요~~",
          "original_form": "맛있으셨겠어요~~",
          "speaker_id": "1",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100040150.1.35",
          "form": "전 점심에 싸이버거먹었는데",
          "original_form": "전 점심에 싸이버거먹었는데",
          "speaker_id": "1",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100040150.1.36",
          "form": "저녁에 치킨먹었어요ㅜㅜ",
          "original_form": "저녁에 치킨먹었어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100040150.1.37",
          "form": "뭔가 닭이 겹쳐서",
          "original_form": "뭔가 닭이 겹쳐서",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100040150.1.38",
          "form": "저녁 치킨은 안먹히더라구요ㅜ",
          "original_form": "저녁 치킨은 안먹히더라구요ㅜ",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100040150.1.39",
          "form": "쭈꾸미라니!!",
          "original_form": "쭈꾸미라니!!",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100040150.1.40",
          "form": "닭도 겹치고 패스트 푸드도 겹치고",
          "original_form": "닭도 겹치고 패스트 푸드도 겹치고",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100040150.1.41",
          "form": "전 그럼 된장 생각 나던데..",
          "original_form": "전 그럼 된장 생각 나던데..",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100040150.1.42",
          "form": "맞아요ㅡㅠ 이런날없었는데 어쩌다보니 이렇게되서",
          "original_form": "맞아요ㅡㅠ 이런날없었는데 어쩌다보니 이렇게되서",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100040150.1.43",
          "form": "말씀하시니  된장국도 먹고싶네요 ㅋㅋㅋㅋ",
          "original_form": "말씀하시니  된장국도 먹고싶네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100040150.1.44",
          "form": "ㅋㅋㅋ싸이버거는 맛있어요?",
          "original_form": "ㅋㅋㅋ싸이버거는 맛있어요?",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100040150.1.45",
          "form": "궁금하다 쿠폰 있었는데 근처에 맘터가 없어서 그냥 판매했었거든요",
          "original_form": "궁금하다 쿠폰 있었는데 근처에 맘터가 없어서 그냥 판매했었거든요",
          "speaker_id": "2",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100040150.1.46",
          "form": "네^^ 싸이버거맛나요!",
          "original_form": "네^^ 싸이버거맛나요!",
          "speaker_id": "1",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100040150.1.47",
          "form": "저희동네는 맘터만있어서",
          "original_form": "저희동네는 맘터만있어서",
          "speaker_id": "1",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100040150.1.48",
          "form": "동네서 먹으렴 선택권이없어욬ㅋㅋ",
          "original_form": "동네서 먹으렴 선택권이없어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100040150.1.49",
          "form": "저희는 버거킹하고 노브랜드가 떡하니 있어서",
          "original_form": "저희는 버거킹하고 노브랜드가 떡하니 있어서",
          "speaker_id": "2",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100040150.1.50",
          "form": "만터가 안들어왔나봐요ㅜㅜ",
          "original_form": "만터가 안들어왔나봐요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100040150.1.51",
          "form": "버거에 콜라 다세요 아님 다른 음료드세요?",
          "original_form": "버거에 콜라 다세요 아님 다른 음료드세요?",
