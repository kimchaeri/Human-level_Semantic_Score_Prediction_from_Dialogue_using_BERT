      "utterance": [
        {
          "id": "MDRW2100018933.1.1",
          "form": "네 저는 사실 지리학을 전공해서",
          "original_form": "네 저는 사실 지리학을 전공해서",
          "speaker_id": "2",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018933.1.2",
          "form": "이런 저런곳을 다녀야했어요",
          "original_form": "이런 저런곳을 다녀야했어요",
          "speaker_id": "2",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018933.1.3",
          "form": "와 지리학",
          "original_form": "와 지리학",
          "speaker_id": "1",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100018933.1.4",
          "form": "지리학 전공분 처음 봬요",
          "original_form": "지리학 전공분 처음 봬요",
          "speaker_id": "1",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100018933.1.5",
          "form": "멋있네요",
          "original_form": "멋있네요",
          "speaker_id": "1",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100018933.1.6",
          "form": "국내에 과가 많은 편은 아니라ㅋㅋㅋ",
          "original_form": "국내에 과가 많은 편은 아니라ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100018933.1.7",
          "form": "그래도 힘들게 다녀온 곳들이 기억에 남네요",
          "original_form": "그래도 힘들게 다녀온 곳들이 기억에 남네요",
          "speaker_id": "2",
          "time": "20210827 13:44"
        },
        {
          "id": "MDRW2100018933.1.8",
          "form": "전국 안가본 곳이 없으시겟어요",
          "original_form": "전국 안가본 곳이 없으시겟어요",
          "speaker_id": "1",
          "time": "20210827 13:46"
        },
        {
          "id": "MDRW2100018933.1.9",
          "form": "ㅋㅋㅋ답사 못가본 곳도 있긴 한데",
          "original_form": "ㅋㅋㅋ답사 못가본 곳도 있긴 한데",
          "speaker_id": "2",
          "time": "20210827 13:47"
        },
        {
          "id": "MDRW2100018933.1.10",
          "form": "근데 한라산이 기억에 남아요",
          "original_form": "근데 한라산이 기억에 남아요",
          "speaker_id": "2",
          "time": "20210827 13:47"
        },
        {
          "id": "MDRW2100018933.1.11",
          "form": "한라산 너무 예쁘죠",
          "original_form": "한라산 너무 예쁘죠",
          "speaker_id": "1",
          "time": "20210827 13:50"
        },
        {
          "id": "MDRW2100018933.1.12",
          "form": "네 등산 하산 합쳐서 한 9시간했더니ㅎㅎ 예쁘기도 하고 고생도 했고 ㅎㅎ",
          "original_form": "네 등산 하산 합쳐서 한 9시간했더니ㅎㅎ 예쁘기도 하고 고생도 했고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 14:14"
        },
        {
          "id": "MDRW2100018933.1.13",
          "form": "한라산 다녀오셨나봐요!",
          "original_form": "한라산 다녀오셨나봐요!",
          "speaker_id": "2",
          "time": "20210827 14:15"
        },
        {
          "id": "MDRW2100018933.1.14",
          "form": "네 예전이긴하지만 예뻐서 감탄했던 기억이 있어요 ㅎㅎ",
          "original_form": "네 예전이긴하지만 예뻐서 감탄했던 기억이 있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 14:16"
        },
        {
          "id": "MDRW2100018933.1.15",
          "form": "여행을 많이 다녀오셨네요ㅎㅎ",
          "original_form": "여행을 많이 다녀오셨네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 14:22"
        },
        {
          "id": "MDRW2100018933.1.16",
          "form": "여행을 굉장히 좋아해요 ㅋㅋㅋㅋ 코로나 이후로는 간 적이 없지만 ㅠㅠ",
          "original_form": "여행을 굉장히 좋아해요 ㅋㅋㅋㅋ 코로나 이후로는 간 적이 없지만 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 14:29"
        },
        {
          "id": "MDRW2100018933.1.17",
          "form": "국내여행도 야금야금 다녔어요 ㅋㅋㅋㅋ 학교다닐땐 내일로여행도 많이하고 ㅋㅋㅋㅋ",
          "original_form": "국내여행도 야금야금 다녔어요 ㅋㅋㅋㅋ 학교다닐땐 내일로여행도 많이하고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:30"
        },
        {
          "id": "MDRW2100018933.1.18",
          "form": "와아 내일로여행 진짜 힘들지 않던가요?ㅋㅋㅋㅋ",
          "original_form": "와아 내일로여행 진짜 힘들지 않던가요?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:32"
        },
        {
          "id": "MDRW2100018933.1.19",
          "form": "저는 한번 다녀오고 더이상 못가겠더라구요ㅋㅋㅋ",
          "original_form": "저는 한번 다녀오고 더이상 못가겠더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:36"
        },
        {
          "id": "MDRW2100018933.1.20",
          "form": "저는 기차여행을 워낙 좋아해서요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 기차여행을 워낙 좋아해서요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:10"
        },
        {
          "id": "MDRW2100018933.1.21",
          "form": "기차따라 여행다니는 맛이 잇더라구요",
          "original_form": "기차따라 여행다니는 맛이 잇더라구요",
          "speaker_id": "1",
          "time": "20210827 15:11"
        },
        {
          "id": "MDRW2100018933.1.22",
          "form": "맞아요 낭만이 있어요",
          "original_form": "맞아요 낭만이 있어요",
          "speaker_id": "2",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100018933.1.23",
          "form": "근데 허리가 부숴지는 기분이 들었어요",
          "original_form": "근데 허리가 부숴지는 기분이 들었어요",
          "speaker_id": "2",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100018933.1.24",
          "form": "좌석이 불편해서요?",
          "original_form": "좌석이 불편해서요?",
          "speaker_id": "1",
          "time": "20210827 15:21"
        },
        {
          "id": "MDRW2100018933.1.25",
          "form": "네 좌석이 모자라기도 해서 엄청 서서 갔어요",
          "original_form": "네 좌석이 모자라기도 해서 엄청 서서 갔어요",
          "speaker_id": "2",
          "time": "20210827 15:22"
        },
        {
          "id": "MDRW2100018933.1.26",
          "form": "아 서서가면 힘들죠 ㅠㅠㅋㅋㅋㅋㅋ",
          "original_form": "아 서서가면 힘들죠 ㅠㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:24"
        },
        {
          "id": "MDRW2100018933.1.27",
          "form": "저는 아예 바닥에 주저앉아서...ㅋㅋㅋㅋㅋ",
          "original_form": "저는 아예 바닥에 주저앉아서...ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:25"
        },
        {
          "id": "MDRW2100018933.1.28",
          "form": "ㅋㅋㅋㅋ맞아요 주저앉아야겠더라구요",
          "original_form": "ㅋㅋㅋㅋ맞아요 주저앉아야겠더라구요",
          "speaker_id": "2",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100018933.1.29",
          "form": "친구들이랑 갔고 또 어릴때라 그땐 서서가는것도 너무 신나더라구요 ㅋㅋㅋㅋㅋ",
          "original_form": "친구들이랑 갔고 또 어릴때라 그땐 서서가는것도 너무 신나더라구요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100018933.1.30",
          "form": "좀 날씨 좋을때 가셨나요",
          "original_form": "좀 날씨 좋을때 가셨나요",
          "speaker_id": "2",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100018933.1.31",
          "form": "전 엄청 땡볕이어서 좀 힘들었던 것도 있어요 ㅋㅋㅋㅋ",
          "original_form": "전 엄청 땡볕이어서 좀 힘들었던 것도 있어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100018933.1.32",
          "form": "학생때니까 한여름아니면 한겨울이었어요 ㅋㅋㅋㅋ 저도 날씨때문에 고생은 좀 했어요",
          "original_form": "학생때니까 한여름아니면 한겨울이었어요 ㅋㅋㅋㅋ 저도 날씨때문에 고생은 좀 했어요",
          "speaker_id": "1",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100018933.1.33",
          "form": "여름엔 진짜 땀투성이되고 겨울엔 입돌아갈뻔햇거든요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "여름엔 진짜 땀투성이되고 겨울엔 입돌아갈뻔햇거든요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100018933.1.34",
          "form": "앜ㅋ 겨울도 힘들겠네요ㅋㅋㅋ 그렇게 막 떠날 수 있었던 때가 좀 그립네요 흑흑",
          "original_form": "앜ㅋ 겨울도 힘들겠네요ㅋㅋㅋ 그렇게 막 떠날 수 있었던 때가 좀 그립네요 흑흑",
          "speaker_id": "2",
          "time": "20210827 16:40"
        },
        {
          "id": "MDRW2100018933.1.35",
          "form": "맞아요 그때가 너무 그리워요 ㅠㅠ",
          "original_form": "맞아요 그때가 너무 그리워요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 16:43"
        }
      ]
    }
