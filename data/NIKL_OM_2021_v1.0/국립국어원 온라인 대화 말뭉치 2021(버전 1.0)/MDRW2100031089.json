      "utterance": [
        {
          "id": "MDRW2100031089.1.1",
          "form": "도서관도 있고 좋은회사네요",
          "original_form": "도서관도 있고 좋은회사네요",
          "speaker_id": "2",
          "time": "20210904 16:38"
        },
        {
          "id": "MDRW2100031089.1.2",
          "form": "큰데 다니시나봐요",
          "original_form": "큰데 다니시나봐요",
          "speaker_id": "2",
          "time": "20210904 16:38"
        },
        {
          "id": "MDRW2100031089.1.3",
          "form": "요즘 공공도서관도 잘 되어있어서",
          "original_form": "요즘 공공도서관도 잘 되어있어서",
          "speaker_id": "1",
          "time": "20210904 16:38"
        },
        {
          "id": "MDRW2100031089.1.4",
          "form": "영화는 좋아하세요?",
          "original_form": "영화는 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100031089.1.5",
          "form": "잘되있는데 시간내서 잘안가게되네요",
          "original_form": "잘되있는데 시간내서 잘안가게되네요",
          "speaker_id": "2",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100031089.1.6",
          "form": "아 공공기관이에요",
          "original_form": "아 공공기관이에요",
          "speaker_id": "1",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100031089.1.7",
          "form": "영화는 진짜 가끔요",
          "original_form": "영화는 진짜 가끔요",
          "speaker_id": "2",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100031089.1.8",
          "form": "최근에 뭐보셨어요?",
          "original_form": "최근에 뭐보셨어요?",
          "speaker_id": "2",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100031089.1.9",
          "form": "저도 영화는 그닥 안보는데",
          "original_form": "저도 영화는 그닥 안보는데",
          "speaker_id": "1",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100031089.1.10",
          "form": "박물관같은건 관심있으세요?",
          "original_form": "박물관같은건 관심있으세요?",
          "speaker_id": "1",
          "time": "20210904 16:40"
        },
        {
          "id": "MDRW2100031089.1.11",
          "form": "관심은 있는데 거기도 진짜 가끔가요",
          "original_form": "관심은 있는데 거기도 진짜 가끔가요",
          "speaker_id": "2",
          "time": "20210904 16:40"
        },
        {
          "id": "MDRW2100031089.1.12",
          "form": "일년에 한번?",
          "original_form": "일년에 한번?",
          "speaker_id": "2",
          "time": "20210904 16:40"
        },
        {
          "id": "MDRW2100031089.1.13",
          "form": "박물관도 자주 다니세요?",
          "original_form": "박물관도 자주 다니세요?",
          "speaker_id": "2",
          "time": "20210904 16:41"
        },
        {
          "id": "MDRW2100031089.1.14",
          "form": "아니요 전 해외갈때만 갑니다",
          "original_form": "아니요 전 해외갈때만 갑니다",
          "speaker_id": "1",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100031089.1.15",
          "form": "일년에 한번이면 자주 다니시는군요",
          "original_form": "일년에 한번이면 자주 다니시는군요",
          "speaker_id": "1",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100031089.1.16",
          "form": "아 해외박물관은 멋있죠",
          "original_form": "아 해외박물관은 멋있죠",
          "speaker_id": "2",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100031089.1.17",
          "form": "근데 요번년도는 못갔네요",
          "original_form": "근데 요번년도는 못갔네요",
          "speaker_id": "2",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100031089.1.18",
          "form": "그렇죠 박물관은 주로 애들때문에 가시는건가요?",
          "original_form": "그렇죠 박물관은 주로 애들때문에 가시는건가요?",
          "speaker_id": "1",
          "time": "20210904 16:48"
        },
        {
          "id": "MDRW2100031089.1.19",
          "form": "아뇨 그냥 가고싶으면 가요 ㅎ",
          "original_form": "아뇨 그냥 가고싶으면 가요 ㅎ",
          "speaker_id": "2",
          "time": "20210904 16:48"
        },
        {
          "id": "MDRW2100031089.1.20",
          "form": "전시회는 좋아하세요?",
          "original_form": "전시회는 좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 16:48"
        },
        {
          "id": "MDRW2100031089.1.21",
          "form": "제가 관심잏는 분야면 가려고 노력하는 편이에요",
          "original_form": "제가 관심잏는 분야면 가려고 노력하는 편이에요",
          "speaker_id": "1",
          "time": "20210904 16:49"
        }
