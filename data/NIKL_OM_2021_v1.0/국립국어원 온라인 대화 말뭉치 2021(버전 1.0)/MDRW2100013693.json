        },
        {
          "id": "MDRW2100013693.1.2",
          "form": "네 점심 뭐드셨나요",
          "original_form": "네 점심 뭐드셨나요",
          "speaker_id": "2",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013693.1.3",
          "form": "저 파스타 먹었어요ㅋㅋ",
          "original_form": "저 파스타 먹었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013693.1.4",
          "form": "오 무슨 파스타요?",
          "original_form": "오 무슨 파스타요?",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013693.1.5",
          "form": "크림파스타 만들어 먹었어요ㅋㅋ",
          "original_form": "크림파스타 만들어 먹었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013693.1.6",
          "form": "솜씨가 좋으시네요^^",
          "original_form": "솜씨가 좋으시네요^^",
          "speaker_id": "2",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013693.1.7",
          "form": "감사합니다ㅋㅋ파스타는 만들기 간단해서요ㅋㅋ",
          "original_form": "감사합니다ㅋㅋ파스타는 만들기 간단해서요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013693.1.8",
          "form": "맞아요~ 아이들도 크림파스타 좋아하더라구요",
          "original_form": "맞아요~ 아이들도 크림파스타 좋아하더라구요",
          "speaker_id": "2",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013693.1.9",
          "form": "ㅋㅋ우유랑 비슷하게 느낄 것 같아요",
          "original_form": "ㅋㅋ우유랑 비슷하게 느낄 것 같아요",
          "speaker_id": "1",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013693.1.10",
          "form": "큰아이는 토마토 파스타는 매운색깔이라고 안먹어요 ..ㅋ",
          "original_form": "큰아이는 토마토 파스타는 매운색깔이라고 안먹어요 ..ㅋ",
          "speaker_id": "2",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013693.1.11",
          "form": "ㅋㅋ아이 눈에는 그렇게 보일수도 있겠네요",
          "original_form": "ㅋㅋ아이 눈에는 그렇게 보일수도 있겠네요",
          "speaker_id": "1",
          "time": "20210825 15:57"
        },
        {
          "id": "MDRW2100013693.1.12",
          "form": "토마토 파스타도 새콤한 맛이 맛있는데요ㅎㅎ",
          "original_form": "토마토 파스타도 새콤한 맛이 맛있는데요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 15:57"
        },
        {
          "id": "MDRW2100013693.1.13",
          "form": "그러게요~ 골고루 잘 먹으면 좋을텐데요~",
          "original_form": "그러게요~ 골고루 잘 먹으면 좋을텐데요~",
          "speaker_id": "2",
          "time": "20210825 15:59"
        },
        {
          "id": "MDRW2100013693.1.14",
          "form": "그러게요~ 아이가 좋아하는 다른 거 섞어주면 어때요?",
          "original_form": "그러게요~ 아이가 좋아하는 다른 거 섞어주면 어때요?",
          "speaker_id": "1",
