      "utterance": [
        {
          "id": "MDRW2100007785.1.1",
          "form": "프린세스메이커인데",
          "original_form": "프린세스메이커인데",
          "speaker_id": "2",
          "time": "20210822 10:43"
        },
        {
          "id": "MDRW2100007785.1.2",
          "form": "정작 왕자랑 결혼시키기 왤게 어려운지 ㅋㅋ",
          "original_form": "정작 왕자랑 결혼시키기 왤게 어려운지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:44"
        },
        {
          "id": "MDRW2100007785.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 아 진엔딩은 왕자랑 결혼인가요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 아 진엔딩은 왕자랑 결혼인가요?",
          "speaker_id": "1",
          "time": "20210822 10:44"
        },
        {
          "id": "MDRW2100007785.1.4",
          "form": "쉽지않네요 왕자가 어떤인간인지도 모르곸ㅋㅋ",
          "original_form": "쉽지않네요 왕자가 어떤인간인지도 모르곸ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:44"
        },
        {
          "id": "MDRW2100007785.1.5",
          "form": "사실 최종목표? 는그거죠",
          "original_form": "사실 최종목표? 는그거죠",
          "speaker_id": "2",
          "time": "20210822 10:44"
        },
        {
          "id": "MDRW2100007785.1.6",
          "form": "겜 제목이 퀸메이커였어야했나",
          "original_form": "겜 제목이 퀸메이커였어야했나",
          "speaker_id": "2",
          "time": "20210822 10:45"
        },
        {
          "id": "MDRW2100007785.1.7",
          "form": "그러게요 이미 공주로 태어나지 못한것을..",
          "original_form": "그러게요 이미 공주로 태어나지 못한것을..",
          "speaker_id": "1",
          "time": "20210822 10:45"
        },
        {
          "id": "MDRW2100007785.1.8",
          "form": "왕자랑 결혼시킨 적 있으세요?",
          "original_form": "왕자랑 결혼시킨 적 있으세요?",
          "speaker_id": "1",
          "time": "20210822 10:45"
        },
        {
          "id": "MDRW2100007785.1.9",
          "form": "키울때 공주처럼 키우란 뜻이었나봐요",
          "original_form": "키울때 공주처럼 키우란 뜻이었나봐요",
          "speaker_id": "2",
          "time": "20210822 10:45"
        },
        {
          "id": "MDRW2100007785.1.10",
          "form": "아뇨 ㅜㅜ",
          "original_form": "아뇨 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 10:46"
        },
        {
          "id": "MDRW2100007785.1.11",
          "form": "진짜 어려운 게임인데요?ㅋㅋㅋㅋㅋㅋ",
          "original_form": "진짜 어려운 게임인데요?ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:47"
        },
        {
          "id": "MDRW2100007785.1.12",
          "form": "공주처럼 키우면.. 아빠랑 결혼하고..",
          "original_form": "공주처럼 키우면.. 아빠랑 결혼하고..",
          "speaker_id": "1",
          "time": "20210822 10:47"
        },
        {
          "id": "MDRW2100007785.1.13",
          "form": "그만큼 게임 엔딩이 다양했죠 ㅋㅋ",
          "original_form": "그만큼 게임 엔딩이 다양했죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:47"
        },
        {
          "id": "MDRW2100007785.1.14",
          "form": "되게 잘 만든 게임이네요",
          "original_form": "되게 잘 만든 게임이네요",
          "speaker_id": "1",
          "time": "20210822 10:48"
        },
        {
          "id": "MDRW2100007785.1.15",
          "form": "그래서 유명하구나..싶은",
          "original_form": "그래서 유명하구나..싶은",
          "speaker_id": "1",
          "time": "20210822 10:48"
        },
        {
          "id": "MDRW2100007785.1.16",
          "form": "지금도 아쉬운건",
          "original_form": "지금도 아쉬운건",
          "speaker_id": "2",
          "time": "20210822 10:48"
        },
        {
          "id": "MDRW2100007785.1.17",
          "form": "돈부족하지않게 풍족하게 키웠지만",
          "original_form": "돈부족하지않게 풍족하게 키웠지만",
          "speaker_id": "2",
          "time": "20210822 10:48"
        },
        {
          "id": "MDRW2100007785.1.18",
          "form": "자꾸 가정주부가되거나",
          "original_form": "자꾸 가정주부가되거나",
          "speaker_id": "2",
          "time": "20210822 10:48"
        },
        {
          "id": "MDRW2100007785.1.19",
          "form": "그냥평범한독신녀가되었던 기억이 ㅋㅋㅋ",
          "original_form": "그냥평범한독신녀가되었던 기억이 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:48"
        },
        {
          "id": "MDRW2100007785.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아이거 너무 웃픈데요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아이거 너무 웃픈데요",
          "speaker_id": "1",
          "time": "20210822 10:49"
        },
        {
          "id": "MDRW2100007785.1.21",
          "form": "좀 모자란 환경에 있어야 사람이 발전을 하는짘ㅋㅋㅋ",
          "original_form": "좀 모자란 환경에 있어야 사람이 발전을 하는짘ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:49"
        },
        {
          "id": "MDRW2100007785.1.22",
          "form": "그런가봐요 ㅋㅋ",
          "original_form": "그런가봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:51"
        },
        {
          "id": "MDRW2100007785.1.23",
          "form": "그래도 재미있었어요",
          "original_form": "그래도 재미있었어요",
          "speaker_id": "2",
          "time": "20210822 10:51"
        },
        {
          "id": "MDRW2100007785.1.24",
          "form": "제가 환장했던건 2였는데 언제보니 프린세스메이커5도 나왔더라고요",
          "original_form": "제가 환장했던건 2였는데 언제보니 프린세스메이커5도 나왔더라고요",
          "speaker_id": "2",
          "time": "20210822 10:51"
        },
        {
          "id": "MDRW2100007785.1.25",
          "form": "와 그럼 12345 다 있는거에요?",
          "original_form": "와 그럼 12345 다 있는거에요?",
          "speaker_id": "1",
          "time": "20210822 10:58"
        },
        {
          "id": "MDRW2100007785.1.26",
          "form": "너무 재밌을거같은뎈ㅋㅋㅋ 저도 해봐야겠어요",
          "original_form": "너무 재밌을거같은뎈ㅋㅋㅋ 저도 해봐야겠어요",
          "speaker_id": "1",
          "time": "20210822 10:58"
        },
        {
          "id": "MDRW2100007785.1.27",
          "form": "5는 엔딩이 또 다르려나요",
          "original_form": "5는 엔딩이 또 다르려나요",
          "speaker_id": "1",
          "time": "20210822 10:58"
        },
        {
          "id": "MDRW2100007785.1.28",
          "form": "모든 엔딩은 일단 딸의 직업 딸의 결혼 이니까 비슷할지도요",
          "original_form": "모든 엔딩은 일단 딸의 직업 딸의 결혼 이니까 비슷할지도요",
          "speaker_id": "2",
          "time": "20210822 10:59"
        },
        {
          "id": "MDRW2100007785.1.29",
          "form": "좀더 화질이 좋아지거나 기능은 추가되었을수도요",
          "original_form": "좀더 화질이 좋아지거나 기능은 추가되었을수도요",
          "speaker_id": "2",
          "time": "20210822 11:00"
        },
        {
          "id": "MDRW2100007785.1.30",
          "form": "저는 딸이 좀 좋은 직업 가졌으면 좋겠는데",
          "original_form": "저는 딸이 좀 좋은 직업 가졌으면 좋겠는데",
          "speaker_id": "1",
          "time": "20210822 11:03"
        },
        {
          "id": "MDRW2100007785.1.31",
          "form": "자꾸 결혼하고 주부되면 진짜 따흑ㅋㅋㅋ ㅜㅜ",
          "original_form": "자꾸 결혼하고 주부되면 진짜 따흑ㅋㅋㅋ ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 11:03"
        },
        {
          "id": "MDRW2100007785.1.32",
          "form": "저도 그게 고민이었어요 ㅋㅋ",
          "original_form": "저도 그게 고민이었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:05"
        },
        {
          "id": "MDRW2100007785.1.33",
          "form": "돈많이들여 공부 시켰는데",
          "original_form": "돈많이들여 공부 시켰는데",
          "speaker_id": "2",
          "time": "20210822 11:06"
        },
        {
          "id": "MDRW2100007785.1.34",
          "form": "왜자꾸 취집을 ㅋㅋㅋ",
          "original_form": "왜자꾸 취집을 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:06"
        },
        {
          "id": "MDRW2100007785.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 아 취집엔딩 그것도 평범남이랑..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 아 취집엔딩 그것도 평범남이랑..",
