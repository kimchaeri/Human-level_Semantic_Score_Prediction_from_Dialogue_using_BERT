      "utterance": [
        {
          "id": "MDRW2100003917.1.1",
          "form": "점심 뭐 드셨어요?",
          "original_form": "점심 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210821 13:49"
        },
        {
          "id": "MDRW2100003917.1.2",
          "form": "지금 시켜놓고 기다리는중이에요 name2님은여?",
          "original_form": "지금 시켜놓고 기다리는중이에요 &name2&님은여?",
          "speaker_id": "1",
          "time": "20210821 13:53"
        },
        {
          "id": "MDRW2100003917.1.3",
          "form": "전 보쌈 먹었어요",
          "original_form": "전 보쌈 먹었어요",
          "speaker_id": "2",
          "time": "20210821 13:53"
        },
        {
          "id": "MDRW2100003917.1.4",
          "form": "아맞다 ㅋㅋㅋ",
          "original_form": "아맞다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 13:54"
        },
        {
          "id": "MDRW2100003917.1.5",
          "form": "name2님은 보쌈파세요 족발파세요?",
          "original_form": "&name2&님은 보쌈파세요 족발파세요?",
          "speaker_id": "1",
          "time": "20210821 13:54"
        },
        {
          "id": "MDRW2100003917.1.6",
          "form": "저는 족발이 더 좋아요",
          "original_form": "저는 족발이 더 좋아요",
          "speaker_id": "2",
          "time": "20210821 13:54"
        },
        {
          "id": "MDRW2100003917.1.7",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210821 13:54"
        },
        {
          "id": "MDRW2100003917.1.8",
          "form": "저도 족발을더좋아해요 ㅋㅋㅋ쫄깃하고 부드러워서요",
          "original_form": "저도 족발을더좋아해요 ㅋㅋㅋ쫄깃하고 부드러워서요",
          "speaker_id": "1",
          "time": "20210821 13:55"
        },
        {
          "id": "MDRW2100003917.1.9",
          "form": "물론보쌈도 없어서못먹지만",
          "original_form": "물론보쌈도 없어서못먹지만",
          "speaker_id": "1",
          "time": "20210821 13:55"
        },
        {
          "id": "MDRW2100003917.1.10",
          "form": "맞아요 족발이 쫄깃하거",
          "original_form": "맞아요 족발이 쫄깃하거",
          "speaker_id": "2",
          "time": "20210821 13:56"
        },
        {
          "id": "MDRW2100003917.1.11",
          "form": "콜라겐도 뿜뿜이라 ㅎ",
          "original_form": "콜라겐도 뿜뿜이라 ㅎ",
          "speaker_id": "2",
          "time": "20210821 13:56"
        },
        {
          "id": "MDRW2100003917.1.12",
          "form": "맞아요 먹으면서 피부에도 좋겠지하면서 ㅋㅋㅋㅋ자기위안도하고요",
          "original_form": "맞아요 먹으면서 피부에도 좋겠지하면서 ㅋㅋㅋㅋ자기위안도하고요",
          "speaker_id": "1",
          "time": "20210821 13:57"
        },
        {
          "id": "MDRW2100003917.1.13",
          "form": "저도 그걸 위안 삼으면 먹는답니다ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 그걸 위안 삼으면 먹는답니다ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 13:57"
        },
        {
          "id": "MDRW2100003917.1.14",
          "form": "ㅋㅋㅋㅋ맞아요 이왕먹는거",
          "original_form": "ㅋㅋㅋㅋ맞아요 이왕먹는거",
          "speaker_id": "1",
          "time": "20210821 13:58"
        },
        {
          "id": "MDRW2100003917.1.15",
          "form": "사시는지역에 유명한맛집같은곳있어요? 지역특색같은",
          "original_form": "사시는지역에 유명한맛집같은곳있어요? 지역특색같은",
          "speaker_id": "1",
          "time": "20210821 13:58"
        },
        {
          "id": "MDRW2100003917.1.16",
          "form": "저희지역은 냉면유명해요",
          "original_form": "저희지역은 냉면유명해요",
          "speaker_id": "1",
          "time": "20210821 13:58"
        },
        {
          "id": "MDRW2100003917.1.17",
          "form": "우와 냉면이요? 엄청 맛있겠어요",
          "original_form": "우와 냉면이요? 엄청 맛있겠어요",
          "speaker_id": "2",
          "time": "20210821 13:59"
        },
        {
          "id": "MDRW2100003917.1.18",
          "form": "저희는 한식이요!!!",
          "original_form": "저희는 한식이요!!!",
          "speaker_id": "2",
          "time": "20210821 13:59"
        },
        {
          "id": "MDRW2100003917.1.19",
          "form": "여름만되면 불티나요 ㅎㅎ요번에는 시켜먹었네요",
          "original_form": "여름만되면 불티나요 ㅎㅎ요번에는 시켜먹었네요",
          "speaker_id": "1",
          "time": "20210821 13:59"
        },
        {
          "id": "MDRW2100003917.1.20",
