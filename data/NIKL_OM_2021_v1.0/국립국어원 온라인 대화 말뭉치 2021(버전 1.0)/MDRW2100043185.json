      "utterance": [
        {
          "id": "MDRW2100043185.1.1",
          "form": "왜 아침에 학원다니려니 힘들어???",
          "original_form": "왜 아침에 학원다니려니 힘들어???",
          "speaker_id": "2",
          "time": "20210907 16:55"
        },
        {
          "id": "MDRW2100043185.1.2",
          "form": "와 방금전에 넘 피곤하니까 10분만 자야겠다하고 엎드렸는데",
          "original_form": "와 방금전에 넘 피곤하니까 10분만 자야겠다하고 엎드렸는데",
          "speaker_id": "1",
          "time": "20210907 16:57"
        },
        {
          "id": "MDRW2100043185.1.3",
          "form": "1시간동안 침흘리면서 자고있었어",
          "original_form": "1시간동안 침흘리면서 자고있었어",
          "speaker_id": "1",
          "time": "20210907 16:58"
        },
        {
          "id": "MDRW2100043185.1.4",
          "form": "신나게 잤구먀",
          "original_form": "신나게 잤구먀",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100043185.1.5",
          "form": "그럴 의도는 아니었는데ㅠ",
          "original_form": "그럴 의도는 아니었는데ㅠ",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100043185.1.6",
          "form": "오늘은 좀 일찍자",
          "original_form": "오늘은 좀 일찍자",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100043185.1.7",
          "form": "그럴려구 너무 피곤해서 잇몸이 부었어",
          "original_form": "그럴려구 너무 피곤해서 잇몸이 부었어",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100043185.1.8",
          "form": "백신 맞은날 쌩쌩하고 좋았는데",
          "original_form": "백신 맞은날 쌩쌩하고 좋았는데",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100043185.1.9",
          "form": "백신이 영양제였네",
          "original_form": "백신이 영양제였네",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100043185.1.10",
          "form": "2차가 더 아프다던대",
          "original_form": "2차가 더 아프다던대",
          "speaker_id": "1",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100043185.1.11",
          "form": "2차도 맞으면 쌩쌩해질까",
          "original_form": "2차도 맞으면 쌩쌩해질까",
          "speaker_id": "1",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100043185.1.12",
          "form": "2차가 훨씬 아프대",
          "original_form": "2차가 훨씬 아프대",
          "speaker_id": "2",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100043185.1.13",
          "form": "열도 엄청 나고 힘들다던대",
          "original_form": "열도 엄청 나고 힘들다던대",
          "speaker_id": "2",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100043185.1.14",
          "form": "2차맞는날 학원가는날인데",
          "original_form": "2차맞는날 학원가는날인데",
          "speaker_id": "1",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100043185.1.15",
          "form": "다음날 학원갈 수 있을까",
          "original_form": "다음날 학원갈 수 있을까",
          "speaker_id": "1",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100043185.1.16",
          "form": "아프면 못가는거지 뭐",
          "original_form": "아프면 못가는거지 뭐",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100043185.1.17",
          "form": "신나면서 슬픈마음",
          "original_form": "신나면서 슬픈마음",
          "speaker_id": "1",
