      "utterance": [
        {
          "id": "MDRW2100036829.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210831 13:08"
        },
        {
          "id": "MDRW2100036829.1.2",
          "form": "식사하셨나요?",
          "original_form": "식사하셨나요?",
          "speaker_id": "1",
          "time": "20210831 13:08"
        },
        {
          "id": "MDRW2100036829.1.3",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210831 13:09"
        },
        {
          "id": "MDRW2100036829.1.4",
          "form": "네네^^ 식사하셨어요?",
          "original_form": "네네^^ 식사하셨어요?",
          "speaker_id": "2",
          "time": "20210831 13:09"
        },
        {
          "id": "MDRW2100036829.1.5",
          "form": "네 저는 구내식당에서 먹었어요",
          "original_form": "네 저는 구내식당에서 먹었어요",
          "speaker_id": "1",
          "time": "20210831 13:09"
        },
        {
          "id": "MDRW2100036829.1.6",
          "form": "같이 먹은 남자직원들이 툴툴대더라구요",
          "original_form": "같이 먹은 남자직원들이 툴툴대더라구요",
          "speaker_id": "1",
          "time": "20210831 13:09"
        },
        {
          "id": "MDRW2100036829.1.7",
          "form": "오 구내식당 좋네요",
          "original_form": "오 구내식당 좋네요",
          "speaker_id": "2",
          "time": "20210831 13:09"
        },
        {
          "id": "MDRW2100036829.1.8",
          "form": "다이어트식단같이 나온다고 ㅋ",
          "original_form": "다이어트식단같이 나온다고 ㅋ",
          "speaker_id": "1",
          "time": "20210831 13:09"
        },
        {
          "id": "MDRW2100036829.1.9",
          "form": "헉 메뉴가 별로였나요",
          "original_form": "헉 메뉴가 별로였나요",
          "speaker_id": "2",
          "time": "20210831 13:09"
        },
        {
          "id": "MDRW2100036829.1.10",
          "form": "ㅌㅋㅋㅋㅋ아 ㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋ아 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:09"
        },
        {
          "id": "MDRW2100036829.1.11",
          "form": "저는 구내식당이 없어서ㅠㅠ 맛없어도 좋으니까 생겼으면 좋겠어요!!",
          "original_form": "저는 구내식당이 없어서ㅠㅠ 맛없어도 좋으니까 생겼으면 좋겠어요!!",
          "speaker_id": "2",
          "time": "20210831 13:10"
        },
        {
          "id": "MDRW2100036829.1.12",
          "form": "두부조림 참치양파샐러드 김치 종류",
          "original_form": "두부조림 참치양파샐러드 김치 종류",
          "speaker_id": "1",
          "time": "20210831 13:10"
        },
        {
          "id": "MDRW2100036829.1.13",
          "form": "닭계장이랑요",
          "original_form": "닭계장이랑요",
          "speaker_id": "1",
          "time": "20210831 13:10"
        },
        {
          "id": "MDRW2100036829.1.14",
          "form": "맞아요 있는게 나아요",
          "original_form": "맞아요 있는게 나아요",
          "speaker_id": "1",
          "time": "20210831 13:10"
        },
        {
          "id": "MDRW2100036829.1.15",
          "form": "고기반찬이 없어서인지 남자들은 안좋아하던데",
          "original_form": "고기반찬이 없어서인지 남자들은 안좋아하던데",
          "speaker_id": "1",
          "time": "20210831 13:11"
        },
        {
          "id": "MDRW2100036829.1.16",
          "form": "엥 맛있겠는데요",
          "original_form": "엥 맛있겠는데요",
          "speaker_id": "2",
          "time": "20210831 13:11"
        },
        {
          "id": "MDRW2100036829.1.17",
          "form": "저는 속도편하고 맛있게 먹고왔죠",
          "original_form": "저는 속도편하고 맛있게 먹고왔죠",
          "speaker_id": "1",
          "time": "20210831 13:11"
        },
        {
          "id": "MDRW2100036829.1.18",
          "form": "진짜 다 맛있는거만 있는데!!!",
          "original_form": "진짜 다 맛있는거만 있는데!!!",
          "speaker_id": "2",
          "time": "20210831 13:11"
        },
        {
          "id": "MDRW2100036829.1.19",
          "form": "남직원들이 배가 불렀나봐요",
          "original_form": "남직원들이 배가 불렀나봐요",
          "speaker_id": "2",
          "time": "20210831 13:11"
        },
        {
          "id": "MDRW2100036829.1.20",
          "form": "ㅋㅋㅋㅌㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅌㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:12"
        },
        {
          "id": "MDRW2100036829.1.21",
          "form": "양패살러드 첨먹어봤는데 맛있더라구요",
          "original_form": "양패살러드 첨먹어봤는데 맛있더라구요",
          "speaker_id": "1",
          "time": "20210831 13:12"
        },
        {
          "id": "MDRW2100036829.1.22",
          "form": "그 샐러드용 양파가 아삭아삭하고 안맵고 좋더라구요",
          "original_form": "그 샐러드용 양파가 아삭아삭하고 안맵고 좋더라구요",
          "speaker_id": "2",
          "time": "20210831 13:12"
        },
        {
          "id": "MDRW2100036829.1.23",
          "form": "아 샐러드용양파가 따로있어요?",
          "original_form": "아 샐러드용양파가 따로있어요?",
          "speaker_id": "1",
          "time": "20210831 13:12"
        },
        {
          "id": "MDRW2100036829.1.24",
          "form": "어쩐지 맵지도않다했어요",
          "original_form": "어쩐지 맵지도않다했어요",
          "speaker_id": "1",
          "time": "20210831 13:13"
        },
        {
          "id": "MDRW2100036829.1.25",
          "form": "네네 저도 최근에 알았어요",
          "original_form": "네네 저도 최근에 알았어요",
          "speaker_id": "2",
          "time": "20210831 13:14"
        },
        {
          "id": "MDRW2100036829.1.26",
          "form": "요즘엔 남자들도 샐러드로 식사하는 경우 꽤 있던데 안좋아하는 직원분들인가봐요",
          "original_form": "요즘엔 남자들도 샐러드로 식사하는 경우 꽤 있던데 안좋아하는 직원분들인가봐요",
          "speaker_id": "2",
          "time": "20210831 13:23"
        },
        {
          "id": "MDRW2100036829.1.27",
          "form": "고기반찬같은 자기입맛에 맞는게 안나와서 그런듯요",
          "original_form": "고기반찬같은 자기입맛에 맞는게 안나와서 그런듯요",
          "speaker_id": "1",
          "time": "20210831 13:24"
        },
        {
          "id": "MDRW2100036829.1.28",
          "form": "풀떼기만 나온다고 ㅎ",
          "original_form": "풀떼기만 나온다고 ㅎ",
          "speaker_id": "1",
          "time": "20210831 13:24"
        },
        {
          "id": "MDRW2100036829.1.29",
          "form": "ㅋㅋㅋㅋ고기가 맛있긴하죠",
          "original_form": "ㅋㅋㅋㅋ고기가 맛있긴하죠",
          "speaker_id": "2",
          "time": "20210831 13:25"
        },
        {
          "id": "MDRW2100036829.1.30",
          "form": "근데 요즘은 점점 깔끔하게 먹는 추세가 된 것 같아요",
          "original_form": "근데 요즘은 점점 깔끔하게 먹는 추세가 된 것 같아요",
          "speaker_id": "2",
          "time": "20210831 13:25"
        },
        {
          "id": "MDRW2100036829.1.31",
          "form": "맞아요 건강생각해서요",
          "original_form": "맞아요 건강생각해서요",
          "speaker_id": "1",
          "time": "20210831 13:56"
        }
      ]
    }
