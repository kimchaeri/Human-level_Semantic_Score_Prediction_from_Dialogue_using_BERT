      "utterance": [
        {
          "id": "MDRW2100006080.1.1",
          "form": "요리 하시나요?",
          "original_form": "요리 하시나요?",
          "speaker_id": "2",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100006080.1.2",
          "form": "가끔해요",
          "original_form": "가끔해요",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100006080.1.3",
          "form": "식재료 처리하려고",
          "original_form": "식재료 처리하려고",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100006080.1.4",
          "form": "얼마전에 집에 감자가 많아서 감자수프했고",
          "original_form": "얼마전에 집에 감자가 많아서 감자수프했고",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100006080.1.5",
          "form": "최근에는 두부가 유통기한 지나서",
          "original_form": "최근에는 두부가 유통기한 지나서",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100006080.1.6",
          "form": "두부강정 만들엇어요",
          "original_form": "두부강정 만들엇어요",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100006080.1.7",
          "form": "name2님 요리 자주 하세요?",
          "original_form": "&name2&님 요리 자주 하세요?",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100006080.1.8",
          "form": "간단간단한것만 자주 해요 ㅋㅋㅋㅋㅋ",
          "original_form": "간단간단한것만 자주 해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100006080.1.9",
          "form": "와 두부강정",
          "original_form": "와 두부강정",
          "speaker_id": "2",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100006080.1.10",
          "form": "요리잘하시나봐요",
          "original_form": "요리잘하시나봐요",
          "speaker_id": "2",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100006080.1.11",
          "form": "아뇨 안그래요",
          "original_form": "아뇨 안그래요",
          "speaker_id": "1",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100006080.1.12",
          "form": "유튜브보고 따라했어요",
          "original_form": "유튜브보고 따라했어요",
          "speaker_id": "1",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100006080.1.13",
          "form": "그대로 따라하는것도 어려운걸요 ㅋㅋㅋㅋㅋ",
          "original_form": "그대로 따라하는것도 어려운걸요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100006080.1.14",
          "form": "그런가요ㅎㅎ",
          "original_form": "그런가요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100006080.1.15",
          "form": "간단한 요리는 어떤거에요?",
          "original_form": "간단한 요리는 어떤거에요?",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100006080.1.16",
          "form": "라면? 계란후라이?",
          "original_form": "라면? 계란후라이?",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100006080.1.17",
          "form": "김치볶음밥이나 어묵볶음 같은정도요 ㅋㅋㅋㅋ",
          "original_form": "김치볶음밥이나 어묵볶음 같은정도요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100006080.1.18",
          "form": "그정도면 훌륭하죠",
          "original_form": "그정도면 훌륭하죠",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100006080.1.19",
          "form": "맛이 어댔어요?",
          "original_form": "맛이 어댔어요?",
          "speaker_id": "1",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100006080.1.20",
          "form": "조미료의 힘으로 맛은 나쁘지 않앗어욬ㅋㅋㅋㅋ",
          "original_form": "조미료의 힘으로 맛은 나쁘지 않앗어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100006080.1.21",
          "form": "조미료를 지배하는 사람이 요리를 지배하죠",
          "original_form": "조미료를 지배하는 사람이 요리를 지배하죠",
          "speaker_id": "1",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100006080.1.22",
          "form": "그리고 조미료랑 건강이랑 관련이 없대요",
          "original_form": "그리고 조미료랑 건강이랑 관련이 없대요",
          "speaker_id": "1",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100006080.1.23",
          "form": "많이 써도 돼요",
          "original_form": "많이 써도 돼요",
          "speaker_id": "1",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100006080.1.24",
          "form": "조미료를 쓰지 않고 맛을 낼 정도의 실력이 없어요 ㅋㅋㅋㅋ",
          "original_form": "조미료를 쓰지 않고 맛을 낼 정도의 실력이 없어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100006080.1.25",
          "form": "기술의 힘을 이용하는건 나쁜게 아니라 생각해요",
          "original_form": "기술의 힘을 이용하는건 나쁜게 아니라 생각해요",
          "speaker_id": "1",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100006080.1.26",
          "form": "ㅋㅋㅋㅋㅋ맞아요 제겐 구세주입니다",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 제겐 구세주입니다",
