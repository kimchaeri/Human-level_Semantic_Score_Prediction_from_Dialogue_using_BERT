      "utterance": [
        {
          "id": "MDRW2100030611.1.1",
          "form": "동물키우십니까",
          "original_form": "동물키우십니까",
          "speaker_id": "1",
          "time": "20210904 10:13"
        },
        {
          "id": "MDRW2100030611.1.2",
          "form": "아니요 저는 동물 무서워해요",
          "original_form": "아니요 저는 동물 무서워해요",
          "speaker_id": "2",
          "time": "20210904 10:14"
        },
        {
          "id": "MDRW2100030611.1.3",
          "form": "동물 키우시나요",
          "original_form": "동물 키우시나요",
          "speaker_id": "2",
          "time": "20210904 10:14"
        },
        {
          "id": "MDRW2100030611.1.4",
          "form": "고양이 두마리 키워요",
          "original_form": "고양이 두마리 키워요",
          "speaker_id": "1",
          "time": "20210904 10:15"
        },
        {
          "id": "MDRW2100030611.1.5",
          "form": "똥괭이들",
          "original_form": "똥괭이들",
          "speaker_id": "1",
          "time": "20210904 10:15"
        },
        {
          "id": "MDRW2100030611.1.6",
          "form": "헐헐 두마리나 ㅠㅠㅠㅠ",
          "original_form": "헐헐 두마리나 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 10:16"
        },
        {
          "id": "MDRW2100030611.1.7",
          "form": "키운지얼마나 되셨나요?",
          "original_form": "키운지얼마나 되셨나요?",
          "speaker_id": "2",
          "time": "20210904 10:16"
        },
        {
          "id": "MDRW2100030611.1.8",
          "form": "1년됬어요",
          "original_form": "1년됬어요",
          "speaker_id": "1",
          "time": "20210904 10:26"
        },
        {
          "id": "MDRW2100030611.1.9",
          "form": "한마리 키우려다가",
          "original_form": "한마리 키우려다가",
          "speaker_id": "1",
          "time": "20210904 10:26"
        },
        {
          "id": "MDRW2100030611.1.10",
          "form": "얘네도 외로움을타서",
          "original_form": "얘네도 외로움을타서",
          "speaker_id": "1",
          "time": "20210904 10:26"
        },
        {
          "id": "MDRW2100030611.1.11",
          "form": "오 그래도 둘이 잘 맞았나봐요 둘키우면 엄청싸우는 둘도 있다던데 다행이에요",
          "original_form": "오 그래도 둘이 잘 맞았나봐요 둘키우면 엄청싸우는 둘도 있다던데 다행이에요",
          "speaker_id": "2",
          "time": "20210904 10:27"
        },
        {
          "id": "MDRW2100030611.1.12",
          "form": "제가 있는 공간은 공산주의라",
          "original_form": "제가 있는 공간은 공산주의라",
          "speaker_id": "1",
          "time": "20210904 10:27"
        },
        {
          "id": "MDRW2100030611.1.13",
          "form": "분란이 일어나면 식량배급이 안되서 안싸웁니다",
          "original_form": "분란이 일어나면 식량배급이 안되서 안싸웁니다",
          "speaker_id": "1",
          "time": "20210904 10:27"
        },
        {
          "id": "MDRW2100030611.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ너무웃기네요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ너무웃기네요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 10:28"
        },
        {
          "id": "MDRW2100030611.1.15",
          "form": "식량배급에 사이좋아진 그들....",
          "original_form": "식량배급에 사이좋아진 그들....",
          "speaker_id": "2",
          "time": "20210904 10:28"
        },
        {
          "id": "MDRW2100030611.1.16",
          "form": "전 애들 말안듣고 사고치면",
          "original_form": "전 애들 말안듣고 사고치면",
          "speaker_id": "1",
          "time": "20210904 10:29"
        },
        {
          "id": "MDRW2100030611.1.17",
          "form": "안혼내고 밥을 조금줘요",
          "original_form": "안혼내고 밥을 조금줘요",
          "speaker_id": "1",
          "time": "20210904 10:29"
        },
        {
          "id": "MDRW2100030611.1.18",
          "form": "그럼 말잘듣습니다",
          "original_form": "그럼 말잘듣습니다",
          "speaker_id": "1",
          "time": "20210904 10:29"
        },
        {
          "id": "MDRW2100030611.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 진짜웃기다 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 좋은 학습법이네요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 진짜웃기다 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 좋은 학습법이네요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 10:32"
        },
        {
          "id": "MDRW2100030611.1.20",
          "form": "사람도 말귀를 못알아듣는데",
          "original_form": "사람도 말귀를 못알아듣는데",
          "speaker_id": "1",
          "time": "20210904 10:33"
        },
        {
          "id": "MDRW2100030611.1.21",
          "form": "짐승은 오죽하겠어요",
          "original_form": "짐승은 오죽하겠어요",
          "speaker_id": "1",
          "time": "20210904 10:33"
        },
        {
          "id": "MDRW2100030611.1.22",
          "form": "그럼 말을 잘듣게 길러야져",
          "original_form": "그럼 말을 잘듣게 길러야져",
          "speaker_id": "1",
          "time": "20210904 10:33"
        },
        {
          "id": "MDRW2100030611.1.23",
          "form": "ㅋㅋ안혼내고 잘 키우시네요 ㅋㅋㅋ 애들은 어떻게 만나게 되셨어요?",
          "original_form": "ㅋㅋ안혼내고 잘 키우시네요 ㅋㅋㅋ 애들은 어떻게 만나게 되셨어요?",
          "speaker_id": "2",
          "time": "20210904 10:40"
        },
        {
          "id": "MDRW2100030611.1.24",
          "form": "1층에 있는 사장님이 못키울거같아서 입양했어요",
          "original_form": "1층에 있는 사장님이 못키울거같아서 입양했어요",
          "speaker_id": "1",
          "time": "20210904 10:50"
        },
        {
          "id": "MDRW2100030611.1.25",
          "form": "개2 고2 키우더라구요",
          "original_form": "개2 고2 키우더라구요",
          "speaker_id": "1",
          "time": "20210904 10:50"
        },
        {
          "id": "MDRW2100030611.1.26",
          "form": "귀연놈들",
          "original_form": "귀연놈들",
          "speaker_id": "1",
          "time": "20210904 10:50"
        }
      ]
    }
