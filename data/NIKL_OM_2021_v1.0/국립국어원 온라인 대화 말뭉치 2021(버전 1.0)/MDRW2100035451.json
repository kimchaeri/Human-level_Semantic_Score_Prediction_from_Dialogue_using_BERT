      "utterance": [
        {
          "id": "MDRW2100035451.1.1",
          "form": "게스트 하우스에서",
          "original_form": "게스트 하우스에서",
          "speaker_id": "1",
          "time": "20210829 17:37"
        },
        {
          "id": "MDRW2100035451.1.2",
          "form": "묵어보셨어요?",
          "original_form": "묵어보셨어요?",
          "speaker_id": "1",
          "time": "20210829 17:37"
        },
        {
          "id": "MDRW2100035451.1.3",
          "form": "제주도 여행 갔을 때 가봤어요",
          "original_form": "제주도 여행 갔을 때 가봤어요",
          "speaker_id": "2",
          "time": "20210829 17:40"
        },
        {
          "id": "MDRW2100035451.1.4",
          "form": "거기 묵는 사람들끼리 친해져요? 저는 안가봐서요",
          "original_form": "거기 묵는 사람들끼리 친해져요? 저는 안가봐서요",
          "speaker_id": "1",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100035451.1.5",
          "form": "음.. 그 때 어떤 사람들이 묵느냐에 따라 다른 것 같아요!",
          "original_form": "음.. 그 때 어떤 사람들이 묵느냐에 따라 다른 것 같아요!",
          "speaker_id": "2",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100035451.1.6",
          "form": "공용공간에 안 나오시는 분들도 많았거든요",
          "original_form": "공용공간에 안 나오시는 분들도 많았거든요",
          "speaker_id": "2",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100035451.1.7",
          "form": "아아 그렇군요...",
          "original_form": "아아 그렇군요...",
          "speaker_id": "1",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100035451.1.8",
          "form": "광고하는거랑은 다르네요",
          "original_form": "광고하는거랑은 다르네요",
          "speaker_id": "1",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100035451.1.9",
          "form": "요즘은 코로나 때문에 게하 운영해도",
          "original_form": "요즘은 코로나 때문에 게하 운영해도",
          "speaker_id": "2",
          "time": "20210829 17:43"
        },
        {
          "id": "MDRW2100035451.1.10",
          "form": "공용공간에 아마 못 모이게 할거에요",
          "original_form": "공용공간에 아마 못 모이게 할거에요",
          "speaker_id": "2",
          "time": "20210829 17:43"
        },
        {
          "id": "MDRW2100035451.1.11",
          "form": "아 그렇네요 그렇겠어요...",
          "original_form": "아 그렇네요 그렇겠어요...",
          "speaker_id": "1",
          "time": "20210829 17:44"
        },
        {
          "id": "MDRW2100035451.1.12",
          "form": "나중에 사람 많은 게하 가시면 친해질 기회가 많지 않을까요?",
          "original_form": "나중에 사람 많은 게하 가시면 친해질 기회가 많지 않을까요?",
          "speaker_id": "2",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100035451.1.13",
          "form": "네 그렇겠네요",
          "original_form": "네 그렇겠네요",
          "speaker_id": "1",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100035451.1.14",
          "form": "제주도 어디가 좋으셨어요?",
          "original_form": "제주도 어디가 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100035451.1.15",
          "form": "올레길 다니는게 좋았어요!",
          "original_form": "올레길 다니는게 좋았어요!",
          "speaker_id": "2",
          "time": "20210829 20:12"
        },
        {
          "id": "MDRW2100035451.1.16",
          "form": "바다 좋아하세요?",
          "original_form": "바다 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 20:12"
        },
        {
          "id": "MDRW2100035451.1.17",
          "form": "그럭저럭 좋아해요 ㅎㅎ",
          "original_form": "그럭저럭 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 20:20"
        },
        {
          "id": "MDRW2100035451.1.18",
          "form": "바다 보러 가면 되겠네요",
          "original_form": "바다 보러 가면 되겠네요",
          "speaker_id": "2",
          "time": "20210829 20:23"
        },
        {
          "id": "MDRW2100035451.1.19",
          "form": "네 바다 좋죠",
          "original_form": "네 바다 좋죠",
          "speaker_id": "1",
          "time": "20210829 20:26"
        }
      ]
