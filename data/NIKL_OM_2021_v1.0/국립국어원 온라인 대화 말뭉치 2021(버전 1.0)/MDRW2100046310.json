      "utterance": [
        {
          "id": "MDRW2100046310.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "1",
          "time": "20211019 15:46"
        },
        {
          "id": "MDRW2100046310.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211019 15:46"
        },
        {
          "id": "MDRW2100046310.1.3",
          "form": "여가 시간에는 주로 뭘하시나용",
          "original_form": "여가 시간에는 주로 뭘하시나용",
          "speaker_id": "2",
          "time": "20211019 15:46"
        },
        {
          "id": "MDRW2100046310.1.4",
          "form": "여가시간에 나는 저질 체력이라 잠을 자",
          "original_form": "여가시간에 나는 저질 체력이라 잠을 자",
          "speaker_id": "1",
          "time": "20211019 15:47"
        },
        {
          "id": "MDRW2100046310.1.5",
          "form": "잠이 최고긴하죠",
          "original_form": "잠이 최고긴하죠",
          "speaker_id": "2",
          "time": "20211019 15:47"
        },
        {
          "id": "MDRW2100046310.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211019 15:47"
        },
        {
          "id": "MDRW2100046310.1.7",
          "form": "그리고 책도 읽어",
          "original_form": "그리고 책도 읽어",
          "speaker_id": "1",
          "time": "20211019 15:47"
        },
        {
          "id": "MDRW2100046310.1.8",
          "form": "좋아하는 작가나 책 있으신가요??",
          "original_form": "좋아하는 작가나 책 있으신가요??",
          "speaker_id": "2",
          "time": "20211019 15:47"
        },
        {
          "id": "MDRW2100046310.1.9",
          "form": "좋아하는 작가는 따로 없는데",
          "original_form": "좋아하는 작가는 따로 없는데",
          "speaker_id": "1",
          "time": "20211019 15:48"
        },
        {
          "id": "MDRW2100046310.1.10",
          "form": "소설을 좋아해",
          "original_form": "소설을 좋아해",
          "speaker_id": "1",
          "time": "20211019 15:48"
        },
        {
          "id": "MDRW2100046310.1.11",
          "form": "정말 재밌게 읽은 소설 하나만 추천해주세요~",
          "original_form": "정말 재밌게 읽은 소설 하나만 추천해주세요~",
          "speaker_id": "2",
          "time": "20211019 15:48"
        },
        {
          "id": "MDRW2100046310.1.12",
          "form": "스릴러 좋아해?",
          "original_form": "스릴러 좋아해?",
          "speaker_id": "1",
          "time": "20211019 15:49"
        },
        {
          "id": "MDRW2100046310.1.13",
          "form": "돌이킬 수 없는 약속 읽어봐 시간 가는줄 몰라",
          "original_form": "돌이킬 수 없는 약속 읽어봐 시간 가는줄 몰라",
          "speaker_id": "1",
          "time": "20211019 15:49"
        },
        {
          "id": "MDRW2100046310.1.14",
          "form": "야쿠마루 가쿠꺼야",
          "original_form": "야쿠마루 가쿠꺼야",
          "speaker_id": "1",
          "time": "20211019 15:49"
        },
        {
          "id": "MDRW2100046310.1.15",
          "form": "오 메모해뒀다가 꼭 읽어볼게요",
          "original_form": "오 메모해뒀다가 꼭 읽어볼게요",
          "speaker_id": "2",
          "time": "20211019 15:49"
        },
        {
          "id": "MDRW2100046310.1.16",
          "form": "추천 감사합니다",
          "original_form": "추천 감사합니다",
          "speaker_id": "2",
          "time": "20211019 15:50"
        },
        {
          "id": "MDRW2100046310.1.17",
          "form": "제일 좋아하는 장르가 스릴러에요?",
          "original_form": "제일 좋아하는 장르가 스릴러에요?",
          "speaker_id": "2",
          "time": "20211019 15:50"
        },
        {
          "id": "MDRW2100046310.1.18",
          "form": "나는 재미있으면 다 좋아해 !",
          "original_form": "나는 재미있으면 다 좋아해 !",
          "speaker_id": "1",
          "time": "20211019 15:51"
        },
        {
          "id": "MDRW2100046310.1.19",
          "form": "오호 가리지 않고 읽으시는군요!",
          "original_form": "오호 가리지 않고 읽으시는군요!",
          "speaker_id": "2",
          "time": "20211019 15:51"
        }
      ]
