      "utterance": [
        {
          "id": "MDRW2100001991.1.1",
          "form": "여행 대박적으로 좋아합니다",
          "original_form": "여행 대박적으로 좋아합니다",
          "speaker_id": "2",
          "time": "20210819 22:41"
        },
        {
          "id": "MDRW2100001991.1.2",
          "form": "최애 여행지가.어디세요?",
          "original_form": "최애 여행지가.어디세요?",
          "speaker_id": "2",
          "time": "20210819 22:41"
        },
        {
          "id": "MDRW2100001991.1.3",
          "form": "저는 태국 정말 좋아해요 ㅜㅜ",
          "original_form": "저는 태국 정말 좋아해요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210819 22:43"
        },
        {
          "id": "MDRW2100001991.1.4",
          "form": "열번 물어도 열번 다 태국이요",
          "original_form": "열번 물어도 열번 다 태국이요",
          "speaker_id": "1",
          "time": "20210819 22:43"
        },
        {
          "id": "MDRW2100001991.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ태국 최애 도시가 어디세요? 저도 태국 좋아해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ태국 최애 도시가 어디세요? 저도 태국 좋아해요",
          "speaker_id": "2",
          "time": "20210819 22:44"
        },
        {
          "id": "MDRW2100001991.1.6",
          "form": "후아힌 아세요????",
          "original_form": "후아힌 아세요????",
          "speaker_id": "1",
          "time": "20210819 22:44"
        },
        {
          "id": "MDRW2100001991.1.7",
          "form": "여기 최애입니다!!!",
          "original_form": "여기 최애입니다!!!",
          "speaker_id": "1",
          "time": "20210819 22:44"
        },
        {
          "id": "MDRW2100001991.1.8",
          "form": "와 거기는 안가봤어요 저는 치앙마이에서 기차타고 내려오는 여행을 했었어서",
          "original_form": "와 거기는 안가봤어요 저는 치앙마이에서 기차타고 내려오는 여행을 했었어서",
          "speaker_id": "2",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100001991.1.9",
          "form": "후아힌은 어떤 이유로 젤 좋아하세요?",
          "original_form": "후아힌은 어떤 이유로 젤 좋아하세요?",
          "speaker_id": "2",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100001991.1.10",
          "form": "원래 치앙마이였다가 변경된거긴해요 ㅋㅋ",
          "original_form": "원래 치앙마이였다가 변경된거긴해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100001991.1.11",
          "form": "일단 멀리 들어가서 한국 사람이 거의 없고 조용하고 쉬기에 너무 좋았어요",
          "original_form": "일단 멀리 들어가서 한국 사람이 거의 없고 조용하고 쉬기에 너무 좋았어요",
          "speaker_id": "1",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100001991.1.12",
          "form": "방콕에도 있었는데 정말.. 시끄러웠던 ㅠㅠ",
          "original_form": "방콕에도 있었는데 정말.. 시끄러웠던 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100001991.1.13",
          "form": "저도 방콕은 너무 정신없어서 힘들더라구요",
          "original_form": "저도 방콕은 너무 정신없어서 힘들더라구요",
          "speaker_id": "2",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100001991.1.14",
          "form": "코로나 전에 가본 여행지가 어디세요?",
          "original_form": "코로나 전에 가본 여행지가 어디세요?",
          "speaker_id": "2",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100001991.1.15",
          "form": "19년도에 태국이랑 홍콩 다녀왔어요",
          "original_form": "19년도에 태국이랑 홍콩 다녀왔어요",
          "speaker_id": "1",
          "time": "20210819 22:56"
        },
        {
          "id": "MDRW2100001991.1.16",
          "form": "벌써 2년전이네요 ㅜㅜ",
          "original_form": "벌써 2년전이네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210819 22:56"
        },
        {
          "id": "MDRW2100001991.1.17",
          "form": "국내는 강원도나 부산을 자주 가요",
          "original_form": "국내는 강원도나 부산을 자주 가요",
          "speaker_id": "1",
          "time": "20210819 22:56"
        },
        {
          "id": "MDRW2100001991.1.18",
          "form": "저는 뉴질랜드로 워홀갔다 19년도에 돌아왔다가 한번도 못나갔네요",
          "original_form": "저는 뉴질랜드로 워홀갔다 19년도에 돌아왔다가 한번도 못나갔네요",
          "speaker_id": "2",
          "time": "20210819 22:58"
        },
        {
          "id": "MDRW2100001991.1.19",
          "form": "양양이 좋더라구요",
          "original_form": "양양이 좋더라구요",
          "speaker_id": "2",
          "time": "20210819 22:58"
        },
        {
          "id": "MDRW2100001991.1.20",
          "form": "강원도 최애 도시가 어디신가요",
          "original_form": "강원도 최애 도시가 어디신가요",
          "speaker_id": "2",
          "time": "20210819 22:58"
        },
        {
          "id": "MDRW2100001991.1.21",
          "form": "오 제 친구도 뉴질랜드 워홀 다녀왔어요",
          "original_form": "오 제 친구도 뉴질랜드 워홀 다녀왔어요",
          "speaker_id": "1",
          "time": "20210819 23:02"
        },
        {
          "id": "MDRW2100001991.1.22",
          "form": "저도 양양이나 속초가 좋아요",
          "original_form": "저도 양양이나 속초가 좋아요",
          "speaker_id": "1",
          "time": "20210819 23:02"
        },
        {
          "id": "MDRW2100001991.1.23",
          "form": "바다를 좋아하시나봐요 양양속초 후아힌",
          "original_form": "바다를 좋아하시나봐요 양양속초 후아힌",
          "speaker_id": "2",
          "time": "20210819 23:05"
        },
        {
          "id": "MDRW2100001991.1.24",
          "form": "코로나 끝나면 가고싶은 나라가 어디세요?",
          "original_form": "코로나 끝나면 가고싶은 나라가 어디세요?",
          "speaker_id": "2",
          "time": "20210819 23:05"
        },
        {
          "id": "MDRW2100001991.1.25",
          "form": "맞아요 ㅋㅋㅋ 바다 좋아합니다",
          "original_form": "맞아요 ㅋㅋㅋ 바다 좋아합니다",
          "speaker_id": "1",
          "time": "20210819 23:06"
        },
        {
          "id": "MDRW2100001991.1.26",
          "form": "음 동남아쪽 갈거같아요 베트남 푸꾸옥?? 궁금해요",
          "original_form": "음 동남아쪽 갈거같아요 베트남 푸꾸옥?? 궁금해요",
          "speaker_id": "1",
          "time": "20210819 23:07"
        },
        {
          "id": "MDRW2100001991.1.27",
          "form": "오 되게 유명한 도시가 아닌 알려지지않은 도시들을 좋아하시네요",
          "original_form": "오 되게 유명한 도시가 아닌 알려지지않은 도시들을 좋아하시네요",
          "speaker_id": "2",
          "time": "20210819 23:09"
        },
        {
          "id": "MDRW2100001991.1.28",
          "form": "방금 검색해봤는데 되게 좋아보여요",
          "original_form": "방금 검색해봤는데 되게 좋아보여요",
          "speaker_id": "2",
          "time": "20210819 23:10"
        },
        {
          "id": "MDRW2100001991.1.29",
          "form": "여행가서 제일 기억에 남은거 있으세요?",
          "original_form": "여행가서 제일 기억에 남은거 있으세요?",
          "speaker_id": "2",
          "time": "20210819 23:10"
        },
        {
          "id": "MDRW2100001991.1.30",
          "form": "여행을 가면 사람들이 있기 마련인데 또 그안에서 사람들 많은곳은 피하고싶나봐요 ㅜㅜㅋㅋ",
          "original_form": "여행을 가면 사람들이 있기 마련인데 또 그안에서 사람들 많은곳은 피하고싶나봐요 ㅜㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:11"
        },
        {
          "id": "MDRW2100001991.1.31",
          "form": "음 태국에서 한 8시간을 수영만 했었어요 ㅋㅋㅋㅋ",
          "original_form": "음 태국에서 한 8시간을 수영만 했었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:11"
        }
      ]
