      "utterance": [
        {
          "id": "MDRW2100007054.1.1",
          "form": "전시머 보러안가나",
          "original_form": "전시머 보러안가나",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007054.1.2",
          "form": "전시회? 갈시간이.,.",
          "original_form": "전시회? 갈시간이.,.",
          "speaker_id": "2",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007054.1.3",
          "form": "전시회 잼는거 요즘많이함",
          "original_form": "전시회 잼는거 요즘많이함",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007054.1.4",
          "form": "추천해줄곳있음?",
          "original_form": "추천해줄곳있음?",
          "speaker_id": "2",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007054.1.5",
          "form": "현대미술관에 이건희 전시전",
          "original_form": "현대미술관에 이건희 전시전",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007054.1.6",
          "form": "아하 이건희 콜렉션 짱이다던데",
          "original_form": "아하 이건희 콜렉션 짱이다던데",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007054.1.7",
          "form": "이건희가 가지고 있던 작품들 대방출해서 가봤었음",
          "original_form": "이건희가 가지고 있던 작품들 대방출해서 가봤었음",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007054.1.8",
          "form": "가봤나?",
          "original_form": "가봤나?",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007054.1.9",
          "form": "근데 생각보다 몇점 전시안해놔서",
          "original_form": "근데 생각보다 몇점 전시안해놔서",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007054.1.10",
          "form": "엄청 많이 갖고있댔는데",
          "original_form": "엄청 많이 갖고있댔는데",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007054.1.11",
          "form": "별로였음",
          "original_form": "별로였음",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007054.1.12",
          "form": "그렇군",
          "original_form": "그렇군",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007054.1.13",
          "form": "그거말고도 반고흐전시전도있음",
          "original_form": "그거말고도 반고흐전시전도있음",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007054.1.14",
          "form": "반고흐 전시전?재밌겠네",
          "original_form": "반고흐 전시전?재밌겠네",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007054.1.15",
          "form": "예술의전당에",
          "original_form": "예술의전당에",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007054.1.16",
          "form": "가봐라",
          "original_form": "가봐라",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007054.1.17",
          "form": "아하?",
          "original_form": "아하?",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007054.1.18",
          "form": "알았다",
          "original_form": "알았다",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007054.1.19",
          "form": "반고흐전시전 옌날에 뉴욕에서도잼섯는데",
          "original_form": "반고흐전시전 옌날에 뉴욕에서도잼섯는데",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007054.1.20",
          "form": "뉴욕에서 전시어디어디보러감? 나는 그 중간거리즘에 현대미술관 또가고싶노",
