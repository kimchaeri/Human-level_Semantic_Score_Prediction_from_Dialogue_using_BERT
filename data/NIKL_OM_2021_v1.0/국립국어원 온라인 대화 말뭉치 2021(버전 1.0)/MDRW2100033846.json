      "utterance": [
        {
          "id": "MDRW2100033846.1.1",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "2",
          "time": "20210825 12:17"
        },
        {
          "id": "MDRW2100033846.1.2",
          "form": "라면 좋아하세요?",
          "original_form": "라면 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100033846.1.3",
          "form": "네넹 ㅎㅎ 매운라면 좋아해요",
          "original_form": "네넹 ㅎㅎ 매운라면 좋아해요",
          "speaker_id": "2",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100033846.1.4",
          "form": "진라면vs신라면?",
          "original_form": "진라면vs신라면?",
          "speaker_id": "1",
          "time": "20210825 12:24"
        },
        {
          "id": "MDRW2100033846.1.5",
          "form": "흠 둘다 맛있는데 저는 신라면파용 ㅎㅎ",
          "original_form": "흠 둘다 맛있는데 저는 신라면파용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 12:26"
        },
        {
          "id": "MDRW2100033846.1.6",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210825 12:30"
        },
        {
          "id": "MDRW2100033846.1.7",
          "form": "블랙신라면vs그냥신라면?",
          "original_form": "블랙신라면vs그냥신라면?",
          "speaker_id": "1",
          "time": "20210825 12:31"
        },
        {
          "id": "MDRW2100033846.1.8",
          "form": "ㅋㅋㅋㅋ웃기네용",
          "original_form": "ㅋㅋㅋㅋ웃기네용",
          "speaker_id": "2",
          "time": "20210825 12:31"
        },
        {
          "id": "MDRW2100033846.1.9",
          "form": "블랙이용",
          "original_form": "블랙이용",
          "speaker_id": "2",
          "time": "20210825 12:31"
        },
        {
          "id": "MDRW2100033846.1.10",
          "form": "오 블랙을 더 좋아하시는군요",
          "original_form": "오 블랙을 더 좋아하시는군요",
          "speaker_id": "1",
          "time": "20210825 12:38"
        },
        {
          "id": "MDRW2100033846.1.11",
          "form": "짜파게티vs비빔면은요?",
          "original_form": "짜파게티vs비빔면은요?",
          "speaker_id": "1",
          "time": "20210825 12:38"
        },
        {
          "id": "MDRW2100033846.1.12",
          "form": "비빔면 !",
          "original_form": "비빔면 !",
          "speaker_id": "2",
          "time": "20210825 12:40"
        },
        {
          "id": "MDRW2100033846.1.13",
          "form": "헉 저녁에 비빔면 먹을까..",
          "original_form": "헉 저녁에 비빔면 먹을까..",
          "speaker_id": "2",
          "time": "20210825 12:40"
        },
        {
          "id": "MDRW2100033846.1.14",
          "form": "ㅋㅋㅋ 비빔면좋죠",
          "original_form": "ㅋㅋㅋ 비빔면좋죠",
          "speaker_id": "1",
          "time": "20210825 12:48"
        },
        {
          "id": "MDRW2100033846.1.15",
          "form": "비빔면에\r\n만두vs고기?",
          "original_form": "비빔면에\r\n만두vs고기?",
          "speaker_id": "1",
          "time": "20210825 12:48"
        },
        {
          "id": "MDRW2100033846.1.16",
          "form": "흠 비빔면은 만두요!",
          "original_form": "흠 비빔면은 만두요!",
          "speaker_id": "2",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100033846.1.17",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:22"
        },
        {
          "id": "MDRW2100033846.1.18",
          "form": "떡볶이는 좋아하세요?",
          "original_form": "떡볶이는 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 13:22"
        },
        {
          "id": "MDRW2100033846.1.19",
          "form": "네네 ㅎㅎ",
          "original_form": "네네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100033846.1.20",
          "form": "어제는 떡볶이먹었어요 안그래도 ㅋㅋ",
          "original_form": "어제는 떡볶이먹었어요 안그래도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100033846.1.21",
          "form": "어떤브랜드 좋아하세요?",
          "original_form": "어떤브랜드 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 14:46"
        },
        {
          "id": "MDRW2100033846.1.22",
