      "utterance": [
        {
          "id": "MDRW2100011657.1.1",
          "form": "요즘 사고싶은물건 있으신가요",
          "original_form": "요즘 사고싶은물건 있으신가요",
          "speaker_id": "1",
          "time": "20210824 14:52"
        },
        {
          "id": "MDRW2100011657.1.2",
          "form": "사고 싶은거 진짜 많지",
          "original_form": "사고 싶은거 진짜 많지",
          "speaker_id": "2",
          "time": "20210824 14:52"
        },
        {
          "id": "MDRW2100011657.1.3",
          "form": "일단 팔찌랑 목걸이 귀걸이 세트?",
          "original_form": "일단 팔찌랑 목걸이 귀걸이 세트?",
          "speaker_id": "2",
          "time": "20210824 14:53"
        },
        {
          "id": "MDRW2100011657.1.4",
          "form": "그런거랑",
          "original_form": "그런거랑",
          "speaker_id": "2",
          "time": "20210824 14:53"
        },
        {
          "id": "MDRW2100011657.1.5",
          "form": "갤럭시 워치랑",
          "original_form": "갤럭시 워치랑",
          "speaker_id": "2",
          "time": "20210824 14:53"
        },
        {
          "id": "MDRW2100011657.1.6",
          "form": "나도 워치4사고싶어ㅓㅓㅓ",
          "original_form": "나도 워치4사고싶어ㅓㅓㅓ",
          "speaker_id": "1",
          "time": "20210824 14:53"
        },
        {
          "id": "MDRW2100011657.1.7",
          "form": "마우스도 새로사고싶다",
          "original_form": "마우스도 새로사고싶다",
          "speaker_id": "1",
          "time": "20210824 14:53"
        },
        {
          "id": "MDRW2100011657.1.8",
          "form": "아 그리고",
          "original_form": "아 그리고",
          "speaker_id": "2",
          "time": "20210824 14:53"
        },
        {
          "id": "MDRW2100011657.1.9",
          "form": "코난 전권",
          "original_form": "코난 전권",
          "speaker_id": "2",
          "time": "20210824 14:53"
        },
        {
          "id": "MDRW2100011657.1.10",
          "form": "우와~~~~~",
          "original_form": "우와~~~~~",
          "speaker_id": "1",
          "time": "20210824 14:53"
        },
        {
          "id": "MDRW2100011657.1.11",
          "form": "마우스는 왜?",
          "original_form": "마우스는 왜?",
          "speaker_id": "2",
          "time": "20210824 14:53"
        },
        {
          "id": "MDRW2100011657.1.12",
          "form": "고장났나",
          "original_form": "고장났나",
          "speaker_id": "2",
          "time": "20210824 14:53"
        },
        {
          "id": "MDRW2100011657.1.13",
          "form": "노트북살때 공짜로받은건데",
          "original_form": "노트북살때 공짜로받은건데",
          "speaker_id": "1",
          "time": "20210824 14:54"
        },
        {
          "id": "MDRW2100011657.1.14",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210824 14:54"
        },
        {
          "id": "MDRW2100011657.1.15",
          "form": "요즘 잘 안되는거같아서..",
          "original_form": "요즘 잘 안되는거같아서..",
          "speaker_id": "1",
          "time": "20210824 14:54"
        },
        {
          "id": "MDRW2100011657.1.16",
          "form": "그럼 하나 사야지",
          "original_form": "그럼 하나 사야지",
          "speaker_id": "2",
          "time": "20210824 14:54"
        },
        {
          "id": "MDRW2100011657.1.17",
          "form": "하나 장만해라",
          "original_form": "하나 장만해라",
          "speaker_id": "2",
          "time": "20210824 14:54"
        },
        {
          "id": "MDRW2100011657.1.18",
          "form": "최근에 산 물건은",
          "original_form": "최근에 산 물건은",
          "speaker_id": "1",
          "time": "20210824 14:55"
        },
        {
          "id": "MDRW2100011657.1.19",
          "form": "뭐 있으세야",
          "original_form": "뭐 있으세야",
          "speaker_id": "1",
          "time": "20210824 14:56"
        },
        {
          "id": "MDRW2100011657.1.20",
          "form": "스티커랑 엽서 샀음",
          "original_form": "스티커랑 엽서 샀음",
          "speaker_id": "2",
          "time": "20210824 14:57"
        },
        {
          "id": "MDRW2100011657.1.21",
          "form": "냉동애플망고 5키로 장만햇는데요",
          "original_form": "냉동애플망고 5키로 장만햇는데요",
          "speaker_id": "1",
          "time": "20210824 14:58"
        },
        {
          "id": "MDRW2100011657.1.22",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210824 14:58"
        },
        {
          "id": "MDRW2100011657.1.23",
          "form": "진짜맛있어요",
