      "utterance": [
        {
          "id": "MDRW2100026519.1.1",
          "form": "오늘 너무 흐려서 기분이 쳐지네요",
          "original_form": "오늘 너무 흐려서 기분이 쳐지네요",
          "speaker_id": "1",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100026519.1.2",
          "form": "그래도 안덥잖아오 ㅎㅎ",
          "original_form": "그래도 안덥잖아오 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100026519.1.3",
          "form": "그건 좋네요ㅎㅎ",
          "original_form": "그건 좋네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100026519.1.4",
          "form": "네 이제 에어컨 안켜도 ㅎㅎ",
          "original_form": "네 이제 에어컨 안켜도 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026519.1.5",
          "form": "맞아요 에어컨비 전기세 엄청났죠",
          "original_form": "맞아요 에어컨비 전기세 엄청났죠",
          "speaker_id": "1",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026519.1.6",
          "form": "이제 슬슬 긴옷 좀 꺼내야겠어요",
          "original_form": "이제 슬슬 긴옷 좀 꺼내야겠어요",
          "speaker_id": "1",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026519.1.7",
          "form": "올해 얼마\u0000나 나오셨나요",
          "original_form": "올해 얼마\u0000나 나오셨나요",
          "speaker_id": "2",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026519.1.8",
          "form": "합계는 안내봤는데",
          "original_form": "합계는 안내봤는데",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026519.1.9",
          "form": "거의 평상시의 두배정도?",
          "original_form": "거의 평상시의 두배정도?",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026519.1.10",
          "form": "너무 더워서 그냥 틀어뒀거든요",
          "original_form": "너무 더워서 그냥 틀어뒀거든요",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026519.1.11",
          "form": "두배정도면 뭐 ㅎㅎ",
          "original_form": "두배정도면 뭐 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100026519.1.12",
          "form": "생존하셨으니까 괜찮아요",
          "original_form": "생존하셨으니까 괜찮아요",
          "speaker_id": "2",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100026519.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100026519.1.14",
          "form": "그닥 아깝진않아요ㅎㅎ",
          "original_form": "그닥 아깝진않아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100026519.1.15",
          "form": "ㅋㅋ그쵸 사는게 우선입니다",
          "original_form": "ㅋㅋ그쵸 사는게 우선입니다",
          "speaker_id": "2",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100026519.1.16",
          "form": "오늘은 오히려 춥더라구요",
          "original_form": "오늘은 오히려 춥더라구요",
          "speaker_id": "1",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100026519.1.17",
          "form": "오늘 비기운 있어서 ㅎㅅㄴ",
          "original_form": "오늘 비기운 있어서 ㅎㅅㄴ",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100026519.1.18",
          "form": "ㅜㅜ맞아요",
          "original_form": "ㅜㅜ맞아요",
          "speaker_id": "1",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100026519.1.19",
          "form": "회사에있는데",
          "original_form": "회사에있는데",
          "speaker_id": "1",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100026519.1.20",
          "form": "에어컨켜둬서",
          "original_form": "에어컨켜둬서",
          "speaker_id": "1",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100026519.1.21",
          "form": "너무추웠어요",
          "original_form": "너무추웠어요",
          "speaker_id": "1",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100026519.1.22",
          "form": "누가 이 날씨에",
          "original_form": "누가 이 날씨에",
          "speaker_id": "2",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100026519.1.23",
          "form": "ㅋㅋㅋ상사여서",
          "original_form": "ㅋㅋㅋ상사여서",
          "speaker_id": "1",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100026519.1.24",
          "form": "아무말도못했답니다",
          "original_form": "아무말도못했답니다",
          "speaker_id": "1",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100026519.1.25",
          "form": "아이고 왜...",
          "original_form": "아이고 왜...",
          "speaker_id": "2",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100026519.1.26",
          "form": "옷을 더 입으시지 ㅋㅋ",
          "original_form": "옷을 더 입으시지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100026519.1.27",
          "form": "옷을 안가지고가서ㅜㅜ",
          "original_form": "옷을 안가지고가서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100026519.1.28",
          "form": "담요하나 비치해야겠어요",
          "original_form": "담요하나 비치해야겠어요",
          "speaker_id": "1",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100026519.1.29",
          "form": "무릎담요! 준비하시죠",
          "original_form": "무릎담요! 준비하시죠",
          "speaker_id": "2",
          "time": "20210902 23:52"
        }
      ]
    }
