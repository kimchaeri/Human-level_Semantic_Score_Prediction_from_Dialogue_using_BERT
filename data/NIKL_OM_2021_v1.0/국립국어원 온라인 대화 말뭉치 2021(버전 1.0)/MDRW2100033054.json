      "utterance": [
        {
          "id": "MDRW2100033054.1.1",
          "form": "음 이건 전 우리나라는",
          "original_form": "음 이건 전 우리나라는",
          "speaker_id": "1",
          "time": "20210906 14:47"
        },
        {
          "id": "MDRW2100033054.1.2",
          "form": "아직 안될거라봅니다",
          "original_form": "아직 안될거라봅니다",
          "speaker_id": "1",
          "time": "20210906 14:47"
        },
        {
          "id": "MDRW2100033054.1.3",
          "form": "앗 맞아요 저도",
          "original_form": "앗 맞아요 저도",
          "speaker_id": "2",
          "time": "20210906 15:15"
        },
        {
          "id": "MDRW2100033054.1.4",
          "form": "그리고 진짜 큰일날거같지 않나요...????",
          "original_form": "그리고 진짜 큰일날거같지 않나요...????",
          "speaker_id": "2",
          "time": "20210906 15:15"
        },
        {
          "id": "MDRW2100033054.1.5",
          "form": "뮤 변이나왔다던데",
          "original_form": "뮤 변이나왔다던데",
          "speaker_id": "1",
          "time": "20210906 15:26"
        },
        {
          "id": "MDRW2100033054.1.6",
          "form": "백신맞아도 소용이없는변이라네요",
          "original_form": "백신맞아도 소용이없는변이라네요",
          "speaker_id": "1",
          "time": "20210906 15:27"
        },
        {
          "id": "MDRW2100033054.1.7",
          "form": "네?!?!?!?!",
          "original_form": "네?!?!?!?!",
          "speaker_id": "2",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100033054.1.8",
          "form": "뮤 변이요?!?!?",
          "original_form": "뮤 변이요?!?!?",
          "speaker_id": "2",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100033054.1.9",
          "form": "아니진짜 어디까지 갈 것인가.....;;;;;",
          "original_form": "아니진짜 어디까지 갈 것인가.....;;;;;",
          "speaker_id": "2",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100033054.1.10",
          "form": "백신 부작용 때문에 무서웠는데 역시",
          "original_form": "백신 부작용 때문에 무서웠는데 역시",
          "speaker_id": "2",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100033054.1.11",
          "form": "코로나가 진짜 무섭긴 하네요...",
          "original_form": "코로나가 진짜 무섭긴 하네요...",
          "speaker_id": "2",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100033054.1.12",
          "form": "그쵸 저번주 금요일에",
          "original_form": "그쵸 저번주 금요일에",
          "speaker_id": "1",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100033054.1.13",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100033054.1.14",
          "form": "우리나라도 뮤걸린 외국인 세명 들어왔대요",
          "original_form": "우리나라도 뮤걸린 외국인 세명 들어왔대요",
          "speaker_id": "1",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100033054.1.15",
          "form": "으아아아아",
          "original_form": "으아아아아",
          "speaker_id": "2",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100033054.1.16",
          "form": "아니 이럴거면 좀 외국인을",
          "original_form": "아니 이럴거면 좀 외국인을",
          "speaker_id": "2",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100033054.1.17",
          "form": "들여오지 말았어야지 ㅜㅜㅠㅠㅠㅠㅠㅠ",
          "original_form": "들여오지 말았어야지 ㅜㅜㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100033054.1.18",
          "form": "솔직히 중국이 제일 문제 아니었냐구요 ~!~!!",
          "original_form": "솔직히 중국이 제일 문제 아니었냐구요 ~!~!!",
          "speaker_id": "2",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100033054.1.19",
          "form": "외국인들이 첨에 다 퍼뜨렸죠",
          "original_form": "외국인들이 첨에 다 퍼뜨렸죠",
          "speaker_id": "1",
          "time": "20210906 15:44"
        },
        {
          "id": "MDRW2100033054.1.20",
          "form": "그니까요 중국은 얼마나 편들어주는지",
          "original_form": "그니까요 중국은 얼마나 편들어주는지",
          "speaker_id": "1",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100033054.1.21",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100033054.1.22",
          "form": "아니 진짜 중국 속국 같아요",
          "original_form": "아니 진짜 중국 속국 같아요",
          "speaker_id": "2",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100033054.1.23",
          "form": "이미 맞을지도...ㅎ",
          "original_form": "이미 맞을지도...ㅎ",
          "speaker_id": "2",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100033054.1.24",
          "form": "중국 너무싫어요",
          "original_form": "중국 너무싫어요",
          "speaker_id": "1",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100033054.1.25",
          "form": "백신도 없는데",
          "original_form": "백신도 없는데",
          "speaker_id": "1",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100033054.1.26",
          "form": "저도 진짜 싫어해요ㅠ",
          "original_form": "저도 진짜 싫어해요ㅠ",
          "speaker_id": "2",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100033054.1.27",
          "form": "위드 코로나 하면",
          "original_form": "위드 코로나 하면",
          "speaker_id": "2",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100033054.1.28",
          "form": "뭐 미자들도 맞으라한다던데",
          "original_form": "뭐 미자들도 맞으라한다던데",
          "speaker_id": "1",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100033054.1.29",
          "form": "백신이나 확보하고 그러던가",
          "original_form": "백신이나 확보하고 그러던가",
          "speaker_id": "1",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100033054.1.30",
          "form": "중국인들 놀러 많이들 들어올거같은데...",
          "original_form": "중국인들 놀러 많이들 들어올거같은데...",
          "speaker_id": "2",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100033054.1.31",
          "form": "중국은 요즘 코로나 괜찮나요",
          "original_form": "중국은 요즘 코로나 괜찮나요",
          "speaker_id": "1",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100033054.1.32",
          "form": "별소식이 없어서 모르겠네요",
          "original_form": "별소식이 없어서 모르겠네요",
          "speaker_id": "1",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100033054.1.33",
          "form": "위드코로나 이야기가 제일",
          "original_form": "위드코로나 이야기가 제일",
          "speaker_id": "2",
          "time": "20210906 15:51"
        },
        {
          "id": "MDRW2100033054.1.34",
          "form": "할 말이 많아요 ㅋㅋㅋㅋㅋ",
          "original_form": "할 말이 많아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:51"
        }
      ]
