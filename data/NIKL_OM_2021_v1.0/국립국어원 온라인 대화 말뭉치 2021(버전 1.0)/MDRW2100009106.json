      "utterance": [
        {
          "id": "MDRW2100009106.1.1",
          "form": "대학생인시가요?",
          "original_form": "대학생인시가요?",
          "speaker_id": "2",
          "time": "20210822 21:17"
        },
        {
          "id": "MDRW2100009106.1.2",
          "form": "신",
          "original_form": "신",
          "speaker_id": "2",
          "time": "20210822 21:17"
        },
        {
          "id": "MDRW2100009106.1.3",
          "form": "안녕하세요 전 affiliation 다니네요 어디다니세요",
          "original_form": "안녕하세요 전 &affiliation& 다니네요 어디다니세요",
          "speaker_id": "1",
          "time": "20210822 21:17"
        },
        {
          "id": "MDRW2100009106.1.4",
          "form": "저는 부산 쪽 대학 다녀요",
          "original_form": "저는 부산 쪽 대학 다녀요",
          "speaker_id": "2",
          "time": "20210822 21:17"
        },
        {
          "id": "MDRW2100009106.1.5",
          "form": "시골이부산인데반갑네요?",
          "original_form": "시골이부산인데반갑네요?",
          "speaker_id": "1",
          "time": "20210822 21:17"
        },
        {
          "id": "MDRW2100009106.1.6",
          "form": "도시쪽 아니면 산쪽인가요?",
          "original_form": "도시쪽 아니면 산쪽인가요?",
          "speaker_id": "1",
          "time": "20210822 21:18"
        },
        {
          "id": "MDRW2100009106.1.7",
          "form": "도시쪽이에요",
          "original_form": "도시쪽이에요",
          "speaker_id": "2",
          "time": "20210822 21:18"
        },
        {
          "id": "MDRW2100009106.1.8",
          "form": "집서 몇분거리인가요",
          "original_form": "집서 몇분거리인가요",
          "speaker_id": "1",
          "time": "20210822 21:18"
        },
        {
          "id": "MDRW2100009106.1.9",
          "form": "좀 걸리죠?",
          "original_form": "좀 걸리죠?",
          "speaker_id": "2",
          "time": "20210822 21:18"
        },
        {
          "id": "MDRW2100009106.1.10",
          "form": "다른 지역이라서 오래걸려요",
          "original_form": "다른 지역이라서 오래걸려요",
          "speaker_id": "2",
          "time": "20210822 21:18"
        },
        {
          "id": "MDRW2100009106.1.11",
          "form": "저는 address 서 이태원이라 버스나 지하철 30분ㅎ",
          "original_form": "저는 &address& 서 이태원이라 버스나 지하철 30분ㅎ",
          "speaker_id": "1",
          "time": "20210822 21:18"
        },
        {
          "id": "MDRW2100009106.1.12",
          "form": "혹시 젤좋아하시는과목은?",
          "original_form": "혹시 젤좋아하시는과목은?",
          "speaker_id": "1",
          "time": "20210822 21:19"
        },
        {
          "id": "MDRW2100009106.1.13",
          "form": "문학, 도덕, 예체능 과목 좋아해요",
          "original_form": "문학, 도덕, 예체능 과목 좋아해요",
          "speaker_id": "2",
          "time": "20210822 21:19"
        },
        {
          "id": "MDRW2100009106.1.14",
          "form": "그러시군요 전 캐드가 젤 낫네요 ㅎ",
          "original_form": "그러시군요 전 캐드가 젤 낫네요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 21:20"
        },
        {
          "id": "MDRW2100009106.1.15",
          "form": "젤 싫어하는 과목?",
          "original_form": "젤 싫어하는 과목?",
          "speaker_id": "1",
          "time": "20210822 21:20"
        },
        {
          "id": "MDRW2100009106.1.16",
          "form": "수학이요!",
          "original_form": "수학이요!",
          "speaker_id": "2",
          "time": "20210822 21:20"
        },
        {
          "id": "MDRW2100009106.1.17",
          "form": "진짜 싫어요",
          "original_form": "진짜 싫어요",
          "speaker_id": "2",
          "time": "20210822 21:20"
        },
        {
          "id": "MDRW2100009106.1.18",
          "form": "전 물리여 ㅎ 아예배운적없어서 ㅎ",
          "original_form": "전 물리여 ㅎ 아예배운적없어서 ㅎ",
          "speaker_id": "1",
          "time": "20210822 21:21"
        },
        {
          "id": "MDRW2100009106.1.19",
          "form": "대학와서는 수학 안 하게 될 줄 알았는데",
          "original_form": "대학와서는 수학 안 하게 될 줄 알았는데",
          "speaker_id": "2",
          "time": "20210822 21:21"
        },
        {
          "id": "MDRW2100009106.1.20",
          "form": "통계 때문에 하게 되네요ㅠㅜ",
          "original_form": "통계 때문에 하게 되네요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210822 21:21"
        },
        {
          "id": "MDRW2100009106.1.21",
          "form": "수학은 외우면되는데 물리수학은 노답 ㅠ",
          "original_form": "수학은 외우면되는데 물리수학은 노답 ㅠ",
          "speaker_id": "1",
          "time": "20210822 21:21"
        },
        {
          "id": "MDRW2100009106.1.22",
          "form": "그러시군요ㅠㅜ",
          "original_form": "그러시군요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210822 21:23"
        },
        {
          "id": "MDRW2100009106.1.23",
          "form": "지금 1학년 2학년?",
          "original_form": "지금 1학년 2학년?",
          "speaker_id": "1",
          "time": "20210822 21:24"
        },
        {
          "id": "MDRW2100009106.1.24",
          "form": "전 1학년이네요",
          "original_form": "전 1학년이네요",
          "speaker_id": "1",
          "time": "20210822 21:24"
        },
        {
          "id": "MDRW2100009106.1.25",
          "form": "전 3학년이에요",
          "original_form": "전 3학년이에요",
          "speaker_id": "2",
          "time": "20210822 21:24"
        },
        {
          "id": "MDRW2100009106.1.26",
          "form": "부렵네요",
          "original_form": "부렵네요",
          "speaker_id": "1",
          "time": "20210822 21:25"
        },
        {
          "id": "MDRW2100009106.1.27",
          "form": "전 이번이 대학 3번째라 ㅋ",
          "original_form": "전 이번이 대학 3번째라 ㅋ",
          "speaker_id": "1",
          "time": "20210822 21:25"
        },
        {
          "id": "MDRW2100009106.1.28",
          "form": "농대졸업 사회복지학과 졸업",
          "original_form": "농대졸업 사회복지학과 졸업",
          "speaker_id": "1",
          "time": "20210822 21:26"
        },
        {
          "id": "MDRW2100009106.1.29",
          "form": "국어국문학과 휴학 ㅋ",
          "original_form": "국어국문학과 휴학 ㅋ",
          "speaker_id": "1",
          "time": "20210822 21:26"
        },
        {
          "id": "MDRW2100009106.1.30",
          "form": "프로 입시러시네요",
          "original_form": "프로 입시러시네요",
          "speaker_id": "2",
          "time": "20210822 21:26"
        },
        {
          "id": "MDRW2100009106.1.31",
          "form": "아그럴수있네요",
          "original_form": "아그럴수있네요",
          "speaker_id": "1",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100009106.1.32",
          "form": "동아리로넘어갈꺼요",
          "original_form": "동아리로넘어갈꺼요",
          "speaker_id": "1",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100009106.1.33",
          "form": "하시는것 있나요?",
          "original_form": "하시는것 있나요?",
          "speaker_id": "1",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100009106.1.34",
          "form": "영상제작 동아리에 있어서요",
          "original_form": "영상제작 동아리에 있어서요",
          "speaker_id": "2",
          "time": "20210822 21:28"
        },
        {
          "id": "MDRW2100009106.1.35",
          "form": "기술력이 뛰어난 건 아닌데",
          "original_form": "기술력이 뛰어난 건 아닌데",
          "speaker_id": "2",
          "time": "20210822 21:28"
        },
        {
          "id": "MDRW2100009106.1.36",
          "form": "제작한건 있나요?",
          "original_form": "제작한건 있나요?",
          "speaker_id": "1",
          "time": "20210822 21:28"
        },
        {
          "id": "MDRW2100009106.1.37",
          "form": "간단한 영상 제작은 행ㆍ",
          "original_form": "간단한 영상 제작은 행ㆍ",
          "speaker_id": "2",
          "time": "20210822 21:28"
        },
        {
          "id": "MDRW2100009106.1.38",
          "form": "요",
          "original_form": "요",
          "speaker_id": "2",
          "time": "20210822 21:28"
        },
        {
          "id": "MDRW2100009106.1.39",
          "form": "있어요",
          "original_form": "있어요",
          "speaker_id": "2",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100009106.1.40",
          "form": "전 전에 affiliation에서 others",
          "original_form": "전 전에 &affiliation&에서 &others&",
          "speaker_id": "1",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100009106.1.41",
          "form": "3학년때 1학년 지도하고 아이디어 제가 다 내서",
          "original_form": "3학년때 1학년 지도하고 아이디어 제가 다 내서",
          "speaker_id": "1",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100009106.1.42",
          "form": "ㅣ등 장관상 받았네요",
          "original_form": "ㅣ등 장관상 받았네요",
          "speaker_id": "1",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100009106.1.43",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100009106.1.44",
          "form": "대단하시네요!",
          "original_form": "대단하시네요!",
          "speaker_id": "2",
          "time": "20210822 21:30"
        },
        {
          "id": "MDRW2100009106.1.45",
          "form": "현재 대학에서는",
          "original_form": "현재 대학에서는",
          "speaker_id": "1",
          "time": "20210822 21:30"
        },
        {
          "id": "MDRW2100009106.1.46",
          "form": "드론동아리신청했는데",
          "original_form": "드론동아리신청했는데",
          "speaker_id": "1",
          "time": "20210822 21:30"
        },
        {
          "id": "MDRW2100009106.1.47",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210822 21:30"
        },
        {
          "id": "MDRW2100009106.1.48",
          "form": "흐지부지되서 망했네요",
          "original_form": "흐지부지되서 망했네요",
          "speaker_id": "1",
          "time": "20210822 21:30"
        },
        {
          "id": "MDRW2100009106.1.49",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "2",
          "time": "20210822 21:30"
        }
      ]
    }
  ]
