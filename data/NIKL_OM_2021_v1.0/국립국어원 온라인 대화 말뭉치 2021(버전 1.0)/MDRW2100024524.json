        {
          "id": "MDRW2100024524.1.26",
          "form": "모르는데",
          "original_form": "모르는데",
          "speaker_id": "2",
          "time": "20210831 13:32"
        },
        {
          "id": "MDRW2100024524.1.27",
          "form": "그정도도 안하고 적당히 바꾸고있고",
          "original_form": "그정도도 안하고 적당히 바꾸고있고",
          "speaker_id": "2",
          "time": "20210831 13:32"
        },
        {
          "id": "MDRW2100024524.1.28",
          "form": "이 겜이 그냥 카드 감상겜이면 몰라",
          "original_form": "이 겜이 그냥 카드 감상겜이면 몰라",
          "speaker_id": "1",
          "time": "20210831 13:33"
        },
        {
          "id": "MDRW2100024524.1.29",
          "form": "쓰리디 모델링으로 월드 탐험하는 내용이면서",
          "original_form": "쓰리디 모델링으로 월드 탐험하는 내용이면서",
          "speaker_id": "1",
          "time": "20210831 13:33"
        },
        {
          "id": "MDRW2100024524.1.30",
          "form": "뭔 자신감인데...",
          "original_form": "뭔 자신감인데...",
          "speaker_id": "1",
          "time": "20210831 13:33"
        },
        {
          "id": "MDRW2100024524.1.31",
          "form": "ㅇㅇ그니까",
          "original_form": "ㅇㅇ그니까",
          "speaker_id": "2",
          "time": "20210831 13:33"
        },
        {
          "id": "MDRW2100024524.1.32",
          "form": "분명 초기캐들 모델링은",
          "original_form": "분명 초기캐들 모델링은",
          "speaker_id": "2",
          "time": "20210831 13:34"
        },
        {
          "id": "MDRW2100024524.1.33",
          "form": "조금씩은 다른데",
          "original_form": "조금씩은 다른데",
          "speaker_id": "2",
          "time": "20210831 13:34"
        },
        {
          "id": "MDRW2100024524.1.34",
          "form": "이나즈마에서는",
          "original_form": "이나즈마에서는",
          "speaker_id": "2",
          "time": "20210831 13:34"
        },
        {
          "id": "MDRW2100024524.1.35",
          "form": "번역도 별로고",
          "original_form": "번역도 별로고",
          "speaker_id": "2",
          "time": "20210831 13:34"
        },
        {
          "id": "MDRW2100024524.1.36",
          "form": "모델링은 돌려쓴 티나고",
          "original_form": "모델링은 돌려쓴 티나고",
          "speaker_id": "2",
          "time": "20210831 13:34"
        },
        {
          "id": "MDRW2100024524.1.37",
          "form": "뭔가 성의가 없어",
          "original_form": "뭔가 성의가 없어",
          "speaker_id": "2",
          "time": "20210831 13:35"
        },
        {
          "id": "MDRW2100024524.1.38",
          "form": "ㅇㅇ 수익은 갈수록 더 날텐데",
          "original_form": "ㅇㅇ 수익은 갈수록 더 날텐데",
          "speaker_id": "1",
          "time": "20210831 13:35"
        },
        {
          "id": "MDRW2100024524.1.39",
          "form": "뭐가 문젠데...",
          "original_form": "뭐가 문젠데...",
          "speaker_id": "1",
          "time": "20210831 13:35"
        },
        {
          "id": "MDRW2100024524.1.40",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210831 13:35"
        },
        {
          "id": "MDRW2100024524.1.41",
          "form": "나 원신처럼",
