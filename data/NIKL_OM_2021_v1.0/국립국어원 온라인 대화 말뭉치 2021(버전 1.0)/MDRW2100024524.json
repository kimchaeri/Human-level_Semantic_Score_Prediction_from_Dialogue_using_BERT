        {
          "id": "MDRW2100024524.1.1",
          "form": "아예 인게임에 등장시킬 생각이",
          "original_form": "아예 인게임에 등장시킬 생각이",
          "speaker_id": "2",
          "time": "20210831 13:10"
        },
        {
          "id": "MDRW2100024524.1.2",
          "form": "없어보여",
          "original_form": "없어보여",
          "speaker_id": "2",
          "time": "20210831 13:10"
        },
        {
          "id": "MDRW2100024524.1.3",
          "form": "그래픽 작업 따로 안할거라",
          "original_form": "그래픽 작업 따로 안할거라",
          "speaker_id": "2",
          "time": "20210831 13:10"
        },
        {
          "id": "MDRW2100024524.1.4",
          "form": "걍 캐디에 힘준거지...",
          "original_form": "걍 캐디에 힘준거지...",
          "speaker_id": "2",
          "time": "20210831 13:10"
        },
        {
          "id": "MDRW2100024524.1.5",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210831 13:12"
        },
        {
          "id": "MDRW2100024524.1.6",
          "form": "쓰리디 모델링도 돌려쓴다며",
          "original_form": "쓰리디 모델링도 돌려쓴다며",
          "speaker_id": "1",
          "time": "20210831 13:12"
        },
        {
          "id": "MDRW2100024524.1.7",
          "form": "이번에 카즈하가 남주인동 모델링 그대로 갖다썼대서",
          "original_form": "이번에 카즈하가 남주인동 모델링 그대로 갖다썼대서",
          "speaker_id": "1",
          "time": "20210831 13:13"
        },
        {
          "id": "MDRW2100024524.1.8",
          "form": "그것땜에 뽑은 사람도 꽤 있었음",
          "original_form": "그것땜에 뽑은 사람도 꽤 있었음",
          "speaker_id": "1",
          "time": "20210831 13:13"
        },
        {
          "id": "MDRW2100024524.1.9",
          "form": "모델링 돌려쓰는것까지 뭐라할 생각은 없는데",
          "original_form": "모델링 돌려쓰는것까지 뭐라할 생각은 없는데",
          "speaker_id": "2",
          "time": "20210831 13:14"
        },
        {
          "id": "MDRW2100024524.1.10",
          "form": "좀 성의있게라도",
          "original_form": "좀 성의있게라도",
          "speaker_id": "2",
          "time": "20210831 13:14"
        },
        {
          "id": "MDRW2100024524.1.11",
          "form": "돌려썼으면",
          "original_form": "돌려썼으면",
          "speaker_id": "2",
          "time": "20210831 13:14"
        },
        {
          "id": "MDRW2100024524.1.12",
          "form": "글킨 해 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "글킨 해 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:14"
        },
        {
          "id": "MDRW2100024524.1.13",
          "form": "소도 생각보다 키 작고...",
          "original_form": "소도 생각보다 키 작고...",
          "speaker_id": "1",
          "time": "20210831 13:14"
        },
        {
          "id": "MDRW2100024524.1.14",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210831 13:15"
        },
        {
          "id": "MDRW2100024524.1.15",
          "form": "좀 베껴도",
          "original_form": "좀 베껴도",
          "speaker_id": "2",
          "time": "20210831 13:15"
        },
        {
          "id": "MDRW2100024524.1.16",
          "form": "베낀 티는 안나게",
          "original_form": "베낀 티는 안나게",
          "speaker_id": "2",
          "time": "20210831 13:15"
        },
        {
          "id": "MDRW2100024524.1.17",
          "form": "베껴주지 않겠냐고",
          "original_form": "베껴주지 않겠냐고",
          "speaker_id": "2",
          "time": "20210831 13:15"
        },
        {
          "id": "MDRW2100024524.1.18",
          "form": "좀만 수정해가면서 돌려써도",
          "original_form": "좀만 수정해가면서 돌려써도",
          "speaker_id": "2",
          "time": "20210831 13:16"
        },
        {
          "id": "MDRW2100024524.1.19",
          "form": "나같은 알못은 모르는데",
          "original_form": "나같은 알못은 모르는데",
          "speaker_id": "2",
          "time": "20210831 13:16"
        },
        {
          "id": "MDRW2100024524.1.20",
          "form": "그치 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그치 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:31"
        },
        {
          "id": "MDRW2100024524.1.21",
          "form": "나도 사실",
          "original_form": "나도 사실",
          "speaker_id": "1",
          "time": "20210831 13:31"
        },
        {
          "id": "MDRW2100024524.1.22",
          "form": "뼈대가 같아도 겉 껍데기 좀 바꿔주고",
          "original_form": "뼈대가 같아도 겉 껍데기 좀 바꿔주고",
          "speaker_id": "1",
          "time": "20210831 13:31"
        },
        {
          "id": "MDRW2100024524.1.23",
          "form": "스킬모션 화려하게 넣어주면 아무것도 못알아차린다구",
          "original_form": "스킬모션 화려하게 넣어주면 아무것도 못알아차린다구",
          "speaker_id": "1",
          "time": "20210831 13:31"
        },
        {
          "id": "MDRW2100024524.1.24",
          "form": "ㅇㅇㅋㅋㅋㄱㅋㅋㅋ머리 눈 옷 좀",
