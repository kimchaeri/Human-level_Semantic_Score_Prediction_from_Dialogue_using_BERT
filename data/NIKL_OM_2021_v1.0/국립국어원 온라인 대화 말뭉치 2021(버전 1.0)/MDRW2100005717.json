      "utterance": [
        {
          "id": "MDRW2100005717.1.1",
          "form": "종교 있으세여?",
          "original_form": "종교 있으세여?",
          "speaker_id": "2",
          "time": "20210907 11:25"
        },
        {
          "id": "MDRW2100005717.1.2",
          "form": "아 전 무교요",
          "original_form": "아 전 무교요",
          "speaker_id": "1",
          "time": "20210907 11:33"
        },
        {
          "id": "MDRW2100005717.1.3",
          "form": "아 저도요 하지만 불교를 좀 더 좋아해요",
          "original_form": "아 저도요 하지만 불교를 좀 더 좋아해요",
          "speaker_id": "2",
          "time": "20210907 12:06"
        },
        {
          "id": "MDRW2100005717.1.4",
          "form": "사찰이랑 그런 문화 좋아해요",
          "original_form": "사찰이랑 그런 문화 좋아해요",
          "speaker_id": "2",
          "time": "20210907 12:06"
        },
        {
          "id": "MDRW2100005717.1.5",
          "form": "저도 부모님이 불교셔서",
          "original_form": "저도 부모님이 불교셔서",
          "speaker_id": "1",
          "time": "20210907 12:13"
        },
        {
          "id": "MDRW2100005717.1.6",
          "form": "오 네 저도 불교유치원을 나와서 친숙하고 좋아요",
          "original_form": "오 네 저도 불교유치원을 나와서 친숙하고 좋아요",
          "speaker_id": "2",
          "time": "20210907 12:13"
        },
        {
          "id": "MDRW2100005717.1.7",
          "form": "마음이 편해지죠",
          "original_form": "마음이 편해지죠",
          "speaker_id": "1",
          "time": "20210907 12:13"
        },
        {
          "id": "MDRW2100005717.1.8",
          "form": "그쵸 향 냄새도 좋아요",
          "original_form": "그쵸 향 냄새도 좋아요",
          "speaker_id": "2",
          "time": "20210907 12:13"
        },
        {
          "id": "MDRW2100005717.1.9",
          "form": "코로나끝나고 템플스테이할까해요",
          "original_form": "코로나끝나고 템플스테이할까해요",
          "speaker_id": "1",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100005717.1.10",
          "form": "우와 저도 추운 겨율 템플 스테이 로망이에여",
          "original_form": "우와 저도 추운 겨율 템플 스테이 로망이에여",
          "speaker_id": "2",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100005717.1.11",
          "form": "아 절밥이랑!!! 완벽한 조화죠!",
          "original_form": "아 절밥이랑!!! 완벽한 조화죠!",
          "speaker_id": "1",
          "time": "20210907 12:25"
        },
        {
          "id": "MDRW2100005717.1.12",
          "form": "구쵸ㅠㅠ 뭔가 편안한것이 산속이기도 하고 해서 다 좋아",
          "original_form": "구쵸ㅠㅠ 뭔가 편안한것이 산속이기도 하고 해서 다 좋아",
          "speaker_id": "2",
          "time": "20210907 12:38"
        },
        {
          "id": "MDRW2100005717.1.13",
          "form": "맞아요! 나중에 나이 더 먹고 외로울때 봉사도하고 그러려고요",
          "original_form": "맞아요! 나중에 나이 더 먹고 외로울때 봉사도하고 그러려고요",
          "speaker_id": "1",
          "time": "20210907 12:39"
        },
        {
          "id": "MDRW2100005717.1.14",
          "form": "와와 맞아요 보살님 봉사 !! ㅋㅋㅋ",
          "original_form": "와와 맞아요 보살님 봉사 !! ㅋㅋㅋ",
