      "utterance": [
        {
          "id": "MDRW2100004921.1.1",
          "form": "집안일은 잘 도와주나요?",
          "original_form": "집안일은 잘 도와주나요?",
          "speaker_id": "2",
          "time": "20210821 19:22"
        },
        {
          "id": "MDRW2100004921.1.2",
          "form": "네 잘도와줘요",
          "original_form": "네 잘도와줘요",
          "speaker_id": "1",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004921.1.3",
          "form": "잔소리하거든요",
          "original_form": "잔소리하거든요",
          "speaker_id": "1",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004921.1.4",
          "form": "ㅎㅎ 다행이네요",
          "original_form": "ㅎㅎ 다행이네요",
          "speaker_id": "2",
          "time": "20210821 19:43"
        },
        {
          "id": "MDRW2100004921.1.5",
          "form": "저희는 신랑이 더 깔끔한편이라 주말에 집에오면 알아서 움직이더라구요",
          "original_form": "저희는 신랑이 더 깔끔한편이라 주말에 집에오면 알아서 움직이더라구요",
          "speaker_id": "2",
          "time": "20210821 19:44"
        },
        {
          "id": "MDRW2100004921.1.6",
          "form": "좋겠어요 부러워요",
          "original_form": "좋겠어요 부러워요",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100004921.1.7",
          "form": "동갑이신가요",
          "original_form": "동갑이신가요",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100004921.1.8",
          "form": "아뇨 저도 오빠 한살위요",
          "original_form": "아뇨 저도 오빠 한살위요",
          "speaker_id": "2",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100004921.1.9",
          "form": "한살이면 완전 친구같겠다",
          "original_form": "한살이면 완전 친구같겠다",
          "speaker_id": "1",
          "time": "20210821 20:15"
        },
        {
          "id": "MDRW2100004921.1.10",
          "form": "저는 7살이예요",
          "original_form": "저는 7살이예요",
          "speaker_id": "1",
          "time": "20210821 20:15"
        },
        {
          "id": "MDRW2100004921.1.11",
          "form": "와 차이 많으시네요",
          "original_form": "와 차이 많으시네요",
          "speaker_id": "2",
          "time": "20210821 20:15"
        },
        {
          "id": "MDRW2100004921.1.12",
          "form": "애기있으세요?",
          "original_form": "애기있으세요?",
          "speaker_id": "1",
          "time": "20210821 20:15"
        },
        {
          "id": "MDRW2100004921.1.13",
          "form": "그래도 잔소리 잘들어준다니 보기좋아요 ㅎ",
          "original_form": "그래도 잔소리 잘들어준다니 보기좋아요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 20:15"
        },
        {
          "id": "MDRW2100004921.1.14",
          "form": "네 아기 2명이묘",
          "original_form": "네 아기 2명이묘",
          "speaker_id": "2",
          "time": "20210821 20:15"
        },
        {
          "id": "MDRW2100004921.1.15",
          "form": "와!! 저는 이제 돌쟁이딸 있어요",
          "original_form": "와!! 저는 이제 돌쟁이딸 있어요",
          "speaker_id": "1",
          "time": "20210821 20:16"
        },
        {
          "id": "MDRW2100004921.1.16",
          "form": "몇살이예요?",
          "original_form": "몇살이예요?",
          "speaker_id": "1",
          "time": "20210821 20:16"
        },
        {
          "id": "MDRW2100004921.1.17",
          "form": "5살 6살이요",
          "original_form": "5살 6살이요",
          "speaker_id": "2",
          "time": "20210821 20:17"
        },
        {
          "id": "MDRW2100004921.1.18",
          "form": "집은 넓으신가요!! 요즘 집값이 비싸져서 이사 엄두가 안나요",
          "original_form": "집은 넓으신가요!! 요즘 집값이 비싸져서 이사 엄두가 안나요",
          "speaker_id": "1",
          "time": "20210821 20:18"
        },
        {
          "id": "MDRW2100004921.1.19",
          "form": "저도 집 넓혀서 가야되는데 집값이 장난아니라서 고민이예요 ㅎ",
          "original_form": "저도 집 넓혀서 가야되는데 집값이 장난아니라서 고민이예요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 20:19"
        }
      ]
