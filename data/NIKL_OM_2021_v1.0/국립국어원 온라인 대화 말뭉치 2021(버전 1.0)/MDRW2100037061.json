        },
        {
          "id": "MDRW2100037061.1.2",
          "form": "점심에 맛있는 거 먹었어?",
          "original_form": "점심에 맛있는 거 먹었어?",
          "speaker_id": "1",
          "time": "20210831 18:34"
        },
        {
          "id": "MDRW2100037061.1.3",
          "form": "라멘 먹었어!",
          "original_form": "라멘 먹었어!",
          "speaker_id": "2",
          "time": "20210831 18:38"
        },
        {
          "id": "MDRW2100037061.1.4",
          "form": "헐 맛있었겠다",
          "original_form": "헐 맛있었겠다",
          "speaker_id": "1",
          "time": "20210831 18:38"
        },
        {
          "id": "MDRW2100037061.1.5",
          "form": "나 너랑 먹은 게 마지막이야",
          "original_form": "나 너랑 먹은 게 마지막이야",
          "speaker_id": "1",
          "time": "20210831 18:38"
        },
        {
          "id": "MDRW2100037061.1.6",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210831 18:39"
        },
        {
          "id": "MDRW2100037061.1.7",
          "form": "우리 어디였지",
          "original_form": "우리 어디였지",
          "speaker_id": "1",
          "time": "20210831 18:40"
        },
        {
          "id": "MDRW2100037061.1.8",
          "form": "어디서 라멘 먹었지..?",
          "original_form": "어디서 라멘 먹었지..?",
          "speaker_id": "1",
          "time": "20210831 18:40"
        },
        {
          "id": "MDRW2100037061.1.9",
          "form": "그 목이 버섯 들어간 약간 특이한 라멘 먹었을 때",
          "original_form": "그 목이 버섯 들어간 약간 특이한 라멘 먹었을 때",
          "speaker_id": "1",
          "time": "20210831 18:40"
        },
        {
          "id": "MDRW2100037061.1.10",
          "form": "성신",
          "original_form": "성신",
          "speaker_id": "2",
          "time": "20210831 18:42"
        },
        {
          "id": "MDRW2100037061.1.11",
          "form": "어딘가",
          "original_form": "어딘가",
          "speaker_id": "2",
          "time": "20210831 18:42"
        },
        {
          "id": "MDRW2100037061.1.12",
          "form": "성신이었나?",
          "original_form": "성신이었나?",
          "speaker_id": "1",
          "time": "20210831 18:43"
        },
        {
          "id": "MDRW2100037061.1.13",
          "form": "엉엉",
          "original_form": "엉엉",
          "speaker_id": "2",
          "time": "20210831 18:43"
        },
        {
          "id": "MDRW2100037061.1.14",
          "form": "골목 골목 다녔던 거 같아서",
          "original_form": "골목 골목 다녔던 거 같아서",
          "speaker_id": "1",
          "time": "20210831 18:43"
        },
        {
          "id": "MDRW2100037061.1.15",
          "form": "오늘은 어디서 먹었어?",
          "original_form": "오늘은 어디서 먹었어?",
          "speaker_id": "1",
          "time": "20210831 18:44"
        },
        {
          "id": "MDRW2100037061.1.16",
          "form": "가로수길!",
          "original_form": "가로수길!",
          "speaker_id": "2",
          "time": "20210831 18:44"
        },
        {
          "id": "MDRW2100037061.1.17",
          "form": "오호 카페도 그 근처로 간 거야?",
          "original_form": "오호 카페도 그 근처로 간 거야?",
          "speaker_id": "1",
          "time": "20210831 18:45"
        },
        {
          "id": "MDRW2100037061.1.18",
          "form": "마자마자!",
          "original_form": "마자마자!",
          "speaker_id": "2",
          "time": "20210831 18:47"
        },
