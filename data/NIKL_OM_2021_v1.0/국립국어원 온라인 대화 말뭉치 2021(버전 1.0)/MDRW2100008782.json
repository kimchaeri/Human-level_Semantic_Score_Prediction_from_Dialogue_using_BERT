      "utterance": [
        {
          "id": "MDRW2100008782.1.1",
          "form": "알바 해?",
          "original_form": "알바 해?",
          "speaker_id": "1",
          "time": "20210822 18:15"
        },
        {
          "id": "MDRW2100008782.1.2",
          "form": "회사다녀",
          "original_form": "회사다녀",
          "speaker_id": "2",
          "time": "20210822 18:16"
        },
        {
          "id": "MDRW2100008782.1.3",
          "form": "주제 바꾸는 게 나을라나",
          "original_form": "주제 바꾸는 게 나을라나",
          "speaker_id": "1",
          "time": "20210822 18:16"
        },
        {
          "id": "MDRW2100008782.1.4",
          "form": "난 상관없음",
          "original_form": "난 상관없음",
          "speaker_id": "2",
          "time": "20210822 18:17"
        },
        {
          "id": "MDRW2100008782.1.5",
          "form": "알바 했던 경험은 얼마나 돼??",
          "original_form": "알바 했던 경험은 얼마나 돼??",
          "speaker_id": "1",
          "time": "20210822 18:19"
        },
        {
          "id": "MDRW2100008782.1.6",
          "form": "길진않아 고깃집이랑 국수집 이런데에서 몇달씩?",
          "original_form": "길진않아 고깃집이랑 국수집 이런데에서 몇달씩?",
          "speaker_id": "2",
          "time": "20210822 18:21"
        },
        {
          "id": "MDRW2100008782.1.7",
          "form": "넌 알바 많이 해봤어?",
          "original_form": "넌 알바 많이 해봤어?",
          "speaker_id": "2",
          "time": "20210822 18:21"
        },
        {
          "id": "MDRW2100008782.1.8",
          "form": "아니아니 나도 별로 많이 안해봤어",
          "original_form": "아니아니 나도 별로 많이 안해봤어",
          "speaker_id": "1",
          "time": "20210822 18:22"
        },
        {
          "id": "MDRW2100008782.1.9",
          "form": "근데 고깃집 힘들었겠다",
          "original_form": "근데 고깃집 힘들었겠다",
          "speaker_id": "1",
          "time": "20210822 18:23"
        },
        {
          "id": "MDRW2100008782.1.10",
          "form": "어렵지 않았어?",
          "original_form": "어렵지 않았어?",
          "speaker_id": "1",
          "time": "20210822 18:23"
        },
        {
          "id": "MDRW2100008782.1.11",
          "form": "사람들이 별로여서 진짜 금방 나왔어 ㅋㅋ",
          "original_form": "사람들이 별로여서 진짜 금방 나왔어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:23"
        },
        {
          "id": "MDRW2100008782.1.12",
          "form": "헐 왜왜",
          "original_form": "헐 왜왜",
          "speaker_id": "1",
          "time": "20210822 18:23"
        },
        {
          "id": "MDRW2100008782.1.13",
          "form": "주방 아줌마들이 텃세부렸어 ㅠㅠ",
          "original_form": "주방 아줌마들이 텃세부렸어 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 18:24"
        },
        {
          "id": "MDRW2100008782.1.14",
          "form": "와 진짜 나랑 개똑같다",
          "original_form": "와 진짜 나랑 개똑같다",
          "speaker_id": "1",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008782.1.15",
          "form": "나 고깃집 한번 해봤었는데",
          "original_form": "나 고깃집 한번 해봤었는데",
          "speaker_id": "1",
          "time": "20210822 18:26"
        },
        {
          "id": "MDRW2100008782.1.16",
          "form": "헐 대박이넹",
          "original_form": "헐 대박이넹",
          "speaker_id": "2",
          "time": "20210822 18:26"
        },
        {
          "id": "MDRW2100008782.1.17",
          "form": "거기 주방이모가 되게 뭐라해서",
          "original_form": "거기 주방이모가 되게 뭐라해서",
          "speaker_id": "1",
          "time": "20210822 18:26"
        },
        {
          "id": "MDRW2100008782.1.18",
          "form": "걍 그만뒀었어",
          "original_form": "걍 그만뒀었어",
          "speaker_id": "1",
          "time": "20210822 18:26"
        },
        {
          "id": "MDRW2100008782.1.19",
          "form": "이상한 사람 참많아",
          "original_form": "이상한 사람 참많아",
          "speaker_id": "2",
          "time": "20210822 18:26"
        },
        {
          "id": "MDRW2100008782.1.20",
          "form": "좋은 기억은 아님..",
          "original_form": "좋은 기억은 아님..",
          "speaker_id": "1",
