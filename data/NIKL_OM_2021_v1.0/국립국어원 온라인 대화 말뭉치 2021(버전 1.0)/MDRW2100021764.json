      "utterance": [
        {
          "id": "MDRW2100021764.1.1",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100021764.1.2",
          "form": "꿈나라 여행 좋아해요",
          "original_form": "꿈나라 여행 좋아해요",
          "speaker_id": "2",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100021764.1.3",
          "form": "ㅎㅎ꿈나라여행 꿈자주꾸시나요",
          "original_form": "ㅎㅎ꿈나라여행 꿈자주꾸시나요",
          "speaker_id": "1",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021764.1.4",
          "form": "네 악몽 꾸는 게 제일 스릴 넘치더라구여",
          "original_form": "네 악몽 꾸는 게 제일 스릴 넘치더라구여",
          "speaker_id": "2",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021764.1.5",
          "form": "쫓기고 도망가고..",
          "original_form": "쫓기고 도망가고..",
          "speaker_id": "2",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021764.1.6",
          "form": "ㅎㅎ저도 쫓기는꿈 자주꿔요",
          "original_form": "ㅎㅎ저도 쫓기는꿈 자주꿔요",
          "speaker_id": "1",
          "time": "20210828 22:30"
        },
        {
          "id": "MDRW2100021764.1.7",
          "form": "가장좋았던 여행지있으세요?",
          "original_form": "가장좋았던 여행지있으세요?",
          "speaker_id": "1",
          "time": "20210828 22:30"
        },
        {
          "id": "MDRW2100021764.1.8",
          "form": "제주도가 좋았어요~",
          "original_form": "제주도가 좋았어요~",
          "speaker_id": "2",
          "time": "20210828 22:31"
        },
        {
          "id": "MDRW2100021764.1.9",
          "form": "국내 여행지라서 진부하긴 하지만 해외 뺨치게 풍경 좋은 곳도 많고 바다 물 색깔도 예뻐요 ㅜ",
          "original_form": "국내 여행지라서 진부하긴 하지만 해외 뺨치게 풍경 좋은 곳도 많고 바다 물 색깔도 예뻐요 ㅜ",
          "speaker_id": "2",
          "time": "20210828 22:31"
        },
        {
          "id": "MDRW2100021764.1.10",
          "form": "저도 제주도 국내중에 제일좋아해요",
          "original_form": "저도 제주도 국내중에 제일좋아해요",
          "speaker_id": "1",
          "time": "20210828 22:43"
        },
        {
          "id": "MDRW2100021764.1.11",
          "form": "코로나끝나면 제주도부터갈거에요ㅜㅜ",
          "original_form": "코로나끝나면 제주도부터갈거에요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 22:43"
        },
        {
          "id": "MDRW2100021764.1.12",
          "form": "제주도 어디제일좋으셨어요",
          "original_form": "제주도 어디제일좋으셨어요",
          "speaker_id": "1",
          "time": "20210828 22:43"
        },
        {
          "id": "MDRW2100021764.1.13",
          "form": "제주도 중에 애월 쪽이 좋았어요",
          "original_form": "제주도 중에 애월 쪽이 좋았어요",
          "speaker_id": "2",
          "time": "20210828 22:47"
        },
        {
          "id": "MDRW2100021764.1.14",
          "form": "거기 바닷가가 에메랄드 빛이고 바로 옆이 카페거리인데 인스타용으로 사진 찍으러 많이 오더라구요 ㅎ",
          "original_form": "거기 바닷가가 에메랄드 빛이고 바로 옆이 카페거리인데 인스타용으로 사진 찍으러 많이 오더라구요 ㅎ",
          "speaker_id": "2",
          "time": "20210828 22:47"
        },
        {
          "id": "MDRW2100021764.1.15",
          "form": "맞아요 애월해안도로 너무좋아서 저 스쿠터로 달렸는데 계속 멈추고사진찍고 했었던기억나요ㅜㅜ",
          "original_form": "맞아요 애월해안도로 너무좋아서 저 스쿠터로 달렸는데 계속 멈추고사진찍고 했었던기억나요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100021764.1.16",
          "form": "오 스쿠터는 대여하신거에요??",
          "original_form": "오 스쿠터는 대여하신거에요??",
          "speaker_id": "2",
          "time": "20210828 23:41"
        },
        {
          "id": "MDRW2100021764.1.17",
          "form": "얼마 정도 해요?",
          "original_form": "얼마 정도 해요?",
          "speaker_id": "2",
          "time": "20210828 23:41"
        },
        {
          "id": "MDRW2100021764.1.18",
          "form": "몇년돼서 기억안나는데 글케비싸진않았어요",
          "original_form": "몇년돼서 기억안나는데 글케비싸진않았어요",
          "speaker_id": "1",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100021764.1.19",
          "form": "네 하루대여해서ㅎㅎ",
          "original_form": "네 하루대여해서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100021764.1.20",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "2",
          "time": "20210828 23:44"
        },
        {
          "id": "MDRW2100021764.1.21",
          "form": "요새 스쿠터 사고 많아서 좀 무섭네요",
          "original_form": "요새 스쿠터 사고 많아서 좀 무섭네요",
          "speaker_id": "2",
          "time": "20210828 23:44"
        },
        {
          "id": "MDRW2100021764.1.22",
          "form": "대여할때도 속도만 무리해서 안내면 사고안난다고하더라구여",
          "original_form": "대여할때도 속도만 무리해서 안내면 사고안난다고하더라구여",
          "speaker_id": "1",
          "time": "20210828 23:44"
        },
        {
          "id": "MDRW2100021764.1.23",
          "form": "근데 화물차이런건 넘무섭고..",
          "original_form": "근데 화물차이런건 넘무섭고..",
          "speaker_id": "1",
          "time": "20210828 23:44"
        },
        {
          "id": "MDRW2100021764.1.24",
          "form": "제주도에",
          "original_form": "제주도에",
          "speaker_id": "2",
          "time": "20210828 23:45"
        },
        {
          "id": "MDRW2100021764.1.25",
          "form": "화물차는 많이 없죠?",
          "original_form": "화물차는 많이 없죠?",
          "speaker_id": "2",
          "time": "20210828 23:45"
        },
        {
          "id": "MDRW2100021764.1.26",
          "form": "화물차지나갈때마다 갓길주차했어요",
          "original_form": "화물차지나갈때마다 갓길주차했어요",
          "speaker_id": "1",
          "time": "20210828 23:45"
        },
        {
          "id": "MDRW2100021764.1.27",
          "form": "아 지나가긴 해요? ㄷㄷ",
          "original_form": "아 지나가긴 해요? ㄷㄷ",
          "speaker_id": "2",
          "time": "20210828 23:45"
        },
        {
          "id": "MDRW2100021764.1.28",
          "form": "엄청많아요..",
          "original_form": "엄청많아요..",
          "speaker_id": "1",
          "time": "20210828 23:45"
        },
        {
          "id": "MDRW2100021764.1.29",
          "form": "아이 ㅠ 담에 여행 가면 스쿠터 타보고싶었는데..",
          "original_form": "아이 ㅠ 담에 여행 가면 스쿠터 타보고싶었는데..",
          "speaker_id": "2",
          "time": "20210828 23:45"
        },
        {
          "id": "MDRW2100021764.1.30",
          "form": "거기 땅콩밭이런게많아서그런지",
          "original_form": "거기 땅콩밭이런게많아서그런지",
          "speaker_id": "1",
          "time": "20210828 23:45"
        },
        {
          "id": "MDRW2100021764.1.31",
          "form": "ㅋㅋㅋㅋ땅콩ㅋㅋㅋ 최고죠",
          "original_form": "ㅋㅋㅋㅋ땅콩ㅋㅋㅋ 최고죠",
          "speaker_id": "2",
          "time": "20210828 23:45"
        },
        {
          "id": "MDRW2100021764.1.32",
          "form": "진짜 이렇게많은화물차가있다니 싶을정도로..",
          "original_form": "진짜 이렇게많은화물차가있다니 싶을정도로..",
          "speaker_id": "1",
          "time": "20210828 23:45"
        }
      ]
    }
