      "utterance": [
        {
          "id": "MDRW2100009984.1.1",
          "form": "거기도 비 오나요?",
          "original_form": "거기도 비 오나요?",
          "speaker_id": "1",
          "time": "20210823 12:45"
        },
        {
          "id": "MDRW2100009984.1.2",
          "form": "네 조금씩 오고 있어요",
          "original_form": "네 조금씩 오고 있어요",
          "speaker_id": "2",
          "time": "20210823 12:46"
        },
        {
          "id": "MDRW2100009984.1.3",
          "form": "여기두요",
          "original_form": "여기두요",
          "speaker_id": "1",
          "time": "20210823 12:46"
        },
        {
          "id": "MDRW2100009984.1.4",
          "form": "비 오는 거 좋아하세요?",
          "original_form": "비 오는 거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 12:46"
        },
        {
          "id": "MDRW2100009984.1.5",
          "form": "아뇨 우산 챙겨가기 번거로워서 안좋아해요ㅜㅜ",
          "original_form": "아뇨 우산 챙겨가기 번거로워서 안좋아해요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 12:48"
        },
        {
          "id": "MDRW2100009984.1.6",
          "form": "님은용?",
          "original_form": "님은용?",
          "speaker_id": "2",
          "time": "20210823 12:48"
        },
        {
          "id": "MDRW2100009984.1.7",
          "form": "저두요 ㅎ",
          "original_form": "저두요 ㅎ",
          "speaker_id": "1",
          "time": "20210823 12:49"
        },
        {
          "id": "MDRW2100009984.1.8",
          "form": "비오면 다 젖어서 싫어요",
          "original_form": "비오면 다 젖어서 싫어요",
          "speaker_id": "1",
          "time": "20210823 12:49"
        },
        {
          "id": "MDRW2100009984.1.9",
          "form": "ㅋㅋㅋㅋㅋ그쵸ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ그쵸ㅠ",
          "speaker_id": "2",
          "time": "20210823 12:49"
        },
        {
          "id": "MDRW2100009984.1.10",
          "form": "근데 같은 물성분인데도 저는 눈 맞는건 좀 관대해지는 것 같아요",
          "original_form": "근데 같은 물성분인데도 저는 눈 맞는건 좀 관대해지는 것 같아요",
          "speaker_id": "2",
          "time": "20210823 12:50"
        },
        {
          "id": "MDRW2100009984.1.11",
          "form": "눈은 뭔가 털어내면 안젖는데",
          "original_form": "눈은 뭔가 털어내면 안젖는데",
          "speaker_id": "1",
          "time": "20210823 12:50"
        },
        {
          "id": "MDRW2100009984.1.12",
          "form": "비는 확.. 젖으니까요",
          "original_form": "비는 확.. 젖으니까요",
          "speaker_id": "1",
          "time": "20210823 12:50"
        },
        {
          "id": "MDRW2100009984.1.13",
          "form": "역시 그렇죠~교통체증은 답없지만...",
          "original_form": "역시 그렇죠~교통체증은 답없지만...",
          "speaker_id": "2",
          "time": "20210823 12:51"
        },
        {
          "id": "MDRW2100009984.1.14",
          "form": "요즘은 미세먼지 없어서 넘 좋아요",
          "original_form": "요즘은 미세먼지 없어서 넘 좋아요",
          "speaker_id": "2",
          "time": "20210823 12:51"
        },
        {
          "id": "MDRW2100009984.1.15",
          "form": "그건 그래요",
          "original_form": "그건 그래요",
          "speaker_id": "1",
          "time": "20210823 13:02"
        },
        {
          "id": "MDRW2100009984.1.16",
          "form": "비오면 미세먼지가 완전 좋아요 ㅎ",
          "original_form": "비오면 미세먼지가 완전 좋아요 ㅎ",
          "speaker_id": "1",
          "time": "20210823 13:02"
        },
        {
          "id": "MDRW2100009984.1.17",
          "form": "비의 순기능이네요ㅎㅎ",
          "original_form": "비의 순기능이네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 13:32"
        },
        {
          "id": "MDRW2100009984.1.18",
          "form": "지금 밖에 나왔는데 비와서 좀 쌀쌀하네용",
          "original_form": "지금 밖에 나왔는데 비와서 좀 쌀쌀하네용",
          "speaker_id": "2",
          "time": "20210823 13:32"
        },
        {
          "id": "MDRW2100009984.1.19",
          "form": "밖은 시원한데 안은 좀 덥더라구요 ㅠㅠ",
          "original_form": "밖은 시원한데 안은 좀 덥더라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 13:54"
        },
        {
          "id": "MDRW2100009984.1.20",
          "form": "다행히 습기는 덜한 거 같아요",
          "original_form": "다행히 습기는 덜한 거 같아요",
          "speaker_id": "1",
          "time": "20210823 13:54"
        },
        {
          "id": "MDRW2100009984.1.21",
          "form": "그러네요~ㅎㅎ 태풍 상륙한다던데 비 조금 오고 끝났으면 좋겠어요!",
          "original_form": "그러네요~ㅎㅎ 태풍 상륙한다던데 비 조금 오고 끝났으면 좋겠어요!",
          "speaker_id": "2",
          "time": "20210823 14:05"
        },
        {
          "id": "MDRW2100009984.1.22",
          "form": "오 진짜여? 정말 짧게 끝났으면 좋겠네요 흑",
          "original_form": "오 진짜여? 정말 짧게 끝났으면 좋겠네요 흑",
          "speaker_id": "1",
          "time": "20210823 14:30"
        }
      ]
