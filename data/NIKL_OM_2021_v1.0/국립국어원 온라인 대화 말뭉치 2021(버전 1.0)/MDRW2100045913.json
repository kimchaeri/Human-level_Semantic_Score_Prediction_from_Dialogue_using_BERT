      "utterance": [
        {
          "id": "MDRW2100045913.1.1",
          "form": "드라마 보세요?",
          "original_form": "드라마 보세요?",
          "speaker_id": "1",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100045913.1.2",
          "form": "네 봐요",
          "original_form": "네 봐요",
          "speaker_id": "2",
          "time": "20210908 08:37"
        },
        {
          "id": "MDRW2100045913.1.3",
          "form": "펜하 보고",
          "original_form": "펜하 보고",
          "speaker_id": "2",
          "time": "20210908 08:37"
        },
        {
          "id": "MDRW2100045913.1.4",
          "form": "어제는 하이클래스 잠깐봤는데",
          "original_form": "어제는 하이클래스 잠깐봤는데",
          "speaker_id": "2",
          "time": "20210908 08:37"
        },
        {
          "id": "MDRW2100045913.1.5",
          "form": "아직 무슨내용인지파악중이요 ㅎ",
          "original_form": "아직 무슨내용인지파악중이요 ㅎ",
          "speaker_id": "2",
          "time": "20210908 08:37"
        },
        {
          "id": "MDRW2100045913.1.6",
          "form": "펜트하우스 ㅋㅋㅋ 드디어 끝나네요 ㅠ",
          "original_form": "펜트하우스 ㅋㅋㅋ 드디어 끝나네요 ㅠ",
          "speaker_id": "1",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100045913.1.7",
          "form": "글쵸?",
          "original_form": "글쵸?",
          "speaker_id": "2",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100045913.1.8",
          "form": "지난주에 못봤어요",
          "original_form": "지난주에 못봤어요",
          "speaker_id": "2",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100045913.1.9",
          "form": "몰아서 보세용 ㅜㅜ 막장이에여 ㅋㅋ",
          "original_form": "몰아서 보세용 ㅜㅜ 막장이에여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100045913.1.10",
          "form": "지난주가 끝인건가요?",
          "original_form": "지난주가 끝인건가요?",
          "speaker_id": "2",
          "time": "20210908 08:39"
        },
        {
          "id": "MDRW2100045913.1.11",
          "form": "진짜 첨부터 막장이랬는데 ㅎㅎ",
          "original_form": "진짜 첨부터 막장이랬는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 08:39"
        },
        {
          "id": "MDRW2100045913.1.12",
          "form": "아뇨 이번주요 ㅋㅋㅋ",
          "original_form": "아뇨 이번주요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:40"
        },
        {
          "id": "MDRW2100045913.1.13",
          "form": "저도 지난주가 끝인줄 알앗어요 ㅋㅋ",
          "original_form": "저도 지난주가 끝인줄 알앗어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:40"
        },
        {
          "id": "MDRW2100045913.1.14",
          "form": "앗 그래요?",
          "original_form": "앗 그래요?",
          "speaker_id": "2",
          "time": "20210908 08:40"
        },
        {
          "id": "MDRW2100045913.1.15",
          "form": "아직남은건가요?ㅎ",
          "original_form": "아직남은건가요?ㅎ",
          "speaker_id": "2",
          "time": "20210908 08:40"
        },
        {
          "id": "MDRW2100045913.1.16",
          "form": "넘 끄는건아닌건지?",
          "original_form": "넘 끄는건아닌건지?",
          "speaker_id": "2",
          "time": "20210908 08:41"
        },
        {
          "id": "MDRW2100045913.1.17",
          "form": "네네 !!!! 주인공들 다 죽은거같아욬ㅋ",
          "original_form": "네네 !!!! 주인공들 다 죽은거같아욬ㅋ",
          "speaker_id": "1",
          "time": "20210908 08:41"
        },
        {
          "id": "MDRW2100045913.1.18",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210908 08:41"
        },
        {
          "id": "MDRW2100045913.1.19",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "2",
          "time": "20210908 08:41"
        },
        {
          "id": "MDRW2100045913.1.20",
          "form": "네네 ㅠㅠ 다시 살아날수도있어요 ㅋㅋ",
          "original_form": "네네 ㅠㅠ 다시 살아날수도있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:41"
        },
        {
          "id": "MDRW2100045913.1.21",
          "form": "하긴 펜하는 죽었다살아났다 자유자재로 하니 ㅋ",
          "original_form": "하긴 펜하는 죽었다살아났다 자유자재로 하니 ㅋ",
          "speaker_id": "2",
          "time": "20210908 08:42"
        },
        {
          "id": "MDRW2100045913.1.22",
          "form": "로건도 죽나요?",
          "original_form": "로건도 죽나요?",
          "speaker_id": "2",
          "time": "20210908 08:42"
        },
        {
          "id": "MDRW2100045913.1.23",
          "form": "아뇨 살앗어요",
          "original_form": "아뇨 살앗어요",
          "speaker_id": "1",
          "time": "20210908 08:43"
        }
      ]
    }
