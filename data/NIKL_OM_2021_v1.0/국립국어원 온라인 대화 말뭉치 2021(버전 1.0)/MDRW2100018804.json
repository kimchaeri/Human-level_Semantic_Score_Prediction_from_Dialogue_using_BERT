      "utterance": [
        {
          "id": "MDRW2100018804.1.1",
          "form": "놀라는 표정이 귀여워요",
          "original_form": "놀라는 표정이 귀여워요",
          "speaker_id": "1",
          "time": "20210827 12:19"
        },
        {
          "id": "MDRW2100018804.1.2",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100018804.1.3",
          "form": "궁금하네요",
          "original_form": "궁금하네요",
          "speaker_id": "2",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100018804.1.4",
          "form": "나중에 돋보기로 보여주세요",
          "original_form": "나중에 돋보기로 보여주세요",
          "speaker_id": "2",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100018804.1.5",
          "form": "좋아요ㅎㅎ 거미 밥 주려고 파리 잡고있어요!",
          "original_form": "좋아요ㅎㅎ 거미 밥 주려고 파리 잡고있어요!",
          "speaker_id": "1",
          "time": "20210827 12:21"
        },
        {
          "id": "MDRW2100018804.1.6",
          "form": "저도 생선 잡으러 가야겠어요",
          "original_form": "저도 생선 잡으러 가야겠어요",
          "speaker_id": "2",
          "time": "20210827 12:22"
        },
        {
          "id": "MDRW2100018804.1.7",
          "form": "파리는 주로 양시가시나요?",
          "original_form": "파리는 주로 양시가시나요?",
          "speaker_id": "2",
          "time": "20210827 12:22"
        },
        {
          "id": "MDRW2100018804.1.8",
          "form": "옛날엔 양식했는데 입맛이 까다로워서 자연산만 먹더라구요",
          "original_form": "옛날엔 양식했는데 입맛이 까다로워서 자연산만 먹더라구요",
          "speaker_id": "1",
          "time": "20210827 12:23"
        },
        {
          "id": "MDRW2100018804.1.9",
          "form": "우와 참 지배인이시네요.",
          "original_form": "우와 참 지배인이시네요.",
          "speaker_id": "2",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100018804.1.10",
          "form": "거미들이 좋아하겠어요.",
          "original_form": "거미들이 좋아하겠어요.",
          "speaker_id": "2",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018804.1.11",
          "form": "집사의 본분을 다 하려고 해요ㅎㅎ name3주인님은 뭐 하고 계세요?",
          "original_form": "집사의 본분을 다 하려고 해요ㅎㅎ &name3&주인님은 뭐 하고 계세요?",
          "speaker_id": "1",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018804.1.12",
          "form": "이름도 있나요?",
          "original_form": "이름도 있나요?",
          "speaker_id": "2",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018804.1.13",
          "form": "name4이요??",
          "original_form": "&name4&이요??",
          "speaker_id": "1",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018804.1.14",
          "form": "아 이름이 name4인 거에요.",
          "original_form": "아 이름이 &name4&인 거에요.",
          "speaker_id": "2",
          "time": "20210827 12:44"
        },
        {
          "id": "MDRW2100018804.1.15",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210827 12:44"
        },
        {
          "id": "MDRW2100018804.1.16",
          "form": "네ㅎㅎ 거미 이름이 name4이에요!",
          "original_form": "네ㅎㅎ 거미 이름이 &name4&이에요!",
          "speaker_id": "1",
          "time": "20210827 12:45"
        },
        {
          "id": "MDRW2100018804.1.17",
          "form": "이런",
          "original_form": "이런",
          "speaker_id": "2",
          "time": "20210827 12:47"
        },
        {
          "id": "MDRW2100018804.1.18",
          "form": "실수를 했네요",
          "original_form": "실수를 했네요",
          "speaker_id": "2",
          "time": "20210827 12:47"
        },
        {
          "id": "MDRW2100018804.1.19",
          "form": "성이 name4고 이름이 name4인 건가요?",
          "original_form": "성이 &name4&고 이름이 &name4&인 건가요?",
          "speaker_id": "2",
          "time": "20210827 12:47"
        },
        {
          "id": "MDRW2100018804.1.20",
          "form": "성은 무음처리했어요! 바프처럼요ㅎㅎ 이름이 name4이에요",
          "original_form": "성은 무음처리했어요! 바프처럼요ㅎㅎ 이름이 &name4&이에요",
          "speaker_id": "1",
