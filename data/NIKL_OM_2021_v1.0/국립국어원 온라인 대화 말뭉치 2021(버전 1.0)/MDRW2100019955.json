        },
        {
          "id": "MDRW2100019955.1.2",
          "form": "비엔나 먹고싶네오",
          "original_form": "비엔나 먹고싶네오",
          "speaker_id": "2",
          "time": "20210827 22:58"
        },
        {
          "id": "MDRW2100019955.1.3",
          "form": "분홍소세지",
          "original_form": "분홍소세지",
          "speaker_id": "2",
          "time": "20210827 22:58"
        },
        {
          "id": "MDRW2100019955.1.4",
          "form": "계란을 함께",
          "original_form": "계란을 함께",
          "speaker_id": "1",
          "time": "20210827 23:00"
        },
        {
          "id": "MDRW2100019955.1.5",
          "form": "왠지 옛날도시락이",
          "original_form": "왠지 옛날도시락이",
          "speaker_id": "1",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100019955.1.6",
          "form": "계란은 애호박과도",
          "original_form": "계란은 애호박과도",
          "speaker_id": "2",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100019955.1.7",
          "form": "옛도 좋쵸",
          "original_form": "옛도 좋쵸",
          "speaker_id": "2",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100019955.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100019955.1.9",
          "form": "계란찜도 최고의 반찬 or 안주가 되지요",
          "original_form": "계란찜도 최고의 반찬 or 안주가 되지요",
          "speaker_id": "1",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100019955.1.10",
          "form": "계란은 참 많이 다양하게 사용할 수 있는 만능같습니다.",
          "original_form": "계란은 참 많이 다양하게 사용할 수 있는 만능같습니다.",
          "speaker_id": "1",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019955.1.11",
          "form": "완전식품인가-?)",
          "original_form": "완전식품인가-?)",
          "speaker_id": "2",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019955.1.12",
          "form": "그렇다고 봐야지요 하하",
          "original_form": "그렇다고 봐야지요 하하",
          "speaker_id": "1",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019955.1.13",
          "form": "누가 계란 먹을 생각을 한건지",
          "original_form": "누가 계란 먹을 생각을 한건지",
          "speaker_id": "1",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019955.1.14",
          "form": "참 신기방기하네요",
          "original_form": "참 신기방기하네요",
          "speaker_id": "1",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019955.1.15",
          "form": "잠좀 자야겠어요 졸러서ㅠ",
          "original_form": "잠좀 자야겠어요 졸러서ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019955.1.16",
          "form": "네 편안한 밤 되세요",
          "original_form": "네 편안한 밤 되세요",
          "speaker_id": "1",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019955.1.17",
          "form": "내일 또 대화를 시도할께요 대표님",
          "original_form": "내일 또 대화를 시도할께요 대표님",
          "speaker_id": "1",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019955.1.18",
          "form": "",
          "original_form": "",
          "speaker_id": "1",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019955.1.19",
          "form": "없는게 없죠",
          "original_form": "없는게 없죠",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100019955.1.20",
          "form": "와인을 좋아하시나요",
          "original_form": "와인을 좋아하시나요",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100019955.1.21",
          "form": "가끔은 좋더라고요",
          "original_form": "가끔은 좋더라고요",
          "speaker_id": "2",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100019955.1.22",
          "form": "저는 종종..",
          "original_form": "저는 종종..",
          "speaker_id": "1",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100019955.1.23",
          "form": "딱히 술을 즐기지 않아서요",
          "original_form": "딱히 술을 즐기지 않아서요",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100019955.1.24",
          "form": "커피파입니다",
          "original_form": "커피파입니다",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100019955.1.25",
          "form": "전 늘 술이라서",
          "original_form": "전 늘 술이라서",
          "speaker_id": "2",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100019955.1.26",
          "form": "술은 못먹지 않는데",
          "original_form": "술은 못먹지 않는데",
          "speaker_id": "1",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100019955.1.27",
          "form": "다음날의 숙취가 너무 싫네요",
          "original_form": "다음날의 숙취가 너무 싫네요",
          "speaker_id": "1",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100019955.1.28",
          "form": "안주를 거의 안먹는 스타일이라",
          "original_form": "안주를 거의 안먹는 스타일이라",
          "speaker_id": "1",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100019955.1.29",
          "form": "물만드시는군요",
          "original_form": "물만드시는군요",
          "speaker_id": "2",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100019955.1.30",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100019955.1.31",
          "form": "물 또는 커피",
          "original_form": "물 또는 커피",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100019955.1.32",
          "form": "아이스크림?",
          "original_form": "아이스크림?",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100019955.1.33",
          "form": "오~~커피도 너무 많이드심 좋은건아니라서",
          "original_form": "오~~커피도 너무 많이드심 좋은건아니라서",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100019955.1.34",
          "form": "안주빨? 있어야지 숙취가 없다던데",
          "original_form": "안주빨? 있어야지 숙취가 없다던데",
          "speaker_id": "1",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100019955.1.35",
          "form": "맞습니다",
          "original_form": "맞습니다",
          "speaker_id": "1",
