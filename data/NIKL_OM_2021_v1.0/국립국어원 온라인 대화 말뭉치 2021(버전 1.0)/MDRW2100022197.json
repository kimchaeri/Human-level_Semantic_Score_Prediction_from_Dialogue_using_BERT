      "utterance": [
        {
          "id": "MDRW2100022197.1.1",
          "form": "관심은 있어요",
          "original_form": "관심은 있어요",
          "speaker_id": "2",
          "time": "20210829 12:21"
        },
        {
          "id": "MDRW2100022197.1.2",
          "form": "가서 관람한 적은 없구요",
          "original_form": "가서 관람한 적은 없구요",
          "speaker_id": "2",
          "time": "20210829 12:21"
        },
        {
          "id": "MDRW2100022197.1.3",
          "form": "영화는 조아하시나요",
          "original_form": "영화는 조아하시나요",
          "speaker_id": "1",
          "time": "20210829 12:55"
        },
        {
          "id": "MDRW2100022197.1.4",
          "form": "네 가끔 보러가요",
          "original_form": "네 가끔 보러가요",
          "speaker_id": "2",
          "time": "20210829 13:10"
        },
        {
          "id": "MDRW2100022197.1.5",
          "form": "최근에 어떤거보셨나요",
          "original_form": "최근에 어떤거보셨나요",
          "speaker_id": "1",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100022197.1.6",
          "form": "요샌 보러 못갔어요",
          "original_form": "요샌 보러 못갔어요",
          "speaker_id": "2",
          "time": "20210829 13:18"
        },
        {
          "id": "MDRW2100022197.1.7",
          "form": "보신거 있나요?",
          "original_form": "보신거 있나요?",
          "speaker_id": "2",
          "time": "20210829 13:18"
        },
        {
          "id": "MDRW2100022197.1.8",
          "form": "모가디슈요!",
          "original_form": "모가디슈요!",
          "speaker_id": "1",
          "time": "20210829 13:19"
        },
        {
          "id": "MDRW2100022197.1.9",
          "form": "재난영화맞죠? 재밌나요?",
          "original_form": "재난영화맞죠? 재밌나요?",
          "speaker_id": "2",
          "time": "20210829 13:22"
        },
        {
          "id": "MDRW2100022197.1.10",
          "form": "재밌어요! 조인성도나오구",
          "original_form": "재밌어요! 조인성도나오구",
          "speaker_id": "1",
          "time": "20210829 13:23"
        },
        {
          "id": "MDRW2100022197.1.11",
          "form": "전 싱크홀 보려고 생각중이에요",
          "original_form": "전 싱크홀 보려고 생각중이에요",
          "speaker_id": "2",
          "time": "20210829 13:25"
        },
        {
          "id": "MDRW2100022197.1.12",
          "form": "싱크홀이 재난영화죠?",
          "original_form": "싱크홀이 재난영화죠?",
          "speaker_id": "1",
          "time": "20210829 13:26"
        },
        {
          "id": "MDRW2100022197.1.13",
          "form": "네 근데 코믹이 들어가서 재밌을거 같아요.",
          "original_form": "네 근데 코믹이 들어가서 재밌을거 같아요.",
          "speaker_id": "2",
          "time": "20210829 13:29"
        },
        {
          "id": "MDRW2100022197.1.14",
          "form": "광수나오는거 맞나요ㅎㅎ",
          "original_form": "광수나오는거 맞나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 13:30"
        },
        {
          "id": "MDRW2100022197.1.15",
          "form": "네 맞아요ㅋㅋ",
          "original_form": "네 맞아요ㅋㅋ",
