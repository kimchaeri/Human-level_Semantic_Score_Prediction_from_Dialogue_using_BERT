      "utterance": [
        {
          "id": "MDRW2100020322.1.1",
          "form": "별로 안 친한 후배잇는데",
          "original_form": "별로 안 친한 후배잇는데",
          "speaker_id": "1",
          "time": "20210906 14:21"
        },
        {
          "id": "MDRW2100020322.1.2",
          "form": "분당산다고하더라고요",
          "original_form": "분당산다고하더라고요",
          "speaker_id": "1",
          "time": "20210906 14:21"
        },
        {
          "id": "MDRW2100020322.1.3",
          "form": "저랑 동갑일ㄷㅡㅅ",
          "original_form": "저랑 동갑일ㄷㅡㅅ",
          "speaker_id": "2",
          "time": "20210906 14:21"
        },
        {
          "id": "MDRW2100020322.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210906 14:22"
        },
        {
          "id": "MDRW2100020322.1.5",
          "form": "딱히 추억은 없음",
          "original_form": "딱히 추억은 없음",
          "speaker_id": "1",
          "time": "20210906 14:22"
        },
        {
          "id": "MDRW2100020322.1.6",
          "form": "걍 걔 친구들이",
          "original_form": "걍 걔 친구들이",
          "speaker_id": "1",
          "time": "20210906 14:22"
        },
        {
          "id": "MDRW2100020322.1.7",
          "form": "별로였음",
          "original_form": "별로였음",
          "speaker_id": "1",
          "time": "20210906 14:22"
        },
        {
          "id": "MDRW2100020322.1.8",
          "form": "전해줄게요",
          "original_form": "전해줄게요",
          "speaker_id": "2",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100020322.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100020322.1.10",
          "form": "사실임",
          "original_form": "사실임",
          "speaker_id": "1",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100020322.1.11",
          "form": "어떻게 별로인데요?",
          "original_form": "어떻게 별로인데요?",
          "speaker_id": "2",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100020322.1.12",
          "form": "근데 박씨 절친의 여친이",
          "original_form": "근데 박씨 절친의 여친이",
          "speaker_id": "2",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100020322.1.13",
          "form": "제친구 무리아닌가요",
          "original_form": "제친구 무리아닌가요",
          "speaker_id": "2",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100020322.1.14",
          "form": "ㄴㄴ무리는 아니었고",
          "original_form": "ㄴㄴ무리는 아니었고",
          "speaker_id": "1",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100020322.1.15",
          "form": "걍 신씨만 친했죠",
          "original_form": "걍 신씨만 친했죠",
          "speaker_id": "1",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100020322.1.16",
          "form": "어라",
          "original_form": "어라",
          "speaker_id": "1",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100020322.1.17",
          "form": "무리는 아녔음",
          "original_form": "무리는 아녔음",
          "speaker_id": "1",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100020322.1.18",
          "form": "앗항",
          "original_form": "앗항",
          "speaker_id": "2",
          "time": "20210906 14:55"
        },
        {
          "id": "MDRW2100020322.1.19",
          "form": "그랫군",
          "original_form": "그랫군",
          "speaker_id": "2",
          "time": "20210906 14:55"
        },
        {
          "id": "MDRW2100020322.1.20",
          "form": "신씨가 이래저래인맥이 혼자넓네요",
          "original_form": "신씨가 이래저래인맥이 혼자넓네요",
          "speaker_id": "2",
          "time": "20210906 14:55"
        },
        {
          "id": "MDRW2100020322.1.21",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210906 14:57"
        },
        {
          "id": "MDRW2100020322.1.22",
          "form": "인맥은 나죠",
          "original_form": "인맥은 나죠",
          "speaker_id": "1",
          "time": "20210906 14:58"
        },
        {
          "id": "MDRW2100020322.1.23",
          "form": "아예그렇다고 칠게요",
          "original_form": "아예그렇다고 칠게요",
          "speaker_id": "2",
          "time": "20210906 15:12"
        },
        {
          "id": "MDRW2100020322.1.24",
          "form": "고등학생때는",
          "original_form": "고등학생때는",
          "speaker_id": "1",
          "time": "20210906 15:13"
        },
        {
          "id": "MDRW2100020322.1.25",
          "form": "시끄럽다고 소문났던듯",
          "original_form": "시끄럽다고 소문났던듯",
          "speaker_id": "1",
          "time": "20210906 15:13"
        },
        {
          "id": "MDRW2100020322.1.26",
          "form": "ㅋㅋ ㅋㅋㅋㅋ지금도 똑같이 소문나고있음",
          "original_form": "ㅋㅋ ㅋㅋㅋㅋ지금도 똑같이 소문나고있음",
          "speaker_id": "2",
          "time": "20210906 15:15"
        },
        {
          "id": "MDRW2100020322.1.27",
          "form": "며칠전에",
          "original_form": "며칠전에",
          "speaker_id": "1",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100020322.1.28",
          "form": "제 친구가 affiliation 동갑 여친 사귀었는데",
          "original_form": "제 친구가 &affiliation& 동갑 여친 사귀었는데",
          "speaker_id": "1",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100020322.1.29",
          "form": "저 아냐고 물어봤는데 안다했대요",
          "original_form": "저 아냐고 물어봤는데 안다했대요",
          "speaker_id": "1",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100020322.1.30",
          "form": "근데 반대는 모름",
          "original_form": "근데 반대는 모름",
          "speaker_id": "1",
          "time": "20210906 15:17"
        },
