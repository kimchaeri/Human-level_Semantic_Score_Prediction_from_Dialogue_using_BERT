      "utterance": [
        {
          "id": "MDRW2100004355.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004355.1.2",
          "form": "혼자.. 사시나용...?",
          "original_form": "혼자.. 사시나용...?",
          "speaker_id": "1",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004355.1.3",
          "form": "넵 저는 혼자 살아요! 가사노동 귀찮습니다",
          "original_form": "넵 저는 혼자 살아요! 가사노동 귀찮습니다",
          "speaker_id": "2",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004355.1.4",
          "form": "가사 및 가족은 무슨 얘길 해야하지 ",
          "original_form": "가사 및 가족은 무슨 얘길 해야하지 ",
          "speaker_id": "1",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004355.1.5",
          "form": "아 그렇죸ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 그렇죸ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004355.1.6",
          "form": "혼자 사세요??",
          "original_form": "혼자 사세요??",
          "speaker_id": "2",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004355.1.7",
          "form": "저는 본가에서 살아요",
          "original_form": "저는 본가에서 살아요",
          "speaker_id": "1",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004355.1.8",
          "form": "한번도 혼자 살아본적이 없어서",
          "original_form": "한번도 혼자 살아본적이 없어서",
          "speaker_id": "1",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004355.1.9",
          "form": "약간의 로망이 있습니당",
          "original_form": "약간의 로망이 있습니당",
          "speaker_id": "1",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004355.1.10",
          "form": "오..",
          "original_form": "오..",
          "speaker_id": "2",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004355.1.11",
          "form": "자취는.....머랄까...",
          "original_form": "자취는.....머랄까...",
          "speaker_id": "2",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004355.1.12",
          "form": "반반입니다",
          "original_form": "반반입니다",
          "speaker_id": "2",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004355.1.13",
          "form": "두근",
          "original_form": "두근",
          "speaker_id": "1",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004355.1.14",
          "form": "즐거움과 고통을",
          "original_form": "즐거움과 고통을",
          "speaker_id": "2",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004355.1.15",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004355.1.16",
          "form": "모두 느낄 수 있죠",
          "original_form": "모두 느낄 수 있죠",
          "speaker_id": "2",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004355.1.17",
          "form": "저는 긍데",
          "original_form": "저는 긍데",
          "speaker_id": "1",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004355.1.18",
          "form": "치우는걸 싫어해서",
          "original_form": "치우는걸 싫어해서",
          "speaker_id": "1",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004355.1.19",
          "form": "처음 부터 어지르질 않아요",
          "original_form": "처음 부터 어지르질 않아요",
          "speaker_id": "1",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004355.1.20",
          "form": "그래서 아마 혼자 살면",
          "original_form": "그래서 아마 혼자 살면",
          "speaker_id": "1",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004355.1.21",
          "form": "오 그럼 잘 사시겠는데요!??",
          "original_form": "오 그럼 잘 사시겠는데요!??",
          "speaker_id": "2",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004355.1.22",
          "form": "아사한 변사체로 발견되지 않을가..",
          "original_form": "아사한 변사체로 발견되지 않을가..",
          "speaker_id": "1",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004355.1.23",
          "form": "앗?",
          "original_form": "앗?",
          "speaker_id": "2",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004355.1.24",
          "form": "치우기 귀찮아서 밥을 안 해먹거든요..",
          "original_form": "치우기 귀찮아서 밥을 안 해먹거든요..",
          "speaker_id": "1",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004355.1.25",
          "form": "아아아",
          "original_form": "아아아",
          "speaker_id": "2",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004355.1.26",
          "form": "저는 오히려 혼자사니까",
          "original_form": "저는 오히려 혼자사니까",
          "speaker_id": "2",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004355.1.27",
          "form": "해먹는거 좋아해가지고",
          "original_form": "해먹는거 좋아해가지고",
          "speaker_id": "2",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004355.1.28",
          "form": "우와 부지런하시네요 멋지다",
          "original_form": "우와 부지런하시네요 멋지다",
          "speaker_id": "1",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004355.1.29",
          "form": "하지만 특별한걸 해먹진 않아요..!",
          "original_form": "하지만 특별한걸 해먹진 않아요..!",
          "speaker_id": "2",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004355.1.30",
          "form": "전 라면까지가 한계라서",
          "original_form": "전 라면까지가 한계라서",
          "speaker_id": "1",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004355.1.31",
          "form": "다이어트식으로 간단하게..!?",
          "original_form": "다이어트식으로 간단하게..!?",
          "speaker_id": "2",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004355.1.32",
          "form": "밥 잘하는 배우자를 만나야지 결심중입니다",
          "original_form": "밥 잘하는 배우자를 만나야지 결심중입니다",
          "speaker_id": "1",
          "time": "20210821 16:10"
        },
        {
          "id": "MDRW2100004355.1.33",
          "form": "집에 살때는 오히려 다이어트식 챙겨먹는게",
          "original_form": "집에 살때는 오히려 다이어트식 챙겨먹는게",
          "speaker_id": "2",
          "time": "20210821 16:10"
        },
        {
          "id": "MDRW2100004355.1.34",
          "form": "저는 빨래를 잘해요..!",
          "original_form": "저는 빨래를 잘해요..!",
          "speaker_id": "1",
          "time": "20210821 16:10"
        },
        {
          "id": "MDRW2100004355.1.35",
          "form": "쉽지않았는데 혼자사니까 그건 좋더라구요",
          "original_form": "쉽지않았는데 혼자사니까 그건 좋더라구요",
          "speaker_id": "2",
          "time": "20210821 16:10"
        },
        {
          "id": "MDRW2100004355.1.36",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210821 16:10"
        },
        {
          "id": "MDRW2100004355.1.37",
          "form": "요리잘하는 남자",
          "original_form": "요리잘하는 남자",
          "speaker_id": "2",
          "time": "20210821 16:10"
        },
        {
          "id": "MDRW2100004355.1.38",
          "form": "오... 그렇겠네요",
          "original_form": "오... 그렇겠네요",
          "speaker_id": "1",
          "time": "20210821 16:10"
        },
        {
          "id": "MDRW2100004355.1.39",
          "form": "최고...",
          "original_form": "최고...",
          "speaker_id": "2",
          "time": "20210821 16:10"
        },
        {
          "id": "MDRW2100004355.1.40",
          "form": "최고....",
          "original_form": "최고....",
          "speaker_id": "1",
          "time": "20210821 16:10"
        }
      ]
