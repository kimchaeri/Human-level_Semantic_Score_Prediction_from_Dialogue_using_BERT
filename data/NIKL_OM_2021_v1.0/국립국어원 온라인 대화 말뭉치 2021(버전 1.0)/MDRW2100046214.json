      "utterance": [
        {
          "id": "MDRW2100046214.1.1",
          "form": "스벅 신메뉴나왔어요",
          "original_form": "스벅 신메뉴나왔어요",
          "speaker_id": "1",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046214.1.2",
          "form": "오 맛나는건가요?",
          "original_form": "오 맛나는건가요?",
          "speaker_id": "2",
          "time": "20210908 11:17"
        },
        {
          "id": "MDRW2100046214.1.3",
          "form": "글레이즈드는 기존거같은데",
          "original_form": "글레이즈드는 기존거같은데",
          "speaker_id": "1",
          "time": "20210908 11:17"
        },
        {
          "id": "MDRW2100046214.1.4",
          "form": "별잔치도 같이하나요?",
          "original_form": "별잔치도 같이하나요?",
          "speaker_id": "2",
          "time": "20210908 11:17"
        },
        {
          "id": "MDRW2100046214.1.5",
          "form": "어제 3개줘서 한잔먹었네요",
          "original_form": "어제 3개줘서 한잔먹었네요",
          "speaker_id": "1",
          "time": "20210908 11:17"
        },
        {
          "id": "MDRW2100046214.1.6",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210908 11:17"
        },
        {
          "id": "MDRW2100046214.1.7",
          "form": "텀블가져가서 별5개ㅎㅎ",
          "original_form": "텀블가져가서 별5개ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 11:17"
        },
        {
          "id": "MDRW2100046214.1.8",
          "form": "별쌓으러가야겠네요",
          "original_form": "별쌓으러가야겠네요",
          "speaker_id": "2",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046214.1.9",
          "form": "신메뉴 종류많아요?",
          "original_form": "신메뉴 종류많아요?",
          "speaker_id": "2",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046214.1.10",
          "form": "글레이즈드라떼 맛나요",
          "original_form": "글레이즈드라떼 맛나요",
          "speaker_id": "1",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046214.1.11",
          "form": "진하고 달달하니",
          "original_form": "진하고 달달하니",
          "speaker_id": "1",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046214.1.12",
          "form": "달달구리",
          "original_form": "달달구리",
          "speaker_id": "2",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046214.1.13",
          "form": "좋아하는데",
          "original_form": "좋아하는데",
          "speaker_id": "2",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046214.1.14",
          "form": "들려야겠네요",
          "original_form": "들려야겠네요",
          "speaker_id": "2",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046214.1.15",
          "form": "나머지 2개 있어요 녹차라 티종류인가",
          "original_form": "나머지 2개 있어요 녹차라 티종류인가",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046214.1.16",
          "form": "아 녹차?",
          "original_form": "아 녹차?",
          "speaker_id": "2",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046214.1.17",
          "form": "근데 비싸네요 라떼인데",
          "original_form": "근데 비싸네요 라떼인데",
          "speaker_id": "1",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046214.1.18",
          "form": "말차라인인가?",
          "original_form": "말차라인인가?",
          "speaker_id": "2",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046214.1.19",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046214.1.20",
          "form": "비스무리한듯요",
          "original_form": "비스무리한듯요",
          "speaker_id": "1",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046214.1.21",
          "form": "신메뉴는 가격들이 후덜덜",
          "original_form": "신메뉴는 가격들이 후덜덜",
          "speaker_id": "2",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100046214.1.22",
          "form": "글레이즈가 6100원",
          "original_form": "글레이즈가 6100원",
          "speaker_id": "1",
