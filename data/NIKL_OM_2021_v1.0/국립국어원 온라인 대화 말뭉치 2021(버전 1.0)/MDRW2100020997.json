      "utterance": [
        {
          "id": "MDRW2100020997.1.1",
          "form": "아 저도 아이유 콘서트 가보고싶어요 ㅜㅜ",
          "original_form": "아 저도 아이유 콘서트 가보고싶어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 14:50"
        },
        {
          "id": "MDRW2100020997.1.2",
          "form": "아는 노래도 많아서ㅜㅠ 좋을거같아요",
          "original_form": "아는 노래도 많아서ㅜㅠ 좋을거같아요",
          "speaker_id": "1",
          "time": "20210828 14:51"
        },
        {
          "id": "MDRW2100020997.1.3",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210828 14:54"
        },
        {
          "id": "MDRW2100020997.1.4",
          "form": "떼창 저도 해보고싶어요",
          "original_form": "떼창 저도 해보고싶어요",
          "speaker_id": "2",
          "time": "20210828 14:54"
        },
        {
          "id": "MDRW2100020997.1.5",
          "form": "맞아요 ㅜㅜ 그 체조경기장에서 한거 보셨나요?",
          "original_form": "맞아요 ㅜㅜ 그 체조경기장에서 한거 보셨나요?",
          "speaker_id": "1",
          "time": "20210828 14:57"
        },
        {
          "id": "MDRW2100020997.1.6",
          "form": "엇 어떤 곡이에요??",
          "original_form": "엇 어떤 곡이에요??",
          "speaker_id": "2",
          "time": "20210828 15:36"
        },
        {
          "id": "MDRW2100020997.1.7",
          "form": "그 이름에게요 !",
          "original_form": "그 이름에게요 !",
          "speaker_id": "1",
          "time": "20210828 15:57"
        },
        {
          "id": "MDRW2100020997.1.8",
          "form": "아아 본것같아요",
          "original_form": "아아 본것같아요",
          "speaker_id": "2",
          "time": "20210828 17:40"
        },
        {
          "id": "MDRW2100020997.1.9",
          "form": "히외가수에 관심있으세요?",
          "original_form": "히외가수에 관심있으세요?",
          "speaker_id": "2",
          "time": "20210828 17:40"
        },
        {
          "id": "MDRW2100020997.1.10",
          "form": "제가 전에 크리스토퍼 내한한거 유튜브로 봤는데",
          "original_form": "제가 전에 크리스토퍼 내한한거 유튜브로 봤는데",
          "speaker_id": "2",
          "time": "20210828 17:41"
        },
        {
          "id": "MDRW2100020997.1.11",
          "form": "재밌어보이더라구요",
          "original_form": "재밌어보이더라구요",
          "speaker_id": "2",
          "time": "20210828 17:41"
        },
        {
          "id": "MDRW2100020997.1.12",
          "form": "크게 관심은 없는데 내한한거는 유튭으로 봤어요 ㅋㅋ",
          "original_form": "크게 관심은 없는데 내한한거는 유튭으로 봤어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:51"
        },
        {
          "id": "MDRW2100020997.1.13",
          "form": "그그 bad 떼창하는거 맞죠?",
          "original_form": "그그 bad 떼창하는거 맞죠?",
          "speaker_id": "1",
          "time": "20210828 17:52"
        },
        {
          "id": "MDRW2100020997.1.14",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "2",
          "time": "20210828 17:52"
        },
        {
          "id": "MDRW2100020997.1.15",
          "form": "같은거 봣나봐요ㅋㅋㅋㅋ",
          "original_form": "같은거 봣나봐요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:52"
        },
        {
          "id": "MDRW2100020997.1.16",
          "form": "알고리즘에 떠서 봤는데 ㅋㅋ 대박이였어요",
          "original_form": "알고리즘에 떠서 봤는데 ㅋㅋ 대박이였어요",
          "speaker_id": "1",
          "time": "20210828 17:52"
        },
        {
          "id": "MDRW2100020997.1.17",
          "form": "제가 가수가 되고싶던걸요 ㅜㅜ",
          "original_form": "제가 가수가 되고싶던걸요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 17:53"
        },
        {
          "id": "MDRW2100020997.1.18",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210828 17:53"
        },
        {
          "id": "MDRW2100020997.1.19",
          "form": "다른 나라 사람들이 자기 노래 부르면 진짜 기분 좋을거같아요",
          "original_form": "다른 나라 사람들이 자기 노래 부르면 진짜 기분 좋을거같아요",
          "speaker_id": "2",
          "time": "20210828 17:53"
        },
        {
          "id": "MDRW2100020997.1.20",
          "form": "맞아요 !",
          "original_form": "맞아요 !",
          "speaker_id": "1",
          "time": "20210828 17:53"
        },
        {
          "id": "MDRW2100020997.1.21",
          "form": "근데 또 보면 일본은 엄청 조용하게 보더라구요 ㅋㅋ",
          "original_form": "근데 또 보면 일본은 엄청 조용하게 보더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:54"
        },
        {
          "id": "MDRW2100020997.1.22",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210828 17:54"
        },
        {
          "id": "MDRW2100020997.1.23",
          "form": "일본 콘서트 영상 봤는데",
          "original_form": "일본 콘서트 영상 봤는데",
          "speaker_id": "2",
          "time": "20210828 17:54"
        },
        {
          "id": "MDRW2100020997.1.24",
          "form": "호응도 없길래 존 놀랬어요",
          "original_form": "호응도 없길래 존 놀랬어요",
          "speaker_id": "2",
          "time": "20210828 17:54"
        },
        {
          "id": "MDRW2100020997.1.25",
          "form": "그렇게  조용히 들을줄이야..",
          "original_form": "그렇게  조용히 들을줄이야..",
          "speaker_id": "2",
          "time": "20210828 17:54"
        },
        {
          "id": "MDRW2100020997.1.26",
          "form": "한국이랑은 분위기가 완전 다르더라구요",
          "original_form": "한국이랑은 분위기가 완전 다르더라구요",
          "speaker_id": "2",
          "time": "20210828 17:55"
        },
        {
          "id": "MDRW2100020997.1.27",
          "form": "아 진짜요 ㅋㅋ 호응이 전~~혀 없어서",
          "original_form": "아 진짜요 ㅋㅋ 호응이 전~~혀 없어서",
          "speaker_id": "1",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100020997.1.28",
          "form": "제가 가수라면 민망할거같아요ㆍ ㅋㄱㅋ",
          "original_form": "제가 가수라면 민망할거같아요ㆍ ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100020997.1.29",
