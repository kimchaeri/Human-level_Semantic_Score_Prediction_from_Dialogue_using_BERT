      "utterance": [
        {
          "id": "MDRW2100039116.1.1",
          "form": "그러니까요~ 저도 징징거리는거 진짜 싫어하거든요",
          "original_form": "그러니까요~ 저도 징징거리는거 진짜 싫어하거든요",
          "speaker_id": "1",
          "time": "20210903 10:33"
        },
        {
          "id": "MDRW2100039116.1.2",
          "form": "그래서 회사에서 저런 징징이들만 자기 성과 인정받을때",
          "original_form": "그래서 회사에서 저런 징징이들만 자기 성과 인정받을때",
          "speaker_id": "1",
          "time": "20210903 10:33"
        },
        {
          "id": "MDRW2100039116.1.3",
          "form": "너무 짜증나요",
          "original_form": "너무 짜증나요",
          "speaker_id": "1",
          "time": "20210903 10:33"
        },
        {
          "id": "MDRW2100039116.1.4",
          "form": "그니깐여 나는 징징이들 징징대는거도 받아줘야하지 일도하지",
          "original_form": "그니깐여 나는 징징이들 징징대는거도 받아줘야하지 일도하지",
          "speaker_id": "2",
          "time": "20210903 10:34"
        },
        {
          "id": "MDRW2100039116.1.5",
          "form": "불공평해요",
          "original_form": "불공평해요",
          "speaker_id": "2",
          "time": "20210903 10:35"
        },
        {
          "id": "MDRW2100039116.1.6",
          "form": "맞아요..... 대 공감입니다...",
          "original_form": "맞아요..... 대 공감입니다...",
          "speaker_id": "1",
          "time": "20210903 10:35"
        },
        {
          "id": "MDRW2100039116.1.7",
          "form": "징징이들 징징거리는거 받아주는거 너무 힘들어요",
          "original_form": "징징이들 징징거리는거 받아주는거 너무 힘들어요",
          "speaker_id": "1",
          "time": "20210903 10:35"
        },
        {
          "id": "MDRW2100039116.1.8",
          "form": "우리도 똑같이 그 일 하고있는건데 ㅋㅋㅋㅋㅋ하",
          "original_form": "우리도 똑같이 그 일 하고있는건데 ㅋㅋㅋㅋㅋ하",
          "speaker_id": "1",
          "time": "20210903 10:35"
        },
        {
          "id": "MDRW2100039116.1.9",
          "form": "정말 그만두고싶네요..!!",
          "original_form": "정말 그만두고싶네요..!!",
          "speaker_id": "2",
          "time": "20210903 10:37"
        },
        {
          "id": "MDRW2100039116.1.10",
          "form": "1인기업할수있는거 없나요 진심...",
          "original_form": "1인기업할수있는거 없나요 진심...",
          "speaker_id": "2",
          "time": "20210903 10:37"
        },
        {
          "id": "MDRW2100039116.1.11",
          "form": "그러니까요...흑흑... name2님 진짜 블로그나 유투브 작게라도 시작해보세요 ㅠ 전 믿어요 name2님 센스",
          "original_form": "그러니까요...흑흑... &name2&님 진짜 블로그나 유투브 작게라도 시작해보세요 ㅠ 전 믿어요 &name2&님 센스",
          "speaker_id": "1",
          "time": "20210903 10:39"
        },
        {
          "id": "MDRW2100039116.1.12",
          "form": "게을러서 못해요 ...ㅜㅜ",
          "original_form": "게을러서 못해요 ...ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 10:39"
        },
        {
          "id": "MDRW2100039116.1.13",
          "form": "저두요.......흑 ㅋㅋㅋㅋ 전 센스도 없지만요ㅠ",
          "original_form": "저두요.......흑 ㅋㅋㅋㅋ 전 센스도 없지만요ㅠ",
          "speaker_id": "1",
          "time": "20210903 10:40"
        },
        {
          "id": "MDRW2100039116.1.14",
          "form": "그런거로 보면 전 그냥 회사다니는게 답인것 같긴한디",
          "original_form": "그런거로 보면 전 그냥 회사다니는게 답인것 같긴한디",
          "speaker_id": "1",
          "time": "20210903 10:40"
        },
        {
          "id": "MDRW2100039116.1.15",
          "form": "너무 귀찮네요 회사생활마저도...ㅠ",
          "original_form": "너무 귀찮네요 회사생활마저도...ㅠ",
