      "utterance": [
        {
          "id": "MDRW2100042870.1.1",
          "form": "제주에서 계시는동안엔 따로 여행 안하시겠어요",
          "original_form": "제주에서 계시는동안엔 따로 여행 안하시겠어요",
          "speaker_id": "1",
          "time": "20210907 12:44"
        },
        {
          "id": "MDRW2100042870.1.2",
          "form": "서울에 친구보러 가끔 갈 듯 해요^^",
          "original_form": "서울에 친구보러 가끔 갈 듯 해요^^",
          "speaker_id": "2",
          "time": "20210907 15:03"
        },
        {
          "id": "MDRW2100042870.1.3",
          "form": "여행은 사람이잖아요",
          "original_form": "여행은 사람이잖아요",
          "speaker_id": "2",
          "time": "20210907 15:03"
        },
        {
          "id": "MDRW2100042870.1.4",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:11"
        },
        {
          "id": "MDRW2100042870.1.5",
          "form": "아끼는 여행 기념품 있으세요",
          "original_form": "아끼는 여행 기념품 있으세요",
          "speaker_id": "2",
          "time": "20210907 15:11"
        },
        {
          "id": "MDRW2100042870.1.6",
          "form": "제주가서 살면 매일이 여행일거같고",
          "original_form": "제주가서 살면 매일이 여행일거같고",
          "speaker_id": "1",
          "time": "20210907 15:11"
        },
        {
          "id": "MDRW2100042870.1.7",
          "form": "전 과자만 몽땅 사와서ㅋㅋ 먹는게 남는거라며",
          "original_form": "전 과자만 몽땅 사와서ㅋㅋ 먹는게 남는거라며",
          "speaker_id": "1",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100042870.1.8",
          "form": "현지친구들이 준 텀블러있어요!",
          "original_form": "현지친구들이 준 텀블러있어요!",
          "speaker_id": "1",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100042870.1.9",
          "form": "저는 일본여행다녀 와서 친구가 자개 병뚜껑 건 손톱깍기 있는데 지금도 잘 써요",
          "original_form": "저는 일본여행다녀 와서 친구가 자개 병뚜껑 건 손톱깍기 있는데 지금도 잘 써요",
          "speaker_id": "2",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100042870.1.10",
          "form": "아 진짜 유용하네요",
          "original_form": "아 진짜 유용하네요",
          "speaker_id": "1",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100042870.1.11",
          "form": "전 다음엔 도장을 파올까 생각중이에요",
          "original_form": "전 다음엔 도장을 파올까 생각중이에요",
          "speaker_id": "1",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100042870.1.12",
          "form": "요즘 서명시대자나요",
          "original_form": "요즘 서명시대자나요",
          "speaker_id": "2",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100042870.1.13",
          "form": "ㅋㅋ 아날로그느낌",
          "original_form": "ㅋㅋ 아날로그느낌",
          "speaker_id": "1",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100042870.1.14",
          "form": "여행가셔도 지도보실듯",
          "original_form": "여행가셔도 지도보실듯",
          "speaker_id": "2",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100042870.1.15",
          "form": "ㅋㅋ 종이지도요? ㅋㅋ",
          "original_form": "ㅋㅋ 종이지도요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:20"
        },
        {
          "id": "MDRW2100042870.1.16",
          "form": "네 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:21"
        },
        {
          "id": "MDRW2100042870.1.17",
          "form": "길눈 밝으신가요? 저 완전 길치",
          "original_form": "길눈 밝으신가요? 저 완전 길치",
          "speaker_id": "1",
          "time": "20210907 15:21"
        },
        {
          "id": "MDRW2100042870.1.18",
          "form": "저는 오름 계획세워 가려고 오름 트레킹맵 샀어요",
          "original_form": "저는 오름 계획세워 가려고 오름 트레킹맵 샀어요",
          "speaker_id": "2",
          "time": "20210907 15:21"
        },
        {
          "id": "MDRW2100042870.1.19",
          "form": "오오!!!",
          "original_form": "오오!!!",
          "speaker_id": "1",
          "time": "20210907 15:21"
        },
