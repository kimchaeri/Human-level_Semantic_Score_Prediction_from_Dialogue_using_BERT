      "utterance": [
        {
          "id": "MDRW2100002777.1.1",
          "form": "요즘 쇼핑은 어떻게 해결하세요??",
          "original_form": "요즘 쇼핑은 어떻게 해결하세요??",
          "speaker_id": "1",
          "time": "20210820 15:31"
        },
        {
          "id": "MDRW2100002777.1.2",
          "form": "거의 온라인이죠",
          "original_form": "거의 온라인이죠",
          "speaker_id": "2",
          "time": "20210820 15:31"
        },
        {
          "id": "MDRW2100002777.1.3",
          "form": "그쵸??",
          "original_form": "그쵸??",
          "speaker_id": "1",
          "time": "20210820 15:31"
        },
        {
          "id": "MDRW2100002777.1.4",
          "form": "저는 원래 대면형 쇼퍼였는데..ㅠ",
          "original_form": "저는 원래 대면형 쇼퍼였는데..ㅠ",
          "speaker_id": "1",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002777.1.5",
          "form": "코로나 이후 택배가 얼마나 많이오는지...",
          "original_form": "코로나 이후 택배가 얼마나 많이오는지...",
          "speaker_id": "1",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002777.1.6",
          "form": "저는 원래도 거의 온라인만해서",
          "original_form": "저는 원래도 거의 온라인만해서",
          "speaker_id": "2",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002777.1.7",
          "form": "택배 정말많이와요",
          "original_form": "택배 정말많이와요",
          "speaker_id": "2",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002777.1.8",
          "form": "앗 진짜요??",
          "original_form": "앗 진짜요??",
          "speaker_id": "1",
          "time": "20210820 15:32"
        },
        {
          "id": "MDRW2100002777.1.9",
          "form": "저는 여기 추가 택배비가 붙어서 잘 안샀거든요..",
          "original_form": "저는 여기 추가 택배비가 붙어서 잘 안샀거든요..",
          "speaker_id": "1",
          "time": "20210820 15:33"
        },
        {
          "id": "MDRW2100002777.1.10",
          "form": "근데..... 추가 택배비 붙어도 이제는 인터넷이 더 싼 느낌..ㅠ",
          "original_form": "근데..... 추가 택배비 붙어도 이제는 인터넷이 더 싼 느낌..ㅠ",
          "speaker_id": "1",
          "time": "20210820 15:33"
        },
        {
          "id": "MDRW2100002777.1.11",
          "form": "아 ㅋㅋ 인터넷이 워낙 싸니까요",
          "original_form": "아 ㅋㅋ 인터넷이 워낙 싸니까요",
          "speaker_id": "2",
          "time": "20210820 15:33"
        },
        {
          "id": "MDRW2100002777.1.12",
          "form": "그쵸...",
          "original_form": "그쵸...",
          "speaker_id": "1",
          "time": "20210820 15:33"
        },
        {
          "id": "MDRW2100002777.1.13",
          "form": "요즘 주로 사는건 아이 물품인데...",
          "original_form": "요즘 주로 사는건 아이 물품인데...",
          "speaker_id": "1",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002777.1.14",
          "form": "핫딜 카페 들어가면 장보도 수두룩하더라고요.",
          "original_form": "핫딜 카페 들어가면 장보도 수두룩하더라고요.",
          "speaker_id": "1",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002777.1.15",
          "form": "인터넷으로 뭐 주로 사세요?",
          "original_form": "인터넷으로 뭐 주로 사세요?",
          "speaker_id": "1",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002777.1.16",
          "form": "간편식이런거도 많이 사고요",
          "original_form": "간편식이런거도 많이 사고요",
          "speaker_id": "2",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002777.1.17",
          "form": "생필품은 거의 인터넷이요",
          "original_form": "생필품은 거의 인터넷이요",
          "speaker_id": "2",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002777.1.18",
          "form": "그쵸그쵸??",
          "original_form": "그쵸그쵸??",
          "speaker_id": "1",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002777.1.19",
          "form": "쿠폰도 많고 적립금이런거 잘찾으면 훨씬 저렴해요",
          "original_form": "쿠폰도 많고 적립금이런거 잘찾으면 훨씬 저렴해요",
          "speaker_id": "2",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002777.1.20",
          "form": "그렇더라고요!!",
          "original_form": "그렇더라고요!!",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002777.1.21",
          "form": "휴지도 예전에는 암꺼도 몰라서 그냥 마트서 30롤에 17000원 이렇게 주고 샀는데..",
          "original_form": "휴지도 예전에는 암꺼도 몰라서 그냥 마트서 30롤에 17000원 이렇게 주고 샀는데..",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002777.1.22",
          "form": "핫 넘비싸다",
          "original_form": "핫 넘비싸다",
          "speaker_id": "2",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002777.1.23",
          "form": "인터넷은 두배를 그가격에 팔고 있더라고요...?",
          "original_form": "인터넷은 두배를 그가격에 팔고 있더라고요...?",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002777.1.24",
          "form": "그쵸그죠",
          "original_form": "그쵸그죠",
          "speaker_id": "2",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002777.1.25",
          "form": "하... 진짜 마트 호구로 살어온 세월.....",
          "original_form": "하... 진짜 마트 호구로 살어온 세월.....",
          "speaker_id": "1",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002777.1.26",
          "form": "이제는 나가서 사는건 신선식품밖에 없는거 같아요.",
          "original_form": "이제는 나가서 사는건 신선식품밖에 없는거 같아요.",
          "speaker_id": "1",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002777.1.27",
          "form": "야채나 고기는 눈으로 고르고 싶어서...ㅎㅎ",
          "original_form": "야채나 고기는 눈으로 고르고 싶어서...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002777.1.28",
          "form": "그런건 좀 걱정되긴하져",
          "original_form": "그런건 좀 걱정되긴하져",
          "speaker_id": "2",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002777.1.29",
          "form": "그래서 간편식 냉동식품 밀키트로 연명합니다",
          "original_form": "그래서 간편식 냉동식품 밀키트로 연명합니다",
          "speaker_id": "2",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002777.1.30",
          "form": "밀키트 어디꺼 드세요??",
          "original_form": "밀키트 어디꺼 드세요??",
          "speaker_id": "1",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002777.1.31",
          "form": "딱정하진 않고 그냥 여기저기서 사봐요",
          "original_form": "딱정하진 않고 그냥 여기저기서 사봐요",
          "speaker_id": "2",
          "time": "20210820 15:46"
        },
        {
          "id": "MDRW2100002777.1.32",
          "form": "ㅎㅎ 추천해주실만한 밀키트 있으세요??",
          "original_form": "ㅎㅎ 추천해주실만한 밀키트 있으세요??",
          "speaker_id": "1",
          "time": "20210820 16:26"
        },
        {
          "id": "MDRW2100002777.1.33",
          "form": "지마켓에서 형제소곱창전골 사봤는데 맛있더라고요",
          "original_form": "지마켓에서 형제소곱창전골 사봤는데 맛있더라고요",
          "speaker_id": "2",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002777.1.34",
          "form": "우동넣어도 맛나고요",
          "original_form": "우동넣어도 맛나고요",
          "speaker_id": "2",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002777.1.35",
          "form": "메모해 뒀다가 사봐야겠어요",
          "original_form": "메모해 뒀다가 사봐야겠어요",
          "speaker_id": "1",
          "time": "20210820 17:31"
        },
        {
          "id": "MDRW2100002777.1.36",
          "form": "저는 구스통자주 시켰었는데 여기도 괜찮았어요",
          "original_form": "저는 구스통자주 시켰었는데 여기도 괜찮았어요",
          "speaker_id": "1",
          "time": "20210820 17:32"
        },
        {
          "id": "MDRW2100002777.1.37",
          "form": "처음들어보는데 저도 봐야겠네요",
          "original_form": "처음들어보는데 저도 봐야겠네요",
          "speaker_id": "2",
          "time": "20210820 17:32"
        },
        {
          "id": "MDRW2100002777.1.38",
          "form": "여긴 공구같이 여는 곳인데",
          "original_form": "여긴 공구같이 여는 곳인데",
          "speaker_id": "1",
          "time": "20210820 17:33"
        },
        {
          "id": "MDRW2100002777.1.39",
          "form": "여러업체 입점해 있어요.",
          "original_form": "여러업체 입점해 있어요.",
          "speaker_id": "1",
          "time": "20210820 17:33"
        },
        {
          "id": "MDRW2100002777.1.40",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210820 17:46"
        },
        {
          "id": "MDRW2100002777.1.41",
          "form": "떡갈비랑. 닭갈비 시켰었는데 괜찮더라고요",
          "original_form": "떡갈비랑. 닭갈비 시켰었는데 괜찮더라고요",
          "speaker_id": "1",
          "time": "20210820 17:59"
        },
        {
          "id": "MDRW2100002777.1.42",
          "form": "둘다 좋아하는 건데 ㅋㅋ",
          "original_form": "둘다 좋아하는 건데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:02"
        },
        {
          "id": "MDRW2100002777.1.43",
          "form": "추천합니다. 한번 시켜보세요!",
          "original_form": "추천합니다. 한번 시켜보세요!",
          "speaker_id": "1",
