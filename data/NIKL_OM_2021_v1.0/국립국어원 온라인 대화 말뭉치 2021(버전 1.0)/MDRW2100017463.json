      "utterance": [
        {
          "id": "MDRW2100017463.1.1",
          "form": "내일도 일한다",
          "original_form": "내일도 일한다",
          "speaker_id": "2",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100017463.1.2",
          "form": "일하기 싫다",
          "original_form": "일하기 싫다",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017463.1.3",
          "form": "ㅋㅋ 미투",
          "original_form": "ㅋㅋ 미투",
          "speaker_id": "1",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017463.1.4",
          "form": "걍 기계처럼 영혼없이 함",
          "original_form": "걍 기계처럼 영혼없이 함",
          "speaker_id": "1",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017463.1.5",
          "form": "난 프리랜서라 일 있을때만 나가는데",
          "original_form": "난 프리랜서라 일 있을때만 나가는데",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017463.1.6",
          "form": "매일 일이 있네",
          "original_form": "매일 일이 있네",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017463.1.7",
          "form": "아ㅋㅋ난 근무환경 좋은편이라 스트레스는 없는데",
          "original_form": "아ㅋㅋ난 근무환경 좋은편이라 스트레스는 없는데",
          "speaker_id": "1",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100017463.1.8",
          "form": "내가 파워집수니라서",
          "original_form": "내가 파워집수니라서",
          "speaker_id": "1",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100017463.1.9",
          "form": "나도 파워집수니",
          "original_form": "나도 파워집수니",
          "speaker_id": "2",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100017463.1.10",
          "form": "집밖에나가는순간 피곤햐",
          "original_form": "집밖에나가는순간 피곤햐",
          "speaker_id": "1",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100017463.1.11",
          "form": "침대가 그리워",
          "original_form": "침대가 그리워",
          "speaker_id": "1",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100017463.1.12",
          "form": "코로나 전에도 사회적거리두기 함",
          "original_form": "코로나 전에도 사회적거리두기 함",
          "speaker_id": "2",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100017463.1.13",
          "form": "일하는거 넘 시름",
          "original_form": "일하는거 넘 시름",
          "speaker_id": "2",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100017463.1.14",
          "form": "ㅋㅋ거리두기 딱 내 재질",
          "original_form": "ㅋㅋ거리두기 딱 내 재질",
          "speaker_id": "1",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100017463.1.15",
          "form": "프리랜서 멋잇응",
          "original_form": "프리랜서 멋잇응",
          "speaker_id": "1",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100017463.1.16",
          "form": "프리랜서인데 4대보험 이런거없다",
          "original_form": "프리랜서인데 4대보험 이런거없다",
          "speaker_id": "2",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100017463.1.17",
          "form": "출퇴근없는삶 부러워",
          "original_form": "출퇴근없는삶 부러워",
          "speaker_id": "1",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100017463.1.18",
          "form": "토요일도 일함",
          "original_form": "토요일도 일함",
          "speaker_id": "2",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100017463.1.19",
          "form": "오홍",
          "original_form": "오홍",
          "speaker_id": "1",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100017463.1.20",
          "form": "글쿠나",
          "original_form": "글쿠나",
          "speaker_id": "1",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100017463.1.21",
          "form": "그리고 10시에나가서 9시에 퇴근할때도 있읍니다",
          "original_form": "그리고 10시에나가서 9시에 퇴근할때도 있읍니다",
          "speaker_id": "2",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100017463.1.22",
          "form": "적게일하고 마니벌어야하는데",
          "original_form": "적게일하고 마니벌어야하는데",
          "speaker_id": "1",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017463.1.23",
          "form": "우리네인생",
          "original_form": "우리네인생",
          "speaker_id": "1",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017463.1.24",
