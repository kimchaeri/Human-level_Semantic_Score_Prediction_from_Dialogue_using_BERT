      "utterance": [
        {
          "id": "MDRW2100040326.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210904 16:18"
        },
        {
          "id": "MDRW2100040326.1.2",
          "form": "자주 보시는 유튜브 있으신가요",
          "original_form": "자주 보시는 유튜브 있으신가요",
          "speaker_id": "1",
          "time": "20210904 16:18"
        },
        {
          "id": "MDRW2100040326.1.3",
          "form": "네 에버랜드 호랑이남매랑  올리버쌤 자주봐요",
          "original_form": "네 에버랜드 호랑이남매랑  올리버쌤 자주봐요",
          "speaker_id": "2",
          "time": "20210904 16:19"
        },
        {
          "id": "MDRW2100040326.1.4",
          "form": "호랑이남매요?ㅋㅋㅋ",
          "original_form": "호랑이남매요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:19"
        },
        {
          "id": "MDRW2100040326.1.5",
          "form": "뭔가 새롭네요",
          "original_form": "뭔가 새롭네요",
          "speaker_id": "1",
          "time": "20210904 16:19"
        },
        {
          "id": "MDRW2100040326.1.6",
          "form": "귀여울거 같은데",
          "original_form": "귀여울거 같은데",
          "speaker_id": "1",
          "time": "20210904 16:19"
        },
        {
          "id": "MDRW2100040326.1.7",
          "form": "네 은근히 사육사님한테 애교도 부리고 귀엽더라구요",
          "original_form": "네 은근히 사육사님한테 애교도 부리고 귀엽더라구요",
          "speaker_id": "2",
          "time": "20210904 16:20"
        },
        {
          "id": "MDRW2100040326.1.8",
          "form": "아직 태어난 지 얼마 안됐나 봐요",
          "original_form": "아직 태어난 지 얼마 안됐나 봐요",
          "speaker_id": "1",
          "time": "20210904 16:21"
        },
        {
          "id": "MDRW2100040326.1.9",
          "form": "고양이 유튜브 자주 보는데 한번 봐야겠네요",
          "original_form": "고양이 유튜브 자주 보는데 한번 봐야겠네요",
          "speaker_id": "1",
          "time": "20210904 16:22"
        },
        {
          "id": "MDRW2100040326.1.10",
          "form": "아... 호랑이 16개월 정도 됐을거예요 아직 성체는 아닌데 커요 ㅋㅋ",
          "original_form": "아... 호랑이 16개월 정도 됐을거예요 아직 성체는 아닌데 커요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:23"
        },
        {
          "id": "MDRW2100040326.1.11",
          "form": "전 커서 귀엽더라구요 왕크왕귀!!",
          "original_form": "전 커서 귀엽더라구요 왕크왕귀!!",
          "speaker_id": "2",
          "time": "20210904 16:23"
        },
        {
          "id": "MDRW2100040326.1.12",
          "form": "왕 크 왕 귀 완전 설레는 단어 네요",
          "original_form": "왕 크 왕 귀 완전 설레는 단어 네요",
          "speaker_id": "1",
          "time": "20210904 16:23"
        },
        {
          "id": "MDRW2100040326.1.13",
          "form": "올리버쌤은 어떤 내용이에요",
          "original_form": "올리버쌤은 어떤 내용이에요",
          "speaker_id": "1",
          "time": "20210904 16:24"
        },
        {
          "id": "MDRW2100040326.1.14",
          "form": "한국사시던 미국인영어쌤인데 지금은 한국인 아내분이랑 아기낳고 진돗개 두마리 키우시는데 일상이 엄청 재밌어요",
          "original_form": "한국사시던 미국인영어쌤인데 지금은 한국인 아내분이랑 아기낳고 진돗개 두마리 키우시는데 일상이 엄청 재밌어요",
          "speaker_id": "2",
          "time": "20210904 16:25"
        },
        {
          "id": "MDRW2100040326.1.15",
          "form": "지금은 미국에 살고계세요",
          "original_form": "지금은 미국에 살고계세요",
          "speaker_id": "2",
          "time": "20210904 16:26"
        },
        {
          "id": "MDRW2100040326.1.16",
          "form": "아! 본거같아요!",
          "original_form": "아! 본거같아요!",
          "speaker_id": "1",
          "time": "20210904 16:27"
        },
        {
          "id": "MDRW2100040326.1.17",
          "form": "걔 이름이 무엇이었죠?",
          "original_form": "걔 이름이 무엇이었죠?",
          "speaker_id": "1",
          "time": "20210904 16:27"
        },
        {
          "id": "MDRW2100040326.1.18",
          "form": "공주랑 왕자요!",
          "original_form": "공주랑 왕자요!",
          "speaker_id": "2",
          "time": "20210904 16:27"
        },
        {
          "id": "MDRW2100040326.1.19",
          "form": "애들 엄청 사랑스러웠는데ㅋㅋㅋ",
          "original_form": "애들 엄청 사랑스러웠는데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:30"
        },
        {
          "id": "MDRW2100040326.1.20",
          "form": "네네 그집 강아지들 똑똑하고 이뻐요!",
          "original_form": "네네 그집 강아지들 똑똑하고 이뻐요!",
          "speaker_id": "2",
          "time": "20210904 16:31"
        },
        {
          "id": "MDRW2100040326.1.21",
          "form": "어떻게 교육하면 그렇게 똑똑한지ㅋㅋ",
          "original_form": "어떻게 교육하면 그렇게 똑똑한지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:32"
        }
      ]
