      "utterance": [
        {
          "id": "MDRW2100016400.1.1",
          "form": "거기 스키장이잖아요",
          "original_form": "거기 스키장이잖아요",
          "speaker_id": "1",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100016400.1.2",
          "form": "겁나 위에있는데 올라가더니 조금 내려와서는",
          "original_form": "겁나 위에있는데 올라가더니 조금 내려와서는",
          "speaker_id": "1",
          "time": "20210826 12:32"
        },
        {
          "id": "MDRW2100016400.1.3",
          "form": "도저히 못하겠다고 지나가는 강사를 붙잡더니 저기요",
          "original_form": "도저히 못하겠다고 지나가는 강사를 붙잡더니 저기요",
          "speaker_id": "1",
          "time": "20210826 12:32"
        },
        {
          "id": "MDRW2100016400.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:32"
        },
        {
          "id": "MDRW2100016400.1.5",
          "form": "저기요 (살려주세요)",
          "original_form": "저기요 (살려주세요)",
          "speaker_id": "2",
          "time": "20210826 12:33"
        },
        {
          "id": "MDRW2100016400.1.6",
          "form": "미안한데",
          "original_form": "미안한데",
          "speaker_id": "1",
          "time": "20210826 12:33"
        },
        {
          "id": "MDRW2100016400.1.7",
          "form": "역시 스키장은 용기다.",
          "original_form": "역시 스키장은 용기다.",
          "speaker_id": "2",
          "time": "20210826 12:33"
        },
        {
          "id": "MDRW2100016400.1.8",
          "form": "제 장비좀 가지고 내려가주시겠어요?",
          "original_form": "제 장비좀 가지고 내려가주시겠어요?",
          "speaker_id": "1",
          "time": "20210826 12:33"
        },
        {
          "id": "MDRW2100016400.1.9",
          "form": "그거 듣던 강사가 네? 겁나 얼타서 장비를 왜요?",
          "original_form": "그거 듣던 강사가 네? 겁나 얼타서 장비를 왜요?",
          "speaker_id": "1",
          "time": "20210826 12:33"
        },
        {
          "id": "MDRW2100016400.1.10",
          "form": "누나가 그 때 살면서 제일 진지하게 걸어내려가려구요",
          "original_form": "누나가 그 때 살면서 제일 진지하게 걸어내려가려구요",
          "speaker_id": "1",
          "time": "20210826 12:34"
        },
        {
          "id": "MDRW2100016400.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅇㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅇㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:34"
        },
        {
          "id": "MDRW2100016400.1.12",
          "form": "여기까지..올라와서요..?",
          "original_form": "여기까지..올라와서요..?",
          "speaker_id": "2",
          "time": "20210826 12:34"
        },
        {
          "id": "MDRW2100016400.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:34"
        },
        {
          "id": "MDRW2100016400.1.14",
          "form": "ㄹㅇ 귀여우시다",
          "original_form": "ㄹㅇ 귀여우시다",
          "speaker_id": "2",
          "time": "20210826 12:34"
        },
        {
          "id": "MDRW2100016400.1.15",
          "form": "그래서 강사가",
          "original_form": "그래서 강사가",
          "speaker_id": "1",
          "time": "20210826 12:34"
        },
        {
          "id": "MDRW2100016400.1.16",
          "form": "겁나얼타서 아..ㅇㅋ 하고 갖고내려가는데",
          "original_form": "겁나얼타서 아..ㅇㅋ 하고 갖고내려가는데",
          "speaker_id": "1",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100016400.1.17",
          "form": "누나가 이제 그 때 부터 걷는거야",
          "original_form": "누나가 이제 그 때 부터 걷는거야",
          "speaker_id": "1",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100016400.1.18",
          "form": "터벅 터벅 터벅",
          "original_form": "터벅 터벅 터벅",
          "speaker_id": "1",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100016400.1.19",
          "form": "그 추운데 저 위에서 아래까지",
          "original_form": "그 추운데 저 위에서 아래까지",
          "speaker_id": "1",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100016400.1.20",
          "form": "터벅 터벅 터벅",
          "original_form": "터벅 터벅 터벅",
          "speaker_id": "1",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100016400.1.21",
          "form": "근데 내려가다 누굴 만난지 알아요?",
          "original_form": "근데 내려가다 누굴 만난지 알아요?",
          "speaker_id": "1",
          "time": "20210826 12:36"
        },
        {
          "id": "MDRW2100016400.1.22",
          "form": "ㄷㄷ 누굴만났죠",
          "original_form": "ㄷㄷ 누굴만났죠",
          "speaker_id": "2",
          "time": "20210826 12:37"
        },
        {
          "id": "MDRW2100016400.1.23",
          "form": "끊기 신공",
          "original_form": "끊기 신공",
          "speaker_id": "2",
          "time": "20210826 12:37"
        },
        {
          "id": "MDRW2100016400.1.24",
          "form": "자기랑 같이 배우던 사람을 만난거에욬ㅋㅋㄲ\n 그사람도 타다가 승질나서 못하겠다 그랬는지",
          "original_form": "자기랑 같이 배우던 사람을 만난거에욬ㅋㅋㄲ\n 그사람도 타다가 승질나서 못하겠다 그랬는지",
          "speaker_id": "1",
          "time": "20210826 12:38"
        },
        {
          "id": "MDRW2100016400.1.25",
          "form": "터벅터벅 걷고있더래",
          "original_form": "터벅터벅 걷고있더래",
          "speaker_id": "1",
          "time": "20210826 12:38"
        },
        {
          "id": "MDRW2100016400.1.26",
          "form": "그래서 그 둘이서 그 스키장최초로 스키 안타고 걸어내려온 사람이 됐대요",
          "original_form": "그래서 그 둘이서 그 스키장최초로 스키 안타고 걸어내려온 사람이 됐대요",
          "speaker_id": "1",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016400.1.27",
          "form": "ㅇㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016400.1.28",
          "form": "천생연분 만나신듯",
          "original_form": "천생연분 만나신듯",
          "speaker_id": "2",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016400.1.29",
          "form": "거의 2시간반?을 걸어내려온거같은데",
          "original_form": "거의 2시간반?을 걸어내려온거같은데",
          "speaker_id": "1",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016400.1.30",
          "form": "ㄹㅇ 그게 더 힘들지 않나요",
          "original_form": "ㄹㅇ 그게 더 힘들지 않나요",
          "speaker_id": "2",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100016400.1.31",
          "form": "걸어내려오기",
          "original_form": "걸어내려오기",
          "speaker_id": "2",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016400.1.32",
          "form": "제가 슝~한번 지나갈 때 마다",
          "original_form": "제가 슝~한번 지나갈 때 마다",
          "speaker_id": "1",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016400.1.33",
          "form": "조금 씩 이동하더라구요",
          "original_form": "조금 씩 이동하더라구요",
          "speaker_id": "1",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016400.1.34",
          "form": "그 다음 날 앓아누웠어요",
          "original_form": "그 다음 날 앓아누웠어요",
          "speaker_id": "1",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016400.1.35",
          "form": "누나는 그럼 스키장 별로 안좋아하시나요",
          "original_form": "누나는 그럼 스키장 별로 안좋아하시나요",
          "speaker_id": "2",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016400.1.36",
          "form": "2시간 걸었으니 그럴만두",
          "original_form": "2시간 걸었으니 그럴만두",
          "speaker_id": "2",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100016400.1.37",
          "form": "그 다음 부터 가재도 안가더라구요",
          "original_form": "그 다음 부터 가재도 안가더라구요",
