      "utterance": [
        {
          "id": "MDRW2100010119.1.1",
          "form": "헐 감기 보셨나요",
          "original_form": "헐 감기 보셨나요",
          "speaker_id": "2",
          "time": "20210823 14:32"
        },
        {
          "id": "MDRW2100010119.1.2",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210823 14:32"
        },
        {
          "id": "MDRW2100010119.1.3",
          "form": "첨은 기억이 안나요",
          "original_form": "첨은 기억이 안나요",
          "speaker_id": "2",
          "time": "20210823 14:32"
        },
        {
          "id": "MDRW2100010119.1.4",
          "form": "굿.",
          "original_form": "굿.",
          "speaker_id": "2",
          "time": "20210823 14:32"
        },
        {
          "id": "MDRW2100010119.1.5",
          "form": "그럼요 저 그 두번봤어요",
          "original_form": "그럼요 저 그 두번봤어요",
          "speaker_id": "1",
          "time": "20210823 14:32"
        },
        {
          "id": "MDRW2100010119.1.6",
          "form": "그 무슨",
          "original_form": "그 무슨",
          "speaker_id": "2",
          "time": "20210823 14:32"
        },
        {
          "id": "MDRW2100010119.1.7",
          "form": "실험하다 바이러스 퍼진거였나요?",
          "original_form": "실험하다 바이러스 퍼진거였나요?",
          "speaker_id": "2",
          "time": "20210823 14:32"
        },
        {
          "id": "MDRW2100010119.1.8",
          "form": "근데 그거 시작이 아마 조류독감인가 하튼 뭔 독감 옮은사람이...... 컨테이너에 몰래 타서",
          "original_form": "근데 그거 시작이 아마 조류독감인가 하튼 뭔 독감 옮은사람이...... 컨테이너에 몰래 타서",
          "speaker_id": "1",
          "time": "20210823 14:33"
        },
        {
          "id": "MDRW2100010119.1.9",
          "form": "불법...........밀항하다가 ....어저고 아니에요?",
          "original_form": "불법...........밀항하다가 ....어저고 아니에요?",
          "speaker_id": "1",
          "time": "20210823 14:33"
        },
        {
          "id": "MDRW2100010119.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그 컨테이너 안 보면 사람들 죽어있고 막..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그 컨테이너 안 보면 사람들 죽어있고 막..",
          "speaker_id": "1",
          "time": "20210823 14:33"
        },
        {
          "id": "MDRW2100010119.1.11",
          "form": "어맞다맞가!!!!!!",
          "original_form": "어맞다맞가!!!!!!",
          "speaker_id": "2",
          "time": "20210823 14:33"
        },
        {
          "id": "MDRW2100010119.1.12",
          "form": "두번본 사람은 다르다",
          "original_form": "두번본 사람은 다르다",
          "speaker_id": "2",
          "time": "20210823 14:33"
        },
        {
          "id": "MDRW2100010119.1.13",
          "form": "실험체는 괴물이었다요",
          "original_form": "실험체는 괴물이었다요",
          "speaker_id": "2",
          "time": "20210823 14:34"
        },
        {
          "id": "MDRW2100010119.1.14",
          "form": "맞다요",
          "original_form": "맞다요",
          "speaker_id": "2",
          "time": "20210823 14:34"
        },
        {
          "id": "MDRW2100010119.1.15",
          "form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 14:34"
        },
        {
          "id": "MDRW2100010119.1.16",
          "form": "그래서 그 컨테이너에서 살은 사람이 탈출하면서 바이러스 다 퍼지고 그러잖아요",
          "original_form": "그래서 그 컨테이너에서 살은 사람이 탈출하면서 바이러스 다 퍼지고 그러잖아요",
          "speaker_id": "1",
          "time": "20210823 14:35"
        },
        {
          "id": "MDRW2100010119.1.17",
          "form": "그 거기 배경이 분당이라는게 기억나요",
          "original_form": "그 거기 배경이 분당이라는게 기억나요",
          "speaker_id": "2",
          "time": "20210823 14:35"
        },
        {
          "id": "MDRW2100010119.1.18",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210823 14:35"
        },
        {
          "id": "MDRW2100010119.1.19",
          "form": "아 맞아욬ㅋㅋㅋㅋ 분당폐쇄하고",
          "original_form": "아 맞아욬ㅋㅋㅋㅋ 분당폐쇄하고",
          "speaker_id": "1",
          "time": "20210823 14:35"
        },
        {
          "id": "MDRW2100010119.1.20",
          "form": "그놈이 주인공 딸래미한테 발견되잖아요",
          "original_form": "그놈이 주인공 딸래미한테 발견되잖아요",
          "speaker_id": "2",
          "time": "20210823 14:35"
        },
        {
          "id": "MDRW2100010119.1.21",
          "form": "근데 항체가 그놈한테 있었나",
          "original_form": "근데 항체가 그놈한테 있었나",
          "speaker_id": "2",
          "time": "20210823 14:35"
        },
        {
          "id": "MDRW2100010119.1.22",
          "form": "근데 그 바이러스 퍼지는 장면이 전문가 입장에서는 너무 적나라하게 잘 보여주는 작품이었다고",
          "original_form": "근데 그 바이러스 퍼지는 장면이 전문가 입장에서는 너무 적나라하게 잘 보여주는 작품이었다고",
          "speaker_id": "1",
          "time": "20210823 14:35"
        },
        {
          "id": "MDRW2100010119.1.23",
          "form": "네네 맞아요!",
          "original_form": "네네 맞아요!",
          "speaker_id": "1",
          "time": "20210823 14:35"
        },
        {
          "id": "MDRW2100010119.1.24",
          "form": "그 애기 지금 성인일것같은데",
          "original_form": "그 애기 지금 성인일것같은데",
          "speaker_id": "2",
          "time": "20210823 14:36"
        },
        {
          "id": "MDRW2100010119.1.25",
          "form": "암튼",
          "original_form": "암튼",
          "speaker_id": "2",
          "time": "20210823 14:36"
        },
        {
          "id": "MDRW2100010119.1.26",
          "form": "그래서 그 원래는 검사해서 항체 어쩌고 해야하는데 애기 엄마가 그냥 아이한테 바로 주지않나요?",
          "original_form": "그래서 그 원래는 검사해서 항체 어쩌고 해야하는데 애기 엄마가 그냥 아이한테 바로 주지않나요?",
          "speaker_id": "1",
          "time": "20210823 14:36"
        },
        {
          "id": "MDRW2100010119.1.27",
          "form": "맞아요 그래서 더 무서웠어요 너무 적나라해서",
          "original_form": "맞아요 그래서 더 무서웠어요 너무 적나라해서",
          "speaker_id": "2",
          "time": "20210823 14:36"
        },
        {
          "id": "MDRW2100010119.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그렇게 항체가 생겨 지구를 구하게 된...아기.",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그렇게 항체가 생겨 지구를 구하게 된...아기.",
          "speaker_id": "1",
          "time": "20210823 14:36"
        },
        {
          "id": "MDRW2100010119.1.29",
          "form": "아기가 미래다",
          "original_form": "아기가 미래다",
          "speaker_id": "2",
          "time": "20210823 14:36"
        },
        {
          "id": "MDRW2100010119.1.30",
          "form": "홀 방그석 1열 보시는군요",
          "original_form": "홀 방그석 1열 보시는군요",
          "speaker_id": "2",
          "time": "20210823 14:37"
        }
      ]
    }
