      "utterance": [
        {
          "id": "MDRW2100017649.1.1",
          "form": "커피좋아하시나용?",
          "original_form": "커피좋아하시나용?",
          "speaker_id": "1",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017649.1.2",
          "form": "저는 비엔나에서마신커피생각이나네요",
          "original_form": "저는 비엔나에서마신커피생각이나네요",
          "speaker_id": "1",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017649.1.3",
          "form": "저는 커피 마시면 잠을 못자서요ㅠ",
          "original_form": "저는 커피 마시면 잠을 못자서요ㅠ",
          "speaker_id": "2",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100017649.1.4",
          "form": "저두요 근데 맛있어서한번씩마셔요",
          "original_form": "저두요 근데 맛있어서한번씩마셔요",
          "speaker_id": "1",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100017649.1.5",
          "form": "커피땡길때가있어요",
          "original_form": "커피땡길때가있어요",
          "speaker_id": "1",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100017649.1.6",
          "form": "여행가서는 한번씩 마셔요 저도ㅎ",
          "original_form": "여행가서는 한번씩 마셔요 저도ㅎ",
          "speaker_id": "2",
          "time": "20210829 18:47"
        },
        {
          "id": "MDRW2100017649.1.7",
          "form": "여행어디가제일좋으셨나요?",
          "original_form": "여행어디가제일좋으셨나요?",
          "speaker_id": "1",
          "time": "20210829 18:47"
        },
        {
          "id": "MDRW2100017649.1.8",
          "form": "전 최근에 갔던 부산이요!",
          "original_form": "전 최근에 갔던 부산이요!",
          "speaker_id": "2",
          "time": "20210829 18:47"
        },
        {
          "id": "MDRW2100017649.1.9",
          "form": "저도부산자주가는데",
          "original_form": "저도부산자주가는데",
          "speaker_id": "1",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100017649.1.10",
          "form": "부산 해운대 가셨나요?",
          "original_form": "부산 해운대 가셨나요?",
          "speaker_id": "2",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100017649.1.11",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100017649.1.12",
          "form": "여름휴가해운대로갔어요",
          "original_form": "여름휴가해운대로갔어요",
          "speaker_id": "1",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100017649.1.13",
          "form": "해운대 좋죠~",
          "original_form": "해운대 좋죠~",
          "speaker_id": "2",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100017649.1.14",
          "form": "해수욕장에 사람별로없더라구요",
          "original_form": "해수욕장에 사람별로없더라구요",
          "speaker_id": "1",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100017649.1.15",
          "form": "코로나때문에ㅠ그래도 사람붐비는거보단",
          "original_form": "코로나때문에ㅠ그래도 사람붐비는거보단",
          "speaker_id": "2",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100017649.1.16",
          "form": "네 저도거의호텔에있었어요",
          "original_form": "네 저도거의호텔에있었어요",
          "speaker_id": "1",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100017649.1.17",
          "form": "해운대 근처 숙소였나요?",
          "original_form": "해운대 근처 숙소였나요?",
          "speaker_id": "2",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100017649.1.18",
          "form": "네 바다보이는곳이여서",
          "original_form": "네 바다보이는곳이여서",
          "speaker_id": "1",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100017649.1.19",
          "form": "좋았어요",
          "original_form": "좋았어요",
          "speaker_id": "1",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100017649.1.20",
          "form": "오션뷰 끝내줬겠어요ㅠㅠ",
          "original_form": "오션뷰 끝내줬겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100017649.1.21",
          "form": "해운대에 맛집도 많죠?",
          "original_form": "해운대에 맛집도 많죠?",
          "speaker_id": "2",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100017649.1.22",
          "form": "맞아요 밤에도예쁘고",
          "original_form": "맞아요 밤에도예쁘고",
          "speaker_id": "1",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100017649.1.23",
          "form": "네 배터지게먹었네요",
          "original_form": "네 배터지게먹었네요",
          "speaker_id": "1",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100017649.1.24",
          "form": "해운대에 더 베이도 유명하더라고요ㅎㅎ",
          "original_form": "해운대에 더 베이도 유명하더라고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100017649.1.25",
          "form": "해운대칼국수추천요",
          "original_form": "해운대칼국수추천요",
          "speaker_id": "1",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100017649.1.26",
          "form": "제주도가보셧나요",
          "original_form": "제주도가보셧나요",
          "speaker_id": "1",
          "time": "20210829 18:52"
        }
      ]
    }
  ]
