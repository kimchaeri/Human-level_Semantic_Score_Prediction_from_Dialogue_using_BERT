          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007372.1.3",
          "form": "오케",
          "original_form": "오케",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007372.1.4",
          "form": "두부면 짱맛 오늘 또먹음",
          "original_form": "두부면 짱맛 오늘 또먹음",
          "speaker_id": "1",
          "time": "20210822 16:59"
        },
        {
          "id": "MDRW2100007372.1.5",
          "form": "ㅋㅌㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅌㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:08"
        },
        {
          "id": "MDRW2100007372.1.6",
          "form": "그거 어케하는거야",
          "original_form": "그거 어케하는거야",
          "speaker_id": "2",
          "time": "20210822 17:08"
        },
        {
          "id": "MDRW2100007372.1.7",
          "form": "두부면?",
          "original_form": "두부면?",
          "speaker_id": "2",
          "time": "20210822 17:08"
        },
        {
          "id": "MDRW2100007372.1.8",
          "form": "ㅋㅋㅋ풀무원 두부면 쳐봐",
          "original_form": "ㅋㅋㅋ풀무원 두부면 쳐봐",
          "speaker_id": "1",
          "time": "20210822 17:08"
        },
        {
          "id": "MDRW2100007372.1.9",
          "form": "걍 면대신 두부면 넣고 소스넣고 끝",
          "original_form": "걍 면대신 두부면 넣고 소스넣고 끝",
          "speaker_id": "1",
          "time": "20210822 17:09"
        },
        {
          "id": "MDRW2100007372.1.10",
          "form": "아 나도 그거 먹어봤어",
          "original_form": "아 나도 그거 먹어봤어",
          "speaker_id": "2",
          "time": "20210822 19:12"
        },
        {
          "id": "MDRW2100007372.1.11",
          "form": "근데 나는 그닥이더라",
          "original_form": "근데 나는 그닥이더라",
          "speaker_id": "2",
          "time": "20210822 19:12"
        },
        {
          "id": "MDRW2100007372.1.12",
          "form": "담연 당근면 요리 먹어보고 싶어",
          "original_form": "담연 당근면 요리 먹어보고 싶어",
          "speaker_id": "2",
          "time": "20210822 19:12"
        },
        {
          "id": "MDRW2100007372.1.13",
          "form": "당근면은 기계로 직접만들어야하는거 아닌가?",
          "original_form": "당근면은 기계로 직접만들어야하는거 아닌가?",
          "speaker_id": "1",
          "time": "20210822 19:48"
        },
        {
          "id": "MDRW2100007372.1.14",
          "form": "마져",
          "original_form": "마져",
          "speaker_id": "2",
          "time": "20210822 21:05"
        },
        {
          "id": "MDRW2100007372.1.15",
          "form": "기계 돌려서 길게 뽑더라",
          "original_form": "기계 돌려서 길게 뽑더라",
          "speaker_id": "2",
          "time": "20210822 21:05"
        },
        {
          "id": "MDRW2100007372.1.16",
          "form": "나는 내가 하는거 말고 남이 만들어주는거 ㅋㅋㅋㅋ 먹고싶어",
          "original_form": "나는 내가 하는거 말고 남이 만들어주는거 ㅋㅋㅋㅋ 먹고싶어",
          "speaker_id": "2",
          "time": "20210822 21:05"
        },
        {
          "id": "MDRW2100007372.1.17",
          "form": "ㅋㅋㅋㅋ나도 누가좀 해줬으면",
          "original_form": "ㅋㅋㅋㅋ나도 누가좀 해줬으면",
          "speaker_id": "1",
          "time": "20210822 21:58"
        },
        {
          "id": "MDRW2100007372.1.18",
          "form": "마켓컬리 당근면 안파나",
          "original_form": "마켓컬리 당근면 안파나",
          "speaker_id": "1",
          "time": "20210822 21:58"
        },
        {
          "id": "MDRW2100007372.1.19",
          "form": "빨리 만들라고",
          "original_form": "빨리 만들라고",
          "speaker_id": "1",
          "time": "20210822 21:58"
        },
        {
          "id": "MDRW2100007372.1.20",
          "form": "ㅋㅋㅋㅋㅋ 당근면이랑 오이면? 또 있았던거 같아",
          "original_form": "ㅋㅋㅋㅋㅋ 당근면이랑 오이면? 또 있았던거 같아",
          "speaker_id": "2",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100007372.1.21",
          "form": "여전히 배달음식 많이 못시켜먹어?",
          "original_form": "여전히 배달음식 많이 못시켜먹어?",
          "speaker_id": "2",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100007372.1.22",
          "form": "오이면으로 콩국수 해먹더라",
          "original_form": "오이면으로 콩국수 해먹더라",
          "speaker_id": "1",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100007372.1.23",
          "form": "저번에 치킨시켝먹긴했어ㅋㅋㅋㅋ",
          "original_form": "저번에 치킨시켝먹긴했어ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100007372.1.24",
          "form": "근데또 차라리 잘된겨",
          "original_form": "근데또 차라리 잘된겨",
          "speaker_id": "1",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100007372.1.25",
          "form": "배달 잘된다? 엄청 시켜먹지",
          "original_form": "배달 잘된다? 엄청 시켜먹지",
          "speaker_id": "1",
          "time": "20210822 22:24"
        },
        {
          "id": "MDRW2100007372.1.26",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:30"
        },
        {
          "id": "MDRW2100007372.1.27",
          "form": "그때 배달 문때문에 어렵다고 하지 않았는가",
          "original_form": "그때 배달 문때문에 어렵다고 하지 않았는가",
          "speaker_id": "2",
          "time": "20210822 22:30"
        },
        {
          "id": "MDRW2100007372.1.28",
          "form": "잘됬더니 좋네",
          "original_form": "잘됬더니 좋네",
          "speaker_id": "2",
          "time": "20210822 22:30"
        },
        {
          "id": "MDRW2100007372.1.29",
          "form": "우리나라 배달 짱짱 좋은거 같아",
          "original_form": "우리나라 배달 짱짱 좋은거 같아",
          "speaker_id": "2",
          "time": "20210822 22:31"
        },
        {
          "id": "MDRW2100007372.1.30",
          "form": "빠르고 정확하고 종류도 많고 너무 편해",
          "original_form": "빠르고 정확하고 종류도 많고 너무 편해",
          "speaker_id": "2",
          "time": "20210822 22:31"
        },
        {
          "id": "MDRW2100007372.1.31",
          "form": "그 배민에서 음식말고 시키는거 빠르더라",
          "original_form": "그 배민에서 음식말고 시키는거 빠르더라",
          "speaker_id": "1",
          "time": "20210822 22:32"
        },
        {
          "id": "MDRW2100007372.1.32",
          "form": "b배달인가",
          "original_form": "b배달인가",
          "speaker_id": "1",
          "time": "20210822 22:32"
        }
      ]
