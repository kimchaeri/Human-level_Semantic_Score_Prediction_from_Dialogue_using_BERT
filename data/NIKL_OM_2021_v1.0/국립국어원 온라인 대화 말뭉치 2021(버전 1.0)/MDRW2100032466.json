      "utterance": [
        {
          "id": "MDRW2100032466.1.1",
          "form": "재택근무 생각 있어?",
          "original_form": "재택근무 생각 있어?",
          "speaker_id": "1",
          "time": "20210905 18:40"
        },
        {
          "id": "MDRW2100032466.1.2",
          "form": "생각은 있지만 능률은 최악일듯ㅋㅋㅋ",
          "original_form": "생각은 있지만 능률은 최악일듯ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:57"
        },
        {
          "id": "MDRW2100032466.1.3",
          "form": "의외로 괜찮지 않았어?",
          "original_form": "의외로 괜찮지 않았어?",
          "speaker_id": "1",
          "time": "20210905 19:34"
        },
        {
          "id": "MDRW2100032466.1.4",
          "form": "지난번 출판사일?",
          "original_form": "지난번 출판사일?",
          "speaker_id": "1",
          "time": "20210905 19:34"
        },
        {
          "id": "MDRW2100032466.1.5",
          "form": "그거는 끽해야 하루에 너댓시간이면 끝나는거였지만",
          "original_form": "그거는 끽해야 하루에 너댓시간이면 끝나는거였지만",
          "speaker_id": "2",
          "time": "20210905 20:05"
        },
        {
          "id": "MDRW2100032466.1.6",
          "form": "알바가 아니라 본업으로 해야하는거면",
          "original_form": "알바가 아니라 본업으로 해야하는거면",
          "speaker_id": "2",
          "time": "20210905 20:06"
        },
        {
          "id": "MDRW2100032466.1.7",
          "form": "난 날 믿지 않아서 ㅋㅋㅋ",
          "original_form": "난 날 믿지 않아서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:06"
        },
        {
          "id": "MDRW2100032466.1.8",
          "form": "그때 했던 일이 본업이면 어떨거 같아?",
          "original_form": "그때 했던 일이 본업이면 어떨거 같아?",
          "speaker_id": "1",
          "time": "20210905 20:11"
        },
        {
          "id": "MDRW2100032466.1.9",
          "form": "글세..?",
          "original_form": "글세..?",
          "speaker_id": "2",
          "time": "20210905 20:58"
        },
        {
          "id": "MDRW2100032466.1.10",
          "form": "하루에 4~5작품은 해야 생활이 가능할거같은데",
          "original_form": "하루에 4~5작품은 해야 생활이 가능할거같은데",
          "speaker_id": "2",
          "time": "20210905 20:58"
        },
        {
          "id": "MDRW2100032466.1.11",
          "form": "매일 타이핑하느라 죽을듯ㅋㅋㅋ",
          "original_form": "매일 타이핑하느라 죽을듯ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:59"
        },
        {
          "id": "MDRW2100032466.1.12",
          "form": "작가로서가 아니니까",
          "original_form": "작가로서가 아니니까",
          "speaker_id": "1",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100032466.1.13",
          "form": "그래도 평소에 보는걸 못보겠지만",
          "original_form": "그래도 평소에 보는걸 못보겠지만",
          "speaker_id": "1",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100032466.1.14",
          "form": "좀 더 여유롭지 않을까?",
          "original_form": "좀 더 여유롭지 않을까?",
          "speaker_id": "1",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100032466.1.15",
          "form": "작품당 2만원 생각하고 한달 월급 채울려면",
          "original_form": "작품당 2만원 생각하고 한달 월급 채울려면",
          "speaker_id": "2",
          "time": "20210905 21:28"
        },
        {
          "id": "MDRW2100032466.1.16",
          "form": "달에 100편은 봐야 200 나오니깐ㅋㅋㅋ",
          "original_form": "달에 100편은 봐야 200 나오니깐ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:28"
        },
        {
          "id": "MDRW2100032466.1.17",
          "form": "...왜 못해?!",
          "original_form": "...왜 못해?!",
          "speaker_id": "1",
          "time": "20210905 21:28"
        },
        {
          "id": "MDRW2100032466.1.18",
          "form": "그래도 주말없이 하루에 3개는 봐야겟네",
          "original_form": "그래도 주말없이 하루에 3개는 봐야겟네",
          "speaker_id": "2",
          "time": "20210905 21:28"
        },
        {
          "id": "MDRW2100032466.1.19",
          "form": "아니 보는건 무리없는데",
          "original_form": "아니 보는건 무리없는데",
          "speaker_id": "2",
          "time": "20210905 21:28"
        },
        {
          "id": "MDRW2100032466.1.20",
          "form": "말햇잖아 타이핑하느라 죽을것 같다곸ㅋㅋㅋ",
          "original_form": "말햇잖아 타이핑하느라 죽을것 같다곸ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:29"
        },
        {
          "id": "MDRW2100032466.1.21",
          "form": "그거 감상글 쓰는게 여간 쉬운게 아니어야지ㅜ",
          "original_form": "그거 감상글 쓰는게 여간 쉬운게 아니어야지ㅜ",
          "speaker_id": "2",
          "time": "20210905 21:29"
        },
        {
          "id": "MDRW2100032466.1.22",
          "form": "에.. 숙련되면 타이핑 속도가 늘어나지 않을까",
          "original_form": "에.. 숙련되면 타이핑 속도가 늘어나지 않을까",
          "speaker_id": "1",
          "time": "20210905 21:32"
        },
        {
          "id": "MDRW2100032466.1.23",
          "form": "타이핑 속도는 충분한데",
          "original_form": "타이핑 속도는 충분한데",
          "speaker_id": "2",
          "time": "20210905 21:33"
        },
        {
          "id": "MDRW2100032466.1.24",
          "form": "글 짜내는 게..",
          "original_form": "글 짜내는 게..",
          "speaker_id": "2",
          "time": "20210905 21:33"
        },
        {
          "id": "MDRW2100032466.1.25",
          "form": "이제 온갖 로판의 미사여구를 데려다 쓰는거지 ㅋㅋ",
          "original_form": "이제 온갖 로판의 미사여구를 데려다 쓰는거지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:36"
        }
      ]
