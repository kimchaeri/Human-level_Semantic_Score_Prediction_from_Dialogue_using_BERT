      "utterance": [
        {
          "id": "MDRW2100019668.1.1",
          "form": "요리 잘해요?",
          "original_form": "요리 잘해요?",
          "speaker_id": "2",
          "time": "20210827 21:07"
        },
        {
          "id": "MDRW2100019668.1.2",
          "form": "요리 그냥 자취음식 정도 해요",
          "original_form": "요리 그냥 자취음식 정도 해요",
          "speaker_id": "1",
          "time": "20210827 21:07"
        },
        {
          "id": "MDRW2100019668.1.3",
          "form": "요리 잘하세요?",
          "original_form": "요리 잘하세요?",
          "speaker_id": "1",
          "time": "20210827 21:07"
        },
        {
          "id": "MDRW2100019668.1.4",
          "form": "전 요리 잘하는 편이에요 즐겨하기도 하구요 ㅎㅎ",
          "original_form": "전 요리 잘하는 편이에요 즐겨하기도 하구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:10"
        },
        {
          "id": "MDRW2100019668.1.5",
          "form": "그래서 집에서 할 수 있는 요리는 밖에서 잘 안사먹어요",
          "original_form": "그래서 집에서 할 수 있는 요리는 밖에서 잘 안사먹어요",
          "speaker_id": "2",
          "time": "20210827 21:10"
        },
        {
          "id": "MDRW2100019668.1.6",
          "form": "어떤거 제일 자주 해드세요?",
          "original_form": "어떤거 제일 자주 해드세요?",
          "speaker_id": "1",
          "time": "20210827 21:13"
        },
        {
          "id": "MDRW2100019668.1.7",
          "form": "간단히 해먹을 땐 파스타를 주로 먹구요",
          "original_form": "간단히 해먹을 땐 파스타를 주로 먹구요",
          "speaker_id": "2",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100019668.1.8",
          "form": "한번 삘 꽂히면 거의 그것만 먹는 거 같아요ㅋㅋ 리조또 같은거요",
          "original_form": "한번 삘 꽂히면 거의 그것만 먹는 거 같아요ㅋㅋ 리조또 같은거요",
          "speaker_id": "2",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100019668.1.9",
          "form": "예전엔 라자냐에 꽂혀서 1일 1라자냐 했어요..",
          "original_form": "예전엔 라자냐에 꽂혀서 1일 1라자냐 했어요..",
          "speaker_id": "2",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100019668.1.10",
          "form": "그럼 가장 자신있는 메뉴는 뭐예요?",
          "original_form": "그럼 가장 자신있는 메뉴는 뭐예요?",
          "speaker_id": "1",
          "time": "20210827 21:19"
        },
        {
          "id": "MDRW2100019668.1.11",
          "form": "어렵네요.. 딱 하나만 골라야해요?",
          "original_form": "어렵네요.. 딱 하나만 골라야해요?",
          "speaker_id": "2",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100019668.1.12",
          "form": "여러개도 좋아요",
          "original_form": "여러개도 좋아요",
          "speaker_id": "1",
          "time": "20210827 21:21"
        },
        {
          "id": "MDRW2100019668.1.13",
          "form": "파스타랑 부추전이요 ㅋㅋㅋ",
          "original_form": "파스타랑 부추전이요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:23"
        },
        {
          "id": "MDRW2100019668.1.14",
          "form": "파스타는 베이컨이랑 새우랑 해물 완전 가득히 채워넣어서 만들어요!",
          "original_form": "파스타는 베이컨이랑 새우랑 해물 완전 가득히 채워넣어서 만들어요!",
          "speaker_id": "2",
          "time": "20210827 21:23"
        },
        {
          "id": "MDRW2100019668.1.15",
          "form": "요즘 비많이 오는데 부추전 좋겠네요",
          "original_form": "요즘 비많이 오는데 부추전 좋겠네요",
          "speaker_id": "1",
          "time": "20210827 21:23"
        },
        {
          "id": "MDRW2100019668.1.16",
          "form": "우와 맛있겠어요",
          "original_form": "우와 맛있겠어요",
          "speaker_id": "1",
          "time": "20210827 21:23"
        },
        {
          "id": "MDRW2100019668.1.17",
          "form": "집에서 만들면 그런게 장점인거 같아요",
          "original_form": "집에서 만들면 그런게 장점인거 같아요",
          "speaker_id": "1",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100019668.1.18",
          "form": "저 부추전 진짜 바삭바삭하게 잘만들어요!",
          "original_form": "저 부추전 진짜 바삭바삭하게 잘만들어요!",
          "speaker_id": "2",
          "time": "20210827 21:28"
        },
        {
          "id": "MDRW2100019668.1.19",
          "form": "김치전도 잘 만드는데 최근에 실패 많이해서 자신감 떨어졌어요..",
          "original_form": "김치전도 잘 만드는데 최근에 실패 많이해서 자신감 떨어졌어요..",
          "speaker_id": "2",
          "time": "20210827 21:28"
        },
        {
          "id": "MDRW2100019668.1.20",
          "form": "그러시구나!",
          "original_form": "그러시구나!",
          "speaker_id": "1",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100019668.1.21",
          "form": "저는 요리다 좋은데 식재료 관리가 제일 힘들더라구요",
          "original_form": "저는 요리다 좋은데 식재료 관리가 제일 힘들더라구요",
          "speaker_id": "1",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100019668.1.22",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210829 10:58"
        }
      ]
    }
