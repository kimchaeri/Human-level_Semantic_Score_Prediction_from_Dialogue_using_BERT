      "utterance": [
        {
          "id": "MDRW2100023605.1.1",
          "form": "안녕하세용 점심은 드셨어용?",
          "original_form": "안녕하세용 점심은 드셨어용?",
          "speaker_id": "1",
          "time": "20210830 12:41"
        },
        {
          "id": "MDRW2100023605.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210830 12:41"
        },
        {
          "id": "MDRW2100023605.1.3",
          "form": "아직이요 출근중이라서 못먹었습니다",
          "original_form": "아직이요 출근중이라서 못먹었습니다",
          "speaker_id": "2",
          "time": "20210830 12:41"
        },
        {
          "id": "MDRW2100023605.1.4",
          "form": "아아ㅜㅠ 출근하시는구나",
          "original_form": "아아ㅜㅠ 출근하시는구나",
          "speaker_id": "1",
          "time": "20210830 12:42"
        },
        {
          "id": "MDRW2100023605.1.5",
          "form": "일하시려면 더 든든히 드셔야할텐데",
          "original_form": "일하시려면 더 든든히 드셔야할텐데",
          "speaker_id": "1",
          "time": "20210830 12:42"
        },
        {
          "id": "MDRW2100023605.1.6",
          "form": "사무직이라서 항,상출근해서 간단하게 먹습니다ㅋㅋㅋ",
          "original_form": "사무직이라서 항,상출근해서 간단하게 먹습니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 12:43"
        },
        {
          "id": "MDRW2100023605.1.7",
          "form": "아하ㅎㅎ 음식 뭐 좋아하세요?",
          "original_form": "아하ㅎㅎ 음식 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 12:43"
        },
        {
          "id": "MDRW2100023605.1.8",
          "form": "피자좋아해요 저는ㅋㅋㅋ",
          "original_form": "피자좋아해요 저는ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 12:45"
        },
        {
          "id": "MDRW2100023605.1.9",
          "form": "뭐좋아하세요?",
          "original_form": "뭐좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 12:46"
        },
        {
          "id": "MDRW2100023605.1.10",
          "form": "저도 피자랑 치킨이요ㅋㅋㅋ",
          "original_form": "저도 피자랑 치킨이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 12:46"
        },
        {
          "id": "MDRW2100023605.1.11",
          "form": "어디피자 좋아하세요?",
          "original_form": "어디피자 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 12:46"
        },
        {
          "id": "MDRW2100023605.1.12",
          "form": "피자에땅 맵달피자 최애입니다!",
          "original_form": "피자에땅 맵달피자 최애입니다!",
          "speaker_id": "2",
          "time": "20210830 12:49"
        },
        {
          "id": "MDRW2100023605.1.13",
          "form": "오오 아직 한번도 안먹어봤는데",
          "original_form": "오오 아직 한번도 안먹어봤는데",
          "speaker_id": "1",
          "time": "20210830 12:49"
        },
        {
          "id": "MDRW2100023605.1.14",
          "form": "꼭 먹어볼게용",
          "original_form": "꼭 먹어볼게용",
          "speaker_id": "1",
          "time": "20210830 12:49"
        },
        {
          "id": "MDRW2100023605.1.15",
          "form": "전 파파존스 수퍼파파스 좋아해요",
          "original_form": "전 파파존스 수퍼파파스 좋아해요",
          "speaker_id": "1",
          "time": "20210830 12:49"
        },
        {
          "id": "MDRW2100023605.1.16",
          "form": "오? 저도 거긴 처음인데 시켜봐야겠네요",
          "original_form": "오? 저도 거긴 처음인데 시켜봐야겠네요",
          "speaker_id": "2",
          "time": "20210830 12:50"
        },
        {
          "id": "MDRW2100023605.1.17",
          "form": "토마토 베이스가 진해서",
          "original_form": "토마토 베이스가 진해서",
          "speaker_id": "1",
          "time": "20210830 12:50"
        },
        {
          "id": "MDRW2100023605.1.18",
          "form": "추천드려요ㅎㅎ",
          "original_form": "추천드려요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 12:50"
        },
        {
          "id": "MDRW2100023605.1.19",
          "form": "아하 추천감사해요",
          "original_form": "아하 추천감사해요",
          "speaker_id": "2",
          "time": "20210830 12:53"
        },
        {
          "id": "MDRW2100023605.1.20",
          "form": "시키실때 사이드로 꼭 브라우니도 같이 시켜서 드셔보세용1",
          "original_form": "시키실때 사이드로 꼭 브라우니도 같이 시켜서 드셔보세용1",
          "speaker_id": "1",
          "time": "20210830 12:54"
