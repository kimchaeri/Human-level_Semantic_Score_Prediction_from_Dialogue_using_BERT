      "utterance": [
        {
          "id": "MDRW2100045849.1.1",
          "form": "비가 계속 오더니 날씨 좋아졌네",
          "original_form": "비가 계속 오더니 날씨 좋아졌네",
          "speaker_id": "2",
          "time": "20210908 09:10"
        },
        {
          "id": "MDRW2100045849.1.2",
          "form": "응 그런데 담주에 태풍온데",
          "original_form": "응 그런데 담주에 태풍온데",
          "speaker_id": "1",
          "time": "20210908 09:10"
        },
        {
          "id": "MDRW2100045849.1.3",
          "form": "그래도 1주일 정도는 날씨 좋아",
          "original_form": "그래도 1주일 정도는 날씨 좋아",
          "speaker_id": "1",
          "time": "20210908 09:11"
        },
        {
          "id": "MDRW2100045849.1.4",
          "form": "날씨 좋을 때 할 일 다 해놔야겠다",
          "original_form": "날씨 좋을 때 할 일 다 해놔야겠다",
          "speaker_id": "2",
          "time": "20210908 09:12"
        },
        {
          "id": "MDRW2100045849.1.5",
          "form": "응 빨래해야지 ㅠㅋㅋ 환기도 자주하고",
          "original_form": "응 빨래해야지 ㅠㅋㅋ 환기도 자주하고",
          "speaker_id": "1",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100045849.1.6",
          "form": "방수 신발도 구비해야고 맛집 먼 데도 미리 가놔야할 듯",
          "original_form": "방수 신발도 구비해야고 맛집 먼 데도 미리 가놔야할 듯",
          "speaker_id": "2",
          "time": "20210908 09:15"
        },
        {
          "id": "MDRW2100045849.1.7",
          "form": "태풍 대비용으로?",
          "original_form": "태풍 대비용으로?",
          "speaker_id": "1",
          "time": "20210908 09:15"
        },
        {
          "id": "MDRW2100045849.1.8",
          "form": "응. 지금 신발들은 발 아프니까",
          "original_form": "응. 지금 신발들은 발 아프니까",
          "speaker_id": "2",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100045849.1.9",
          "form": "태풍 저번에는 가을장마랑 겹쳐서 비 많이 왔지 ㅠ",
          "original_form": "태풍 저번에는 가을장마랑 겹쳐서 비 많이 왔지 ㅠ",
          "speaker_id": "1",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100045849.1.10",
          "form": "비 많이 안 왔으면 좋겠다. ㅠㅠ",
          "original_form": "비 많이 안 왔으면 좋겠다. ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100045849.1.11",
          "form": "비 거의 자주 왔지 ㅠ 이젠 지겨워",
          "original_form": "비 거의 자주 왔지 ㅠ 이젠 지겨워",
          "speaker_id": "1",
          "time": "20210908 09:20"
        },
        {
          "id": "MDRW2100045849.1.12",
          "form": "맞아. 뚜벅이들에겐 힘들어.",
          "original_form": "맞아. 뚜벅이들에겐 힘들어.",
          "speaker_id": "2",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045849.1.13",
          "form": "당분간 좋은 날씨를 즐기자! ㅎㅎ",
          "original_form": "당분간 좋은 날씨를 즐기자! ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045849.1.14",
          "form": "응. 지금을 즐겨야겠어",
          "original_form": "응. 지금을 즐겨야겠어",
          "speaker_id": "2",
          "time": "20210908 09:23"
        }
      ]
