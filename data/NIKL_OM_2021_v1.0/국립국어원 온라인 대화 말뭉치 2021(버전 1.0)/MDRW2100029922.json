      "utterance": [
        {
          "id": "MDRW2100029922.1.1",
          "form": "게임 같은 것도 하세요?",
          "original_form": "게임 같은 것도 하세요?",
          "speaker_id": "1",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100029922.1.2",
          "form": "어 게임 좋아했었어요!",
          "original_form": "어 게임 좋아했었어요!",
          "speaker_id": "2",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100029922.1.3",
          "form": "게임 하시나요?",
          "original_form": "게임 하시나요?",
          "speaker_id": "2",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100029922.1.4",
          "form": "지금은 쿠킹덤만 해요",
          "original_form": "지금은 쿠킹덤만 해요",
          "speaker_id": "1",
          "time": "20210903 16:37"
        },
        {
          "id": "MDRW2100029922.1.5",
          "form": "name2님은 어떤 게임 하셨어요?",
          "original_form": "&name2&님은 어떤 게임 하셨어요?",
          "speaker_id": "1",
          "time": "20210903 16:37"
        },
        {
          "id": "MDRW2100029922.1.6",
          "form": "헐 쿠키런 ...",
          "original_form": "헐 쿠키런 ...",
          "speaker_id": "2",
          "time": "20210903 16:42"
        },
        {
          "id": "MDRW2100029922.1.7",
          "form": "전 온라인게임종류 위주했었는데",
          "original_form": "전 온라인게임종류 위주했었는데",
          "speaker_id": "2",
          "time": "20210903 16:42"
        },
        {
          "id": "MDRW2100029922.1.8",
          "form": "예전에ㅜ쿠키런도 열심히했었어요",
          "original_form": "예전에ㅜ쿠키런도 열심히했었어요",
          "speaker_id": "2",
          "time": "20210903 16:42"
        },
        {
          "id": "MDRW2100029922.1.9",
          "form": "온라인게임은 예전에 파판 잠깐 했어요 런칭 초기에...",
          "original_form": "온라인게임은 예전에 파판 잠깐 했어요 런칭 초기에...",
          "speaker_id": "1",
          "time": "20210903 16:44"
        },
        {
          "id": "MDRW2100029922.1.10",
          "form": "파이널판타지 재미있져 ㅎㅎ",
          "original_form": "파이널판타지 재미있져 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 16:44"
        },
        {
          "id": "MDRW2100029922.1.11",
          "form": "쿠키런킹덤이였는지는 모르겠는데",
          "original_form": "쿠키런킹덤이였는지는 모르겠는데",
          "speaker_id": "2",
          "time": "20210903 16:44"
        },
        {
          "id": "MDRW2100029922.1.12",
          "form": "한 1-2년전에",
          "original_form": "한 1-2년전에",
          "speaker_id": "2",
          "time": "20210903 16:44"
        },
        {
          "id": "MDRW2100029922.1.13",
          "form": "엄청열심히ㅜ했었던 기억있네요!",
          "original_form": "엄청열심히ㅜ했었던 기억있네요!",
          "speaker_id": "2",
          "time": "20210903 16:44"
        },
        {
          "id": "MDRW2100029922.1.14",
          "form": "쿠키런 킹덤은 최근에 런칭된 거니까",
          "original_form": "쿠키런 킹덤은 최근에 런칭된 거니까",
          "speaker_id": "1",
          "time": "20210903 16:46"
        },
        {
          "id": "MDRW2100029922.1.15",
          "form": "쿠키런 오븐브레이크일거예요! 달리는거~",
          "original_form": "쿠키런 오븐브레이크일거예요! 달리는거~",
          "speaker_id": "1",
          "time": "20210903 16:46"
        },
        {
          "id": "MDRW2100029922.1.16",
          "form": "아 맞아요!",
          "original_form": "아 맞아요!",
          "speaker_id": "2",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100029922.1.17",
          "form": "요즘엔 그냥",
          "original_form": "요즘엔 그냥",
          "speaker_id": "2",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100029922.1.18",
          "form": "자동사냥같이 하는거같던데",
          "original_form": "자동사냥같이 하는거같던데",
          "speaker_id": "2",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100029922.1.19",
          "form": "맞나요?",
          "original_form": "맞나요?",
          "speaker_id": "2",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100029922.1.20",
          "form": "네네 왕국 꾸미고 전투하고 해요",
          "original_form": "네네 왕국 꾸미고 전투하고 해요",
          "speaker_id": "1",
          "time": "20210903 16:48"
        },
        {
          "id": "MDRW2100029922.1.21",
          "form": "아ㅜ친구들이 재밌게하는거봤어요",
          "original_form": "아ㅜ친구들이 재밌게하는거봤어요",
          "speaker_id": "2",
          "time": "20210903 16:48"
        },
        {
          "id": "MDRW2100029922.1.22",
          "form": "고수들은 스킬 타이밍 맞춰서 수동전투 하던데 전 그냥 자동 써요",
          "original_form": "고수들은 스킬 타이밍 맞춰서 수동전투 하던데 전 그냥 자동 써요",
          "speaker_id": "1",
          "time": "20210903 16:48"
        },
        {
          "id": "MDRW2100029922.1.23",
          "form": "막 조금이라도 손해안보려구",
          "original_form": "막 조금이라도 손해안보려구",
          "speaker_id": "2",
          "time": "20210903 16:48"
        },
        {
          "id": "MDRW2100029922.1.24",
          "form": "열심히 오토돌리구 ㅋㅋㅋㅋㅋ",
          "original_form": "열심히 오토돌리구 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:48"
        },
        {
          "id": "MDRW2100029922.1.25",
          "form": "게임은 아기자기하고 귀여운데",
          "original_form": "게임은 아기자기하고 귀여운데",
          "speaker_id": "1",
          "time": "20210903 16:49"
        },
        {
          "id": "MDRW2100029922.1.26",
          "form": "생각보다 해야할게 많더라고요",
          "original_form": "생각보다 해야할게 많더라고요",
          "speaker_id": "1",
          "time": "20210903 16:49"
        }
      ]
    }
