        },
        {
          "id": "MDRW2100043046.1.10",
          "form": "기장물산 비빔밥용 해초입니다",
          "original_form": "기장물산 비빔밥용 해초입니다",
          "speaker_id": "2",
          "time": "20210907 13:46"
        },
        {
          "id": "MDRW2100043046.1.11",
          "form": "ㅋㅋㅋㅋ감사해요 맛나겠다 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ감사해요 맛나겠다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:47"
        },
        {
