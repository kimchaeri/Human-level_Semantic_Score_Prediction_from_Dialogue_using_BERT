      "utterance": [
        {
          "id": "MDRW2100041496.1.1",
          "form": "저녁 먹었어?",
          "original_form": "저녁 먹었어?",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100041496.1.2",
          "form": "응 아빠랑 같이 감자탕 먹었어",
          "original_form": "응 아빠랑 같이 감자탕 먹었어",
          "speaker_id": "2",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100041496.1.3",
          "form": "난 비 오니까 파전이 먹고 싶네~~",
          "original_form": "난 비 오니까 파전이 먹고 싶네~~",
          "speaker_id": "1",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100041496.1.4",
          "form": "엄마도 파전 땡긴다",
          "original_form": "엄마도 파전 땡긴다",
          "speaker_id": "2",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100041496.1.5",
          "form": "너는 저녁 먹었어?",
          "original_form": "너는 저녁 먹었어?",
          "speaker_id": "2",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100041496.1.6",
          "form": "응 칼국수 먹었어",
          "original_form": "응 칼국수 먹었어",
          "speaker_id": "1",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100041496.1.7",
          "form": "칼국수도 맛있었겠네",
          "original_form": "칼국수도 맛있었겠네",
          "speaker_id": "2",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100041496.1.8",
          "form": "김치랑 같이 먹으면 딱이잖아",
          "original_form": "김치랑 같이 먹으면 딱이잖아",
          "speaker_id": "2",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100041496.1.9",
          "form": "ㅋㅋㅋ고렇지",
          "original_form": "ㅋㅋㅋ고렇지",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100041496.1.10",
          "form": "볶음김치랑 같이 먹었어",
          "original_form": "볶음김치랑 같이 먹었어",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100041496.1.11",
          "form": "엄미는 갈치구이 먹고 싶어",
          "original_form": "엄미는 갈치구이 먹고 싶어",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100041496.1.12",
          "form": "언제 같이 생선구이집 갈까?",
          "original_form": "언제 같이 생선구이집 갈까?",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100041496.1.13",
          "form": "나도 생선 안 먹은지 엄청 오래됐어",
          "original_form": "나도 생선 안 먹은지 엄청 오래됐어",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100041496.1.14",
          "form": "그래 좋아.",
          "original_form": "그래 좋아.",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100041496.1.15",
          "form": "회사 근처에 생선 정식하는 식당 있어",
          "original_form": "회사 근처에 생선 정식하는 식당 있어",
          "speaker_id": "2",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100041496.1.16",
          "form": "오케~~ 밥 먹고 내가 커피까지 쏠게",
          "original_form": "오케~~ 밥 먹고 내가 커피까지 쏠게",
          "speaker_id": "1",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100041496.1.17",
          "form": "아싸. 비싼 거 마셔야지",
          "original_form": "아싸. 비싼 거 마셔야지",
          "speaker_id": "2",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100041496.1.18",
          "form": "카라멜 마끼야또 같은 거",
          "original_form": "카라멜 마끼야또 같은 거",
          "speaker_id": "2",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100041496.1.19",
          "form": "ㅋㅋㅋㅋ 다 사주지~ 나는 설탕 끊으려고 아메리카노 마셔",
          "original_form": "ㅋㅋㅋㅋ 다 사주지~ 나는 설탕 끊으려고 아메리카노 마셔",
          "speaker_id": "1",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100041496.1.20",
          "form": "잘했어 이제 따뜻한 차 마셔",
          "original_form": "잘했어 이제 따뜻한 차 마셔",
