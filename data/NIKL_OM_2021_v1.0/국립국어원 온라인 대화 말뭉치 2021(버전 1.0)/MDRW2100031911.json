      "utterance": [
        {
          "id": "MDRW2100031911.1.1",
          "form": "어제 부대찌개 맛있었지?",
          "original_form": "어제 부대찌개 맛있었지?",
          "speaker_id": "1",
          "time": "20210907 10:26"
        },
        {
          "id": "MDRW2100031911.1.2",
          "form": "시켜먹은 거 치고는 괜찮았음",
          "original_form": "시켜먹은 거 치고는 괜찮았음",
          "speaker_id": "2",
          "time": "20210907 10:28"
        },
        {
          "id": "MDRW2100031911.1.3",
          "form": "담꾹이 괜찮네",
          "original_form": "담꾹이 괜찮네",
          "speaker_id": "1",
          "time": "20210907 10:29"
        },
        {
          "id": "MDRW2100031911.1.4",
          "form": "놀부부대찌개보다 나는 좋았어",
          "original_form": "놀부부대찌개보다 나는 좋았어",
          "speaker_id": "1",
          "time": "20210907 10:29"
        },
        {
          "id": "MDRW2100031911.1.5",
          "form": "놀부는 원래 맛이 없음 ㅋ",
          "original_form": "놀부는 원래 맛이 없음 ㅋ",
          "speaker_id": "2",
          "time": "20210907 10:29"
        },
        {
          "id": "MDRW2100031911.1.6",
          "form": "왕김치를 넣을 수 있어서ㅋㅋㅋ",
          "original_form": "왕김치를 넣을 수 있어서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:29"
        },
        {
          "id": "MDRW2100031911.1.7",
          "form": "고약한 놈이 만드는거라 ㅋㅋ",
          "original_form": "고약한 놈이 만드는거라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:29"
        },
        {
          "id": "MDRW2100031911.1.8",
          "form": "흥부는 재료가 없을듯ㅋㅋ",
          "original_form": "흥부는 재료가 없을듯ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:30"
        },
        {
          "id": "MDRW2100031911.1.9",
          "form": "흥부가 만들어야 맛있는데 ㅋㅋㅋ",
          "original_form": "흥부가 만들어야 맛있는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:30"
        },
        {
          "id": "MDRW2100031911.1.10",
          "form": "왱? 흥부 부자가 되지 않았는가 ㅋ",
          "original_form": "왱? 흥부 부자가 되지 않았는가 ㅋ",
          "speaker_id": "2",
          "time": "20210907 10:30"
        },
        {
          "id": "MDRW2100031911.1.11",
          "form": "비오니까 국물음식이 땡기네",
          "original_form": "비오니까 국물음식이 땡기네",
          "speaker_id": "1",
          "time": "20210907 10:30"
        },
        {
          "id": "MDRW2100031911.1.12",
          "form": "부대찌개 남았네 ㅋ 그거 먹게나",
          "original_form": "부대찌개 남았네 ㅋ 그거 먹게나",
          "speaker_id": "2",
          "time": "20210907 10:30"
        },
        {
          "id": "MDRW2100031911.1.13",
          "form": "부찌 김찌 맛있어",
          "original_form": "부찌 김찌 맛있어",
          "speaker_id": "1",
          "time": "20210907 10:30"
        },
        {
          "id": "MDRW2100031911.1.14",
          "form": "왕김치를 넣어주오",
          "original_form": "왕김치를 넣어주오",
          "speaker_id": "1",
          "time": "20210907 10:31"
        },
        {
          "id": "MDRW2100031911.1.15",
          "form": "왕김치를 넣으면 김치찌개가 아닌가",
          "original_form": "왕김치를 넣으면 김치찌개가 아닌가",
          "speaker_id": "2",
          "time": "20210907 10:31"
        },
        {
          "id": "MDRW2100031911.1.16",
          "form": "얼큰이 칼국수도 맛있는데",
          "original_form": "얼큰이 칼국수도 맛있는데",
          "speaker_id": "1",
          "time": "20210907 10:31"
        },
        {
          "id": "MDRW2100031911.1.17",
          "form": "먹고 싶은게 많구먼 ㅋ",
          "original_form": "먹고 싶은게 많구먼 ㅋ",
          "speaker_id": "2",
          "time": "20210907 10:31"
        },
        {
          "id": "MDRW2100031911.1.18",
          "form": "아 그런가 나는 부찌가 김찌같아서 좋은거였나",
          "original_form": "아 그런가 나는 부찌가 김찌같아서 좋은거였나",
          "speaker_id": "1",
          "time": "20210907 10:31"
        }
      ]
