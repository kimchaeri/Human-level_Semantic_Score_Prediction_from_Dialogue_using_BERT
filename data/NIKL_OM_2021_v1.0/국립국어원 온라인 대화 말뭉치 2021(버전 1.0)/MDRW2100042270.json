      "utterance": [
        {
          "id": "MDRW2100042270.1.1",
          "form": "여행 가는 거 좋아하세요?",
          "original_form": "여행 가는 거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100042270.1.2",
          "form": "네 좋아하죠! 최근에 간 여행지 있으세요?",
          "original_form": "네 좋아하죠! 최근에 간 여행지 있으세요?",
          "speaker_id": "2",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100042270.1.3",
          "form": "가장 최근에는 부산을 다녀왔어요!",
          "original_form": "가장 최근에는 부산을 다녀왔어요!",
          "speaker_id": "1",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100042270.1.4",
          "form": "요즘 해외 여행은 힘들잖아요ㅋㅋㅠㅠ",
          "original_form": "요즘 해외 여행은 힘들잖아요ㅋㅋㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100042270.1.5",
          "form": "네 그렇죠 부산 재밌죠ㅜㅜ어디어디 가셨나요",
          "original_form": "네 그렇죠 부산 재밌죠ㅜㅜ어디어디 가셨나요",
          "speaker_id": "2",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100042270.1.6",
          "form": "영도 가서 바다도 보고 전시도 보고",
          "original_form": "영도 가서 바다도 보고 전시도 보고",
          "speaker_id": "1",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100042270.1.7",
          "form": "서면 번화가도 둘러 보고",
          "original_form": "서면 번화가도 둘러 보고",
          "speaker_id": "1",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042270.1.8",
          "form": "해운대에서 캡슐 열차도 탔어요!",
          "original_form": "해운대에서 캡슐 열차도 탔어요!",
          "speaker_id": "1",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042270.1.9",
          "form": "국내 여행 자주 가세요?",
          "original_form": "국내 여행 자주 가세요?",
          "speaker_id": "1",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042270.1.10",
          "form": "알차게 놀고오셨네요 !! 캡슐열차는 처음들어봐요",
          "original_form": "알차게 놀고오셨네요 !! 캡슐열차는 처음들어봐요",
          "speaker_id": "2",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042270.1.11",
          "form": "저는 쫄보라 코땡땡 터진뒤론 여행을 안갔어요...",
          "original_form": "저는 쫄보라 코땡땡 터진뒤론 여행을 안갔어요...",
          "speaker_id": "2",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042270.1.12",
          "form": "부산은 예전에 많이 갔었네요ㅜㅜㅎㅎ",
          "original_form": "부산은 예전에 많이 갔었네요ㅜㅜㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042270.1.13",
          "form": "그게 맞는 거죠ㅠㅠ 저는 고향이 부산쪽이라 이번에 집 가는 길에",
          "original_form": "그게 맞는 거죠ㅠㅠ 저는 고향이 부산쪽이라 이번에 집 가는 길에",
          "speaker_id": "1",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100042270.1.14",
          "form": "들렸어요",
          "original_form": "들렸어요",
          "speaker_id": "1",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100042270.1.15",
          "form": "그러셨구나 저도 부산 좋아하는데 또 가고싶네요",
          "original_form": "그러셨구나 저도 부산 좋아하는데 또 가고싶네요",
          "speaker_id": "2",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100042270.1.16",
          "form": "여행은 그럼 친구들이랑 자주 가여?",
          "original_form": "여행은 그럼 친구들이랑 자주 가여?",
          "speaker_id": "1",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100042270.1.17",
          "form": "가족이나 친구랑 다녔어요 남친있을땐 남친이랑도...",
          "original_form": "가족이나 친구랑 다녔어요 남친있을땐 남친이랑도...",
          "speaker_id": "2",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100042270.1.18",
          "form": "좋죠~ 요즘은 호캉스도 많이 가던데",
          "original_form": "좋죠~ 요즘은 호캉스도 많이 가던데",
          "speaker_id": "1",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100042270.1.19",
          "form": "코로나가 걱정이라면 호캉스는 생각 없으세요?",
          "original_form": "코로나가 걱정이라면 호캉스는 생각 없으세요?",
          "speaker_id": "1",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100042270.1.20",
          "form": "맞아요 호캉스는 괜찮을것같아요!!",
          "original_form": "맞아요 호캉스는 괜찮을것같아요!!",
          "speaker_id": "2",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100042270.1.21",
          "form": "제주도도 가고싶어용ㅋㅋ",
          "original_form": "제주도도 가고싶어용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100042270.1.22",
          "form": "헐 제주도 너무 좋아요ㅠㅠㅠ",
          "original_form": "헐 제주도 너무 좋아요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100042270.1.23",
          "form": "제주도 여행은 진짜 다녀온 사람들은 다 만족하는 것 같아요",
          "original_form": "제주도 여행은 진짜 다녀온 사람들은 다 만족하는 것 같아요",
          "speaker_id": "1",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100042270.1.24",
          "form": "일단 저도 그렇구요ㅋㅋㅋ",
          "original_form": "일단 저도 그렇구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100042270.1.25",
          "form": "제주도 저는 비가 많이 올때 갔었어요ㅜㅜㅋㅋ",
          "original_form": "제주도 저는 비가 많이 올때 갔었어요ㅜㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100042270.1.26",
          "form": "맑은날에 한번 더 가고싶네요",
          "original_form": "맑은날에 한번 더 가고싶네요",
          "speaker_id": "2",
          "time": "20210906 21:25"
        }
      ]
    }
