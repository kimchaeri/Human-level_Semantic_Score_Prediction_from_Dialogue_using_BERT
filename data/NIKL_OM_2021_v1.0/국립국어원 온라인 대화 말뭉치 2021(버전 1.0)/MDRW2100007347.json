      "utterance": [
        {
          "id": "MDRW2100007347.1.1",
          "form": "물과 음료 정도요",
          "original_form": "물과 음료 정도요",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007347.1.2",
          "form": "줌수업이라면!",
          "original_form": "줌수업이라면!",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007347.1.3",
          "form": "근데 과자도 가능할 것 같아요",
          "original_form": "근데 과자도 가능할 것 같아요",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007347.1.4",
          "form": "전 최근에 크로플을 먹었는데",
          "original_form": "전 최근에 크로플을 먹었는데",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007347.1.5",
          "form": "입에 넣을 때마다 비디오를 껐거든요",
          "original_form": "입에 넣을 때마다 비디오를 껐거든요",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007347.1.6",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007347.1.7",
          "form": "저희 어머니도",
          "original_form": "저희 어머니도",
          "speaker_id": "1",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007347.1.8",
          "form": "줌수업을 하시는데",
          "original_form": "줌수업을 하시는데",
          "speaker_id": "1",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007347.1.9",
          "form": "눈만 내놓고",
          "original_form": "눈만 내놓고",
          "speaker_id": "1",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007347.1.10",
          "form": "삼겹살 드시더라고여",
          "original_form": "삼겹살 드시더라고여",
          "speaker_id": "1",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007347.1.11",
          "form": "삼겹살은 좀 심하잖아요 ㅋㅋㅋㅋㅋㅋㅋ ㅠㅠ",
          "original_form": "삼겹살은 좀 심하잖아요 ㅋㅋㅋㅋㅋㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007347.1.12",
          "form": "하지만 꽤 좋은? 방법이네요",
          "original_form": "하지만 꽤 좋은? 방법이네요",
          "speaker_id": "2",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007347.1.13",
          "form": "눈만 내놓을 생각을 못했어요",
          "original_form": "눈만 내놓을 생각을 못했어요",
          "speaker_id": "2",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007347.1.14",
          "form": "직접 굽는게 아니고",
          "original_form": "직접 굽는게 아니고",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007347.1.15",
          "form": "아비부가 구워준걸",
          "original_form": "아비부가 구워준걸",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007347.1.16",
          "form": "먹기만 하면",
          "original_form": "먹기만 하면",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007347.1.17",
          "form": "괜찮지않을까요?",
          "original_form": "괜찮지않을까요?",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007347.1.18",
          "form": "그쵸...",
          "original_form": "그쵸...",
          "speaker_id": "2",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007347.1.19",
          "form": "중요한 건 아마",
          "original_form": "중요한 건 아마",
          "speaker_id": "2",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007347.1.20",
          "form": "안 걸리는 거?",
          "original_form": "안 걸리는 거?",
          "speaker_id": "2",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007347.1.21",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007347.1.22",
          "form": "쌈도 싸드셨나요?",
          "original_form": "쌈도 싸드셨나요?",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007347.1.23",
          "form": "수업에서 쌈 싸먹기...",
          "original_form": "수업에서 쌈 싸먹기...",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007347.1.24",
          "form": "제가 싸서",
          "original_form": "제가 싸서",
          "speaker_id": "1",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007347.1.25",
          "form": "입에 넣어드렷어요",
          "original_form": "입에 넣어드렷어요",
          "speaker_id": "1",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007347.1.26",
          "form": "줌 수업이니까",
          "original_form": "줌 수업이니까",
          "speaker_id": "1",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007347.1.27",
          "form": "뭐...",
          "original_form": "뭐...",
          "speaker_id": "1",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007347.1.28",
          "form": "너무 즐거운 학교생활이에요",
          "original_form": "너무 즐거운 학교생활이에요",
          "speaker_id": "2",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007347.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007347.1.30",
          "form": "곧 개학이라서",
          "original_form": "곧 개학이라서",
          "speaker_id": "2",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007347.1.31",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007347.1.32",
          "form": "월요일은 학교를 가보려고 하거든요",
          "original_form": "월요일은 학교를 가보려고 하거든요",
          "speaker_id": "2",
          "time": "20210822 01:40"
        }
      ]
    }
