      "utterance": [
        {
          "id": "MDRW2100030469.1.1",
          "form": "요즘 모기가 너무 많아서 잘 때 짜증나",
          "original_form": "요즘 모기가 너무 많아서 잘 때 짜증나",
          "speaker_id": "2",
          "time": "20210904 01:34"
        },
        {
          "id": "MDRW2100030469.1.2",
          "form": "자꾸 깨",
          "original_form": "자꾸 깨",
          "speaker_id": "2",
          "time": "20210904 01:34"
        },
        {
          "id": "MDRW2100030469.1.3",
          "form": "모기장치고 자야돼",
          "original_form": "모기장치고 자야돼",
          "speaker_id": "1",
          "time": "20210904 01:34"
        },
        {
          "id": "MDRW2100030469.1.4",
          "form": "안그럼 못자",
          "original_form": "안그럼 못자",
          "speaker_id": "1",
          "time": "20210904 01:35"
        },
        {
          "id": "MDRW2100030469.1.5",
          "form": "요즘 모기 왜 이렇게 똑똑해??",
          "original_form": "요즘 모기 왜 이렇게 똑똑해??",
          "speaker_id": "2",
          "time": "20210904 01:35"
        },
        {
          "id": "MDRW2100030469.1.6",
          "form": "잡으려고 불켜면 사라져있더라",
          "original_form": "잡으려고 불켜면 사라져있더라",
          "speaker_id": "2",
          "time": "20210904 01:35"
        },
        {
          "id": "MDRW2100030469.1.7",
          "form": "불끄면 다시 오고",
          "original_form": "불끄면 다시 오고",
          "speaker_id": "2",
          "time": "20210904 01:35"
        },
        {
          "id": "MDRW2100030469.1.8",
          "form": "요즘 모기가 그렇다니까",
          "original_form": "요즘 모기가 그렇다니까",
          "speaker_id": "1",
          "time": "20210904 01:36"
        },
        {
          "id": "MDRW2100030469.1.9",
          "form": "눈치가 엄청빨라",
          "original_form": "눈치가 엄청빨라",
          "speaker_id": "1",
          "time": "20210904 01:36"
        },
        {
          "id": "MDRW2100030469.1.10",
          "form": "모기장 안치면 못자",
          "original_form": "모기장 안치면 못자",
          "speaker_id": "1",
          "time": "20210904 01:37"
        },
        {
          "id": "MDRW2100030469.1.11",
          "form": "집에 모기장 또 있어???",
          "original_form": "집에 모기장 또 있어???",
          "speaker_id": "2",
          "time": "20210904 01:37"
        },
        {
          "id": "MDRW2100030469.1.12",
          "form": "옥상에 원터치모기장 있어",
          "original_form": "옥상에 원터치모기장 있어",
          "speaker_id": "1",
          "time": "20210904 01:37"
        },
        {
          "id": "MDRW2100030469.1.13",
          "form": "내일 좀 찾아줘",
          "original_form": "내일 좀 찾아줘",
          "speaker_id": "2",
          "time": "20210904 01:37"
        },
        {
          "id": "MDRW2100030469.1.14",
          "form": "알겠어 내일 찾아놓을게",
          "original_form": "알겠어 내일 찾아놓을게",
          "speaker_id": "1",
          "time": "20210904 01:42"
        },
        {
          "id": "MDRW2100030469.1.15",
          "form": "옥상에서 겨울옷도 미리 꺼내놓을까",
          "original_form": "옥상에서 겨울옷도 미리 꺼내놓을까",
          "speaker_id": "2",
          "time": "20210904 01:43"
        },
        {
          "id": "MDRW2100030469.1.16",
          "form": "그래 꺼내서 옷장에 넣어놓자",
          "original_form": "그래 꺼내서 옷장에 넣어놓자",
          "speaker_id": "1",
          "time": "20210904 01:44"
        },
        {
          "id": "MDRW2100030469.1.17",
          "form": "겨울옷은 내가 꺼낼게",
          "original_form": "겨울옷은 내가 꺼낼게",
          "speaker_id": "2",
          "time": "20210904 01:44"
        },
        {
          "id": "MDRW2100030469.1.18",
          "form": "누구누구거 꺼내??",
          "original_form": "누구누구거 꺼내??",
          "speaker_id": "2",
          "time": "20210904 01:44"
        },
        {
          "id": "MDRW2100030469.1.19",
          "form": "니 옷만 옷장에 넣어놔",
          "original_form": "니 옷만 옷장에 넣어놔",
          "speaker_id": "1",
          "time": "20210904 01:46"
        },
        {
          "id": "MDRW2100030469.1.20",
          "form": "다 드라이한거지???",
          "original_form": "다 드라이한거지???",
          "speaker_id": "2",
          "time": "20210904 01:46"
        },
        {
          "id": "MDRW2100030469.1.21",
          "form": "응 드라이한거야",
          "original_form": "응 드라이한거야",
          "speaker_id": "1",
          "time": "20210904 01:46"
        },
        {
          "id": "MDRW2100030469.1.22",
          "form": "비오니까 어제보다 더 일찍출발해야겠어",
          "original_form": "비오니까 어제보다 더 일찍출발해야겠어",
          "speaker_id": "1",
          "time": "20210907 07:26"
        }
      ]
