      "utterance": [
        {
          "id": "MDRW2100043256.1.1",
          "form": "저희는 비가 그치는가 싶더니",
          "original_form": "저희는 비가 그치는가 싶더니",
          "speaker_id": "1",
          "time": "20210907 17:22"
        },
        {
          "id": "MDRW2100043256.1.2",
          "form": "다시 내리고 있어쇼",
          "original_form": "다시 내리고 있어쇼",
          "speaker_id": "1",
          "time": "20210907 17:22"
        },
        {
          "id": "MDRW2100043256.1.3",
          "form": "있어요",
          "original_form": "있어요",
          "speaker_id": "1",
          "time": "20210907 17:22"
        },
        {
          "id": "MDRW2100043256.1.4",
          "form": "여기는 그치긴 했는데 어둑어둑ㅜㅠ",
          "original_form": "여기는 그치긴 했는데 어둑어둑ㅜㅠ",
          "speaker_id": "2",
          "time": "20210907 17:23"
        },
        {
          "id": "MDRW2100043256.1.5",
          "form": "내일 야외행사가 있는데ㅠ",
          "original_form": "내일 야외행사가 있는데ㅠ",
          "speaker_id": "1",
          "time": "20210907 17:24"
        },
        {
          "id": "MDRW2100043256.1.6",
          "form": "계속 비가 내리니 걱정이예요",
          "original_form": "계속 비가 내리니 걱정이예요",
          "speaker_id": "1",
          "time": "20210907 17:24"
        },
        {
          "id": "MDRW2100043256.1.7",
          "form": "아 진짜요??? 연기나 취소 안되는건가요?? ㅜㅠ",
          "original_form": "아 진짜요??? 연기나 취소 안되는건가요?? ㅜㅠ",
          "speaker_id": "2",
          "time": "20210907 17:39"
        },
        {
          "id": "MDRW2100043256.1.8",
          "form": "서울은 내일 오후에 그친대요",
          "original_form": "서울은 내일 오후에 그친대요",
          "speaker_id": "2",
          "time": "20210907 17:39"
        },
        {
          "id": "MDRW2100043256.1.9",
          "form": "네 일단 진행한다는데",
          "original_form": "네 일단 진행한다는데",
          "speaker_id": "1",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100043256.1.10",
          "form": "저흰 오전에 행사라ㅠ",
          "original_form": "저흰 오전에 행사라ㅠ",
          "speaker_id": "1",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100043256.1.11",
          "form": "ㅜㅜ 하필이면... 부디 내일도 기상청이 틀리길ㅜㅠ",
          "original_form": "ㅜㅜ 하필이면... 부디 내일도 기상청이 틀리길ㅜㅠ",
          "speaker_id": "2",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100043256.1.12",
          "form": "ㄷ퇴근길 하늘 어마무시하네요",
          "original_form": "ㄷ퇴근길 하늘 어마무시하네요",
          "speaker_id": "2",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100043256.1.13",
          "form": "그쵸 어둑어둑",
          "original_form": "그쵸 어둑어둑",
          "speaker_id": "1",
          "time": "20210907 19:00"
        },
        {
          "id": "MDRW2100043256.1.14",
          "form": "사무실에서 보면 무서워요ㅋㅋ 하늘만 보면",
          "original_form": "사무실에서 보면 무서워요ㅋㅋ 하늘만 보면",
          "speaker_id": "2",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100043256.1.15",
          "form": "네 지난 주말의 파랗고 예쁜 하늘은 어디 갔는지",
          "original_form": "네 지난 주말의 파랗고 예쁜 하늘은 어디 갔는지",
          "speaker_id": "1",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100043256.1.16",
          "form": "우산이 필요없네요ㅋㅋ",
          "original_form": "우산이 필요없네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:10"
        },
        {
          "id": "MDRW2100043256.1.17",
          "form": "비가 그친 건가요?",
          "original_form": "비가 그친 건가요?",
          "speaker_id": "1",
          "time": "20210907 19:10"
        },
        {
          "id": "MDRW2100043256.1.18",
          "form": "아님 너무 세차게 오는건가요?",
          "original_form": "아님 너무 세차게 오는건가요?",
          "speaker_id": "1",
          "time": "20210907 19:10"
        },
        {
          "id": "MDRW2100043256.1.19",
          "form": "한방울도... ㅋㅋ",
          "original_form": "한방울도... ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:11"
        },
        {
          "id": "MDRW2100043256.1.20",
          "form": "오지않아서 또 제 비전용신발이 민망하네요 ㅋㅋ",
          "original_form": "오지않아서 또 제 비전용신발이 민망하네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:11"
        },
        {
          "id": "MDRW2100043256.1.21",
          "form": "앗 레인부츠 신으셨나요",
          "original_form": "앗 레인부츠 신으셨나요",
          "speaker_id": "1",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100043256.1.22",
          "form": "ㅋㅋ 저만 또 진심이었죠...",
          "original_form": "ㅋㅋ 저만 또 진심이었죠...",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043256.1.23",
          "form": "ㅋㅋ그래도 땅엔 물기가 많으니",
          "original_form": "ㅋㅋ그래도 땅엔 물기가 많으니",
          "speaker_id": "1",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043256.1.24",
          "form": "레인부츠 어색하지 않을 거예요",
          "original_form": "레인부츠 어색하지 않을 거예요",
          "speaker_id": "1",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043256.1.25",
          "form": "하지만 지하철.. 그 누구도ㅋㅋ",
          "original_form": "하지만 지하철.. 그 누구도ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100043256.1.26",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "1",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100043256.1.27",
          "form": "비 오는 날이었으니까",
          "original_form": "비 오는 날이었으니까",
          "speaker_id": "1",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100043256.1.28",
          "form": "ㅋㅋㅋ모두들 이해하겠죠",
          "original_form": "ㅋㅋㅋ모두들 이해하겠죠",
          "speaker_id": "1",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100043256.1.29",
          "form": "ㅋㅋ머피의 법칙...",
          "original_form": "ㅋㅋ머피의 법칙...",
          "speaker_id": "2",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043256.1.30",
          "form": "요즘은 잘 안쓰는ㅋㅋ",
          "original_form": "요즘은 잘 안쓰는ㅋㅋ",
          "speaker_id": "2",
