      "utterance": [
        {
          "id": "MDRW2100022703.1.1",
          "form": "그런적이 없어서..",
          "original_form": "그런적이 없어서..",
          "speaker_id": "1",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100022703.1.2",
          "form": "딱 필요한것만 사시는군요ㅡ??",
          "original_form": "딱 필요한것만 사시는군요ㅡ??",
          "speaker_id": "2",
          "time": "20210829 17:10"
        },
        {
          "id": "MDRW2100022703.1.3",
          "form": "아뇨... 살쪄서 못입는 옷이 엄서서요.. ㅋㅋㅋ",
          "original_form": "아뇨... 살쪄서 못입는 옷이 엄서서요.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022703.1.4",
          "form": "전 비슥한 색 까먹고 자주사요..",
          "original_form": "전 비슥한 색 까먹고 자주사요..",
          "speaker_id": "1",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100022703.1.5",
          "form": "사고 작년 옷 꺼내보면.. 비슥한게 꼭 잇어요..",
          "original_form": "사고 작년 옷 꺼내보면.. 비슥한게 꼭 잇어요..",
          "speaker_id": "1",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100022703.1.6",
          "form": "아 ㅋㅋ 전 그래서 옷장 한번 보고 쇼핑해요 ㅋㅋ",
          "original_form": "아 ㅋㅋ 전 그래서 옷장 한번 보고 쇼핑해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022703.1.7",
          "form": "무슨색 좋아하세요?",
          "original_form": "무슨색 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 17:14"
        },
        {
          "id": "MDRW2100022703.1.8",
          "form": "베이지, 올리브 계열이요..",
          "original_form": "베이지, 올리브 계열이요..",
          "speaker_id": "1",
          "time": "20210829 17:14"
        },
        {
          "id": "MDRW2100022703.1.9",
          "form": "노란색도 좋고..",
          "original_form": "노란색도 좋고..",
          "speaker_id": "1",
          "time": "20210829 17:14"
        },
        {
          "id": "MDRW2100022703.1.10",
          "form": "오오 ㅎㅎ 전 노란색 안어울려서 잘 안입어요 ㅋㅋㅋ",
          "original_form": "오오 ㅎㅎ 전 노란색 안어울려서 잘 안입어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:15"
        },
        {
          "id": "MDRW2100022703.1.11",
          "form": "그럴땐 내가 못생겨서 그런건 아닌지 거울을 봅니다.. ㅋㅋㅋㅋㅋ",
          "original_form": "그럴땐 내가 못생겨서 그런건 아닌지 거울을 봅니다.. ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100022703.1.12",
          "form": "주로 어떤색 구매하시나여?",
          "original_form": "주로 어떤색 구매하시나여?",
          "speaker_id": "1",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100022703.1.13",
          "form": "ㅋㅋㅋ 전 톤이 안맞아요 ㅎㅎ",
          "original_form": "ㅋㅋㅋ 전 톤이 안맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 17:20"
        },
        {
          "id": "MDRW2100022703.1.14",
          "form": "전 카키 갈색 머스타드요 ㅎㅎ",
          "original_form": "전 카키 갈색 머스타드요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 17:20"
        },
        {
          "id": "MDRW2100022703.1.15",
          "form": "가을웜톤 이새요?",
          "original_form": "가을웜톤 이새요?",
          "speaker_id": "1",
          "time": "20210829 17:21"
        },
        {
          "id": "MDRW2100022703.1.16",
          "form": "오 네네 ㅋㅋㅋ 맞아요 ㅋㅋ",
          "original_form": "오 네네 ㅋㅋㅋ 맞아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022703.1.17",
          "form": "25호?",
          "original_form": "25호?",
          "speaker_id": "1",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022703.1.18",
          "form": "아니요 ㅎㅎ 저 피부톤은 21호에요 ㅋㅋ",
          "original_form": "아니요 ㅎㅎ 저 피부톤은 21호에요 ㅋㅋ",
