      "utterance": [
        {
          "id": "MDRW2100024572.1.1",
          "form": "저녁머드세용",
          "original_form": "저녁머드세용",
          "speaker_id": "2",
          "time": "20210831 18:20"
        },
        {
          "id": "MDRW2100024572.1.2",
          "form": "히히 이미.먹엇어용 감자전이랑 어묵탕이용ㅋㅋㅋ",
          "original_form": "히히 이미.먹엇어용 감자전이랑 어묵탕이용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:06"
        },
        {
          "id": "MDRW2100024572.1.3",
          "form": "name2님도 드셧나요?",
          "original_form": "&name2&님도 드셧나요?",
          "speaker_id": "1",
          "time": "20210831 19:06"
        },
        {
          "id": "MDRW2100024572.1.4",
          "form": "대충 샌드위치 머것어요",
          "original_form": "대충 샌드위치 머것어요",
          "speaker_id": "2",
          "time": "20210831 19:07"
        },
        {
          "id": "MDRW2100024572.1.5",
          "form": "마싯는거만 잘드시네요",
          "original_form": "마싯는거만 잘드시네요",
          "speaker_id": "2",
          "time": "20210831 19:07"
        },
        {
          "id": "MDRW2100024572.1.6",
          "form": "감자전 ㅜ비오는날딱",
          "original_form": "감자전 ㅜ비오는날딱",
          "speaker_id": "2",
          "time": "20210831 19:07"
        },
        {
          "id": "MDRW2100024572.1.7",
          "form": "그쵸 크크 비와서 날씨에 찰떡인 음식으로 먹었어용",
          "original_form": "그쵸 크크 비와서 날씨에 찰떡인 음식으로 먹었어용",
          "speaker_id": "1",
          "time": "20210831 19:08"
        },
        {
          "id": "MDRW2100024572.1.8",
          "form": "무슨샌드위치드셨나용 직접하셧어요?",
          "original_form": "무슨샌드위치드셨나용 직접하셧어요?",
          "speaker_id": "1",
          "time": "20210831 19:08"
        },
        {
          "id": "MDRW2100024572.1.9",
          "form": "그냥 ㅠ 야채안머근지오래됏는데 섭웨가기귀찮아서",
          "original_form": "그냥 ㅠ 야채안머근지오래됏는데 섭웨가기귀찮아서",
          "speaker_id": "2",
          "time": "20210831 19:08"
        },
        {
          "id": "MDRW2100024572.1.10",
          "form": "어디카페서 시켰어요 불고기샌드위치",
          "original_form": "어디카페서 시켰어요 불고기샌드위치",
          "speaker_id": "2",
          "time": "20210831 19:09"
        },
        {
          "id": "MDRW2100024572.1.11",
          "form": "걍 그러네요 ㅎㅎ",
          "original_form": "걍 그러네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 19:09"
        },
        {
          "id": "MDRW2100024572.1.12",
          "form": "오 ㅋㅋㅋ시켰는데 맛없으면 슬퍼요",
          "original_form": "오 ㅋㅋㅋ시켰는데 맛없으면 슬퍼요",
          "speaker_id": "1",
          "time": "20210831 19:10"
        },
        {
          "id": "MDRW2100024572.1.13",
          "form": "저는 요즘 뭔가 섭웨 샌드위치가 안땡겨요 ㅋㅋㅋ",
          "original_form": "저는 요즘 뭔가 섭웨 샌드위치가 안땡겨요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:10"
        },
        {
          "id": "MDRW2100024572.1.14",
          "form": "뭔가 좀더 후레시한 느낌나는 샌드위치가 땡기네용",
          "original_form": "뭔가 좀더 후레시한 느낌나는 샌드위치가 땡기네용",
          "speaker_id": "1",
          "time": "20210831 19:10"
        },
        {
          "id": "MDRW2100024572.1.15",
          "form": "저도... 넘 먹어서 지겹기도하고",
          "original_form": "저도... 넘 먹어서 지겹기도하고",
          "speaker_id": "2",
          "time": "20210831 19:11"
        },
        {
          "id": "MDRW2100024572.1.16",
          "form": "후레시.! ㅠㅜ",
          "original_form": "후레시.! ㅠㅜ",
          "speaker_id": "2",
          "time": "20210831 19:11"
        },
        {
          "id": "MDRW2100024572.1.17",
          "form": "술 좋아하시나요?",
          "original_form": "술 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210831 19:31"
        },
        {
          "id": "MDRW2100024572.1.18",
          "form": "저요.. 네...알쓰긴한데",
          "original_form": "저요.. 네...알쓰긴한데",
          "speaker_id": "2",
          "time": "20210831 19:32"
        },
        {
          "id": "MDRW2100024572.1.19",
          "form": "맛있는술 조아요",
          "original_form": "맛있는술 조아요",
          "speaker_id": "2",
          "time": "20210831 19:32"
        },
        {
          "id": "MDRW2100024572.1.20",
          "form": "오홍 맛있는 술은 어떤거에요?",
          "original_form": "오홍 맛있는 술은 어떤거에요?",
          "speaker_id": "1",
          "time": "20210831 19:33"
        },
        {
          "id": "MDRW2100024572.1.21",
          "form": "맥주!와인!ㅋㅋㅋㅋ 유자술 ! 머이런거요",
          "original_form": "맥주!와인!ㅋㅋㅋㅋ 유자술 ! 머이런거요",
          "speaker_id": "2",
          "time": "20210831 19:33"
        },
        {
          "id": "MDRW2100024572.1.22",
          "form": "ㅋㅋㅋ소주빼고 다 좋아하시는것 같은걸요?",
          "original_form": "ㅋㅋㅋ소주빼고 다 좋아하시는것 같은걸요?",
          "speaker_id": "1",
          "time": "20210831 19:35"
        },
        {
          "id": "MDRW2100024572.1.23",
          "form": "마자요 소주는 넘 강해요 ㅋㅋ",
          "original_form": "마자요 소주는 넘 강해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:40"
        },
        {
          "id": "MDRW2100024572.1.24",
          "form": "아... 시원한 하이볼 먹고싶다",
          "original_form": "아... 시원한 하이볼 먹고싶다",
          "speaker_id": "2",
          "time": "20210831 19:40"
        },
        {
          "id": "MDRW2100024572.1.25",
          "form": "하이볼은 어떤 술 들어가는거에요?",
          "original_form": "하이볼은 어떤 술 들어가는거에요?",
          "speaker_id": "1",
          "time": "20210831 20:02"
        },
        {
          "id": "MDRW2100024572.1.26",
          "form": "저는 술을 잘 못마셔요ㅠ 그나마 맥주가 젤낫고.. 와인은 아주가끔 조금만 ㅠ",
          "original_form": "저는 술을 잘 못마셔요ㅠ 그나마 맥주가 젤낫고.. 와인은 아주가끔 조금만 ㅠ",
          "speaker_id": "1",
          "time": "20210831 20:02"
        }
      ]
    }
