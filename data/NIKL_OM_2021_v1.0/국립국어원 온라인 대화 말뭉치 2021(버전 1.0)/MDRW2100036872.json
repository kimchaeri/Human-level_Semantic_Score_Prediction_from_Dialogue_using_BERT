      "utterance": [
        {
          "id": "MDRW2100036872.1.1",
          "form": "안녕하세요~ 식사하셨나요?",
          "original_form": "안녕하세요~ 식사하셨나요?",
          "speaker_id": "1",
          "time": "20210831 19:06"
        },
        {
          "id": "MDRW2100036872.1.2",
          "form": "안녕하세요 아직 안먹었어요",
          "original_form": "안녕하세요 아직 안먹었어요",
          "speaker_id": "2",
          "time": "20210831 19:06"
        },
        {
          "id": "MDRW2100036872.1.3",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210831 19:06"
        },
        {
          "id": "MDRW2100036872.1.4",
          "form": "네 좀 전에 챙겨 먹었어요!",
          "original_form": "네 좀 전에 챙겨 먹었어요!",
          "speaker_id": "1",
          "time": "20210831 19:07"
        },
        {
          "id": "MDRW2100036872.1.5",
          "form": "어떤거 드셨나요?",
          "original_form": "어떤거 드셨나요?",
          "speaker_id": "2",
          "time": "20210831 19:08"
        },
        {
          "id": "MDRW2100036872.1.6",
          "form": "미역국이랑 있는 반찬 꺼내서 먹었어요",
          "original_form": "미역국이랑 있는 반찬 꺼내서 먹었어요",
          "speaker_id": "1",
          "time": "20210831 19:09"
        },
        {
          "id": "MDRW2100036872.1.7",
          "form": "뭐 드실 예정이신가요?",
          "original_form": "뭐 드실 예정이신가요?",
          "speaker_id": "1",
          "time": "20210831 19:09"
        },
        {
          "id": "MDRW2100036872.1.8",
          "form": "전 쌀국수 먹으려고요",
          "original_form": "전 쌀국수 먹으려고요",
          "speaker_id": "2",
          "time": "20210831 19:09"
        },
        {
          "id": "MDRW2100036872.1.9",
          "form": "날이 쌀쌀해서 국물이 땡기네요",
          "original_form": "날이 쌀쌀해서 국물이 땡기네요",
          "speaker_id": "2",
          "time": "20210831 19:09"
        },
        {
          "id": "MDRW2100036872.1.10",
          "form": "쌀국수 맛있죠! 직접 만들어 드실 건가요?",
          "original_form": "쌀국수 맛있죠! 직접 만들어 드실 건가요?",
          "speaker_id": "1",
          "time": "20210831 19:10"
        },
        {
          "id": "MDRW2100036872.1.11",
          "form": "아뇨 사먹으려구요 ㅎㄹ",
          "original_form": "아뇨 사먹으려구요 ㅎㄹ",
          "speaker_id": "2",
          "time": "20210831 19:11"
        },
        {
          "id": "MDRW2100036872.1.12",
          "form": "미분당 아세요?",
          "original_form": "미분당 아세요?",
          "speaker_id": "2",
          "time": "20210831 19:11"
        },
        {
          "id": "MDRW2100036872.1.13",
          "form": "처음 들어봐요! 맛있나요?!",
          "original_form": "처음 들어봐요! 맛있나요?!",
          "speaker_id": "1",
          "time": "20210831 19:11"
        },
        {
          "id": "MDRW2100036872.1.14",
          "form": "체인점인데 무난해요",
          "original_form": "체인점인데 무난해요",
          "speaker_id": "2",
          "time": "20210831 19:17"
        },
        {
          "id": "MDRW2100036872.1.15",
          "form": "면이랑 숙주가 무한리필이라 좋아요",
          "original_form": "면이랑 숙주가 무한리필이라 좋아요",
          "speaker_id": "2",
          "time": "20210831 19:17"
        },
        {
          "id": "MDRW2100036872.1.16",
          "form": "헉 너무 좋네요! 집 근처에 있는지 찾아봐야겠어요ㅎㅎ",
          "original_form": "헉 너무 좋네요! 집 근처에 있는지 찾아봐야겠어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 19:18"
        },
        {
          "id": "MDRW2100036872.1.17",
          "form": "나시고랭도 좋아하시나요? 저는 가끔 쌀국수 말고 밥 먹고 싶으면 시켜 먹거든요!",
          "original_form": "나시고랭도 좋아하시나요? 저는 가끔 쌀국수 말고 밥 먹고 싶으면 시켜 먹거든요!",
          "speaker_id": "1",
          "time": "20210831 19:19"
        },
        {
          "id": "MDRW2100036872.1.18",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210831 19:33"
        },
        {
          "id": "MDRW2100036872.1.19",
          "form": "파는 곳 찾기 어려워서 자주는 못먹지만요 ㅠ",
          "original_form": "파는 곳 찾기 어려워서 자주는 못먹지만요 ㅠ",
          "speaker_id": "2",
          "time": "20210831 19:33"
        },
        {
          "id": "MDRW2100036872.1.20",
          "form": "맞아요ㅠㅠ 잘 안 파는 거 같아요",
          "original_form": "맞아요ㅠㅠ 잘 안 파는 거 같아요",
          "speaker_id": "1",
          "time": "20210831 19:34"
        }
      ]
