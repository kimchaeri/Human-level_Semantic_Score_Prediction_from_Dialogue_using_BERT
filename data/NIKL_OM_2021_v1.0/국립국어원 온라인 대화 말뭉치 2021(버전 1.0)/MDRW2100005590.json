      "utterance": [
        {
          "id": "MDRW2100005590.1.1",
          "form": "그렇구나! 소개팅 어땠어?",
          "original_form": "그렇구나! 소개팅 어땠어?",
          "speaker_id": "2",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005590.1.2",
          "form": "내가 생각보다 보수적인? 편이라서ㅋㅋㅋ 요즘 소개팅은 세네번이면 거의 고백하고 사귀잖아",
          "original_form": "내가 생각보다 보수적인? 편이라서ㅋㅋㅋ 요즘 소개팅은 세네번이면 거의 고백하고 사귀잖아",
          "speaker_id": "1",
          "time": "20210821 23:13"
        },
        {
          "id": "MDRW2100005590.1.3",
          "form": "나는 그게 나한테 좀 빠르다고 여겨지더라구",
          "original_form": "나는 그게 나한테 좀 빠르다고 여겨지더라구",
          "speaker_id": "1",
          "time": "20210821 23:13"
        },
        {
          "id": "MDRW2100005590.1.4",
          "form": "사람 자체는 괜찮았어",
          "original_form": "사람 자체는 괜찮았어",
          "speaker_id": "1",
          "time": "20210821 23:13"
        },
        {
          "id": "MDRW2100005590.1.5",
          "form": "오 좋다 그런 감성",
          "original_form": "오 좋다 그런 감성",
          "speaker_id": "2",
          "time": "20210821 23:13"
        },
        {
          "id": "MDRW2100005590.1.6",
          "form": "그래서 굳이 연애를 한다면 자만추야!",
          "original_form": "그래서 굳이 연애를 한다면 자만추야!",
          "speaker_id": "1",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005590.1.7",
          "form": "그렇구나 나 요새 연애의 발견 보는데",
          "original_form": "그렇구나 나 요새 연애의 발견 보는데",
          "speaker_id": "2",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005590.1.8",
          "form": "너무 재밌다 흑흑",
          "original_form": "너무 재밌다 흑흑",
          "speaker_id": "2",
          "time": "20210821 23:17"
        },
        {
          "id": "MDRW2100005590.1.9",
          "form": "알아?",
          "original_form": "알아?",
          "speaker_id": "2",
          "time": "20210821 23:17"
        },
        {
          "id": "MDRW2100005590.1.10",
          "form": "드라마?? 나 그거 보다가 중간에 하차했었어..",
          "original_form": "드라마?? 나 그거 보다가 중간에 하차했었어..",
          "speaker_id": "1",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005590.1.11",
          "form": "나 지금도 다시보는데 큰일났어",
          "original_form": "나 지금도 다시보는데 큰일났어",
          "speaker_id": "2",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005590.1.12",
          "form": "여기 묻혀야겠어",
          "original_form": "여기 묻혀야겠어",
          "speaker_id": "2",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005590.1.13",
          "form": "몇화까지 봤어??",
          "original_form": "몇화까지 봤어??",
          "speaker_id": "1",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005590.1.14",
          "form": "나 그냥 클립 계속 다시봐",
          "original_form": "나 그냥 클립 계속 다시봐",
          "speaker_id": "2",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005590.1.15",
          "form": "아ㅋㅋ 요즘 클립으로 다시보는 거 좋아",
          "original_form": "아ㅋㅋ 요즘 클립으로 다시보는 거 좋아",
          "speaker_id": "1",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005590.1.16",
          "form": "그럼 name2는 에릭파야??",
          "original_form": "그럼 &name2&는 에릭파야??",
          "speaker_id": "1",
