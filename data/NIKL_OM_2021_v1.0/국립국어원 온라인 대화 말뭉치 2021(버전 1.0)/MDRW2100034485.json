      "utterance": [
        {
          "id": "MDRW2100034485.1.1",
          "form": "오늘은 날이 맑네요",
          "original_form": "오늘은 날이 맑네요",
          "speaker_id": "1",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100034485.1.2",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100034485.1.3",
          "form": "어느 지역인가요?",
          "original_form": "어느 지역인가요?",
          "speaker_id": "2",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100034485.1.4",
          "form": "저는 경기도요",
          "original_form": "저는 경기도요",
          "speaker_id": "1",
          "time": "20210828 13:38"
        },
        {
          "id": "MDRW2100034485.1.5",
          "form": "가까운 편이네요 여긴 서울인데 맑아요 ㅋㅋ",
          "original_form": "가까운 편이네요 여긴 서울인데 맑아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:38"
        },
        {
          "id": "MDRW2100034485.1.6",
          "form": "아 그렇네요",
          "original_form": "아 그렇네요",
          "speaker_id": "1",
          "time": "20210828 13:39"
        },
        {
          "id": "MDRW2100034485.1.7",
          "form": "근데 나가서 걸으니 좀 덥더라고요",
          "original_form": "근데 나가서 걸으니 좀 덥더라고요",
          "speaker_id": "1",
          "time": "20210828 13:39"
        },
        {
          "id": "MDRW2100034485.1.8",
          "form": "아직 낮이라 그런가봐요",
          "original_form": "아직 낮이라 그런가봐요",
          "speaker_id": "2",
          "time": "20210828 13:39"
        },
        {
          "id": "MDRW2100034485.1.9",
          "form": "어느계절 좋아하세요?",
          "original_form": "어느계절 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 13:39"
        },
        {
          "id": "MDRW2100034485.1.10",
          "form": "저는 봄가을이요",
          "original_form": "저는 봄가을이요",
          "speaker_id": "1",
          "time": "20210828 13:40"
        },
        {
          "id": "MDRW2100034485.1.11",
          "form": "원래는 봄이 좋았는데",
          "original_form": "원래는 봄이 좋았는데",
          "speaker_id": "1",
          "time": "20210828 13:40"
        },
        {
          "id": "MDRW2100034485.1.12",
          "form": "요즘은 가을도 좋네요",
          "original_form": "요즘은 가을도 좋네요",
          "speaker_id": "1",
          "time": "20210828 13:40"
        },
        {
          "id": "MDRW2100034485.1.13",
          "form": "저도 봄가을이 젤 좋더라구요",
          "original_form": "저도 봄가을이 젤 좋더라구요",
          "speaker_id": "2",
          "time": "20210828 13:40"
        },
        {
          "id": "MDRW2100034485.1.14",
          "form": "이유가 있나요??",
          "original_form": "이유가 있나요??",
          "speaker_id": "2",
          "time": "20210828 13:40"
        },
        {
          "id": "MDRW2100034485.1.15",
          "form": "네 걷기에도 좋고",
          "original_form": "네 걷기에도 좋고",
          "speaker_id": "1",
          "time": "20210828 13:40"
        },
        {
          "id": "MDRW2100034485.1.16",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210828 13:40"
        },
        {
          "id": "MDRW2100034485.1.17",
          "form": "그냥 맑고 푸른 하늘이랑 경치가 좋아요",
          "original_form": "그냥 맑고 푸른 하늘이랑 경치가 좋아요",
          "speaker_id": "1",
          "time": "20210828 13:41"
        },
        {
          "id": "MDRW2100034485.1.18",
          "form": "맞아요 유독 더 푸른거같아요",
          "original_form": "맞아요 유독 더 푸른거같아요",
          "speaker_id": "2",
          "time": "20210828 13:41"
        },
        {
          "id": "MDRW2100034485.1.19",
          "form": "네네 하늘이 참 예쁜 계절이예요",
          "original_form": "네네 하늘이 참 예쁜 계절이예요",
          "speaker_id": "1",
          "time": "20210828 13:42"
        }
      ]
