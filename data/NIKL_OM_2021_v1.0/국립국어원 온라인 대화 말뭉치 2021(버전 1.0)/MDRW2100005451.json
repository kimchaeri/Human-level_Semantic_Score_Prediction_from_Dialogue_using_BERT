      "utterance": [
        {
          "id": "MDRW2100005451.1.1",
          "form": "요즘 날씨가 갑자기 비오다가 해가 쨍쨍하다가",
          "original_form": "요즘 날씨가 갑자기 비오다가 해가 쨍쨍하다가",
          "speaker_id": "2",
          "time": "20210821 22:27"
        },
        {
          "id": "MDRW2100005451.1.2",
          "form": "아주 난리야",
          "original_form": "아주 난리야",
          "speaker_id": "2",
          "time": "20210821 22:27"
        },
        {
          "id": "MDRW2100005451.1.3",
          "form": "지구온난화 때문에",
          "original_form": "지구온난화 때문에",
          "speaker_id": "2",
          "time": "20210821 22:28"
        },
        {
          "id": "MDRW2100005451.1.4",
          "form": "그런가",
          "original_form": "그런가",
          "speaker_id": "2",
          "time": "20210821 22:28"
        },
        {
          "id": "MDRW2100005451.1.5",
          "form": "지구가 망해가고 있는듯 ㅠ",
          "original_form": "지구가 망해가고 있는듯 ㅠ",
          "speaker_id": "1",
          "time": "20210821 22:28"
        },
        {
          "id": "MDRW2100005451.1.6",
          "form": "바람도 엄청 불더라",
          "original_form": "바람도 엄청 불더라",
          "speaker_id": "1",
          "time": "20210821 22:28"
        },
        {
          "id": "MDRW2100005451.1.7",
          "form": "마져 거의 태풍느낌",
          "original_form": "마져 거의 태풍느낌",
          "speaker_id": "2",
          "time": "20210821 22:29"
        },
        {
          "id": "MDRW2100005451.1.8",
          "form": "내일 날씨거 비슷하랴나",
          "original_form": "내일 날씨거 비슷하랴나",
          "speaker_id": "2",
          "time": "20210821 22:29"
        },
        {
          "id": "MDRW2100005451.1.9",
          "form": "흠.. 예측하기가 너무 어려워",
          "original_form": "흠.. 예측하기가 너무 어려워",
          "speaker_id": "1",
          "time": "20210821 22:30"
        },
        {
          "id": "MDRW2100005451.1.10",
          "form": "그냥 우산을 들고 다니던가 해야지",
          "original_form": "그냥 우산을 들고 다니던가 해야지",
          "speaker_id": "1",
          "time": "20210821 22:30"
        },
        {
          "id": "MDRW2100005451.1.11",
          "form": "양우산이 진짜 최고다",
          "original_form": "양우산이 진짜 최고다",
          "speaker_id": "2",
          "time": "20210821 22:31"
        },
        {
          "id": "MDRW2100005451.1.12",
          "form": "비고막고 햇빛도 막고",
          "original_form": "비고막고 햇빛도 막고",
          "speaker_id": "2",
          "time": "20210821 22:31"
        },
        {
          "id": "MDRW2100005451.1.13",
          "form": "맞아맞아 이젠 생존품이지",
          "original_form": "맞아맞아 이젠 생존품이지",
          "speaker_id": "1",
          "time": "20210821 22:32"
        },
        {
          "id": "MDRW2100005451.1.14",
          "form": "양산 쓰면 정수리가 서늘해",
          "original_form": "양산 쓰면 정수리가 서늘해",
          "speaker_id": "1",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005451.1.15",
          "form": "서늘ㅋㅋㅋㅋㅋㅋㅋㅋ맞어 머리가 시원해",
          "original_form": "서늘ㅋㅋㅋㅋㅋㅋㅋㅋ맞어 머리가 시원해",
          "speaker_id": "2",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005451.1.16",
          "form": "그나저나 벌서 가을이 왓다",
          "original_form": "그나저나 벌서 가을이 왓다",
          "speaker_id": "2",
          "time": "20210821 22:43"
        },
        {
          "id": "MDRW2100005451.1.17",
          "form": "그러니까 우리집 에어컨 안 튼지 일주일 넘었어",
          "original_form": "그러니까 우리집 에어컨 안 튼지 일주일 넘었어",
          "speaker_id": "1",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005451.1.18",
          "form": "나도 이제ㅡ이불을 덮고 잔다",
          "original_form": "나도 이제ㅡ이불을 덮고 잔다",
          "speaker_id": "2",
          "time": "20210821 22:56"
        },
        {
          "id": "MDRW2100005451.1.19",
          "form": "너도 드디어 이불을 덮고 자는 구만 ㅋㅋㅋㅋㅋ",
          "original_form": "너도 드디어 이불을 덮고 자는 구만 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005451.1.20",
          "form": "새벽엔 추웡",
          "original_form": "새벽엔 추웡",
          "speaker_id": "1",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005451.1.21",
          "form": "마졐ㅋㅋㅋㅋㅋㅋ쌀쌀해서 이불꼭덮어야래",
          "original_form": "마졐ㅋㅋㅋㅋㅋㅋ쌀쌀해서 이불꼭덮어야래",
          "speaker_id": "2",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005451.1.22",
          "form": "그렇게 더웠는데 벌써 살만해졌다는 게 믿기지가 않는다",
          "original_form": "그렇게 더웠는데 벌써 살만해졌다는 게 믿기지가 않는다",
          "speaker_id": "1",
          "time": "20210821 23:01"
        },
        {
          "id": "MDRW2100005451.1.23",
          "form": "신기한 한국의 날씨야",
          "original_form": "신기한 한국의 날씨야",
          "speaker_id": "2",
          "time": "20210821 23:02"
        },
        {
          "id": "MDRW2100005451.1.24",
          "form": "말복되면 시원해져",
          "original_form": "말복되면 시원해져",
          "speaker_id": "2",
          "time": "20210821 23:02"
        }
      ]
    }
  ]
