      "utterance": [
        {
          "id": "MDRW2100047370.1.1",
          "form": "더위 많이 타시나요?",
          "original_form": "더위 많이 타시나요?",
          "speaker_id": "2",
          "time": "20210826 16:53"
        },
        {
          "id": "MDRW2100047370.1.2",
          "form": "네 추위 더위 다 많이 타요",
          "original_form": "네 추위 더위 다 많이 타요",
          "speaker_id": "1",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100047370.1.3",
          "form": "올여름 진짜 힘드셨겠네요",
          "original_form": "올여름 진짜 힘드셨겠네요",
          "speaker_id": "2",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100047370.1.4",
          "form": "넘 심했잖아요 ㅋㅋㅋ",
          "original_form": "넘 심했잖아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100047370.1.5",
          "form": "아 안나가서 다행히 살아 남았러요",
          "original_form": "아 안나가서 다행히 살아 남았러요",
          "speaker_id": "1",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100047370.1.6",
          "form": "나갈땐 넘 더워서 숨막히고 ㅜㅜ마스크가 사람잡아요",
          "original_form": "나갈땐 넘 더워서 숨막히고 ㅜㅜ마스크가 사람잡아요",
          "speaker_id": "1",
          "time": "20210826 16:58"
        },
        {
          "id": "MDRW2100047370.1.7",
          "form": "아 정말요.. 저도 숨을 막 크게 들이쉬는 스타일이라 ㅋㅋ",
          "original_form": "아 정말요.. 저도 숨을 막 크게 들이쉬는 스타일이라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:58"
        },
        {
          "id": "MDRW2100047370.1.8",
          "form": "올여름 너무 힘들더라고요",
          "original_form": "올여름 너무 힘들더라고요",
          "speaker_id": "2",
          "time": "20210826 16:58"
        },
        {
          "id": "MDRW2100047370.1.9",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100047370.1.10",
          "form": "여름에 더운데 숨도 맘대로 못쉬니",
          "original_form": "여름에 더운데 숨도 맘대로 못쉬니",
          "speaker_id": "1",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100047370.1.11",
          "form": "여름은 힘들어요",
          "original_form": "여름은 힘들어요",
          "speaker_id": "1",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100047370.1.12",
          "form": "겨울은 어떠세요",
          "original_form": "겨울은 어떠세요",
          "speaker_id": "1",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100047370.1.13",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100047370.1.14",
          "form": "추위타시나요?",
          "original_form": "추위타시나요?",
          "speaker_id": "1",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100047370.1.15",
          "form": "저도 추위 많이 타서",
          "original_form": "저도 추위 많이 타서",
          "speaker_id": "2",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100047370.1.16",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100047370.1.17",
          "form": "롱패딩 없이 못 살아요",
          "original_form": "롱패딩 없이 못 살아요",
          "speaker_id": "2",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100047370.1.18",
          "form": "난방비 많이 나오고",
          "original_form": "난방비 많이 나오고",
          "speaker_id": "1",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100047370.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 17:00"
        },
        {
          "id": "MDRW2100047370.1.20",
          "form": "숏패딩 못입어요 ㅋㅋ",
          "original_form": "숏패딩 못입어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:00"
        },
        {
          "id": "MDRW2100047370.1.21",
          "form": "패딩은 롱이죠",
          "original_form": "패딩은 롱이죠",
          "speaker_id": "1",
          "time": "20210826 17:00"
        },
        {
          "id": "MDRW2100047370.1.22",
          "form": "그쵸 ㅋㅋㅋ 궁디 시려워서..",
          "original_form": "그쵸 ㅋㅋㅋ 궁디 시려워서..",
          "speaker_id": "2",
          "time": "20210826 17:00"
        },
        {
          "id": "MDRW2100047370.1.23",
          "form": "올해 하나 장만 하고 싶네요 ㅋㅋ",
          "original_form": "올해 하나 장만 하고 싶네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:00"
        },
        {
          "id": "MDRW2100047370.1.24",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 17:00"
        },
        {
          "id": "MDRW2100047370.1.25",
          "form": "색깔 다른걸로 패딩 사고파요 ㅋㅋ",
          "original_form": "색깔 다른걸로 패딩 사고파요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:00"
        },
        {
          "id": "MDRW2100047370.1.26",
          "form": "추석 전에 사야된대여 롱패딩은",
          "original_form": "추석 전에 사야된대여 롱패딩은",
          "speaker_id": "2",
          "time": "20210826 17:00"
        },
        {
          "id": "MDRW2100047370.1.27",
          "form": "추석지나고 비싸진다네요",
          "original_form": "추석지나고 비싸진다네요",
          "speaker_id": "2",
          "time": "20210826 17:00"
        },
        {
          "id": "MDRW2100047370.1.28",
          "form": "어머 그래요?",
          "original_form": "어머 그래요?",
          "speaker_id": "1",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100047370.1.29",
          "form": "할인 봐야겠네요",
          "original_form": "할인 봐야겠네요",
          "speaker_id": "1",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100047370.1.30",
          "form": "어차피 딸도 사야해요",
          "original_form": "어차피 딸도 사야해요",
          "speaker_id": "1",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100047370.1.31",
          "form": "중학교 가니",
          "original_form": "중학교 가니",
          "speaker_id": "1",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100047370.1.32",
          "form": "겨울 만반의 준비 해야죠",
          "original_form": "겨울 만반의 준비 해야죠",
          "speaker_id": "1",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100047370.1.33",
          "form": "아 교복위에 롱패딩 필수죠..",
          "original_form": "아 교복위에 롱패딩 필수죠..",
          "speaker_id": "2",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100047370.1.34",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100047370.1.35",
          "form": "좋은거 사줘야 할텐데",
          "original_form": "좋은거 사줘야 할텐데",
          "speaker_id": "1",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100047370.1.36",
          "form": "참 고민이네요 ㅋㅋ",
          "original_form": "참 고민이네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100047370.1.37",
          "form": "아 애기들은 또 브랜드 보잖아요 그쵸 ㅎㅎ",
          "original_form": "아 애기들은 또 브랜드 보잖아요 그쵸 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100047370.1.38",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100047370.1.39",
          "form": "참나 조사 들어가야 겠어요",
          "original_form": "참나 조사 들어가야 겠어요",
          "speaker_id": "1",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100047370.1.40",
          "form": "패딩 중요하죠 메이커",
          "original_form": "패딩 중요하죠 메이커",
          "speaker_id": "1",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100047370.1.41",
          "form": "ㅋㅋㅋ 내가 입을거는 암거나 가성비 좋은거면 되는데 말이에요",
          "original_form": "ㅋㅋㅋ 내가 입을거는 암거나 가성비 좋은거면 되는데 말이에요",
          "speaker_id": "2",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100047370.1.42",
          "form": "구러니까요",
          "original_form": "구러니까요",
          "speaker_id": "1",
          "time": "20210826 17:04"
        }
      ]
    }
