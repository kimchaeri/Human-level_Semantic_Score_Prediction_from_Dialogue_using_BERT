      "utterance": [
        {
          "id": "MDRW2100007490.1.1",
          "form": "나 궁금한게있어",
          "original_form": "나 궁금한게있어",
          "speaker_id": "1",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007490.1.2",
          "form": "뭔데",
          "original_form": "뭔데",
          "speaker_id": "2",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007490.1.3",
          "form": "왜.. 여신님이 보고계셔도 다시 온라인재공연을 했는데",
          "original_form": "왜.. 여신님이 보고계셔도 다시 온라인재공연을 했는데",
          "speaker_id": "1",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007490.1.4",
          "form": "마리퀴리는안하는걸까",
          "original_form": "마리퀴리는안하는걸까",
          "speaker_id": "1",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007490.1.5",
          "form": "라이브 뮤지컬 제작사가 일을 안 하는거라고 볼 수 있지",
          "original_form": "라이브 뮤지컬 제작사가 일을 안 하는거라고 볼 수 있지",
          "speaker_id": "2",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007490.1.6",
          "form": "왜 일을 안 할까?",
          "original_form": "왜 일을 안 할까?",
          "speaker_id": "2",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007490.1.7",
          "form": "라이브 뮤지컬 제작사는 왜 뮤지컬 마리퀴리 ost도 내주질 않는걸까",
          "original_form": "라이브 뮤지컬 제작사는 왜 뮤지컬 마리퀴리 ost도 내주질 않는걸까",
          "speaker_id": "1",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007490.1.8",
          "form": "왜 일을 안 할까?",
          "original_form": "왜 일을 안 할까?",
          "speaker_id": "1",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007490.1.9",
          "form": "뮤지컬 마리퀴리 DVD를 내줄 수는 없을까?",
          "original_form": "뮤지컬 마리퀴리 DVD를 내줄 수는 없을까?",
          "speaker_id": "2",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007490.1.10",
          "form": "라이브에 대한 의문이 참 많아",
          "original_form": "라이브에 대한 의문이 참 많아",
          "speaker_id": "2",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007490.1.11",
          "form": "우리가 너무 큰걸 바라는건 아닐까?",
          "original_form": "우리가 너무 큰걸 바라는건 아닐까?",
          "speaker_id": "1",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007490.1.12",
          "form": "우리가 한 일들이 헛짓이었을까",
          "original_form": "우리가 한 일들이 헛짓이었을까",
          "speaker_id": "1",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007490.1.13",
          "form": "하지만 같은 제작사인 팬레터는 내줬잖아",
          "original_form": "하지만 같은 제작사인 팬레터는 내줬잖아",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007490.1.14",
          "form": "우리가 틀렸던걸까",
          "original_form": "우리가 틀렸던걸까",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007490.1.15",
          "form": "어딘가에 우릴 원하는 제작사가 있으리라",
          "original_form": "어딘가에 우릴 원하는 제작사가 있으리라",
          "speaker_id": "1",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007490.1.16",
          "form": "그런 제작사 없더라",
          "original_form": "그런 제작사 없더라",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007490.1.17",
          "form": "name2 나 방금 비속어쓸뻔했잖니",
          "original_form": "&name2& 나 방금 비속어쓸뻔했잖니",
          "speaker_id": "1",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007490.1.18",
          "form": "뮤지컬 연극 올리는 제작사들은 다 관객들에게 비참함만을 안겨줘",
          "original_form": "뮤지컬 연극 올리는 제작사들은 다 관객들에게 비참함만을 안겨줘",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007490.1.19",
          "form": "name2 너는 뮤지컬 마리퀴리가 만~약 온라인 재등판한다면",
          "original_form": "&name2& 너는 뮤지컬 마리퀴리가 만~약 온라인 재등판한다면",
          "speaker_id": "1",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007490.1.20",
          "form": "어떤 캐스트를 보고싶니?",
          "original_form": "어떤 캐스트를 보고싶니?",
          "speaker_id": "1",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007490.1.21",
          "form": "너무 고민돼",
          "original_form": "너무 고민돼",
          "speaker_id": "2",
          "time": "20210822 02:34"
        },
        {
          "id": "MDRW2100007490.1.22",
          "form": "옥마리 별피에르 양루벤까지는 막힘이 없었는데",
          "original_form": "옥마리 별피에르 양루벤까지는 막힘이 없었는데",
          "speaker_id": "2",
          "time": "20210822 02:35"
        },
        {
          "id": "MDRW2100007490.1.23",
          "form": "안느가 너무 고민이야",
          "original_form": "안느가 너무 고민이야",
          "speaker_id": "2",
          "time": "20210822 02:35"
        },
        {
          "id": "MDRW2100007490.1.24",
          "form": "나는",
          "original_form": "나는",
          "speaker_id": "1",
          "time": "20210822 02:35"
        },
        {
          "id": "MDRW2100007490.1.25",
          "form": "극강 김히어라야.",
          "original_form": "극강 김히어라야.",
          "speaker_id": "1",
          "time": "20210822 02:35"
        },
        {
          "id": "MDRW2100007490.1.26",
          "form": "이유가 있어?",
          "original_form": "이유가 있어?",
          "speaker_id": "2",
          "time": "20210822 02:35"
        },
        {
          "id": "MDRW2100007490.1.27",
          "form": "이유가..있어?",
          "original_form": "이유가..있어?",
          "speaker_id": "1",
          "time": "20210822 02:35"
        },
        {
          "id": "MDRW2100007490.1.28",
          "form": "난 각 캐스트 별로 다 이유가 있는데",
          "original_form": "난 각 캐스트 별로 다 이유가 있는데",
          "speaker_id": "2",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007490.1.29",
          "form": "나는 없어.. 그냥 뇌에 제우스가 번개를 따악 쏴줘",
          "original_form": "나는 없어.. 그냥 뇌에 제우스가 번개를 따악 쏴줘",
          "speaker_id": "1",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007490.1.30",
          "form": "너 신내림 받니?",
          "original_form": "너 신내림 받니?",
          "speaker_id": "2",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007490.1.31",
          "form": "응 여신님이라고",
          "original_form": "응 여신님이라고",
          "speaker_id": "1",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007490.1.32",
          "form": "계셔",
          "original_form": "계셔",
          "speaker_id": "1",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007490.1.33",
          "form": "여신님 혹시 나도 보고 계실까?",
          "original_form": "여신님 혹시 나도 보고 계실까?",
          "speaker_id": "2",
          "time": "20210822 02:37"
        },
        {
          "id": "MDRW2100007490.1.34",
          "form": "나 최연우 여신님 보고 싶었어",
          "original_form": "나 최연우 여신님 보고 싶었어",
          "speaker_id": "2",
          "time": "20210822 02:37"
        },
        {
          "id": "MDRW2100007490.1.35",
          "form": "물론이지 뮤지컬 여신님이 보고계셔의 여신님은",
          "original_form": "물론이지 뮤지컬 여신님이 보고계셔의 여신님은",
          "speaker_id": "1",
          "time": "20210822 02:37"
        },
        {
          "id": "MDRW2100007490.1.36",
          "form": "몸이아파 잠들지 못하는 밤 작은 숨 소리 마저 아려와 그림자 뒤로 숨고만 싶은 밤에도 우리를 굽어살펴주셔",
          "original_form": "몸이아파 잠들지 못하는 밤 작은 숨 소리 마저 아려와 그림자 뒤로 숨고만 싶은 밤에도 우리를 굽어살펴주셔",
          "speaker_id": "1",
          "time": "20210822 02:37"
        },
        {
          "id": "MDRW2100007490.1.37",
          "form": "연우여신갓갓갓",
          "original_form": "연우여신갓갓갓",
