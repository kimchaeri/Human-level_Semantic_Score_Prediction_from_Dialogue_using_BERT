      "utterance": [
        {
          "id": "MDRW2100036818.1.1",
          "form": "제 친구들도ㅠ",
          "original_form": "제 친구들도ㅠ",
          "speaker_id": "2",
          "time": "20210831 12:57"
        },
        {
          "id": "MDRW2100036818.1.2",
          "form": "링피트 때문에 닌텐도 많이 사더라구요!",
          "original_form": "링피트 때문에 닌텐도 많이 사더라구요!",
          "speaker_id": "2",
          "time": "20210831 12:57"
        },
        {
          "id": "MDRW2100036818.1.3",
          "form": "코로나라 더 많이들 사시는거 같아요",
          "original_form": "코로나라 더 많이들 사시는거 같아요",
          "speaker_id": "1",
          "time": "20210831 13:00"
        },
        {
          "id": "MDRW2100036818.1.4",
          "form": "게임 좋아하시나요?",
          "original_form": "게임 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210831 13:00"
        },
        {
          "id": "MDRW2100036818.1.5",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210831 13:04"
        },
        {
          "id": "MDRW2100036818.1.6",
          "form": "좋아하기는 하는데",
          "original_form": "좋아하기는 하는데",
          "speaker_id": "2",
          "time": "20210831 13:04"
        },
        {
          "id": "MDRW2100036818.1.7",
          "form": "그렇게 자주는 못하는 것 같아요",
          "original_form": "그렇게 자주는 못하는 것 같아요",
          "speaker_id": "2",
          "time": "20210831 13:04"
        },
        {
          "id": "MDRW2100036818.1.8",
          "form": "저 예전에는 쿠키런 정말 좋아했어요 ㅎㅎㅎ",
          "original_form": "저 예전에는 쿠키런 정말 좋아했어요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 13:04"
        },
        {
          "id": "MDRW2100036818.1.9",
          "form": "쿠키런! 그럼 이번 쿠키런 킹덤도 하셨나요?",
          "original_form": "쿠키런! 그럼 이번 쿠키런 킹덤도 하셨나요?",
          "speaker_id": "1",
          "time": "20210831 13:28"
        },
        {
          "id": "MDRW2100036818.1.10",
          "form": "아 그것도 해보고 싶었는데",
          "original_form": "아 그것도 해보고 싶었는데",
          "speaker_id": "2",
          "time": "20210831 13:43"
        },
        {
          "id": "MDRW2100036818.1.11",
          "form": "너무 낯선 쿠키들이 많아서",
          "original_form": "너무 낯선 쿠키들이 많아서",
          "speaker_id": "2",
          "time": "20210831 13:44"
        },
        {
          "id": "MDRW2100036818.1.12",
          "form": "쿠키들이랑 낯가려서 못하겠어요",
          "original_form": "쿠키들이랑 낯가려서 못하겠어요",
          "speaker_id": "2",
          "time": "20210831 13:44"
        },
        {
          "id": "MDRW2100036818.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ이상하죠 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ이상하죠 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:44"
        },
        {
          "id": "MDRW2100036818.1.14",
          "form": "앜ㅋㅋㄲㅋㄲ이미 예전친구들이 너무 익숙하군요ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㄲㅋㄲ이미 예전친구들이 너무 익숙하군요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:53"
        },
        {
          "id": "MDRW2100036818.1.15",
          "form": "어떤쿠키 자주 쓰셨나요?",
          "original_form": "어떤쿠키 자주 쓰셨나요?",
          "speaker_id": "1",
          "time": "20210831 13:53"
        },
        {
          "id": "MDRW2100036818.1.16",
          "form": "맞아요....!!!!",
          "original_form": "맞아요....!!!!",
          "speaker_id": "2",
          "time": "20210831 14:00"
        },
        {
          "id": "MDRW2100036818.1.17",
          "form": "저는 항상",
          "original_form": "저는 항상",
          "speaker_id": "2",
          "time": "20210831 14:00"
        },
        {
          "id": "MDRW2100036818.1.18",
          "form": "천사쿠키한테서 받은 자석 보물이랑",
          "original_form": "천사쿠키한테서 받은 자석 보물이랑",
          "speaker_id": "2",
          "time": "20210831 14:00"
        },
        {
          "id": "MDRW2100036818.1.19",
          "form": "주로 달리던 쿠키는.... 누구더라...",
          "original_form": "주로 달리던 쿠키는.... 누구더라...",
          "speaker_id": "2",
          "time": "20210831 14:00"
        },
        {
          "id": "MDRW2100036818.1.20",
          "form": "마법사맛 쿠키?!",
          "original_form": "마법사맛 쿠키?!",
          "speaker_id": "2",
          "time": "20210831 14:00"
        },
        {
          "id": "MDRW2100036818.1.21",
          "form": "기본쿠키 잘쓰셨네요",
          "original_form": "기본쿠키 잘쓰셨네요",
          "speaker_id": "1",
          "time": "20210831 14:48"
        },
        {
          "id": "MDRW2100036818.1.22",
          "form": "저는 소다맛인가? 서핑보드타던친구 자주썼어요",
          "original_form": "저는 소다맛인가? 서핑보드타던친구 자주썼어요",
          "speaker_id": "1",
          "time": "20210831 14:48"
        },
        {
          "id": "MDRW2100036818.1.23",
          "form": "헉 마법사맛 쿠키가 기본쿠키예요?",
          "original_form": "헉 마법사맛 쿠키가 기본쿠키예요?",
          "speaker_id": "2",
          "time": "20210831 15:36"
        },
        {
          "id": "MDRW2100036818.1.24",
          "form": "저 때만 해도",
          "original_form": "저 때만 해도",
          "speaker_id": "2",
          "time": "20210831 15:36"
        },
        {
          "id": "MDRW2100036818.1.25",
          "form": "되게 fancy한 쿠키였는데...",
          "original_form": "되게 fancy한 쿠키였는데...{emoji:☆}",
          "speaker_id": "2",
          "time": "20210831 15:36"
        },
        {
          "id": "MDRW2100036818.1.26",
          "form": "저 할 때는 소다맛이 없었어요!!!",
          "original_form": "저 할 때는 소다맛이 없었어요!!!",
          "speaker_id": "2",
          "time": "20210831 15:36"
        },
        {
          "id": "MDRW2100036818.1.27",
          "form": "쿠키런 이렇게 보니 진짜 오래된 게임이네요 ㅋㅋㅋㅋㅋ",
          "original_form": "쿠키런 이렇게 보니 진짜 오래된 게임이네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:36"
        },
        {
          "id": "MDRW2100036818.1.28",
          "form": "앗 킹덤하고헷갈렸나봐요",
          "original_form": "앗 킹덤하고헷갈렸나봐요",
          "speaker_id": "1",
          "time": "20210831 16:41"
        },
        {
          "id": "MDRW2100036818.1.29",
          "form": "킹덤은 기본쿠키였어요!",
          "original_form": "킹덤은 기본쿠키였어요!",
          "speaker_id": "1",
          "time": "20210831 16:41"
        },
        {
          "id": "MDRW2100036818.1.30",
          "form": "아!!! 그게 또 다르군요 ㅎㅎ",
          "original_form": "아!!! 그게 또 다르군요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100036818.1.31",
          "form": "그래도 마법사 쿠키 정도면 이제",
          "original_form": "그래도 마법사 쿠키 정도면 이제",
          "speaker_id": "2",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100036818.1.32",
          "form": "연차가 꽤 됐을거예요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "연차가 꽤 됐을거예요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100036818.1.33",
          "form": "무한히 확장하는 쿠키런 쿠키들...",
          "original_form": "무한히 확장하는 쿠키런 쿠키들...{emoji:☆}",
          "speaker_id": "2",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100036818.1.34",
          "form": "그죠ㅋㅋㅋ진짜 오만쿠키가 다있던데요",
          "original_form": "그죠ㅋㅋㅋ진짜 오만쿠키가 다있던데요",
          "speaker_id": "1",
          "time": "20210831 16:45"
        },
        {
          "id": "MDRW2100036818.1.35",
          "form": "초반엔 좀비맛이 있으면 그렇게 사기였는데",
          "original_form": "초반엔 좀비맛이 있으면 그렇게 사기였는데",
          "speaker_id": "1",
          "time": "20210831 16:45"
        },
        {
          "id": "MDRW2100036818.1.36",
          "form": "이젠 좀비맛도 안쓴대요",
          "original_form": "이젠 좀비맛도 안쓴대요",
          "speaker_id": "1",
          "time": "20210831 16:45"
        },
        {
          "id": "MDRW2100036818.1.37",
          "form": "아 좀비맛도 이제 대중에게서 버림받은건가요?",
          "original_form": "아 좀비맛도 이제 대중에게서 버림받은건가요?",
          "speaker_id": "2",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100036818.1.38",
          "form": "너무 기본이다 이건가,,,",
          "original_form": "너무 기본이다 이건가,,,",
          "speaker_id": "2",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100036818.1.39",
          "form": "하긴 천사맛 쿠키도",
          "original_form": "하긴 천사맛 쿠키도",
          "speaker_id": "2",
          "time": "20210831 16:49"
        },
        {
          "id": "MDRW2100036818.1.40",
          "form": "자석 보물만 얻으면",
          "original_form": "자석 보물만 얻으면",
          "speaker_id": "2",
          "time": "20210831 16:49"
        },
        {
          "id": "MDRW2100036818.1.41",
          "form": "쿠키는 안쓰게 되더라구요ㅠ 생긴건 귀여운대",
          "original_form": "쿠키는 안쓰게 되더라구요ㅠ 생긴건 귀여운대",
          "speaker_id": "2",
          "time": "20210831 16:49"
        },
        {
          "id": "MDRW2100036818.1.42",
          "form": "데",
          "original_form": "데",
          "speaker_id": "2",
          "time": "20210831 16:49"
        },
        {
          "id": "MDRW2100036818.1.43",
          "form": "저는 그친구 조금 무섭던데ㅋㅋㅋㄲ",
          "original_form": "저는 그친구 조금 무섭던데ㅋㅋㅋㄲ",
