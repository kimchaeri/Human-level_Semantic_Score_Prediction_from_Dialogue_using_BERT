      "utterance": [
        {
          "id": "MDRW2100016793.1.1",
          "form": "최근 사고싶었던 물건 있으신가요?",
          "original_form": "최근 사고싶었던 물건 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 15:27"
        },
        {
          "id": "MDRW2100016793.1.2",
          "form": "저는 최근 삼성제트플립에 눈이 돌아있어요..",
          "original_form": "저는 최근 삼성제트플립에 눈이 돌아있어요..",
          "speaker_id": "1",
          "time": "20210826 15:31"
        },
        {
          "id": "MDRW2100016793.1.3",
          "form": "저도 갖고싶어요..",
          "original_form": "저도 갖고싶어요..",
          "speaker_id": "2",
          "time": "20210826 15:43"
        },
        {
          "id": "MDRW2100016793.1.4",
          "form": "제트플립 탐나요",
          "original_form": "제트플립 탐나요",
          "speaker_id": "2",
          "time": "20210826 15:43"
        },
        {
          "id": "MDRW2100016793.1.5",
          "form": "맞아요 ㅜ 실물보고 뒤집어졌다니깐요~",
          "original_form": "맞아요 ㅜ 실물보고 뒤집어졌다니깐요~",
          "speaker_id": "1",
          "time": "20210826 15:46"
        },
        {
          "id": "MDRW2100016793.1.6",
          "form": "폰 안바꾼지 꽤 돼서 그런가",
          "original_form": "폰 안바꾼지 꽤 돼서 그런가",
          "speaker_id": "2",
          "time": "20210826 15:48"
        },
        {
          "id": "MDRW2100016793.1.7",
          "form": "탐나는ㅠ",
          "original_form": "탐나는ㅠ",
          "speaker_id": "2",
          "time": "20210826 15:48"
        },
        {
          "id": "MDRW2100016793.1.8",
          "form": "그쵸 ㅜ 라벤더가 넘예쁘더라구요",
          "original_form": "그쵸 ㅜ 라벤더가 넘예쁘더라구요",
          "speaker_id": "1",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100016793.1.9",
          "form": "색감 엄청 예뻐서 저도 만약 산다면 라벤더 사고싶었어요",
          "original_form": "색감 엄청 예뻐서 저도 만약 산다면 라벤더 사고싶었어요",
          "speaker_id": "2",
          "time": "20210826 15:50"
        },
        {
          "id": "MDRW2100016793.1.10",
          "form": "맞아요~ 근데실물보면 생각지도않던 다크그린이 갑자기 훅한다니깐요!",
          "original_form": "맞아요~ 근데실물보면 생각지도않던 다크그린이 갑자기 훅한다니깐요!",
          "speaker_id": "1",
          "time": "20210826 15:52"
        },
        {
          "id": "MDRW2100016793.1.11",
          "form": "다크그린도 실물깡패인가요?",
          "original_form": "다크그린도 실물깡패인가요?",
          "speaker_id": "2",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016793.1.12",
          "form": "견물생심이라고.. 보다보면 다 탐나는ㅠ",
          "original_form": "견물생심이라고.. 보다보면 다 탐나는ㅠ",
          "speaker_id": "2",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016793.1.13",
          "form": "맞아요!! 다크그린 엄청고급지고예뻐요",
          "original_form": "맞아요!! 다크그린 엄청고급지고예뻐요",
          "speaker_id": "1",
          "time": "20210826 15:59"
        },
        {
          "id": "MDRW2100016793.1.14",
          "form": "이번에 색깔 되게 잘 뽑았네요ㅠㅠ",
          "original_form": "이번에 색깔 되게 잘 뽑았네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 16:01"
        },
