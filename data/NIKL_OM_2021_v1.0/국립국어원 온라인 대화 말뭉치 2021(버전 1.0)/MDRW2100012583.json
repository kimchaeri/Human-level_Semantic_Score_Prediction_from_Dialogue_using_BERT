      "utterance": [
        {
          "id": "MDRW2100012583.1.1",
          "form": "평소에 집안일 하세요?",
          "original_form": "평소에 집안일 하세요?",
          "speaker_id": "1",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100012583.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100012583.1.3",
          "form": "제가 초3때 완전 혼자 사는법을 터득했거든요",
          "original_form": "제가 초3때 완전 혼자 사는법을 터득했거든요",
          "speaker_id": "2",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100012583.1.4",
          "form": "전 외동입니다..",
          "original_form": "전 외동입니다..",
          "speaker_id": "2",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100012583.1.5",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100012583.1.6",
          "form": "능력자시네요",
          "original_form": "능력자시네요",
          "speaker_id": "1",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100012583.1.7",
          "form": "전 동생 둘 있어요",
          "original_form": "전 동생 둘 있어요",
          "speaker_id": "1",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100012583.1.8",
          "form": "휴",
          "original_form": "휴",
          "speaker_id": "1",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100012583.1.9",
          "form": "초3때 어머니 마저 가정형편 책임 지시겠다고 일을 나가셔서",
          "original_form": "초3때 어머니 마저 가정형편 책임 지시겠다고 일을 나가셔서",
          "speaker_id": "2",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100012583.1.10",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100012583.1.11",
          "form": "저 혼자 집에 머물러 있었죠",
          "original_form": "저 혼자 집에 머물러 있었죠",
          "speaker_id": "2",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100012583.1.12",
          "form": "그때부터 혼자 모든 것을 할 줄 아는",
          "original_form": "그때부터 혼자 모든 것을 할 줄 아는",
          "speaker_id": "1",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100012583.1.13",
          "form": "학원 갔다와서 집에서 밥하고 빨래하고",
          "original_form": "학원 갔다와서 집에서 밥하고 빨래하고",
          "speaker_id": "2",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100012583.1.14",
          "form": "만능 캐릭터가 되셨네요",
          "original_form": "만능 캐릭터가 되셨네요",
          "speaker_id": "1",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100012583.1.15",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "2",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100012583.1.16",
          "form": "과언이십니다",
          "original_form": "과언이십니다",
          "speaker_id": "2",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100012583.1.17",
          "form": "초3이면 진짜 어린데",
          "original_form": "초3이면 진짜 어린데",
          "speaker_id": "1",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100012583.1.18",
          "form": "대단해요",
          "original_form": "대단해요",
          "speaker_id": "1",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100012583.1.19",
          "form": "주로 뭐 해먹어요?",
          "original_form": "주로 뭐 해먹어요?",
          "speaker_id": "1",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100012583.1.20",
          "form": "메뉴 추천 좀 해주세요",
          "original_form": "메뉴 추천 좀 해주세요",
          "speaker_id": "1",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100012583.1.21",
          "form": "자취생에게 절실한 팁",
          "original_form": "자취생에게 절실한 팁",
          "speaker_id": "1",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100012583.1.22",
          "form": "바질페스토 파스타 아세요?",
          "original_form": "바질페스토 파스타 아세요?",
          "speaker_id": "2",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100012583.1.23",
          "form": "알긴 알아요",
          "original_form": "알긴 알아요",
          "speaker_id": "1",
          "time": "20210824 23:12"
        },
        {
          "id": "MDRW2100012583.1.24",
          "form": "그걸 해드세요?",
          "original_form": "그걸 해드세요?",
          "speaker_id": "1",
          "time": "20210824 23:12"
        },
        {
          "id": "MDRW2100012583.1.25",
          "form": "뭔가 엄지를 드는 이모티콘이 절실한데 여긴 없네요",
          "original_form": "뭔가 엄지를 드는 이모티콘이 절실한데 여긴 없네요",
          "speaker_id": "1",
          "time": "20210824 23:12"
        },
        {
          "id": "MDRW2100012583.1.26",
          "form": "김밀란님 채널 구독자라",
          "original_form": "김밀란님 채널 구독자라",
          "speaker_id": "2",
          "time": "20210824 23:12"
        },
        {
          "id": "MDRW2100012583.1.27",
          "form": "리조또도 해먹고",
          "original_form": "리조또도 해먹고",
          "speaker_id": "2",
          "time": "20210824 23:13"
        },
        {
          "id": "MDRW2100012583.1.28",
          "form": "김밀란님?",
          "original_form": "김밀란님?",
          "speaker_id": "1",
          "time": "20210824 23:13"
        },
        {
          "id": "MDRW2100012583.1.29",
          "form": "당장 유튜브에 쳐볼게요",
          "original_form": "당장 유튜브에 쳐볼게요",
          "speaker_id": "1",
          "time": "20210824 23:13"
        },
        {
          "id": "MDRW2100012583.1.30",
          "form": "네 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:14"
        },
        {
          "id": "MDRW2100012583.1.31",
          "form": "완전 좋아요",
          "original_form": "완전 좋아요",
          "speaker_id": "1",
          "time": "20210824 23:14"
        },
        {
          "id": "MDRW2100012583.1.32",
          "form": "다만 문제가 지금 배고파졌어요",
          "original_form": "다만 문제가 지금 배고파졌어요",
          "speaker_id": "1",
          "time": "20210824 23:14"
        },
        {
          "id": "MDRW2100012583.1.33",
          "form": "아 ㅋㅋ",
          "original_form": "아 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100012583.1.34",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100012583.1.35",
          "form": "바질페스토 리조또 보고",
          "original_form": "바질페스토 리조또 보고",
          "speaker_id": "1",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100012583.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100012583.1.37",
          "form": "이분 이탈리안 마스터시네요",
          "original_form": "이분 이탈리안 마스터시네요",
          "speaker_id": "1",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100012583.1.38",
          "form": "좋다",
          "original_form": "좋다",
          "speaker_id": "1",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100012583.1.39",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100012583.1.40",
          "form": "시칠리아 가고 싶어요",
          "original_form": "시칠리아 가고 싶어요",
          "speaker_id": "2",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100012583.1.41",
          "form": "ㅋㅋㅎㅋㅎㅋ",
          "original_form": "ㅋㅋㅎㅋㅎㅋ",
          "speaker_id": "2",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100012583.1.42",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100012583.1.43",
          "form": "이탈리아 남부 섬이랑",
          "original_form": "이탈리아 남부 섬이랑",
          "speaker_id": "1",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100012583.1.44",
          "form": "저 여행 길게 가보고 싶어요",
          "original_form": "저 여행 길게 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100012583.1.45",
          "form": "마피아가 되는겁니다",
          "original_form": "마피아가 되는겁니다",
          "speaker_id": "2",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100012583.1.46",
          "form": "이탈리아 1달 있으면서 지역별 파스타를 먹는 거죠",
          "original_form": "이탈리아 1달 있으면서 지역별 파스타를 먹는 거죠",
          "speaker_id": "1",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100012583.1.47",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100012583.1.48",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100012583.1.49",
          "form": "가사일 정도는 부하들이 해주겠네요",
          "original_form": "가사일 정도는 부하들이 해주겠네요",
          "speaker_id": "1",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012583.1.50",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012583.1.51",
          "form": "그렇죠 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그렇죠 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012583.1.52",
          "form": "제가 바라던 삶이 맞긴 한데",
          "original_form": "제가 바라던 삶이 맞긴 한데",
          "speaker_id": "1",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012583.1.53",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012583.1.54",
          "form": "음식물 쓰 레 기 처리랑",
          "original_form": "음식물 쓰 레 기 처리랑",
          "speaker_id": "1",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012583.1.55",
          "form": "화장실 청소가 진짜 싫어서 맨날 미뤄요",
          "original_form": "화장실 청소가 진짜 싫어서 맨날 미뤄요",
          "speaker_id": "1",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012583.1.56",
          "form": "혼납시다",
          "original_form": "혼납시다",
          "speaker_id": "2",
          "time": "20210824 23:18"
        },
        {
          "id": "MDRW2100012583.1.57",
          "form": "아악",
          "original_form": "아악",
          "speaker_id": "1",
          "time": "20210824 23:18"
        },
        {
          "id": "MDRW2100012583.1.58",
          "form": "삼일만 미뤄게 해줍시다",
          "original_form": "삼일만 미뤄게 해줍시다",
          "speaker_id": "1",
          "time": "20210824 23:19"
        },
        {
          "id": "MDRW2100012583.1.59",
          "form": "룸메나 가족이 있으면 나을 것 같은데",
          "original_form": "룸메나 가족이 있으면 나을 것 같은데",
          "speaker_id": "1",
          "time": "20210824 23:19"
        },
        {
          "id": "MDRW2100012583.1.60",
          "form": "혼자 사니까 차일피일 계속 미루게 돼요",
          "original_form": "혼자 사니까 차일피일 계속 미루게 돼요",
          "speaker_id": "1",
          "time": "20210824 23:19"
        },
        {
          "id": "MDRW2100012583.1.61",
          "form": "밥 먹고 설거지 하는 게 아니라",
          "original_form": "밥 먹고 설거지 하는 게 아니라",
          "speaker_id": "1",
          "time": "20210824 23:19"
        },
        {
          "id": "MDRW2100012583.1.62",
          "form": "먹기 전에 하고",
          "original_form": "먹기 전에 하고",
          "speaker_id": "1",
          "time": "20210824 23:19"
        },
        {
          "id": "MDRW2100012583.1.63",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210824 23:19"
        },
        {
          "id": "MDRW2100012583.1.64",
          "form": "제가 룸메가 되면 힘들어질겁니다 ㅋㅋ",
          "original_form": "제가 룸메가 되면 힘들어질겁니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100012583.1.65",
          "form": "님은 설거지 혼자 있을 때도 바로 하세요?",
          "original_form": "님은 설거지 혼자 있을 때도 바로 하세요?",
          "speaker_id": "1",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100012583.1.66",
          "form": "아 저번에 친구가 저희집에 1주일간 살았던 적 있어요",
          "original_form": "아 저번에 친구가 저희집에 1주일간 살았던 적 있어요",
          "speaker_id": "1",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100012583.1.67",
          "form": "그 친구 때문에",
