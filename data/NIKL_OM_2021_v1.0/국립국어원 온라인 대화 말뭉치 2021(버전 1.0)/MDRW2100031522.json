      "utterance": [
        {
          "id": "MDRW2100031522.1.1",
          "form": "그냥 애니학과 졸업생들이 하는 채널인데",
          "original_form": "그냥 애니학과 졸업생들이 하는 채널인데",
          "speaker_id": "2",
          "time": "20210904 22:23"
        },
        {
          "id": "MDRW2100031522.1.2",
          "form": "게임도 종종하고 고양이랑 그림도 있는 채널이에요",
          "original_form": "게임도 종종하고 고양이랑 그림도 있는 채널이에요",
          "speaker_id": "2",
          "time": "20210904 22:24"
        },
        {
          "id": "MDRW2100031522.1.3",
          "form": "다양하네요",
          "original_form": "다양하네요",
          "speaker_id": "1",
          "time": "20210904 22:24"
        },
        {
          "id": "MDRW2100031522.1.4",
          "form": "저도 가끔 고양이 봐요",
          "original_form": "저도 가끔 고양이 봐요",
          "speaker_id": "1",
          "time": "20210904 22:24"
        },
        {
          "id": "MDRW2100031522.1.5",
          "form": "고양이들 보면 힐링되서 종종 보는거 같아요",
          "original_form": "고양이들 보면 힐링되서 종종 보는거 같아요",
          "speaker_id": "2",
          "time": "20210904 22:26"
        },
        {
          "id": "MDRW2100031522.1.6",
          "form": "맞아요 실제로 못키우니",
          "original_form": "맞아요 실제로 못키우니",
          "speaker_id": "1",
          "time": "20210904 22:27"
        },
        {
          "id": "MDRW2100031522.1.7",
          "form": "랜선집사라도 ㅋㅋㅋ",
          "original_form": "랜선집사라도 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:27"
        },
        {
          "id": "MDRW2100031522.1.8",
          "form": "비록 랜선집사라도",
          "original_form": "비록 랜선집사라도",
          "speaker_id": "2",
          "time": "20210904 22:28"
        },
        {
          "id": "MDRW2100031522.1.9",
          "form": "이 액정 너머로 보면서 광대가 승천해요ㅋㅋㅋ",
          "original_form": "이 액정 너머로 보면서 광대가 승천해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:29"
        },
        {
          "id": "MDRW2100031522.1.10",
          "form": "맞아요 예전에 아리는고양이내가주인?",
          "original_form": "맞아요 예전에 아리는고양이내가주인?",
          "speaker_id": "1",
          "time": "20210904 22:30"
        },
        {
          "id": "MDRW2100031522.1.11",
          "form": "그런 채널도 봤었는데 웃기더라구요",
          "original_form": "그런 채널도 봤었는데 웃기더라구요",
          "speaker_id": "1",
          "time": "20210904 22:30"
        },
        {
          "id": "MDRW2100031522.1.12",
          "form": "채널 제목이 웃기네요ㅋㅋㅋㅋ",
          "original_form": "채널 제목이 웃기네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:32"
        },
        {
          "id": "MDRW2100031522.1.13",
          "form": "아 그거 뭔지 알거같아요",
          "original_form": "아 그거 뭔지 알거같아요",
          "speaker_id": "2",
          "time": "20210904 22:32"
        },
        {
          "id": "MDRW2100031522.1.14",
          "form": "남자분이 고양이 기르는 채널이죠!!",
          "original_form": "남자분이 고양이 기르는 채널이죠!!",
          "speaker_id": "2",
          "time": "20210904 22:32"
        },
        {
          "id": "MDRW2100031522.1.15",
          "form": "고양이가 주인 무는 ᆢ ㅎㅎ",
          "original_form": "고양이가 주인 무는 ᆢ ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 22:32"
        },
        {
          "id": "MDRW2100031522.1.16",
          "form": "네네 남자분이요",
          "original_form": "네네 남자분이요",
          "speaker_id": "1",
          "time": "20210904 22:32"
        },
        {
          "id": "MDRW2100031522.1.17",
          "form": "그거 영상 하나 봤었는데 너무 웃기더라고요ㅋㅋ",
          "original_form": "그거 영상 하나 봤었는데 너무 웃기더라고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:34"
        },
        {
          "id": "MDRW2100031522.1.18",
          "form": "네 나중에 수의사분인가? 나와서 말씀하시는데",
          "original_form": "네 나중에 수의사분인가? 나와서 말씀하시는데",
          "speaker_id": "1",
          "time": "20210904 22:36"
        },
        {
          "id": "MDRW2100031522.1.19",
          "form": "고양이는 스트레스 아닐거라고 ㅎ",
          "original_form": "고양이는 스트레스 아닐거라고 ㅎ",
          "speaker_id": "1",
          "time": "20210904 22:36"
        },
        {
          "id": "MDRW2100031522.1.20",
          "form": "고양이들은 너무 귀여워요ㅋㅋㅋㅋ",
          "original_form": "고양이들은 너무 귀여워요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100031522.1.21",
          "form": "유튜브한테 요즘 고마워하고 있어요",
          "original_form": "유튜브한테 요즘 고마워하고 있어요",
          "speaker_id": "2",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100031522.1.22",
          "form": "유튜브 덕분에 랜선집사도 하고 좋아요",
          "original_form": "유튜브 덕분에 랜선집사도 하고 좋아요",
          "speaker_id": "2",
          "time": "20210904 22:39"
        },
        {
          "id": "MDRW2100031522.1.23",
          "form": "맞아요 유투브 그런면으론 고맙죠",
          "original_form": "맞아요 유투브 그런면으론 고맙죠",
          "speaker_id": "1",
          "time": "20210904 22:40"
        },
        {
          "id": "MDRW2100031522.1.24",
          "form": "가끔은 꺼내서 만지고싶지만요",
          "original_form": "가끔은 꺼내서 만지고싶지만요",
          "speaker_id": "1",
          "time": "20210904 22:45"
        },
        {
          "id": "MDRW2100031522.1.25",
          "form": "맞아요 손을 액정안으로 넣어서 쓰다듬고 싶어요ㅠ",
          "original_form": "맞아요 손을 액정안으로 넣어서 쓰다듬고 싶어요ㅠ",
          "speaker_id": "2",
          "time": "20210904 22:49"
        }
      ]
