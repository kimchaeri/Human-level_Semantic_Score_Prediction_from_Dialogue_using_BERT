      "utterance": [
        {
          "id": "MDRW2100028975.1.1",
          "form": "유튜브 보면",
          "original_form": "유튜브 보면",
          "speaker_id": "2",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028975.1.2",
          "form": "시간이 빨리가는것같어",
          "original_form": "시간이 빨리가는것같어",
          "speaker_id": "2",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028975.1.3",
          "form": "인정?",
          "original_form": "인정?",
          "speaker_id": "2",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028975.1.4",
          "form": "인정!!",
          "original_form": "인정!!",
          "speaker_id": "1",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028975.1.5",
          "form": "꿀꿀할때도",
          "original_form": "꿀꿀할때도",
          "speaker_id": "1",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028975.1.6",
          "form": "유튜브보면 좋잖어",
          "original_form": "유튜브보면 좋잖어",
          "speaker_id": "1",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028975.1.7",
          "form": "맞아 ㅎㅎ",
          "original_form": "맞아 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028975.1.8",
          "form": "특히 새벽유튜브가 짱이지",
          "original_form": "특히 새벽유튜브가 짱이지",
          "speaker_id": "1",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100028975.1.9",
          "form": "근데 점점 광고가 늘어나는것같아..",
          "original_form": "근데 점점 광고가 늘어나는것같아..",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028975.1.10",
          "form": "맞아!!",
          "original_form": "맞아!!",
          "speaker_id": "1",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028975.1.11",
          "form": "예전엔 1영상당 1개거나",
          "original_form": "예전엔 1영상당 1개거나",
          "speaker_id": "1",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028975.1.12",
          "form": "광고 보는게 젤 귀찮아",
          "original_form": "광고 보는게 젤 귀찮아",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028975.1.13",
          "form": "맞어",
          "original_form": "맞어",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028975.1.14",
          "form": "2영상당 1개였는데",
          "original_form": "2영상당 1개였는데",
          "speaker_id": "1",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028975.1.15",
          "form": "이젠 1영상에 4개정도되는거같아",
          "original_form": "이젠 1영상에 4개정도되는거같아",
          "speaker_id": "1",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028975.1.16",
          "form": "인정!",
          "original_form": "인정!",
          "speaker_id": "1",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028975.1.17",
          "form": "완전 많네...",
          "original_form": "완전 많네...",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028975.1.18",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028975.1.19",
          "form": "동생이 유튜브 사장이엇으면 좋앗을텐데...",
          "original_form": "동생이 유튜브 사장이엇으면 좋앗을텐데...",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028975.1.20",
          "form": "근데 유튜브는 대체할 앱이 없는거같아",
          "original_form": "근데 유튜브는 대체할 앱이 없는거같아",
          "speaker_id": "1",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028975.1.21",
          "form": "그러게 ㅎㅎ",
          "original_form": "그러게 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028975.1.22",
          "form": "그건 그래 ㅎㅎ",
          "original_form": "그건 그래 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028975.1.23",
          "form": "내가 유튜브사장이었으면",
          "original_form": "내가 유튜브사장이었으면",
          "speaker_id": "1",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028975.1.24",
          "form": "좋았겠다 ㅎㅎ",
          "original_form": "좋았겠다 ㅎㅎ",
