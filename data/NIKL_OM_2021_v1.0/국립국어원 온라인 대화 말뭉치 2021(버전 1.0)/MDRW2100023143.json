      "utterance": [
        {
          "id": "MDRW2100023143.1.1",
          "form": "어렸을때\r\n불량식품 좀 드셨었나염? ㅋㅋ",
          "original_form": "어렸을때\r\n불량식품 좀 드셨었나염? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:16"
        },
        {
          "id": "MDRW2100023143.1.2",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100023143.1.3",
          "form": "많이먹었죠",
          "original_form": "많이먹었죠",
          "speaker_id": "2",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100023143.1.4",
          "form": "먹는테이프 아폴로",
          "original_form": "먹는테이프 아폴로",
          "speaker_id": "2",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100023143.1.5",
          "form": "이런거 ㅋㅋ",
          "original_form": "이런거 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100023143.1.6",
          "form": "피져는 안좋아하셨어요? ㅋㅋ",
          "original_form": "피져는 안좋아하셨어요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:30"
        },
        {
          "id": "MDRW2100023143.1.7",
          "form": "그거 카라멜?",
          "original_form": "그거 카라멜?",
          "speaker_id": "2",
          "time": "20210829 22:37"
        },
        {
          "id": "MDRW2100023143.1.8",
          "form": "이썩을까바....ㄱㅋㅋㅋㅋ",
          "original_form": "이썩을까바....ㄱㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:37"
        },
        {
          "id": "MDRW2100023143.1.9",
          "form": "ㅋㅋㅋㅋ밭두렁은요?",
          "original_form": "ㅋㅋㅋㅋ밭두렁은요?",
          "speaker_id": "1",
          "time": "20210829 22:42"
        },
        {
          "id": "MDRW2100023143.1.10",
          "form": "이 깨질거같은데ㅋㅋ",
          "original_form": "이 깨질거같은데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:42"
        },
        {
          "id": "MDRW2100023143.1.11",
          "form": "그건 어릴때니까 이 안깨져요",
          "original_form": "그건 어릴때니까 이 안깨져요",
          "speaker_id": "2",
          "time": "20210829 22:43"
        },
        {
          "id": "MDRW2100023143.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:43"
        },
        {
          "id": "MDRW2100023143.1.13",
          "form": "ㅋㅋ문어발도 좋아했었는데",
          "original_form": "ㅋㅋ문어발도 좋아했었는데",
          "speaker_id": "1",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023143.1.14",
          "form": "그립네요 ㅋㅋ",
          "original_form": "그립네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023143.1.15",
          "form": "그건 지금도 많이 팔자나요",
          "original_form": "그건 지금도 많이 팔자나요",
          "speaker_id": "2",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023143.1.16",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023143.1.17",
          "form": "어디가서 사요? ㅋㅋ\r\n요샌 문방구도 잘 없는거 같던데",
          "original_form": "어디가서 사요? ㅋㅋ\r\n요샌 문방구도 잘 없는거 같던데",
          "speaker_id": "1",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100023143.1.18",
          "form": "편의점에 없어요?",
          "original_form": "편의점에 없어요?",
          "speaker_id": "2",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100023143.1.19",
          "form": "숏다리 말씀하시는거에여?ㅋㅋ",
          "original_form": "숏다리 말씀하시는거에여?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:05"
        },
        {
          "id": "MDRW2100023143.1.20",
          "form": "아 그거랑 다른거구나",
          "original_form": "아 그거랑 다른거구나",
          "speaker_id": "2",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100023143.1.21",
          "form": "검색해보니 다르네요",
          "original_form": "검색해보니 다르네요",
          "speaker_id": "2",
          "time": "20210829 23:08"
        },
        {
          "id": "MDRW2100023143.1.22",
          "form": "30대 후반이라 모르시나보당ㅋㅋㅋ\r\n30대 중반들만 즐겨먹었나봐요 문어발ㅋㅋ",
          "original_form": "30대 후반이라 모르시나보당ㅋㅋㅋ\r\n30대 중반들만 즐겨먹었나봐요 문어발ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:08"
        },
        {
          "id": "MDRW2100023143.1.23",
          "form": "아놔",
          "original_form": "아놔",
          "speaker_id": "2",
          "time": "20210829 23:08"
        },
        {
          "id": "MDRW2100023143.1.24",
          "form": "이런식으로 하시겠다?",
          "original_form": "이런식으로 하시겠다?",
          "speaker_id": "2",
          "time": "20210829 23:09"
        },
        {
          "id": "MDRW2100023143.1.25",
          "form": "문어발과 숏다리는 매우 달라요ㅋㅋ",
          "original_form": "문어발과 숏다리는 매우 달라요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:12"
        },
        {
          "id": "MDRW2100023143.1.26",
          "form": "나이공격!",
          "original_form": "나이공격!",
          "speaker_id": "2",
          "time": "20210829 23:12"
        },
        {
          "id": "MDRW2100023143.1.27",
          "form": "자꾸하시면",
          "original_form": "자꾸하시면",
          "speaker_id": "2",
          "time": "20210829 23:12"
        },
        {
          "id": "MDRW2100023143.1.28",
          "form": "저도 공격할거 찾을거에요",
          "original_form": "저도 공격할거 찾을거에요",
          "speaker_id": "2",
          "time": "20210829 23:13"
        },
        {
          "id": "MDRW2100023143.1.29",
          "form": "오늘 저녁은 뭐였나여?",
          "original_form": "오늘 저녁은 뭐였나여?",
