      "utterance": [
        {
          "id": "MDRW2100022640.1.1",
          "form": "재택근무라심 따로 건강도 챙기시나여?",
          "original_form": "재택근무라심 따로 건강도 챙기시나여?",
          "speaker_id": "1",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100022640.1.2",
          "form": "요즘 계단걷기 시작했어요",
          "original_form": "요즘 계단걷기 시작했어요",
          "speaker_id": "2",
          "time": "20210829 19:58"
        },
        {
          "id": "MDRW2100022640.1.3",
          "form": "몇층정도 걸으시나요?",
          "original_form": "몇층정도 걸으시나요?",
          "speaker_id": "1",
          "time": "20210829 20:00"
        },
        {
          "id": "MDRW2100022640.1.4",
          "form": "15층이요!",
          "original_form": "15층이요!",
          "speaker_id": "2",
          "time": "20210829 20:03"
        },
        {
          "id": "MDRW2100022640.1.5",
          "form": "앗 name1님도 운동?!!",
          "original_form": "앗 &name1&님도 운동?!!",
          "speaker_id": "2",
          "time": "20210829 20:03"
        },
        {
          "id": "MDRW2100022640.1.6",
          "form": "전 등산자주다녀요..",
          "original_form": "전 등산자주다녀요..",
          "speaker_id": "1",
          "time": "20210829 20:04"
        },
        {
          "id": "MDRW2100022640.1.7",
          "form": "15층 몇회 왕복이요?",
          "original_form": "15층 몇회 왕복이요?",
          "speaker_id": "1",
          "time": "20210829 20:04"
        },
        {
          "id": "MDRW2100022640.1.8",
          "form": "와 등산!! 저 5번이요 그것도 노력해서ㅜㅜ",
          "original_form": "와 등산!! 저 5번이요 그것도 노력해서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 20:04"
        },
        {
          "id": "MDRW2100022640.1.9",
          "form": "15층 다섯본이면.. 엄청 하시네요..",
          "original_form": "15층 다섯본이면.. 엄청 하시네요..",
          "speaker_id": "1",
          "time": "20210829 20:05"
        },
        {
          "id": "MDRW2100022640.1.10",
          "form": "선출이세요?",
          "original_form": "선출이세요?",
          "speaker_id": "1",
          "time": "20210829 20:05"
        },
        {
          "id": "MDRW2100022640.1.11",
          "form": "ㅋㅋㅋㅋ내려갈땐 엘배ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ내려갈땐 엘배ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:06"
        },
        {
          "id": "MDRW2100022640.1.12",
          "form": "너무지루해서 하기싫어요ㅠㅜ",
          "original_form": "너무지루해서 하기싫어요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210829 20:06"
        },
        {
          "id": "MDRW2100022640.1.13",
          "form": "근데 하체운동 그만한게 없죠.. 무릎에도 부담덜가고",
          "original_form": "근데 하체운동 그만한게 없죠.. 무릎에도 부담덜가고",
          "speaker_id": "1",
          "time": "20210829 20:25"
        },
        {
          "id": "MDRW2100022640.1.14",
          "form": "그래요? 원래 줄넘기 하려다가 다들 말려서 계단하는데",
          "original_form": "그래요? 원래 줄넘기 하려다가 다들 말려서 계단하는데",
          "speaker_id": "2",
          "time": "20210829 20:26"
        },
        {
          "id": "MDRW2100022640.1.15",
          "form": "줄넘기는 딱 30중반까지 같아요.. \r\n무릎 충격 엄청나죠..",
          "original_form": "줄넘기는 딱 30중반까지 같아요.. \r\n무릎 충격 엄청나죠..",
          "speaker_id": "1",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100022640.1.16",
          "form": "의사 백이면 백.. 무조건 계단 추천이죠",
          "original_form": "의사 백이면 백.. 무조건 계단 추천이죠",
          "speaker_id": "1",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100022640.1.17",
          "form": "계단 해야겠네요.. 줄넘기 새로 샀는데ㅜㅜ",
          "original_form": "계단 해야겠네요.. 줄넘기 새로 샀는데ㅜㅜ",
