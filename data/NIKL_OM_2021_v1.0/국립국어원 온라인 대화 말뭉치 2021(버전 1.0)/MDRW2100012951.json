      "utterance": [
        {
          "id": "MDRW2100012951.1.1",
          "form": "네 저도요",
          "original_form": "네 저도요",
          "speaker_id": "2",
          "time": "20210825 09:22"
        },
        {
          "id": "MDRW2100012951.1.2",
          "form": "이제 운전하니까 눈도 싫고",
          "original_form": "이제 운전하니까 눈도 싫고",
          "speaker_id": "1",
          "time": "20210825 09:22"
        },
        {
          "id": "MDRW2100012951.1.3",
          "form": "이젠 교통에 질척이는거 거정에",
          "original_form": "이젠 교통에 질척이는거 거정에",
          "speaker_id": "2",
          "time": "20210825 09:22"
        },
        {
          "id": "MDRW2100012951.1.4",
          "form": "싫어요",
          "original_form": "싫어요",
          "speaker_id": "2",
          "time": "20210825 09:22"
        },
        {
          "id": "MDRW2100012951.1.5",
          "form": "겨울의 장점이 저에게는 앖네요 ㅋ",
          "original_form": "겨울의 장점이 저에게는 앖네요 ㅋ",
          "speaker_id": "1",
          "time": "20210825 09:22"
        },
        {
          "id": "MDRW2100012951.1.6",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210825 09:23"
        },
        {
          "id": "MDRW2100012951.1.7",
          "form": "춥고 눈오고 미세먼지에 감기",
          "original_form": "춥고 눈오고 미세먼지에 감기",
          "speaker_id": "1",
          "time": "20210825 09:23"
        },
        {
          "id": "MDRW2100012951.1.8",
          "form": "작년에 난방비 폭탄 ㅠㅜ",
          "original_form": "작년에 난방비 폭탄 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210825 09:23"
        },
        {
          "id": "MDRW2100012951.1.9",
          "form": "또슬퍼지네요",
          "original_form": "또슬퍼지네요",
          "speaker_id": "2",
          "time": "20210825 09:23"
        },
        {
          "id": "MDRW2100012951.1.10",
          "form": "그러게요 ㅜ",
          "original_form": "그러게요 ㅜ",
          "speaker_id": "1",
          "time": "20210825 09:23"
        },
        {
          "id": "MDRW2100012951.1.11",
          "form": "맞아요 미세먼지 진짜 힘들어요",
          "original_form": "맞아요 미세먼지 진짜 힘들어요",
          "speaker_id": "2",
          "time": "20210825 09:23"
        },
        {
          "id": "MDRW2100012951.1.12",
          "form": "전 봄이 좋던데",
          "original_form": "전 봄이 좋던데",
          "speaker_id": "1",
          "time": "20210825 09:23"
        },
        {
          "id": "MDRW2100012951.1.13",
          "form": "안오면 좋겠오요",
          "original_form": "안오면 좋겠오요",
          "speaker_id": "2",
          "time": "20210825 09:23"
        },
        {
          "id": "MDRW2100012951.1.14",
          "form": "저 꽃알러지 ㅜㅜ",
          "original_form": "저 꽃알러지 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 09:23"
        },
        {
          "id": "MDRW2100012951.1.15",
          "form": "아~ 봄도 별로네요^^;",
          "original_form": "아~ 봄도 별로네요^^;",
          "speaker_id": "1",
          "time": "20210825 09:24"
        },
        {
          "id": "MDRW2100012951.1.16",
          "form": "네 ㅜㅜ 약을 달고 살아요",
          "original_form": "네 ㅜㅜ 약을 달고 살아요",
          "speaker_id": "2",
          "time": "20210825 09:24"
        },
        {
          "id": "MDRW2100012951.1.17",
          "form": "결국 여름만 멀쩡해요 ㅎㅎ",
          "original_form": "결국 여름만 멀쩡해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 09:24"
        },
        {
          "id": "MDRW2100012951.1.18",
          "form": "봄은 그래도 좋긴해요",
          "original_form": "봄은 그래도 좋긴해요",
          "speaker_id": "2",
          "time": "20210825 09:24"
        },
        {
          "id": "MDRW2100012951.1.19",
          "form": "꽃이 날싫어해도",
          "original_form": "꽃이 날싫어해도",
          "speaker_id": "2",
          "time": "20210825 09:24"
        },
        {
          "id": "MDRW2100012951.1.20",
          "form": "이뿌니까",
          "original_form": "이뿌니까",
          "speaker_id": "2",
          "time": "20210825 09:24"
        },
        {
          "id": "MDRW2100012951.1.21",
          "form": "예전 벚꽃행사 참좋았는데 애들도 좋아하고요 ㅎㅎ",
          "original_form": "예전 벚꽃행사 참좋았는데 애들도 좋아하고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 09:25"
        },
        {
          "id": "MDRW2100012951.1.22",
          "form": "전부 약먹지만요",
          "original_form": "전부 약먹지만요",
          "speaker_id": "2",
          "time": "20210825 09:25"
        },
        {
          "id": "MDRW2100012951.1.23",
          "form": "벚꽃 올 봄에 진짜 예뻤어요 / 전 근처 공원에서 봤거든요",
          "original_form": "벚꽃 올 봄에 진짜 예뻤어요 / 전 근처 공원에서 봤거든요",
          "speaker_id": "1",
          "time": "20210825 09:26"
        },
        {
          "id": "MDRW2100012951.1.24",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210825 09:26"
        },
        {
          "id": "MDRW2100012951.1.25",
          "form": "전 드라이브하며 지나갔어요",
          "original_form": "전 드라이브하며 지나갔어요",
          "speaker_id": "2",
          "time": "20210825 09:26"
        },
        {
          "id": "MDRW2100012951.1.26",
          "form": "꽃길 너무 예뻤어요",
          "original_form": "꽃길 너무 예뻤어요",
          "speaker_id": "2",
          "time": "20210825 09:26"
        },
        {
          "id": "MDRW2100012951.1.27",
          "form": "여긴 폐쇄되서 드라이브로 지나갔어요",
          "original_form": "여긴 폐쇄되서 드라이브로 지나갔어요",
          "speaker_id": "2",
          "time": "20210825 09:26"
        },
        {
          "id": "MDRW2100012951.1.28",
          "form": "코로나가 계절을 지우네요",
          "original_form": "코로나가 계절을 지우네요",
          "speaker_id": "2",
          "time": "20210825 09:27"
        },
        {
          "id": "MDRW2100012951.1.29",
          "form": "전 사실 벚꽃길 벚꽃비 내리는거 좋던데",
          "original_form": "전 사실 벚꽃길 벚꽃비 내리는거 좋던데",
          "speaker_id": "2",
          "time": "20210825 09:27"
        },
        {
          "id": "MDRW2100012951.1.30",
          "form": "분위기 있고요 ㅎㅎ",
          "original_form": "분위기 있고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 09:27"
        },
        {
          "id": "MDRW2100012951.1.31",
          "form": "코로나가 계절을 지운단 말 인상깊네요",
          "original_form": "코로나가 계절을 지운단 말 인상깊네요",
          "speaker_id": "1",
          "time": "20210825 09:30"
        },
        {
          "id": "MDRW2100012951.1.32",
          "form": "좋아하는 걸 제대로 즐기지도 못하게 하고",
          "original_form": "좋아하는 걸 제대로 즐기지도 못하게 하고",
          "speaker_id": "1",
          "time": "20210825 09:30"
        }
      ]
