      "utterance": [
        {
          "id": "MDRW2100015970.1.1",
          "form": "요즘에 거의 매일... 쇼핑하는 것 같아요",
          "original_form": "요즘에 거의 매일... 쇼핑하는 것 같아요",
          "speaker_id": "1",
          "time": "20210826 04:22"
        },
        {
          "id": "MDRW2100015970.1.2",
          "form": "거의 쇼핑중독이에여",
          "original_form": "거의 쇼핑중독이에여",
          "speaker_id": "1",
          "time": "20210826 04:22"
        },
        {
          "id": "MDRW2100015970.1.3",
          "form": "핫딜의 맛...",
          "original_form": "핫딜의 맛...",
          "speaker_id": "2",
          "time": "20210826 04:22"
        },
        {
          "id": "MDRW2100015970.1.4",
          "form": "저도 택배 안오면 이상해요",
          "original_form": "저도 택배 안오면 이상해요",
          "speaker_id": "2",
          "time": "20210826 04:22"
        },
        {
          "id": "MDRW2100015970.1.5",
          "form": "근데 또 택배 쌓여있으면 넘 기분좋아요",
          "original_form": "근데 또 택배 쌓여있으면 넘 기분좋아요",
          "speaker_id": "1",
          "time": "20210826 04:22"
        },
        {
          "id": "MDRW2100015970.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 04:22"
        },
        {
          "id": "MDRW2100015970.1.7",
          "form": "설레요ㅋㅋㅋ택배오기전부터",
          "original_form": "설레요ㅋㅋㅋ택배오기전부터",
          "speaker_id": "2",
          "time": "20210826 04:23"
        },
        {
          "id": "MDRW2100015970.1.8",
          "form": "오늘도 택배 4개받았어요",
          "original_form": "오늘도 택배 4개받았어요",
          "speaker_id": "1",
          "time": "20210826 04:23"
        },
        {
          "id": "MDRW2100015970.1.9",
          "form": "우왕 뭐뭐 사셨어요?",
          "original_form": "우왕 뭐뭐 사셨어요?",
          "speaker_id": "2",
          "time": "20210826 04:23"
        },
        {
          "id": "MDRW2100015970.1.10",
          "form": "만두랑 섬유향수랑 잇츠스킨이랑 쭈꾸미여!",
          "original_form": "만두랑 섬유향수랑 잇츠스킨이랑 쭈꾸미여!",
          "speaker_id": "1",
          "time": "20210826 04:23"
        },
        {
          "id": "MDRW2100015970.1.11",
          "form": "핫...",
          "original_form": "핫...",
          "speaker_id": "2",
          "time": "20210826 04:23"
        },
        {
          "id": "MDRW2100015970.1.12",
          "form": "뷰티를 혹시 좋아하시나요....?",
          "original_form": "뷰티를 혹시 좋아하시나요....?",
          "speaker_id": "2",
