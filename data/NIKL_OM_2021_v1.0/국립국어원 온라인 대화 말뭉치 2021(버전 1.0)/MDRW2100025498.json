      "utterance": [
        {
          "id": "MDRW2100025498.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210901 12:14"
        },
        {
          "id": "MDRW2100025498.1.2",
          "form": "저는 고양이 2마리 키워요",
          "original_form": "저는 고양이 2마리 키워요",
          "speaker_id": "2",
          "time": "20210901 12:14"
        },
        {
          "id": "MDRW2100025498.1.3",
          "form": "그쪽은요?",
          "original_form": "그쪽은요?",
          "speaker_id": "2",
          "time": "20210901 12:15"
        },
        {
          "id": "MDRW2100025498.1.4",
          "form": "여건이 아직",
          "original_form": "여건이 아직",
          "speaker_id": "1",
          "time": "20210901 12:16"
        },
        {
          "id": "MDRW2100025498.1.5",
          "form": "어떤 반려동물 키우고 싶으세요",
          "original_form": "어떤 반려동물 키우고 싶으세요",
          "speaker_id": "2",
          "time": "20210901 12:17"
        },
        {
          "id": "MDRW2100025498.1.6",
          "form": "강아지요",
          "original_form": "강아지요",
          "speaker_id": "1",
          "time": "20210901 12:17"
        },
        {
          "id": "MDRW2100025498.1.7",
          "form": "저는 강아지 키우면 산책 같이 가보고 싶어요",
          "original_form": "저는 강아지 키우면 산책 같이 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210901 12:18"
        },
        {
          "id": "MDRW2100025498.1.8",
          "form": "좋내요",
          "original_form": "좋내요",
          "speaker_id": "1",
          "time": "20210901 12:19"
        },
        {
          "id": "MDRW2100025498.1.9",
          "form": "친구가 강아지 키우는데 치료비만 벌써 400은 썼더라구요",
          "original_form": "친구가 강아지 키우는데 치료비만 벌써 400은 썼더라구요",
          "speaker_id": "2",
          "time": "20210901 12:23"
        },
        {
          "id": "MDRW2100025498.1.10",
          "form": "학",
          "original_form": "학",
          "speaker_id": "1",
          "time": "20210901 12:24"
        },
        {
          "id": "MDRW2100025498.1.11",
          "form": "그렇게나 많이?",
          "original_form": "그렇게나 많이?",
          "speaker_id": "1",
          "time": "20210901 12:25"
        },
        {
          "id": "MDRW2100025498.1.12",
          "form": "푸들인데 한번은 뛰다가 다리가 부러지고",
          "original_form": "푸들인데 한번은 뛰다가 다리가 부러지고",
          "speaker_id": "2",
          "time": "20210901 12:40"
        },
        {
          "id": "MDRW2100025498.1.13",
          "form": "한번은 중성화를 늦게 했는데 유선 종양이 생겼데요 ㅠ",
          "original_form": "한번은 중성화를 늦게 했는데 유선 종양이 생겼데요 ㅠ",
          "speaker_id": "2",
          "time": "20210901 12:40"
        },
        {
          "id": "MDRW2100025498.1.14",
          "form": "그런일이",
          "original_form": "그런일이",
          "speaker_id": "1",
          "time": "20210901 12:57"
        },
        {
          "id": "MDRW2100025498.1.15",
          "form": "일이 많네여",
          "original_form": "일이 많네여",
          "speaker_id": "1",
          "time": "20210901 18:23"
        },
        {
          "id": "MDRW2100025498.1.16",
          "form": "저는 집와서 고양이들 밥 주고 있네요",
          "original_form": "저는 집와서 고양이들 밥 주고 있네요",
          "speaker_id": "2",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100025498.1.17",
