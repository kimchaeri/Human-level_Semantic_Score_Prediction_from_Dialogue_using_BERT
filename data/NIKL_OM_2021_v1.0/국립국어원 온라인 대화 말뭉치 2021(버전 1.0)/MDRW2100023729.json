      "utterance": [
        {
          "id": "MDRW2100023729.1.1",
          "form": "월요일 시러시러...",
          "original_form": "월요일 시러시러...",
          "speaker_id": "2",
          "time": "20210830 16:42"
        },
        {
          "id": "MDRW2100023729.1.2",
          "form": "일하기 시러시러",
          "original_form": "일하기 시러시러",
          "speaker_id": "2",
          "time": "20210830 16:42"
        },
        {
          "id": "MDRW2100023729.1.3",
          "form": "이번주 대타 계속해야돼서",
          "original_form": "이번주 대타 계속해야돼서",
          "speaker_id": "1",
          "time": "20210831 14:58"
        },
        {
          "id": "MDRW2100023729.1.4",
          "form": "아침 일곱시 출근에 밤 열시 퇴근...",
          "original_form": "아침 일곱시 출근에 밤 열시 퇴근...",
          "speaker_id": "1",
          "time": "20210831 14:58"
        },
        {
          "id": "MDRW2100023729.1.5",
          "form": "죽을맛입니다",
          "original_form": "죽을맛입니다",
          "speaker_id": "1",
          "time": "20210831 14:58"
        },
        {
          "id": "MDRW2100023729.1.6",
          "form": "으어....",
          "original_form": "으어....",
          "speaker_id": "2",
          "time": "20210831 15:10"
        },
        {
          "id": "MDRW2100023729.1.7",
          "form": "장난아니네요...",
          "original_form": "장난아니네요...",
          "speaker_id": "2",
          "time": "20210831 15:10"
        },
        {
          "id": "MDRW2100023729.1.8",
          "form": "제일 힘든건 정작 와서 별거 안하는거...",
          "original_form": "제일 힘든건 정작 와서 별거 안하는거...",
          "speaker_id": "1",
          "time": "20210831 15:11"
        },
        {
          "id": "MDRW2100023729.1.9",
          "form": "시간이 겁나게 안가네요",
          "original_form": "시간이 겁나게 안가네요",
          "speaker_id": "1",
          "time": "20210831 15:11"
        },
        {
          "id": "MDRW2100023729.1.10",
          "form": "그렇다고 핸드폰도 못보고",
          "original_form": "그렇다고 핸드폰도 못보고",
          "speaker_id": "1",
          "time": "20210831 15:11"
        },
        {
          "id": "MDRW2100023729.1.11",
          "form": "ㅋㅋㅋㅋ...멍때리기해요..",
          "original_form": "ㅋㅋㅋㅋ...멍때리기해요..",
          "speaker_id": "2",
          "time": "20210831 15:12"
        },
        {
          "id": "MDRW2100023729.1.12",
          "form": "같이 알바하는 사람은 없나요",
          "original_form": "같이 알바하는 사람은 없나요",
          "speaker_id": "2",
          "time": "20210831 15:13"
        },
        {
          "id": "MDRW2100023729.1.13",
          "form": "사장님이...",
          "original_form": "사장님이...",
          "speaker_id": "1",
          "time": "20210831 15:17"
        },
        {
          "id": "MDRW2100023729.1.14",
          "form": "아.. 사장님",
          "original_form": "아.. 사장님",
          "speaker_id": "2",
          "time": "20210831 15:17"
        },
        {
          "id": "MDRW2100023729.1.15",
          "form": "사장님은 폰 안보나요...",
          "original_form": "사장님은 폰 안보나요...",
          "speaker_id": "2",
          "time": "20210831 15:18"
        },
        {
          "id": "MDRW2100023729.1.16",
          "form": "자리를 거의 비우긴 하시는데",
          "original_form": "자리를 거의 비우긴 하시는데",
          "speaker_id": "1",
          "time": "20210831 15:20"
        },
        {
          "id": "MDRW2100023729.1.17",
          "form": "너무 대놓고 놀고있으면 월급 주기 싫으실것같아서",
          "original_form": "너무 대놓고 놀고있으면 월급 주기 싫으실것같아서",
          "speaker_id": "1",
          "time": "20210831 15:20"
        },
        {
          "id": "MDRW2100023729.1.18",
          "form": "제가 자제하고있어요",
          "original_form": "제가 자제하고있어요",
          "speaker_id": "1",
          "time": "20210831 15:20"
        },
        {
          "id": "MDRW2100023729.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:21"
        },
        {
          "id": "MDRW2100023729.1.20",
          "form": "ㅋㅋㅋ아 지금 폰도 못본다해서 폰이 없는줄 알았는데 이거어케하는거지 했어요ㅋㅋㅋㅋㅋㅋ순간 바보가 되어버렸음",
          "original_form": "ㅋㅋㅋ아 지금 폰도 못본다해서 폰이 없는줄 알았는데 이거어케하는거지 했어요ㅋㅋㅋㅋㅋㅋ순간 바보가 되어버렸음",
          "speaker_id": "2",
          "time": "20210831 15:21"
        },
        {
          "id": "MDRW2100023729.1.21",
          "form": "안닦아도 되는데 괜히 막 청소하고 그러는건가요",
          "original_form": "안닦아도 되는데 괜히 막 청소하고 그러는건가요",
          "speaker_id": "2",
          "time": "20210831 15:22"
        },
        {
          "id": "MDRW2100023729.1.22",
          "form": "글쵸 그냥 막 괜히 스팀기 행주로 슥 닦고",
          "original_form": "글쵸 그냥 막 괜히 스팀기 행주로 슥 닦고",
          "speaker_id": "1",
          "time": "20210831 15:24"
        },
        {
          "id": "MDRW2100023729.1.23",
          "form": "냉동고 열어서 치아바타 갯수세는척",
          "original_form": "냉동고 열어서 치아바타 갯수세는척",
          "speaker_id": "1",
          "time": "20210831 15:24"
        },
        {
          "id": "MDRW2100023729.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:24"
        },
        {
          "id": "MDRW2100023729.1.25",
          "form": "뭔가 그래야 할 것 같아요",
          "original_form": "뭔가 그래야 할 것 같아요",
          "speaker_id": "2",
          "time": "20210831 15:24"
        },
        {
          "id": "MDRW2100023729.1.26",
          "form": "전에 손님없어서 담배피러간다고 나갔다가 20분만에 돌아왔는데",
          "original_form": "전에 손님없어서 담배피러간다고 나갔다가 20분만에 돌아왔는데",
          "speaker_id": "1",
          "time": "20210831 15:25"
        },
        {
          "id": "MDRW2100023729.1.27",
          "form": "돌아와보니 네명 앉아서 커피마시고있는거보고 식은땀이 그냥",
          "original_form": "돌아와보니 네명 앉아서 커피마시고있는거보고 식은땀이 그냥",
          "speaker_id": "1",
          "time": "20210831 15:25"
        },
        {
          "id": "MDRW2100023729.1.28",
          "form": "....!20분간 담배를 ....?",
          "original_form": "....!20분간 담배를 ....?",
          "speaker_id": "2",
          "time": "20210831 15:25"
        },
        {
          "id": "MDRW2100023729.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:25"
        },
        {
          "id": "MDRW2100023729.1.30",
          "form": "여자친구랑 전화하고 들어왔어요ㅋㅋㅋ",
          "original_form": "여자친구랑 전화하고 들어왔어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:25"
        },
        {
          "id": "MDRW2100023729.1.31",
          "form": "식은땀 날만하네요...",
          "original_form": "식은땀 날만하네요...",
          "speaker_id": "2",
          "time": "20210831 15:25"
        },
        {
          "id": "MDRW2100023729.1.32",
          "form": "초반에 여자친구한테 여기 한가하다고 개꿀이라고",
          "original_form": "초반에 여자친구한테 여기 한가하다고 개꿀이라고",
          "speaker_id": "1",
          "time": "20210831 15:26"
        },
        {
          "id": "MDRW2100023729.1.33",
          "form": "전화하고 들어가는데",
          "original_form": "전화하고 들어가는데",
          "speaker_id": "1",
          "time": "20210831 15:26"
        },
        {
          "id": "MDRW2100023729.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:26"
        },
