        {
          "id": "MDRW2100016459.1.1",
          "form": "영화 좋아하세요?",
          "original_form": "영화 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 12:55"
        },
        {
          "id": "MDRW2100016459.1.2",
          "form": "많이 좋아합니다",
          "original_form": "많이 좋아합니다",
          "speaker_id": "2",
          "time": "20210826 12:58"
        },
        {
          "id": "MDRW2100016459.1.3",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210826 13:00"
        },
        {
          "id": "MDRW2100016459.1.4",
          "form": "영화관에서 최근에 모가디슈 봤어요",
          "original_form": "영화관에서 최근에 모가디슈 봤어요",
          "speaker_id": "2",
          "time": "20210826 13:01"
        },
        {
          "id": "MDRW2100016459.1.5",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210826 13:02"
        },
        {
          "id": "MDRW2100016459.1.6",
          "form": "저도 예정이에요",
          "original_form": "저도 예정이에요",
          "speaker_id": "1",
          "time": "20210826 13:02"
        },
        {
          "id": "MDRW2100016459.1.7",
          "form": "아프간 터지기 전에 봤는데",
          "original_form": "아프간 터지기 전에 봤는데",
          "speaker_id": "2",
          "time": "20210826 13:02"
        },
        {
          "id": "MDRW2100016459.1.8",
          "form": "지금 보시면 더 인상깊으실거에요",
          "original_form": "지금 보시면 더 인상깊으실거에요",
          "speaker_id": "2",
          "time": "20210826 13:03"
        },
        {
          "id": "MDRW2100016459.1.9",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 13:03"
        },
        {
          "id": "MDRW2100016459.1.10",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210826 13:03"
        },
        {
          "id": "MDRW2100016459.1.11",
          "form": "이번주엔 프리가이 볼 예정이에요",
          "original_form": "이번주엔 프리가이 볼 예정이에요",
          "speaker_id": "2",
          "time": "20210826 13:04"
        },
        {
          "id": "MDRW2100016459.1.12",
          "form": "모가디슈 강추!",
          "original_form": "모가디슈 강추!",
          "speaker_id": "2",
          "time": "20210826 13:04"
        },
        {
          "id": "MDRW2100016459.1.13",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210826 13:04"
        },
        {
          "id": "MDRW2100016459.1.14",
          "form": "추천할 영화 잇으신가요?",
          "original_form": "추천할 영화 잇으신가요?",
          "speaker_id": "2",
          "time": "20210826 13:14"
        },
        {
          "id": "MDRW2100016459.1.15",
          "form": "저도 모가디슈 들었어요",
          "original_form": "저도 모가디슈 들었어요",
          "speaker_id": "1",
          "time": "20210826 17:51"
        },
        {
          "id": "MDRW2100016459.1.16",
          "form": "좋아하는 영화 궁금해요",
          "original_form": "좋아하는 영화 궁금해요",
          "speaker_id": "2",
          "time": "20210826 18:16"
        },
        {
          "id": "MDRW2100016459.1.17",
          "form": "로코요",
          "original_form": "로코요",
