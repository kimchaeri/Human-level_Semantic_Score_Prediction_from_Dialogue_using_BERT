      "utterance": [
        {
          "id": "MDRW2100016251.1.1",
          "form": "가보고싶은 여행지가 있으십니까",
          "original_form": "가보고싶은 여행지가 있으십니까",
          "speaker_id": "2",
          "time": "20210827 14:42"
        },
        {
          "id": "MDRW2100016251.1.2",
          "form": "저는 영국가보고 싶어요ㅎㅎ",
          "original_form": "저는 영국가보고 싶어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100016251.1.3",
          "form": "영국 너무 멋지죠",
          "original_form": "영국 너무 멋지죠",
          "speaker_id": "2",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100016251.1.4",
          "form": "가고싶은 이유가 있으신가요",
          "original_form": "가고싶은 이유가 있으신가요",
          "speaker_id": "2",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100016251.1.5",
          "form": "제가 축구를 좋아하는데 좋아하는 축구팀이 영국 축구팀이어서요ㅎㅎ",
          "original_form": "제가 축구를 좋아하는데 좋아하는 축구팀이 영국 축구팀이어서요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100016251.1.6",
          "form": "직관 한번 해보고싶네요",
          "original_form": "직관 한번 해보고싶네요",
          "speaker_id": "1",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100016251.1.7",
          "form": "축구를 좋아하시는군요!",
          "original_form": "축구를 좋아하시는군요!",
          "speaker_id": "2",
          "time": "20210827 15:09"
        },
        {
          "id": "MDRW2100016251.1.8",
          "form": "좋아하는 선수가 잇나요?",
          "original_form": "좋아하는 선수가 잇나요?",
          "speaker_id": "2",
          "time": "20210827 15:10"
        },
        {
          "id": "MDRW2100016251.1.9",
          "form": "선수는 아놀드 좋아하는데 아시나요?",
          "original_form": "선수는 아놀드 좋아하는데 아시나요?",
          "speaker_id": "1",
          "time": "20210827 15:30"
        },
        {
          "id": "MDRW2100016251.1.10",
          "form": "이름은 들어봤어요",
          "original_form": "이름은 들어봤어요",
          "speaker_id": "2",
          "time": "20210827 15:30"
        },
        {
          "id": "MDRW2100016251.1.11",
          "form": "영국 가면 축구말고 또 보고싶은건 없으신가요",
          "original_form": "영국 가면 축구말고 또 보고싶은건 없으신가요",
          "speaker_id": "2",
          "time": "20210827 15:30"
        },
        {
          "id": "MDRW2100016251.1.12",
          "form": "글쎄요ㅋㅋ 축구말고는",
          "original_form": "글쎄요ㅋㅋ 축구말고는",
          "speaker_id": "1",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100016251.1.13",
          "form": "아하 그 셜록홈즈 거리도 가보고싶어욥ㅋㄱ",
          "original_form": "아하 그 셜록홈즈 거리도 가보고싶어욥ㅋㄱ",
          "speaker_id": "1",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100016251.1.14",
          "form": "아 영국하면 셜록홈즈도 있죠",
          "original_form": "아 영국하면 셜록홈즈도 있죠",
          "speaker_id": "2",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100016251.1.15",
          "form": "그 거리 걸으면 그 분위기 느낄 수 있어서 좋겠어요",
          "original_form": "그 거리 걸으면 그 분위기 느낄 수 있어서 좋겠어요",
          "speaker_id": "2",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100016251.1.16",
          "form": "실제 소설 속 셜록홈즈 방처럼 꾸민",
          "original_form": "실제 소설 속 셜록홈즈 방처럼 꾸민",
          "speaker_id": "1",
          "time": "20210827 15:49"
        },
        {
          "id": "MDRW2100016251.1.17",
          "form": "박물관도 있다고 하더라고요ㅋㅋ",
          "original_form": "박물관도 있다고 하더라고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:49"
        },
        {
          "id": "MDRW2100016251.1.18",
          "form": "너무 신기하네요",
          "original_form": "너무 신기하네요",
          "speaker_id": "2",
          "time": "20210827 15:49"
        },
        {
          "id": "MDRW2100016251.1.19",
          "form": "박물관도 있다니",
          "original_form": "박물관도 있다니",
          "speaker_id": "2",
          "time": "20210827 15:49"
        },
        {
          "id": "MDRW2100016251.1.20",
          "form": "name2은 가보고싶은 곳이 있나요",
          "original_form": "&name2&은 가보고싶은 곳이 있나요",
          "speaker_id": "1",
          "time": "20210827 15:49"
        },
        {
          "id": "MDRW2100016251.1.21",
          "form": "전 이탈리아 가보고싶어요 유적지 구경을 좋아해서",
