      "utterance": [
        {
          "id": "MDRW2100014671.1.1",
          "form": "비가옵니까  거기는?",
          "original_form": "비가옵니까  거기는?",
          "speaker_id": "1",
          "time": "20210825 20:13"
        },
        {
          "id": "MDRW2100014671.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014671.1.3",
          "form": "습하고 더워요",
          "original_form": "습하고 더워요",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014671.1.4",
          "form": "요즘 날씨가 이상하긴하네요",
          "original_form": "요즘 날씨가 이상하긴하네요",
          "speaker_id": "1",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014671.1.5",
          "form": "가을이 오려고 하는지",
          "original_form": "가을이 오려고 하는지",
          "speaker_id": "1",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014671.1.6",
          "form": "가을장마라고 하더라구요",
          "original_form": "가을장마라고 하더라구요",
          "speaker_id": "2",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014671.1.7",
          "form": "그래도 매미가 가고 귀뚜라미가 우네요",
          "original_form": "그래도 매미가 가고 귀뚜라미가 우네요",
          "speaker_id": "2",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014671.1.8",
          "form": "여긴 아직 여름 매미소리 들려요",
          "original_form": "여긴 아직 여름 매미소리 들려요",
          "speaker_id": "1",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014671.1.9",
          "form": "요즘 죽은 매미들이 너무 많아요 ㅜㅠ",
          "original_form": "요즘 죽은 매미들이 너무 많아요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014671.1.10",
          "form": "세상에 . 태풍때문에...",
          "original_form": "세상에 . 태풍때문에...",
          "speaker_id": "1",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014671.1.11",
          "form": "그런걸까요",
          "original_form": "그런걸까요",
          "speaker_id": "1",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014671.1.12",
          "form": "그것도있고 매미는 원래 7일간 운다고 하더라구요",
          "original_form": "그것도있고 매미는 원래 7일간 운다고 하더라구요",
          "speaker_id": "2",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014671.1.13",
          "form": "그리고 띠로링",
          "original_form": "그리고 띠로링",
          "speaker_id": "2",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014671.1.14",
          "form": "그걸 생각하면 불쌍한데 막상 시끄럽고 징그럽고",
          "original_form": "그걸 생각하면 불쌍한데 막상 시끄럽고 징그럽고",
          "speaker_id": "2",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014671.1.15",
          "form": "전 가을되면 은행똥냄새  너무 싫어요",
          "original_form": "전 가을되면 은행똥냄새  너무 싫어요",
          "speaker_id": "1",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014671.1.16",
          "form": "앜. 지뢰",
          "original_form": "앜. 지뢰",
          "speaker_id": "2",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014671.1.17",
          "form": "토냄새같지않나요?",
          "original_form": "토냄새같지않나요?",
          "speaker_id": "2",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014671.1.18",
          "form": "너무 싫어요  .. ㅋㅋㅋㅋ",
          "original_form": "너무 싫어요  .. ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014671.1.19",
          "form": "이제 진짜 여름 다지나가네요",
          "original_form": "이제 진짜 여름 다지나가네요",
          "speaker_id": "1",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014671.1.20",
          "form": "한해의 3분의 1일",
          "original_form": "한해의 3분의 1일",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014671.1.21",
          "form": "남았네요",
          "original_form": "남았네요",
          "speaker_id": "2",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014671.1.22",
          "form": "내일은  비안올거같은데",
          "original_form": "내일은  비안올거같은데",
          "speaker_id": "1",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014671.1.23",
          "form": "놀러가야겟어요",
          "original_form": "놀러가야겟어요",
          "speaker_id": "1",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014671.1.24",
          "form": "그래요? 빨래를 해야하는데...",
          "original_form": "그래요? 빨래를 해야하는데...",
          "speaker_id": "2",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014671.1.25",
          "form": "내일도 습할건데요",
          "original_form": "내일도 습할건데요",
          "speaker_id": "1",
          "time": "20210825 20:34"
        }
      ]
    }
