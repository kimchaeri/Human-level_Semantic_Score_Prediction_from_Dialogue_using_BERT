      "utterance": [
        {
          "id": "MDRW2100033773.1.1",
          "form": "응 오빠꺼 줬어",
          "original_form": "응 오빠꺼 줬어",
          "speaker_id": "2",
          "time": "20210825 01:21"
        },
        {
          "id": "MDRW2100033773.1.2",
          "form": "근데 작년에도 탈락인데 올해도 안될듯",
          "original_form": "근데 작년에도 탈락인데 올해도 안될듯",
          "speaker_id": "2",
          "time": "20210825 01:22"
        },
        {
          "id": "MDRW2100033773.1.3",
          "form": "응응 그래서 그거 받으러오신거구나ㅋ",
          "original_form": "응응 그래서 그거 받으러오신거구나ㅋ",
          "speaker_id": "1",
          "time": "20210825 01:22"
        },
        {
          "id": "MDRW2100033773.1.4",
          "form": "왜 탈락이야?",
          "original_form": "왜 탈락이야?",
          "speaker_id": "1",
          "time": "20210825 01:22"
        },
        {
          "id": "MDRW2100033773.1.5",
          "form": "재산?떄문이겟지...ㅋㅋㅋ",
          "original_form": "재산?떄문이겟지...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:22"
        },
        {
          "id": "MDRW2100033773.1.6",
          "form": "노령연금 받으신적 업음 ㅋㅋ",
          "original_form": "노령연금 받으신적 업음 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:22"
        },
        {
          "id": "MDRW2100033773.1.7",
          "form": "아아 재산ㅋㅋㅋ",
          "original_form": "아아 재산ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:23"
        },
        {
          "id": "MDRW2100033773.1.8",
          "form": "노령연금 안타도 재산이 그정도면야ㅋㅋ",
          "original_form": "노령연금 안타도 재산이 그정도면야ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:23"
        },
        {
          "id": "MDRW2100033773.1.9",
          "form": "응 기준이 복잡한테 뭐 몰라 ㅋㅋ",
          "original_form": "응 기준이 복잡한테 뭐 몰라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:23"
        },
        {
          "id": "MDRW2100033773.1.10",
          "form": "좋지좋지ㅋㅋ",
          "original_form": "좋지좋지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:23"
        },
        {
          "id": "MDRW2100033773.1.11",
          "form": "알제 어중간하면 이것도 저것도 아님 ㅋ",
          "original_form": "알제 어중간하면 이것도 저것도 아님 ㅋ",
          "speaker_id": "2",
          "time": "20210825 01:23"
        },
        {
          "id": "MDRW2100033773.1.12",
          "form": "땅때문인가?",
          "original_form": "땅때문인가?",
          "speaker_id": "1",
          "time": "20210825 01:23"
        },
        {
          "id": "MDRW2100033773.1.13",
          "form": "아마도? 땅이 클듯.",
          "original_form": "아마도? 땅이 클듯.",
          "speaker_id": "2",
          "time": "20210825 01:23"
        },
        {
          "id": "MDRW2100033773.1.14",
          "form": "그런가보다ㅋㅋ",
          "original_form": "그런가보다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:24"
        },
        {
          "id": "MDRW2100033773.1.15",
          "form": "노령연금 안타도 땅있음 좋지ㅋㅋ",
          "original_form": "노령연금 안타도 땅있음 좋지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:24"
        },
        {
          "id": "MDRW2100033773.1.16",
          "form": "집터도 있고 하니 그래도 뭔가 남들 다 타는데 못타니 ㅋㅋ 아깝지모",
          "original_form": "집터도 있고 하니 그래도 뭔가 남들 다 타는데 못타니 ㅋㅋ 아깝지모",
          "speaker_id": "2",
          "time": "20210825 01:24"
        },
        {
          "id": "MDRW2100033773.1.17",
          "form": "나 오늘 우딜 2건했어 내꺼",
          "original_form": "나 오늘 우딜 2건했어 내꺼",
          "speaker_id": "2",
          "time": "20210825 01:24"
        },
        {
          "id": "MDRW2100033773.1.18",
          "form": "그래 아깝긴하지",
          "original_form": "그래 아깝긴하지",
          "speaker_id": "1",
          "time": "20210825 01:25"
        },
        {
          "id": "MDRW2100033773.1.19",
          "form": "오오 우딜달렸군ㅋ",
          "original_form": "오오 우딜달렸군ㅋ",
          "speaker_id": "1",
          "time": "20210825 01:25"
        },
        {
          "id": "MDRW2100033773.1.20",
          "form": "카페봤어?? 과자사는거",
          "original_form": "카페봤어?? 과자사는거",
          "speaker_id": "2",
          "time": "20210825 01:25"
        },
        {
