      "utterance": [
        {
          "id": "MDRW2100037294.1.1",
          "form": "우리 멍멍이들 어디깠지",
          "original_form": "우리 멍멍이들 어디깠지",
          "speaker_id": "1",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037294.1.2",
          "form": "어디갔지",
          "original_form": "어디갔지",
          "speaker_id": "1",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037294.1.3",
          "form": "네 방에 있겠지",
          "original_form": "네 방에 있겠지",
          "speaker_id": "2",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037294.1.4",
          "form": "없는데!",
          "original_form": "없는데!",
          "speaker_id": "1",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037294.1.5",
          "form": "몰라 그럼",
          "original_form": "몰라 그럼",
          "speaker_id": "2",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037294.1.6",
          "form": "집 나간거 아니야?",
          "original_form": "집 나간거 아니야?",
          "speaker_id": "1",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037294.1.7",
          "form": "집을 어케 나가니",
          "original_form": "집을 어케 나가니",
          "speaker_id": "2",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037294.1.8",
          "form": "ㅋㅋㅋ 문 안열려있는가..",
          "original_form": "ㅋㅋㅋ 문 안열려있는가..",
          "speaker_id": "1",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037294.1.9",
          "form": "열려있어도 안나간다",
          "original_form": "열려있어도 안나간다",
          "speaker_id": "2",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037294.1.10",
          "form": "그런가 ㅋㅋ 나갈것같은데",
          "original_form": "그런가 ㅋㅋ 나갈것같은데",
          "speaker_id": "1",
          "time": "20210831 22:15"
        },
        {
          "id": "MDRW2100037294.1.11",
          "form": "안방에 있을듯",
          "original_form": "안방에 있을듯",
          "speaker_id": "2",
          "time": "20210831 22:16"
        },
        {
          "id": "MDRW2100037294.1.12",
          "form": "보고올게",
          "original_form": "보고올게",
          "speaker_id": "1",
          "time": "20210831 22:16"
        },
        {
          "id": "MDRW2100037294.1.13",
          "form": "그래라",
          "original_form": "그래라",
          "speaker_id": "2",
          "time": "20210831 22:16"
        },
        {
          "id": "MDRW2100037294.1.14",
          "form": "자고있네 ㅋㅋ",
          "original_form": "자고있네 ㅋㅋ",
