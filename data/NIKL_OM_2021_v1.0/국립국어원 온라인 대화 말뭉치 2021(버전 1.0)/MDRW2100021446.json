      "utterance": [
        {
          "id": "MDRW2100021446.1.1",
          "form": "진로 두꺼비 진짜 귀여운거 같아요",
          "original_form": "진로 두꺼비 진짜 귀여운거 같아요",
          "speaker_id": "2",
          "time": "20210828 19:07"
        },
        {
          "id": "MDRW2100021446.1.2",
          "form": "맞아요 실제 두꺼비는 별로인데 캐릭터는 귀여워요",
          "original_form": "맞아요 실제 두꺼비는 별로인데 캐릭터는 귀여워요",
          "speaker_id": "1",
          "time": "20210828 19:07"
        },
        {
          "id": "MDRW2100021446.1.3",
          "form": "콜라보 많이하던데 구매하신 적 있나요?",
          "original_form": "콜라보 많이하던데 구매하신 적 있나요?",
          "speaker_id": "1",
          "time": "20210828 19:08"
        },
        {
          "id": "MDRW2100021446.1.4",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:11"
        },
        {
          "id": "MDRW2100021446.1.5",
          "form": "아니요 구매하신적 있으신가요?",
          "original_form": "아니요 구매하신적 있으신가요?",
          "speaker_id": "2",
          "time": "20210828 19:11"
        },
        {
          "id": "MDRW2100021446.1.6",
          "form": "전 라인 샐리랑 카카오 라이언 콜라보 제품 나오면 사요",
          "original_form": "전 라인 샐리랑 카카오 라이언 콜라보 제품 나오면 사요",
          "speaker_id": "1",
          "time": "20210828 19:15"
        },
        {
          "id": "MDRW2100021446.1.7",
          "form": "샐리랑 라이언 좋아하시는군요!",
          "original_form": "샐리랑 라이언 좋아하시는군요!",
          "speaker_id": "2",
          "time": "20210828 19:17"
        },
        {
          "id": "MDRW2100021446.1.8",
          "form": "요즘 카카오가 여러 콜라보 많이하더라구요",
          "original_form": "요즘 카카오가 여러 콜라보 많이하더라구요",
          "speaker_id": "2",
          "time": "20210828 19:20"
        },
        {
          "id": "MDRW2100021446.1.9",
          "form": "네 좋아해요 그래서 둘의 콜라보를 꿈꾸고  있어요",
          "original_form": "네 좋아해요 그래서 둘의 콜라보를 꿈꾸고  있어요",
          "speaker_id": "1",
          "time": "20210828 19:31"
        },
        {
          "id": "MDRW2100021446.1.10",
          "form": "카카오는 열일하죠 라인은...",
          "original_form": "카카오는 열일하죠 라인은...",
          "speaker_id": "1",
          "time": "20210828 19:32"
        },
        {
          "id": "MDRW2100021446.1.11",
          "form": "라인은 잘 안하나보네요ㅜ",
          "original_form": "라인은 잘 안하나보네요ㅜ",
          "speaker_id": "2",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100021446.1.12",
          "form": "해외쪽 콜라보만 잘해요",
          "original_form": "해외쪽 콜라보만 잘해요",
          "speaker_id": "1",
          "time": "20210828 19:35"
        },
        {
          "id": "MDRW2100021446.1.13",
          "form": "으윽ㅠ 속상하네요 그건",
          "original_form": "으윽ㅠ 속상하네요 그건",
          "speaker_id": "2",
          "time": "20210828 19:38"
        },
        {
          "id": "MDRW2100021446.1.14",
          "form": "네 그래서 그냥 기대를 안 해요",
          "original_form": "네 그래서 그냥 기대를 안 해요",
          "speaker_id": "1",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100021446.1.15",
          "form": "그렇게 될 것 같아요",
          "original_form": "그렇게 될 것 같아요",
          "speaker_id": "2",
          "time": "20210828 19:50"
        },
        {
          "id": "MDRW2100021446.1.16",
          "form": "많이 해주면 좋은데",
          "original_form": "많이 해주면 좋은데",
          "speaker_id": "2",
          "time": "20210828 19:51"
        },
        {
          "id": "MDRW2100021446.1.17",
          "form": "요즘 콜라보들은 다 외국이랑 하는거같아요",
          "original_form": "요즘 콜라보들은 다 외국이랑 하는거같아요",
          "speaker_id": "2",
          "time": "20210828 19:51"
        },
        {
          "id": "MDRW2100021446.1.18",
          "form": "글로벌만 노리고 있는 것 같아요ㅠ 국내도 해주면 좋은데요",
          "original_form": "글로벌만 노리고 있는 것 같아요ㅠ 국내도 해주면 좋은데요",
          "speaker_id": "1",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100021446.1.19",
          "form": "화장품쪽 콜라보는 혹시 사 보신적 있을까요?",
          "original_form": "화장품쪽 콜라보는 혹시 사 보신적 있을까요?",
          "speaker_id": "1",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100021446.1.20",
          "form": "그니까요ㅠ",
          "original_form": "그니까요ㅠ",
          "speaker_id": "2",
          "time": "20210828 20:00"
        },
        {
          "id": "MDRW2100021446.1.21",
          "form": "네! 맥 카카오는 한 번 구매해봤어요!",
          "original_form": "네! 맥 카카오는 한 번 구매해봤어요!",
          "speaker_id": "2",
          "time": "20210828 20:01"
        },
        {
          "id": "MDRW2100021446.1.22",
          "form": "오 화알못인 저도 맥 카카오 소식은 들었습니다",
          "original_form": "오 화알못인 저도 맥 카카오 소식은 들었습니다",
          "speaker_id": "1",
          "time": "20210828 20:04"
        }
      ]
    }
