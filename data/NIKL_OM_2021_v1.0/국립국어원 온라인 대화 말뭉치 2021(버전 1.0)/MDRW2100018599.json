      "utterance": [
        {
          "id": "MDRW2100018599.1.1",
          "form": "안녕하세요.",
          "original_form": "안녕하세요.",
          "speaker_id": "1",
          "time": "20210827 08:56"
        },
        {
          "id": "MDRW2100018599.1.2",
          "form": "오늘은 비가 안 오네요.",
          "original_form": "오늘은 비가 안 오네요.",
          "speaker_id": "1",
          "time": "20210827 08:57"
        },
        {
          "id": "MDRW2100018599.1.3",
          "form": "안녕하세요 거긴 비가 오나요",
          "original_form": "안녕하세요 거긴 비가 오나요",
          "speaker_id": "2",
          "time": "20210827 08:57"
        },
        {
          "id": "MDRW2100018599.1.4",
          "form": "거긴 어떤가요?",
          "original_form": "거긴 어떤가요?",
          "speaker_id": "1",
          "time": "20210827 08:57"
        },
        {
          "id": "MDRW2100018599.1.5",
          "form": "아 여기는 비가 와요",
          "original_form": "아 여기는 비가 와요",
          "speaker_id": "2",
          "time": "20210827 08:57"
        },
        {
          "id": "MDRW2100018599.1.6",
          "form": "저녁까지 계속 오겠어요",
          "original_form": "저녁까지 계속 오겠어요",
          "speaker_id": "2",
          "time": "20210827 08:58"
        },
        {
          "id": "MDRW2100018599.1.7",
          "form": "지역이 어디인가요",
          "original_form": "지역이 어디인가요",
          "speaker_id": "2",
          "time": "20210827 08:58"
        },
        {
          "id": "MDRW2100018599.1.8",
          "form": "저는 전남 남부요!",
          "original_form": "저는 전남 남부요!",
          "speaker_id": "1",
          "time": "20210827 08:59"
        },
        {
          "id": "MDRW2100018599.1.9",
          "form": "여기는 아침에 조금 내리더니 맑아요!",
          "original_form": "여기는 아침에 조금 내리더니 맑아요!",
          "speaker_id": "1",
          "time": "20210827 08:59"
        },
        {
          "id": "MDRW2100018599.1.10",
          "form": "아 여기는 수원입니다",
          "original_form": "아 여기는 수원입니다",
          "speaker_id": "2",
          "time": "20210827 09:00"
        },
        {
          "id": "MDRW2100018599.1.11",
          "form": "한 7시부터 부슬비가 내리네요",
          "original_form": "한 7시부터 부슬비가 내리네요",
          "speaker_id": "2",
          "time": "20210827 09:00"
        },
        {
          "id": "MDRW2100018599.1.12",
          "form": "어제도 비가 안 왔나요?",
          "original_form": "어제도 비가 안 왔나요?",
          "speaker_id": "2",
          "time": "20210827 09:01"
        },
        {
          "id": "MDRW2100018599.1.13",
          "form": "어제도 조금씩만 오고 맑았어요.",
          "original_form": "어제도 조금씩만 오고 맑았어요.",
          "speaker_id": "1",
          "time": "20210827 09:02"
        },
        {
          "id": "MDRW2100018599.1.14",
          "form": "요새 처서 지나고",
          "original_form": "요새 처서 지나고",
          "speaker_id": "1",
          "time": "20210827 09:02"
        },
        {
          "id": "MDRW2100018599.1.15",
          "form": "완전 가을이 된 것 같네요.",
          "original_form": "완전 가을이 된 것 같네요.",
          "speaker_id": "1",
          "time": "20210827 09:02"
        },
        {
          "id": "MDRW2100018599.1.16",
          "form": "맞아요 비 오면서 완연히 접어들었네요",
          "original_form": "맞아요 비 오면서 완연히 접어들었네요",
          "speaker_id": "2",
          "time": "20210827 09:03"
        },
        {
          "id": "MDRW2100018599.1.17",
          "form": "조금 아쉽기도 해요",
          "original_form": "조금 아쉽기도 해요",
          "speaker_id": "2",
          "time": "20210827 09:03"
        },
        {
          "id": "MDRW2100018599.1.18",
          "form": "무슨 계절 좋아하세요",
          "original_form": "무슨 계절 좋아하세요",
          "speaker_id": "2",
          "time": "20210827 09:03"
        },
        {
          "id": "MDRW2100018599.1.19",
          "form": "저는 겨울이요!",
          "original_form": "저는 겨울이요!",
          "speaker_id": "1",
          "time": "20210827 09:06"
        },
        {
          "id": "MDRW2100018599.1.20",
          "form": "여름 좋아하세요?",
          "original_form": "여름 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 09:06"
        },
        {
          "id": "MDRW2100018599.1.21",
          "form": "네 더위를 별로 안타서요",
          "original_form": "네 더위를 별로 안타서요",
          "speaker_id": "2",
          "time": "20210827 09:08"
        },
        {
          "id": "MDRW2100018599.1.22",
          "form": "추위는 많이 타요",
          "original_form": "추위는 많이 타요",
          "speaker_id": "2",
          "time": "20210827 09:08"
        },
        {
          "id": "MDRW2100018599.1.23",
          "form": "님은 반대시려나요",
          "original_form": "님은 반대시려나요",
          "speaker_id": "2",
          "time": "20210827 09:08"
        },
        {
          "id": "MDRW2100018599.1.24",
          "form": "반대인 것 같아요.",
          "original_form": "반대인 것 같아요.",
          "speaker_id": "1",
          "time": "20210827 09:12"
        },
        {
          "id": "MDRW2100018599.1.25",
          "form": "저는 추위는 괜찮은데",
          "original_form": "저는 추위는 괜찮은데",
          "speaker_id": "1",
          "time": "20210827 09:12"
        },
        {
          "id": "MDRW2100018599.1.26",
          "form": "더운 건 영 못 참아요.",
          "original_form": "더운 건 영 못 참아요.",
          "speaker_id": "1",
          "time": "20210827 09:12"
        },
        {
          "id": "MDRW2100018599.1.27",
          "form": "그럼 여름이 곤욕이었겠네요ㅜ",
          "original_form": "그럼 여름이 곤욕이었겠네요ㅜ",
          "speaker_id": "2",
          "time": "20210827 09:13"
        },
        {
          "id": "MDRW2100018599.1.28",
          "form": "어릴때부터 추위에 강하셨나요",
          "original_form": "어릴때부터 추위에 강하셨나요",
          "speaker_id": "2",
          "time": "20210827 09:14"
        },
        {
          "id": "MDRW2100018599.1.29",
          "form": "추위도 힘들긴 한데 옷 더 입으면 되니까 괜찮았어요.",
          "original_form": "추위도 힘들긴 한데 옷 더 입으면 되니까 괜찮았어요.",
          "speaker_id": "1",
          "time": "20210827 11:16"
        },
        {
          "id": "MDRW2100018599.1.30",
          "form": "근데 더운 건 정말 어찌 할 도리가 없더라고요.",
          "original_form": "근데 더운 건 정말 어찌 할 도리가 없더라고요.",
          "speaker_id": "1",
          "time": "20210827 11:16"
        }
      ]
    }
