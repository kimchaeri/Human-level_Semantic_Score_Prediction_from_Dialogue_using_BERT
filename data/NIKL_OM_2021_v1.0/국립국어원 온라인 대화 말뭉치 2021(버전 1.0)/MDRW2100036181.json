      "utterance": [
        {
          "id": "MDRW2100036181.1.1",
          "form": "어떤 과 나오셨어요?!",
          "original_form": "어떤 과 나오셨어요?!",
          "speaker_id": "2",
          "time": "20210830 18:18"
        },
        {
          "id": "MDRW2100036181.1.2",
          "form": "저는 공대쪽 나왔어요",
          "original_form": "저는 공대쪽 나왔어요",
          "speaker_id": "1",
          "time": "20210830 18:19"
        },
        {
          "id": "MDRW2100036181.1.3",
          "form": "무슨 과 예요?",
          "original_form": "무슨 과 예요?",
          "speaker_id": "1",
          "time": "20210830 18:19"
        },
        {
          "id": "MDRW2100036181.1.4",
          "form": "저는 영상이요!",
          "original_form": "저는 영상이요!",
          "speaker_id": "2",
          "time": "20210830 18:28"
        },
        {
          "id": "MDRW2100036181.1.5",
          "form": "학교다니실때 기억에 남거나 특이한 수업 있으셨나요?",
          "original_form": "학교다니실때 기억에 남거나 특이한 수업 있으셨나요?",
          "speaker_id": "1",
          "time": "20210830 19:57"
        },
        {
          "id": "MDRW2100036181.1.6",
          "form": "뮤직뱅크 관람간거요!!",
          "original_form": "뮤직뱅크 관람간거요!!",
          "speaker_id": "2",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100036181.1.7",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100036181.1.8",
          "form": "뮤직뱅크 관람이 수업이였어요?",
          "original_form": "뮤직뱅크 관람이 수업이였어요?",
          "speaker_id": "1",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100036181.1.9",
          "form": "그때 교수님이 높은분이셨는데 보러가게 해주셨어용!",
          "original_form": "그때 교수님이 높은분이셨는데 보러가게 해주셨어용!",
          "speaker_id": "2",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100036181.1.10",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "2",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100036181.1.11",
          "form": "진짜 독특하네요 우와",
          "original_form": "진짜 독특하네요 우와",
          "speaker_id": "1",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100036181.1.12",
          "form": "그때 아이유 3단고음이 있을때였오요",
          "original_form": "그때 아이유 3단고음이 있을때였오요",
          "speaker_id": "2",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100036181.1.13",
          "form": "마지막이라 듣진 못했지만요ㅜㅜㅜㅜ",
          "original_form": "마지막이라 듣진 못했지만요ㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100036181.1.14",
          "form": "우와....완전 신기한경험이였겠어요",
          "original_form": "우와....완전 신기한경험이였겠어요",
          "speaker_id": "1",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100036181.1.15",
          "form": "기억나는 무대있나요?",
          "original_form": "기억나는 무대있나요?",
          "speaker_id": "1",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100036181.1.16",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "2",
          "time": "20210830 20:20"
        },
        {
          "id": "MDRW2100036181.1.17",
          "form": "음 브아걸이였나? 팬이 4명이서 1당 100했던거요!",
          "original_form": "음 브아걸이였나? 팬이 4명이서 1당 100했던거요!",
          "speaker_id": "2",
          "time": "20210830 20:21"
        },
        {
          "id": "MDRW2100036181.1.18",
          "form": "진짜 멋있더라구요",
          "original_form": "진짜 멋있더라구요",
          "speaker_id": "2",
          "time": "20210830 20:21"
        },
        {
          "id": "MDRW2100036181.1.19",
          "form": "헉ㅠㅠㅠ 브아걸ㅠㅠㅠㅠ",
          "original_form": "헉ㅠㅠㅠ 브아걸ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 20:25"
        },
        {
          "id": "MDRW2100036181.1.20",
          "form": "지금 2배로 부러워졌어요",
          "original_form": "지금 2배로 부러워졌어요",
          "speaker_id": "1",
          "time": "20210830 20:26"
        },
        {
          "id": "MDRW2100036181.1.21",
          "form": "영상쪽을 갔어야 했을까요...",
          "original_form": "영상쪽을 갔어야 했을까요...",
          "speaker_id": "1",
          "time": "20210830 20:26"
        },
        {
          "id": "MDRW2100036181.1.22",
          "form": "아.. 저도 그냥 전공 살려야할지 싶네요..",
          "original_form": "아.. 저도 그냥 전공 살려야할지 싶네요..",
