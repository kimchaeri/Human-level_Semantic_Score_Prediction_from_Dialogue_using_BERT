      "utterance": [
        {
          "id": "MDRW2100012083.1.1",
          "form": "애들이랑 운동은 몇시부터 해?",
          "original_form": "애들이랑 운동은 몇시부터 해?",
          "speaker_id": "2",
          "time": "20210824 19:04"
        },
        {
          "id": "MDRW2100012083.1.2",
          "form": "이제 할라고",
          "original_form": "이제 할라고",
          "speaker_id": "1",
          "time": "20210824 19:04"
        },
        {
          "id": "MDRW2100012083.1.3",
          "form": "애들 내옆에 다닥다닥 붙어 있어 ㅋㅋ",
          "original_form": "애들 내옆에 다닥다닥 붙어 있어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:04"
        },
        {
          "id": "MDRW2100012083.1.4",
          "form": "ㅎㅎ 곧 탈출시켜줄께",
          "original_form": "ㅎㅎ 곧 탈출시켜줄께",
          "speaker_id": "1",
          "time": "20210824 19:05"
        },
        {
          "id": "MDRW2100012083.1.5",
          "form": "애들 스트레칭이라도 하고 있으라고 할까?",
          "original_form": "애들 스트레칭이라도 하고 있으라고 할까?",
          "speaker_id": "2",
          "time": "20210824 19:06"
        },
        {
          "id": "MDRW2100012083.1.6",
          "form": "ㅇㅇ 내가 얘기할께 ㅎㅎ 3분만 기다려줘",
          "original_form": "ㅇㅇ 내가 얘기할께 ㅎㅎ 3분만 기다려줘",
          "speaker_id": "1",
          "time": "20210824 19:06"
        },
        {
          "id": "MDRW2100012083.1.7",
          "form": "알았응 ㅎㅎ 저녁마다 신랑이 애들 데리고 운동하니까 좋네",
          "original_form": "알았응 ㅎㅎ 저녁마다 신랑이 애들 데리고 운동하니까 좋네",
          "speaker_id": "2",
          "time": "20210824 19:06"
        },
        {
          "id": "MDRW2100012083.1.8",
          "form": "같이 건강해져야지.. 애들 요즘 운동량이 너무 적어",
          "original_form": "같이 건강해져야지.. 애들 요즘 운동량이 너무 적어",
          "speaker_id": "1",
          "time": "20210824 19:07"
        },
        {
          "id": "MDRW2100012083.1.9",
          "form": "진짜 우리 때는 밥 먹는 시간 빼고는 다 나가서 놀았는데 ㅎㅎ",
          "original_form": "진짜 우리 때는 밥 먹는 시간 빼고는 다 나가서 놀았는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 19:07"
        },
        {
          "id": "MDRW2100012083.1.10",
          "form": "우리하고는 다른 시대에 살고 있으니까",
          "original_form": "우리하고는 다른 시대에 살고 있으니까",
          "speaker_id": "1",
          "time": "20210824 19:07"
        },
        {
          "id": "MDRW2100012083.1.11",
          "form": "오늘도 한시간 정도 하나?",
          "original_form": "오늘도 한시간 정도 하나?",
          "speaker_id": "2",
          "time": "20210824 19:07"
        },
        {
          "id": "MDRW2100012083.1.12",
          "form": "보고 ㅎㅎ 체력이 되는데까지",
          "original_form": "보고 ㅎㅎ 체력이 되는데까지",
          "speaker_id": "1",
          "time": "20210824 19:08"
        },
        {
          "id": "MDRW2100012083.1.13",
          "form": "퇴근하고 와서 힘들텐데 대단해 ㅎㅎ",
          "original_form": "퇴근하고 와서 힘들텐데 대단해 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 19:08"
        },
        {
          "id": "MDRW2100012083.1.14",
          "form": "애들 어릴 때 사진 보니 너무 귀엽더라",
          "original_form": "애들 어릴 때 사진 보니 너무 귀엽더라",
