        },
        {
          "id": "MDRW2100019918.1.3",
          "form": "무슨 맛이냐니",
          "original_form": "무슨 맛이냐니",
          "speaker_id": "2",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019918.1.4",
          "form": "다 똑같은",
          "original_form": "다 똑같은",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019918.1.5",
          "form": "이상한 냄새 남",
          "original_form": "이상한 냄새 남",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019918.1.6",
          "form": "안똑같다",
          "original_form": "안똑같다",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019918.1.7",
          "form": "이상한 냄새 안남",
          "original_form": "이상한 냄새 안남",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019918.1.8",
          "form": "차갑게 무봐라",
          "original_form": "차갑게 무봐라",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019918.1.9",
          "form": "난다",
          "original_form": "난다",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019918.1.10",
          "form": "안난다",
          "original_form": "안난다",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019918.1.11",
          "form": "차갑게?먹음괜찮나보네",
          "original_form": "차갑게?먹음괜찮나보네",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019918.1.12",
          "form": "차갑게 먹음 먹을만함",
          "original_form": "차갑게 먹음 먹을만함",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019918.1.13",
          "form": "맥주 마시면 화장실이나 가고싶지",
          "original_form": "맥주 마시면 화장실이나 가고싶지",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019918.1.14",
          "form": "이제알았나",
          "original_form": "이제알았나",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019918.1.15",
          "form": "그런거 몇캔씩 왜 먹는지",
          "original_form": "그런거 몇캔씩 왜 먹는지",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019918.1.16",
          "form": "그건맞지",
          "original_form": "그건맞지",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019918.1.17",
          "form": "이해가 안된다",
          "original_form": "이해가 안된다",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019918.1.18",
          "form": "시원한ㅁ",
          "original_form": "시원한ㅁ",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019918.1.19",
          "form": "으로 먹는거다",
          "original_form": "으로 먹는거다",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019918.1.20",
          "form": "니는 맥주 얼마나 마시는데",
          "original_form": "니는 맥주 얼마나 마시는데",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
