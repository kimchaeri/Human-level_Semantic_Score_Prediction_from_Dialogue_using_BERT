      "utterance": [
        {
          "id": "MDRW2100008083.1.1",
          "form": "요새 좋아하는 음식 있으세요?",
          "original_form": "요새 좋아하는 음식 있으세요?",
          "speaker_id": "2",
          "time": "20210822 13:13"
        },
        {
          "id": "MDRW2100008083.1.2",
          "form": "저는 요새 감튀가 땡기는것같아요!",
          "original_form": "저는 요새 감튀가 땡기는것같아요!",
          "speaker_id": "1",
          "time": "20210822 13:13"
        },
        {
          "id": "MDRW2100008083.1.3",
          "form": "맥주 좋아하시나요",
          "original_form": "맥주 좋아하시나요",
          "speaker_id": "2",
          "time": "20210822 13:21"
        },
        {
          "id": "MDRW2100008083.1.4",
          "form": "감튀에 맥주 진짜 맛나는데",
          "original_form": "감튀에 맥주 진짜 맛나는데",
          "speaker_id": "2",
          "time": "20210822 13:21"
        },
        {
          "id": "MDRW2100008083.1.5",
          "form": "와 감튀에 맥주 진리죠...ㅠㅠㅠ",
          "original_form": "와 감튀에 맥주 진리죠...ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100008083.1.6",
          "form": "맥주 어떤맥주 좋아하세요?",
          "original_form": "맥주 어떤맥주 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100008083.1.7",
          "form": "저 흑맥주 좋아해요!!",
          "original_form": "저 흑맥주 좋아해요!!",
          "speaker_id": "2",
          "time": "20210822 13:38"
        },
        {
          "id": "MDRW2100008083.1.8",
          "form": "고소하고 단내나는게 너무 좋더라구요",
          "original_form": "고소하고 단내나는게 너무 좋더라구요",
          "speaker_id": "2",
          "time": "20210822 13:38"
        },
        {
          "id": "MDRW2100008083.1.9",
          "form": "좋아하는 맥주 있으신가요",
          "original_form": "좋아하는 맥주 있으신가요",
          "speaker_id": "2",
          "time": "20210822 13:38"
        },
        {
          "id": "MDRW2100008083.1.10",
          "form": "와 흑맥주",
          "original_form": "와 흑맥주",
          "speaker_id": "1",
          "time": "20210822 13:39"
        },
        {
          "id": "MDRW2100008083.1.11",
          "form": "흑맥주는 좀 무겁지않던가요??",
          "original_form": "흑맥주는 좀 무겁지않던가요??",
          "speaker_id": "1",
          "time": "20210822 13:40"
        },
        {
          "id": "MDRW2100008083.1.12",
          "form": "저는 거의 밥먹는 그런느낌이라 맛은 있는데 잘 못마시겠더라구요",
          "original_form": "저는 거의 밥먹는 그런느낌이라 맛은 있는데 잘 못마시겠더라구요",
          "speaker_id": "1",
          "time": "20210822 13:40"
        },
        {
          "id": "MDRW2100008083.1.13",
          "form": "저는 IPA 종류 맥주 자주 마시는것같아요",
          "original_form": "저는 IPA 종류 맥주 자주 마시는것같아요",
          "speaker_id": "1",
          "time": "20210822 13:40"
        },
        {
          "id": "MDRW2100008083.1.14",
          "form": "밥먹는ㅋㅋㅋㅋ 아 제가 안주 잘 안먹고 술만 먹어서 괜찮은거같아요",
          "original_form": "밥먹는ㅋㅋㅋㅋ 아 제가 안주 잘 안먹고 술만 먹어서 괜찮은거같아요",
          "speaker_id": "2",
          "time": "20210822 13:58"
        },
        {
          "id": "MDRW2100008083.1.15",
          "form": "ipa 저 원래 안좋아햇는데 나이드니까 향긋하고(?) 좋더라구요 맛도 입체적이고",
          "original_form": "ipa 저 원래 안좋아햇는데 나이드니까 향긋하고(?) 좋더라구요 맛도 입체적이고",
          "speaker_id": "2",
          "time": "20210822 13:59"
        },
        {
          "id": "MDRW2100008083.1.16",
          "form": "와 안주잘안먹구 술만 먹다니...",
          "original_form": "와 안주잘안먹구 술만 먹다니...",
          "speaker_id": "1",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100008083.1.17",
          "form": "대단하신데요",
          "original_form": "대단하신데요",
          "speaker_id": "1",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100008083.1.18",
          "form": "저는 안주 맨날 집어먹어서",
          "original_form": "저는 안주 맨날 집어먹어서",
          "speaker_id": "1",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100008083.1.19",
          "form": "가벼운게 좋더라구요",
          "original_form": "가벼운게 좋더라구요",
          "speaker_id": "1",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100008083.1.20",
          "form": "안주랑 먹으면 가벼운 맥주가 좋은 거 같아요",
          "original_form": "안주랑 먹으면 가벼운 맥주가 좋은 거 같아요",
          "speaker_id": "2",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008083.1.21",
          "form": "양꼬치에 칭따오가 괜히 나온 말이 아닌듯한",
          "original_form": "양꼬치에 칭따오가 괜히 나온 말이 아닌듯한",
          "speaker_id": "2",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008083.1.22",
          "form": "양꼬치에 칭따오... 그것두 좋네요",
          "original_form": "양꼬치에 칭따오... 그것두 좋네요",
          "speaker_id": "1",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008083.1.23",
          "form": "자극적인 안주에\r\n심심한 맥주",
          "original_form": "자극적인 안주에\r\n심심한 맥주",
          "speaker_id": "2",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008083.1.24",
          "form": "너무 좋네요...ㅎ",
          "original_form": "너무 좋네요...ㅎ",
          "speaker_id": "1",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100008083.1.25",
          "form": "술 맥주를 가장 좋아하시나유",
