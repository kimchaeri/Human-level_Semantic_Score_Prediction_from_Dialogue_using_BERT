      "utterance": [
        {
          "id": "MDRW2100038962.1.1",
          "form": "아침드시구가셨습니까  계속식사안부만묻고있습니다ㅋㅋㅋ",
          "original_form": "아침드시구가셨습니까  계속식사안부만묻고있습니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 10:49"
        },
        {
          "id": "MDRW2100038962.1.2",
          "form": "님은 드셨나요ㅋㅋ",
          "original_form": "님은 드셨나요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100038962.1.3",
          "form": "저는 점심 저녁 야식파예요ㅎㅎ",
          "original_form": "저는 점심 저녁 야식파예요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100038962.1.4",
          "form": "아 저두안먹었습니다..ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 저두안먹었습니다..ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 10:54"
        },
        {
          "id": "MDRW2100038962.1.5",
          "form": "거의거릅니다..ㅋㅋㅋ",
          "original_form": "거의거릅니다..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 10:54"
        },
        {
          "id": "MDRW2100038962.1.6",
          "form": "ㅎㅎ 사실 습관 돼서 점심까지도 많이 안 고파요ㅎㅎ",
          "original_form": "ㅎㅎ 사실 습관 돼서 점심까지도 많이 안 고파요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 10:57"
        },
        {
          "id": "MDRW2100038962.1.7",
          "form": "비극은  저녁부터 시작되죠ㅎ",
          "original_form": "비극은  저녁부터 시작되죠ㅎ",
          "speaker_id": "1",
          "time": "20210903 10:58"
        },
        {
          "id": "MDRW2100038962.1.8",
          "form": "어! 저도요! 점심도 대충이예요\r\n비극의시작ㅋㅋㅋ악ㅋㅋㅋ",
          "original_form": "어! 저도요! 점심도 대충이예요\r\n비극의시작ㅋㅋㅋ악ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:38"
        },
        {
          "id": "MDRW2100038962.1.9",
          "form": "그래서 점심은 뭐 드실 건가요",
          "original_form": "그래서 점심은 뭐 드실 건가요",
          "speaker_id": "1",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100038962.1.10",
          "form": "또컵라면...?",
          "original_form": "또컵라면...?",
          "speaker_id": "2",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100038962.1.11",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100038962.1.12",
          "form": "뭐드시나요!",
          "original_form": "뭐드시나요!",
          "speaker_id": "2",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100038962.1.13",
          "form": "아니ㅋㅋ",
          "original_form": "아니ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:48"
        },
        {
          "id": "MDRW2100038962.1.14",
          "form": "전 백신 접종 기념 회 시켜먹으려고",
          "original_form": "전 백신 접종 기념 회 시켜먹으려고",
          "speaker_id": "1",
          "time": "20210903 11:48"
        },
        {
          "id": "MDRW2100038962.1.15",
          "form": "배민 보고 있습니다ㅎ",
          "original_form": "배민 보고 있습니다ㅎ",
          "speaker_id": "1",
          "time": "20210903 11:48"
        },
        {
          "id": "MDRW2100038962.1.16",
          "form": "아맞다 회드신다하셨닼!!!",
          "original_form": "아맞다 회드신다하셨닼!!!",
          "speaker_id": "2",
          "time": "20210903 11:51"
        },
        {
          "id": "MDRW2100038962.1.17",
          "form": "맛있는걸로시키십숑!!!!!",
          "original_form": "맛있는걸로시키십숑!!!!!",
          "speaker_id": "2",
          "time": "20210903 11:51"
        },
        {
          "id": "MDRW2100038962.1.18",
          "form": "네네 님도 컵라면에 뭐라도 곁들이세요!",
          "original_form": "네네 님도 컵라면에 뭐라도 곁들이세요!",
          "speaker_id": "1",
          "time": "20210903 11:51"
        },
        {
          "id": "MDRW2100038962.1.19",
          "form": "국밥이라거나 피자라거나 치킨이라거나?",
          "original_form": "국밥이라거나 피자라거나 치킨이라거나?",
          "speaker_id": "1",
          "time": "20210903 11:52"
        },
        {
          "id": "MDRW2100038962.1.20",
          "form": "ㅋㅈㅋㅈㅋㅋ곁들이래놓고 왜 더 푸짐한것들이 나오는거죠ㅋㅋㅋㅋㅋㅋㅋ악ㅋㅋㅋㅋ",
          "original_form": "ㅋㅈㅋㅈㅋㅋ곁들이래놓고 왜 더 푸짐한것들이 나오는거죠ㅋㅋㅋㅋㅋㅋㅋ악ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:54"
        },
        {
          "id": "MDRW2100038962.1.21",
          "form": "위도 먹고 살아야쥬!",
          "original_form": "위도 먹고 살아야쥬!",
          "speaker_id": "1",
          "time": "20210903 11:55"
        },
        {
          "id": "MDRW2100038962.1.22",
