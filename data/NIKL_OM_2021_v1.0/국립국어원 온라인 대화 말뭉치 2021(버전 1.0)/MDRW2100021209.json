      "utterance": [
        {
          "id": "MDRW2100021209.1.1",
          "form": "집 가사일을 힘드시지는 않나요",
          "original_form": "집 가사일을 힘드시지는 않나요",
          "speaker_id": "1",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021209.1.2",
          "form": "힘든거 까지는 아니구",
          "original_form": "힘든거 까지는 아니구",
          "speaker_id": "2",
          "time": "20210828 18:01"
        },
        {
          "id": "MDRW2100021209.1.3",
          "form": "귀찮죠 ㅎㅎ",
          "original_form": "귀찮죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 18:01"
        },
        {
          "id": "MDRW2100021209.1.4",
          "form": "집안일중 청소가 젤 하기 싫으네요",
          "original_form": "집안일중 청소가 젤 하기 싫으네요",
          "speaker_id": "1",
          "time": "20210828 20:34"
        },
        {
          "id": "MDRW2100021209.1.5",
          "form": "ㅋㅋ맞아요 뭔가",
          "original_form": "ㅋㅋ맞아요 뭔가",
          "speaker_id": "2",
          "time": "20210829 06:58"
        },
        {
          "id": "MDRW2100021209.1.6",
          "form": "자잘자잘한게",
          "original_form": "자잘자잘한게",
          "speaker_id": "2",
          "time": "20210829 06:58"
        },
        {
          "id": "MDRW2100021209.1.7",
          "form": "끊이지 않아요",
          "original_form": "끊이지 않아요",
          "speaker_id": "2",
          "time": "20210829 06:58"
        },
        {
          "id": "MDRW2100021209.1.8",
          "form": "전 음식하는건 제일 좋아 한답니다",
          "original_form": "전 음식하는건 제일 좋아 한답니다",
          "speaker_id": "1",
          "time": "20210829 07:30"
        },
        {
          "id": "MDRW2100021209.1.9",
          "form": "ㅋㅋ요리 잘하시나봐요",
          "original_form": "ㅋㅋ요리 잘하시나봐요",
          "speaker_id": "2",
          "time": "20210829 08:59"
        },
        {
          "id": "MDRW2100021209.1.10",
          "form": "잘하진 못해도 열심히 자주 한답니다",
          "original_form": "잘하진 못해도 열심히 자주 한답니다",
          "speaker_id": "1",
          "time": "20210829 11:22"
        },
        {
          "id": "MDRW2100021209.1.11",
          "form": "ㅋㅋㅋ 주 메뉴가 뭐인가요",
          "original_form": "ㅋㅋㅋ 주 메뉴가 뭐인가요",
          "speaker_id": "2",
          "time": "20210829 11:46"
        },
        {
          "id": "MDRW2100021209.1.12",
          "form": "주로 김치 된장 청국장 콩나물국 육개장 해요",
          "original_form": "주로 김치 된장 청국장 콩나물국 육개장 해요",
          "speaker_id": "1",
          "time": "20210829 11:55"
        },
        {
          "id": "MDRW2100021209.1.13",
          "form": "육개장 맛있겠다요",
          "original_form": "육개장 맛있겠다요",
          "speaker_id": "2",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100021209.1.14",
          "form": "글쎄요 맛은 보장 못해요",
          "original_form": "글쎄요 맛은 보장 못해요",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100021209.1.15",
          "form": "혹 잘하는 음식이 있나요",
          "original_form": "혹 잘하는 음식이 있나요",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100021209.1.16",
          "form": "저 계란찜 고수임니다ㅋㅋ",
          "original_form": "저 계란찜 고수임니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 01:20"
        },
        {
          "id": "MDRW2100021209.1.17",
          "form": "계란찜 맛을내는 비법이 먼가요",
