      "utterance": [
        {
          "id": "MDRW2100014164.1.1",
          "form": "주식은",
          "original_form": "주식은",
          "speaker_id": "1",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014164.1.2",
          "form": "그럼 삼전도 마이너스엿나요",
          "original_form": "그럼 삼전도 마이너스엿나요",
          "speaker_id": "1",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014164.1.3",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014164.1.4",
          "form": "미장이 먹여살림",
          "original_form": "미장이 먹여살림",
          "speaker_id": "2",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014164.1.5",
          "form": "수익 20퍼으로 캐리함요",
          "original_form": "수익 20퍼으로 캐리함요",
          "speaker_id": "2",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014164.1.6",
          "form": "저런",
          "original_form": "저런",
          "speaker_id": "1",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100014164.1.7",
          "form": "어찌됐든수익0아닌가요",
          "original_form": "어찌됐든수익0아닌가요",
          "speaker_id": "1",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100014164.1.8",
          "form": "맞습니다",
          "original_form": "맞습니다",
          "speaker_id": "2",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100014164.1.9",
          "form": "그래서 금반지파려고요",
          "original_form": "그래서 금반지파려고요",
          "speaker_id": "2",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100014164.1.10",
          "form": "전세금넣고 쓸 생활비로ㅋㅋ",
          "original_form": "전세금넣고 쓸 생활비로ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100014164.1.11",
          "form": "이사다음날이",
          "original_form": "이사다음날이",
          "speaker_id": "1",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100014164.1.12",
          "form": "월급날아닌가요",
          "original_form": "월급날아닌가요",
          "speaker_id": "1",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100014164.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014164.1.14",
          "form": "그래도 돈은 항상 궁하죠",
          "original_form": "그래도 돈은 항상 궁하죠",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014164.1.15",
          "form": "가을옷 사야됨",
          "original_form": "가을옷 사야됨",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014164.1.16",
          "form": "옷 잘 안사신다면서요",
          "original_form": "옷 잘 안사신다면서요",
          "speaker_id": "1",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014164.1.17",
          "form": "계절마다사여",
          "original_form": "계절마다사여",
          "speaker_id": "2",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014164.1.18",
          "form": "가을옷은 뭐살건가요?",
          "original_form": "가을옷은 뭐살건가요?",
          "speaker_id": "1",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014164.1.19",
          "form": "일단 가죽자켓",
          "original_form": "일단 가죽자켓",
          "speaker_id": "2",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014164.1.20",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014164.1.21",
          "form": "ㅋㅋ ㅋ  ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋ ㅋ  ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014164.1.22",
          "form": "아 저도라이더있어요",
          "original_form": "아 저도라이더있어요",
          "speaker_id": "1",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100014164.1.23",
          "form": "오토바이 타는 자세",
          "original_form": "오토바이 타는 자세",
          "speaker_id": "1",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100014164.1.24",
          "form": "아니 오토바이 위에서 사진한방 찍어주세요",
          "original_form": "아니 오토바이 위에서 사진한방 찍어주세요",
          "speaker_id": "1",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100014164.1.25",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014164.1.26",
          "form": "절 찍으라고요?",
          "original_form": "절 찍으라고요?",
          "speaker_id": "2",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014164.1.27",
          "form": "아님 찍어달라고요?",
          "original_form": "아님 찍어달라고요?",
          "speaker_id": "2",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014164.1.28",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014164.1.29",
          "form": "절 찍어달라고요",
          "original_form": "절 찍어달라고요",
          "speaker_id": "1",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014164.1.30",
          "form": "제 버킷리스트",
          "original_form": "제 버킷리스트",
          "speaker_id": "1",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014164.1.31",
          "form": "라이더입고 오토바이타기인ㄴ데",
          "original_form": "라이더입고 오토바이타기인ㄴ데",
          "speaker_id": "1",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014164.1.32",
          "form": "80퍼정도만 이루면된거죠 뭐",
          "original_form": "80퍼정도만 이루면된거죠 뭐",
          "speaker_id": "1",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014164.1.33",
          "form": "그려요ㅋㅋ",
          "original_form": "그려요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100014164.1.34",
          "form": "비안오면 함 태워드림",
          "original_form": "비안오면 함 태워드림",
          "speaker_id": "2",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100014164.1.35",
          "form": "저어제 모자 배송왔음",
          "original_form": "저어제 모자 배송왔음",
          "speaker_id": "1",
