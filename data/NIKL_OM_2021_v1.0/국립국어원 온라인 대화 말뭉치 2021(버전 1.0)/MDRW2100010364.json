        },
        {
          "id": "MDRW2100010364.1.2",
          "form": "여름휴가 다녀오셨어요?",
          "original_form": "여름휴가 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210823 17:12"
        },
        {
          "id": "MDRW2100010364.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210823 17:15"
        },
        {
          "id": "MDRW2100010364.1.4",
          "form": "여름.. 휴가는 회사가 바빠서",
          "original_form": "여름.. 휴가는 회사가 바빠서",
          "speaker_id": "2",
          "time": "20210823 17:16"
        },
        {
          "id": "MDRW2100010364.1.5",
          "form": "주말에 하루 붙여서 잠깐 다녀왔네요",
          "original_form": "주말에 하루 붙여서 잠깐 다녀왔네요",
          "speaker_id": "2",
          "time": "20210823 17:16"
        },
        {
          "id": "MDRW2100010364.1.6",
          "form": "코로나 때문에 뭐 갈 곳도 마땅찮고",
          "original_form": "코로나 때문에 뭐 갈 곳도 마땅찮고",
          "speaker_id": "2",
          "time": "20210823 17:16"
        },
        {
          "id": "MDRW2100010364.1.7",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210823 17:17"
        },
        {
          "id": "MDRW2100010364.1.8",
          "form": "아ㅠㅠ많이 바쁘시구나",
          "original_form": "아ㅠㅠ많이 바쁘시구나",
          "speaker_id": "1",
          "time": "20210823 17:17"
        },
        {
          "id": "MDRW2100010364.1.9",
          "form": "어디로 다녀오셨어요?",
          "original_form": "어디로 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210823 17:17"
        },
        {
          "id": "MDRW2100010364.1.10",
          "form": "네 좋아해요!! 저는 가까운곳이라도 가려고 하고있어요",
          "original_form": "네 좋아해요!! 저는 가까운곳이라도 가려고 하고있어요",
          "speaker_id": "1",
          "time": "20210823 17:17"
        },
        {
          "id": "MDRW2100010364.1.11",
          "form": "안동아랑 예천, 문경 이렇게 돌았어요",
          "original_form": "안동아랑 예천, 문경 이렇게 돌았어요",
          "speaker_id": "2",
          "time": "20210823 17:17"
        },
        {
          "id": "MDRW2100010364.1.12",
          "form": "안동이랑ㅋㅋ",
          "original_form": "안동이랑ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 17:17"
        },
        {
          "id": "MDRW2100010364.1.13",
          "form": "아직 휴가 안가셨나봐요",
          "original_form": "아직 휴가 안가셨나봐요",
          "speaker_id": "2",
          "time": "20210823 17:18"
        },
        {
          "id": "MDRW2100010364.1.14",
          "form": "저는 10월쯤에 제주도가려구요",
          "original_form": "저는 10월쯤에 제주도가려구요",
          "speaker_id": "1",
          "time": "20210823 17:18"
        },
        {
          "id": "MDRW2100010364.1.15",
          "form": "2주전에 오이도다녀왓어여",
          "original_form": "2주전에 오이도다녀왓어여",
          "speaker_id": "1",
          "time": "20210823 17:19"
        },
        {
          "id": "MDRW2100010364.1.16",
          "form": "헐!? 오이도 저희 동네 근천데",
          "original_form": "헐!? 오이도 저희 동네 근천데",
          "speaker_id": "2",
          "time": "20210823 17:20"
        },
        {
          "id": "MDRW2100010364.1.17",
          "form": "제주도 좋네요 전 해외 자주 갔었는데 코로나 때문에",
          "original_form": "제주도 좋네요 전 해외 자주 갔었는데 코로나 때문에",
          "speaker_id": "2",
          "time": "20210823 17:20"
        },
        {
          "id": "MDRW2100010364.1.18",
          "form": "앗 ㅋㅋㅋ 신기하네요",
          "original_form": "앗 ㅋㅋㅋ 신기하네요",
          "speaker_id": "1",
          "time": "20210823 17:21"
        },
        {
          "id": "MDRW2100010364.1.19",
          "form": "언제 나갈지 모르겠네요",
          "original_form": "언제 나갈지 모르겠네요",
          "speaker_id": "2",
          "time": "20210823 17:21"
        },
        {
          "id": "MDRW2100010364.1.20",
          "form": "저도 작년 3월에 대만가려고 표도 끊어놨는데 취소당했어요 ㅠㅠ",
          "original_form": "저도 작년 3월에 대만가려고 표도 끊어놨는데 취소당했어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 17:21"
        },
        {
          "id": "MDRW2100010364.1.21",
          "form": "취소라니..",
          "original_form": "취소라니..",
          "speaker_id": "2",
          "time": "20210823 17:21"
        },
        {
          "id": "MDRW2100010364.1.22",
          "form": "항공사에서 취소하드라구여,,,",
          "original_form": "항공사에서 취소하드라구여,,,",
          "speaker_id": "1",
          "time": "20210823 17:22"
        },
        {
          "id": "MDRW2100010364.1.23",
          "form": "대만 어디 가려고 했었나요?",
          "original_form": "대만 어디 가려고 했었나요?",
          "speaker_id": "2",
          "time": "20210823 17:22"
        },
        {
          "id": "MDRW2100010364.1.24",
          "form": "대만 시내 돌아다니려고 그랬어여 너무 옛날이라 기억도안나네요 ㅋㅋㅋ",
          "original_form": "대만 시내 돌아다니려고 그랬어여 너무 옛날이라 기억도안나네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:22"
        },
        {
          "id": "MDRW2100010364.1.25",
          "form": "저는 16년도였나ㅋㅋㅋ",
          "original_form": "저는 16년도였나ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010364.1.26",
          "form": "갔다왔었는데 지우펀 재밌었어요",
          "original_form": "갔다왔었는데 지우펀 재밌었어요",
          "speaker_id": "2",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010364.1.27",
          "form": "역시 여행은 젊을때 가야하나봐요",
          "original_form": "역시 여행은 젊을때 가야하나봐요",
          "speaker_id": "1",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010364.1.28",
          "form": "시내 있을 때는 하필 태풍이 스쳐지나가서",
          "original_form": "시내 있을 때는 하필 태풍이 스쳐지나가서",
          "speaker_id": "2",
          "time": "20210823 17:24"
        },
        {
          "id": "MDRW2100010364.1.29",
          "form": "인정합니다",
          "original_form": "인정합니다",
          "speaker_id": "2",
          "time": "20210823 17:24"
        },
        {
          "id": "MDRW2100010364.1.30",
          "form": "젊을때 가야 뽈뽈거리고 싸돌아댕기죠",
          "original_form": "젊을때 가야 뽈뽈거리고 싸돌아댕기죠",
          "speaker_id": "2",
          "time": "20210823 17:24"
        },
        {
          "id": "MDRW2100010364.1.31",
          "form": "젊을때는 놀이기구도 잘 탔는데 요즘에는 별거아닌것만 타고 멀미하드라구요",
          "original_form": "젊을때는 놀이기구도 잘 탔는데 요즘에는 별거아닌것만 타고 멀미하드라구요",
          "speaker_id": "1",
          "time": "20210823 17:24"
        },
        {
          "id": "MDRW2100010364.1.32",
          "form": "저는 유럽가보고싶어요 ㅠㅠ",
          "original_form": "저는 유럽가보고싶어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 17:24"
        },
        {
          "id": "MDRW2100010364.1.33",
          "form": "놀이공원 간 기억이 언젠지ㅋㅋㅋㅋ",
          "original_form": "놀이공원 간 기억이 언젠지ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 17:25"
        },
        {
          "id": "MDRW2100010364.1.34",
          "form": "동유럽 갔다왔었는데 이게 돌다보니까 거기서 거기더라구요ㅋㅋ",
          "original_form": "동유럽 갔다왔었는데 이게 돌다보니까 거기서 거기더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 17:25"
        },
        {
          "id": "MDRW2100010364.1.35",
          "form": "여행진짜 많이다니셨구나",
          "original_form": "여행진짜 많이다니셨구나",
          "speaker_id": "1",
          "time": "20210823 17:25"
        },
        {
          "id": "MDRW2100010364.1.36",
          "form": "9박 11일이었는데",
          "original_form": "9박 11일이었는데",
          "speaker_id": "2",
          "time": "20210823 17:25"
        },
        {
          "id": "MDRW2100010364.1.37",
          "form": "코로나만 아니면 진짜 많이 돌아다닐텐데 아쉬워요 ㅠ",
          "original_form": "코로나만 아니면 진짜 많이 돌아다닐텐데 아쉬워요 ㅠ",
          "speaker_id": "1",
          "time": "20210823 17:26"
        },
        {
          "id": "MDRW2100010364.1.38",
          "form": "많이까지는 아니구요 나름 힐링겸 다닌거라ㅋㅋㅋ",
          "original_form": "많이까지는 아니구요 나름 힐링겸 다닌거라ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 17:26"
        },
        {
          "id": "MDRW2100010364.1.39",
          "form": "일하면서 지친걸 여행으로 버텼던?",
          "original_form": "일하면서 지친걸 여행으로 버텼던?",
          "speaker_id": "2",
          "time": "20210823 17:26"
        },
        {
          "id": "MDRW2100010364.1.40",
          "form": "유럽은 어디 가보고 싶으세요?",
          "original_form": "유럽은 어디 가보고 싶으세요?",
          "speaker_id": "2",
          "time": "20210823 17:27"
        },
        {
          "id": "MDRW2100010364.1.41",
          "form": "저는 스위스요 ㅠㅠ프랑스랑 이탈리아 그냥 다가보고싶어요 ㅋㅋㅋㅋ",
          "original_form": "저는 스위스요 ㅠㅠ프랑스랑 이탈리아 그냥 다가보고싶어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 19:02"
        },
        {
          "id": "MDRW2100010364.1.42",
          "form": "여권괜히만들엇어요 증말",
          "original_form": "여권괜히만들엇어요 증말",
          "speaker_id": "1",
          "time": "20210823 19:02"
        },
        {
          "id": "MDRW2100010364.1.43",
          "form": "ㅋㅋㅋ이태리는 가방 뒤로 메면 네것\r\n옆으로 메면 우리의 것\r\n앞으로 메면 내것이라던데요",
          "original_form": "ㅋㅋㅋ이태리는 가방 뒤로 메면 네것\r\n옆으로 메면 우리의 것\r\n앞으로 메면 내것이라던데요",
          "speaker_id": "2",
          "time": "20210823 19:18"
        },
        {
          "id": "MDRW2100010364.1.44",
          "form": "스위스... 물가 넘 비싸다고 얘기들어서",
          "original_form": "스위스... 물가 넘 비싸다고 얘기들어서",
          "speaker_id": "2",
          "time": "20210823 19:18"
        },
        {
          "id": "MDRW2100010364.1.45",
          "form": "라면만 먹고 살아야한다길래 포기ㅋㅋㅋ",
          "original_form": "라면만 먹고 살아야한다길래 포기ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:18"
        },
        {
          "id": "MDRW2100010364.1.46",
          "form": "ㅋㅋㅋㅋㅋㅋ 역시 우리나라사람들이 손소독제 안훔쳐가는거보고 외국인들이 엄청신기해햇다자나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 역시 우리나라사람들이 손소독제 안훔쳐가는거보고 외국인들이 엄청신기해햇다자나요",
          "speaker_id": "1",
          "time": "20210823 19:27"
        },
        {
          "id": "MDRW2100010364.1.47",
          "form": "나중에 신혼여행으로라도 유럽은 꼭 죽기전에 가보고싶네요",
          "original_form": "나중에 신혼여행으로라도 유럽은 꼭 죽기전에 가보고싶네요",
          "speaker_id": "1",
          "time": "20210823 19:27"
        },
        {
          "id": "MDRW2100010364.1.48",
          "form": "독일가서 하리보왕창사오고싶어요 ㅋㅋㅋㅋ",
          "original_form": "독일가서 하리보왕창사오고싶어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 19:27"
        },
        {
          "id": "MDRW2100010364.1.49",
          "form": "저 독일가서 하리보 먹었었는데 생각보다는 실망이요ㅠ",
          "original_form": "저 독일가서 하리보 먹었었는데 생각보다는 실망이요ㅠ",
          "speaker_id": "2",
          "time": "20210823 19:39"
        },
        {
          "id": "MDRW2100010364.1.50",
          "form": "스머프 모양의 빨간색 파란색 젤리였는데",
          "original_form": "스머프 모양의 빨간색 파란색 젤리였는데",
          "speaker_id": "2",
          "time": "20210823 19:39"
        },
        {
          "id": "MDRW2100010364.1.51",
          "form": "제가 싫어하는 식감의 젤리였어요 푸석푸섯한?",
          "original_form": "제가 싫어하는 식감의 젤리였어요 푸석푸섯한?",
          "speaker_id": "2",
          "time": "20210823 19:40"
        },
        {
          "id": "MDRW2100010364.1.52",
          "form": "쫀득한 젤리 좋아하는데",
          "original_form": "쫀득한 젤리 좋아하는데",
          "speaker_id": "2",
          "time": "20210823 19:40"
        },
        {
          "id": "MDRW2100010364.1.53",
          "form": "신행은 크로아티아로 갈겁니다ㅋㅋㅋ",
          "original_form": "신행은 크로아티아로 갈겁니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:40"
        },
        {
          "id": "MDRW2100010364.1.54",
          "form": "아껴두는중이었는데",
          "original_form": "아껴두는중이었는데",
          "speaker_id": "2",
          "time": "20210823 19:40"
        },
