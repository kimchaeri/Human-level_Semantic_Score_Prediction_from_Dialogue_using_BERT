      "utterance": [
        {
          "id": "MDRW2100018523.1.1",
          "form": "푸딩 좋아하세요?",
          "original_form": "푸딩 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 07:41"
        },
        {
          "id": "MDRW2100018523.1.2",
          "form": "에그타르트 맛있어요",
          "original_form": "에그타르트 맛있어요",
          "speaker_id": "1",
          "time": "20210827 07:41"
        },
        {
          "id": "MDRW2100018523.1.3",
          "form": "푸딩 좋아합니다",
          "original_form": "푸딩 좋아합니다",
          "speaker_id": "1",
          "time": "20210827 07:41"
        },
        {
          "id": "MDRW2100018523.1.4",
          "form": "맛있는데 양이 적어서 아쉬워요ㅋㅋㅋ",
          "original_form": "맛있는데 양이 적어서 아쉬워요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 07:42"
        },
        {
          "id": "MDRW2100018523.1.5",
          "form": "케이크는 당근케이크가 맛있어요",
          "original_form": "케이크는 당근케이크가 맛있어요",
          "speaker_id": "2",
          "time": "20210827 07:42"
        },
        {
          "id": "MDRW2100018523.1.6",
          "form": "그건 그래요 뭔가 뭔가 얼마안먹었는데 사라진 기븐",
          "original_form": "그건 그래요 뭔가 뭔가 얼마안먹었는데 사라진 기븐",
          "speaker_id": "1",
          "time": "20210827 07:42"
        },
        {
          "id": "MDRW2100018523.1.7",
          "form": "약간 쪄진식감을 좋아하시냐봐요",
          "original_form": "약간 쪄진식감을 좋아하시냐봐요",
          "speaker_id": "1",
          "time": "20210827 07:42"
        },
        {
          "id": "MDRW2100018523.1.8",
          "form": "네네ㅋㅋㅋ 손바닥보다 작아요",
          "original_form": "네네ㅋㅋㅋ 손바닥보다 작아요",
          "speaker_id": "2",
          "time": "20210827 07:42"
        },
        {
          "id": "MDRW2100018523.1.9",
          "form": "한입이에요",
          "original_form": "한입이에요",
          "speaker_id": "2",
          "time": "20210827 07:42"
        },
        {
          "id": "MDRW2100018523.1.10",
          "form": "오 그러고보니 그러네요",
          "original_form": "오 그러고보니 그러네요",
          "speaker_id": "2",
          "time": "20210827 07:42"
        },
        {
          "id": "MDRW2100018523.1.11",
          "form": "그럼 떡도 좋아하시나요",
          "original_form": "그럼 떡도 좋아하시나요",
          "speaker_id": "1",
          "time": "20210827 07:42"
        },
        {
          "id": "MDRW2100018523.1.12",
          "form": "네네! 오메기떡 좋아해요",
          "original_form": "네네! 오메기떡 좋아해요",
          "speaker_id": "2",
          "time": "20210827 07:43"
        },
        {
          "id": "MDRW2100018523.1.13",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 07:43"
        },
        {
          "id": "MDRW2100018523.1.14",
          "form": "오메기떡 맛있더라구요",
          "original_form": "오메기떡 맛있더라구요",
          "speaker_id": "1",
          "time": "20210827 07:43"
        },
        {
          "id": "MDRW2100018523.1.15",
          "form": "저 약간 꿀떡아시죠",
          "original_form": "저 약간 꿀떡아시죠",
          "speaker_id": "1",
          "time": "20210827 07:43"
        },
        {
          "id": "MDRW2100018523.1.16",
          "form": "안에 설탕만 들은거",
          "original_form": "안에 설탕만 들은거",
          "speaker_id": "1",
          "time": "20210827 07:43"
        },
        {
          "id": "MDRW2100018523.1.17",
          "form": "네네 꿀 가득 들어간거 맛있져ㅋㅋ",
          "original_form": "네네 꿀 가득 들어간거 맛있져ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 07:43"
        },
        {
          "id": "MDRW2100018523.1.18",
          "form": "깨들은건 싫더라구요",
          "original_form": "깨들은건 싫더라구요",
          "speaker_id": "1",
          "time": "20210827 07:44"
        },
        {
          "id": "MDRW2100018523.1.19",
          "form": "톡톡 터지는게 맛있어요",
          "original_form": "톡톡 터지는게 맛있어요",
          "speaker_id": "2",
          "time": "20210827 07:44"
        },
        {
          "id": "MDRW2100018523.1.20",
          "form": "맞아요ㅎ 잘못먹으면 설탕물이 발사되서 그렇지",
          "original_form": "맞아요ㅎ 잘못먹으면 설탕물이 발사되서 그렇지",
          "speaker_id": "1",
          "time": "20210827 07:44"
        },
        {
          "id": "MDRW2100018523.1.21",
          "form": "달콤한 맛이 너무 좋더라구요",
          "original_form": "달콤한 맛이 너무 좋더라구요",
          "speaker_id": "1",
          "time": "20210827 07:44"
        },
        {
          "id": "MDRW2100018523.1.22",
          "form": "꿀떡을 잘하는 집이 맛있는 떡집이라고 생각해요",
          "original_form": "꿀떡을 잘하는 집이 맛있는 떡집이라고 생각해요",
          "speaker_id": "2",
          "time": "20210827 07:45"
        }
