        {
          "id": "MDRW2100015744.1.1",
          "form": "마카롱 좋아하시나요?",
          "original_form": "마카롱 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 00:45"
        },
        {
          "id": "MDRW2100015744.1.2",
          "form": "단거는 좋아라세여?",
          "original_form": "단거는 좋아라세여?",
          "speaker_id": "1",
          "time": "20210826 00:47"
        },
        {
          "id": "MDRW2100015744.1.3",
          "form": "네 단짠음식 좋아해요",
          "original_form": "네 단짠음식 좋아해요",
          "speaker_id": "2",
          "time": "20210826 00:47"
        },
        {
          "id": "MDRW2100015744.1.4",
          "form": "커피도 달달",
          "original_form": "커피도 달달",
          "speaker_id": "2",
          "time": "20210826 00:47"
        },
        {
          "id": "MDRW2100015744.1.5",
          "form": "name1은?",
          "original_form": "&name1&은?",
          "speaker_id": "2",
          "time": "20210826 00:47"
        },
        {
          "id": "MDRW2100015744.1.6",
          "form": "단짠 싫어하는 사람이 어딨나여..ㅋㅋㅋ",
          "original_form": "단짠 싫어하는 사람이 어딨나여..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:49"
        },
        {
          "id": "MDRW2100015744.1.7",
          "form": "치킨 피자 어느쪽이세요?",
          "original_form": "치킨 피자 어느쪽이세요?",
          "speaker_id": "1",
          "time": "20210826 00:49"
        },
        {
          "id": "MDRW2100015744.1.8",
          "form": "치킨이요ㅎ",
          "original_form": "치킨이요ㅎ",
          "speaker_id": "2",
          "time": "20210826 00:50"
        },
        {
          "id": "MDRW2100015744.1.9",
          "form": "둘다일줄 알았는데.. 욕심이 없으시네요..ㅋㅋㅋㅋ",
          "original_form": "둘다일줄 알았는데.. 욕심이 없으시네요..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:51"
        },
        {
          "id": "MDRW2100015744.1.10",
          "form": "ㅎㅎ 피자는 글케좋아하진않아서 ㅋ",
          "original_form": "ㅎㅎ 피자는 글케좋아하진않아서 ㅋ",
          "speaker_id": "2",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100015744.1.11",
          "form": "나이먹으니 한식이더좋더라구요",
          "original_form": "나이먹으니 한식이더좋더라구요",
          "speaker_id": "2",
          "time": "20210826 00:52"
        },
        {
          "id": "MDRW2100015744.1.12",
          "form": "한식 백반 같은거요?",
          "original_form": "한식 백반 같은거요?",
          "speaker_id": "1",
          "time": "20210826 00:53"
        },
        {
          "id": "MDRW2100015744.1.13",
          "form": "고기ㅎ 회ㅎ",
          "original_form": "고기ㅎ 회ㅎ",
          "speaker_id": "2",
          "time": "20210826 00:55"
        },
        {
          "id": "MDRW2100015744.1.14",
          "form": "찌개도좋고",
          "original_form": "찌개도좋고",
          "speaker_id": "2",
          "time": "20210826 00:55"
        },
        {
          "id": "MDRW2100015744.1.15",
          "form": "파스타이런건 가끔ㅎ",
          "original_form": "파스타이런건 가끔ㅎ",
          "speaker_id": "2",
          "time": "20210826 00:55"
        },
        {
          "id": "MDRW2100015744.1.16",
          "form": "늙었나봐요ㅋ",
          "original_form": "늙었나봐요ㅋ",
