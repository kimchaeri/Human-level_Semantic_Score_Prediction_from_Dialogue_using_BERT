      "utterance": [
        {
          "id": "MDRW2100018564.1.1",
          "form": "배달음식 좋아하세요?",
          "original_form": "배달음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 08:27"
        },
        {
          "id": "MDRW2100018564.1.2",
          "form": "음식은 다 좋죠ㅎ",
          "original_form": "음식은 다 좋죠ㅎ",
          "speaker_id": "1",
          "time": "20210827 08:27"
        },
        {
          "id": "MDRW2100018564.1.3",
          "form": "최근에 뭐 시키셨어요?",
          "original_form": "최근에 뭐 시키셨어요?",
          "speaker_id": "1",
          "time": "20210827 08:27"
        },
        {
          "id": "MDRW2100018564.1.4",
          "form": "저 요즘 다이어트 중이라..",
          "original_form": "저 요즘 다이어트 중이라..",
          "speaker_id": "2",
          "time": "20210827 08:30"
        },
        {
          "id": "MDRW2100018564.1.5",
          "form": "아 영빈루 짬뽕이랑 군만두요",
          "original_form": "아 영빈루 짬뽕이랑 군만두요",
          "speaker_id": "2",
          "time": "20210827 08:30"
        },
        {
          "id": "MDRW2100018564.1.6",
          "form": "오오 이름 들어본 곳이에요",
          "original_form": "오오 이름 들어본 곳이에요",
          "speaker_id": "1",
          "time": "20210827 08:31"
        },
        {
          "id": "MDRW2100018564.1.7",
          "form": "아빠가 좋아하시는데 집근처에 분점이 생겼더라구요",
          "original_form": "아빠가 좋아하시는데 집근처에 분점이 생겼더라구요",
          "speaker_id": "2",
          "time": "20210827 08:31"
        },
        {
          "id": "MDRW2100018564.1.8",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "2",
          "time": "20210827 08:31"
        },
        {
          "id": "MDRW2100018564.1.9",
          "form": "거기도 배달을 해주네요ㅎ 요새 맛집이 많이 입점했어요",
          "original_form": "거기도 배달을 해주네요ㅎ 요새 맛집이 많이 입점했어요",
          "speaker_id": "1",
          "time": "20210827 08:31"
        },
        {
          "id": "MDRW2100018564.1.10",
          "form": "아 분점 희소식이네요",
          "original_form": "아 분점 희소식이네요",
          "speaker_id": "1",
          "time": "20210827 08:31"
        },
        {
          "id": "MDRW2100018564.1.11",
          "form": "고기짬뽕 9천원.. 군만두 5개에 7000원..",
          "original_form": "고기짬뽕 9천원.. 군만두 5개에 7000원..",
          "speaker_id": "2",
          "time": "20210827 08:31"
        },
        {
          "id": "MDRW2100018564.1.12",
          "form": "근데 비싸도 맛있어요",
          "original_form": "근데 비싸도 맛있어요",
          "speaker_id": "2",
          "time": "20210827 08:32"
        },
        {
          "id": "MDRW2100018564.1.13",
          "form": "군만두가 왤케 비싸죠ㅜ",
          "original_form": "군만두가 왤케 비싸죠ㅜ",
          "speaker_id": "1",
          "time": "20210827 08:32"
        },
        {
          "id": "MDRW2100018564.1.14",
          "form": "엄청 커요",
          "original_form": "엄청 커요",
          "speaker_id": "2",
          "time": "20210827 08:32"
        },
        {
          "id": "MDRW2100018564.1.15",
          "form": "본점은 좀 작아요",
          "original_form": "본점은 좀 작아요",
          "speaker_id": "2",
          "time": "20210827 08:32"
        },
        {
          "id": "MDRW2100018564.1.16",
          "form": "헉 찐만두도 아니고 군만두가 크다니 신기하네요",
          "original_form": "헉 찐만두도 아니고 군만두가 크다니 신기하네요",
          "speaker_id": "1",
          "time": "20210827 08:32"
        },
        {
          "id": "MDRW2100018564.1.17",
          "form": "다음에 리뷰 쓸 때 좀 작게 여러개 달라고 해보려구용",
          "original_form": "다음에 리뷰 쓸 때 좀 작게 여러개 달라고 해보려구용",
          "speaker_id": "2",
          "time": "20210827 08:32"
        },
        {
          "id": "MDRW2100018564.1.18",
          "form": "이연복쉐프가 만두 만들 때 손이 많이 간다고 하니..",
          "original_form": "이연복쉐프가 만두 만들 때 손이 많이 간다고 하니..",
          "speaker_id": "2",
          "time": "20210827 08:33"
        },
        {
          "id": "MDRW2100018564.1.19",
          "form": "비싼건 이해가 되긴 해요",
          "original_form": "비싼건 이해가 되긴 해요",
          "speaker_id": "2",
          "time": "20210827 08:33"
        },
        {
          "id": "MDRW2100018564.1.20",
          "form": "저희도 만두 만들기 귀찮으니까요ㅎㅎ",
          "original_form": "저희도 만두 만들기 귀찮으니까요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 08:33"
        },
        {
          "id": "MDRW2100018564.1.21",
          "form": "ㅎㅎ 아 그렇게 말하니 이해가 가네요",
          "original_form": "ㅎㅎ 아 그렇게 말하니 이해가 가네요",
          "speaker_id": "1",
          "time": "20210827 08:34"
        },
        {
          "id": "MDRW2100018564.1.22",
          "form": "이연복 셰프집은 만두 안 팔죠?",
          "original_form": "이연복 셰프집은 만두 안 팔죠?",
          "speaker_id": "1",
          "time": "20210827 08:34"
        },
        {
          "id": "MDRW2100018564.1.23",
          "form": "안 판다 했던 거 같아요. 손이 너무 많이 간다네요",
          "original_form": "안 판다 했던 거 같아요. 손이 너무 많이 간다네요",
          "speaker_id": "2",
          "time": "20210827 08:35"
        },
        {
          "id": "MDRW2100018564.1.24",
          "form": "이연복 셰프집은 동파육 꼭 드시래요",
          "original_form": "이연복 셰프집은 동파육 꼭 드시래요",
          "speaker_id": "2",
          "time": "20210827 08:35"
        },
        {
          "id": "MDRW2100018564.1.25",
          "form": "다른 메뉴는 비슷한데 동파육이 확연한 차이를 보인대요",
          "original_form": "다른 메뉴는 비슷한데 동파육이 확연한 차이를 보인대요",
          "speaker_id": "2",
          "time": "20210827 08:36"
        }
      ]
    }
