      "utterance": [
        {
          "id": "MDRW2100021970.1.1",
          "form": "아이디어스 등록해봐",
          "original_form": "아이디어스 등록해봐",
          "speaker_id": "1",
          "time": "20210829 00:18"
        },
        {
          "id": "MDRW2100021970.1.2",
          "form": "들키면 안되는데 가명으로 해볼까?",
          "original_form": "들키면 안되는데 가명으로 해볼까?",
          "speaker_id": "2",
          "time": "20210829 00:18"
        },
        {
          "id": "MDRW2100021970.1.3",
          "form": "소일거리 가능할걸",
          "original_form": "소일거리 가능할걸",
          "speaker_id": "1",
          "time": "20210829 00:18"
        },
        {
          "id": "MDRW2100021970.1.4",
          "form": "솔깃하다",
          "original_form": "솔깃하다",
          "speaker_id": "2",
          "time": "20210829 00:18"
        },
        {
          "id": "MDRW2100021970.1.5",
          "form": "근데 왜 투잡안돼?",
          "original_form": "근데 왜 투잡안돼?",
          "speaker_id": "1",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100021970.1.6",
          "form": "계약서에 있나 암튼 그럴껄",
          "original_form": "계약서에 있나 암튼 그럴껄",
          "speaker_id": "2",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100021970.1.7",
          "form": "아하 글쿤",
          "original_form": "아하 글쿤",
          "speaker_id": "1",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100021970.1.8",
          "form": "간단하게 커미션받고 해주는건 괜찮지않아?",
          "original_form": "간단하게 커미션받고 해주는건 괜찮지않아?",
          "speaker_id": "1",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100021970.1.9",
          "form": "퇴직하고 내일하고싶다",
          "original_form": "퇴직하고 내일하고싶다",
          "speaker_id": "2",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021970.1.10",
          "form": "투잡에서 바로ㅋㄱㅋㅋ",
          "original_form": "투잡에서 바로ㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021970.1.11",
          "form": "그런것도 들키면안될껄",
          "original_form": "그런것도 들키면안될껄",
          "speaker_id": "2",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021970.1.12",
          "form": "좀 그런가...",
          "original_form": "좀 그런가...",
          "speaker_id": "1",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021970.1.13",
          "form": "어렵네 그런게",
          "original_form": "어렵네 그런게",
          "speaker_id": "1",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021970.1.14",
          "form": "이모티콘 몰래해볼까해 ㅋㅋㅋ",
          "original_form": "이모티콘 몰래해볼까해 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021970.1.15",
          "form": "티만 안나면",
          "original_form": "티만 안나면",
          "speaker_id": "1",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100021970.1.16",
          "form": "너도 요즘 만드는거 쩔던데",
          "original_form": "너도 요즘 만드는거 쩔던데",
          "speaker_id": "2",
