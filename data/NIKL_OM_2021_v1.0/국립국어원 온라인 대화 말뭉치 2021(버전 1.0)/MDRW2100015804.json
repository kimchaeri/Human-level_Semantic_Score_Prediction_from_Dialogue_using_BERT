      "utterance": [
        {
          "id": "MDRW2100015804.1.1",
          "form": "너 후생관갈비알아?",
          "original_form": "너 후생관갈비알아?",
          "speaker_id": "1",
          "time": "20210826 01:20"
        },
        {
          "id": "MDRW2100015804.1.2",
          "form": "아니 이거 제출하라는게 왜케 많아",
          "original_form": "아니 이거 제출하라는게 왜케 많아",
          "speaker_id": "2",
          "time": "20210826 01:20"
        },
        {
          "id": "MDRW2100015804.1.3",
          "form": "다섯번은 한듯",
          "original_form": "다섯번은 한듯",
          "speaker_id": "2",
          "time": "20210826 01:20"
        },
        {
          "id": "MDRW2100015804.1.4",
          "form": "어디서 파는거야?",
          "original_form": "어디서 파는거야?",
          "speaker_id": "2",
          "time": "20210826 01:20"
        },
        {
          "id": "MDRW2100015804.1.5",
          "form": "후생관갈비 후생관에서 파는거",
          "original_form": "후생관갈비 후생관에서 파는거",
          "speaker_id": "1",
          "time": "20210826 01:21"
        },
        {
          "id": "MDRW2100015804.1.6",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210826 01:21"
        },
        {
          "id": "MDRW2100015804.1.7",
          "form": "후생관 갈비얘기를 해보자",
          "original_form": "후생관 갈비얘기를 해보자",
          "speaker_id": "1",
          "time": "20210826 01:22"
        },
        {
          "id": "MDRW2100015804.1.8",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210826 01:22"
        },
        {
          "id": "MDRW2100015804.1.9",
          "form": "후생관갈비 들어본적있음?",
          "original_form": "후생관갈비 들어본적있음?",
          "speaker_id": "1",
          "time": "20210826 01:22"
        },
        {
          "id": "MDRW2100015804.1.10",
          "form": "후생관갈비 들어본적없음!",
          "original_form": "후생관갈비 들어본적없음!",
          "speaker_id": "2",
          "time": "20210826 01:23"
        },
        {
          "id": "MDRW2100015804.1.11",
          "form": "유명한곳이야?",
          "original_form": "유명한곳이야?",
          "speaker_id": "2",
          "time": "20210826 01:23"
        },
        {
          "id": "MDRW2100015804.1.12",
          "form": "후생관은알지?",
          "original_form": "후생관은알지?",
          "speaker_id": "1",
          "time": "20210826 01:23"
        },
        {
          "id": "MDRW2100015804.1.13",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210826 01:23"
        },
        {
          "id": "MDRW2100015804.1.14",
          "form": "후생관이 약간 마트같은거잖아 거기 갈비를 파는데 유명하데",
          "original_form": "후생관이 약간 마트같은거잖아 거기 갈비를 파는데 유명하데",
          "speaker_id": "1",
          "time": "20210826 01:24"
        },
        {
          "id": "MDRW2100015804.1.15",
          "form": "오 먹어보고싶다",
          "original_form": "오 먹어보고싶다",
          "speaker_id": "2",
          "time": "20210826 01:24"
        },
        {
          "id": "MDRW2100015804.1.16",
          "form": "은근 갈비맛있는곳이ㅜ없어",
          "original_form": "은근 갈비맛있는곳이ㅜ없어",
          "speaker_id": "2",
          "time": "20210826 01:24"
        },
        {
          "id": "MDRW2100015804.1.17",
          "form": "울산현대 무슨축구선수가 어디서 갈비맛있다고 말했나봐 ㅋㅋ그래서 유명해지;",
          "original_form": "울산현대 무슨축구선수가 어디서 갈비맛있다고 말했나봐 ㅋㅋ그래서 유명해지;",
          "speaker_id": "1",
          "time": "20210826 01:25"
        },
        {
          "id": "MDRW2100015804.1.18",
          "form": "짐",
          "original_form": "짐",
          "speaker_id": "1",
          "time": "20210826 01:25"
        },
        {
          "id": "MDRW2100015804.1.19",
          "form": "ㅋㅋㅋㅋ신기하다ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ신기하다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:26"
        },
        {
          "id": "MDRW2100015804.1.20",
          "form": "너 먹어봄??",
          "original_form": "너 먹어봄??",
          "speaker_id": "2",
          "time": "20210826 01:26"
        },
        {
          "id": "MDRW2100015804.1.21",
          "form": "그래서 주말에 엄마아빠가 사다먹었는데 맛있데",
          "original_form": "그래서 주말에 엄마아빠가 사다먹었는데 맛있데",
          "speaker_id": "1",
          "time": "20210826 01:26"
        },
        {
          "id": "MDRW2100015804.1.22",
          "form": "난 못먹음...",
          "original_form": "난 못먹음...",
          "speaker_id": "1",
          "time": "20210826 01:26"
        },
        {
          "id": "MDRW2100015804.1.23",
          "form": "먹고싶다 ㅜㅜㅜㅜㅜ",
          "original_form": "먹고싶다 ㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 01:26"
        },
        {
          "id": "MDRW2100015804.1.24",
          "form": "근데 갈비가 갈비가 아니래 ㅋㅋㅋ",
          "original_form": "근데 갈비가 갈비가 아니래 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:27"
        },
        {
          "id": "MDRW2100015804.1.25",
          "form": "그러면??",
          "original_form": "그러면??",
          "speaker_id": "2",
          "time": "20210826 01:27"
        },
        {
          "id": "MDRW2100015804.1.26",
          "form": "목살인데 양념갈비식으로 양녀",
          "original_form": "목살인데 양념갈비식으로 양녀",
          "speaker_id": "1",
          "time": "20210826 01:28"
        },
        {
          "id": "MDRW2100015804.1.27",
          "form": "념한건가봐",
          "original_form": "념한건가봐",
          "speaker_id": "1",
          "time": "20210826 01:28"
        },
        {
          "id": "MDRW2100015804.1.28",
          "form": "아 그거좋지",
          "original_form": "아 그거좋지",
          "speaker_id": "2",
          "time": "20210826 01:28"
        },
        {
          "id": "MDRW2100015804.1.29",
          "form": "난 갈비있는거 굽기 힘들어서 싫음ㅋㅋ",
          "original_form": "난 갈비있는거 굽기 힘들어서 싫음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:28"
        },
        {
          "id": "MDRW2100015804.1.30",
          "form": "원래 목살도 이렇게 해먹음?",
          "original_form": "원래 목살도 이렇게 해먹음?",
          "speaker_id": "1",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100015804.1.31",
          "form": "그런듯? 나 형제 숯불갈비가면",
          "original_form": "그런듯? 나 형제 숯불갈비가면",
          "speaker_id": "2",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100015804.1.32",
          "form": "목살로만 주세여",
          "original_form": "목살로만 주세여",
          "speaker_id": "2",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100015804.1.33",
          "form": "그러는데ㅋㅋ",
          "original_form": "그러는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100015804.1.34",
          "form": "헐진짜??ㅡ 헐헐 몰랐음",
          "original_form": "헐진짜??ㅡ 헐헐 몰랐음",
          "speaker_id": "1",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100015804.1.35",
          "form": "나 원래도목살좋아함",
          "original_form": "나 원래도목살좋아함",
          "speaker_id": "1",
          "time": "20210826 01:29"
        },
        {
          "id": "MDRW2100015804.1.36",
          "form": "ㅋㅋ맛있옹",
          "original_form": "ㅋㅋ맛있옹",
          "speaker_id": "2",
          "time": "20210826 01:30"
        },
        {
          "id": "MDRW2100015804.1.37",
          "form": "전하시장 놀러가고싶다",
          "original_form": "전하시장 놀러가고싶다",
          "speaker_id": "2",
          "time": "20210826 01:30"
        },
        {
          "id": "MDRW2100015804.1.38",
          "form": "그 막 기름부분이 없어서 좋아",
          "original_form": "그 막 기름부분이 없어서 좋아",
          "speaker_id": "1",
          "time": "20210826 01:30"
        }
      ]
    }
