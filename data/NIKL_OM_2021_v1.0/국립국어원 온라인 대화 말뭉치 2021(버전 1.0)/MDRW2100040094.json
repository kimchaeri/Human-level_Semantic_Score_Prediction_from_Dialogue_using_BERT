      "utterance": [
        {
          "id": "MDRW2100040094.1.1",
          "form": "아침은 먹었나요?^^",
          "original_form": "아침은 먹었나요?^^",
          "speaker_id": "2",
          "time": "20210904 09:09"
        },
        {
          "id": "MDRW2100040094.1.2",
          "form": "이제 먹으려구요",
          "original_form": "이제 먹으려구요",
          "speaker_id": "1",
          "time": "20210904 09:17"
        },
        {
          "id": "MDRW2100040094.1.3",
          "form": "드셨어요?",
          "original_form": "드셨어요?",
          "speaker_id": "1",
          "time": "20210904 09:17"
        },
        {
          "id": "MDRW2100040094.1.4",
          "form": "저희는 아침 꼬박꼬박 먹는답니다",
          "original_form": "저희는 아침 꼬박꼬박 먹는답니다",
          "speaker_id": "2",
          "time": "20210904 10:00"
        },
        {
          "id": "MDRW2100040094.1.5",
          "form": "저희두요.ㅋㅋ 안먹으면 너무배고파해서요ㅋㅋ",
          "original_form": "저희두요.ㅋㅋ 안먹으면 너무배고파해서요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:03"
        },
        {
          "id": "MDRW2100040094.1.6",
          "form": "일찍 일어나는 아침형 인간인가봅니다.. 저희가 그렇거든요",
          "original_form": "일찍 일어나는 아침형 인간인가봅니다.. 저희가 그렇거든요",
          "speaker_id": "2",
          "time": "20210904 11:09"
        },
        {
          "id": "MDRW2100040094.1.7",
          "form": "일찍 일어나기 떄문에 점심까지 못 기다려요",
          "original_form": "일찍 일어나기 떄문에 점심까지 못 기다려요",
          "speaker_id": "2",
          "time": "20210904 11:09"
        },
        {
          "id": "MDRW2100040094.1.8",
          "form": "아 그러시구나ㅋㅋ 아침형인간 ㅋㅋ부지런하신가봐요ㅋㅋ",
          "original_form": "아 그러시구나ㅋㅋ 아침형인간 ㅋㅋ부지런하신가봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:22"
        },
        {
          "id": "MDRW2100040094.1.9",
          "form": "그냥 식구대로 일찍 자고 일찍 일어나다보니 그렇게 됐네요..ㅎㅎ",
          "original_form": "그냥 식구대로 일찍 자고 일찍 일어나다보니 그렇게 됐네요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 14:05"
        },
        {
          "id": "MDRW2100040094.1.10",
          "form": "대신 일찍 자니까 야식은 다들 안먹어요..;)",
          "original_form": "대신 일찍 자니까 야식은 다들 안먹어요..;)",
          "speaker_id": "2",
          "time": "20210904 14:05"
        },
        {
          "id": "MDRW2100040094.1.11",
          "form": "아 완전좋은데요?ㅋㅋ",
          "original_form": "아 완전좋은데요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100040094.1.12",
          "form": "건강한삶이네요ㅋㅋ",
          "original_form": "건강한삶이네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100040094.1.13",
          "form": "점심은 뭐드셨어요?",
          "original_form": "점심은 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100040094.1.14",
          "form": "호박나물이랑 가지볶음이랑 버섯무침이랑 감자채볶음 막 해서 먹었어요",
          "original_form": "호박나물이랑 가지볶음이랑 버섯무침이랑 감자채볶음 막 해서 먹었어요",
          "speaker_id": "2",
          "time": "20210904 14:12"
        },
        {
          "id": "MDRW2100040094.1.15",
          "form": "와ㅋㅋㅋㅋ",
          "original_form": "와ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100040094.1.16",
          "form": "건강식ㅋㅋ맛있었겠어요ㅋ",
          "original_form": "건강식ㅋㅋ맛있었겠어요ㅋ",
          "speaker_id": "1",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100040094.1.17",
          "form": "저는 삼각김밥해먹었어요ㅋㅋ",
          "original_form": "저는 삼각김밥해먹었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100040094.1.18",
          "form": "집에서요? 능력자네요",
          "original_form": "집에서요? 능력자네요",
          "speaker_id": "2",
          "time": "20210904 14:19"
        },
        {
          "id": "MDRW2100040094.1.19",
          "form": "아 정말 간단한거예요ㅋㅋ",
          "original_form": "아 정말 간단한거예요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:20"
        },
        {
          "id": "MDRW2100040094.1.20",
          "form": "저 반찬들이 더 어렵죠ㅋ",
          "original_form": "저 반찬들이 더 어렵죠ㅋ",
          "speaker_id": "1",
          "time": "20210904 14:21"
        },
        {
          "id": "MDRW2100040094.1.21",
          "form": "그런가요? 제가 못하는 건 어려워보여서요",
          "original_form": "그런가요? 제가 못하는 건 어려워보여서요",
          "speaker_id": "2",
          "time": "20210904 14:21"
        },
        {
          "id": "MDRW2100040094.1.22",
          "form": "한번도 집에서 삼각김밥 만들어 본 적 없는데",
          "original_form": "한번도 집에서 삼각김밥 만들어 본 적 없는데",
          "speaker_id": "2",
          "time": "20210904 14:21"
        },
        {
          "id": "MDRW2100040094.1.23",
          "form": "네 정말 간단해요ㅋㅋ",
          "original_form": "네 정말 간단해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:21"
        }
      ]
    }
