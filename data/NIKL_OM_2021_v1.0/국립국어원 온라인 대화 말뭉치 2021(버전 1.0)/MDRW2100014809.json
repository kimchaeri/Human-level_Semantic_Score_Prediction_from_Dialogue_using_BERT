      "utterance": [
        {
          "id": "MDRW2100014809.1.1",
          "form": "하시는 운동 있으세요?",
          "original_form": "하시는 운동 있으세요?",
          "speaker_id": "1",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014809.1.2",
          "form": "네 전 요가해요 name1님은요?",
          "original_form": "네 전 요가해요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014809.1.3",
          "form": "저는 요즘 스트레칭만 해요",
          "original_form": "저는 요즘 스트레칭만 해요",
          "speaker_id": "1",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014809.1.4",
          "form": "기회되면 수영 배워보고 싶어요",
          "original_form": "기회되면 수영 배워보고 싶어요",
          "speaker_id": "1",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014809.1.5",
          "form": "수영 진짜 재밌어요",
          "original_form": "수영 진짜 재밌어요",
          "speaker_id": "2",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014809.1.6",
          "form": "와 수영할 줄 아세요?",
          "original_form": "와 수영할 줄 아세요?",
          "speaker_id": "1",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014809.1.7",
          "form": "저는 이시국 전까지는했었는데...",
          "original_form": "저는 이시국 전까지는했었는데...",
          "speaker_id": "2",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014809.1.8",
          "form": "네 관둬서 아쉬워요",
          "original_form": "네 관둬서 아쉬워요",
          "speaker_id": "2",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014809.1.9",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014809.1.10",
          "form": "네 근데 1년밖에 못했어요 ㅋㅋ",
          "original_form": "네 근데 1년밖에 못했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014809.1.11",
          "form": "성인 돼서 배우셨나요?",
          "original_form": "성인 돼서 배우셨나요?",
          "speaker_id": "1",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014809.1.12",
          "form": "저 운동신경이 없어서 무탈히 배울 수 있을까 걱정이 있어요ㅋㅋ",
          "original_form": "저 운동신경이 없어서 무탈히 배울 수 있을까 걱정이 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014809.1.13",
          "form": "네 성인 돼서 배웠는데 재밌어요",
          "original_form": "네 성인 돼서 배웠는데 재밌어요",
          "speaker_id": "2",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014809.1.14",
          "form": "어딜가도 기초부터 잘알려줄거에요!!",
          "original_form": "어딜가도 기초부터 잘알려줄거에요!!",
          "speaker_id": "2",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014809.1.15",
          "form": "전 백신맞으면 다시 하려구요 ㅋㅋ",
          "original_form": "전 백신맞으면 다시 하려구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014809.1.16",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014809.1.17",
          "form": "수영 영법 어디까지 배우셨어요?",
          "original_form": "수영 영법 어디까지 배우셨어요?",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014809.1.18",
          "form": "저는 기초영법은 다배웠는데",
          "original_form": "저는 기초영법은 다배웠는데",
          "speaker_id": "2",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014809.1.19",
          "form": "다이빙 배우고싶은데 못배우고 그전까지만 했네요",
          "original_form": "다이빙 배우고싶은데 못배우고 그전까지만 했네요",
          "speaker_id": "2",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014809.1.20",
          "form": "와 다이빙은 무서움도 좀 이겨내야할 것 같아요",
          "original_form": "와 다이빙은 무서움도 좀 이겨내야할 것 같아요",
          "speaker_id": "1",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014809.1.21",
          "form": "맞아요 그거 무섭다는 사람 많더라구요 ㅋㅋ",
          "original_form": "맞아요 그거 무섭다는 사람 많더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014809.1.22",
          "form": "스트레칭은 홈트하시는건가요?",
          "original_form": "스트레칭은 홈트하시는건가요?",
          "speaker_id": "2",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014809.1.23",
          "form": "ㅋㅋ 네 여기저기 소소하게 아픈 데가 많아서 스트레칭 없이 살 수가 없어요",
          "original_form": "ㅋㅋ 네 여기저기 소소하게 아픈 데가 많아서 스트레칭 없이 살 수가 없어요",
          "speaker_id": "1",
