        },
        {
          "id": "MDRW2100000725.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210819 18:45"
        },
        {
          "id": "MDRW2100000725.1.3",
          "form": "뮤지컬이나 연극 좋아하시나요",
          "original_form": "뮤지컬이나 연극 좋아하시나요",
          "speaker_id": "1",
          "time": "20210819 18:46"
        },
        {
          "id": "MDRW2100000725.1.4",
          "form": "뮤지컬은 예전에 종종 봤었어요",
          "original_form": "뮤지컬은 예전에 종종 봤었어요",
          "speaker_id": "2",
          "time": "20210819 18:49"
        },
        {
          "id": "MDRW2100000725.1.5",
          "form": "저도 예전에 서울가서 종종 봤는데",
          "original_form": "저도 예전에 서울가서 종종 봤는데",
          "speaker_id": "1",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100000725.1.6",
          "form": "요즘은 코로나때문에ㅜㅜ",
          "original_form": "요즘은 코로나때문에ㅜㅜ",
          "speaker_id": "1",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100000725.1.7",
          "form": "뮤지컬은 어떤거 보셨어요?",
          "original_form": "뮤지컬은 어떤거 보셨어요?",
          "speaker_id": "2",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100000725.1.8",
          "form": "캣츠인가",
          "original_form": "캣츠인가",
          "speaker_id": "1",
          "time": "20210819 18:59"
        },
        {
          "id": "MDRW2100000725.1.9",
          "form": "고양이 뛰댕기는거 봤습니다",
          "original_form": "고양이 뛰댕기는거 봤습니다",
          "speaker_id": "1",
          "time": "20210819 18:59"
        },
        {
          "id": "MDRW2100000725.1.10",
          "form": "아 그거 재밌죠",
          "original_form": "아 그거 재밌죠",
          "speaker_id": "2",
          "time": "20210819 18:59"
        },
        {
          "id": "MDRW2100000725.1.11",
          "form": "영화로도 나왔잖아요",
          "original_form": "영화로도 나왔잖아요",
          "speaker_id": "2",
          "time": "20210819 18:59"
        },
        {
          "id": "MDRW2100000725.1.12",
          "form": "아 정말요?",
          "original_form": "아 정말요?",
          "speaker_id": "1",
          "time": "20210819 18:59"
        },
        {
          "id": "MDRW2100000725.1.13",
          "form": "한번 봐야겠네요",
          "original_form": "한번 봐야겠네요",
          "speaker_id": "1",
          "time": "20210819 18:59"
        },
        {
          "id": "MDRW2100000725.1.14",
          "form": "괴작이라고 소문이 자자합니다",
          "original_form": "괴작이라고 소문이 자자합니다",
          "speaker_id": "2",
          "time": "20210819 18:59"
        },
        {
          "id": "MDRW2100000725.1.15",
          "form": "고등학생때 반친구들이랑 보고왔던 기억이 있습니다",
          "original_form": "고등학생때 반친구들이랑 보고왔던 기억이 있습니다",
          "speaker_id": "1",
          "time": "20210819 18:59"
        },
        {
          "id": "MDRW2100000725.1.16",
          "form": "괴작이요?",
          "original_form": "괴작이요?",
          "speaker_id": "1",
          "time": "20210819 19:00"
        },
        {
          "id": "MDRW2100000725.1.17",
          "form": "네 이상해요 영화가",
          "original_form": "네 이상해요 영화가",
          "speaker_id": "2",
          "time": "20210819 19:00"
        },
        {
          "id": "MDRW2100000725.1.18",
          "form": "기괴한 작품인가요ㅋㅋㅋㅋ",
          "original_form": "기괴한 작품인가요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 19:00"
        },
        {
          "id": "MDRW2100000725.1.19",
          "form": "바퀴벌래 나오고 막",
          "original_form": "바퀴벌래 나오고 막",
          "speaker_id": "2",
          "time": "20210819 19:00"
        },
        {
          "id": "MDRW2100000725.1.20",
          "form": "뮤지컬은 그렇지 않았던걸로 기억하는데",
          "original_form": "뮤지컬은 그렇지 않았던걸로 기억하는데",
          "speaker_id": "1",
          "time": "20210819 19:00"
        },
        {
          "id": "MDRW2100000725.1.21",
          "form": "ㄷㄷ",
          "original_form": "ㄷㄷ",
          "speaker_id": "1",
          "time": "20210819 19:00"
        },
        {
          "id": "MDRW2100000725.1.22",
          "form": "혼란스러웠습니다. 영화는",
          "original_form": "혼란스러웠습니다. 영화는",
