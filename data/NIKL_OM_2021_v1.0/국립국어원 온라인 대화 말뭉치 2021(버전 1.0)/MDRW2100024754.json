        {
          "id": "MDRW2100024754.1.1",
          "form": "얼큰한 거 먹고 싶네요",
          "original_form": "얼큰한 거 먹고 싶네요",
          "speaker_id": "2",
          "time": "20210831 18:18"
        },
        {
          "id": "MDRW2100024754.1.2",
          "form": "저도요 얼큰한거 뭐 있을까요?",
          "original_form": "저도요 얼큰한거 뭐 있을까요?",
          "speaker_id": "1",
          "time": "20210831 18:33"
        },
        {
          "id": "MDRW2100024754.1.3",
          "form": "라면 김치찌개요",
          "original_form": "라면 김치찌개요",
          "speaker_id": "2",
          "time": "20210831 18:33"
        },
        {
          "id": "MDRW2100024754.1.4",
          "form": "오모리 김치찌개 좋죠",
          "original_form": "오모리 김치찌개 좋죠",
          "speaker_id": "1",
          "time": "20210831 18:34"
        },
        {
          "id": "MDRW2100024754.1.5",
          "form": "라면엔 김밥이죠",
          "original_form": "라면엔 김밥이죠",
          "speaker_id": "2",
          "time": "20210831 18:35"
        },
        {
          "id": "MDRW2100024754.1.6",
          "form": "네 김치찌개 라면에 참치김밥 좋죠",
          "original_form": "네 김치찌개 라면에 참치김밥 좋죠",
          "speaker_id": "1",
          "time": "20210831 18:37"
        },
        {
          "id": "MDRW2100024754.1.7",
          "form": "깔끔하고 얼큰한 국물이 당겨요",
          "original_form": "깔끔하고 얼큰한 국물이 당겨요",
          "speaker_id": "2",
          "time": "20210831 18:37"
        },
        {
          "id": "MDRW2100024754.1.8",
          "form": "네 좋죠 빨간국물",
          "original_form": "네 좋죠 빨간국물",
          "speaker_id": "1",
          "time": "20210831 18:39"
        },
        {
          "id": "MDRW2100024754.1.9",
          "form": "저녁 먹을 시간인데 혹시 주변 맛집 아시나요",
          "original_form": "저녁 먹을 시간인데 혹시 주변 맛집 아시나요",
          "speaker_id": "2",
          "time": "20210831 18:51"
        },
        {
          "id": "MDRW2100024754.1.10",
          "form": "님 주변이어딘데요",
          "original_form": "님 주변이어딘데요",
          "speaker_id": "1",
          "time": "20210831 18:51"
        },
        {
