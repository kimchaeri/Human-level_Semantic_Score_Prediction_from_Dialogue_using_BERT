      "utterance": [
        {
          "id": "MDRW2100034491.1.1",
          "form": "뭐 드시는 영양제",
          "original_form": "뭐 드시는 영양제",
          "speaker_id": "2",
          "time": "20210828 14:01"
        },
        {
          "id": "MDRW2100034491.1.2",
          "form": "있으시나여?",
          "original_form": "있으시나여?",
          "speaker_id": "2",
          "time": "20210828 14:01"
        },
        {
          "id": "MDRW2100034491.1.3",
          "form": "음 저는 유산균이랑 비타민 디 챙겨먹어요~",
          "original_form": "음 저는 유산균이랑 비타민 디 챙겨먹어요~",
          "speaker_id": "1",
          "time": "20210828 14:03"
        },
        {
          "id": "MDRW2100034491.1.4",
          "form": "효과 있는 거 같나요?",
          "original_form": "효과 있는 거 같나요?",
          "speaker_id": "2",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100034491.1.5",
          "form": "솔직히 잘 모르겠어요 ㅋㅋ 그냥 집에 있는김에 먹는거라",
          "original_form": "솔직히 잘 모르겠어요 ㅋㅋ 그냥 집에 있는김에 먹는거라",
          "speaker_id": "1",
          "time": "20210828 14:40"
        },
        {
          "id": "MDRW2100034491.1.6",
          "form": "ㅋㅋㅋㅋ어디거 드시나요",
          "original_form": "ㅋㅋㅋㅋ어디거 드시나요",
          "speaker_id": "2",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100034491.1.7",
          "form": "유산균은 종균당먹고 비타민디는 뉴트리데이? 여기꺼요",
          "original_form": "유산균은 종균당먹고 비타민디는 뉴트리데이? 여기꺼요",
          "speaker_id": "1",
          "time": "20210828 16:07"
        },
        {
          "id": "MDRW2100034491.1.8",
          "form": "오 ~",
          "original_form": "오 ~",
          "speaker_id": "2",
          "time": "20210828 17:21"
        },
        {
          "id": "MDRW2100034491.1.9",
          "form": "저도 한번 알아봐야겠어요",
          "original_form": "저도 한번 알아봐야겠어요",
          "speaker_id": "2",
          "time": "20210828 17:21"
        },
        {
          "id": "MDRW2100034491.1.10",
          "form": "네~ㅋㅋㅋ 챙겨드시는거있어요?",
          "original_form": "네~ㅋㅋㅋ 챙겨드시는거있어요?",
          "speaker_id": "1",
          "time": "20210829 06:47"
        },
        {
          "id": "MDRW2100034491.1.11",
          "form": "전 프로폴리스랑 비타민c 유산균",
          "original_form": "전 프로폴리스랑 비타민c 유산균",
          "speaker_id": "2",
          "time": "20210829 12:02"
        },
        {
          "id": "MDRW2100034491.1.12",
          "form": "가끔이요ㅋㅋㅋㅌ",
          "original_form": "가끔이요ㅋㅋㅋㅌ",
          "speaker_id": "2",
          "time": "20210829 12:02"
        },
        {
          "id": "MDRW2100034491.1.13",
          "form": "아하! 프로폴리스는 어디에좋나요?",
          "original_form": "아하! 프로폴리스는 어디에좋나요?",
          "speaker_id": "1",
          "time": "20210830 10:44"
        },
        {
          "id": "MDRW2100034491.1.14",
          "form": "면역력 키우기에 좋다고 들었어요",
          "original_form": "면역력 키우기에 좋다고 들었어요",
          "speaker_id": "2",
          "time": "20210830 12:02"
        },
        {
          "id": "MDRW2100034491.1.15",
          "form": "감기에 좋죠ㅋㅋ",
          "original_form": "감기에 좋죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 12:02"
        },
        {
          "id": "MDRW2100034491.1.16",
          "form": "아하~ 맞다 듣자하니 목에도 좋다더라구요!",
          "original_form": "아하~ 맞다 듣자하니 목에도 좋다더라구요!",
          "speaker_id": "1",
          "time": "20210830 13:25"
        },
        {
          "id": "MDRW2100034491.1.17",
          "form": "맞아여ㅋㅋㅋㅋㅋㅋㅋ!",
          "original_form": "맞아여ㅋㅋㅋㅋㅋㅋㅋ!",
          "speaker_id": "2",
          "time": "20210830 15:21"
        },
        {
          "id": "MDRW2100034491.1.18",
          "form": "노래 하는 사람들이 잘 먹는거죠!",
          "original_form": "노래 하는 사람들이 잘 먹는거죠!",
