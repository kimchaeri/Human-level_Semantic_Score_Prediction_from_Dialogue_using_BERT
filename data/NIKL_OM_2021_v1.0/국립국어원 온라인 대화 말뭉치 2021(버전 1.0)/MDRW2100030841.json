      "utterance": [
        {
          "id": "MDRW2100030841.1.1",
          "form": "무슨 게임 하시남요?",
          "original_form": "무슨 게임 하시남요?",
          "speaker_id": "1",
          "time": "20210904 13:23"
        },
        {
          "id": "MDRW2100030841.1.2",
          "form": "스타크래프트라고...",
          "original_form": "스타크래프트라고...",
          "speaker_id": "2",
          "time": "20210904 13:24"
        },
        {
          "id": "MDRW2100030841.1.3",
          "form": "고인물게임있는데여",
          "original_form": "고인물게임있는데여",
          "speaker_id": "2",
          "time": "20210904 13:24"
        },
        {
          "id": "MDRW2100030841.1.4",
          "form": "임요환이라고",
          "original_form": "임요환이라고",
          "speaker_id": "2",
          "time": "20210904 13:24"
        },
        {
          "id": "MDRW2100030841.1.5",
          "form": "아ㅋㅋ 알아요!",
          "original_form": "아ㅋㅋ 알아요!",
          "speaker_id": "1",
          "time": "20210904 13:24"
        },
        {
          "id": "MDRW2100030841.1.6",
          "form": "황제",
          "original_form": "황제",
          "speaker_id": "1",
          "time": "20210904 13:25"
        },
        {
          "id": "MDRW2100030841.1.7",
          "form": "아 네 황제가 하는 그게임입니다",
          "original_form": "아 네 황제가 하는 그게임입니다",
          "speaker_id": "2",
          "time": "20210904 13:25"
        },
        {
          "id": "MDRW2100030841.1.8",
          "form": "인류 하시나요?",
          "original_form": "인류 하시나요?",
          "speaker_id": "1",
          "time": "20210904 13:25"
        },
        {
          "id": "MDRW2100030841.1.9",
          "form": "잡종입니다",
          "original_form": "잡종입니다",
          "speaker_id": "2",
          "time": "20210904 13:25"
        },
        {
          "id": "MDRW2100030841.1.10",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:25"
        },
        {
          "id": "MDRW2100030841.1.11",
          "form": "전 균형을 중시하려구요",
          "original_form": "전 균형을 중시하려구요",
          "speaker_id": "2",
          "time": "20210904 13:25"
        },
        {
          "id": "MDRW2100030841.1.12",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "2",
          "time": "20210904 13:25"
        },
        {
          "id": "MDRW2100030841.1.13",
          "form": "ㅎㅎ 다재다능하시군요ㅎㅎ",
          "original_form": "ㅎㅎ 다재다능하시군요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 13:25"
        },
        {
          "id": "MDRW2100030841.1.14",
          "form": "아닙니다 초보입니당 ㅋㅋ",
          "original_form": "아닙니다 초보입니당 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:26"
        },
        {
          "id": "MDRW2100030841.1.15",
          "form": "게임 하셔요?",
          "original_form": "게임 하셔요?",
          "speaker_id": "2",
          "time": "20210904 13:26"
        },
        {
          "id": "MDRW2100030841.1.16",
          "form": "안하시죠?",
          "original_form": "안하시죠?",
          "speaker_id": "2",
          "time": "20210904 13:26"
        },
        {
          "id": "MDRW2100030841.1.17",
          "form": "아뇨ㅎ",
          "original_form": "아뇨ㅎ",
          "speaker_id": "1",
          "time": "20210904 13:26"
        },
        {
          "id": "MDRW2100030841.1.18",
          "form": "안어울림",
          "original_form": "안어울림",
          "speaker_id": "2",
          "time": "20210904 13:26"
        },
        {
          "id": "MDRW2100030841.1.19",
          "form": "맞네용 ㅎㅎ",
          "original_form": "맞네용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 13:26"
        },
        {
          "id": "MDRW2100030841.1.20",
          "form": "ㅎㅎ저 게임 하면 백퍼 폐인될 거 알아서",
          "original_form": "ㅎㅎ저 게임 하면 백퍼 폐인될 거 알아서",
          "speaker_id": "1",
          "time": "20210904 13:26"
        },
        {
          "id": "MDRW2100030841.1.21",
          "form": "시도도 안합니다",
          "original_form": "시도도 안합니다",
          "speaker_id": "1",
          "time": "20210904 13:26"
        },
        {
          "id": "MDRW2100030841.1.22",
          "form": "자제력이 한없이 제로에 수렴해요ㅎㅎ",
          "original_form": "자제력이 한없이 제로에 수렴해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 13:27"
        },
        {
          "id": "MDRW2100030841.1.23",
          "form": "한 우물만 파시나요?",
          "original_form": "한 우물만 파시나요?",
          "speaker_id": "2",
          "time": "20210904 13:27"
        },
        {
          "id": "MDRW2100030841.1.24",
          "form": "네네 그리고 좀 집요하게 파서ㅎㅎ",
          "original_form": "네네 그리고 좀 집요하게 파서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 13:27"
        },
        {
          "id": "MDRW2100030841.1.25",
          "form": "아 그럴것같아요",
          "original_form": "아 그럴것같아요",
          "speaker_id": "2",
          "time": "20210904 13:27"
        },
        {
          "id": "MDRW2100030841.1.26",
          "form": "찍히면 죽는다!",
          "original_form": "찍히면 죽는다!",
          "speaker_id": "2",
          "time": "20210904 13:27"
        },
        {
          "id": "MDRW2100030841.1.27",
          "form": "평화주의자에게 무슨 말씀이시쥬!",
          "original_form": "평화주의자에게 무슨 말씀이시쥬!",
          "speaker_id": "1",
          "time": "20210904 13:28"
        }
      ]
    }
