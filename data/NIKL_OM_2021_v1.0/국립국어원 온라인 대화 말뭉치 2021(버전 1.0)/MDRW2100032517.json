      "utterance": [
        {
          "id": "MDRW2100032517.1.1",
          "form": "좋아 그럼 부산 가서 사는 물건 말하기!!",
          "original_form": "좋아 그럼 부산 가서 사는 물건 말하기!!",
          "speaker_id": "1",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100032517.1.2",
          "form": "음...음...",
          "original_form": "음...음...",
          "speaker_id": "2",
          "time": "20210905 20:00"
        },
        {
          "id": "MDRW2100032517.1.3",
          "form": "책 같은거?",
          "original_form": "책 같은거?",
          "speaker_id": "2",
          "time": "20210905 20:00"
        },
        {
          "id": "MDRW2100032517.1.4",
          "form": "아니면 옷이라던가",
          "original_form": "아니면 옷이라던가",
          "speaker_id": "2",
          "time": "20210905 20:00"
        },
        {
          "id": "MDRW2100032517.1.5",
          "form": "오 주로 무슨 책 사??",
          "original_form": "오 주로 무슨 책 사??",
          "speaker_id": "1",
          "time": "20210905 20:00"
        },
        {
          "id": "MDRW2100032517.1.6",
          "form": "음...예전에는 참고서를 샀었는데...",
          "original_form": "음...예전에는 참고서를 샀었는데...",
          "speaker_id": "2",
          "time": "20210905 20:00"
        },
        {
          "id": "MDRW2100032517.1.7",
          "form": "요즘에는 소설?",
          "original_form": "요즘에는 소설?",
          "speaker_id": "2",
          "time": "20210905 20:01"
        },
        {
          "id": "MDRW2100032517.1.8",
          "form": "뭔가 알라딘 같은 곳도 있으니까",
          "original_form": "뭔가 알라딘 같은 곳도 있으니까",
          "speaker_id": "2",
          "time": "20210905 20:01"
        },
        {
          "id": "MDRW2100032517.1.9",
          "form": "나도 시내 가면 주로 알라딘 가는데",
          "original_form": "나도 시내 가면 주로 알라딘 가는데",
          "speaker_id": "1",
          "time": "20210905 20:01"
        },
        {
          "id": "MDRW2100032517.1.10",
          "form": "헉!!",
          "original_form": "헉!!",
          "speaker_id": "2",
          "time": "20210905 20:01"
        },
        {
          "id": "MDRW2100032517.1.11",
          "form": "알라딘 뭔가 신기해!",
          "original_form": "알라딘 뭔가 신기해!",
          "speaker_id": "2",
          "time": "20210905 20:01"
        },
        {
          "id": "MDRW2100032517.1.12",
          "form": "중고서점 구경 넘 재밌어 ㅋㅋㅋ",
          "original_form": "중고서점 구경 넘 재밌어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:02"
        },
        {
          "id": "MDRW2100032517.1.13",
          "form": "마자마자!!",
          "original_form": "마자마자!!",
          "speaker_id": "2",
          "time": "20210905 20:03"
        },
        {
          "id": "MDRW2100032517.1.14",
          "form": "거제에는 없어가지고ㅠㅠ",
          "original_form": "거제에는 없어가지고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 20:03"
        },
        {
          "id": "MDRW2100032517.1.15",
          "form": "온라인으로 중고거래는 안 해??",
          "original_form": "온라인으로 중고거래는 안 해??",
          "speaker_id": "1",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100032517.1.16",
          "form": "온라인은 있지만 오프라인이 뭔가 좋더라구~!",
          "original_form": "온라인은 있지만 오프라인이 뭔가 좋더라구~!",
          "speaker_id": "2",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100032517.1.17",
          "form": "나는 온라인으로 사는 경우가 더 많은듯",
          "original_form": "나는 온라인으로 사는 경우가 더 많은듯",
          "speaker_id": "1",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100032517.1.18",
          "form": "파는 것도 그렇고",
          "original_form": "파는 것도 그렇고",
          "speaker_id": "1",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100032517.1.19",
          "form": "오옹",
          "original_form": "오옹",
          "speaker_id": "2",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100032517.1.20",
          "form": "아무래도 온라인이 편하니까",
          "original_form": "아무래도 온라인이 편하니까",
          "speaker_id": "2",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100032517.1.21",
          "form": "사실 알라딘 구경 재밌긴 한데",
          "original_form": "사실 알라딘 구경 재밌긴 한데",
          "speaker_id": "1",
          "time": "20210905 22:34"
        },
        {
          "id": "MDRW2100032517.1.22",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210905 22:34"
        },
        {
          "id": "MDRW2100032517.1.23",
          "form": "오프라인 매장엔 내가 원하는 책이 없을때가 많아",
          "original_form": "오프라인 매장엔 내가 원하는 책이 없을때가 많아",
          "speaker_id": "1",
          "time": "20210905 22:34"
        },
        {
          "id": "MDRW2100032517.1.24",
          "form": "아앗ㅠㅜㅠ그렇지...",
          "original_form": "아앗ㅠㅜㅠ그렇지...",
          "speaker_id": "2",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032517.1.25",
          "form": "아 혹시 링월드라는 소설 알아??",
          "original_form": "아 혹시 링월드라는 소설 알아??",
          "speaker_id": "1",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032517.1.26",
          "form": "절판돼서 한 4-5만 원에 팔리는 책인데",
          "original_form": "절판돼서 한 4-5만 원에 팔리는 책인데",
          "speaker_id": "1",
          "time": "20210905 22:36"
        },
        {
          "id": "MDRW2100032517.1.27",
          "form": "나 그거 알라딘 동성로에서 1900원에 샀어 ㅋㅋㅋ",
          "original_form": "나 그거 알라딘 동성로에서 1900원에 샀어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:36"
        },
        {
          "id": "MDRW2100032517.1.28",
          "form": "헐 대박",
          "original_form": "헐 대박",
          "speaker_id": "2",
          "time": "20210905 22:36"
        },
        {
          "id": "MDRW2100032517.1.29",
          "form": "어떤 책인지는 모르지만",
          "original_form": "어떤 책인지는 모르지만",
          "speaker_id": "2",
          "time": "20210905 22:36"
        },
        {
          "id": "MDRW2100032517.1.30",
          "form": "싸게 겟했구나!",
          "original_form": "싸게 겟했구나!",
          "speaker_id": "2",
          "time": "20210905 22:36"
        },
        {
          "id": "MDRW2100032517.1.31",
          "form": "귀한 책인거 모르고",
          "original_form": "귀한 책인거 모르고",
          "speaker_id": "1",
          "time": "20210905 22:37"
        },
        {
          "id": "MDRW2100032517.1.32",
          "form": "ㅋㅋㅋㅋㄱㄱㅋㄱㅋㄱㅋㅋㄱㄲ",
          "original_form": "ㅋㅋㅋㅋㄱㄱㅋㄱㅋㄱㅋㅋㄱㄲ",
          "speaker_id": "2",
          "time": "20210905 22:38"
        },
        {
          "id": "MDRW2100032517.1.33",
          "form": "헌책이라고 싸게 팔았나봐",
          "original_form": "헌책이라고 싸게 팔았나봐",
          "speaker_id": "1",
          "time": "20210905 22:38"
        },
        {
          "id": "MDRW2100032517.1.34",
          "form": "그런가봐ㅋㄲㅋ",
          "original_form": "그런가봐ㅋㄲㅋ",
