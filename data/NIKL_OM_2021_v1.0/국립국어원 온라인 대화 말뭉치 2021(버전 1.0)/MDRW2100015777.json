        },
        {
          "id": "MDRW2100015777.1.2",
          "form": "안녕하세요 반가워요",
          "original_form": "안녕하세요 반가워요",
          "speaker_id": "2",
          "time": "20210826 00:58"
        },
        {
          "id": "MDRW2100015777.1.3",
          "form": "여기는 습하네요",
          "original_form": "여기는 습하네요",
          "speaker_id": "1",
          "time": "20210826 00:58"
        },
        {
          "id": "MDRW2100015777.1.4",
          "form": "맞아요 오늘 너무 습하고 더웠어요",
          "original_form": "맞아요 오늘 너무 습하고 더웠어요",
          "speaker_id": "2",
          "time": "20210826 00:58"
        },
        {
          "id": "MDRW2100015777.1.5",
          "form": "그래도 이제 아침 저녁으론 쌀쌀하네요",
          "original_form": "그래도 이제 아침 저녁으론 쌀쌀하네요",
          "speaker_id": "2",
          "time": "20210826 00:58"
        },
        {
          "id": "MDRW2100015777.1.6",
          "form": "제습기 열심히 돌아가네요",
          "original_form": "제습기 열심히 돌아가네요",
          "speaker_id": "1",
          "time": "20210826 00:59"
        },
        {
          "id": "MDRW2100015777.1.7",
          "form": "장마철에 제습기가 열일하네요",
          "original_form": "장마철에 제습기가 열일하네요",
          "speaker_id": "2",
          "time": "20210826 01:00"
        },
        {
          "id": "MDRW2100015777.1.8",
          "form": "태풍이 지나가서 다행이에요",
          "original_form": "태풍이 지나가서 다행이에요",
          "speaker_id": "2",
          "time": "20210826 01:00"
        },
        {
          "id": "MDRW2100015777.1.9",
          "form": "좋아하는 계절있나요",
          "original_form": "좋아하는 계절있나요",
          "speaker_id": "1",
          "time": "20210826 01:01"
        },
        {
          "id": "MDRW2100015777.1.10",
          "form": "저는 여름빼고 다 좋아해요 name1님은요?",
          "original_form": "저는 여름빼고 다 좋아해요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 01:01"
        },
        {
          "id": "MDRW2100015777.1.11",
          "form": "저는 여름을 좋아해요",
          "original_form": "저는 여름을 좋아해요",
          "speaker_id": "1",
          "time": "20210826 01:02"
        },
        {
          "id": "MDRW2100015777.1.12",
          "form": "앗 그러시군요 더위를 즐기시나봐요",
          "original_form": "앗 그러시군요 더위를 즐기시나봐요",
          "speaker_id": "2",
          "time": "20210826 01:02"
        },
        {
          "id": "MDRW2100015777.1.13",
          "form": "추은게 싫어서요",
          "original_form": "추은게 싫어서요",
          "speaker_id": "1",
          "time": "20210826 01:04"
        },
        {
          "id": "MDRW2100015777.1.14",
          "form": "이번 겨울엔 적당히 추웠으면 좋겠네요",
          "original_form": "이번 겨울엔 적당히 추웠으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210826 01:04"
        },
