        },
        {
          "id": "MDRW2100009374.1.2",
          "form": "종교 있으세요?",
          "original_form": "종교 있으세요?",
          "speaker_id": "1",
          "time": "20210822 23:02"
        },
        {
          "id": "MDRW2100009374.1.3",
          "form": "안녕하세요~ 전 없어요 name1님은요",
          "original_form": "안녕하세요~ 전 없어요 &name1&님은요",
          "speaker_id": "2",
          "time": "20210822 23:02"
        },
        {
          "id": "MDRW2100009374.1.4",
          "form": "사실 저도요",
          "original_form": "사실 저도요",
          "speaker_id": "1",
          "time": "20210822 23:02"
        },
        {
          "id": "MDRW2100009374.1.5",
          "form": "무교예요",
          "original_form": "무교예요",
          "speaker_id": "1",
          "time": "20210822 23:02"
        },
        {
          "id": "MDRW2100009374.1.6",
          "form": "원래 무교셨어요?",
          "original_form": "원래 무교셨어요?",
          "speaker_id": "1",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009374.1.7",
          "form": "앗 네 저두요",
          "original_form": "앗 네 저두요",
          "speaker_id": "2",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009374.1.8",
          "form": "아니면 원래는 종교가 있으셨어요?",
          "original_form": "아니면 원래는 종교가 있으셨어요?",
          "speaker_id": "1",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009374.1.9",
          "form": "저는 없었어요. 님은요?",
          "original_form": "저는 없었어요. 님은요?",
          "speaker_id": "2",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009374.1.10",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009374.1.11",
          "form": "그래서 종교 시설이 궁금해요",
          "original_form": "그래서 종교 시설이 궁금해요",
          "speaker_id": "1",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009374.1.12",
          "form": "주변에 종교 있는 사람은 있으세요?",
          "original_form": "주변에 종교 있는 사람은 있으세요?",
          "speaker_id": "1",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009374.1.13",
          "form": "가족들도 종교가 없구 저는 접점이 없네요",
          "original_form": "가족들도 종교가 없구 저는 접점이 없네요",
          "speaker_id": "2",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009374.1.14",
          "form": "저도요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100009374.1.15",
          "form": "종교 시설 가보셨나요?",
          "original_form": "종교 시설 가보셨나요?",
          "speaker_id": "2",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100009374.1.16",
          "form": "불국사도 절이라면",
          "original_form": "불국사도 절이라면",
          "speaker_id": "1",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100009374.1.17",
          "form": "얼마 전에 다녀왔어요",
          "original_form": "얼마 전에 다녀왔어요",
          "speaker_id": "1",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100009374.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100009374.1.19",
          "form": "종교 시설 가보셨어요?",
          "original_form": "종교 시설 가보셨어요?",
          "speaker_id": "1",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100009374.1.20",
          "form": "절은ㅋㅋㅋ",
          "original_form": "절은ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100009374.1.21",
          "form": "등산이나 여행 가면 가구 그래요",
          "original_form": "등산이나 여행 가면 가구 그래요",
          "speaker_id": "2",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100009374.1.22",
          "form": "뭔가 종교시설 느낌은 아닌..",
          "original_form": "뭔가 종교시설 느낌은 아닌..",
          "speaker_id": "2",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100009374.1.23",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100009374.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:09"
        },
        {
          "id": "MDRW2100009374.1.25",
          "form": "절은 다른 종교시설과 다르게",
          "original_form": "절은 다른 종교시설과 다르게",
          "speaker_id": "1",
          "time": "20210822 23:09"
        },
        {
          "id": "MDRW2100009374.1.26",
          "form": "약간 그냥 가도 될 것 같은 느낌이 있어요",
          "original_form": "약간 그냥 가도 될 것 같은 느낌이 있어요",
          "speaker_id": "1",
          "time": "20210822 23:09"
        },
        {
          "id": "MDRW2100009374.1.27",
          "form": "뭔가 편안한 느낌?",
          "original_form": "뭔가 편안한 느낌?",
          "speaker_id": "1",
          "time": "20210822 23:09"
        },
        {
          "id": "MDRW2100009374.1.28",
          "form": "가끔 가서 밥도 먹어요",
          "original_form": "가끔 가서 밥도 먹어요",
          "speaker_id": "1",
          "time": "20210822 23:09"
        },
        {
          "id": "MDRW2100009374.1.29",
          "form": "절에서 밥 드셔 보셨어요?",
          "original_form": "절에서 밥 드셔 보셨어요?",
          "speaker_id": "1",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009374.1.30",
          "form": "오 넵 등산가서 비빔밥 먹어봤어요",
          "original_form": "오 넵 등산가서 비빔밥 먹어봤어요",
          "speaker_id": "2",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009374.1.31",
          "form": "밥 드셔보셨나요?",
          "original_form": "밥 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210822 23:10"
        },
