      "utterance": [
        {
          "id": "MDRW2100023170.1.1",
          "form": "맞아 그래서 난 축구 관련 영상 많이 봐",
          "original_form": "맞아 그래서 난 축구 관련 영상 많이 봐",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100023170.1.2",
          "form": "구독하는 유튜브가 있어?",
          "original_form": "구독하는 유튜브가 있어?",
          "speaker_id": "1",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100023170.1.3",
          "form": "응  있어",
          "original_form": "응  있어",
          "speaker_id": "2",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100023170.1.4",
          "form": "그냥 수다떠는 거야 아니면 좀 전문적으로 분석해주는거야?",
          "original_form": "그냥 수다떠는 거야 아니면 좀 전문적으로 분석해주는거야?",
          "speaker_id": "1",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100023170.1.5",
          "form": "둘 다 ㅎㅎ",
          "original_form": "둘 다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100023170.1.6",
          "form": "야구채널은 보는 거 없오?",
          "original_form": "야구채널은 보는 거 없오?",
          "speaker_id": "1",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100023170.1.7",
          "form": "야구는 딱히..",
          "original_form": "야구는 딱히..",
          "speaker_id": "2",
          "time": "20210829 22:30"
        },
        {
          "id": "MDRW2100023170.1.8",
          "form": "야구는 딱히",
          "original_form": "야구는 딱히",
          "speaker_id": "2",
          "time": "20210829 22:33"
        },
        {
          "id": "MDRW2100023170.1.9",
          "form": "그래? 야구도 분석할 게 많잖아",
          "original_form": "그래? 야구도 분석할 게 많잖아",
          "speaker_id": "1",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100023170.1.10",
          "form": "근데 축구가 더 재밌더라고",
          "original_form": "근데 축구가 더 재밌더라고",
          "speaker_id": "2",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100023170.1.11",
          "form": "야구도 분석할 게 많잖어",
          "original_form": "야구도 분석할 게 많잖어",
          "speaker_id": "1",
          "time": "20210829 22:37"
        },
        {
          "id": "MDRW2100023170.1.12",
          "form": "그렇긴 한데 축구가 더 다양한 느낌이야",
          "original_form": "그렇긴 한데 축구가 더 다양한 느낌이야",
          "speaker_id": "2",
          "time": "20210829 22:38"
        },
        {
          "id": "MDRW2100023170.1.13",
          "form": "그래? 전술같은거?",
          "original_form": "그래? 전술같은거?",
          "speaker_id": "1",
          "time": "20210829 22:40"
        },
        {
          "id": "MDRW2100023170.1.14",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210829 22:40"
        },
        {
          "id": "MDRW2100023170.1.15",
