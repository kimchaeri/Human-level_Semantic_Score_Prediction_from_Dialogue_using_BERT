      "utterance": [
        {
          "id": "MDRW2100022237.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210829 11:29"
        },
        {
          "id": "MDRW2100022237.1.2",
          "form": "안녕하세요^^ 혹시 백신 맞으셨거나 예약하셨나요?",
          "original_form": "안녕하세요^^ 혹시 백신 맞으셨거나 예약하셨나요?",
          "speaker_id": "2",
          "time": "20210829 11:40"
        },
        {
          "id": "MDRW2100022237.1.3",
          "form": "사전예약 했습니다!",
          "original_form": "사전예약 했습니다!",
          "speaker_id": "1",
          "time": "20210829 12:51"
        },
        {
          "id": "MDRW2100022237.1.4",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "2",
          "time": "20210829 12:55"
        },
        {
          "id": "MDRW2100022237.1.5",
          "form": "저는 9월 8일 접종인데 언제세요?",
          "original_form": "저는 9월 8일 접종인데 언제세요?",
          "speaker_id": "2",
          "time": "20210829 12:55"
        },
        {
          "id": "MDRW2100022237.1.6",
          "form": "전 9.9일이요!",
          "original_form": "전 9.9일이요!",
          "speaker_id": "1",
          "time": "20210829 13:00"
        },
        {
          "id": "MDRW2100022237.1.7",
          "form": "ㅠㅠ저 바로 다음날이네요",
          "original_form": "ㅠㅠ저 바로 다음날이네요",
          "speaker_id": "2",
          "time": "20210829 13:16"
        },
        {
          "id": "MDRW2100022237.1.8",
          "form": "화이자? 모더나? 뭐 맞으시나요?",
          "original_form": "화이자? 모더나? 뭐 맞으시나요?",
          "speaker_id": "2",
          "time": "20210829 13:16"
        },
        {
          "id": "MDRW2100022237.1.9",
          "form": "둘중에 아직 정해지지 않은것같아요ㅜ",
          "original_form": "둘중에 아직 정해지지 않은것같아요ㅜ",
          "speaker_id": "1",
          "time": "20210829 13:18"
        },
        {
          "id": "MDRW2100022237.1.10",
          "form": "저도요ㅠㅠ신청할때는 화이자라더니",
          "original_form": "저도요ㅠㅠ신청할때는 화이자라더니",
          "speaker_id": "2",
          "time": "20210829 13:20"
        },
        {
          "id": "MDRW2100022237.1.11",
          "form": "신청하고나니 둘중 하나래요",
          "original_form": "신청하고나니 둘중 하나래요",
          "speaker_id": "2",
          "time": "20210829 13:20"
        },
        {
          "id": "MDRW2100022237.1.12",
          "form": "모더나는 탈모 부작용 있을수 있다해서 맞기싫은데..",
          "original_form": "모더나는 탈모 부작용 있을수 있다해서 맞기싫은데..",
          "speaker_id": "2",
          "time": "20210829 13:20"
        },
        {
          "id": "MDRW2100022237.1.13",
          "form": "코로나 때문에 난리도 아니네요ㅠ",
          "original_form": "코로나 때문에 난리도 아니네요ㅠ",
          "speaker_id": "2",
          "time": "20210829 13:21"
        },
        {
          "id": "MDRW2100022237.1.14",
          "form": "아 탈모요..? 너무 싫네요",
          "original_form": "아 탈모요..? 너무 싫네요",
          "speaker_id": "1",
          "time": "20210829 13:22"
        },
        {
          "id": "MDRW2100022237.1.15",
          "form": "네ㅠㅠ 혹시 운동은 하시는거 있으세요?",
          "original_form": "네ㅠㅠ 혹시 운동은 하시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210829 13:31"
        },
        {
          "id": "MDRW2100022237.1.16",
          "form": "홈트 해보려구요!",
          "original_form": "홈트 해보려구요!",
          "speaker_id": "1",
          "time": "20210829 13:34"
        },
        {
          "id": "MDRW2100022237.1.17",
          "form": "저도 홈트 해보려고 시도해봤는데 영상보고 따라하기가 쉽지않더라구요ㅠ",
          "original_form": "저도 홈트 해보려고 시도해봤는데 영상보고 따라하기가 쉽지않더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210829 13:34"
        },
        {
          "id": "MDRW2100022237.1.18",
          "form": "제대로 하는지도 모르겠고",
          "original_form": "제대로 하는지도 모르겠고",
          "speaker_id": "2",
          "time": "20210829 13:34"
        },
        {
          "id": "MDRW2100022237.1.19",
          "form": "오 어떤거하셨나요운동",
          "original_form": "오 어떤거하셨나요운동",
          "speaker_id": "1",
          "time": "20210829 13:37"
        }
      ]
