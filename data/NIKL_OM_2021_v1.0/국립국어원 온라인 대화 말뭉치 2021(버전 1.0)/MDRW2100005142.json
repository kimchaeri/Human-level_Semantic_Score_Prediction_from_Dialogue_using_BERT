      "utterance": [
        {
          "id": "MDRW2100005142.1.1",
          "form": "요리하는거 좋아하시나요?",
          "original_form": "요리하는거 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005142.1.2",
          "form": "요리 귀찮아서 잘 안해먹어용",
          "original_form": "요리 귀찮아서 잘 안해먹어용",
          "speaker_id": "2",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005142.1.3",
          "form": "name1님은여?",
          "original_form": "&name1&님은여?",
          "speaker_id": "2",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005142.1.4",
          "form": "전 가끔 분식이나 찌개  요런건 해먹어요",
          "original_form": "전 가끔 분식이나 찌개  요런건 해먹어요",
          "speaker_id": "1",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005142.1.5",
          "form": "삼겹살도 구워먹고",
          "original_form": "삼겹살도 구워먹고",
          "speaker_id": "1",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005142.1.6",
          "form": "오 그래도 잘해드시네요",
          "original_form": "오 그래도 잘해드시네요",
          "speaker_id": "2",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005142.1.7",
          "form": "저는 부대찌개나 제육볶음 밀키트로 나온거 가끔 먹어용",
          "original_form": "저는 부대찌개나 제육볶음 밀키트로 나온거 가끔 먹어용",
          "speaker_id": "2",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005142.1.8",
          "form": "요즘 밀키트 잘나오더라구요",
          "original_form": "요즘 밀키트 잘나오더라구요",
          "speaker_id": "1",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100005142.1.9",
          "form": "그춍",
          "original_form": "그춍",
          "speaker_id": "2",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100005142.1.10",
          "form": "편하고 재료도 다들어있어서",
          "original_form": "편하고 재료도 다들어있어서",
          "speaker_id": "1",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100005142.1.11",
          "form": "맛도 있고 편하더라고요",
          "original_form": "맛도 있고 편하더라고요",
          "speaker_id": "2",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100005142.1.12",
          "form": "얼마전에 신림백순대 밀키트팔길래 샀는데",
          "original_form": "얼마전에 신림백순대 밀키트팔길래 샀는데",
          "speaker_id": "1",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100005142.1.13",
          "form": "진짜 신림서 먹는 맛이더라구요",
          "original_form": "진짜 신림서 먹는 맛이더라구요",
          "speaker_id": "1",
          "time": "20210821 21:03"
        },
        {
          "id": "MDRW2100005142.1.14",
          "form": "헉 정말요? 나중에 먹어볼게여",
          "original_form": "헉 정말요? 나중에 먹어볼게여",
          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005142.1.15",
          "form": "맛있어요 기회되면 사서 드셔보세요",
          "original_form": "맛있어요 기회되면 사서 드셔보세요",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005142.1.16",
          "form": "그래야겠어용",
          "original_form": "그래야겠어용",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005142.1.17",
          "form": "오늘은 뭐드셨어요?",
          "original_form": "오늘은 뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005142.1.18",
          "form": "낮에 감자고로케랑 안주야 곱창볶음ㅎ 좀전에 토마토에 모짜렐라치즈넣고 발사믹소스에 비벼먹었어요ㅎ",
          "original_form": "낮에 감자고로케랑 안주야 곱창볶음ㅎ 좀전에 토마토에 모짜렐라치즈넣고 발사믹소스에 비벼먹었어요ㅎ",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005142.1.19",
          "form": "저녁은 뭐드셨어요?",
          "original_form": "저녁은 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005142.1.20",
          "form": "오 맛있는거 드셨네요",
          "original_form": "오 맛있는거 드셨네요",
          "speaker_id": "2",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005142.1.21",
          "form": "저는 치즈라볶이 먹었어요! 카스테라랑",
          "original_form": "저는 치즈라볶이 먹었어요! 카스테라랑",
          "speaker_id": "2",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005142.1.22",
          "form": "떡볶이에 카스테라요? ㅎ 조합이 특이하네요",
          "original_form": "떡볶이에 카스테라요? ㅎ 조합이 특이하네요",
          "speaker_id": "1",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005142.1.23",
          "form": "따로따로 먹었어요ㅋㅋㅋㅋㅋ카스테라는 간식이영",
          "original_form": "따로따로 먹었어요ㅋㅋㅋㅋㅋ카스테라는 간식이영",
          "speaker_id": "2",
          "time": "20210821 21:50"
        }
      ]
    }
