      "utterance": [
        {
          "id": "MDRW2100042022.1.1",
          "form": "아티라미수사먹고싶다",
          "original_form": "아티라미수사먹고싶다",
          "speaker_id": "2",
          "time": "20210906 18:49"
        },
        {
          "id": "MDRW2100042022.1.2",
          "form": "브라우니는 뚜레쥬르가맛있대요",
          "original_form": "브라우니는 뚜레쥬르가맛있대요",
          "speaker_id": "2",
          "time": "20210906 18:49"
        },
        {
          "id": "MDRW2100042022.1.3",
          "form": "연산동 친구네 가게에 티라미수 맛있대",
          "original_form": "연산동 친구네 가게에 티라미수 맛있대",
          "speaker_id": "1",
          "time": "20210906 18:49"
        },
        {
          "id": "MDRW2100042022.1.4",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210906 18:49"
        },
        {
          "id": "MDRW2100042022.1.5",
          "form": "아 뚜레주르 브라우니 한번도 못봤어",
          "original_form": "아 뚜레주르 브라우니 한번도 못봤어",
          "speaker_id": "1",
          "time": "20210906 18:49"
        },
        {
          "id": "MDRW2100042022.1.6",
          "form": "맛잇겟당",
          "original_form": "맛잇겟당",
          "speaker_id": "2",
          "time": "20210906 18:49"
        },
        {
          "id": "MDRW2100042022.1.7",
          "form": "거기 바닐라러떼도 맛나대",
          "original_form": "거기 바닐라러떼도 맛나대",
          "speaker_id": "1",
          "time": "20210906 18:49"
        },
        {
          "id": "MDRW2100042022.1.8",
          "form": "연산 이마트 뒤인데 담에 같이 가자",
          "original_form": "연산 이마트 뒤인데 담에 같이 가자",
          "speaker_id": "1",
          "time": "20210906 18:49"
        },
        {
          "id": "MDRW2100042022.1.9",
          "form": "오오오오",
          "original_form": "오오오오",
          "speaker_id": "2",
          "time": "20210906 18:49"
        },
        {
          "id": "MDRW2100042022.1.10",
          "form": "우리 밀면도 먹었는데 그 근처에서",
          "original_form": "우리 밀면도 먹었는데 그 근처에서",
          "speaker_id": "1",
          "time": "20210906 18:49"
        },
        {
          "id": "MDRW2100042022.1.11",
          "form": "이마트출첵중인데",
          "original_form": "이마트출첵중인데",
          "speaker_id": "2",
          "time": "20210906 18:49"
        },
        {
          "id": "MDRW2100042022.1.12",
          "form": "이마트갈일이없어서",
          "original_form": "이마트갈일이없어서",
          "speaker_id": "2",
          "time": "20210906 18:49"
        },
        {
          "id": "MDRW2100042022.1.13",
          "form": "포인트계속소멸됨ㅋㅋㅋㅋ",
          "original_form": "포인트계속소멸됨ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:49"
        },
        {
          "id": "MDRW2100042022.1.14",
          "form": "아하 이마트도 그런거하는군",
          "original_form": "아하 이마트도 그런거하는군",
          "speaker_id": "1",
          "time": "20210906 18:50"
        },
        {
          "id": "MDRW2100042022.1.15",
          "form": "요샌 안가서인지 쿠폰 안주네",
          "original_form": "요샌 안가서인지 쿠폰 안주네",
          "speaker_id": "1",
          "time": "20210906 18:50"
        },
        {
