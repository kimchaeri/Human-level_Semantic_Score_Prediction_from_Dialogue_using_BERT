        },
        {
          "id": "MDRW2100025223.1.2",
          "form": "미안 잊어버렷엇내",
          "original_form": "미안 잊어버렷엇내",
          "speaker_id": "1",
          "time": "20210901 01:19"
        },
        {
          "id": "MDRW2100025223.1.3",
          "form": "빨리좀 사주셔요",
          "original_form": "빨리좀 사주셔요",
          "speaker_id": "2",
          "time": "20210901 01:20"
        },
        {
          "id": "MDRW2100025223.1.4",
          "form": "네넵 딴건또 ?",
          "original_form": "네넵 딴건또 ?",
          "speaker_id": "1",
          "time": "20210901 01:20"
        },
        {
          "id": "MDRW2100025223.1.5",
          "form": "아직은 웂는뎅",
          "original_form": "아직은 웂는뎅",
          "speaker_id": "2",
          "time": "20210901 01:20"
        },
        {
          "id": "MDRW2100025223.1.6",
          "form": "생각나면말해",
          "original_form": "생각나면말해",
          "speaker_id": "1",
          "time": "20210901 01:21"
        },
        {
          "id": "MDRW2100025223.1.7",
          "form": "아아 스트레칭 되는고 그거 사죠",
          "original_form": "아아 스트레칭 되는고 그거 사죠",
          "speaker_id": "2",
          "time": "20210901 01:21"
        },
        {
          "id": "MDRW2100025223.1.8",
          "form": "스트레칭? 어떤종류",
          "original_form": "스트레칭? 어떤종류",
          "speaker_id": "1",
          "time": "20210901 01:21"
        },
        {
          "id": "MDRW2100025223.1.9",
          "form": "서있기만 해도 스트레칭 되는거잇어",
          "original_form": "서있기만 해도 스트레칭 되는거잇어",
          "speaker_id": "2",
          "time": "20210901 01:21"
        },
        {
          "id": "MDRW2100025223.1.10",
          "form": "아 그래 검색해볼게",
          "original_form": "아 그래 검색해볼게",
          "speaker_id": "1",
          "time": "20210901 01:21"
        },
        {
          "id": "MDRW2100025223.1.11",
          "form": "가격 저렴해  쿠팡에서봣엉",
