      "utterance": [
        {
          "id": "MDRW2100020927.1.1",
          "form": "전시회 좋아하세요?",
          "original_form": "전시회 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100020927.1.2",
          "form": "좋아하긴 하는데 시간이 없어서 자주 못가게 되네요",
          "original_form": "좋아하긴 하는데 시간이 없어서 자주 못가게 되네요",
          "speaker_id": "1",
          "time": "20210828 14:58"
        },
        {
          "id": "MDRW2100020927.1.3",
          "form": "최근에는 이건희 콜렉션 가고싶더라구요...",
          "original_form": "최근에는 이건희 콜렉션 가고싶더라구요...",
          "speaker_id": "1",
          "time": "20210828 14:58"
        },
        {
          "id": "MDRW2100020927.1.4",
          "form": "name2님은요?!;",
          "original_form": "&name2&님은요?!;",
          "speaker_id": "1",
          "time": "20210828 14:58"
        },
        {
          "id": "MDRW2100020927.1.5",
          "form": "아니 왜자꾸 ;이 쳐질까요 ㅋㅋㅋㅋ",
          "original_form": "아니 왜자꾸 ;이 쳐질까요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:58"
        },
        {
          "id": "MDRW2100020927.1.6",
          "form": "이건희 콜렉션이 뭐에요??",
          "original_form": "이건희 콜렉션이 뭐에요??",
          "speaker_id": "2",
          "time": "20210828 15:48"
        },
        {
          "id": "MDRW2100020927.1.7",
          "form": "삼성 이건희 맞아요??",
          "original_form": "삼성 이건희 맞아요??",
          "speaker_id": "2",
          "time": "20210828 15:48"
        },
        {
          "id": "MDRW2100020927.1.8",
          "form": "네네!!!",
          "original_form": "네네!!!",
          "speaker_id": "1",
          "time": "20210828 15:59"
        },
        {
          "id": "MDRW2100020927.1.9",
          "form": "그 국립중앙박물관에서",
          "original_form": "그 국립중앙박물관에서",
          "speaker_id": "1",
          "time": "20210828 15:59"
        },
        {
          "id": "MDRW2100020927.1.10",
          "form": "이건희 콜렉션을 하는데",
          "original_form": "이건희 콜렉션을 하는데",
          "speaker_id": "1",
          "time": "20210828 15:59"
        },
        {
          "id": "MDRW2100020927.1.11",
          "form": "무료예요!!! 대신 수강신청만큼 빡세다고 하더라구요",
          "original_form": "무료예요!!! 대신 수강신청만큼 빡세다고 하더라구요",
          "speaker_id": "1",
          "time": "20210828 15:59"
        },
        {
          "id": "MDRW2100020927.1.12",
          "form": "이 정말요??",
          "original_form": "이 정말요??",
          "speaker_id": "2",
          "time": "20210828 17:38"
        },
        {
          "id": "MDRW2100020927.1.13",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210828 17:38"
        },
        {
          "id": "MDRW2100020927.1.14",
          "form": "그림 전시화인가봐요",
          "original_form": "그림 전시화인가봐요",
          "speaker_id": "2",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100020927.1.15",
          "form": "그림 관심 많으세요?",
          "original_form": "그림 관심 많으세요?",
          "speaker_id": "2",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100020927.1.16",
          "form": "네네!! 그림도 있고",
          "original_form": "네네!! 그림도 있고",
          "speaker_id": "1",
          "time": "20210828 17:45"
        },
        {
          "id": "MDRW2100020927.1.17",
          "form": "조각도 있고 그렇다던데",
          "original_form": "조각도 있고 그렇다던데",
          "speaker_id": "1",
          "time": "20210828 17:45"
        },
        {
          "id": "MDRW2100020927.1.18",
          "form": "나라에 기증하는 대신",
          "original_form": "나라에 기증하는 대신",
          "speaker_id": "1",
          "time": "20210828 17:45"
        },
        {
          "id": "MDRW2100020927.1.19",
          "form": "삼성가 세금 감면해주는 그런 조건이었을거예요 ㅎㅎ",
          "original_form": "삼성가 세금 감면해주는 그런 조건이었을거예요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 17:45"
        },
        {
          "id": "MDRW2100020927.1.20",
          "form": "그림은... 사실 그렇게 관심이 많지는 않은데",
          "original_form": "그림은... 사실 그렇게 관심이 많지는 않은데",
          "speaker_id": "1",
          "time": "20210828 17:45"
        },
        {
          "id": "MDRW2100020927.1.21",
          "form": "아앜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아앜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:45"
        },
        {
          "id": "MDRW2100020927.1.22",
          "form": "그런 고상한 취미가 있으면",
          "original_form": "그런 고상한 취미가 있으면",
          "speaker_id": "1",
          "time": "20210828 17:46"
        },
        {
          "id": "MDRW2100020927.1.23",
          "form": "마음이 평화로워질 것 같더라구요,,,",
          "original_form": "마음이 평화로워질 것 같더라구요,,,",
          "speaker_id": "1",
          "time": "20210828 17:46"
        },
        {
          "id": "MDRW2100020927.1.24",
          "form": "아 그쵸",
          "original_form": "아 그쵸",
          "speaker_id": "2",
          "time": "20210828 17:46"
        },
        {
          "id": "MDRW2100020927.1.25",
          "form": "네네! 아무래도 그런 전시회에서는 말도 안하고..",
          "original_form": "네네! 아무래도 그런 전시회에서는 말도 안하고..",
          "speaker_id": "1",
          "time": "20210828 17:46"
        },
        {
          "id": "MDRW2100020927.1.26",
          "form": "클래식 듣고 이런거도 괜찮을거같더라구요",
          "original_form": "클래식 듣고 이런거도 괜찮을거같더라구요",
          "speaker_id": "2",
          "time": "20210828 17:46"
        },
        {
          "id": "MDRW2100020927.1.27",
          "form": "저는 전에 이집트유물 전시한다고 해서 가봤어요",
          "original_form": "저는 전에 이집트유물 전시한다고 해서 가봤어요",
          "speaker_id": "2",
          "time": "20210828 17:47"
        },
        {
          "id": "MDRW2100020927.1.28",
          "form": "파라오랑 고양이조각? 같은게 많더라구요",
          "original_form": "파라오랑 고양이조각? 같은게 많더라구요",
          "speaker_id": "2",
          "time": "20210828 17:48"
        },
        {
          "id": "MDRW2100020927.1.29",
          "form": "가본 전시화 있으세요?",
          "original_form": "가본 전시화 있으세요?",
          "speaker_id": "2",
          "time": "20210828 17:48"
        },
        {
          "id": "MDRW2100020927.1.30",
          "form": "우와 이집트 유물 전시라니",
          "original_form": "우와 이집트 유물 전시라니",
          "speaker_id": "1",
          "time": "20210828 20:02"
        },
        {
          "id": "MDRW2100020927.1.31",
          "form": "진짜 재밌었을 것 같아요",
          "original_form": "진짜 재밌었을 것 같아요",
          "speaker_id": "1",
          "time": "20210828 20:03"
        },
        {
          "id": "MDRW2100020927.1.32",
          "form": "저... 지금은 생각이 안나는데 어렸을 때",
          "original_form": "저... 지금은 생각이 안나는데 어렸을 때",
          "speaker_id": "1",
          "time": "20210828 20:03"
        },
        {
          "id": "MDRW2100020927.1.33",
          "form": "인체의 신비전 본 게",
          "original_form": "인체의 신비전 본 게",
          "speaker_id": "1",
          "time": "20210828 20:03"
        },
        {
          "id": "MDRW2100020927.1.34",
          "form": "아직도 생생하게 기억나요 ㅠㅠㅠㅋㅋㅋㅋㅋ",
          "original_form": "아직도 생생하게 기억나요 ㅠㅠㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:03"
        },
        {
          "id": "MDRW2100020927.1.35",
          "form": "우와 인체의 신비전이요??",
          "original_form": "우와 인체의 신비전이요??",
          "speaker_id": "2",
          "time": "20210829 10:31"
        },
        {
          "id": "MDRW2100020927.1.36",
          "form": "네!! 되게 어렸을 때 갔던건데",
          "original_form": "네!! 되게 어렸을 때 갔던건데",
          "speaker_id": "1",
          "time": "20210829 11:01"
        },
        {
          "id": "MDRW2100020927.1.37",
          "form": "인체 표본이 그대로 있었어요...!",
          "original_form": "인체 표본이 그대로 있었어요...!",
          "speaker_id": "1",
          "time": "20210829 11:01"
        },
        {
          "id": "MDRW2100020927.1.38",
          "form": "헤에엑 기억날만 하네요",
          "original_form": "헤에엑 기억날만 하네요",
          "speaker_id": "2",
          "time": "20210829 11:04"
        },
        {
          "id": "MDRW2100020927.1.39",
          "form": "인체표본이라니..",
          "original_form": "인체표본이라니..",
          "speaker_id": "2",
          "time": "20210829 11:05"
        }
      ]
    }
