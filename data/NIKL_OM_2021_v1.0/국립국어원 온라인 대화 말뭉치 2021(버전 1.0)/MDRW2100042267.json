      "utterance": [
        {
          "id": "MDRW2100042267.1.1",
          "form": "밤공기가 쌀쌀하네요",
          "original_form": "밤공기가 쌀쌀하네요",
          "speaker_id": "2",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100042267.1.2",
          "form": "많이 쌀쌀해졌어요",
          "original_form": "많이 쌀쌀해졌어요",
          "speaker_id": "1",
          "time": "20210906 21:16"
        },
        {
          "id": "MDRW2100042267.1.3",
          "form": "옷정리해야되는데엄두가안ㅇ나네요ㅋㅋ",
          "original_form": "옷정리해야되는데엄두가안ㅇ나네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:16"
        },
        {
          "id": "MDRW2100042267.1.4",
          "form": "네 어느샌가 계절이 바뀌었네요",
          "original_form": "네 어느샌가 계절이 바뀌었네요",
          "speaker_id": "2",
          "time": "20210906 21:16"
        },
        {
          "id": "MDRW2100042267.1.5",
          "form": "저도 옷정리 계획만",
          "original_form": "저도 옷정리 계획만",
          "speaker_id": "2",
          "time": "20210906 21:16"
        },
        {
          "id": "MDRW2100042267.1.6",
          "form": "여름이 금방지나갔어요",
          "original_form": "여름이 금방지나갔어요",
          "speaker_id": "1",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100042267.1.7",
          "form": "크게 더웠던 건 2주정도?",
          "original_form": "크게 더웠던 건 2주정도?",
          "speaker_id": "2",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100042267.1.8",
          "form": "그쵸 옛날엔 여름이 많이힘들었는데",
          "original_form": "그쵸 옛날엔 여름이 많이힘들었는데",
          "speaker_id": "1",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100042267.1.9",
          "form": "나이먹으니참아지네요",
          "original_form": "나이먹으니참아지네요",
          "speaker_id": "1",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100042267.1.10",
          "form": "아 정말요?",
          "original_form": "아 정말요?",
          "speaker_id": "2",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100042267.1.11",
          "form": "전 점점 더위가 힘들던데",
          "original_form": "전 점점 더위가 힘들던데",
          "speaker_id": "2",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100042267.1.12",
          "form": "물론에어컨과함께^^",
          "original_form": "물론에어컨과함께^^",
          "speaker_id": "1",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100042267.1.13",
          "form": "아이가자꾸나가고싶어해서요ㅠ",
          "original_form": "아이가자꾸나가고싶어해서요ㅠ",
          "speaker_id": "1",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100042267.1.14",
          "form": "아 에어컨은 필수죠",
          "original_form": "아 에어컨은 필수죠",
          "speaker_id": "2",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100042267.1.15",
          "form": "애들은 더워도 나가서 놀고 싶어하죠",
          "original_form": "애들은 더워도 나가서 놀고 싶어하죠",
          "speaker_id": "2",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100042267.1.16",
          "form": "4단계초반에유치원도못가고답답하니까ㅠ",
          "original_form": "4단계초반에유치원도못가고답답하니까ㅠ",
          "speaker_id": "1",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100042267.1.17",
          "form": "아 그랬구나ㅠ",
          "original_form": "아 그랬구나ㅠ",
          "speaker_id": "2",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042267.1.18",
          "form": "더운데 아이랑 바깥놀이까지 하려면",
          "original_form": "더운데 아이랑 바깥놀이까지 하려면",
          "speaker_id": "2",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042267.1.19",
          "form": "고생하셨겠어요",
          "original_form": "고생하셨겠어요",
          "speaker_id": "2",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042267.1.20",
          "form": "힘들었어요",
          "original_form": "힘들었어요",
          "speaker_id": "1",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042267.1.21",
          "form": "나갔다오면살빠지드라구요ㅋㅋ",
          "original_form": "나갔다오면살빠지드라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042267.1.22",
          "form": "아 더위다이어트...",
          "original_form": "아 더위다이어트...",
