      "utterance": [
        {
          "id": "MDRW2100039081.1.1",
          "form": "아니요 그냥 먹고살려고 들어요ㅠ",
          "original_form": "아니요 그냥 먹고살려고 들어요ㅠ",
          "speaker_id": "2",
          "time": "20210903 09:42"
        },
        {
          "id": "MDRW2100039081.1.2",
          "form": "자주잇나요 교육",
          "original_form": "자주잇나요 교육",
          "speaker_id": "1",
          "time": "20210903 09:43"
        },
        {
          "id": "MDRW2100039081.1.3",
          "form": "아니요 교육 거의없어요",
          "original_form": "아니요 교육 거의없어요",
          "speaker_id": "2",
          "time": "20210903 09:44"
        },
        {
          "id": "MDRW2100039081.1.4",
          "form": "회사는 일시키느라 바빠서 ㅋㅋ ㅠㅠ",
          "original_form": "회사는 일시키느라 바빠서 ㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 09:44"
        },
        {
          "id": "MDRW2100039081.1.5",
          "form": "ㅋㅋㅋ 일vs교육",
          "original_form": "ㅋㅋㅋ 일vs교육",
          "speaker_id": "1",
          "time": "20210903 09:44"
        },
        {
          "id": "MDRW2100039081.1.6",
          "form": "아 근데 일이 나아요",
          "original_form": "아 근데 일이 나아요",
          "speaker_id": "2",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100039081.1.7",
          "form": "교육피곤해요",
          "original_form": "교육피곤해요",
          "speaker_id": "2",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100039081.1.8",
          "form": "헉...ㅠㅡㅠ",
          "original_form": "헉...ㅠㅡㅠ",
          "speaker_id": "1",
          "time": "20210903 09:47"
        },
        {
          "id": "MDRW2100039081.1.9",
          "form": "졸면 절대안대나요",
          "original_form": "졸면 절대안대나요",
          "speaker_id": "1",
          "time": "20210903 09:47"
        },
        {
          "id": "MDRW2100039081.1.10",
          "form": "ㅋㅋ졸면 나중에 따로 공부해서 숙지해야하니까...흑흑...",
          "original_form": "ㅋㅋ졸면 나중에 따로 공부해서 숙지해야하니까...흑흑...",
          "speaker_id": "2",
          "time": "20210903 09:50"
        },
        {
          "id": "MDRW2100039081.1.11",
          "form": "그치망 교육은 너무 스캐쥴이 빡빳해서 힘들어요",
          "original_form": "그치망 교육은 너무 스캐쥴이 빡빳해서 힘들어요",
          "speaker_id": "2",
          "time": "20210903 09:51"
        },
        {
          "id": "MDRW2100039081.1.12",
          "form": "하루종일 집중해야하니까",
          "original_form": "하루종일 집중해야하니까",
          "speaker_id": "2",
          "time": "20210903 09:51"
        },
        {
          "id": "MDRW2100039081.1.13",
          "form": "아이궁 .. 중요한거구나 흑흑",
          "original_form": "아이궁 .. 중요한거구나 흑흑",
          "speaker_id": "1",
          "time": "20210903 09:51"
        },
        {
          "id": "MDRW2100039081.1.14",
          "form": "좀 나눠하지ㅡㅡ",
          "original_form": "좀 나눠하지ㅡㅡ",
          "speaker_id": "1",
          "time": "20210903 09:51"
        },
        {
          "id": "MDRW2100039081.1.15",
          "form": "넹 그냥 평소 업무면 ㅋㅋ 업무하다가 중간에 커피마시러 가기도하고",
          "original_form": "넹 그냥 평소 업무면 ㅋㅋ 업무하다가 중간에 커피마시러 가기도하고",
          "speaker_id": "2",
          "time": "20210903 09:52"
        },
        {
          "id": "MDRW2100039081.1.16",
          "form": "힘들때 쉬었다가 할수있는데 교육은 죙일 앉아있어야해요",
          "original_form": "힘들때 쉬었다가 할수있는데 교육은 죙일 앉아있어야해요",
          "speaker_id": "2",
          "time": "20210903 09:52"
        },
        {
          "id": "MDRW2100039081.1.17",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 09:52"
        },
        {
          "id": "MDRW2100039081.1.18",
          "form": "쉬는시간 별루 안주나요ㅠ",
          "original_form": "쉬는시간 별루 안주나요ㅠ",
          "speaker_id": "1",
          "time": "20210903 09:53"
        },
        {
          "id": "MDRW2100039081.1.19",
          "form": "지금마침 딱 쉬는시간이네용 50~55분 하고 10분쉬어요ㅠㅠ",
          "original_form": "지금마침 딱 쉬는시간이네용 50~55분 하고 10분쉬어요ㅠㅠ",
          "speaker_id": "2",
