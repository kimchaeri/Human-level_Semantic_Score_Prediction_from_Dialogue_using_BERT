      "utterance": [
        {
          "id": "MDRW2100024547.1.1",
          "form": "솔직히",
          "original_form": "솔직히",
          "speaker_id": "1",
          "time": "20210831 13:52"
        },
        {
          "id": "MDRW2100024547.1.2",
          "form": "마음같아서는",
          "original_form": "마음같아서는",
          "speaker_id": "1",
          "time": "20210831 13:52"
        },
        {
          "id": "MDRW2100024547.1.3",
          "form": "도시 별 가게들도 다 진출입 가능하게 열어주고",
          "original_form": "도시 별 가게들도 다 진출입 가능하게 열어주고",
          "speaker_id": "1",
          "time": "20210831 13:52"
        },
        {
          "id": "MDRW2100024547.1.4",
          "form": "이나즈마는 목욕탕 주인 이런것도 있던데",
          "original_form": "이나즈마는 목욕탕 주인 이런것도 있던데",
          "speaker_id": "1",
          "time": "20210831 13:52"
        },
        {
          "id": "MDRW2100024547.1.5",
          "form": "그런것도 구현해주고",
          "original_form": "그런것도 구현해주고",
          "speaker_id": "1",
          "time": "20210831 13:52"
        },
        {
          "id": "MDRW2100024547.1.6",
          "form": "ㅇㅇ이왕 할거면",
          "original_form": "ㅇㅇ이왕 할거면",
          "speaker_id": "2",
          "time": "20210831 13:52"
        },
        {
          "id": "MDRW2100024547.1.7",
          "form": "찻집에서 차 마실 수 있게 해주고",
          "original_form": "찻집에서 차 마실 수 있게 해주고",
          "speaker_id": "1",
          "time": "20210831 13:52"
        },
        {
          "id": "MDRW2100024547.1.8",
          "form": "했음 좋겠는데",
          "original_form": "했음 좋겠는데",
          "speaker_id": "1",
          "time": "20210831 13:52"
        },
        {
          "id": "MDRW2100024547.1.9",
          "form": "ㅇㅇ이런 게임은",
          "original_form": "ㅇㅇ이런 게임은",
          "speaker_id": "2",
          "time": "20210831 13:52"
        },
        {
          "id": "MDRW2100024547.1.10",
          "form": "남의 집 들어가서",
          "original_form": "남의 집 들어가서",
          "speaker_id": "2",
          "time": "20210831 13:52"
        },
        {
          "id": "MDRW2100024547.1.11",
          "form": "맘대로 터는게",
          "original_form": "맘대로 터는게",
          "speaker_id": "2",
          "time": "20210831 13:52"
        },
        {
          "id": "MDRW2100024547.1.12",
          "form": "재미인데",
          "original_form": "재미인데",
          "speaker_id": "2",
          "time": "20210831 13:52"
        },
        {
          "id": "MDRW2100024547.1.13",
          "form": "그럼 대체 얼마나 데이터를 먹을지 상상도 안가긴 함...",
          "original_form": "그럼 대체 얼마나 데이터를 먹을지 상상도 안가긴 함...",
          "speaker_id": "1",
          "time": "20210831 13:52"
        },
        {
          "id": "MDRW2100024547.1.14",
          "form": "난 적어도",
          "original_form": "난 적어도",
          "speaker_id": "2",
          "time": "20210831 13:52"
        },
        {
          "id": "MDRW2100024547.1.15",
          "form": "모나네 그 봉인되어있던 방",
          "original_form": "모나네 그 봉인되어있던 방",
          "speaker_id": "2",
          "time": "20210831 13:53"
        },
        {
          "id": "MDRW2100024547.1.16",
          "form": "구경은 시켜줄줄........",
          "original_form": "구경은 시켜줄줄........",
          "speaker_id": "2",
          "time": "20210831 13:53"
        },
        {
          "id": "MDRW2100024547.1.17",
          "form": "ㅇㅇ 그니까 ㅋㅋㅋㅋ",
          "original_form": "ㅇㅇ 그니까 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:54"
        },
        {
          "id": "MDRW2100024547.1.18",
          "form": "걍 혼자 냅다 들어가서는",
          "original_form": "걍 혼자 냅다 들어가서는",
          "speaker_id": "2",
          "time": "20210831 13:55"
        },
        {
          "id": "MDRW2100024547.1.19",
          "form": "문 걸어잠그고 안나오더라...",
          "original_form": "문 걸어잠그고 안나오더라...",
          "speaker_id": "2",
          "time": "20210831 13:55"
        },
        {
          "id": "MDRW2100024547.1.20",
          "form": "그니까 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:55"
        },
        {
          "id": "MDRW2100024547.1.21",
          "form": "나도 구경 좀 시켜달라고...",
          "original_form": "나도 구경 좀 시켜달라고...",
          "speaker_id": "1",
          "time": "20210831 13:55"
        },
        {
          "id": "MDRW2100024547.1.22",
          "form": "고트 호텔도 기대했는데",
          "original_form": "고트 호텔도 기대했는데",
          "speaker_id": "1",
          "time": "20210831 13:55"
        },
        {
          "id": "MDRW2100024547.1.23",
          "form": "우인단이 점거하고 문도 안열어줌;;",
          "original_form": "우인단이 점거하고 문도 안열어줌;;",
          "speaker_id": "1",
          "time": "20210831 13:55"
        },
        {
          "id": "MDRW2100024547.1.24",
          "form": "ㅇㅇ......아니",
          "original_form": "ㅇㅇ......아니",
          "speaker_id": "2",
          "time": "20210831 13:56"
        },
        {
          "id": "MDRW2100024547.1.25",
          "form": "그럴거면 왜만든거야",
          "original_form": "그럴거면 왜만든거야",
          "speaker_id": "2",
          "time": "20210831 13:56"
        },
        {
          "id": "MDRW2100024547.1.26",
          "form": "내가 타탈 데려가면",
          "original_form": "내가 타탈 데려가면",
          "speaker_id": "2",
          "time": "20210831 13:56"
        },
        {
          "id": "MDRW2100024547.1.27",
          "form": "열어줄거냐구",
          "original_form": "열어줄거냐구",
          "speaker_id": "2",
          "time": "20210831 13:56"
        },
        {
          "id": "MDRW2100024547.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:57"
        },
        {
          "id": "MDRW2100024547.1.29",
          "form": "아니 좀",
          "original_form": "아니 좀",
          "speaker_id": "2",
          "time": "20210831 13:58"
        },
        {
          "id": "MDRW2100024547.1.30",
          "form": "타탈한테",
          "original_form": "타탈한테",
          "speaker_id": "2",
          "time": "20210831 13:58"
        },
        {
          "id": "MDRW2100024547.1.31",
          "form": "고트호텔 우인단이",
          "original_form": "고트호텔 우인단이",
          "speaker_id": "2",
          "time": "20210831 13:58"
        },
        {
          "id": "MDRW2100024547.1.32",
