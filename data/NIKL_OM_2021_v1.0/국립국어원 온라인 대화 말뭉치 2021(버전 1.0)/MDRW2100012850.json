      "utterance": [
        {
          "id": "MDRW2100012850.1.1",
          "form": "그 이준기 나온?",
          "original_form": "그 이준기 나온?",
          "speaker_id": "2",
          "time": "20210825 02:46"
        },
        {
          "id": "MDRW2100012850.1.2",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210825 02:46"
        },
        {
          "id": "MDRW2100012850.1.3",
          "form": "뒤로 갈수록 볼만 하더라고요",
          "original_form": "뒤로 갈수록 볼만 하더라고요",
          "speaker_id": "1",
          "time": "20210825 02:46"
        },
        {
          "id": "MDRW2100012850.1.4",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "2",
          "time": "20210825 02:46"
        },
        {
          "id": "MDRW2100012850.1.5",
          "form": "추천하시나요?",
          "original_form": "추천하시나요?",
          "speaker_id": "2",
          "time": "20210825 02:46"
        },
        {
          "id": "MDRW2100012850.1.6",
          "form": "음 그 정도는 아니에요 ㅋㅋㅋㅋ",
          "original_form": "음 그 정도는 아니에요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 02:46"
        },
        {
          "id": "MDRW2100012850.1.7",
          "form": "그냥 시간 때울정도?",
          "original_form": "그냥 시간 때울정도?",
          "speaker_id": "1",
          "time": "20210825 02:46"
        },
        {
          "id": "MDRW2100012850.1.8",
          "form": "아하 ㅎㅎ",
          "original_form": "아하 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 02:46"
        },
        {
          "id": "MDRW2100012850.1.9",
          "form": "다음엔 뭐보세요?",
          "original_form": "다음엔 뭐보세요?",
          "speaker_id": "2",
          "time": "20210825 02:47"
        },
        {
          "id": "MDRW2100012850.1.10",
          "form": "일단 보던 영화",
          "original_form": "일단 보던 영화",
          "speaker_id": "1",
          "time": "20210825 02:47"
        },
        {
          "id": "MDRW2100012850.1.11",
          "form": "죽음보다 무서운 비밀 더 볼 것 같아요",
          "original_form": "죽음보다 무서운 비밀 더 볼 것 같아요",
          "speaker_id": "1",
          "time": "20210825 02:47"
        },
        {
          "id": "MDRW2100012850.1.12",
          "form": "무슨 영화에요?",
          "original_form": "무슨 영화에요?",
          "speaker_id": "2",
          "time": "20210825 02:47"
        },
        {
          "id": "MDRW2100012850.1.13",
          "form": "좀 옛날 영환데",
          "original_form": "좀 옛날 영환데",
          "speaker_id": "1",
          "time": "20210825 02:47"
        },
        {
          "id": "MDRW2100012850.1.14",
          "form": "저도 아직 잘 모르겠어요",
          "original_form": "저도 아직 잘 모르겠어요",
          "speaker_id": "1",
          "time": "20210825 02:47"
        },
        {
          "id": "MDRW2100012850.1.15",
          "form": "주제가 뭔지 ㅋㅋ",
          "original_form": "주제가 뭔지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 02:47"
        },
        {
          "id": "MDRW2100012850.1.16",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210825 02:47"
        },
        {
          "id": "MDRW2100012850.1.17",
          "form": "그것도 보고 평가 공유해주세요",
          "original_form": "그것도 보고 평가 공유해주세요",
          "speaker_id": "2",
          "time": "20210825 02:47"
        },
        {
          "id": "MDRW2100012850.1.18",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210825 02:48"
        },
        {
          "id": "MDRW2100012850.1.19",
          "form": "근데 아직은 별로 재미없어요",
          "original_form": "근데 아직은 별로 재미없어요",
          "speaker_id": "1",
          "time": "20210825 02:48"
        },
        {
          "id": "MDRW2100012850.1.20",
          "form": "아쉽네요",
          "original_form": "아쉽네요",
          "speaker_id": "2",
          "time": "20210825 02:48"
        },
        {
          "id": "MDRW2100012850.1.21",
          "form": "네 그래서 그냥 자기전에",
          "original_form": "네 그래서 그냥 자기전에",
          "speaker_id": "1",
          "time": "20210825 02:48"
        },
        {
          "id": "MDRW2100012850.1.22",
          "form": "틀어놓고 금방 자요 ㅋㅋㅋ",
          "original_form": "틀어놓고 금방 자요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 02:48"
        },
        {
          "id": "MDRW2100012850.1.23",
          "form": "그런 효과가  ㅎㅎㅎ",
          "original_form": "그런 효과가  ㅎㅎㅎ",
