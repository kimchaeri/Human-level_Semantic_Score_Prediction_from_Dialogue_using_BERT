      "utterance": [
        {
          "id": "MDRW2100032201.1.1",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "1",
          "time": "20210905 14:02"
        },
        {
          "id": "MDRW2100032201.1.2",
          "form": "네 떡국먹었습니다 님은요?",
          "original_form": "네 떡국먹었습니다 님은요?",
          "speaker_id": "2",
          "time": "20210905 14:04"
        },
        {
          "id": "MDRW2100032201.1.3",
          "form": "오 설날분이기 ㅋㅋ 전 만두국이요 ㅋㅋㅋ",
          "original_form": "오 설날분이기 ㅋㅋ 전 만두국이요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:04"
        },
        {
          "id": "MDRW2100032201.1.4",
          "form": "비슷하네요 만두도 넣어먹었거든요",
          "original_form": "비슷하네요 만두도 넣어먹었거든요",
          "speaker_id": "2",
          "time": "20210905 14:06"
        },
        {
          "id": "MDRW2100032201.1.5",
          "form": "직접하셨나요?!",
          "original_form": "직접하셨나요?!",
          "speaker_id": "1",
          "time": "20210905 14:06"
        },
        {
          "id": "MDRW2100032201.1.6",
          "form": "와이프가요..",
          "original_form": "와이프가요..",
          "speaker_id": "2",
          "time": "20210905 14:07"
        },
        {
          "id": "MDRW2100032201.1.7",
          "form": "ㅋㅋㅋ 가끔 요리 해주시나요?",
          "original_form": "ㅋㅋㅋ 가끔 요리 해주시나요?",
          "speaker_id": "1",
          "time": "20210905 14:08"
        },
        {
          "id": "MDRW2100032201.1.8",
          "form": "네 종종합니다 와이프가 해산물 손질을 안좋아해서 그걸 주로 해요",
          "original_form": "네 종종합니다 와이프가 해산물 손질을 안좋아해서 그걸 주로 해요",
          "speaker_id": "2",
          "time": "20210905 15:46"
        },
        {
          "id": "MDRW2100032201.1.9",
          "form": "그렇군요! 자상하시네요! 요리 잘하시겠어요!",
          "original_form": "그렇군요! 자상하시네요! 요리 잘하시겠어요!",
          "speaker_id": "1",
          "time": "20210905 16:34"
        },
        {
          "id": "MDRW2100032201.1.10",
          "form": "비교적 손쉽게 하는 편이예요",
          "original_form": "비교적 손쉽게 하는 편이예요",
          "speaker_id": "2",
          "time": "20210905 16:47"
        },
        {
          "id": "MDRW2100032201.1.11",
          "form": "자신있는 요리가 있으신가요?",
          "original_form": "자신있는 요리가 있으신가요?",
          "speaker_id": "1",
          "time": "20210905 17:00"
        },
        {
          "id": "MDRW2100032201.1.12",
          "form": "전복요리 자주 해요",
          "original_form": "전복요리 자주 해요",
          "speaker_id": "2",
          "time": "20210905 17:04"
        },
        {
          "id": "MDRW2100032201.1.13",
          "form": "오 전복구이 많이들 먹는거 같아요! 회로도 먹구요",
          "original_form": "오 전복구이 많이들 먹는거 같아요! 회로도 먹구요",
          "speaker_id": "1",
          "time": "20210905 17:41"
        },
        {
          "id": "MDRW2100032201.1.14",
          "form": "네 저는.전복찜과 전복밥을 자주 해먹어요",
          "original_form": "네 저는.전복찜과 전복밥을 자주 해먹어요",
          "speaker_id": "2",
          "time": "20210905 18:49"
        },
        {
          "id": "MDRW2100032201.1.15",
          "form": "다른 조개류도 좋아하세요?",
          "original_form": "다른 조개류도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 19:17"
        },
        {
          "id": "MDRW2100032201.1.16",
