      "utterance": [
        {
          "id": "MDRW2100041864.1.1",
          "form": "안녕하세요 여행 좋아하시나요?",
          "original_form": "안녕하세요 여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210906 16:46"
        },
        {
          "id": "MDRW2100041864.1.2",
          "form": "저는좋아해요~",
          "original_form": "저는좋아해요~",
          "speaker_id": "2",
          "time": "20210906 16:59"
        },
        {
          "id": "MDRW2100041864.1.3",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 16:59"
        },
        {
          "id": "MDRW2100041864.1.4",
          "form": "네 여행 좋아해요!",
          "original_form": "네 여행 좋아해요!",
          "speaker_id": "1",
          "time": "20210906 17:00"
        },
        {
          "id": "MDRW2100041864.1.5",
          "form": "해외여행 국내여행 어디가 좋으세요?",
          "original_form": "해외여행 국내여행 어디가 좋으세요?",
          "speaker_id": "1",
          "time": "20210906 17:00"
        },
        {
          "id": "MDRW2100041864.1.6",
          "form": "해외가좋지만 요새는 어렵네요ㅎㅎ",
          "original_form": "해외가좋지만 요새는 어렵네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 17:00"
        },
        {
          "id": "MDRW2100041864.1.7",
          "form": "그러게요 코로나맘 아니면 갔을텐데",
          "original_form": "그러게요 코로나맘 아니면 갔을텐데",
          "speaker_id": "1",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100041864.1.8",
          "form": "다녀온 곳 중에 어디가 좋으셨나요?",
          "original_form": "다녀온 곳 중에 어디가 좋으셨나요?",
          "speaker_id": "1",
          "time": "20210906 17:38"
        },
        {
          "id": "MDRW2100041864.1.9",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041864.1.10",
          "form": "저는런던이랑라오스요~",
          "original_form": "저는런던이랑라오스요~",
          "speaker_id": "2",
          "time": "20210906 17:39"
        },
        {
          "id": "MDRW2100041864.1.11",
          "form": "둘다 전 안 가본 곳이에요 각각 뭐가 좋으셨나요?",
          "original_form": "둘다 전 안 가본 곳이에요 각각 뭐가 좋으셨나요?",
          "speaker_id": "1",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100041864.1.12",
          "form": "런던은 유럽이 처음이라 좋았고 라오스는 첫사랑이랑 같이가서 좋았어요",
          "original_form": "런던은 유럽이 처음이라 좋았고 라오스는 첫사랑이랑 같이가서 좋았어요{emoji:❤}",
          "speaker_id": "2",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041864.1.13",
          "form": "좋으셨겠네요!",
          "original_form": "좋으셨겠네요!",
          "speaker_id": "1",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100041864.1.14",
          "form": "좋았어요 기억에남는여행지는어디세요?",
          "original_form": "좋았어요 기억에남는여행지는어디세요?",
          "speaker_id": "2",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100041864.1.15",
          "form": "태국 여행이 기억에 남아요! 썸남이랑 가서 그랬는짘ㅋㅋ 근데 다녀와서 깨졌어요!",
          "original_form": "태국 여행이 기억에 남아요! 썸남이랑 가서 그랬는짘ㅋㅋ 근데 다녀와서 깨졌어요!",
          "speaker_id": "1",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100041864.1.16",
          "form": "엇아앗...여행때재밌었음됬죠!",
          "original_form": "엇아앗...여행때재밌었음됬죠!",
          "speaker_id": "2",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100041864.1.17",
          "form": "네 ㅋㅋㅋ 태국 자체는 재밌었어요~",
          "original_form": "네 ㅋㅋㅋ 태국 자체는 재밌었어요~",
