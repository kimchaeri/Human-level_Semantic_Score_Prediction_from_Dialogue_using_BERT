      "utterance": [
        {
          "id": "MDRW2100041966.1.1",
          "form": "넥슨 입사하게되면 끝내주겠네요",
          "original_form": "넥슨 입사하게되면 끝내주겠네요",
          "speaker_id": "1",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041966.1.2",
          "form": "레알요.... 욕은 먹어도 복지는 진짜 좋던데",
          "original_form": "레알요.... 욕은 먹어도 복지는 진짜 좋던데",
          "speaker_id": "2",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100041966.1.3",
          "form": "나름 큰기업이긴 하니까요..",
          "original_form": "나름 큰기업이긴 하니까요..",
          "speaker_id": "1",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100041966.1.4",
          "form": "부럽다 ....!",
          "original_form": "부럽다 ....!",
          "speaker_id": "1",
          "time": "20210906 18:02"
        },
        {
          "id": "MDRW2100041966.1.5",
          "form": "전 항상 응원합니다",
          "original_form": "전 항상 응원합니다",
          "speaker_id": "2",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041966.1.6",
          "form": "그리고 제 계정으로 죽데",
          "original_form": "그리고 제 계정으로 죽데",
          "speaker_id": "2",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041966.1.7",
          "form": "기다립니다",
          "original_form": "기다립니다",
          "speaker_id": "2",
          "time": "20210906 18:03"
        },
        {
          "id": "MDRW2100041966.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 아놔",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 아놔",
          "speaker_id": "1",
          "time": "20210906 18:04"
        },
        {
          "id": "MDRW2100041966.1.9",
          "form": "너무나도 불법이고 개발쪽으로가야  되겠어요",
          "original_form": "너무나도 불법이고 개발쪽으로가야  되겠어요",
          "speaker_id": "1",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100041966.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ 아쉽네요 ^^ 개발쪽이면.... 엄청 구르겠네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 아쉽네요 ^^ 개발쪽이면.... 엄청 구르겠네요",
          "speaker_id": "2",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041966.1.11",
          "form": "아니면 기획쪽으로 가서\r\n검토하겠습니다 남발하기",
          "original_form": "아니면 기획쪽으로 가서\r\n검토하겠습니다 남발하기",
          "speaker_id": "1",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041966.1.12",
          "form": "아놔 ㅋㅋ",
          "original_form": "아놔 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100041966.1.13",
          "form": "그럼... name3씨가 되는 건가요? ㄷㄷ",
          "original_form": "그럼... &name3&씨가 되는 건가요? ㄷㄷ",
          "speaker_id": "2",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100041966.1.14",
          "form": "와 진짜 강해보이는 이름인데요",
          "original_form": "와 진짜 강해보이는 이름인데요",
          "speaker_id": "1",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100041966.1.15",
          "form": "\"name3\" 기획팀장입니다.",
          "original_form": "\"&name3&\" 기획팀장입니다.",
          "speaker_id": "2",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100041966.1.16",
          "form": "게임 말고는 어느 쪽 생각하고 계세요?",
          "original_form": "게임 말고는 어느 쪽 생각하고 계세요?",
          "speaker_id": "2",
          "time": "20210906 18:13"
        },
        {
          "id": "MDRW2100041966.1.17",
          "form": "게임말고는 적당히 사무직같은곳 생각중이랄지..",
          "original_form": "게임말고는 적당히 사무직같은곳 생각중이랄지..",
          "speaker_id": "1",
          "time": "20210906 18:14"
        },
        {
          "id": "MDRW2100041966.1.18",
          "form": "막 여기가야지!!! 이런건 없는상태네요",
          "original_form": "막 여기가야지!!! 이런건 없는상태네요",
          "speaker_id": "1",
          "time": "20210906 18:14"
        },
        {
          "id": "MDRW2100041966.1.19",
          "form": "아하 ㅜㅜ 어디든 복지 좋고 봉급도 좋은 곳 붙으셨음 좋겠네요",
          "original_form": "아하 ㅜㅜ 어디든 복지 좋고 봉급도 좋은 곳 붙으셨음 좋겠네요",
          "speaker_id": "2",
          "time": "20210906 18:15"
        },
        {
          "id": "MDRW2100041966.1.20",
          "form": "하 ..  그러게요",
          "original_form": "하 ..  그러게요",
          "speaker_id": "1",
          "time": "20210906 18:15"
        },
        {
          "id": "MDRW2100041966.1.21",
          "form": "복지도 봉급도 좋지만.. 사람좋은게 제일 큰듯해요",
          "original_form": "복지도 봉급도 좋지만.. 사람좋은게 제일 큰듯해요",
          "speaker_id": "1",
          "time": "20210906 18:15"
        },
        {
          "id": "MDRW2100041966.1.22",
          "form": "맞죠..... 근데 어디 사람 때문에 고생하신 분이 있었나봐요??",
          "original_form": "맞죠..... 근데 어디 사람 때문에 고생하신 분이 있었나봐요??",
          "speaker_id": "2",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100041966.1.23",
          "form": "저는 아니고 친구가ㅋㅋㅋㅋ",
          "original_form": "저는 아니고 친구가ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041966.1.24",
          "form": "아하 이런;;",
          "original_form": "아하 이런;;",
          "speaker_id": "2",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041966.1.25",
          "form": "원장 성격이 별로라서 열받아있거든요",
          "original_form": "원장 성격이 별로라서 열받아있거든요",
          "speaker_id": "1",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041966.1.26",
          "form": "어휴... 고생하시겠네요",
          "original_form": "어휴... 고생하시겠네요",
          "speaker_id": "2",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041966.1.27",
          "form": "그래서 말 들을때마다 참\r\n사람이 중요하구나 싶어요",
          "original_form": "그래서 말 들을때마다 참\r\n사람이 중요하구나 싶어요",
          "speaker_id": "1",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100041966.1.28",
          "form": "ㅜㅜ 그것도 그렇네요..... 일하는 환경이 싫으면.. 많이 버는 것도 뭐....",
          "original_form": "ㅜㅜ 그것도 그렇네요..... 일하는 환경이 싫으면.. 많이 버는 것도 뭐....",
          "speaker_id": "2",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100041966.1.29",
          "form": "진짜 사람이 중요하더라구요",
          "original_form": "진짜 사람이 중요하더라구요",
          "speaker_id": "1",
          "time": "20210906 18:20"
        },
        {
          "id": "MDRW2100041966.1.30",
          "form": "워라밸이 괜히 나온말이 아니랄지..",
          "original_form": "워라밸이 괜히 나온말이 아니랄지..",
          "speaker_id": "1",
          "time": "20210906 18:21"
        }
      ]
