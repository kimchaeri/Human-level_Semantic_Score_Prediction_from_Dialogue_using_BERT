      "utterance": [
        {
          "id": "MDRW2100013373.1.1",
          "form": "좋아하는 동물이 있으십니까?",
          "original_form": "좋아하는 동물이 있으십니까?",
          "speaker_id": "1",
          "time": "20210825 14:15"
        },
        {
          "id": "MDRW2100013373.1.2",
          "form": "좋아하는 동물 있으신가오",
          "original_form": "좋아하는 동물 있으신가오",
          "speaker_id": "1",
          "time": "20210825 14:15"
        },
        {
          "id": "MDRW2100013373.1.3",
          "form": "있죠 저는 요즘 앵무새 좋아해요",
          "original_form": "있죠 저는 요즘 앵무새 좋아해요",
          "speaker_id": "2",
          "time": "20210825 14:15"
        },
        {
          "id": "MDRW2100013373.1.4",
          "form": "앗 답해주셨구나",
          "original_form": "앗 답해주셨구나",
          "speaker_id": "1",
          "time": "20210825 14:56"
        },
        {
          "id": "MDRW2100013373.1.5",
          "form": "앵무새 어떤점에 매력을 느끼셨나요?",
          "original_form": "앵무새 어떤점에 매력을 느끼셨나요?",
          "speaker_id": "1",
          "time": "20210825 14:56"
        },
        {
          "id": "MDRW2100013373.1.6",
          "form": "유튜브를 봤거든요",
          "original_form": "유튜브를 봤거든요",
          "speaker_id": "2",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100013373.1.7",
          "form": "앵무새 4마리가 매력이 다 달라요ㅋㅋ",
          "original_form": "앵무새 4마리가 매력이 다 달라요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100013373.1.8",
          "form": "어떻게 다르던가요?",
          "original_form": "어떻게 다르던가요?",
          "speaker_id": "1",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013373.1.9",
          "form": "성격이야 새마다 다르겠지만",
          "original_form": "성격이야 새마다 다르겠지만",
          "speaker_id": "1",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013373.1.10",
          "form": "이 점에 꽂혔다 하는거 있나요",
          "original_form": "이 점에 꽂혔다 하는거 있나요",
          "speaker_id": "1",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013373.1.11",
          "form": "모든 앵무새가 다 말을 잘하는건 아니더라구요",
          "original_form": "모든 앵무새가 다 말을 잘하는건 아니더라구요",
          "speaker_id": "2",
          "time": "20210825 15:10"
        },
        {
          "id": "MDRW2100013373.1.12",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "1",
          "time": "20210825 15:10"
        },
        {
          "id": "MDRW2100013373.1.13",
          "form": "잘 하는 새도 있고 안하는 새도 있어요",
          "original_form": "잘 하는 새도 있고 안하는 새도 있어요",
          "speaker_id": "2",
          "time": "20210825 15:10"
        },
        {
          "id": "MDRW2100013373.1.14",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "1",
          "time": "20210825 15:10"
        },
        {
          "id": "MDRW2100013373.1.15",
          "form": "앵무새는 다 말을 하는 줄 알았어요",
          "original_form": "앵무새는 다 말을 하는 줄 알았어요",
          "speaker_id": "1",
          "time": "20210825 15:10"
        },
        {
          "id": "MDRW2100013373.1.16",
          "form": "사람 말을 따라하는 새도 있고 의성어만 따라하는 새도 있어요",
          "original_form": "사람 말을 따라하는 새도 있고 의성어만 따라하는 새도 있어요",
          "speaker_id": "2",
          "time": "20210825 15:10"
        },
        {
          "id": "MDRW2100013373.1.17",
          "form": "앵무새도 그렇게 갈리는구나",
          "original_form": "앵무새도 그렇게 갈리는구나",
          "speaker_id": "1",
          "time": "20210825 15:11"
        },
        {
          "id": "MDRW2100013373.1.18",
          "form": "넹 저도 그런줄 알았어요ㅋㅋㅋㅋ",
          "original_form": "넹 저도 그런줄 알았어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:11"
        },
        {
          "id": "MDRW2100013373.1.19",
          "form": "처음 안 사실이 놀랍네요",
          "original_form": "처음 안 사실이 놀랍네요",
          "speaker_id": "1",
          "time": "20210825 15:11"
        },
        {
          "id": "MDRW2100013373.1.20",
          "form": "소리지르는 앵무새도 있어요ㅋㅋㅋㅋ",
          "original_form": "소리지르는 앵무새도 있어요ㅋㅋㅋㅋ",
