        {
          "id": "MDRW2100011592.1.1",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100011592.1.2",
          "form": "네 좋아합니다!",
          "original_form": "네 좋아합니다!",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011592.1.3",
          "form": "ㅎ 어떤거 좋아하세요?",
          "original_form": "ㅎ 어떤거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 14:24"
        },
        {
          "id": "MDRW2100011592.1.4",
          "form": "전 게임은 잘 몰라요",
          "original_form": "전 게임은 잘 몰라요",
          "speaker_id": "2",
          "time": "20210824 14:24"
        },
        {
          "id": "MDRW2100011592.1.5",
          "form": "쿠키런이런종류만 몇개알아요",
          "original_form": "쿠키런이런종류만 몇개알아요",
          "speaker_id": "2",
          "time": "20210824 14:24"
        },
        {
          "id": "MDRW2100011592.1.6",
          "form": "오 쿠키런 예전언 했었는데",
          "original_form": "오 쿠키런 예전언 했었는데",
          "speaker_id": "1",
          "time": "20210824 14:26"
        },
        {
          "id": "MDRW2100011592.1.7",
          "form": "저도 초기에 몇번~~ 돈모아서 캐릭터바꾸고하느라 쫌 했었죠",
          "original_form": "저도 초기에 몇번~~ 돈모아서 캐릭터바꾸고하느라 쫌 했었죠",
          "speaker_id": "2",
          "time": "20210824 14:27"
        },
        {
          "id": "MDRW2100011592.1.8",
          "form": "애니팡도 했었고ㅎㅎ",
          "original_form": "애니팡도 했었고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 14:28"
        },
        {
          "id": "MDRW2100011592.1.9",
          "form": "아~~ ㅎ 그것도 밤새면서 했었네요. 친구한테 하트보내고^^",
          "original_form": "아~~ ㅎ 그것도 밤새면서 했었네요. 친구한테 하트보내고^^",
          "speaker_id": "2",
          "time": "20210824 14:29"
        },
        {
          "id": "MDRW2100011592.1.10",
          "form": "추억의 게임들이네요ㅎㅎ",
          "original_form": "추억의 게임들이네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 14:29"
        },
        {
          "id": "MDRW2100011592.1.11",
          "form": "그러게요^^",
          "original_form": "그러게요^^",
          "speaker_id": "2",
          "time": "20210824 14:30"
        },
        {
          "id": "MDRW2100011592.1.12",
          "form": "요즘은아예 게임 안하시나요",
          "original_form": "요즘은아예 게임 안하시나요",
          "speaker_id": "1",
          "time": "20210824 17:33"
        },
        {
          "id": "MDRW2100011592.1.13",
          "form": "네. 한번빠지면 잠도안자고해서 아예 안해요",
          "original_form": "네. 한번빠지면 잠도안자고해서 아예 안해요",
          "speaker_id": "2",
          "time": "20210824 17:34"
        },
