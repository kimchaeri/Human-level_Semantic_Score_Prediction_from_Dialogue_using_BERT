        },
        {
          "id": "MDRW2100000660.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000660.1.3",
          "form": "어떤 게임 좋아하세용?",
          "original_form": "어떤 게임 좋아하세용?",
          "speaker_id": "2",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000660.1.4",
          "form": "저는요즘 게임은 잘안하는데 예전에는 꿈의정원 한창 했었어요ㅎㅎ",
          "original_form": "저는요즘 게임은 잘안하는데 예전에는 꿈의정원 한창 했었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 17:28"
        },
        {
          "id": "MDRW2100000660.1.5",
          "form": "뭔지아시나요?ㅎ",
          "original_form": "뭔지아시나요?ㅎ",
          "speaker_id": "1",
          "time": "20210819 17:28"
        },
        {
          "id": "MDRW2100000660.1.6",
          "form": "아니요ㅎㅎ모바일게임인가요?",
          "original_form": "아니요ㅎㅎ모바일게임인가요?",
          "speaker_id": "2",
          "time": "20210819 17:28"
        },
        {
          "id": "MDRW2100000660.1.7",
          "form": "단순게임 깰때마다 ㅎㅎ 집꾸미는 아이템을 받아서 꾸미는거였어요ㅎ",
          "original_form": "단순게임 깰때마다 ㅎㅎ 집꾸미는 아이템을 받아서 꾸미는거였어요ㅎ",
          "speaker_id": "1",
          "time": "20210819 17:29"
        },
        {
          "id": "MDRW2100000660.1.8",
          "form": "ㅋㅋㅋ오! 약간 동물의 숲게임같은 건가요?",
          "original_form": "ㅋㅋㅋ오! 약간 동물의 숲게임같은 건가요?",
          "speaker_id": "2",
          "time": "20210819 17:30"
        },
        {
          "id": "MDRW2100000660.1.9",
          "form": "전 옛날에 심즈나 문명.. 롤러코스터했어요ㅋㅋ",
          "original_form": "전 옛날에 심즈나 문명.. 롤러코스터했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:30"
        },
        {
          "id": "MDRW2100000660.1.10",
          "form": "앗비슷해요ㅋㅋㅋ",
          "original_form": "앗비슷해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:31"
        },
        {
          "id": "MDRW2100000660.1.11",
          "form": "ㅋㅋㅋㅋㅋ옛날사람들게임이군여",
          "original_form": "ㅋㅋㅋㅋㅋ옛날사람들게임이군여",
          "speaker_id": "2",
          "time": "20210819 17:31"
        },
        {
          "id": "MDRW2100000660.1.12",
          "form": "요새는 배그쪼금해요",
          "original_form": "요새는 배그쪼금해요",
          "speaker_id": "2",
          "time": "20210819 17:32"
        },
        {
          "id": "MDRW2100000660.1.13",
          "form": "제가 옛날사람이라는게 들켰네용",
          "original_form": "제가 옛날사람이라는게 들켰네용",
          "speaker_id": "1",
          "time": "20210819 17:32"
        },
        {
          "id": "MDRW2100000660.1.14",
          "form": "ㅋㅋㅋㅋㅋ저두마친가지에요",
          "original_form": "ㅋㅋㅋㅋㅋ저두마친가지에요",
          "speaker_id": "2",
          "time": "20210819 17:32"
        },
        {
          "id": "MDRW2100000660.1.15",
          "form": "프로게이머 누구좋아하세오?",
          "original_form": "프로게이머 누구좋아하세오?",
          "speaker_id": "2",
          "time": "20210819 17:32"
        },
        {
          "id": "MDRW2100000660.1.16",
          "form": "전 김요한밖에몰라요",
          "original_form": "전 김요한밖에몰라요",
          "speaker_id": "2",
          "time": "20210819 17:33"
        },
        {
          "id": "MDRW2100000660.1.17",
          "form": "저도요ㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:33"
        },
        {
          "id": "MDRW2100000660.1.18",
          "form": "요즘 남자친구보니깐 롤을 엄청하던데요",
          "original_form": "요즘 남자친구보니깐 롤을 엄청하던데요",
          "speaker_id": "2",
          "time": "20210819 17:35"
        },
        {
          "id": "MDRW2100000660.1.19",
          "form": "롤롤ㅋㅋ듣기만해봤어요ㅋㅋㅋ",
          "original_form": "롤롤ㅋㅋ듣기만해봤어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000660.1.20",
          "form": "롤 모바일용 pc용따로있어서 중독되겠더라구요",
          "original_form": "롤 모바일용 pc용따로있어서 중독되겠더라구요",
          "speaker_id": "2",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000660.1.21",
          "form": "와ㅎㅎ. 동생도 겜할때보니 모니터를2개로ㅋ",
