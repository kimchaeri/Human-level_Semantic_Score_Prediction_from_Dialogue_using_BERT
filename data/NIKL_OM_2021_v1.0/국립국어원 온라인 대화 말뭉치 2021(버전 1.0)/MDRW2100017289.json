        },
        {
          "id": "MDRW2100017289.1.3",
          "form": "저녁되니 시원하네요",
          "original_form": "저녁되니 시원하네요",
          "speaker_id": "1",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017289.1.4",
          "form": "가을 간절해요",
          "original_form": "가을 간절해요",
          "speaker_id": "1",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017289.1.5",
          "form": "가을이제일좋아요",
          "original_form": "가을이제일좋아요",
          "speaker_id": "2",
          "time": "20210826 19:48"
        },
        {
          "id": "MDRW2100017289.1.6",
          "form": "빨리왔음좋겠어요",
