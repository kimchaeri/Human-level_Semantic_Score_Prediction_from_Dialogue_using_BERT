      "utterance": [
        {
          "id": "MDRW2100043901.1.1",
          "form": "네 얼큰하니 속풀리는 기분 ㅠㅠ",
          "original_form": "네 얼큰하니 속풀리는 기분 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043901.1.2",
          "form": "이름부터 좋지 않나요",
          "original_form": "이름부터 좋지 않나요",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043901.1.3",
          "form": "육개장 말씀하시는거 맞나요?!",
          "original_form": "육개장 말씀하시는거 맞나요?!",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043901.1.4",
          "form": "네!!!!",
          "original_form": "네!!!!",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043901.1.5",
          "form": "제가 아는거랑 다른걸까요..?",
          "original_form": "제가 아는거랑 다른걸까요..?",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043901.1.6",
          "form": "빨간 국물에 고사리 고기 숙주인지 콩나물인지랑 파 이런거 들어간거",
          "original_form": "빨간 국물에 고사리 고기 숙주인지 콩나물인지랑 파 이런거 들어간거",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043901.1.7",
          "form": "아니에요?",
          "original_form": "아니에요?",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043901.1.8",
          "form": "맞아요!@",
          "original_form": "맞아요!@",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043901.1.9",
          "form": "저 육개장 완전 좋아해요ㅎㅎ",
          "original_form": "저 육개장 완전 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043901.1.10",
          "form": "육개장 좋아하시나요?",
          "original_form": "육개장 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043901.1.11",
          "form": "ㅋㅋㅋㅋ 그러셨구나 네네! 좋아하죠",
          "original_form": "ㅋㅋㅋㅋ 그러셨구나 네네! 좋아하죠",
          "speaker_id": "2",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043901.1.12",
          "form": "너무 맛있죠ㅠㅠㅠ",
          "original_form": "너무 맛있죠ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043901.1.13",
          "form": "국이랑 찌개 어떤거 가장 좋아하세요?",
          "original_form": "국이랑 찌개 어떤거 가장 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043901.1.14",
          "form": "저는 다 좋아요ㅋㅋㅋㅋㅋ",
          "original_form": "저는 다 좋아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043901.1.15",
          "form": "하지만 김치는 찌개가 좋아요",
          "original_form": "하지만 김치는 찌개가 좋아요",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043901.1.16",
          "form": "국은 밍밍해요",
          "original_form": "국은 밍밍해요",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043901.1.17",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043901.1.18",
          "form": "찌개가 확실히 맛있더라구요",
          "original_form": "찌개가 확실히 맛있더라구요",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043901.1.19",
          "form": "족발도 좋아하시나요?!",
          "original_form": "족발도 좋아하시나요?!",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043901.1.20",
          "form": "족발도 좋지만 저는 보쌈이 더 좋아요 ㅎㅎㅎ",
          "original_form": "족발도 좋지만 저는 보쌈이 더 좋아요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100043901.1.21",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100043901.1.22",
          "form": "저도 보쌈이 더 좋아요!",
          "original_form": "저도 보쌈이 더 좋아요!",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100043901.1.23",
          "form": "탕수육 부먹, 찍먹 어떻게 되시나요?",
          "original_form": "탕수육 부먹, 찍먹 어떻게 되시나요?",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100043901.1.24",
          "form": "많먹이요 ㅎㅁㅎ",
          "original_form": "많먹이요 ㅎㅁㅎ",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100043901.1.25",
          "form": "다 상관없어요",
          "original_form": "다 상관없어요",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100043901.1.26",
          "form": "다 맛있어요 ㅋㅋㅋ",
          "original_form": "다 맛있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100043901.1.27",
          "form": "저도 다 좋아해요ㅎㅎ",
          "original_form": "저도 다 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100043901.1.28",
          "form": "아이스크림 좋아하세요?",
          "original_form": "아이스크림 좋아하세요?",
