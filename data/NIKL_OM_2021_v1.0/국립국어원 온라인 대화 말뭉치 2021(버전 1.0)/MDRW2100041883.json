      "utterance": [
        {
          "id": "MDRW2100041883.1.1",
          "form": "가끔봐요 그 남자 이름이..",
          "original_form": "가끔봐요 그 남자 이름이..",
          "speaker_id": "2",
          "time": "20210906 17:06"
        },
        {
          "id": "MDRW2100041883.1.2",
          "form": "아 류수영인가",
          "original_form": "아 류수영인가",
          "speaker_id": "2",
          "time": "20210906 17:06"
        },
        {
          "id": "MDRW2100041883.1.3",
          "form": "잘하더라고요",
          "original_form": "잘하더라고요",
          "speaker_id": "2",
          "time": "20210906 17:06"
        },
        {
          "id": "MDRW2100041883.1.4",
          "form": "거기서 밀키트 나오더라고요",
          "original_form": "거기서 밀키트 나오더라고요",
          "speaker_id": "1",
          "time": "20210906 17:06"
        },
        {
          "id": "MDRW2100041883.1.5",
          "form": "그래서 한번 사볼까 할때마다 맛없어보이는 밀키트라 말았어욬ㅋㅋㅋㅋ",
          "original_form": "그래서 한번 사볼까 할때마다 맛없어보이는 밀키트라 말았어욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:07"
        },
        {
          "id": "MDRW2100041883.1.6",
          "form": "밀키트 좋은점은 간을 안맞춰도 대는거져\n간을 모르겠어여ㅋㅋ",
          "original_form": "밀키트 좋은점은 간을 안맞춰도 대는거져\n간을 모르겠어여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:07"
        },
        {
          "id": "MDRW2100041883.1.7",
          "form": "류수영씨 같은 로봇 있었으면 좋겠어욬ㅋㅋ 밥 맛있게 해주는 로봇",
          "original_form": "류수영씨 같은 로봇 있었으면 좋겠어욬ㅋㅋ 밥 맛있게 해주는 로봇",
          "speaker_id": "1",
          "time": "20210906 17:07"
        },
        {
          "id": "MDRW2100041883.1.8",
          "form": "맛아여 넘 좋을거겉아여",
          "original_form": "맛아여 넘 좋을거겉아여",
          "speaker_id": "2",
          "time": "20210906 17:08"
        },
        {
          "id": "MDRW2100041883.1.9",
          "form": "진짜 간 계속 먹어도 모르겠죠??",
          "original_form": "진짜 간 계속 먹어도 모르겠죠??",
          "speaker_id": "1",
          "time": "20210906 17:08"
        },
        {
          "id": "MDRW2100041883.1.10",
          "form": "만드는 사람은 다 그런가봐옄ㅋㅋㅋ",
          "original_form": "만드는 사람은 다 그런가봐옄ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:08"
        },
        {
          "id": "MDRW2100041883.1.11",
          "form": "음식잘하는건 역시 간잘보는 사람이더라고요",
          "original_form": "음식잘하는건 역시 간잘보는 사람이더라고요",
          "speaker_id": "2",
          "time": "20210906 17:09"
        },
        {
          "id": "MDRW2100041883.1.12",
          "form": "저는 먹어도 모르겠어욬ㅋㅋㅋ",
          "original_form": "저는 먹어도 모르겠어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:10"
        },
        {
          "id": "MDRW2100041883.1.13",
          "form": "진짜 계속먹어도 점점 미궁속으로",
          "original_form": "진짜 계속먹어도 점점 미궁속으로",
          "speaker_id": "2",
          "time": "20210906 17:10"
        },
        {
          "id": "MDRW2100041883.1.14",
          "form": "어쩔수 없이 감으로 해요ㅋㅋㅋ",
          "original_form": "어쩔수 없이 감으로 해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:10"
        },
        {
          "id": "MDRW2100041883.1.15",
          "form": "깜박하고 안먹어 봤을때도 잇어요",
          "original_form": "깜박하고 안먹어 봤을때도 잇어요",
          "speaker_id": "2",
          "time": "20210906 17:11"
        },
        {
          "id": "MDRW2100041883.1.16",
          "form": "맛 어떠셨어요??ㅋㅋㅋ",
          "original_form": "맛 어떠셨어요??ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:12"
        },
        {
          "id": "MDRW2100041883.1.17",
          "form": "식겁하져ㅋㅋ",
          "original_form": "식겁하져ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:14"
        },
        {
          "id": "MDRW2100041883.1.18",
          "form": "음식에 간 맞춰주는 기계 있었으면 좋겠네욬ㅋㅋㅋ",
          "original_form": "음식에 간 맞춰주는 기계 있었으면 좋겠네욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:15"
        }
