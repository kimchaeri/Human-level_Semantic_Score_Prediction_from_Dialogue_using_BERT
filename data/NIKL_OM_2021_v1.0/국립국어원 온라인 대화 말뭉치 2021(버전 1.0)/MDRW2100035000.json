      "utterance": [
        {
          "id": "MDRW2100035000.1.1",
          "form": "생선이랑 고기중에 뭘더 선호하세요",
          "original_form": "생선이랑 고기중에 뭘더 선호하세요",
          "speaker_id": "2",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100035000.1.2",
          "form": "전 둘다 좋아하는데요",
          "original_form": "전 둘다 좋아하는데요",
          "speaker_id": "1",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100035000.1.3",
          "form": "손질은 고기가 ㅕㄴ해요",
          "original_form": "손질은 고기가 ㅕㄴ해요",
          "speaker_id": "1",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100035000.1.4",
          "form": "생선은 좀 오래되거나 저품이면 비린내가 확 나서요",
          "original_form": "생선은 좀 오래되거나 저품이면 비린내가 확 나서요",
          "speaker_id": "1",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100035000.1.5",
          "form": "전 무조건 고기파에요",
          "original_form": "전 무조건 고기파에요",
          "speaker_id": "2",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100035000.1.6",
          "form": "구운생선을 안좋아해서",
          "original_form": "구운생선을 안좋아해서",
          "speaker_id": "2",
          "time": "20210828 23:44"
        },
        {
          "id": "MDRW2100035000.1.7",
          "form": "어떤생선 좋아하세요",
          "original_form": "어떤생선 좋아하세요",
          "speaker_id": "2",
          "time": "20210828 23:44"
        },
        {
          "id": "MDRW2100035000.1.8",
          "form": "전 고등어나 꽁치 이런고소한 생선을 좋아하는데요",
          "original_form": "전 고등어나 꽁치 이런고소한 생선을 좋아하는데요",
          "speaker_id": "1",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100035000.1.9",
          "form": "이게 좀 오래되거나 냉동상태로 오래두면 기름쩐내가 확나요",
          "original_form": "이게 좀 오래되거나 냉동상태로 오래두면 기름쩐내가 확나요",
          "speaker_id": "1",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100035000.1.10",
          "form": "고기는 좀 저품질이어도 카레에 넣음 되지만",
          "original_form": "고기는 좀 저품질이어도 카레에 넣음 되지만",
          "speaker_id": "1",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100035000.1.11",
          "form": "생선카레는 못만드니 처리해어려워요",
          "original_form": "생선카레는 못만드니 처리해어려워요",
          "speaker_id": "1",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100035000.1.12",
          "form": "맞아요ㅜ 비린내가ㅜㅜ",
          "original_form": "맞아요ㅜ 비린내가ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100035000.1.13",
          "form": "전 고등어는 회는 좋아하는데 그렇게 자주먹을수있는지역이아니라서",
          "original_form": "전 고등어는 회는 좋아하는데 그렇게 자주먹을수있는지역이아니라서",
          "speaker_id": "2",
          "time": "20210828 23:49"
        },
        {
          "id": "MDRW2100035000.1.14",
          "form": "고기는 어떤고기 좋아하세요",
          "original_form": "고기는 어떤고기 좋아하세요",
          "speaker_id": "2",
          "time": "20210828 23:49"
        },
        {
          "id": "MDRW2100035000.1.15",
          "form": "전 모든 고기도 다 사랑합니다",
          "original_form": "전 모든 고기도 다 사랑합니다",
          "speaker_id": "1",
          "time": "20210828 23:49"
        },
        {
          "id": "MDRW2100035000.1.16",
          "form": "오리 돼지 닭 소 돼지",
          "original_form": "오리 돼지 닭 소 돼지",
          "speaker_id": "1",
          "time": "20210828 23:49"
        },
        {
          "id": "MDRW2100035000.1.17",
          "form": "차별하지 않아요",
          "original_form": "차별하지 않아요",
          "speaker_id": "1",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100035000.1.18",
          "form": "ㅎㅎ가리시는거 진짜 거의없으시네요",
          "original_form": "ㅎㅎ가리시는거 진짜 거의없으시네요",
          "speaker_id": "2",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100035000.1.19",
          "form": "저는 돼지닭오리소 순서로좋아해요",
          "original_form": "저는 돼지닭오리소 순서로좋아해요",
          "speaker_id": "2",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100035000.1.20",
          "form": "소화잘되고 잘먹히는거순으로 ㅎㅎ",
          "original_form": "소화잘되고 잘먹히는거순으로 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100035000.1.21",
          "form": "전 순서를 메기자면,, 부위별로 등급별로 나누고 싶은데",
          "original_form": "전 순서를 메기자면,, 부위별로 등급별로 나누고 싶은데",
          "speaker_id": "1",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100035000.1.22",
          "form": "그건넘 오타쿠 같아보이니 다 좋아한다고 말해요 ㅋㅋㅋㅋ",
          "original_form": "그건넘 오타쿠 같아보이니 다 좋아한다고 말해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100035000.1.23",
          "form": "ㅎㅎ 전문적이고 좋은데여",
          "original_form": "ㅎㅎ 전문적이고 좋은데여",
          "speaker_id": "2",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100035000.1.24",
          "form": "소고기 부위중엔 어떤부위를 가장좋아하셔요?",
          "original_form": "소고기 부위중엔 어떤부위를 가장좋아하셔요?",
          "speaker_id": "2",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100035000.1.25",
          "form": "전 스테이크용으로 나오는건 다 좋아해요",
          "original_form": "전 스테이크용으로 나오는건 다 좋아해요",
          "speaker_id": "1",
          "time": "20210828 23:54"
        },
        {
          "id": "MDRW2100035000.1.26",
          "form": "그런 부위들은 다 기름지잖아요",
          "original_form": "그런 부위들은 다 기름지잖아요",
          "speaker_id": "1",
          "time": "20210828 23:54"
        },
        {
          "id": "MDRW2100035000.1.27",
          "form": "기름과 짭짤한 소금과 고기맛을 좋아해요",
          "original_form": "기름과 짭짤한 소금과 고기맛을 좋아해요",
          "speaker_id": "1",
          "time": "20210828 23:55"
        },
        {
          "id": "MDRW2100035000.1.28",
          "form": "별로 안좋아하는건 장조림 부위?",
          "original_form": "별로 안좋아하는건 장조림 부위?",
          "speaker_id": "1",
          "time": "20210828 23:55"
        },
        {
          "id": "MDRW2100035000.1.29",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210828 23:55"
        },
        {
          "id": "MDRW2100035000.1.30",
          "form": "전 돼지 삼겹살 가장좋아하구여",
          "original_form": "전 돼지 삼겹살 가장좋아하구여",
          "speaker_id": "2",
          "time": "20210828 23:58"
        },
        {
          "id": "MDRW2100035000.1.31",
          "form": "소고기는 꽃등심부위 좋아합니다",
          "original_form": "소고기는 꽃등심부위 좋아합니다",
          "speaker_id": "2",
          "time": "20210828 23:58"
        },
        {
          "id": "MDRW2100035000.1.32",
          "form": "부드러운부위좋아해요",
          "original_form": "부드러운부위좋아해요",
          "speaker_id": "2",
          "time": "20210828 23:58"
        },
        {
          "id": "MDRW2100035000.1.33",
          "form": "저랑 비슷하시네요",
          "original_form": "저랑 비슷하시네요",
          "speaker_id": "1",
          "time": "20210828 23:58"
        },
        {
          "id": "MDRW2100035000.1.34",
          "form": ".그리고 다들 비슷하죠",
          "original_form": ".그리고 다들 비슷하죠",
          "speaker_id": "1",
          "time": "20210828 23:59"
        },
        {
          "id": "MDRW2100035000.1.35",
          "form": "그래서 저 부위가 비싸죠..",
          "original_form": "그래서 저 부위가 비싸죠..",
          "speaker_id": "1",
          "time": "20210828 23:59"
        },
        {
          "id": "MDRW2100035000.1.36",
          "form": "네 ㅋㅋㅋㅋㅋ다들좋아하는부위",
