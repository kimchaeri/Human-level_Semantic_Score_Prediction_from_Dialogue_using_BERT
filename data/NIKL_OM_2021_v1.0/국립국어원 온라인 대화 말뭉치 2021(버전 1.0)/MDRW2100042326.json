      "utterance": [
        {
          "id": "MDRW2100042326.1.1",
          "form": "그랴 수고했다 ㅎㅎ",
          "original_form": "그랴 수고했다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042326.1.2",
          "form": "어제 애들재우다가 잠들었닼",
          "original_form": "어제 애들재우다가 잠들었닼",
          "speaker_id": "1",
          "time": "20210907 07:58"
        },
        {
          "id": "MDRW2100042326.1.3",
          "form": "잘했네 일찍 자는날도 있어야지",
          "original_form": "잘했네 일찍 자는날도 있어야지",
          "speaker_id": "2",
          "time": "20210907 09:12"
        },
        {
          "id": "MDRW2100042326.1.4",
          "form": "응  둘째보내고왔다ㅋㅋ",
          "original_form": "응  둘째보내고왔다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:27"
        },
        {
          "id": "MDRW2100042326.1.5",
          "form": "이제출근?",
          "original_form": "이제출근?",
          "speaker_id": "1",
          "time": "20210907 09:27"
        },
        {
          "id": "MDRW2100042326.1.6",
          "form": "어난 출근했지",
          "original_form": "어난 출근했지",
          "speaker_id": "2",
          "time": "20210907 09:28"
        },
        {
          "id": "MDRW2100042326.1.7",
          "form": "이제 나갈 준비 중",
          "original_form": "이제 나갈 준비 중",
          "speaker_id": "2",
          "time": "20210907 09:28"
        },
        {
          "id": "MDRW2100042326.1.8",
          "form": "아 벌써 출근했나 ㅋㅋ",
          "original_form": "아 벌써 출근했나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:32"
        },
        {
          "id": "MDRW2100042326.1.9",
          "form": "아 난 출근했지 9시까지니까ㅋㅋ",
          "original_form": "아 난 출근했지 9시까지니까ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:32"
        },
        {
          "id": "MDRW2100042326.1.10",
          "form": "이제 넌 곧 떠나겠구나",
          "original_form": "이제 넌 곧 떠나겠구나",
          "speaker_id": "2",
          "time": "20210907 09:32"
        },
        {
          "id": "MDRW2100042326.1.11",
          "form": "아아 9시까지구나 ㅋㅋ",
          "original_form": "아아 9시까지구나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:33"
        },
        {
          "id": "MDRW2100042326.1.12",
          "form": "엉 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "엉 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:34"
        },
        {
          "id": "MDRW2100042326.1.13",
          "form": "주사맞고 넘어갈 생각에 들뜨네 ㅋㅋㅋㅋㄱ",
          "original_form": "주사맞고 넘어갈 생각에 들뜨네 ㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210907 09:34"
        },
        {
          "id": "MDRW2100042326.1.14",
          "form": "ㅋㅋㅋ아 웃겨 ㅋㅋ 맞네 맞네 ㅋ",
          "original_form": "ㅋㅋㅋ아 웃겨 ㅋㅋ 맞네 맞네 ㅋ",
          "speaker_id": "1",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042326.1.15",
          "form": "2차 주사도 ㅋㅋ 금방 괜찮아지지싶다",
          "original_form": "2차 주사도 ㅋㅋ 금방 괜찮아지지싶다",
          "speaker_id": "1",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042326.1.16",
          "form": "엉 ㅎㅎ 괜찮겠지뭐 ㅎㅎ",
          "original_form": "엉 ㅎㅎ 괜찮겠지뭐 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 09:38"
        },
        {
          "id": "MDRW2100042326.1.17",
          "form": "응응 맞다 ㅋㅋ 나도 1차 맞고 나서는",
          "original_form": "응응 맞다 ㅋㅋ 나도 1차 맞고 나서는",
          "speaker_id": "1",
          "time": "20210907 09:38"
        },
        {
          "id": "MDRW2100042326.1.18",
          "form": "덜 무섭더라 ㅋㅋ 오히려 ㅋㅋㅋ 괜찮든데 ㅋ",
          "original_form": "덜 무섭더라 ㅋㅋ 오히려 ㅋㅋㅋ 괜찮든데 ㅋ",
          "speaker_id": "1",
          "time": "20210907 09:38"
        },
        {
          "id": "MDRW2100042326.1.19",
          "form": "나도 일차 막기 전에는",
          "original_form": "나도 일차 막기 전에는",
          "speaker_id": "2",
          "time": "20210907 09:39"
        },
        {
          "id": "MDRW2100042326.1.20",
          "form": "취소할까 말까 엄청 겁나는 거야",
