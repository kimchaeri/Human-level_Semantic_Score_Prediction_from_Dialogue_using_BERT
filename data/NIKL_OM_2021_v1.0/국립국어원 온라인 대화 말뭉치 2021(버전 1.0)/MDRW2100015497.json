      "utterance": [
        {
          "id": "MDRW2100015497.1.1",
          "form": "여행은 좋아하세요?",
          "original_form": "여행은 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015497.1.2",
          "form": "아주 좋아해요",
          "original_form": "아주 좋아해요",
          "speaker_id": "2",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015497.1.3",
          "form": "요새 못가서 슬프네요",
          "original_form": "요새 못가서 슬프네요",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015497.1.4",
          "form": "하긴 싫어하는 사람 거의 못봤어요",
          "original_form": "하긴 싫어하는 사람 거의 못봤어요",
          "speaker_id": "1",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015497.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015497.1.6",
          "form": "그치요ㅎㅎ 힐링인걸요",
          "original_form": "그치요ㅎㅎ 힐링인걸요",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015497.1.7",
          "form": "저도 좋아해요 마지막 여행지는 어디셨어요?",
          "original_form": "저도 좋아해요 마지막 여행지는 어디셨어요?",
          "speaker_id": "1",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015497.1.8",
          "form": "가평 애견펜션이요",
          "original_form": "가평 애견펜션이요",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015497.1.9",
          "form": "코로나로 콕 처박혀 있는 여행만 하네요",
          "original_form": "코로나로 콕 처박혀 있는 여행만 하네요",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015497.1.10",
          "form": "와 강아지있으니까 그런곳도 좋겠네요",
          "original_form": "와 강아지있으니까 그런곳도 좋겠네요",
          "speaker_id": "1",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015497.1.11",
          "form": "네ㅎ 강아지 수영장도 있고 뛰어놀 잔디 운동장도 있고 좋아요",
          "original_form": "네ㅎ 강아지 수영장도 있고 뛰어놀 잔디 운동장도 있고 좋아요",
          "speaker_id": "2",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015497.1.12",
          "form": "최근 어디 다녀오셨어요?",
          "original_form": "최근 어디 다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015497.1.13",
          "form": "저도 여행간지 오래됐어요ㅜㅜㅜ",
          "original_form": "저도 여행간지 오래됐어요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015497.1.14",
          "form": "그나마 제일최근이 속초요",
          "original_form": "그나마 제일최근이 속초요",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015497.1.15",
          "form": "속초에서 뭐하고 노셨나요",
          "original_form": "속초에서 뭐하고 노셨나요",
          "speaker_id": "2",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015497.1.16",
          "form": "그냥 예쁜 까페가고 바다보고 그랬어요ㅎㅎ",
          "original_form": "그냥 예쁜 까페가고 바다보고 그랬어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015497.1.17",
          "form": "그거면 됐죠. 제대로 힐링하셨네요",
          "original_form": "그거면 됐죠. 제대로 힐링하셨네요",
          "speaker_id": "2",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015497.1.18",
          "form": "뭔가하기엔 역시 코로나땜시 ㅜㅜㅜ",
          "original_form": "뭔가하기엔 역시 코로나땜시 ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 23:21"
        }
      ]
