      "utterance": [
        {
          "id": "MDRW2100035686.1.1",
          "form": "네 매일 듣는 편이예요",
          "original_form": "네 매일 듣는 편이예요",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035686.1.2",
          "form": "어떤 플랫폼을 사용하시나요?",
          "original_form": "어떤 플랫폼을 사용하시나요?",
          "speaker_id": "1",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100035686.1.3",
          "form": "저는 플로듣는데 요금제땜에 무료여서요 ㅎㅎ",
          "original_form": "저는 플로듣는데 요금제땜에 무료여서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:30"
        },
        {
          "id": "MDRW2100035686.1.4",
          "form": "부럽네요ㅠㅠ 저는 멜론 매달 결제해여ㅠ",
          "original_form": "부럽네요ㅠㅠ 저는 멜론 매달 결제해여ㅠ",
          "speaker_id": "1",
          "time": "20210829 23:20"
        },
        {
          "id": "MDRW2100035686.1.5",
          "form": "근데 멜론이 UI는 훨 편해요",
          "original_form": "근데 멜론이 UI는 훨 편해요",
          "speaker_id": "2",
          "time": "20210830 11:04"
        },
        {
          "id": "MDRW2100035686.1.6",
          "form": "플로로 갈아타볼까 고민하고 있는데 고민이 깊어지겠네요ㅎㅎ",
          "original_form": "플로로 갈아타볼까 고민하고 있는데 고민이 깊어지겠네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 11:10"
        },
        {
          "id": "MDRW2100035686.1.7",
          "form": "ㅎㅎ 전 플로 무료아니면 사용 안할것같아요",
          "original_form": "ㅎㅎ 전 플로 무료아니면 사용 안할것같아요",
          "speaker_id": "2",
          "time": "20210830 11:36"
        },
        {
          "id": "MDRW2100035686.1.8",
          "form": "콘텐츠가 훨씬 적은 느낌?",
          "original_form": "콘텐츠가 훨씬 적은 느낌?",
          "speaker_id": "2",
          "time": "20210830 11:36"
        },
        {
          "id": "MDRW2100035686.1.9",
          "form": "그래요?? 의견 참고할게요, 감사드려요!",
          "original_form": "그래요?? 의견 참고할게요, 감사드려요!",
          "speaker_id": "1",
          "time": "20210830 11:42"
        },
        {
          "id": "MDRW2100035686.1.10",
          "form": "콘텐츠는 확실히 멜론이 젤 많은듯해요",
          "original_form": "콘텐츠는 확실히 멜론이 젤 많은듯해요",
          "speaker_id": "2",
          "time": "20210830 12:07"
        },
        {
          "id": "MDRW2100035686.1.11",
          "form": "탑100이 바로 안 뜨는 게 불편해서요ㅠ",
          "original_form": "탑100이 바로 안 뜨는 게 불편해서요ㅠ",
          "speaker_id": "1",
          "time": "20210830 12:08"
        },
        {
          "id": "MDRW2100035686.1.12",
          "form": "아 ㅋㅋㅋ 저는 플로 탑 100이 이상해서 멜론이랑 가끔 비교해요",
          "original_form": "아 ㅋㅋㅋ 저는 플로 탑 100이 이상해서 멜론이랑 가끔 비교해요",
          "speaker_id": "2",
          "time": "20210830 12:09"
        },
        {
          "id": "MDRW2100035686.1.13",
          "form": "24힛으로 변한 이후 새 곡들이 진입을 잘 못 하는 건 아쉬워요..",
          "original_form": "24힛으로 변한 이후 새 곡들이 진입을 잘 못 하는 건 아쉬워요..",
          "speaker_id": "1",
          "time": "20210830 12:24"
        },
        {
          "id": "MDRW2100035686.1.14",
          "form": "가끔 과거에 사는 느낌 ㅋㅋ",
          "original_form": "가끔 과거에 사는 느낌 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 12:30"
        },
        {
          "id": "MDRW2100035686.1.15",
          "form": "좀처럼 바뀌지 않아서 질리더라구오ㅠ",
