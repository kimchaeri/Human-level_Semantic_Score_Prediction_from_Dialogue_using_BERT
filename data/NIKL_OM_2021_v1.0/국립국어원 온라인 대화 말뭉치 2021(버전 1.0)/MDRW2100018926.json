      "utterance": [
        {
          "id": "MDRW2100018926.1.1",
          "form": "그냥 정으로 본세월이 있어 산단말이 사실입니다",
          "original_form": "그냥 정으로 본세월이 있어 산단말이 사실입니다",
          "speaker_id": "2",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018926.1.2",
          "form": "아 저 지금 피부가",
          "original_form": "아 저 지금 피부가",
          "speaker_id": "2",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018926.1.3",
          "form": "정답게 사는게 최고군요ㅋ 건강에도 최고죠ㅋ",
          "original_form": "정답게 사는게 최고군요ㅋ 건강에도 최고죠ㅋ",
          "speaker_id": "1",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018926.1.4",
          "form": "엉망입니다 ㅡㅡ",
          "original_form": "엉망입니다 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018926.1.5",
          "form": "마스크땜시 뾰루지 ㅋㅋ",
          "original_form": "마스크땜시 뾰루지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018926.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018926.1.7",
          "form": "마스크ㅠ",
          "original_form": "마스크ㅠ",
          "speaker_id": "1",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018926.1.8",
          "form": "그럼요 싸우면 늙어요",
          "original_form": "그럼요 싸우면 늙어요",
          "speaker_id": "2",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018926.1.9",
          "form": "왕만합니다 ㅋㅋ",
          "original_form": "왕만합니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018926.1.10",
          "form": "뾰루지",
          "original_form": "뾰루지",
          "speaker_id": "2",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018926.1.11",
          "form": "그래도 뾰루지 따위",
          "original_form": "그래도 뾰루지 따위",
          "speaker_id": "1",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018926.1.12",
          "form": "근데 원래 여드름도 안난 피부인데",
          "original_form": "근데 원래 여드름도 안난 피부인데",
          "speaker_id": "2",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018926.1.13",
          "form": "저흰 우울감이 있는 가족때문에 걱정이에요",
          "original_form": "저흰 우울감이 있는 가족때문에 걱정이에요",
          "speaker_id": "1",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018926.1.14",
          "form": "당황스럽네요 ㅋㅋ",
          "original_form": "당황스럽네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018926.1.15",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018926.1.16",
          "form": "와 복받으셨다!!",
          "original_form": "와 복받으셨다!!",
          "speaker_id": "1",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018926.1.17",
          "form": "치료하셔야 하는데",
          "original_form": "치료하셔야 하는데",
          "speaker_id": "2",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018926.1.18",
          "form": "그럴까요? ㅠ",
          "original_form": "그럴까요? ㅠ",
          "speaker_id": "1",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018926.1.19",
          "form": "전 약간 공항장애가 코로나로",
          "original_form": "전 약간 공항장애가 코로나로",
          "speaker_id": "2",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018926.1.20",
          "form": "생겼었는데",
          "original_form": "생겼었는데",
          "speaker_id": "2",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018926.1.21",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "1",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018926.1.22",
          "form": "약물치료로 좋아졌어요",
          "original_form": "약물치료로 좋아졌어요",
          "speaker_id": "2",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018926.1.23",
          "form": "치료하셔야 해요",
          "original_form": "치료하셔야 해요",
          "speaker_id": "2",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018926.1.24",
          "form": "진료받는게 좋군요",
          "original_form": "진료받는게 좋군요",
          "speaker_id": "1",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018926.1.25",
          "form": "확실히 다른가요?",
          "original_form": "확실히 다른가요?",
          "speaker_id": "1",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018926.1.26",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018926.1.27",
          "form": "이유가 딱히 없는데",
          "original_form": "이유가 딱히 없는데",
          "speaker_id": "1",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018926.1.28",
          "form": "방치하시면 안돼요",
          "original_form": "방치하시면 안돼요",
          "speaker_id": "2",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018926.1.29",
          "form": "그래서 우울증인가봐요",
          "original_form": "그래서 우울증인가봐요",
          "speaker_id": "1",
          "time": "20210827 13:41"
        },
        {
          "id": "MDRW2100018926.1.30",
          "form": "요새 상황이 그래요",
          "original_form": "요새 상황이 그래요",
          "speaker_id": "2",
          "time": "20210827 13:41"
        }
      ]
    }
  ]
