      "utterance": [
        {
          "id": "MDRW2100021946.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210829 00:02"
        },
        {
          "id": "MDRW2100021946.1.2",
          "form": "요근래에 키우는게 또 늘어났어요",
          "original_form": "요근래에 키우는게 또 늘어났어요",
          "speaker_id": "2",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100021946.1.3",
          "form": "물론 제가 키우는건 아니고 여동생이 키우는거긴한데",
          "original_form": "물론 제가 키우는건 아니고 여동생이 키우는거긴한데",
          "speaker_id": "2",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100021946.1.4",
          "form": "달팽이 키우고있어요",
          "original_form": "달팽이 키우고있어요",
          "speaker_id": "2",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100021946.1.5",
          "form": "오 어렵죠?",
          "original_form": "오 어렵죠?",
          "speaker_id": "1",
          "time": "20210829 00:09"
        },
        {
          "id": "MDRW2100021946.1.6",
          "form": "확실히 어렵긴 하더라고요",
          "original_form": "확실히 어렵긴 하더라고요",
          "speaker_id": "2",
          "time": "20210829 00:10"
        },
        {
          "id": "MDRW2100021946.1.7",
          "form": "땅갈고 뭐하고 먹이주고 하는게 어우 ㅋㅋㅋ",
          "original_form": "땅갈고 뭐하고 먹이주고 하는게 어우 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:10"
        },
        {
          "id": "MDRW2100021946.1.8",
          "form": "보통일이 아니네요",
          "original_form": "보통일이 아니네요",
          "speaker_id": "1",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021946.1.9",
          "form": "맞아요 의외로 조그만한애 키우는게",
          "original_form": "맞아요 의외로 조그만한애 키우는게",
          "speaker_id": "2",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100021946.1.10",
          "form": "더 힘들어요 ㄷㄷ",
          "original_form": "더 힘들어요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100021946.1.11",
          "form": "케어가 5",
          "original_form": "케어가 5",
          "speaker_id": "1",
          "time": "20210829 00:22"
        },
        {
          "id": "MDRW2100021946.1.12",
          "form": "케어도 엄청 해야죠..",
          "original_form": "케어도 엄청 해야죠..",
          "speaker_id": "2",
          "time": "20210829 01:09"
        },
        {
          "id": "MDRW2100021946.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210829 11:42"
        },
        {
          "id": "MDRW2100021946.1.14",
          "form": "조그만한 달팽이 키우는게 이렇게 힘드니 ㄷㄷ..",
          "original_form": "조그만한 달팽이 키우는게 이렇게 힘드니 ㄷㄷ..",
          "speaker_id": "2",
          "time": "20210829 11:48"
        },
        {
          "id": "MDRW2100021946.1.15",
          "form": "사람보다 더ㅜ힘들듯",
          "original_form": "사람보다 더ㅜ힘들듯",
          "speaker_id": "1",
          "time": "20210829 12:05"
        },
        {
          "id": "MDRW2100021946.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210829 12:06"
        },
        {
          "id": "MDRW2100021946.1.17",
          "form": "것고 일이죠",
