      "utterance": [
        {
          "id": "MDRW2100002941.1.1",
          "form": "요새 보는 책 있어?",
          "original_form": "요새 보는 책 있어?",
          "speaker_id": "1",
          "time": "20210820 16:52"
        },
        {
          "id": "MDRW2100002941.1.2",
          "form": "요새 읽는 책 있어?",
          "original_form": "요새 읽는 책 있어?",
          "speaker_id": "1",
          "time": "20210820 16:52"
        },
        {
          "id": "MDRW2100002941.1.3",
          "form": "음 최근에 무슨챡을 잀었도라",
          "original_form": "음 최근에 무슨챡을 잀었도라",
          "speaker_id": "2",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002941.1.4",
          "form": "바빠서",
          "original_form": "바빠서",
          "speaker_id": "1",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002941.1.5",
          "form": "아 최근에 고전을 읽었어",
          "original_form": "아 최근에 고전을 읽었어",
          "speaker_id": "2",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002941.1.6",
          "form": "기억이 안나는구나",
          "original_form": "기억이 안나는구나",
          "speaker_id": "1",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002941.1.7",
          "form": "오 고전이라니 멋있다.",
          "original_form": "오 고전이라니 멋있다.",
          "speaker_id": "1",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002941.1.8",
          "form": "이제 기억이났어",
          "original_form": "이제 기억이났어",
          "speaker_id": "2",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002941.1.9",
          "form": "제목이 뭐니?",
          "original_form": "제목이 뭐니?",
          "speaker_id": "1",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002941.1.10",
          "form": "나는 1987을 읽었어",
          "original_form": "나는 1987을 읽었어",
          "speaker_id": "2",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002941.1.11",
          "form": "너두 읽어봤니??",
          "original_form": "너두 읽어봤니??",
          "speaker_id": "2",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002941.1.12",
          "form": "대단하다.",
          "original_form": "대단하다.",
          "speaker_id": "1",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002941.1.13",
          "form": "줄거리만 알아.",
          "original_form": "줄거리만 알아.",
          "speaker_id": "1",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002941.1.14",
          "form": "사실 잘 기억이 안나",
          "original_form": "사실 잘 기억이 안나",
          "speaker_id": "1",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002941.1.15",
          "form": "근데 줄거리만 알아도 돼",
          "original_form": "근데 줄거리만 알아도 돼",
          "speaker_id": "2",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002941.1.16",
          "form": "몇달전에 읽다가 만 거 같아.",
          "original_form": "몇달전에 읽다가 만 거 같아.",
          "speaker_id": "1",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002941.1.17",
          "form": "책이 두껍고 좀 어렵더라",
          "original_form": "책이 두껍고 좀 어렵더라",
          "speaker_id": "2",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002941.1.18",
          "form": "책에 대해 각박하구나",
          "original_form": "책에 대해 각박하구나",
          "speaker_id": "1",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002941.1.19",
          "form": "맞아 고전은 어려워",
          "original_form": "맞아 고전은 어려워",
          "speaker_id": "1",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002941.1.20",
          "form": "ㅋㅋㅋㅋㅋ고전은 장단점이 있는거같아",
          "original_form": "ㅋㅋㅋㅋㅋ고전은 장단점이 있는거같아",
          "speaker_id": "2",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002941.1.21",
          "form": "어렵지만 고전을 읽어야 뭐 도움이 된다더아",
          "original_form": "어렵지만 고전을 읽어야 뭐 도움이 된다더아",
          "speaker_id": "1",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002941.1.22",
          "form": "동의해",
          "original_form": "동의해",
          "speaker_id": "1",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002941.1.23",
          "form": "맞아 뭐 도움된다는데 뭔지는 모르겠어",
          "original_form": "맞아 뭐 도움된다는데 뭔지는 모르겠어",
          "speaker_id": "2",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002941.1.24",
          "form": "너무 재미만 추구하는 책보다는 가끔 마음의 양식을 쌓고싶어",
          "original_form": "너무 재미만 추구하는 책보다는 가끔 마음의 양식을 쌓고싶어",
          "speaker_id": "1",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002941.1.25",
          "form": "어려운 어휘릉 잘 구사하는 데 도움이 된다는 걸까?",
          "original_form": "어려운 어휘릉 잘 구사하는 데 도움이 된다는 걸까?",
          "speaker_id": "1",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002941.1.26",
          "form": "그럼 넌 자기계발서를 좋아하니?",
          "original_form": "그럼 넌 자기계발서를 좋아하니?",
          "speaker_id": "2",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002941.1.27",
          "form": "오 아니.",
          "original_form": "오 아니.",
          "speaker_id": "1",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002941.1.28",
          "form": "그저 고전에서 내가 교훈을 찾는 것만으로 충분해",
          "original_form": "그저 고전에서 내가 교훈을 찾는 것만으로 충분해",
          "speaker_id": "1",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002941.1.29",
          "form": "나는 어쭙잖은 자기계발서는 책이 아니라고 생각해.",
          "original_form": "나는 어쭙잖은 자기계발서는 책이 아니라고 생각해.",
          "speaker_id": "1",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002941.1.30",
          "form": "너도 그렇지?",
          "original_form": "너도 그렇지?",
          "speaker_id": "1",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002941.1.31",
          "form": "맞아 자기계발서 몇몇은 너무",
          "original_form": "맞아 자기계발서 몇몇은 너무",
          "speaker_id": "2",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002941.1.32",
          "form": "자기자랑같아",
          "original_form": "자기자랑같아",
          "speaker_id": "2",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002941.1.33",
          "form": "맞아.",
          "original_form": "맞아.",
          "speaker_id": "1",
          "time": "20210820 16:55"
        }
      ]
    }
