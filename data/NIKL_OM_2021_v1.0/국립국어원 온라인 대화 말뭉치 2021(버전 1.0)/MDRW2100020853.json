        },
        {
          "id": "MDRW2100020853.1.2",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 13:28"
        },
        {
          "id": "MDRW2100020853.1.3",
          "form": "여행을 많이 가본 건 아니지만 좋아해요!",
          "original_form": "여행을 많이 가본 건 아니지만 좋아해요!",
          "speaker_id": "1",
          "time": "20210828 13:39"
        },
        {
          "id": "MDRW2100020853.1.4",
          "form": "저두요ㅎ",
          "original_form": "저두요ㅎ",
          "speaker_id": "2",
          "time": "20210828 13:40"
        },
        {
          "id": "MDRW2100020853.1.5",
          "form": "어디 가보셨어요?",
          "original_form": "어디 가보셨어요?",
          "speaker_id": "1",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100020853.1.6",
          "form": "태국 일본 중국이여~~",
          "original_form": "태국 일본 중국이여~~",
          "speaker_id": "2",
          "time": "20210828 13:51"
        },
        {
          "id": "MDRW2100020853.1.7",
          "form": "어디가 가장 좋으셨어요?",
          "original_form": "어디가 가장 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210828 18:26"
        },
        {
          "id": "MDRW2100020853.1.8",
          "form": "태국이여",
          "original_form": "태국이여",
          "speaker_id": "2",
          "time": "20210828 21:59"
        },
        {
          "id": "MDRW2100020853.1.9",
          "form": "태국에 맛있는 음식이 많은 거 같아요\r\n.",
          "original_form": "태국에 맛있는 음식이 많은 거 같아요\r\n.",
          "speaker_id": "1",
          "time": "20210829 11:10"
        },
        {
          "id": "MDRW2100020853.1.10",
          "form": "전 음식이 안맞아서ㅠ",
          "original_form": "전 음식이 안맞아서ㅠ",
          "speaker_id": "2",
          "time": "20210829 12:31"
        },
        {
          "id": "MDRW2100020853.1.11",
          "form": "똠양꿍인가",
          "original_form": "똠양꿍인가",
          "speaker_id": "2",
          "time": "20210829 12:31"
        },
        {
          "id": "MDRW2100020853.1.12",
          "form": "ㅜㅜ아쉽네요 어떤 점이 좋으셨어요?",
          "original_form": "ㅜㅜ아쉽네요 어떤 점이 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210829 17:15"
        },
        {
          "id": "MDRW2100020853.1.13",
          "form": "환락가가잼난ㅠㅠ",
          "original_form": "환락가가잼난ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100020853.1.14",
          "form": "우리와 다른점ㅋ",
          "original_form": "우리와 다른점ㅋ",
          "speaker_id": "2",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100020853.1.15",
          "form": "여행가고싶어요",
          "original_form": "여행가고싶어요",
