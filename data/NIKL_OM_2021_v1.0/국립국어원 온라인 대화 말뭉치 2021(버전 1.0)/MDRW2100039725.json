      "utterance": [
        {
          "id": "MDRW2100039725.1.1",
          "form": "앗 네..!",
          "original_form": "앗 네..!",
          "speaker_id": "2",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039725.1.2",
          "form": "민초 오이 좋아해요!!",
          "original_form": "민초 오이 좋아해요!!",
          "speaker_id": "2",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039725.1.3",
          "form": "허억... 전 둘 다 안 먹거든요....",
          "original_form": "허억... 전 둘 다 안 먹거든요....",
          "speaker_id": "1",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039725.1.4",
          "form": "불편한 사람이랑 있으면 먹긴 해요!",
          "original_form": "불편한 사람이랑 있으면 먹긴 해요!",
          "speaker_id": "1",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039725.1.5",
          "form": "거부하기 좀 그래서",
          "original_form": "거부하기 좀 그래서",
          "speaker_id": "1",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039725.1.6",
          "form": "헐 이런..!!!",
          "original_form": "헐 이런..!!!",
          "speaker_id": "2",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039725.1.7",
          "form": "근데 전 민초랑 오이는",
          "original_form": "근데 전 민초랑 오이는",
          "speaker_id": "2",
          "time": "20210903 21:30"
        },
        {
          "id": "MDRW2100039725.1.8",
          "form": "편식은 아니라구 생각해요..",
          "original_form": "편식은 아니라구 생각해요..",
          "speaker_id": "2",
          "time": "20210903 21:30"
        },
        {
          "id": "MDRW2100039725.1.9",
          "form": "ㅎㅎ약간 홍어랑 비슷한 느낌이라고 해여할까여",
          "original_form": "ㅎㅎ약간 홍어랑 비슷한 느낌이라고 해여할까여",
          "speaker_id": "2",
          "time": "20210903 21:30"
        },
        {
          "id": "MDRW2100039725.1.10",
          "form": "헛 약간 취향이긴 하죠 ㅎ.ㅎ !!!!",
          "original_form": "헛 약간 취향이긴 하죠 ㅎ.ㅎ !!!!",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039725.1.11",
          "form": "앗 저 하나 더 안 먹는 거! 고구마 안 먹어요!",
          "original_form": "앗 저 하나 더 안 먹는 거! 고구마 안 먹어요!",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039725.1.12",
          "form": "헐 고규마!",
          "original_form": "헐 고규마!",
          "speaker_id": "2",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100039725.1.13",
          "form": "고구마는 왜 안드세요?",
          "original_form": "고구마는 왜 안드세요?",
          "speaker_id": "2",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100039725.1.14",
          "form": "전 약간 그 푸석푸석한데 단맛이..?",
          "original_form": "전 약간 그 푸석푸석한데 단맛이..?",
          "speaker_id": "1",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039725.1.15",
          "form": "이상하게 느껴지더라거요 ㅎ.ㅎ",
          "original_form": "이상하게 느껴지더라거요 ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039725.1.16",
          "form": "name2님 고구마 좋아하셔요?",
          "original_form": "&name2&님 고구마 좋아하셔요?",
          "speaker_id": "1",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039725.1.17",
          "form": "네! 전 호박고구마! 호! 박! 고! 구! 마!",
          "original_form": "네! 전 호박고구마! 호! 박! 고! 구! 마!",
          "speaker_id": "2",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039725.1.18",
          "form": "호구마!!!",
          "original_form": "호구마!!!",
          "speaker_id": "2",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039725.1.19",
          "form": "완전 좋아해요",
          "original_form": "완전 좋아해요",
          "speaker_id": "2",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039725.1.20",
          "form": "완전 달달하구",
          "original_form": "완전 달달하구",
          "speaker_id": "2",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039725.1.21",
          "form": "약간 찐덕한 느낌이 좋아요",
          "original_form": "약간 찐덕한 느낌이 좋아요",
          "speaker_id": "2",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039725.1.22",
          "form": "아놔!!! 호구마 ㅋㅋㅋㅋㅋㅋㅋㅋ ㅠ 저거 너무 웃기다구요",
          "original_form": "아놔!!! 호구마 ㅋㅋㅋㅋㅋㅋㅋㅋ ㅠ 저거 너무 웃기다구요",
          "speaker_id": "1",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039725.1.23",
          "form": "호구마랑 그냥 고구마랑 조금",
          "original_form": "호구마랑 그냥 고구마랑 조금",
          "speaker_id": "1",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039725.1.24",
          "form": "다른가요??",
          "original_form": "다른가요??",
          "speaker_id": "1",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100039725.1.25",
          "form": "음 호구마랑 밤구마가 이ㅛ는데",
          "original_form": "음 호구마랑 밤구마가 이ㅛ는데",
          "speaker_id": "2",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039725.1.26",
          "form": "밤구마는 약간 더 퍽퍽하고 물기가 업ㅎ는 느낌?이에요",
          "original_form": "밤구마는 약간 더 퍽퍽하고 물기가 업ㅎ는 느낌?이에요",
          "speaker_id": "2",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039725.1.27",
          "form": "호구마는 촉촉하구 달달하구 말랑하구",
          "original_form": "호구마는 촉촉하구 달달하구 말랑하구",
          "speaker_id": "2",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100039725.1.28",
          "form": "존맛입니다",
          "original_form": "존맛입니다",
          "speaker_id": "2",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039725.1.29",
          "form": "호구마의 시대가 온다..!",
          "original_form": "호구마의 시대가 온다..!",
          "speaker_id": "2",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039725.1.30",
          "form": "호구마가 세상을 지배한다..!!",
          "original_form": "호구마가 세상을 지배한다..!!",
          "speaker_id": "2",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039725.1.31",
          "form": "우악 그러니까 뭔가 궁금해지잖아요.....!!!!!",
          "original_form": "우악 그러니까 뭔가 궁금해지잖아요.....!!!!!",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039725.1.32",
          "form": "맛탕 먹었다가 실패한 기억 있는데..",
          "original_form": "맛탕 먹었다가 실패한 기억 있는데..",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039725.1.33",
          "form": "호구마를 먹어봐야할런지...!",
          "original_form": "호구마를 먹어봐야할런지...!",
          "speaker_id": "1",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100039725.1.34",
          "form": "앟 이런..",
          "original_form": "앟 이런..",
          "speaker_id": "2",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100039725.1.35",
          "form": "그럼 누가 먹으면 한입만..찬스릉..!!",
          "original_form": "그럼 누가 먹으면 한입만..찬스릉..!!",
          "speaker_id": "2",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100039725.1.36",
          "form": "아익!!!! 좋은 방법인걸요 ㅎ.ㅎ!",
          "original_form": "아익!!!! 좋은 방법인걸요 ㅎ.ㅎ!",
          "speaker_id": "1",
          "time": "20210903 22:02"
        },
        {
          "id": "MDRW2100039725.1.37",
          "form": "name2님 배달 음긱은 자주 드시나요?!",
          "original_form": "&name2&님 배달 음긱은 자주 드시나요?!",
          "speaker_id": "1",
          "time": "20210903 22:02"
        },
        {
