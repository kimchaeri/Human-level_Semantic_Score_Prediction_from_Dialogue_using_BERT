      "utterance": [
        {
          "id": "MDRW2100027333.1.1",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "1",
          "time": "20210902 11:33"
        },
        {
          "id": "MDRW2100027333.1.2",
          "form": "아직이요 식사하셨나요?",
          "original_form": "아직이요 식사하셨나요?",
          "speaker_id": "2",
          "time": "20210902 11:34"
        },
        {
          "id": "MDRW2100027333.1.3",
          "form": "아침은 먹었어요^^",
          "original_form": "아침은 먹었어요^^",
          "speaker_id": "1",
          "time": "20210902 11:34"
        },
        {
          "id": "MDRW2100027333.1.4",
          "form": "아침드셨어요?",
          "original_form": "아침드셨어요?",
          "speaker_id": "1",
          "time": "20210902 11:35"
        },
        {
          "id": "MDRW2100027333.1.5",
          "form": "감사합니다ㅎㅎ",
          "original_form": "감사합니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 11:35"
        },
        {
          "id": "MDRW2100027333.1.6",
          "form": "네네 원래는 잘 안챙겨먹는데 오늘은 김밥 먹었습니다",
          "original_form": "네네 원래는 잘 안챙겨먹는데 오늘은 김밥 먹었습니다",
          "speaker_id": "2",
          "time": "20210902 11:35"
        },
        {
          "id": "MDRW2100027333.1.7",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210902 11:35"
        },
        {
          "id": "MDRW2100027333.1.8",
          "form": "아침에 저는 장어를 먹었어요",
          "original_form": "아침에 저는 장어를 먹었어요",
          "speaker_id": "1",
          "time": "20210902 11:36"
        },
        {
          "id": "MDRW2100027333.1.9",
          "form": "구워서 한마리",
          "original_form": "구워서 한마리",
          "speaker_id": "1",
          "time": "20210902 11:36"
        },
        {
          "id": "MDRW2100027333.1.10",
          "form": "양념에 졸여서 두마리",
          "original_form": "양념에 졸여서 두마리",
          "speaker_id": "1",
          "time": "20210902 11:36"
        },
        {
          "id": "MDRW2100027333.1.11",
          "form": "아침은 든든하게 먹어요^^",
          "original_form": "아침은 든든하게 먹어요^^",
          "speaker_id": "1",
          "time": "20210902 11:36"
        },
        {
          "id": "MDRW2100027333.1.12",
          "form": "점심은 무얼드실거에요?",
          "original_form": "점심은 무얼드실거에요?",
          "speaker_id": "1",
          "time": "20210902 11:36"
        },
        {
          "id": "MDRW2100027333.1.13",
          "form": "헉 아침에 일찍 일어나시나봐요!",
          "original_form": "헉 아침에 일찍 일어나시나봐요!",
          "speaker_id": "2",
          "time": "20210902 11:36"
        },
        {
          "id": "MDRW2100027333.1.14",
          "form": "점심메뉴를 아직 못골랐어요",
          "original_form": "점심메뉴를 아직 못골랐어요",
          "speaker_id": "2",
          "time": "20210902 11:37"
        },
        {
          "id": "MDRW2100027333.1.15",
          "form": "아침은 어머니가 차려주세요^^",
          "original_form": "아침은 어머니가 차려주세요^^",
          "speaker_id": "1",
          "time": "20210902 11:37"
        },
        {
          "id": "MDRW2100027333.1.16",
          "form": "곧 점심이니 이제 슬슬 결정하세여",
          "original_form": "곧 점심이니 이제 슬슬 결정하세여",
          "speaker_id": "1",
          "time": "20210902 11:37"
        },
        {
          "id": "MDRW2100027333.1.17",
          "form": "샌드위치나",
          "original_form": "샌드위치나",
          "speaker_id": "1",
          "time": "20210902 11:37"
        },
        {
          "id": "MDRW2100027333.1.18",
          "form": "나가서 드시면 돈까스도 좋지요",
          "original_form": "나가서 드시면 돈까스도 좋지요",
          "speaker_id": "1",
          "time": "20210902 11:37"
        },
        {
          "id": "MDRW2100027333.1.19",
          "form": "아하 차려주시는군용!",
          "original_form": "아하 차려주시는군용!",
          "speaker_id": "2",
          "time": "20210902 11:38"
        },
        {
          "id": "MDRW2100027333.1.20",
          "form": "어제도 점심메뉴 결정 못해서 결국 샐러드 먹었어요..",
          "original_form": "어제도 점심메뉴 결정 못해서 결국 샐러드 먹었어요..",
          "speaker_id": "2",
          "time": "20210902 11:38"
        },
        {
          "id": "MDRW2100027333.1.21",
          "form": "대신 저녁식사는 제가 해요",
          "original_form": "대신 저녁식사는 제가 해요",
          "speaker_id": "1",
          "time": "20210902 11:38"
        },
        {
          "id": "MDRW2100027333.1.22",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210902 11:38"
        },
        {
          "id": "MDRW2100027333.1.23",
          "form": "샐러드 좋지요",
          "original_form": "샐러드 좋지요",
          "speaker_id": "1",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100027333.1.24",
          "form": "요리 잘하세요?",
          "original_form": "요리 잘하세요?",
          "speaker_id": "2",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100027333.1.25",
          "form": "어떤샐러드요?",
          "original_form": "어떤샐러드요?",
          "speaker_id": "1",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100027333.1.26",
          "form": "간편한 음식 만들어먹어요",
          "original_form": "간편한 음식 만들어먹어요",
          "speaker_id": "1",
          "time": "20210902 11:39"
        },
        {
