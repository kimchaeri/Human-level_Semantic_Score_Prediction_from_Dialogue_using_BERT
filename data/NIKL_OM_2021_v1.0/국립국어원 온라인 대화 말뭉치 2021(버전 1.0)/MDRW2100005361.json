      "utterance": [
        {
          "id": "MDRW2100005361.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005361.1.2",
          "form": "안녕 애기야",
          "original_form": "안녕 애기야",
          "speaker_id": "2",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005361.1.3",
          "form": "누가봐도..",
          "original_form": "누가봐도..",
          "speaker_id": "2",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005361.1.4",
          "form": "애기인데요",
          "original_form": "애기인데요",
          "speaker_id": "2",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005361.1.5",
          "form": "애기?",
          "original_form": "애기?",
          "speaker_id": "1",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005361.1.6",
          "form": "그게 뭐지?",
          "original_form": "그게 뭐지?",
          "speaker_id": "1",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005361.1.7",
          "form": "그게 뭐지 하면서",
          "original_form": "그게 뭐지 하면서",
          "speaker_id": "2",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005361.1.8",
          "form": "이미 반말인데요",
          "original_form": "이미 반말인데요",
          "speaker_id": "2",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005361.1.9",
          "form": "그렇네 그럼 본론으로 들어가지",
          "original_form": "그렇네 그럼 본론으로 들어가지",
          "speaker_id": "1",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005361.1.10",
          "form": "그러자",
          "original_form": "그러자",
          "speaker_id": "2",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005361.1.11",
          "form": "아 요즘",
          "original_form": "아 요즘",
          "speaker_id": "1",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005361.1.12",
          "form": "요새 책 읽어??",
          "original_form": "요새 책 읽어??",
          "speaker_id": "2",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005361.1.13",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005361.1.14",
          "form": "안읽는데",
          "original_form": "안읽는데",
          "speaker_id": "1",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005361.1.15",
          "form": "몇달전에 읽고 안읽었음",
          "original_form": "몇달전에 읽고 안읽었음",
          "speaker_id": "1",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005361.1.16",
          "form": "너는 뭐 읽고있니",
          "original_form": "너는 뭐 읽고있니",
          "speaker_id": "1",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005361.1.17",
          "form": "나도 안 읽고 있어ㅋㅋ",
          "original_form": "나도 안 읽고 있어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005361.1.18",
          "form": "최근에 읽은 책 제목은 기억하니",
          "original_form": "최근에 읽은 책 제목은 기억하니",
          "speaker_id": "1",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005361.1.19",
          "form": "가장 최근에 읽은 게",
          "original_form": "가장 최근에 읽은 게",
          "speaker_id": "2",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005361.1.20",
          "form": "전공책 뿐..",
          "original_form": "전공책 뿐..",
          "speaker_id": "2",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005361.1.21",
          "form": "책을 읽어야지 하는데 막상 손이 안 가",
          "original_form": "책을 읽어야지 하는데 막상 손이 안 가",
          "speaker_id": "2",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005361.1.22",
          "form": "그거 맞다",
          "original_form": "그거 맞다",
          "speaker_id": "1",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005361.1.23",
          "form": "책 읽을 엄두가 안 난다..",
          "original_form": "책 읽을 엄두가 안 난다..",
          "speaker_id": "1",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005361.1.24",
          "form": "맞아...",
          "original_form": "맞아...",
          "speaker_id": "2",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005361.1.25",
          "form": "시작을 해야 결과가 있는데",
          "original_form": "시작을 해야 결과가 있는데",
          "speaker_id": "2",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005361.1.26",
          "form": "시작도 안 하고 있음",
          "original_form": "시작도 안 하고 있음",
          "speaker_id": "2",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005361.1.27",
          "form": "책 정말 처음 읽는 게 너무 힘듦",
          "original_form": "책 정말 처음 읽는 게 너무 힘듦",
          "speaker_id": "1",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005361.1.28",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005361.1.29",
          "form": "읽으면 그 뒤는 알아서 읽을 수 있는데",
          "original_form": "읽으면 그 뒤는 알아서 읽을 수 있는데",
          "speaker_id": "1",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005361.1.30",
          "form": "근데 막상 읽었는데 재밌다?",
          "original_form": "근데 막상 읽었는데 재밌다?",
          "speaker_id": "2",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005361.1.31",
          "form": "그럼 쭉 읽음",
          "original_form": "그럼 쭉 읽음",
          "speaker_id": "2",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005361.1.32",
          "form": "인정",
          "original_form": "인정",
          "speaker_id": "1",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005361.1.33",
          "form": "재미없으면 못읽지만",
          "original_form": "재미없으면 못읽지만",
          "speaker_id": "1",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005361.1.34",
          "form": "고3때 책 많이 읽었는데",
          "original_form": "고3때 책 많이 읽었는데",
          "speaker_id": "2",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005361.1.35",
          "form": "그때는 책이 재밌었다",
          "original_form": "그때는 책이 재밌었다",
          "speaker_id": "2",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005361.1.36",
          "form": "난 고3때 생기부때문에 읽었는데",
          "original_form": "난 고3때 생기부때문에 읽었는데",
          "speaker_id": "1",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005361.1.37",
          "form": "...",
          "original_form": "...",
          "speaker_id": "1",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005361.1.38",
          "form": "ㄷㄷ",
          "original_form": "ㄷㄷ",
          "speaker_id": "2",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005361.1.39",
          "form": "수시 지원 다 하고 나서 읽은 거라서",
          "original_form": "수시 지원 다 하고 나서 읽은 거라서",
          "speaker_id": "2",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005361.1.40",
          "form": "재밌었나봄",
          "original_form": "재밌었나봄",
          "speaker_id": "2",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005361.1.41",
          "form": "그때 소설 많이 읽었지",
          "original_form": "그때 소설 많이 읽었지",
          "speaker_id": "2",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005361.1.42",
          "form": "잘 읽다가 흥미도가 좀 떨어질때가 있었는데",
          "original_form": "잘 읽다가 흥미도가 좀 떨어질때가 있었는데",
          "speaker_id": "2",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005361.1.43",
          "form": "그럴 때 있지",
          "original_form": "그럴 때 있지",
          "speaker_id": "1",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005361.1.44",
          "form": "그때 읽기 시작한 책이 영애의 경호원이였거든",
          "original_form": "그때 읽기 시작한 책이 영애의 경호원이였거든",
          "speaker_id": "2",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005361.1.45",
          "form": "박씨가 보더니 경애의 경호원이래서",
          "original_form": "박씨가 보더니 경애의 경호원이래서",
          "speaker_id": "2",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005361.1.46",
          "form": "팍 식음",
          "original_form": "팍 식음",
          "speaker_id": "2",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005361.1.47",
          "form": "그 뒤로 안 읽었다",
          "original_form": "그 뒤로 안 읽었다",
          "speaker_id": "2",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005361.1.48",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005361.1.49",
          "form": "책에 대한 기억이..",
          "original_form": "책에 대한 기억이..",
          "speaker_id": "1",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005361.1.50",
          "form": "그 책 이제 못 읽는다",
          "original_form": "그 책 이제 못 읽는다",
          "speaker_id": "2",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005361.1.51",
          "form": "영애라고 생각해야되는데",
          "original_form": "영애라고 생각해야되는데",
          "speaker_id": "2",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005361.1.52",
          "form": "ㅠㅜㅜ",
          "original_form": "ㅠㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005361.1.53",
          "form": "....",
          "original_form": "....",
          "speaker_id": "1",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005361.1.54",
          "form": "책이 안쓰럽네..",
          "original_form": "책이 안쓰럽네..",
          "speaker_id": "1",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005361.1.55",
          "form": "너도..",
          "original_form": "너도..",
          "speaker_id": "1",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005361.1.56",
          "form": "그 뒤에도 추리 소설 보려고 했는데",
          "original_form": "그 뒤에도 추리 소설 보려고 했는데",
          "speaker_id": "2",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005361.1.57",
          "form": "하필.. 그친구가..",
          "original_form": "하필.. 그친구가..",
          "speaker_id": "1",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005361.1.58",
          "form": "ㅜㅜㅜ",
          "original_form": "ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005361.1.59",
          "form": "책이 일본 거야",
          "original_form": "책이 일본 거야",
          "speaker_id": "2",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005361.1.60",
          "form": "뭐야 그사람건가",
          "original_form": "뭐야 그사람건가",
          "speaker_id": "1",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005361.1.61",
          "form": "그 유명한 추리소설 작가",
          "original_form": "그 유명한 추리소설 작가",
          "speaker_id": "1",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005361.1.62",
          "form": "그 분 말고 다른 분!",
          "original_form": "그 분 말고 다른 분!",
          "speaker_id": "2",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005361.1.63",
          "form": "누구지",
          "original_form": "누구지",
          "speaker_id": "1",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005361.1.64",
          "form": "책장 넘기면 시작 부분에 건물 도면?",
          "original_form": "책장 넘기면 시작 부분에 건물 도면?",
          "speaker_id": "2",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005361.1.65",
          "form": "그런게 있길래",
          "original_form": "그런게 있길래",
          "speaker_id": "2",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005361.1.66",
          "form": "위치도 알려주네 좋다~ 했거든",
          "original_form": "위치도 알려주네 좋다~ 했거든",
          "speaker_id": "2",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005361.1.67",
          "form": "책 시작하는데 앞장에 있는 건물 위치 말하고",
          "original_form": "책 시작하는데 앞장에 있는 건물 위치 말하고",
          "speaker_id": "2",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005361.1.68",
          "form": "도면?",
          "original_form": "도면?",
          "speaker_id": "1",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005361.1.69",
          "form": "ㄷㄷ",
          "original_form": "ㄷㄷ",
          "speaker_id": "1",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005361.1.70",
          "form": "캐릭터 이름이 일본인이라서 길고 좀 어렵더라고",
          "original_form": "캐릭터 이름이 일본인이라서 길고 좀 어렵더라고",
          "speaker_id": "2",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005361.1.71",
          "form": "읽기전에 다 이해하고 기억한 후에 책을 읽어야지",
          "original_form": "읽기전에 다 이해하고 기억한 후에 책을 읽어야지",
          "speaker_id": "2",
          "time": "20210821 22:25"
        },
        {
          "id": "MDRW2100005361.1.72",
          "form": "아 맞아",
          "original_form": "아 맞아",
          "speaker_id": "1",
          "time": "20210821 22:25"
        },
        {
          "id": "MDRW2100005361.1.73",
