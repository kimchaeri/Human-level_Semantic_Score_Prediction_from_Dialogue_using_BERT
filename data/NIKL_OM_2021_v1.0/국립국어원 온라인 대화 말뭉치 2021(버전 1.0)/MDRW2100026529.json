      "utterance": [
        {
          "id": "MDRW2100026529.1.1",
          "form": "장날이 토욜이던가",
          "original_form": "장날이 토욜이던가",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026529.1.2",
          "form": "그르네 토요일",
          "original_form": "그르네 토요일",
          "speaker_id": "1",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100026529.1.3",
          "form": "가서 번데기랑 납작만두랑 꽈배기 사와야지ㅋㅋ",
          "original_form": "가서 번데기랑 납작만두랑 꽈배기 사와야지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100026529.1.4",
          "form": "아 납작만두 고새 잊음 ㅋㅋㅋ",
          "original_form": "아 납작만두 고새 잊음 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100026529.1.5",
          "form": "뭐여 계속 먹고싶었대놓고ㅋㅋㅋ",
          "original_form": "뭐여 계속 먹고싶었대놓고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026529.1.6",
          "form": "그르게 ㅋㅋㅋㅋ",
          "original_form": "그르게 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026529.1.7",
          "form": "토욜 기온25도밖에 안되든데 시장까지 걸어가볼깡",
          "original_form": "토욜 기온25도밖에 안되든데 시장까지 걸어가볼깡",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026529.1.8",
          "form": "운동 좀 미리해야 이것저것 사먹지ㅋㅋ",
          "original_form": "운동 좀 미리해야 이것저것 사먹지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100026529.1.9",
          "form": "난 생각만해도 지침 ㅋㅋㅋ",
          "original_form": "난 생각만해도 지침 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026529.1.10",
          "form": "하긴 25도라도 해 쨍쨍하면 덥겠지",
          "original_form": "하긴 25도라도 해 쨍쨍하면 덥겠지",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100026529.1.11",
          "form": "납작만두 먹고싶을때 딱 먹었어야는데 벌써 좀 식음 ㅋㅋㅋㅋ",
          "original_form": "납작만두 먹고싶을때 딱 먹었어야는데 벌써 좀 식음 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026529.1.12",
          "form": "타이밍을 놓쳤군ㅋㅋㅋ",
          "original_form": "타이밍을 놓쳤군ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026529.1.13",
          "form": "비빔만두 만들 생각하니 귀찮아졌다 ㅋㅋㅋ",
          "original_form": "비빔만두 만들 생각하니 귀찮아졌다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026529.1.14",
          "form": "하긴 야채 채썰기...ㅋㅋ",
          "original_form": "하긴 야채 채썰기...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026529.1.15",
          "form": "사서 얼리는 한이 있어도 사놔야지 ㅋㅋㅋㅋ",
          "original_form": "사서 얼리는 한이 있어도 사놔야지 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:35"
        }
