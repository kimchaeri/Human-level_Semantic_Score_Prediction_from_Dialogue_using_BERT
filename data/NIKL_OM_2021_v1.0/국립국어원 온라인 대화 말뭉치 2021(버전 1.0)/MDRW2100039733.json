        {
          "id": "MDRW2100039733.1.1",
          "form": "네 잘 먹었어요",
          "original_form": "네 잘 먹었어요",
          "speaker_id": "2",
          "time": "20210903 21:06"
        },
        {
          "id": "MDRW2100039733.1.2",
          "form": "뭐드셧어요",
          "original_form": "뭐드셧어요",
          "speaker_id": "1",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039733.1.3",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100039733.1.4",
          "form": "돈까스와 파스타요",
          "original_form": "돈까스와 파스타요",
          "speaker_id": "2",
          "time": "20210903 21:57"
        },
        {
          "id": "MDRW2100039733.1.5",
          "form": "오 집에서요?",
          "original_form": "오 집에서요?",
          "speaker_id": "1",
          "time": "20210903 21:59"
        },
        {
          "id": "MDRW2100039733.1.6",
          "form": "네 집에서 먹었어요",
          "original_form": "네 집에서 먹었어요",
          "speaker_id": "2",
          "time": "20210903 21:59"
        },
        {
          "id": "MDRW2100039733.1.7",
          "form": "오 누가요리헷는데?",
          "original_form": "오 누가요리헷는데?",
          "speaker_id": "1",
          "time": "20210903 21:59"
        },
        {
          "id": "MDRW2100039733.1.8",
          "form": "요리는 아무도 안 했죠",
          "original_form": "요리는 아무도 안 했죠",
          "speaker_id": "2",
          "time": "20210903 22:03"
        },
        {
          "id": "MDRW2100039733.1.9",
          "form": "배달의 힘을 빌렸어요",
          "original_form": "배달의 힘을 빌렸어요",
          "speaker_id": "2",
          "time": "20210903 22:03"
        },
        {
          "id": "MDRW2100039733.1.10",
          "form": "오 식당이름이?",
          "original_form": "오 식당이름이?",
          "speaker_id": "1",
          "time": "20210903 22:05"
        },
        {
          "id": "MDRW2100039733.1.11",
          "form": "딜리버리 쿡이었던가? 정확히는 기억 안 나요",
          "original_form": "딜리버리 쿡이었던가? 정확히는 기억 안 나요",
          "speaker_id": "2",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039733.1.12",
          "form": "매장을 동생이 골랐어요\r\n걔가 사는거라서요",
          "original_form": "매장을 동생이 골랐어요\r\n걔가 사는거라서요",
          "speaker_id": "2",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039733.1.13",
          "form": "동생이 몇살?",
          "original_form": "동생이 몇살?",
          "speaker_id": "1",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100039733.1.14",
          "form": "26살요",
          "original_form": "26살요",
          "speaker_id": "2",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100039733.1.15",
          "form": "오 집앞??",
          "original_form": "오 집앞??",
          "speaker_id": "1",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100039733.1.16",
