        },
        {
          "id": "MDRW2100042915.1.3",
          "form": "게임하시는거있으세요?",
          "original_form": "게임하시는거있으세요?",
          "speaker_id": "1",
          "time": "20210907 11:25"
        },
        {
          "id": "MDRW2100042915.1.4",
          "form": "요즘 쿠킹덤 하고 있어요",
          "original_form": "요즘 쿠킹덤 하고 있어요",
          "speaker_id": "2",
          "time": "20210907 11:26"
        },
        {
          "id": "MDRW2100042915.1.5",
          "form": "재밌나요ㅎ슷",
          "original_form": "재밌나요ㅎ슷",
          "speaker_id": "1",
          "time": "20210907 11:27"
        },
        {
          "id": "MDRW2100042915.1.6",
          "form": "그냥저냥하고 있어요 그거 아니면 달리 할 것도 없어서...",
          "original_form": "그냥저냥하고 있어요 그거 아니면 달리 할 것도 없어서...",
          "speaker_id": "2",
          "time": "20210907 11:50"
        },
        {
          "id": "MDRW2100042915.1.7",
          "form": "킬링타임용으로좋군요",
          "original_form": "킬링타임용으로좋군요",
          "speaker_id": "1",
          "time": "20210907 11:52"
        },
        {
          "id": "MDRW2100042915.1.8",
          "form": "네ㅋㅋㅋㅋㅋㅋ 무슨 게임 하세요?",
          "original_form": "네ㅋㅋㅋㅋㅋㅋ 무슨 게임 하세요?",
          "speaker_id": "2",
          "time": "20210907 11:53"
        },
        {
          "id": "MDRW2100042915.1.9",
          "form": "음 저는 모두의마블?그거했었어요ㅎ",
          "original_form": "음 저는 모두의마블?그거했었어요ㅎ",
          "speaker_id": "1",
          "time": "20210907 11:54"
        },
        {
          "id": "MDRW2100042915.1.10",
          "form": "지금은 게임 안 하세요?",
          "original_form": "지금은 게임 안 하세요?",
