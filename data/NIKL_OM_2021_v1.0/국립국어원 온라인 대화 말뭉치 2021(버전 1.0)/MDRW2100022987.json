      "utterance": [
        {
          "id": "MDRW2100022987.1.1",
          "form": "다시동물로 ㅋㅋ",
          "original_form": "다시동물로 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:07"
        },
        {
          "id": "MDRW2100022987.1.2",
          "form": "짐승같은 남자요? ㅋㅋㅋㅋ",
          "original_form": "짐승같은 남자요? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:27"
        },
        {
          "id": "MDRW2100022987.1.3",
          "form": "네?ㅋㅋㅋㅋㅋ",
          "original_form": "네?ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:44"
        },
        {
          "id": "MDRW2100022987.1.4",
          "form": "2pm?ㅋㅋㅋ",
          "original_form": "2pm?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:44"
        },
        {
          "id": "MDRW2100022987.1.5",
          "form": "짐승 가장 좋은게 뭐에요? 강아지? 토끼?",
          "original_form": "짐승 가장 좋은게 뭐에요? 강아지? 토끼?",
          "speaker_id": "1",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100022987.1.6",
          "form": "동물중에는 고양이 강아지 구리고 영상은 해달보는 것도 좋아해요 해달 귀여움",
          "original_form": "동물중에는 고양이 강아지 구리고 영상은 해달보는 것도 좋아해요 해달 귀여움",
          "speaker_id": "2",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100022987.1.7",
          "form": "해달이염? 해달도 개인이 키울 수 있나요?",
          "original_form": "해달이염? 해달도 개인이 키울 수 있나요?",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100022987.1.8",
          "form": "아니여 ㅋㅋㅋㅋ 그냥 해달 조개 까먹는거라던가 손잡고 자는게 귀여워서 ㅋㅋㅋ",
          "original_form": "아니여 ㅋㅋㅋㅋ 그냥 해달 조개 까먹는거라던가 손잡고 자는게 귀여워서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:41"
        },
        {
          "id": "MDRW2100022987.1.9",
          "form": "아.. 그 애기도 배위에 올려놓디 않나요?",
          "original_form": "아.. 그 애기도 배위에 올려놓디 않나요?",
          "speaker_id": "1",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100022987.1.10",
          "form": "마자여 배위에 올려놓고 둥둥 떠다녀요 ㅋㅋㅋ",
          "original_form": "마자여 배위에 올려놓고 둥둥 떠다녀요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:57"
        },
        {
          "id": "MDRW2100022987.1.11",
          "form": "무슨 동물 좋아하세요?",
          "original_form": "무슨 동물 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 22:57"
        },
        {
          "id": "MDRW2100022987.1.12",
          "form": "고양이 계열이랑.. 여우요..",
          "original_form": "고양이 계열이랑.. 여우요..",
          "speaker_id": "1",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100022987.1.13",
          "form": "여우도 음청 귀여움.. 개의 상위호환",
          "original_form": "여우도 음청 귀여움.. 개의 상위호환",
          "speaker_id": "1",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100022987.1.14",
          "form": "티벳여우도요?",
          "original_form": "티벳여우도요?",
          "speaker_id": "2",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100022987.1.15",
          "form": "아.. 그걸로 파고 들어오시네.. ㅋㅋㅋ",
          "original_form": "아.. 그걸로 파고 들어오시네.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100022987.1.16",
          "form": "김상궁...",
          "original_form": "김상궁...",
          "speaker_id": "2",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100022987.1.17",
          "form": "티벳여우도 뭐.. 새끼는.. ㅋㅋ",
          "original_form": "티벳여우도 뭐.. 새끼는.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100022987.1.18",
          "form": "ㅋㅋㅋㅋㅋ사막여우는 귀여운거 인정",
          "original_form": "ㅋㅋㅋㅋㅋ사막여우는 귀여운거 인정",
          "speaker_id": "2",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100022987.1.19",
          "form": "붉은 여우도 엄청 귀여워요..",
          "original_form": "붉은 여우도 엄청 귀여워요..",
          "speaker_id": "1",
          "time": "20210829 23:02"
        },
        {
          "id": "MDRW2100022987.1.20",
          "form": "랫서팬더랑 색구성이 똑같아요.. 검정 갈색 흰색",
          "original_form": "랫서팬더랑 색구성이 똑같아요.. 검정 갈색 흰색",
          "speaker_id": "1",
          "time": "20210829 23:02"
        },
        {
          "id": "MDRW2100022987.1.21",
          "form": "ㅋㅋㅋㅋㅋ 저 그거봤는데",
          "original_form": "ㅋㅋㅋㅋㅋ 저 그거봤는데",
          "speaker_id": "2",
          "time": "20210829 23:06"
        },
        {
          "id": "MDRW2100022987.1.22",
          "form": "하얀이불 눈인줄 알고 여우가 막 파는거",
          "original_form": "하얀이불 눈인줄 알고 여우가 막 파는거",
          "speaker_id": "2",
          "time": "20210829 23:06"
        },
        {
          "id": "MDRW2100022987.1.23",
          "form": "귀여웠어요 ㅋㅋㅋ",
          "original_form": "귀여웠어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:06"
        },
        {
          "id": "MDRW2100022987.1.24",
          "form": "여우 꼬리도 엄청 이쁨.. \n막 엄청 두껍고 크고 우람한게..",
          "original_form": "여우 꼬리도 엄청 이쁨.. \n막 엄청 두껍고 크고 우람한게..",
          "speaker_id": "1",
          "time": "20210829 23:06"
        },
        {
          "id": "MDRW2100022987.1.25",
          "form": "이상형이 여우나 고양이 상이시겠어용ㅋㅋㅋ",
