      "utterance": [
        {
          "id": "MDRW2100033624.1.1",
          "form": "집에 가면 무슨일부터 먼저하세요?",
          "original_form": "집에 가면 무슨일부터 먼저하세요?",
          "speaker_id": "1",
          "time": "20210824 19:32"
        },
        {
          "id": "MDRW2100033624.1.2",
          "form": "저는 바로 씻고 잠옷입어요ㅎㅎ",
          "original_form": "저는 바로 씻고 잠옷입어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 19:34"
        },
        {
          "id": "MDRW2100033624.1.3",
          "form": "저도 바로 씻어요",
          "original_form": "저도 바로 씻어요",
          "speaker_id": "1",
          "time": "20210824 19:35"
        },
        {
          "id": "MDRW2100033624.1.4",
          "form": "넘 늦어지면 귀찮아져서요",
          "original_form": "넘 늦어지면 귀찮아져서요",
          "speaker_id": "1",
          "time": "20210824 19:35"
        },
        {
          "id": "MDRW2100033624.1.5",
          "form": "맞아요 바로 안씻으면 안씻게돼요ㅋㅋ",
          "original_form": "맞아요 바로 안씻으면 안씻게돼요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:37"
        },
        {
          "id": "MDRW2100033624.1.6",
          "form": "그대로 퍼지면 절대안됌 ㅋㅋ 들어와서 손씻는김에 다 씻어야해요",
          "original_form": "그대로 퍼지면 절대안됌 ㅋㅋ 들어와서 손씻는김에 다 씻어야해요",
          "speaker_id": "1",
          "time": "20210824 19:46"
        },
        {
          "id": "MDRW2100033624.1.7",
          "form": "비로 옷 챙겨서 안들어가면 누워버려서ㅋㅋ",
          "original_form": "비로 옷 챙겨서 안들어가면 누워버려서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:47"
        },
        {
          "id": "MDRW2100033624.1.8",
          "form": "그리고 아침되어버림..ㅜㅜㅜ",
          "original_form": "그리고 아침되어버림..ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 19:47"
        },
        {
          "id": "MDRW2100033624.1.9",
          "form": "ㅋㅋㅋㅋ맞아요ㅋㅋ바로 기절ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ맞아요ㅋㅋ바로 기절ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:48"
        },
        {
          "id": "MDRW2100033624.1.10",
          "form": "오늘은 비오니까 더 다운되네요",
          "original_form": "오늘은 비오니까 더 다운되네요",
          "speaker_id": "1",
          "time": "20210824 19:48"
        },
        {
          "id": "MDRW2100033624.1.11",
          "form": "더욱 빠릿하게 움직여야해요",
          "original_form": "더욱 빠릿하게 움직여야해요",
          "speaker_id": "1",
          "time": "20210824 19:48"
        },
        {
          "id": "MDRW2100033624.1.12",
          "form": "맞아요 제습기틀고 빨래걷구 있어요 ㅋㅋ",
          "original_form": "맞아요 제습기틀고 빨래걷구 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:49"
        },
        {
          "id": "MDRW2100033624.1.13",
          "form": "아 빨래하셨군요 비오는데ㅜ",
          "original_form": "아 빨래하셨군요 비오는데ㅜ",
          "speaker_id": "1",
          "time": "20210824 19:50"
        },
        {
          "id": "MDRW2100033624.1.14",
          "form": "주말에 안해서 해야했어요ㅠㅠㅋㅋ",
          "original_form": "주말에 안해서 해야했어요ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:51"
        },
        {
          "id": "MDRW2100033624.1.15",
          "form": "아..그래도 여름옷은 빨리말라서좋아요",
          "original_form": "아..그래도 여름옷은 빨리말라서좋아요",
          "speaker_id": "1",
          "time": "20210824 19:52"
        },
        {
          "id": "MDRW2100033624.1.16",
          "form": "네 얇아서ㅋㅋ니트빨래할 생각하니 벌써 갑갑하네요ㅠㅠ",
          "original_form": "네 얇아서ㅋㅋ니트빨래할 생각하니 벌써 갑갑하네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 19:52"
        }
