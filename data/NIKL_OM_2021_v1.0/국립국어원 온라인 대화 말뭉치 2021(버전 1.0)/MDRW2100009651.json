      "utterance": [
        {
          "id": "MDRW2100009651.1.1",
          "form": "내일 부산에는 태풍이 온대요!",
          "original_form": "내일 부산에는 태풍이 온대요!",
          "speaker_id": "1",
          "time": "20210823 02:26"
        },
        {
          "id": "MDRW2100009651.1.2",
          "form": "name2님 있는 곳은 어떠신가요?",
          "original_form": "&name2&님 있는 곳은 어떠신가요?",
          "speaker_id": "1",
          "time": "20210823 02:26"
        },
        {
          "id": "MDRW2100009651.1.3",
          "form": "제가 부산 쪽에 살고 있어서.. 아마 태풍의 영향을",
          "original_form": "제가 부산 쪽에 살고 있어서.. 아마 태풍의 영향을",
          "speaker_id": "2",
          "time": "20210823 02:27"
        },
        {
          "id": "MDRW2100009651.1.4",
          "form": "많이 받을 것 같습니다..",
          "original_form": "많이 받을 것 같습니다..",
          "speaker_id": "2",
          "time": "20210823 02:27"
        },
        {
          "id": "MDRW2100009651.1.5",
          "form": "저도 부산 살아요 ㅋㅋㅋ",
          "original_form": "저도 부산 살아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 02:27"
        },
        {
          "id": "MDRW2100009651.1.6",
          "form": "내일 부터 비가 온대요 ㅠㅠㅠㅠ 아웅",
          "original_form": "내일 부터 비가 온대요 ㅠㅠㅠㅠ 아웅",
          "speaker_id": "1",
          "time": "20210823 02:27"
        },
        {
          "id": "MDRW2100009651.1.7",
          "form": "아아 근처에 살고계시는군요",
          "original_form": "아아 근처에 살고계시는군요",
          "speaker_id": "2",
          "time": "20210823 02:28"
        },
        {
          "id": "MDRW2100009651.1.8",
          "form": "안그래도 이번 주 내내 비온다고해서..",
          "original_form": "안그래도 이번 주 내내 비온다고해서..",
          "speaker_id": "2",
          "time": "20210823 02:28"
        },
        {
          "id": "MDRW2100009651.1.9",
          "form": "걱정중입니다",
          "original_form": "걱정중입니다",
          "speaker_id": "2",
          "time": "20210823 02:28"
        },
        {
          "id": "MDRW2100009651.1.10",
          "form": "우산 챙겨 다녀야 해!!",
          "original_form": "우산 챙겨 다녀야 해!!",
          "speaker_id": "1",
          "time": "20210823 02:29"
        },
        {
          "id": "MDRW2100009651.1.11",
          "form": "나는 안나가고 싶은데 걱정이야ㅠㅠㅠ",
          "original_form": "나는 안나가고 싶은데 걱정이야ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 02:29"
        },
        {
          "id": "MDRW2100009651.1.12",
          "form": "혹시 나가실 일 있으신가요?",
          "original_form": "혹시 나가실 일 있으신가요?",
          "speaker_id": "2",
          "time": "20210823 02:29"
        },
        {
          "id": "MDRW2100009651.1.13",
          "form": "과외를 가고 있어서 내일 꼭 가야해요ㅠㅠ",
          "original_form": "과외를 가고 있어서 내일 꼭 가야해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 02:30"
        },
        {
          "id": "MDRW2100009651.1.14",
          "form": "아아 오늘이라고 해야하나",
          "original_form": "아아 오늘이라고 해야하나",
          "speaker_id": "2",
          "time": "20210823 02:30"
        },
        {
          "id": "MDRW2100009651.1.15",
          "form": "내일 당장 가시는 건가요?",
          "original_form": "내일 당장 가시는 건가요?",
          "speaker_id": "2",
          "time": "20210823 02:30"
        },
        {
          "id": "MDRW2100009651.1.16",
          "form": "맞아요 내일 오후 1시에요 ㅎㅎㅎ",
          "original_form": "맞아요 내일 오후 1시에요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 02:31"
        },
        {
          "id": "MDRW2100009651.1.17",
          "form": "비오면 더 가기 싫을 것 같은데 어쩌죠~~",
          "original_form": "비오면 더 가기 싫을 것 같은데 어쩌죠~~",
          "speaker_id": "1",
          "time": "20210823 02:31"
        },
        {
          "id": "MDRW2100009651.1.18",
          "form": "아님 비 약할 때 미리 가있으시는 건..? 좀 힘들까요?",
          "original_form": "아님 비 약할 때 미리 가있으시는 건..? 좀 힘들까요?",
          "speaker_id": "2",
          "time": "20210823 02:32"
        },
        {
          "id": "MDRW2100009651.1.19",
          "form": "언제 피크로 오나요??",
          "original_form": "언제 피크로 오나요??",
          "speaker_id": "1",
          "time": "20210823 02:33"
        },
        {
          "id": "MDRW2100009651.1.20",
          "form": "혹시 알고 계신가요 ㅎㅎ",
          "original_form": "혹시 알고 계신가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 02:33"
        },
        {
          "id": "MDRW2100009651.1.21",
          "form": "방금 찾아보니 화요일 새벽 3시쯤에 부산 근처? 쪽에 오는 것 같은데",
          "original_form": "방금 찾아보니 화요일 새벽 3시쯤에 부산 근처? 쪽에 오는 것 같은데",
          "speaker_id": "2",
          "time": "20210823 02:34"
        },
        {
          "id": "MDRW2100009651.1.22",
          "form": "월요일 저녁엔 나가계신가요?",
          "original_form": "월요일 저녁엔 나가계신가요?",
          "speaker_id": "2",
          "time": "20210823 02:34"
        },
        {
          "id": "MDRW2100009651.1.23",
          "form": "헉 그렇구나 내일 과외 말고는 안나가야 겠어요!",
          "original_form": "헉 그렇구나 내일 과외 말고는 안나가야 겠어요!",
          "speaker_id": "1",
          "time": "20210823 02:35"
        },
        {
          "id": "MDRW2100009651.1.24",
          "form": "다행히 새벽에 오는 군요 ㅎㅎㅎ",
          "original_form": "다행히 새벽에 오는 군요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 02:35"
        }
      ]
