      "utterance": [
        {
          "id": "MDRW2100016592.1.1",
          "form": "영화 즐겨보세요?",
          "original_form": "영화 즐겨보세요?",
          "speaker_id": "1",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016592.1.2",
          "form": "아니용ㅠ",
          "original_form": "아니용ㅠ",
          "speaker_id": "2",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016592.1.3",
          "form": "저도 잘 못 봐요. ",
          "original_form": "저도 잘 못 봐요. ",
          "speaker_id": "1",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100016592.1.4",
          "form": "나가서 봐야하는데 넘 불편하져",
          "original_form": "나가서 봐야하는데 넘 불편하져",
          "speaker_id": "2",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100016592.1.5",
          "form": "네 맞아요. 요즘에는 또 코로나 때문에 더 그러네요.",
          "original_form": "네 맞아요. 요즘에는 또 코로나 때문에 더 그러네요.",
          "speaker_id": "1",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100016592.1.6",
          "form": "콘서트가고싶어여ㅠㅠㅠㅜ",
          "original_form": "콘서트가고싶어여ㅠㅠㅠㅜ",
          "speaker_id": "2",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100016592.1.7",
          "form": "어떤 콘서트요?",
          "original_form": "어떤 콘서트요?",
          "speaker_id": "1",
          "time": "20210826 14:04"
        },
        {
          "id": "MDRW2100016592.1.8",
          "form": "그냥 소소하게 잠실주경기장에서하는 콘서트여..",
          "original_form": "그냥 소소하게 잠실주경기장에서하는 콘서트여..",
          "speaker_id": "2",
          "time": "20210826 14:07"
        },
        {
          "id": "MDRW2100016592.1.9",
          "form": "저는 미술관 가는 걸 좋아해요.",
          "original_form": "저는 미술관 가는 걸 좋아해요.",
          "speaker_id": "1",
          "time": "20210826 14:09"
        },
        {
          "id": "MDRW2100016592.1.10",
          "form": "오 이번에 국중박이랑 국현가셨어여?",
          "original_form": "오 이번에 국중박이랑 국현가셨어여?",
          "speaker_id": "2",
          "time": "20210826 14:11"
        },
        {
          "id": "MDRW2100016592.1.11",
          "form": "코로나 덕분에 몸 사리고 있어요 ㅠㅠ",
          "original_form": "코로나 덕분에 몸 사리고 있어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 14:12"
        },
        {
          "id": "MDRW2100016592.1.12",
          "form": "ㅠㅠㅠㅠㅠㅠ좀 널널해지면 다녀오세여",
          "original_form": "ㅠㅠㅠㅠㅠㅠ좀 널널해지면 다녀오세여",
          "speaker_id": "2",
          "time": "20210826 14:16"
        },
        {
          "id": "MDRW2100016592.1.13",
          "form": "네. 꼭 가보려고요.",
          "original_form": "네. 꼭 가보려고요.",
          "speaker_id": "1",
          "time": "20210826 14:17"
        },
        {
          "id": "MDRW2100016592.1.14",
          "form": "저는 액션이나 재난 영화 좋아하는데 어떤 종류의 영화 좋아하세요?",
          "original_form": "저는 액션이나 재난 영화 좋아하는데 어떤 종류의 영화 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 14:18"
        },
        {
          "id": "MDRW2100016592.1.15",
          "form": "저두여 마블이런거 좋아해여 공포도 좋은데",
          "original_form": "저두여 마블이런거 좋아해여 공포도 좋은데",
          "speaker_id": "2",
          "time": "20210826 14:20"
        },
        {
          "id": "MDRW2100016592.1.16",
          "form": "가리고보면 잘봐여",
          "original_form": "가리고보면 잘봐여",
          "speaker_id": "2",
          "time": "20210826 14:20"
        },
        {
          "id": "MDRW2100016592.1.17",
          "form": "공포 영화도 좋죠.",
          "original_form": "공포 영화도 좋죠.",
          "speaker_id": "1",
          "time": "20210826 14:21"
        },
        {
          "id": "MDRW2100016592.1.18",
          "form": "저는 할리퀸 영화 재밌던데요 ㅎㅎ",
          "original_form": "저는 할리퀸 영화 재밌던데요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 14:22"
        },
        {
          "id": "MDRW2100016592.1.19",
          "form": "여자들끼리 깨고 부시고 액션하니깐 속이 다 뚫리는 기분이었요. ㅎㅎ 영화 음악도 시원하니 좋았어요.",
          "original_form": "여자들끼리 깨고 부시고 액션하니깐 속이 다 뚫리는 기분이었요. ㅎㅎ 영화 음악도 시원하니 좋았어요.",
          "speaker_id": "1",
          "time": "20210826 14:22"
        },
        {
          "id": "MDRW2100016592.1.20",
          "form": "헐 저 그거 아직 안봤는데",
          "original_form": "헐 저 그거 아직 안봤는데",
          "speaker_id": "2",
          "time": "20210826 14:23"
        },
        {
          "id": "MDRW2100016592.1.21",
          "form": "블위도 아직 못봤구",
          "original_form": "블위도 아직 못봤구",
          "speaker_id": "2",
          "time": "20210826 14:23"
        },
        {
          "id": "MDRW2100016592.1.22",
          "form": "퓨ㅠㅠㅜ",
          "original_form": "퓨ㅠㅠㅜ",
          "speaker_id": "2",
          "time": "20210826 14:23"
        },
        {
          "id": "MDRW2100016592.1.23",
          "form": "재밌으니 한 번 보셔요. 추천추천 ㅎㅎ",
          "original_form": "재밌으니 한 번 보셔요. 추천추천 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 14:25"
        }
      ]
    }
  ]
