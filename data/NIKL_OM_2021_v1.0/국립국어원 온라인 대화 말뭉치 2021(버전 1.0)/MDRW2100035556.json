      "utterance": [
        {
          "id": "MDRW2100035556.1.1",
          "form": "배달음식도 시켜드시나요?",
          "original_form": "배달음식도 시켜드시나요?",
          "speaker_id": "1",
          "time": "20210829 20:25"
        },
        {
          "id": "MDRW2100035556.1.2",
          "form": "전 주로 치킨만 먹어요 교촌이요",
          "original_form": "전 주로 치킨만 먹어요 교촌이요",
          "speaker_id": "2",
          "time": "20210829 20:26"
        },
        {
          "id": "MDRW2100035556.1.3",
          "form": "나머지음식은 가서 먹는걸 선호해요",
          "original_form": "나머지음식은 가서 먹는걸 선호해요",
          "speaker_id": "2",
          "time": "20210829 20:26"
        },
        {
          "id": "MDRW2100035556.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210829 20:27"
        },
        {
          "id": "MDRW2100035556.1.5",
          "form": "가서먹으면좋죠ㅠㅠ",
          "original_form": "가서먹으면좋죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 20:27"
        },
        {
          "id": "MDRW2100035556.1.6",
          "form": "전그래도 요새 집에서 먹우려해여",
          "original_form": "전그래도 요새 집에서 먹우려해여",
          "speaker_id": "1",
          "time": "20210829 20:27"
        },
        {
          "id": "MDRW2100035556.1.7",
          "form": "지역에 확진자도 많이나오고 ㅠ",
          "original_form": "지역에 확진자도 많이나오고 ㅠ",
          "speaker_id": "1",
          "time": "20210829 20:27"
        },
        {
          "id": "MDRW2100035556.1.8",
          "form": "네 그래서요즘엔 맨날 집밥 치킨 이렇게 먹어요 ㅋㅋㅋ ㅠㅠㅠㅠㅠ 이제 슬슬 외식하고 싶은데,,,",
          "original_form": "네 그래서요즘엔 맨날 집밥 치킨 이렇게 먹어요 ㅋㅋㅋ ㅠㅠㅠㅠㅠ 이제 슬슬 외식하고 싶은데,,,",
          "speaker_id": "2",
          "time": "20210829 20:28"
        },
        {
          "id": "MDRW2100035556.1.9",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "1",
          "time": "20210829 20:29"
        },
        {
          "id": "MDRW2100035556.1.10",
          "form": "먀감시간도 애매하고",
          "original_form": "먀감시간도 애매하고",
          "speaker_id": "1",
          "time": "20210829 20:29"
        },
        {
          "id": "MDRW2100035556.1.11",
          "form": "9시에 닫음 이제 딱 7시 8시 가서 ㅁ거어야지",
          "original_form": "9시에 닫음 이제 딱 7시 8시 가서 ㅁ거어야지",
          "speaker_id": "2",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100035556.1.12",
          "form": "그 뒤엔 못먹죠 ㅠㅠㅋㅋ",
          "original_form": "그 뒤엔 못먹죠 ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100035556.1.13",
          "form": "수도권이시긴요 ㅠ 전 경북이라 아직 10시까지인거같아요ㅠㅠ",
          "original_form": "수도권이시긴요 ㅠ 전 경북이라 아직 10시까지인거같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 20:35"
        },
        {
          "id": "MDRW2100035556.1.14",
          "form": "그래됴 평일에 끝나고 가면 9시라서ㅠㅜㅜㅋㅋ",
          "original_form": "그래됴 평일에 끝나고 가면 9시라서ㅠㅜㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:36"
        },
        {
          "id": "MDRW2100035556.1.15",
          "form": "주말에만 낮에 중국집가고그래요 ㅋㅋ",
          "original_form": "주말에만 낮에 중국집가고그래요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:37"
        },
        {
          "id": "MDRW2100035556.1.16",
          "form": "ㅠㅠ 딱 밥만 먹고 집에 가라는 정책이죠",
          "original_form": "ㅠㅠ 딱 밥만 먹고 집에 가라는 정책이죠",
          "speaker_id": "2",
          "time": "20210829 20:37"
        },
        {
          "id": "MDRW2100035556.1.17",
          "form": "호프집갈 시간이 진짜 안나와요",
          "original_form": "호프집갈 시간이 진짜 안나와요",
          "speaker_id": "2",
          "time": "20210829 20:37"
        },
        {
          "id": "MDRW2100035556.1.18",
          "form": "ㅠㅠ 장사하시는분도 힘들겠어요ㅠ",
          "original_form": "ㅠㅠ 장사하시는분도 힘들겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210829 20:38"
        },
        {
          "id": "MDRW2100035556.1.19",
          "form": "6시이후 인원도 2명이라던데",
          "original_form": "6시이후 인원도 2명이라던데",
          "speaker_id": "1",
          "time": "20210829 20:38"
        },
        {
          "id": "MDRW2100035556.1.20",
          "form": "백신 맞으면 4인도 가능하다지만 ㅋㅋ 하나씩 검사하기 힘들죠",
          "original_form": "백신 맞으면 4인도 가능하다지만 ㅋㅋ 하나씩 검사하기 힘들죠",
          "speaker_id": "2",
          "time": "20210829 20:55"
        },
        {
          "id": "MDRW2100035556.1.21",
          "form": "전 매번 둘이서 식당을 가서 정확히는몰랏네요ㅠ",
          "original_form": "전 매번 둘이서 식당을 가서 정확히는몰랏네요ㅠ",
          "speaker_id": "1",
          "time": "20210829 22:40"
        },
        {
          "id": "MDRW2100035556.1.22",
          "form": "저도 잘 몰라요ㅋㅋㅋ",
          "original_form": "저도 잘 몰라요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100035556.1.23",
          "form": "여럿이 외식안한지 꽤 되서요",
          "original_form": "여럿이 외식안한지 꽤 되서요",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100035556.1.24",
          "form": "그쵸ㅠㅠ",
          "original_form": "그쵸ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100035556.1.25",
          "form": "모임도 없고ㅠ",
          "original_form": "모임도 없고ㅠ",
          "speaker_id": "1",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100035556.1.26",
          "form": "친한몇명은 그냥 가끔 집에ㅠ",
          "original_form": "친한몇명은 그냥 가끔 집에ㅠ",
          "speaker_id": "1",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100035556.1.27",
          "form": "그렇게 할라믄 청소를 해야되잖아요ㅠ",
          "original_form": "그렇게 할라믄 청소를 해야되잖아요ㅠ",
          "speaker_id": "2",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100035556.1.28",
          "form": "그래서 못불렀어요",
          "original_form": "그래서 못불렀어요",
          "speaker_id": "2",
