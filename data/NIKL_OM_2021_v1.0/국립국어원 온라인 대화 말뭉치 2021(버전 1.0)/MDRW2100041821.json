      "utterance": [
        {
          "id": "MDRW2100041821.1.1",
          "form": "박사님들이 만든 조미료는 맛있어요",
          "original_form": "박사님들이 만든 조미료는 맛있어요",
          "speaker_id": "2",
          "time": "20210906 16:19"
        },
        {
          "id": "MDRW2100041821.1.2",
          "form": "도시락 같은데에 있는 김치볶음색이 딱 라면스프색!!!",
          "original_form": "도시락 같은데에 있는 김치볶음색이 딱 라면스프색!!!",
          "speaker_id": "1",
          "time": "20210906 16:19"
        },
        {
          "id": "MDRW2100041821.1.3",
          "form": "ㅋㅋㅋ그런거같네요",
          "original_form": "ㅋㅋㅋ그런거같네요",
          "speaker_id": "2",
          "time": "20210906 16:19"
        },
        {
          "id": "MDRW2100041821.1.4",
          "form": "ㅋㅋㅋ 김치볶음 비법을 알아버리셨네요",
          "original_form": "ㅋㅋㅋ 김치볶음 비법을 알아버리셨네요",
          "speaker_id": "1",
          "time": "20210906 16:20"
        },
        {
          "id": "MDRW2100041821.1.5",
          "form": "그러니 안쓸수가없어요",
          "original_form": "그러니 안쓸수가없어요",
          "speaker_id": "2",
          "time": "20210906 16:21"
        },
        {
          "id": "MDRW2100041821.1.6",
          "form": "된장찌개에 넣으면 기가막힙니다",
          "original_form": "된장찌개에 넣으면 기가막힙니다",
          "speaker_id": "2",
          "time": "20210906 16:21"
        },
        {
          "id": "MDRW2100041821.1.7",
          "form": "저는 들기름만 넣을줄 알았는데 다음번엔 라면스프를 살짝 넣어보겠습니다",
          "original_form": "저는 들기름만 넣을줄 알았는데 다음번엔 라면스프를 살짝 넣어보겠습니다",
          "speaker_id": "1",
          "time": "20210906 16:21"
        },
        {
          "id": "MDRW2100041821.1.8",
          "form": "진짜여???",
          "original_form": "진짜여???",
          "speaker_id": "1",
          "time": "20210906 16:21"
        },
        {
          "id": "MDRW2100041821.1.9",
          "form": "된장찌개에 라면스프 어울리나보네요",
          "original_form": "된장찌개에 라면스프 어울리나보네요",
          "speaker_id": "1",
          "time": "20210906 16:21"
        },
        {
          "id": "MDRW2100041821.1.10",
          "form": "감칠맛을 살려주죠",
          "original_form": "감칠맛을 살려주죠",
          "speaker_id": "2",
          "time": "20210906 16:22"
        },
        {
          "id": "MDRW2100041821.1.11",
          "form": "오 근데 괜찮을거 같아요",
          "original_form": "오 근데 괜찮을거 같아요",
          "speaker_id": "1",
          "time": "20210906 16:22"
        },
        {
          "id": "MDRW2100041821.1.12",
          "form": "네 해보세요ㅋㅋ",
          "original_form": "네 해보세요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:22"
        },
        {
          "id": "MDRW2100041821.1.13",
          "form": "해물넣은 된장찌개에 특히",
          "original_form": "해물넣은 된장찌개에 특히",
          "speaker_id": "1",
          "time": "20210906 16:22"
        },
        {
          "id": "MDRW2100041821.1.14",
          "form": "네 감사해요 해볼게욬ㅋㅋㅋ",
          "original_form": "네 감사해요 해볼게욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:22"
        },
        {
          "id": "MDRW2100041821.1.15",
          "form": "조갯살 넣는거 좋아하거든요 ㅎㅎ",
          "original_form": "조갯살 넣는거 좋아하거든요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 16:23"
        },
        {
          "id": "MDRW2100041821.1.16",
          "form": "어울릴거 같습니다",
          "original_form": "어울릴거 같습니다",
          "speaker_id": "1",
          "time": "20210906 16:23"
        },
        {
          "id": "MDRW2100041821.1.17",
          "form": "맛있겠네요",
          "original_form": "맛있겠네요",
          "speaker_id": "2",
          "time": "20210906 16:23"
        },
        {
          "id": "MDRW2100041821.1.18",
          "form": "ㅎㅎ 오늘 저녁 계획 있으세요?",
          "original_form": "ㅎㅎ 오늘 저녁 계획 있으세요?",
          "speaker_id": "1",
          "time": "20210906 16:24"
        },
        {
          "id": "MDRW2100041821.1.19",
          "form": "두부조림이나 할까 생각중이요",
          "original_form": "두부조림이나 할까 생각중이요",
          "speaker_id": "2",
          "time": "20210906 16:24"
        },
        {
          "id": "MDRW2100041821.1.20",
          "form": "두부조림 맛있겠네요",
          "original_form": "두부조림 맛있겠네요",
          "speaker_id": "1",
          "time": "20210906 16:25"
        },
        {
          "id": "MDRW2100041821.1.21",
          "form": "거기에도 스프살짝 이신가요? ㅎㅎㅎ",
          "original_form": "거기에도 스프살짝 이신가요? ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 16:25"
        },
        {
          "id": "MDRW2100041821.1.22",
          "form": "ㅋㅋㅋ 일단해보고 맛없으면 몰래 넣어보려고요",
          "original_form": "ㅋㅋㅋ 일단해보고 맛없으면 몰래 넣어보려고요",
          "speaker_id": "2",
          "time": "20210906 16:26"
        },
        {
          "id": "MDRW2100041821.1.23",
          "form": "스프 넣는것에 왠지 당당하지 못하네요ㅋㅋ",
          "original_form": "스프 넣는것에 왠지 당당하지 못하네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:30"
        }
      ]
    }
