      "utterance": [
        {
          "id": "MDRW2100029418.1.1",
          "form": "적당히 가려진 햇볕이라면 금상첨화구요",
          "original_form": "적당히 가려진 햇볕이라면 금상첨화구요",
          "speaker_id": "2",
          "time": "20210903 07:33"
        },
        {
          "id": "MDRW2100029418.1.2",
          "form": "네ㅎㅎ 제 생일도 가을이거든요",
          "original_form": "네ㅎㅎ 제 생일도 가을이거든요",
          "speaker_id": "2",
          "time": "20210903 07:33"
        },
        {
          "id": "MDRW2100029418.1.3",
          "form": "저도 가을이 생일이예요~",
          "original_form": "저도 가을이 생일이예요~",
          "speaker_id": "1",
          "time": "20210903 07:34"
        },
        {
          "id": "MDRW2100029418.1.4",
          "form": "동질감이 느껴지네요ㅎㅎ",
          "original_form": "동질감이 느껴지네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 07:34"
        },
        {
          "id": "MDRW2100029418.1.5",
          "form": "그렇네요ㅜ근데 늦가을은 싫어해요ㅜ",
          "original_form": "그렇네요ㅜ근데 늦가을은 싫어해요ㅜ",
          "speaker_id": "1",
          "time": "20210903 07:35"
        },
        {
          "id": "MDRW2100029418.1.6",
          "form": "저도 너무 쌀쌀한건 싫어요",
          "original_form": "저도 너무 쌀쌀한건 싫어요",
          "speaker_id": "2",
          "time": "20210903 07:35"
        },
        {
          "id": "MDRW2100029418.1.7",
          "form": "외투를 입으면 답답하달까",
          "original_form": "외투를 입으면 답답하달까",
          "speaker_id": "2",
          "time": "20210903 07:35"
        },
        {
          "id": "MDRW2100029418.1.8",
          "form": "전 스산한걸 싫어해요ㅜ나이드니 더욱더ㅜ",
          "original_form": "전 스산한걸 싫어해요ㅜ나이드니 더욱더ㅜ",
          "speaker_id": "1",
          "time": "20210903 07:35"
        },
        {
          "id": "MDRW2100029418.1.9",
          "form": "세탁이 번거로워지는 시기이기도 하죠ㅎㅎ",
          "original_form": "세탁이 번거로워지는 시기이기도 하죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 07:35"
        },
        {
          "id": "MDRW2100029418.1.10",
          "form": "그렇죠ㅜ춥기도하고ㅜ",
          "original_form": "그렇죠ㅜ춥기도하고ㅜ",
          "speaker_id": "1",
          "time": "20210903 07:36"
        },
        {
          "id": "MDRW2100029418.1.11",
          "form": "소름돋는 날씨가 찾아오면 나태해지는것 같아요",
          "original_form": "소름돋는 날씨가 찾아오면 나태해지는것 같아요",
          "speaker_id": "2",
          "time": "20210903 07:36"
        },
        {
          "id": "MDRW2100029418.1.12",
          "form": "초여름을 가장 좋아해요",
          "original_form": "초여름을 가장 좋아해요",
          "speaker_id": "1",
          "time": "20210903 07:36"
        },
        {
          "id": "MDRW2100029418.1.13",
          "form": "맞아요 저는 날씨와 계절의 영향을 받아서요",
          "original_form": "맞아요 저는 날씨와 계절의 영향을 받아서요",
          "speaker_id": "1",
          "time": "20210903 07:37"
        },
        {
          "id": "MDRW2100029418.1.14",
          "form": "풀벌레우는 소리가 들리고 쌀쌀한 꽃샘추위가 가시는 날이네요",
          "original_form": "풀벌레우는 소리가 들리고 쌀쌀한 꽃샘추위가 가시는 날이네요",
          "speaker_id": "2",
          "time": "20210903 07:37"
        },
        {
          "id": "MDRW2100029418.1.15",
          "form": "저는 여름에 매미가 울기전까지를 좋아합니다ㅎㅎ",
          "original_form": "저는 여름에 매미가 울기전까지를 좋아합니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 07:38"
        },
        {
          "id": "MDRW2100029418.1.16",
          "form": "아 저두요~시적이시네요^^",
          "original_form": "아 저두요~시적이시네요^^",
          "speaker_id": "1",
          "time": "20210903 07:38"
        },
        {
          "id": "MDRW2100029418.1.17",
          "form": "시를 즐겨썼는데 재능은 없더라구요ㅎ",
          "original_form": "시를 즐겨썼는데 재능은 없더라구요ㅎ",
