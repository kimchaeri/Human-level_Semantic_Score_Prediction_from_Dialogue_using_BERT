      "utterance": [
        {
          "id": "MDRW2100032382.1.1",
          "form": "저는 삼겹살이 오랜만에 먹고싶긴해요 ㅋㅋ",
          "original_form": "저는 삼겹살이 오랜만에 먹고싶긴해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:08"
        },
        {
          "id": "MDRW2100032382.1.2",
          "form": "삼겹살 맛있죠ㅜㅜ 전 요즘 대패삼겹살이...땡기는",
          "original_form": "삼겹살 맛있죠ㅜㅜ 전 요즘 대패삼겹살이...땡기는",
          "speaker_id": "1",
          "time": "20210905 18:09"
        },
        {
          "id": "MDRW2100032382.1.3",
          "form": "생각만해도 맛있죠 ㅠㅠㅠ",
          "original_form": "생각만해도 맛있죠 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 18:54"
        },
        {
          "id": "MDRW2100032382.1.4",
          "form": "저녁은 삼겹살로 할 생각이에요 ㅋㅋㅋ",
          "original_form": "저녁은 삼겹살로 할 생각이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:54"
        },
        {
          "id": "MDRW2100032382.1.5",
          "form": "헉 삼겹살 드셨나요?",
          "original_form": "헉 삼겹살 드셨나요?",
          "speaker_id": "1",
          "time": "20210905 22:21"
        },
        {
          "id": "MDRW2100032382.1.6",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100032382.1.7",
          "form": "오랜만레 먹으네",
          "original_form": "오랜만레 먹으네",
          "speaker_id": "2",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100032382.1.8",
          "form": "너무 맛있네요 ㅠㅠ",
          "original_form": "너무 맛있네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100032382.1.9",
          "form": "헉 뭐 싸서 드시나요 삼겹살 ㅠㅠ",
          "original_form": "헉 뭐 싸서 드시나요 삼겹살 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032382.1.10",
          "form": "전 깻잎 좋아해서 ㅎㅎ",
          "original_form": "전 깻잎 좋아해서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 09:52"
        },
        {
          "id": "MDRW2100032382.1.11",
          "form": "그거랑 같이싸먹았아요",
          "original_form": "그거랑 같이싸먹았아요",
          "speaker_id": "2",
          "time": "20210906 09:52"
        },
        {
          "id": "MDRW2100032382.1.12",
          "form": "넘 맛있을 거 같아요... 깻잎 완전 맛있죠ㅠㅠ",
          "original_form": "넘 맛있을 거 같아요... 깻잎 완전 맛있죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100032382.1.13",
          "form": "맞아용 ㅠㅠㅠ",
          "original_form": "맞아용 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 10:02"
        },
        {
          "id": "MDRW2100032382.1.14",
          "form": "또 먹고싶네여 ㅠ",
          "original_form": "또 먹고싶네여 ㅠ",
          "speaker_id": "2",
          "time": "20210907 10:02"
        },
        {
          "id": "MDRW2100032382.1.15",
          "form": "으악 안 남으셨나요ㅋㅋ",
          "original_form": "으악 안 남으셨나요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:49"
        },
        {
          "id": "MDRW2100032382.1.16",
          "form": "넹 ㅋㅋㅋ 가족모두 먹으니 금방 동나네용 ..ㅎㅎ.ㅜ",
          "original_form": "넹 ㅋㅋㅋ 가족모두 먹으니 금방 동나네용 ..ㅎㅎ.ㅜ",
          "speaker_id": "2",
          "time": "20210907 11:50"
        },
        {
          "id": "MDRW2100032382.1.17",
          "form": "아쉽네요... 오늘도 사는 건...(?)",
          "original_form": "아쉽네요... 오늘도 사는 건...(?)",
          "speaker_id": "1",
          "time": "20210907 12:02"
        },
        {
          "id": "MDRW2100032382.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:16"
        },
        {
          "id": "MDRW2100032382.1.19",
          "form": "그래도 제 주변에도",
          "original_form": "그래도 제 주변에도",
          "speaker_id": "2",
          "time": "20210907 12:16"
        },
        {
          "id": "MDRW2100032382.1.20",
          "form": "주식많이하거든요",
          "original_form": "주식많이하거든요",
          "speaker_id": "2",
          "time": "20210907 12:16"
        },
        {
          "id": "MDRW2100032382.1.21",
          "form": "근데 소소하게 하는건 그래도 괜탆아보이긴해요 ㅋㅋ",
          "original_form": "근데 소소하게 하는건 그래도 괜탆아보이긴해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:16"
        },
        {
          "id": "MDRW2100032382.1.22",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "2",
          "time": "20210907 12:16"
        },
        {
          "id": "MDRW2100032382.1.23",
          "form": "잘못보냈네",
          "original_form": "잘못보냈네",
          "speaker_id": "2",
          "time": "20210907 12:16"
        },
        {
          "id": "MDRW2100032382.1.24",
          "form": "오늘도 사면...저혼자 먹을거같은데...ㅎㅎ..",
          "original_form": "오늘도 사면...저혼자 먹을거같은데...ㅎㅎ..",
          "speaker_id": "2",
          "time": "20210907 12:17"
        },
        {
          "id": "MDRW2100032382.1.25",
          "form": "아빠가 고기를 그렇게 안좋아하셔서 ㅠㅠ",
          "original_form": "아빠가 고기를 그렇게 안좋아하셔서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 12:17"
        },
        {
          "id": "MDRW2100032382.1.26",
          "form": "아이고 고기를 안 좋아하시구나 아쉽네요ㅠ",
          "original_form": "아이고 고기를 안 좋아하시구나 아쉽네요ㅠ",
          "speaker_id": "1",
          "time": "20210907 12:21"
        },
        {
          "id": "MDRW2100032382.1.27",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 12:23"
        },
        {
          "id": "MDRW2100032382.1.28",
          "form": "전 고기가 넘좋아서...",
          "original_form": "전 고기가 넘좋아서...",
          "speaker_id": "2",
          "time": "20210907 12:23"
        },
        {
          "id": "MDRW2100032382.1.29",
          "form": "나중엔 곱창이랑 대창 먹으러가고싶네여..ㅎ",
          "original_form": "나중엔 곱창이랑 대창 먹으러가고싶네여..ㅎ",
          "speaker_id": "2",
          "time": "20210907 12:23"
        },
        {
          "id": "MDRW2100032382.1.30",
          "form": "와.. 곱창 대창 너무 좋아해요...",
          "original_form": "와.. 곱창 대창 너무 좋아해요...",
          "speaker_id": "1",
          "time": "20210907 12:29"
        },
        {
          "id": "MDRW2100032382.1.31",
          "form": "요즘엔...대창이 그렇게 맛있드라구요 ㅠ",
          "original_form": "요즘엔...대창이 그렇게 맛있드라구요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100032382.1.32",
          "form": "대창 기름 많아서 많이는 못 먹겠는데 넘 맛있어요ㅠㅠ",
          "original_form": "대창 기름 많아서 많이는 못 먹겠는데 넘 맛있어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 12:56"
        },
        {
          "id": "MDRW2100032382.1.33",
          "form": "그쵸 ㅠㅠㅠ",
          "original_form": "그쵸 ㅠㅠㅠ",
