      "utterance": [
        {
          "id": "MDRW2100024215.1.1",
          "form": "ㅎㅎ엄청크다보니ㅎ",
          "original_form": "ㅎㅎ엄청크다보니ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100024215.1.2",
          "form": "안고감기는게 너무힘들어서ㅎㅎ 캡하기시작했어요ㅎㅎ",
          "original_form": "안고감기는게 너무힘들어서ㅎㅎ 캡하기시작했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024215.1.3",
          "form": "손목디 성함날이  없엇저 ㅋ 안고 할땐",
          "original_form": "손목디 성함날이  없엇저 ㅋ 안고 할땐",
          "speaker_id": "1",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024215.1.4",
          "form": "지금은 엄마랑할때만 잘하고ㅎ 저는 맨날 울어요ㅎ",
          "original_form": "지금은 엄마랑할때만 잘하고ㅎ 저는 맨날 울어요ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024215.1.5",
          "form": "맞아요ㅠ엄청힘들죠ㅠ",
          "original_form": "맞아요ㅠ엄청힘들죠ㅠ",
          "speaker_id": "2",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024215.1.6",
          "form": "음 ㅋㅋㅋ 투정 부리는거네요",
          "original_form": "음 ㅋㅋㅋ 투정 부리는거네요",
          "speaker_id": "1",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024215.1.7",
          "form": "엄마는 울면 싫어할거 아나봐요",
          "original_form": "엄마는 울면 싫어할거 아나봐요",
          "speaker_id": "1",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024215.1.8",
          "form": "투정인것같은데",
          "original_form": "투정인것같은데",
          "speaker_id": "2",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024215.1.9",
          "form": "자꾸애기를 울리고싶어요ㅎㅎㅎㅎㅎㅎㅎㅎㅎ",
          "original_form": "자꾸애기를 울리고싶어요ㅎㅎㅎㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024215.1.10",
          "form": "너무 귀여워서ㅎㅎ",
          "original_form": "너무 귀여워서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024215.1.11",
          "form": "삐치는게 너무 귀여움ㅎ 우는것고 입을 오~하면서 우니깐ㅎ 귀여워요ㅎ",
          "original_form": "삐치는게 너무 귀여움ㅎ 우는것고 입을 오~하면서 우니깐ㅎ 귀여워요ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024215.1.12",
          "form": "ㅎㅎㅎㅎ  아빠가  장난꾸러기네요",
          "original_form": "ㅎㅎㅎㅎ  아빠가  장난꾸러기네요",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100024215.1.13",
          "form": "억지루 울리지는 마요",
          "original_form": "억지루 울리지는 마요",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100024215.1.14",
          "form": "일부러 그러진않는데ㅎㅎ 너무 좋아서ㅎㅎ",
          "original_form": "일부러 그러진않는데ㅎㅎ 너무 좋아서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100024215.1.15",
          "form": "그게 안좋은 기억이 되기도 해여 애들한테는~",
          "original_form": "그게 안좋은 기억이 되기도 해여 애들한테는~",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100024215.1.16",
          "form": "장난치고 맨날 놀아요 ㅎ",
          "original_form": "장난치고 맨날 놀아요 ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024215.1.17",
          "form": "그렇군요ㅠㅠ 안해야겟다ㅍ",
          "original_form": "그렇군요ㅠㅠ 안해야겟다ㅍ",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024215.1.18",
          "form": "엄청 딸바보 아빠신거 같음 ㅎㅎㅎ",
          "original_form": "엄청 딸바보 아빠신거 같음 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024215.1.19",
          "form": "보기 좋아요~~",
          "original_form": "보기 좋아요~~",
          "speaker_id": "1",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024215.1.20",
          "form": "나중에 좋아하던데 아니엇군요ㅎㅎ",
          "original_form": "나중에 좋아하던데 아니엇군요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024215.1.21",
          "form": "원래부터 애기도 좋아했고",
          "original_form": "원래부터 애기도 좋아했고",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024215.1.22",
          "form": "딸이라 더 좋아해요ㅎ",
          "original_form": "딸이라 더 좋아해요ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100024215.1.23",
          "form": "아들은 의젓하죠?",
          "original_form": "아들은 의젓하죠?",
          "speaker_id": "2",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100024215.1.24",
          "form": "의젓하고 생각보다  애교가 많아요 ㅎㅎ",
          "original_form": "의젓하고 생각보다  애교가 많아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100024215.1.25",
          "form": "애기때부터  사람들이 여자 아이라고 착각을",
          "original_form": "애기때부터  사람들이 여자 아이라고 착각을",
          "speaker_id": "1",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100024215.1.26",
          "form": "두돌 사진도 단발에 드레스 입고 찍엇어여 ㅎㅎ",
          "original_form": "두돌 사진도 단발에 드레스 입고 찍엇어여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100024215.1.27",
          "form": "우왐ㅎ 애교많으면 좋겟당ㅎ",
          "original_form": "우왐ㅎ 애교많으면 좋겟당ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100024215.1.28",
          "form": "ㅎㅎ 이쁘게 생겼어요?",
          "original_form": "ㅎㅎ 이쁘게 생겼어요?",
          "speaker_id": "2",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100024215.1.29",
          "form": "머리도 계속 기르고 했어서  사과머리항상 했거든요",
          "original_form": "머리도 계속 기르고 했어서  사과머리항상 했거든요",
          "speaker_id": "1",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100024215.1.30",
          "form": "전 남자 아이같은데 사람들이 다 여자애인줄 알더라구요",
          "original_form": "전 남자 아이같은데 사람들이 다 여자애인줄 알더라구요",
          "speaker_id": "1",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100024215.1.31",
          "form": "5살되니 형아라고 사과머리 안한다네요 ㅎㅎ",
          "original_form": "5살되니 형아라고 사과머리 안한다네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100024215.1.32",
          "form": "이쁘게 생긴거 맞나보네요ㅎㅎ",
          "original_form": "이쁘게 생긴거 맞나보네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100024215.1.33",
          "form": "이야ㅎㅎ",
          "original_form": "이야ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100024215.1.34",
          "form": "똑같네요ㅎ 저희애도 이제 언니니깐",
          "original_form": "똑같네요ㅎ 저희애도 이제 언니니깐",
          "speaker_id": "2",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100024215.1.35",
          "form": "안한다는거 많이생겼어요 ㅡㅎㅎ",
          "original_form": "안한다는거 많이생겼어요 ㅡㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100024215.1.36",
          "form": "곧 펌해달라고 할거에요 ㅎㅎㅎ",
          "original_form": "곧 펌해달라고 할거에요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100024215.1.37",
          "form": "예뻐보이고 싶어함 공주처럼 ㅎㅎㅎ",
          "original_form": "예뻐보이고 싶어함 공주처럼 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100024215.1.38",
          "form": "어린이집 가면 여자애들  옷으로 은근 신경전해요",
          "original_form": "어린이집 가면 여자애들  옷으로 은근 신경전해요",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100024215.1.39",
          "form": "남자들은 카봇 이나 마블 옷 ㅜㅜ",
          "original_form": "남자들은 카봇 이나 마블 옷 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100024215.1.40",
          "form": "에고ㅎㅎ",
          "original_form": "에고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100024215.1.41",
          "form": "그렇겟네요ㅎㅎ",
          "original_form": "그렇겟네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:26"
        },
        {
          "id": "MDRW2100024215.1.42",
          "form": "전 별루 안좋아해서 ㅋㅋ",
          "original_form": "전 별루 안좋아해서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100024215.1.43",
          "form": "애들은 투머지더라구요 ㅎㅎ",
          "original_form": "애들은 투머지더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100024215.1.44",
          "form": "오늘 무지출을 실천하고싶었는데ㅎㅎ",
          "original_form": "오늘 무지출을 실천하고싶었는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:27"
        },
        {
