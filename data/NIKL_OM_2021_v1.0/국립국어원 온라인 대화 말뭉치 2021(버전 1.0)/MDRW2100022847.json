      "utterance": [
        {
          "id": "MDRW2100022847.1.1",
          "form": "어떤 계절 좋아하세요?",
          "original_form": "어떤 계절 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 18:31"
        },
        {
          "id": "MDRW2100022847.1.2",
          "form": "저는 겨울좋아합니다!",
          "original_form": "저는 겨울좋아합니다!",
          "speaker_id": "2",
          "time": "20210829 18:44"
        },
        {
          "id": "MDRW2100022847.1.3",
          "form": "어느계절좋아하세요?",
          "original_form": "어느계절좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 18:44"
        },
        {
          "id": "MDRW2100022847.1.4",
          "form": "전 가을이요",
          "original_form": "전 가을이요",
          "speaker_id": "1",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100022847.1.5",
          "form": "가을좋죠 좋아는 이유는 무엇인가요?",
          "original_form": "가을좋죠 좋아는 이유는 무엇인가요?",
          "speaker_id": "2",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100022847.1.6",
          "form": "덥지도 않고 춥지도 않고 적당한 온도라서 좋아요\r\n선선한 느낌과 시원한 바람도 좋구요",
          "original_form": "덥지도 않고 춥지도 않고 적당한 온도라서 좋아요\r\n선선한 느낌과 시원한 바람도 좋구요",
          "speaker_id": "1",
          "time": "20210829 18:51"
        },
        {
          "id": "MDRW2100022847.1.7",
          "form": "아하 저도 그런데 왠지 겨울이 더좋더라구요",
          "original_form": "아하 저도 그런데 왠지 겨울이 더좋더라구요",
          "speaker_id": "2",
          "time": "20210829 18:54"
        },
        {
          "id": "MDRW2100022847.1.8",
          "form": "겨울도 좋죠\r\n겨울 좋아하시는 이유가 있나요?",
          "original_form": "겨울도 좋죠\r\n겨울 좋아하시는 이유가 있나요?",
          "speaker_id": "1",
          "time": "20210829 19:13"
        },
        {
          "id": "MDRW2100022847.1.9",
          "form": "흰바탕이좋아서요",
          "original_form": "흰바탕이좋아서요",
          "speaker_id": "2",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100022847.1.10",
          "form": "눈이좋다고해야되나?",
          "original_form": "눈이좋다고해야되나?",
          "speaker_id": "2",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100022847.1.11",
          "form": "더위보단 추위를 잘안타서 좋아하긴하죠",
          "original_form": "더위보단 추위를 잘안타서 좋아하긴하죠",
          "speaker_id": "2",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100022847.1.12",
          "form": "눈내리는 풍경 보고 있으면 넘 예쁘죠",
          "original_form": "눈내리는 풍경 보고 있으면 넘 예쁘죠",
          "speaker_id": "1",
          "time": "20210829 19:25"
        },
        {
          "id": "MDRW2100022847.1.13",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210829 19:29"
        },
        {
          "id": "MDRW2100022847.1.14",
          "form": "항상 사진으로 보관해요 저는",
          "original_form": "항상 사진으로 보관해요 저는",
          "speaker_id": "2",
          "time": "20210829 19:29"
        },
        {
          "id": "MDRW2100022847.1.15",
          "form": "사진 찍어두면 좋죠!",
          "original_form": "사진 찍어두면 좋죠!",
          "speaker_id": "1",
          "time": "20210829 21:12"
        },
        {
          "id": "MDRW2100022847.1.16",
          "form": "추억놀이할때 좋죠ㅋㅋㅋㅋ",
          "original_form": "추억놀이할때 좋죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100022847.1.17",
          "form": "요즘 날이 선선해져서 옷이 사려는데 옷은 자주사시나요?",
          "original_form": "요즘 날이 선선해져서 옷이 사려는데 옷은 자주사시나요?",
          "speaker_id": "2",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100022847.1.18",
          "form": "맞아요ㅋㅋㅋ 사진들 보면 자연스레 기억이 나더라구요",
          "original_form": "맞아요ㅋㅋㅋ 사진들 보면 자연스레 기억이 나더라구요",
          "speaker_id": "1",
          "time": "20210829 21:21"
        }
      ]
