      "utterance": [
        {
          "id": "MDRW2100041267.1.1",
          "form": "추운계절이 곧올것같아요",
          "original_form": "추운계절이 곧올것같아요",
          "speaker_id": "1",
          "time": "20210905 21:07"
        },
        {
          "id": "MDRW2100041267.1.2",
          "form": "그러게요 부지런히 즐겨야해요",
          "original_form": "그러게요 부지런히 즐겨야해요",
          "speaker_id": "2",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100041267.1.3",
          "form": "가을 너무좋은데..",
          "original_form": "가을 너무좋은데..",
          "speaker_id": "1",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100041267.1.4",
          "form": "아쉬워요",
          "original_form": "아쉬워요",
          "speaker_id": "1",
          "time": "20210905 21:10"
        },
        {
          "id": "MDRW2100041267.1.5",
          "form": "가을이 좀 짧죠?",
          "original_form": "가을이 좀 짧죠?",
          "speaker_id": "2",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100041267.1.6",
          "form": "네..오늘날씨보니 겨울이금방올듯",
          "original_form": "네..오늘날씨보니 겨울이금방올듯",
          "speaker_id": "1",
          "time": "20210905 21:14"
        },
        {
          "id": "MDRW2100041267.1.7",
          "form": "ㄷㄷㄷ 가을옷 사자마자 겨울옷 사야겠네요",
          "original_form": "ㄷㄷㄷ 가을옷 사자마자 겨울옷 사야겠네요",
          "speaker_id": "2",
          "time": "20210905 21:14"
        },
        {
          "id": "MDRW2100041267.1.8",
          "form": "가을옷 하나못샀는데",
          "original_form": "가을옷 하나못샀는데",
          "speaker_id": "1",
          "time": "20210905 21:14"
        },
        {
          "id": "MDRW2100041267.1.9",
          "form": "그러게요 부지런히 또 사서 입고 해야 겠다",
          "original_form": "그러게요 부지런히 또 사서 입고 해야 겠다",
          "speaker_id": "2",
          "time": "20210905 21:15"
        },
        {
          "id": "MDRW2100041267.1.10",
          "form": "아가씨니 예쁘게입고다니세요",
          "original_form": "아가씨니 예쁘게입고다니세요",
          "speaker_id": "1",
          "time": "20210905 21:15"
        },
        {
          "id": "MDRW2100041267.1.11",
          "form": "네네 내일은 셔츠랑 벨트치마 입으려고요",
          "original_form": "네네 내일은 셔츠랑 벨트치마 입으려고요",
          "speaker_id": "2",
          "time": "20210905 21:16"
        },
        {
          "id": "MDRW2100041267.1.12",
          "form": "오..예쁘겠네요",
          "original_form": "오..예쁘겠네요",
          "speaker_id": "1",
          "time": "20210905 21:17"
        },
        {
          "id": "MDRW2100041267.1.13",
          "form": "벨트치마",
          "original_form": "벨트치마",
          "speaker_id": "1",
          "time": "20210905 21:17"
        },
        {
          "id": "MDRW2100041267.1.14",
          "form": "저는 항상 루즈핏 바지ㅜㅜ",
          "original_form": "저는 항상 루즈핏 바지ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 21:17"
        },
        {
          "id": "MDRW2100041267.1.15",
          "form": "저도 루즈핏 바지 좋아해용",
          "original_form": "저도 루즈핏 바지 좋아해용",
          "speaker_id": "2",
          "time": "20210905 21:34"
        },
        {
          "id": "MDRW2100041267.1.16",
          "form": "ㅎㅎ 애낳고 몸불은게 아직도 안빠져서",
