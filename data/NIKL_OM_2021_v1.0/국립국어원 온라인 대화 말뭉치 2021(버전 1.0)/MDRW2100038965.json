      "utterance": [
        {
          "id": "MDRW2100038965.1.1",
          "form": "네 !!! 가능합니닷 !!!",
          "original_form": "네 !!! 가능합니닷 !!!",
          "speaker_id": "2",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100038965.1.2",
          "form": "직접 반죽까지 하시는 건가요!?!",
          "original_form": "직접 반죽까지 하시는 건가요!?!",
          "speaker_id": "1",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100038965.1.3",
          "form": "네 !!!",
          "original_form": "네 !!!",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100038965.1.4",
          "form": "근데 반죽 은근히 괜찮아요 !!",
          "original_form": "근데 반죽 은근히 괜찮아요 !!",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100038965.1.5",
          "form": "계량만 잘하면 !!",
          "original_form": "계량만 잘하면 !!",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100038965.1.6",
          "form": "헐 대박 완전 대단하신데요 전 베이킹은 좀 무서워서 안 해봤거든요",
          "original_form": "헐 대박 완전 대단하신데요 전 베이킹은 좀 무서워서 안 해봤거든요",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100038965.1.7",
          "form": "배우신 건가요?!",
          "original_form": "배우신 건가요?!",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100038965.1.8",
          "form": "아니요 !! 저두 그냥 유튜브보고",
          "original_form": "아니요 !! 저두 그냥 유튜브보고",
          "speaker_id": "2",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100038965.1.9",
          "form": "바로 했어요 !!",
          "original_form": "바로 했어요 !!",
          "speaker_id": "2",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100038965.1.10",
          "form": "헛 그래도 대단하신 걸요 ㅎㅎ",
          "original_form": "헛 그래도 대단하신 걸요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100038965.1.11",
          "form": "케익같은 것도 만드셔요?!",
          "original_form": "케익같은 것도 만드셔요?!",
          "speaker_id": "1",
          "time": "20210902 23:51"
        },
        {
          "id": "MDRW2100038965.1.12",
          "form": "아직 거기까진",
          "original_form": "아직 거기까진",
          "speaker_id": "2",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100038965.1.13",
          "form": "도전을 못했습니닷..!",
          "original_form": "도전을 못했습니닷..!",
          "speaker_id": "2",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100038965.1.14",
          "form": "케잌은 재료비가 은근히 비싸서",
          "original_form": "케잌은 재료비가 은근히 비싸서",
          "speaker_id": "2",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100038965.1.15",
          "form": "사먹는게 더 싸대요!!",
          "original_form": "사먹는게 더 싸대요!!",
          "speaker_id": "2",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100038965.1.16",
          "form": "허얼 진짜요?? 완전 몰랐네요 전 그냥 빵에 크림만 바르면 되는쥴.. ㅎㅎ",
          "original_form": "허얼 진짜요?? 완전 몰랐네요 전 그냥 빵에 크림만 바르면 되는쥴.. ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100038965.1.17",
          "form": "역시 사먹어만 봐서 모르나봐요 ㅎ",
          "original_form": "역시 사먹어만 봐서 모르나봐요 ㅎ",
          "speaker_id": "1",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100038965.1.18",
          "form": "맞죠 .. 저두 몰랐어요",
          "original_form": "맞죠 .. 저두 몰랐어요",
          "speaker_id": "2",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100038965.1.19",
          "form": "빵이 비싼이유가 있나바여...",
          "original_form": "빵이 비싼이유가 있나바여...",
          "speaker_id": "2",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100038965.1.20",
          "form": "그르니까요..! 솔직히 조그마한 것만 사도",
          "original_form": "그르니까요..! 솔직히 조그마한 것만 사도",
          "speaker_id": "1",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100038965.1.21",
          "form": "오천원 넘길래 뭔가 싶었는대",
          "original_form": "오천원 넘길래 뭔가 싶었는대",
          "speaker_id": "1",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100038965.1.22",
          "form": "마자여ㅜㅜ",
          "original_form": "마자여ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100038965.1.23",
          "form": "이해가 돨 법도 하구... ㅎㅎ",
          "original_form": "이해가 돨 법도 하구... ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100038965.1.24",
          "form": "전 그래서 최대한 큰 빵을 고릅니다 !!!!",
          "original_form": "전 그래서 최대한 큰 빵을 고릅니다 !!!!",
          "speaker_id": "2",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100038965.1.25",
          "form": "앗 인정해요 ㅎㅎ",
          "original_form": "앗 인정해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100038965.1.26",
          "form": "전 요새 고로케에 빠져서",
          "original_form": "전 요새 고로케에 빠져서",
          "speaker_id": "1",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100038965.1.27",
