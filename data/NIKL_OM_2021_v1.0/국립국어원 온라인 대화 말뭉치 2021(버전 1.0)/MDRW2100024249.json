      "utterance": [
        {
          "id": "MDRW2100024249.1.1",
          "form": "배달자주하세요?",
          "original_form": "배달자주하세요?",
          "speaker_id": "1",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100024249.1.2",
          "form": "ㅎㅎ 집에서 남은 음식으로만 먹어도 맛있죠정말",
          "original_form": "ㅎㅎ 집에서 남은 음식으로만 먹어도 맛있죠정말",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100024249.1.3",
          "form": "자주해요ㅠ 저희는ㅍ",
          "original_form": "자주해요ㅠ 저희는ㅍ",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100024249.1.4",
          "form": "거의... ㅎㅎ",
          "original_form": "거의... ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100024249.1.5",
          "form": "남은거로 볶는거죠",
          "original_form": "남은거로 볶는거죠",
          "speaker_id": "1",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100024249.1.6",
          "form": "ㅎㅎ 맥주는 얼마나드셔요?ㅎ",
          "original_form": "ㅎㅎ 맥주는 얼마나드셔요?ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100024249.1.7",
          "form": "일주일에?",
          "original_form": "일주일에?",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100024249.1.8",
          "form": "매일한캔중자요",
          "original_form": "매일한캔중자요",
          "speaker_id": "1",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100024249.1.9",
          "form": "ㅎㅎ 혼자만의 시간을 즐기시군요ㅎ",
          "original_form": "ㅎㅎ 혼자만의 시간을 즐기시군요ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100024249.1.10",
          "form": "그렇기도하겟어요ㅎ 일하시고 애들하고 놀아주시고 ㅡㅎ",
          "original_form": "그렇기도하겟어요ㅎ 일하시고 애들하고 놀아주시고 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100024249.1.11",
          "form": "너무좋긴하죠 할일이 있으니",
          "original_form": "너무좋긴하죠 할일이 있으니",
          "speaker_id": "1",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100024249.1.12",
          "form": "보통 그럼 몇시에 육퇴하세요?",
          "original_form": "보통 그럼 몇시에 육퇴하세요?",
          "speaker_id": "2",
          "time": "20210830 23:47"
        },
        {
          "id": "MDRW2100024249.1.13",
          "form": "9시정도면 애들이 잠들어요",
          "original_form": "9시정도면 애들이 잠들어요",
          "speaker_id": "1",
          "time": "20210830 23:47"
        },
        {
          "id": "MDRW2100024249.1.14",
          "form": "엄청 좋으시겟다ㅠㅠ",
          "original_form": "엄청 좋으시겟다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100024249.1.15",
          "form": "저흰 10시 반정도?ㅠㅠ",
          "original_form": "저흰 10시 반정도?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100024249.1.16",
          "form": "육퇴시간이늦다보니ㅠ 심심이도늦게ㅠ 잠도 늦게ㅠ",
          "original_form": "육퇴시간이늦다보니ㅠ 심심이도늦게ㅠ 잠도 늦게ㅠ",
          "speaker_id": "2",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100024249.1.17",
          "form": "헉 자유가 없으실듯",
          "original_form": "헉 자유가 없으실듯",
          "speaker_id": "1",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100024249.1.18",
          "form": "저녁은 언제드세요(?)",
          "original_form": "저녁은 언제드세요(?)",
          "speaker_id": "1",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100024249.1.19",
          "form": "근데 맨날 끝나면 자유죠 ㅡㅎㅎ",
          "original_form": "근데 맨날 끝나면 자유죠 ㅡㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100024249.1.20",
          "form": "저녁은 퇴근하고하면 6시반? 7시? 같이먹ㅇ어요ㅎ",
          "original_form": "저녁은 퇴근하고하면 6시반? 7시? 같이먹ㅇ어요ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100024249.1.21",
          "form": "애들재우고는 뭐드세요??",
          "original_form": "애들재우고는 뭐드세요??",
          "speaker_id": "1",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100024249.1.22",
          "form": "아뇨아뇨ㅎㅎ같이먹죠ㅎㅎ",
          "original_form": "아뇨아뇨ㅎㅎ같이먹죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100024249.1.23",
          "form": "오~맥주파(?)",
          "original_form": "오~맥주파(?)",
          "speaker_id": "1",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100024249.1.24",
          "form": "전 잘안먹어요ㅎㅎ 상황에 따라 다른듯ㅎ 취하고싶은날은 소주ㅎ 아니면 간단하게 맥주요 ㅡㅎㅎ",
          "original_form": "전 잘안먹어요ㅎㅎ 상황에 따라 다른듯ㅎ 취하고싶은날은 소주ㅎ 아니면 간단하게 맥주요 ㅡㅎㅎ",
          "speaker_id": "2",
