      "utterance": [
        {
          "id": "MDRW2100014591.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014591.1.2",
          "form": "평소에 전시 보러 다니세요?",
          "original_form": "평소에 전시 보러 다니세요?",
          "speaker_id": "1",
          "time": "20210825 21:40"
        },
        {
          "id": "MDRW2100014591.1.3",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "2",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100014591.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100014591.1.5",
          "form": "저는 이것저것 보러 다녀요 티켓팅하러 인터파크 둘러보는 게 취미예요ㅋㅋ",
          "original_form": "저는 이것저것 보러 다녀요 티켓팅하러 인터파크 둘러보는 게 취미예요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100014591.1.6",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100014591.1.7",
          "form": "어떤거보셨어요 최근에요!",
          "original_form": "어떤거보셨어요 최근에요!",
          "speaker_id": "2",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100014591.1.8",
          "form": "가장 최근에는 한가람미술관 내일의예술전이라구요",
          "original_form": "가장 최근에는 한가람미술관 내일의예술전이라구요",
          "speaker_id": "1",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100014591.1.9",
          "form": "젊은 작가들 전시였어요",
          "original_form": "젊은 작가들 전시였어요",
          "speaker_id": "1",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100014591.1.10",
          "form": "무료전시였는데 기대이상!",
          "original_form": "무료전시였는데 기대이상!",
          "speaker_id": "1",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100014591.1.11",
          "form": "좋네요ㅋㅋㅋㅋ",
          "original_form": "좋네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100014591.1.12",
          "form": "전문가같아요",
          "original_form": "전문가같아요",
          "speaker_id": "2",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100014591.1.13",
          "form": "ㅋㅋ 친구랑 다니고 싶은데 요즘 힘드네요ㅠㅠ 혼자 보는 것도 쓸쓸해요",
          "original_form": "ㅋㅋ 친구랑 다니고 싶은데 요즘 힘드네요ㅠㅠ 혼자 보는 것도 쓸쓸해요",
          "speaker_id": "1",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100014591.1.14",
          "form": "연극이나 영화 콘서트 평소에 보러다니는 거 있으세요?",
          "original_form": "연극이나 영화 콘서트 평소에 보러다니는 거 있으세요?",
          "speaker_id": "1",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100014591.1.15",
          "form": "셋중에 고르자면",
          "original_form": "셋중에 고르자면",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100014591.1.16",
          "form": "아무래도",
          "original_form": "아무래도",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100014591.1.17",
          "form": "영화겠죠?!",
          "original_form": "영화겠죠?!",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100014591.1.18",
          "form": "아 최근에 뭐 보셨어요?",
          "original_form": "아 최근에 뭐 보셨어요?",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100014591.1.19",
          "form": "저는 인질이랑 모가디슈 봤어용",
          "original_form": "저는 인질이랑 모가디슈 봤어용",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100014591.1.20",
          "form": "기억이안날정도네요ㅋㅋㅋ",
          "original_form": "기억이안날정도네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100014591.1.21",
          "form": "인질은 최신영화인가요?",
          "original_form": "인질은 최신영화인가요?",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100014591.1.22",
          "form": "네 아직 걸려있을 거예요",
          "original_form": "네 아직 걸려있을 거예요",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100014591.1.23",
          "form": "황정민이 본인역으로 출연해서 납치되는 영화예요ㅋㅋ",
          "original_form": "황정민이 본인역으로 출연해서 납치되는 영화예요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100014591.1.24",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100014591.1.25",
          "form": "황정민이 나오는군요?",
          "original_form": "황정민이 나오는군요?",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100014591.1.26",
          "form": "진짜 황정민은ㅋㅋㅋㄱㅋ",
          "original_form": "진짜 황정민은ㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210825 22:41"
        }
      ]
    }
  ]
