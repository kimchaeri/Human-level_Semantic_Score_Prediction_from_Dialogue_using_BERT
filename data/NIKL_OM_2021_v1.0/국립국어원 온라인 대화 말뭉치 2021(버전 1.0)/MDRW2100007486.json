      "utterance": [
        {
          "id": "MDRW2100007486.1.1",
          "form": "기숙사에서 배달시켜요?",
          "original_form": "기숙사에서 배달시켜요?",
          "speaker_id": "1",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007486.1.2",
          "form": "네! 코로나로 상황이 바뀌어서",
          "original_form": "네! 코로나로 상황이 바뀌어서",
          "speaker_id": "2",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007486.1.3",
          "form": "방에서 배달 먹을 수 있어요",
          "original_form": "방에서 배달 먹을 수 있어요",
          "speaker_id": "2",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007486.1.4",
          "form": "기숙사에 있어도되요?\r\n저희지역은 학교가 문을닫아시ㅣ",
          "original_form": "기숙사에 있어도되요?\r\n저희지역은 학교가 문을닫아시ㅣ",
          "speaker_id": "1",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007486.1.5",
          "form": "온라인수업만해요",
          "original_form": "온라인수업만해요",
          "speaker_id": "1",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007486.1.6",
          "form": "비대면으로 진행돼도 기숙사에는 있어도 돼요",
          "original_form": "비대면으로 진행돼도 기숙사에는 있어도 돼요",
          "speaker_id": "2",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007486.1.7",
          "form": "저희 학교도 비대면 4주 연장됐어요...ㅠ",
          "original_form": "저희 학교도 비대면 4주 연장됐어요...ㅠ",
          "speaker_id": "2",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007486.1.8",
          "form": "지금방학이죠?",
          "original_form": "지금방학이죠?",
          "speaker_id": "1",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007486.1.9",
          "form": "주로 뭐배달해요?",
          "original_form": "주로 뭐배달해요?",
          "speaker_id": "1",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007486.1.10",
          "form": "맞아요 9월초에 개강해요",
          "original_form": "맞아요 9월초에 개강해요",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007486.1.11",
          "form": "전 떡볶이랑 마라탕 많이 시켜먹는 것같아요",
          "original_form": "전 떡볶이랑 마라탕 많이 시켜먹는 것같아요",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007486.1.12",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007486.1.13",
          "form": "전 집에 살지만ㅋㅋ 족발이요",
          "original_form": "전 집에 살지만ㅋㅋ 족발이요",
          "speaker_id": "1",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007486.1.14",
          "form": "밖에서 먹을때는 삼계탕자주먹고",
          "original_form": "밖에서 먹을때는 삼계탕자주먹고",
          "speaker_id": "1",
          "time": "20210822 02:33"
        },
        {
          "id": "MDRW2100007486.1.15",
          "form": "족발 좋아요ㅠ 보쌈도 좋아하시나요?",
          "original_form": "족발 좋아요ㅠ 보쌈도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210822 02:35"
        },
        {
          "id": "MDRW2100007486.1.16",
          "form": "남편이족발을 좋아해서 연애6년간 결혼2년간 주1회씩 꼭 먹어요 질리지도않나봐요 저는 그만먹고싶은데 어느새 같이먹어요ㅋㅋ",
          "original_form": "남편이족발을 좋아해서 연애6년간 결혼2년간 주1회씩 꼭 먹어요 질리지도않나봐요 저는 그만먹고싶은데 어느새 같이먹어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007486.1.17",
          "form": "우와 정말 좋아하시나봐요ㅋㅋㅋ 족발은 뜯어먹는 맛도 있어서 중독적인 것같아요",
          "original_form": "우와 정말 좋아하시나봐요ㅋㅋㅋ 족발은 뜯어먹는 맛도 있어서 중독적인 것같아요",
          "speaker_id": "2",
          "time": "20210822 02:38"
        },
        {
          "id": "MDRW2100007486.1.18",
          "form": "보쌈도 좋아하시나용?",
          "original_form": "보쌈도 좋아하시나용?",
          "speaker_id": "2",
          "time": "20210822 02:38"
        },
        {
          "id": "MDRW2100007486.1.19",
          "form": "보쌈도 좋아해요",
          "original_form": "보쌈도 좋아해요",
          "speaker_id": "1",
          "time": "20210822 02:38"
        },
        {
          "id": "MDRW2100007486.1.20",
          "form": "집에서 해먹을지경이니..ㅋㅋ",
          "original_form": "집에서 해먹을지경이니..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:38"
        },
        {
          "id": "MDRW2100007486.1.21",
          "form": "우와 먹고 싶어요ㅠ",
          "original_form": "우와 먹고 싶어요ㅠ",
          "speaker_id": "2",
          "time": "20210822 02:39"
        },
        {
          "id": "MDRW2100007486.1.22",
          "form": "전 보쌈이랑 무말랭이를 같이 먹으면 더 맛있어지는 것같아요",
          "original_form": "전 보쌈이랑 무말랭이를 같이 먹으면 더 맛있어지는 것같아요",
          "speaker_id": "2",
          "time": "20210822 02:39"
        },
        {
          "id": "MDRW2100007486.1.23",
          "form": "무말랭이 진짜 맛있죠",
          "original_form": "무말랭이 진짜 맛있죠",
          "speaker_id": "1",
          "time": "20210822 02:40"
        },
        {
          "id": "MDRW2100007486.1.24",
          "form": "저는 닭강정 좋아해요",
          "original_form": "저는 닭강정 좋아해요",
          "speaker_id": "1",
          "time": "20210822 02:40"
        },
        {
          "id": "MDRW2100007486.1.25",
          "form": "허갈닭강정 아세요?",
          "original_form": "허갈닭강정 아세요?",
          "speaker_id": "1",
          "time": "20210822 02:40"
        },
        {
          "id": "MDRW2100007486.1.26",
          "form": "체인점인데",
          "original_form": "체인점인데",
          "speaker_id": "1",
          "time": "20210822 02:40"
        },
        {
          "id": "MDRW2100007486.1.27",
          "form": "엇 저는 처음 들어봤어요",
          "original_form": "엇 저는 처음 들어봤어요",
          "speaker_id": "2",
          "time": "20210822 02:41"
        },
        {
          "id": "MDRW2100007486.1.28",
          "form": "맛있나요??",
          "original_form": "맛있나요??",
          "speaker_id": "2",
          "time": "20210822 02:41"
        },
        {
          "id": "MDRW2100007486.1.29",
          "form": "한번먹으면 못헤어나와요",
          "original_form": "한번먹으면 못헤어나와요",
          "speaker_id": "1",
          "time": "20210822 02:42"
        },
        {
          "id": "MDRW2100007486.1.30",
          "form": "넘 맛있어요",
          "original_form": "넘 맛있어요",
          "speaker_id": "1",
          "time": "20210822 02:42"
        },
        {
          "id": "MDRW2100007486.1.31",
          "form": "우왕 전 만년닭강정밖에 몰랐어요",
          "original_form": "우왕 전 만년닭강정밖에 몰랐어요",
          "speaker_id": "2",
          "time": "20210822 02:43"
        },
        {
          "id": "MDRW2100007486.1.32",
          "form": "만년은 뭐랄까 특유의 향이 있는데",
          "original_form": "만년은 뭐랄까 특유의 향이 있는데",
          "speaker_id": "1",
          "time": "20210822 02:44"
        },
        {
          "id": "MDRW2100007486.1.33",
          "form": "허갈은 다 좋아할맛?",
          "original_form": "허갈은 다 좋아할맛?",
          "speaker_id": "1",
          "time": "20210822 02:44"
        }
      ]
