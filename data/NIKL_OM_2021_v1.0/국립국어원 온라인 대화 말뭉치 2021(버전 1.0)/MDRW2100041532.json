      "utterance": [
        {
          "id": "MDRW2100041532.1.1",
          "form": "출근하셨어요?",
          "original_form": "출근하셨어요?",
          "speaker_id": "1",
          "time": "20210906 09:18"
        },
        {
          "id": "MDRW2100041532.1.2",
          "form": "네 알바다녀왔어요",
          "original_form": "네 알바다녀왔어요",
          "speaker_id": "2",
          "time": "20210906 12:07"
        },
        {
          "id": "MDRW2100041532.1.3",
          "form": "일하고계세요?",
          "original_form": "일하고계세요?",
          "speaker_id": "2",
          "time": "20210906 12:07"
        },
        {
          "id": "MDRW2100041532.1.4",
          "form": "네 일히고있어요 ㅠㅠ",
          "original_form": "네 일히고있어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 13:01"
        },
        {
          "id": "MDRW2100041532.1.5",
          "form": "무슨일 하세요?",
          "original_form": "무슨일 하세요?",
          "speaker_id": "2",
          "time": "20210906 13:01"
        },
        {
          "id": "MDRW2100041532.1.6",
          "form": "저는 그냥 사무직어요 행복님은오?",
          "original_form": "저는 그냥 사무직어요 행복님은오?",
          "speaker_id": "1",
          "time": "20210906 13:03"
        },
        {
          "id": "MDRW2100041532.1.7",
          "form": "저는 복지관에서 미술보조강사요 ㅋ",
          "original_form": "저는 복지관에서 미술보조강사요 ㅋ",
          "speaker_id": "2",
          "time": "20210906 13:22"
        },
        {
          "id": "MDRW2100041532.1.8",
          "form": "일하는건 크게 스트레스 없나요?ㅋ",
          "original_form": "일하는건 크게 스트레스 없나요?ㅋ",
          "speaker_id": "2",
          "time": "20210906 13:22"
        },
        {
          "id": "MDRW2100041532.1.9",
          "form": "오 미술쌤이시네요",
          "original_form": "오 미술쌤이시네요",
          "speaker_id": "1",
          "time": "20210906 13:36"
        },
        {
          "id": "MDRW2100041532.1.10",
          "form": "네 저는 그냥 아주 무난해요 ㅋㅋㅋ",
          "original_form": "네 저는 그냥 아주 무난해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 13:36"
        },
        {
          "id": "MDRW2100041532.1.11",
          "form": "네네 ㅋㅋㅋ 무난해서 좋네요 ㅋㅋ",
          "original_form": "네네 ㅋㅋㅋ 무난해서 좋네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 13:50"
        },
        {
          "id": "MDRW2100041532.1.12",
          "form": "무난한게최고에요진짜",
          "original_form": "무난한게최고에요진짜",
          "speaker_id": "1",
          "time": "20210906 14:09"
        },
        {
          "id": "MDRW2100041532.1.13",
          "form": "네 맞아요 ㅋㅋ 스트레스 안받는것만으로도 감사한거죠",
          "original_form": "네 맞아요 ㅋㅋ 스트레스 안받는것만으로도 감사한거죠",
          "speaker_id": "2",
          "time": "20210906 14:19"
        },
        {
          "id": "MDRW2100041532.1.14",
          "form": "퇴근은 몇시셔요?",
          "original_form": "퇴근은 몇시셔요?",
          "speaker_id": "1",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100041532.1.15",
          "form": "저는 하루에 2시간 하는 알바에요 ㅋㅋ",
          "original_form": "저는 하루에 2시간 하는 알바에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:18"
        },
        {
          "id": "MDRW2100041532.1.16",
