      "utterance": [
        {
          "id": "MDRW2100044498.1.1",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044498.1.2",
          "form": "네! 저녁 먹었어요",
          "original_form": "네! 저녁 먹었어요",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044498.1.3",
          "form": "제 저녁 메뉴 맞춰보실래요ㅎㅎ?",
          "original_form": "제 저녁 메뉴 맞춰보실래요ㅎㅎ?",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044498.1.4",
          "form": "네 매운음식인가요?",
          "original_form": "네 매운음식인가요?",
          "speaker_id": "2",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044498.1.5",
          "form": "으음 빨갛지만 크게 매운 음식은 아니예요",
          "original_form": "으음 빨갛지만 크게 매운 음식은 아니예요",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044498.1.6",
          "form": "고기가 들어가나요?",
          "original_form": "고기가 들어가나요?",
          "speaker_id": "2",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044498.1.7",
          "form": "네! 고기가 들어갑니다",
          "original_form": "네! 고기가 들어갑니다",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044498.1.8",
          "form": "닭고기인가요?",
          "original_form": "닭고기인가요?",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044498.1.9",
          "form": "닭고기는 아니예요",
          "original_form": "닭고기는 아니예요",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044498.1.10",
          "form": "소랑 돼지중에 어떤건가요?",
          "original_form": "소랑 돼지중에 어떤건가요?",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044498.1.11",
          "form": "돼지입니다! 맛있는 돼지고기",
          "original_form": "돼지입니다! 맛있는 돼지고기",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044498.1.12",
          "form": "제육볶음 드셨나요?",
          "original_form": "제육볶음 드셨나요?",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044498.1.13",
          "form": "땡~ 제육볶음 아닙니다",
          "original_form": "땡~ 제육볶음 아닙니다",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044498.1.14",
          "form": "헉 어렵네요 ㅋㅋㅋ",
          "original_form": "헉 어렵네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044498.1.15",
          "form": "힌트 하나만 주세요",
          "original_form": "힌트 하나만 주세요",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044498.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044498.1.17",
          "form": "국물이 있습니다!",
          "original_form": "국물이 있습니다!",
