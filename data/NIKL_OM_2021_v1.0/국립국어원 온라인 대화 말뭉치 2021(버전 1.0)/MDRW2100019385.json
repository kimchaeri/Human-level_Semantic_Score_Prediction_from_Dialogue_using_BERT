      "utterance": [
        {
          "id": "MDRW2100019385.1.1",
          "form": "영화 좋아하세요?",
          "original_form": "영화 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 18:34"
        },
        {
          "id": "MDRW2100019385.1.2",
          "form": "한달에 한두번은 극장에 가서 보는편이에요! 영화 좋아하시나요?",
          "original_form": "한달에 한두번은 극장에 가서 보는편이에요! 영화 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 18:36"
        },
        {
          "id": "MDRW2100019385.1.3",
          "form": "우와ㅋㅋ 저는 웨이브에서 봐요",
          "original_form": "우와ㅋㅋ 저는 웨이브에서 봐요",
          "speaker_id": "1",
          "time": "20210827 19:05"
        },
        {
          "id": "MDRW2100019385.1.4",
          "form": "극장은 1년에 한 번 정도 가는거 같아요ㅋㅋ",
          "original_form": "극장은 1년에 한 번 정도 가는거 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:05"
        },
        {
          "id": "MDRW2100019385.1.5",
          "form": "오 웨이브에 영화많나요? ㅎㅎ 저는 요즘 집중력이 떨어져서",
          "original_form": "오 웨이브에 영화많나요? ㅎㅎ 저는 요즘 집중력이 떨어져서",
          "speaker_id": "2",
          "time": "20210827 19:09"
        },
        {
          "id": "MDRW2100019385.1.6",
          "form": "집에서 보면 자꾸 딴짓하게되더라구요 ㅋㅋ 그래서 일부러 더 극장가는거같아요",
          "original_form": "집에서 보면 자꾸 딴짓하게되더라구요 ㅋㅋ 그래서 일부러 더 극장가는거같아요",
          "speaker_id": "2",
          "time": "20210827 19:09"
        },
        {
          "id": "MDRW2100019385.1.7",
          "form": "아 그건 있죠ㅋㅋ 집중이 깨지긴해요",
          "original_form": "아 그건 있죠ㅋㅋ 집중이 깨지긴해요",
          "speaker_id": "1",
          "time": "20210827 19:38"
        },
        {
          "id": "MDRW2100019385.1.8",
          "form": "멈출수도 있고",
          "original_form": "멈출수도 있고",
          "speaker_id": "1",
          "time": "20210827 19:38"
        },
        {
          "id": "MDRW2100019385.1.9",
          "form": "네 ㅋㅋ 최근엔 뭐보셨어요?",
          "original_form": "네 ㅋㅋ 최근엔 뭐보셨어요?",
          "speaker_id": "2",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100019385.1.10",
          "form": "전 얼마전에 블랙위도우 보구...음...뭐봤더라",
          "original_form": "전 얼마전에 블랙위도우 보구...음...뭐봤더라",
          "speaker_id": "2",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100019385.1.11",
          "form": "마지막이 아이언맨이었어요ㅋㅋㅋ",
          "original_form": "마지막이 아이언맨이었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:11"
        },
        {
          "id": "MDRW2100019385.1.12",
          "form": "집에서는 말할수없는비밀이요",
          "original_form": "집에서는 말할수없는비밀이요",
          "speaker_id": "1",
          "time": "20210827 20:11"
        },
        {
          "id": "MDRW2100019385.1.13",
          "form": "아이언맨이면 진짜 꽤 되셨네요 ㅋㅋㅋ",
          "original_form": "아이언맨이면 진짜 꽤 되셨네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:12"
        },
        {
          "id": "MDRW2100019385.1.14",
          "form": "말할수없는비밀 얘전애 봤었는데!",
          "original_form": "말할수없는비밀 얘전애 봤었는데!",
          "speaker_id": "2",
          "time": "20210827 20:12"
        },
        {
          "id": "MDRW2100019385.1.15",
          "form": "네ㅋㅋ",
          "original_form": "네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100019385.1.16",
          "form": "오옷!",
          "original_form": "오옷!",
          "speaker_id": "1",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100019385.1.17",
          "form": "어떠셨어요?",
          "original_form": "어떠셨어요?",
          "speaker_id": "1",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100019385.1.18",
          "form": "아 제가 이걸 학생때 처음봐서 그시절 생각도나고",
          "original_form": "아 제가 이걸 학생때 처음봐서 그시절 생각도나고",
          "speaker_id": "2",
          "time": "20210827 20:15"
        },
        {
          "id": "MDRW2100019385.1.19",
          "form": "약간 아련한 분위기가 전체적으로 있는거같아요 ㅋㅋㅋ",
          "original_form": "약간 아련한 분위기가 전체적으로 있는거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:16"
        },
        {
          "id": "MDRW2100019385.1.20",
          "form": "맞아요! 묘하게 아련하고 영상미 있어서 꽤 좋아해요",
          "original_form": "맞아요! 묘하게 아련하고 영상미 있어서 꽤 좋아해요",
          "speaker_id": "1",
          "time": "20210827 20:16"
        },
        {
          "id": "MDRW2100019385.1.21",
          "form": "넹 그리구 피아노를 배우고싶어지는 ㅋㅋㅋ",
          "original_form": "넹 그리구 피아노를 배우고싶어지는 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019385.1.22",
          "form": "가끔 생각나서 ost만 듣기두해요",
          "original_form": "가끔 생각나서 ost만 듣기두해요",
          "speaker_id": "2",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019385.1.23",
          "form": "저두용ㅋㅋ 저거때문에 피아노 등록할뻔했어요",
          "original_form": "저두용ㅋㅋ 저거때문에 피아노 등록할뻔했어요",
          "speaker_id": "1",
          "time": "20210827 20:22"
        },
        {
          "id": "MDRW2100019385.1.24",
          "form": "뭔가 저 대만이나 홍콩영화 특유의 느낌이 좋더라구요 ㅎㅎ",
          "original_form": "뭔가 저 대만이나 홍콩영화 특유의 느낌이 좋더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019385.1.25",
          "form": "나의소녀시대같은?",
          "original_form": "나의소녀시대같은?",
          "speaker_id": "2",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019385.1.26",
          "form": "오! 그거 왕대륙에 빠진다는 마성의 영화죠ㅋㅋ",
          "original_form": "오! 그거 왕대륙에 빠진다는 마성의 영화죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019385.1.27",
          "form": "드라마는 장난스런 키스 대만편 좋아해용",
          "original_form": "드라마는 장난스런 키스 대만편 좋아해용",
          "speaker_id": "1",
          "time": "20210827 20:24"
        }
      ]
