      "utterance": [
        {
          "id": "MDRW2100003879.1.1",
          "form": "요즘 좋아하는 음식 있으세요?",
          "original_form": "요즘 좋아하는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210821 13:12"
        },
        {
          "id": "MDRW2100003879.1.2",
          "form": "치킨이요",
          "original_form": "치킨이요",
          "speaker_id": "2",
          "time": "20210821 13:14"
        },
        {
          "id": "MDRW2100003879.1.3",
          "form": "치킨 좋죠~",
          "original_form": "치킨 좋죠~",
          "speaker_id": "1",
          "time": "20210821 13:22"
        },
        {
          "id": "MDRW2100003879.1.4",
          "form": "여긴 비가와서 그런지 파전이 생각나네요 ㅎ",
          "original_form": "여긴 비가와서 그런지 파전이 생각나네요 ㅎ",
          "speaker_id": "1",
          "time": "20210821 13:23"
        },
        {
          "id": "MDRW2100003879.1.5",
          "form": "아 좋죠",
          "original_form": "아 좋죠",
          "speaker_id": "2",
          "time": "20210821 13:23"
        },
        {
          "id": "MDRW2100003879.1.6",
          "form": "점심은 드셨나요?",
          "original_form": "점심은 드셨나요?",
          "speaker_id": "1",
          "time": "20210821 13:25"
        },
        {
          "id": "MDRW2100003879.1.7",
          "form": "아직요",
          "original_form": "아직요",
          "speaker_id": "2",
          "time": "20210821 13:26"
        },
        {
          "id": "MDRW2100003879.1.8",
          "form": "아~ 점심메뉴 고민되네요 ㅎ",
          "original_form": "아~ 점심메뉴 고민되네요 ㅎ",
          "speaker_id": "1",
          "time": "20210821 13:43"
        },
        {
          "id": "MDRW2100003879.1.9",
          "form": "맥도날드 갈릭버거 나왔다던데 궁금하더라구요",
          "original_form": "맥도날드 갈릭버거 나왔다던데 궁금하더라구요",
          "speaker_id": "1",
          "time": "20210821 13:43"
        },
        {
          "id": "MDRW2100003879.1.10",
          "form": "갈릭 어울릴까요?",
          "original_form": "갈릭 어울릴까요?",
          "speaker_id": "2",
          "time": "20210821 13:44"
        },
        {
          "id": "MDRW2100003879.1.11",
          "form": "햄버거에요? 마늘 좋아해서 맛있을것같아요~ ㅎㅎ",
          "original_form": "햄버거에요? 마늘 좋아해서 맛있을것같아요~ ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 13:46"
        },
        {
          "id": "MDRW2100003879.1.12",
          "form": "배달시킬랬더니 주변에 맥날이 없네요 ㅠ",
          "original_form": "배달시킬랬더니 주변에 맥날이 없네요 ㅠ",
          "speaker_id": "1",
          "time": "20210821 13:46"
        },
        {
          "id": "MDRW2100003879.1.13",
          "form": "다른거 드셔야 겠네요",
          "original_form": "다른거 드셔야 겠네요",
          "speaker_id": "2",
          "time": "20210821 13:47"
        },
        {
          "id": "MDRW2100003879.1.14",
          "form": "그쵸 ㅠㅠ",
          "original_form": "그쵸 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 14:04"
        },
        {
          "id": "MDRW2100003879.1.15",
          "form": "좋아하는 치킨 브랜드가 있으신가요?",
          "original_form": "좋아하는 치킨 브랜드가 있으신가요?",
          "speaker_id": "1",
          "time": "20210821 14:04"
        },
        {
          "id": "MDRW2100003879.1.16",
          "form": "네네요",
          "original_form": "네네요",
          "speaker_id": "2",
          "time": "20210821 15:43"
        },
        {
          "id": "MDRW2100003879.1.17",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100003879.1.18",
          "form": "님은?",
          "original_form": "님은?",
          "speaker_id": "2",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100003879.1.19",
          "form": "최근에 교촌치킨에서 나온",
          "original_form": "최근에 교촌치킨에서 나온",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100003879.1.20",
          "form": "허니콤보 먹어봤는데 넘 맛있더라구요!",
          "original_form": "허니콤보 먹어봤는데 넘 맛있더라구요!",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100003879.1.21",
          "form": "신세계였어요",
          "original_form": "신세계였어요",
          "speaker_id": "1",
          "time": "20210821 21:48"
        }
      ]
