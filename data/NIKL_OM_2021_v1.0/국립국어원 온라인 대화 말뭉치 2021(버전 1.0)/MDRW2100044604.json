      "utterance": [
        {
          "id": "MDRW2100044604.1.1",
          "form": "몇 개 했었어",
          "original_form": "몇 개 했었어",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044604.1.2",
          "form": "근데 기억이 안나(?)",
          "original_form": "근데 기억이 안나(?)",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044604.1.3",
          "form": "사실 처음에??원래도 인가??지금도 인가??",
          "original_form": "사실 처음에??원래도 인가??지금도 인가??",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044604.1.4",
          "form": "큐브 시리즈라고 불리잖아",
          "original_form": "큐브 시리즈라고 불리잖아",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044604.1.5",
          "form": "너무 예전에 해서..ㅋ.ㅋ..ㅋ 좀 얼레벌레 했던 기억",
          "original_form": "너무 예전에 해서..ㅋ.ㅋ..ㅋ 좀 얼레벌레 했던 기억",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044604.1.6",
          "form": "나는 몇몇개 인상 깊었던게",
          "original_form": "나는 몇몇개 인상 깊었던게",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044604.1.7",
          "form": "앗 응ㅇ응응 맞아",
          "original_form": "앗 응ㅇ응응 맞아",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044604.1.8",
          "form": "그 하비박스였나?",
          "original_form": "그 하비박스였나?",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044604.1.9",
          "form": "하비 시점으로 플레이 하던게 기억나",
          "original_form": "하비 시점으로 플레이 하던게 기억나",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044604.1.10",
          "form": "응ㅇ응응",
          "original_form": "응ㅇ응응",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044604.1.11",
          "form": "진짜 시리즈 역대급 나는 그게 젤 무서웠다...",
          "original_form": "진짜 시리즈 역대급 나는 그게 젤 무서웠다...",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044604.1.12",
          "form": "마지막에 검은 사람이 상자에다가 손 넣잖아",
          "original_form": "마지막에 검은 사람이 상자에다가 손 넣잖아",
          "speaker_id": "1",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044604.1.13",
          "form": "아니 뭐지..?완전 흥미 그것은 유료인가요?",
          "original_form": "아니 뭐지..?완전 흥미 그것은 유료인가요?",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044604.1.14",
          "form": "넴넴",
          "original_form": "넴넴",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044604.1.15",
          "form": "그게 진짜 너무 무서웠어",
          "original_form": "그게 진짜 너무 무서웠어",
          "speaker_id": "1",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044604.1.16",
          "form": "아니용",
          "original_form": "아니용",
          "speaker_id": "1",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044604.1.17",
          "form": "그거 그냥 구글 스토어 검색하면",
          "original_form": "그거 그냥 구글 스토어 검색하면",
          "speaker_id": "1",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044604.1.18",
          "form": "진짜 cube:",
          "original_form": "진짜 cube:",
          "speaker_id": "1",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044604.1.19",
          "form": "부재로 하비 박스라고 되어있어",
          "original_form": "부재로 하비 박스라고 되어있어",
          "speaker_id": "1",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044604.1.20",
          "form": "헉 그렇구나... 방금 찾아봤는데 그건 아직 안한듯 해봐야지(ㅎㅎ)",
          "original_form": "헉 그렇구나... 방금 찾아봤는데 그건 아직 안한듯 해봐야지(ㅎㅎ)",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044604.1.21",
          "form": "이 하비 친구의 이름 스팰링은 몰라서 ㅈㅅ;",
          "original_form": "이 하비 친구의 이름 스팰링은 몰라서 ㅈㅅ;",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044604.1.22",
          "form": "여튼 응ㅇ응응",
          "original_form": "여튼 응ㅇ응응",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044604.1.23",
          "form": "근데 좀 한글화가 늦은 것두 몇개있구해서",
          "original_form": "근데 좀 한글화가 늦은 것두 몇개있구해서",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044604.1.24",
          "form": "그것 땜에 안 해보신것두 몇몇개 잇을거 같다",
          "original_form": "그것 땜에 안 해보신것두 몇몇개 잇을거 같다",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044604.1.25",
          "form": "나는 걍 노빠꾸로",
          "original_form": "나는 걍 노빠꾸로",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044604.1.26",
          "form": "아 몰라 나는 영어로 한다",
          "original_form": "아 몰라 나는 영어로 한다",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044604.1.27",
          "form": "응ㅇ응 맞아 옛날에 한건 한글화+무료 버전 뿐이었을거야",
          "original_form": "응ㅇ응 맞아 옛날에 한건 한글화+무료 버전 뿐이었을거야",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044604.1.28",
          "form": "이러면서 한 참 빠졌을때",
          "original_form": "이러면서 한 참 빠졌을때",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044604.1.29",
          "form": "아 ㅋ ㅋ ㅠ ㅠ ㅋ ㅋ 개웃겨 근성의 게임ㅇ러",
          "original_form": "아 ㅋ ㅋ ㅠ ㅠ ㅋ ㅋ 개웃겨 근성의 게임ㅇ러",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044604.1.30",
          "form": "했었거든",
          "original_form": "했었거든",
