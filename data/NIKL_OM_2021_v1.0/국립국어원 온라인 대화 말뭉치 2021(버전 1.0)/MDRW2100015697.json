      "utterance": [
        {
          "id": "MDRW2100015697.1.1",
          "form": "어떤 스포츠 좋아하세요?",
          "original_form": "어떤 스포츠 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 00:22"
        },
        {
          "id": "MDRW2100015697.1.2",
          "form": "전 이스포츠 많이 봐요ㅎㅎ",
          "original_form": "전 이스포츠 많이 봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:24"
        },
        {
          "id": "MDRW2100015697.1.3",
          "form": "한때는 야구 자주 봤고요",
          "original_form": "한때는 야구 자주 봤고요",
          "speaker_id": "2",
          "time": "20210826 00:24"
        },
        {
          "id": "MDRW2100015697.1.4",
          "form": "아하 저는 올림픽 때문에 배구에 관심이 좀 생긴 것 같아요",
          "original_form": "아하 저는 올림픽 때문에 배구에 관심이 좀 생긴 것 같아요",
          "speaker_id": "1",
          "time": "20210826 00:25"
        },
        {
          "id": "MDRW2100015697.1.5",
          "form": "좀 늦게 관심이 생겨서 지금 배구팀으로 많은 경기를 볼 수 없는 게 살짝 아쉬워요",
          "original_form": "좀 늦게 관심이 생겨서 지금 배구팀으로 많은 경기를 볼 수 없는 게 살짝 아쉬워요",
          "speaker_id": "1",
          "time": "20210826 00:25"
        },
        {
          "id": "MDRW2100015697.1.6",
          "form": "맞아요 아쉽죠...",
          "original_form": "맞아요 아쉽죠...",
          "speaker_id": "2",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100015697.1.7",
          "form": "리그 개막하면 리그도 보실건가요?",
          "original_form": "리그 개막하면 리그도 보실건가요?",
          "speaker_id": "2",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100015697.1.8",
          "form": "네 시간이 맞으면요 ㅎㅎ",
          "original_form": "네 시간이 맞으면요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 00:29"
        },
        {
          "id": "MDRW2100015697.1.9",
          "form": "야구 어떤 팀 좋아하세요",
          "original_form": "야구 어떤 팀 좋아하세요",
          "speaker_id": "1",
          "time": "20210826 00:29"
        },
        {
          "id": "MDRW2100015697.1.10",
          "form": "키움 응원했었는데 그것도 몇 년 전에나 많이 봤었어요",
          "original_form": "키움 응원했었는데 그것도 몇 년 전에나 많이 봤었어요",
          "speaker_id": "2",
          "time": "20210826 00:30"
        },
        {
          "id": "MDRW2100015697.1.11",
          "form": "여자배구는 팬서비스도 좋다그러고 경기 티켓도 싸서 직관가도 재밌을 것 같아요ㅎㅎ",
          "original_form": "여자배구는 팬서비스도 좋다그러고 경기 티켓도 싸서 직관가도 재밌을 것 같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015697.1.12",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210826 00:35"
        },
        {
          "id": "MDRW2100015697.1.13",
          "form": "요즘 유튜브로 팬서비스 영상들이 알고리즘으로 뜨던데",
          "original_form": "요즘 유튜브로 팬서비스 영상들이 알고리즘으로 뜨던데",
          "speaker_id": "1",
          "time": "20210826 00:35"
        },
        {
          "id": "MDRW2100015697.1.14",
          "form": "여자 배구 선수들이 진짜 팬서비스 너무 좋아서 ㅠㅠ",
          "original_form": "여자 배구 선수들이 진짜 팬서비스 너무 좋아서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100015697.1.15",
          "form": "항상 직관 가고는 싶습니당..",
          "original_form": "항상 직관 가고는 싶습니당..",
          "speaker_id": "1",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100015697.1.16",
          "form": "맞아요 직관은 또 직관가서만 느낄 수 있는 현장감이 있잖아요",
          "original_form": "맞아요 직관은 또 직관가서만 느낄 수 있는 현장감이 있잖아요",
          "speaker_id": "2",
          "time": "20210826 00:37"
        },
        {
          "id": "MDRW2100015697.1.17",
          "form": "그런 거 때문에 계속 가게 되는 것 같아요ㅎㅎ",
          "original_form": "그런 거 때문에 계속 가게 되는 것 같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015697.1.18",
          "form": "사실 저는 직관 한 번도 가본적이 없는데 가보셨나용??? ㅎㅎ",
          "original_form": "사실 저는 직관 한 번도 가본적이 없는데 가보셨나용??? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 00:38"
        },
        {
          "id": "MDRW2100015697.1.19",
          "form": "야구볼 땐 좀 갔었어요ㅎㅎ",
          "original_form": "야구볼 땐 좀 갔었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:39"
        },
        {
          "id": "MDRW2100015697.1.20",
          "form": "집에서 잠실야구장이 가깝거든요",
          "original_form": "집에서 잠실야구장이 가깝거든요",
          "speaker_id": "2",
          "time": "20210826 00:40"
        },
        {
          "id": "MDRW2100015697.1.21",
          "form": "와 부럽다 ㅎㅎㅎㅎㅎ",
          "original_form": "와 부럽다 ㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 00:41"
        },
        {
          "id": "MDRW2100015697.1.22",
          "form": "스포츠 보러 본인이 움직인다는 게",
          "original_form": "스포츠 보러 본인이 움직인다는 게",
          "speaker_id": "1",
          "time": "20210826 00:42"
        },
        {
          "id": "MDRW2100015697.1.23",
          "form": "사람이 부지런하고",
          "original_form": "사람이 부지런하고",
          "speaker_id": "1",
          "time": "20210826 00:42"
        },
        {
          "id": "MDRW2100015697.1.24",
          "form": "활력 넘치게 해주는 것 같아요",
          "original_form": "활력 넘치게 해주는 것 같아요",
          "speaker_id": "1",
          "time": "20210826 00:42"
        },
        {
          "id": "MDRW2100015697.1.25",
          "form": "맞아요 열광하는 기운도 많이 전해지고요ㅎㅎ",
          "original_form": "맞아요 열광하는 기운도 많이 전해지고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 01:08"
        },
        {
          "id": "MDRW2100015697.1.26",
          "form": "잠실이면 스포츠 보기 딱 좋네요",
          "original_form": "잠실이면 스포츠 보기 딱 좋네요",
          "speaker_id": "1",
          "time": "20210826 01:09"
        },
        {
          "id": "MDRW2100015697.1.27",
          "form": "맞아요 그런 장점이 있죠ㅎㅎ",
          "original_form": "맞아요 그런 장점이 있죠ㅎㅎ",
          "speaker_id": "2",
