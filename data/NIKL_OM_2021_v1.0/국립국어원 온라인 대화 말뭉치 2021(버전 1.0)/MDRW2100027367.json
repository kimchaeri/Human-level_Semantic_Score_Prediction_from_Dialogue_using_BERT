      "utterance": [
        {
          "id": "MDRW2100027367.1.1",
          "form": "으악!",
          "original_form": "으악!",
          "speaker_id": "2",
          "time": "20210902 12:03"
        },
        {
          "id": "MDRW2100027367.1.2",
          "form": "으하하",
          "original_form": "으하하",
          "speaker_id": "1",
          "time": "20210902 12:09"
        },
        {
          "id": "MDRW2100027367.1.3",
          "form": "그래서 대학원 라이프를 위해",
          "original_form": "그래서 대학원 라이프를 위해",
          "speaker_id": "1",
          "time": "20210902 12:09"
        },
        {
          "id": "MDRW2100027367.1.4",
          "form": "갤탭이랑",
          "original_form": "갤탭이랑",
          "speaker_id": "1",
          "time": "20210902 12:09"
        },
        {
          "id": "MDRW2100027367.1.5",
          "form": "노트북을 새로 장만했답니다~!",
          "original_form": "노트북을 새로 장만했답니다~!",
          "speaker_id": "1",
          "time": "20210902 12:09"
        },
        {
          "id": "MDRW2100027367.1.6",
          "form": "헐좋은거 장만하셨군요",
          "original_form": "헐좋은거 장만하셨군요",
          "speaker_id": "2",
          "time": "20210902 12:10"
        },
        {
          "id": "MDRW2100027367.1.7",
          "form": "딱 1년 전이니까",
          "original_form": "딱 1년 전이니까",
          "speaker_id": "1",
          "time": "20210902 12:12"
        },
        {
          "id": "MDRW2100027367.1.8",
          "form": "지금 보면 그렇게 좋은건 또 아니네요ㅠ",
          "original_form": "지금 보면 그렇게 좋은건 또 아니네요ㅠ",
          "speaker_id": "1",
          "time": "20210902 12:12"
        },
        {
          "id": "MDRW2100027367.1.9",
          "form": "에이 그래도 산게어딘가요",
          "original_form": "에이 그래도 산게어딘가요",
          "speaker_id": "2",
          "time": "20210902 12:13"
        },
        {
          "id": "MDRW2100027367.1.10",
          "form": "저도 저에게 선물할려고",
          "original_form": "저도 저에게 선물할려고",
          "speaker_id": "2",
          "time": "20210902 12:13"
        },
        {
          "id": "MDRW2100027367.1.11",
          "form": "게이밍 컴퓨터 샀답니당",
          "original_form": "게이밍 컴퓨터 샀답니당",
          "speaker_id": "2",
          "time": "20210902 12:13"
        },
        {
          "id": "MDRW2100027367.1.12",
          "form": "오오 게이밍 컴퓨터!!!!!",
          "original_form": "오오 게이밍 컴퓨터!!!!!",
          "speaker_id": "1",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100027367.1.13",
          "form": "왠지 마우스도 버티컬 마우스 쓰실 것 같아요 !!",
          "original_form": "왠지 마우스도 버티컬 마우스 쓰실 것 같아요 !!",
          "speaker_id": "1",
          "time": "20210902 12:16"
        },
        {
          "id": "MDRW2100027367.1.14",
          "form": "아쉽게도 로지텍입니덩",
          "original_form": "아쉽게도 로지텍입니덩",
          "speaker_id": "2",
          "time": "20210902 12:16"
        },
        {
          "id": "MDRW2100027367.1.15",
          "form": "아하 그래도",
          "original_form": "아하 그래도",
          "speaker_id": "1",
          "time": "20210902 12:17"
        },
        {
          "id": "MDRW2100027367.1.16",
          "form": "로지텍 정도면 가성비 좋죠 !!!!",
          "original_form": "로지텍 정도면 가성비 좋죠 !!!!",
          "speaker_id": "1",
          "time": "20210902 12:17"
        },
        {
          "id": "MDRW2100027367.1.17",
          "form": "맞죠??저도 가성비로 샀어욬ㅋㅋ",
          "original_form": "맞죠??저도 가성비로 샀어욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027367.1.18",
          "form": "맞아요,, 믿고 살 수 있는 기업 중 하나예요!!!",
          "original_form": "맞아요,, 믿고 살 수 있는 기업 중 하나예요!!!",
          "speaker_id": "1",
          "time": "20210902 12:25"
        },
        {
          "id": "MDRW2100027367.1.19",
          "form": "저도 예전에 마우스 로지텍꺼 썼었어요 ㅎㅎ",
          "original_form": "저도 예전에 마우스 로지텍꺼 썼었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 12:25"
        },
        {
          "id": "MDRW2100027367.1.20",
          "form": "맞아요 먼가 조터라구요",
          "original_form": "맞아요 먼가 조터라구요",
          "speaker_id": "2",
          "time": "20210902 12:36"
        },
        {
          "id": "MDRW2100027367.1.21",
          "form": "마우스패드는 어떤걸로 쓰세요?!",
          "original_form": "마우스패드는 어떤걸로 쓰세요?!",
          "speaker_id": "1",
          "time": "20210902 12:56"
        },
        {
          "id": "MDRW2100027367.1.22",
          "form": "아무거나 써요",
          "original_form": "아무거나 써요",
          "speaker_id": "2",
          "time": "20210902 13:01"
        },
        {
          "id": "MDRW2100027367.1.23",
