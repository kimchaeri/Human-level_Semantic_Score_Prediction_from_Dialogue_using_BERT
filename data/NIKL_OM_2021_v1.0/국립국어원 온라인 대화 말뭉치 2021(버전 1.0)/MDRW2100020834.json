      "utterance": [
        {
          "id": "MDRW2100020834.1.1",
          "form": "2.1 업데이트 벌써 3일밖에 안남았어",
          "original_form": "2.1 업데이트 벌써 3일밖에 안남았어",
          "speaker_id": "1",
          "time": "20210828 13:11"
        },
        {
          "id": "MDRW2100020834.1.2",
          "form": "은근 시간 빠르게 가는듯",
          "original_form": "은근 시간 빠르게 가는듯",
          "speaker_id": "1",
          "time": "20210828 13:11"
        },
        {
          "id": "MDRW2100020834.1.3",
          "form": "ㅎㅏ 가챠",
          "original_form": "ㅎㅏ 가챠",
          "speaker_id": "2",
          "time": "20210828 13:12"
        },
        {
          "id": "MDRW2100020834.1.4",
          "form": "진짜 어떡하지",
          "original_form": "진짜 어떡하지",
          "speaker_id": "2",
          "time": "20210828 13:12"
        },
        {
          "id": "MDRW2100020834.1.5",
          "form": "애증의 가챠.....",
          "original_form": "애증의 가챠.....",
          "speaker_id": "2",
          "time": "20210828 13:12"
        },
        {
          "id": "MDRW2100020834.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:12"
        },
        {
          "id": "MDRW2100020834.1.7",
          "form": "일단 원석은 최대한 모으고",
          "original_form": "일단 원석은 최대한 모으고",
          "speaker_id": "1",
          "time": "20210828 13:12"
        },
        {
          "id": "MDRW2100020834.1.8",
          "form": "라이덴 뽑는건 좀 기다려봐",
          "original_form": "라이덴 뽑는건 좀 기다려봐",
          "speaker_id": "1",
          "time": "20210828 13:12"
        },
        {
          "id": "MDRW2100020834.1.9",
          "form": "신규 컨텐츠 업데이트 하면",
          "original_form": "신규 컨텐츠 업데이트 하면",
          "speaker_id": "1",
          "time": "20210828 13:12"
        },
        {
          "id": "MDRW2100020834.1.10",
          "form": "ㅇㅇ그러려구",
          "original_form": "ㅇㅇ그러려구",
          "speaker_id": "2",
          "time": "20210828 13:12"
        },
        {
          "id": "MDRW2100020834.1.11",
          "form": "다른 사람들이 달려들어서 성능 테스트 해줄테니까",
          "original_form": "다른 사람들이 달려들어서 성능 테스트 해줄테니까",
          "speaker_id": "1",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100020834.1.12",
          "form": "2천장.......",
          "original_form": "2천장.......",
          "speaker_id": "2",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100020834.1.13",
          "form": "ㅎㅏ",
          "original_form": "ㅎㅏ",
          "speaker_id": "2",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100020834.1.14",
          "form": "근데 나 카즈하 뽑을때",
          "original_form": "근데 나 카즈하 뽑을때",
          "speaker_id": "1",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100020834.1.15",
          "form": "반천장으로 나왔어",
          "original_form": "반천장으로 나왔어",
          "speaker_id": "1",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100020834.1.16",
          "form": "언니는",
          "original_form": "언니는",
          "speaker_id": "2",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100020834.1.17",
          "form": "50프로 확률이라",
          "original_form": "50프로 확률이라",
          "speaker_id": "1",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100020834.1.18",
          "form": "그냥 뽑아도",
          "original_form": "그냥 뽑아도",
          "speaker_id": "2",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100020834.1.19",
          "form": "나왓잔아...",
          "original_form": "나왓잔아...",
          "speaker_id": "2",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100020834.1.20",
          "form": "아닠ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아닠ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100020834.1.21",
          "form": "난.....70연까지",
          "original_form": "난.....70연까지",
          "speaker_id": "2",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100020834.1.22",
          "form": "이젠 안나와....",
          "original_form": "이젠 안나와....",
          "speaker_id": "1",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100020834.1.23",
          "form": "아무도 안왓다구",
          "original_form": "아무도 안왓다구",
          "speaker_id": "2",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100020834.1.24",
          "form": "에일로이 쓸만하면",
          "original_form": "에일로이 쓸만하면",
          "speaker_id": "2",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100020834.1.25",
          "form": "4성칼 케이아한테 뺏어야지",
          "original_form": "4성칼 케이아한테 뺏어야지",
          "speaker_id": "2",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100020834.1.26",
          "form": "에일로이 플레이스테이션으로 로그인하면 무료 증정한다는데",
          "original_form": "에일로이 플레이스테이션으로 로그인하면 무료 증정한다는데",
          "speaker_id": "1",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100020834.1.27",
          "form": "활캐야",
          "original_form": "활캐야",
          "speaker_id": "1",
          "time": "20210828 13:15"
        },
        {
          "id": "MDRW2100020834.1.28",
          "form": "아 주인공이",
          "original_form": "아 주인공이",
          "speaker_id": "2",
          "time": "20210828 13:15"
        },
        {
          "id": "MDRW2100020834.1.29",
          "form": "3성칼쓰고잇거든",
          "original_form": "3성칼쓰고잇거든",
          "speaker_id": "2",
          "time": "20210828 13:15"
        },
        {
          "id": "MDRW2100020834.1.30",
          "form": "파티에서 뺄거면",
          "original_form": "파티에서 뺄거면",
          "speaker_id": "2",
          "time": "20210828 13:15"
        },
        {
          "id": "MDRW2100020834.1.31",
          "form": "반납을하고 가야지",
          "original_form": "반납을하고 가야지",
          "speaker_id": "2",
          "time": "20210828 13:15"
        },
        {
          "id": "MDRW2100020834.1.32",
          "form": "ㅇㅎㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅎㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:15"
        },
        {
          "id": "MDRW2100020834.1.33",
          "form": "매정한 플레이어구나",
          "original_form": "매정한 플레이어구나",
          "speaker_id": "1",
          "time": "20210828 13:16"
        },
        {
          "id": "MDRW2100020834.1.34",
          "form": "한손검 4성은",
          "original_form": "한손검 4성은",
          "speaker_id": "2",
          "time": "20210828 13:16"
        },
        {
          "id": "MDRW2100020834.1.35",
          "form": "ㅓ쩔수없어...",
          "original_form": "ㅓ쩔수없어...",
          "speaker_id": "2",
          "time": "20210828 13:16"
        },
        {
          "id": "MDRW2100020834.1.36",
          "form": "내가 4성창이랑 활은",
          "original_form": "내가 4성창이랑 활은",
          "speaker_id": "2",
          "time": "20210828 13:16"
        },
        {
          "id": "MDRW2100020834.1.37",
          "form": "캐릭터랑 무기공급 밸런스가 안맞긴 해..",
          "original_form": "캐릭터랑 무기공급 밸런스가 안맞긴 해..",
          "speaker_id": "1",
          "time": "20210828 13:16"
        },
        {
          "id": "MDRW2100020834.1.38",
          "form": "애들 걍 다 들고있으라고",
          "original_form": "애들 걍 다 들고있으라고",
          "speaker_id": "2",
          "time": "20210828 13:16"
        },
        {
          "id": "MDRW2100020834.1.39",
          "form": "들려줬다구",
          "original_form": "들려줬다구",
          "speaker_id": "2",
          "time": "20210828 13:16"
        },
        {
          "id": "MDRW2100020834.1.40",
          "form": "나도 4성 활 엠버한테 들려줌",
          "original_form": "나도 4성 활 엠버한테 들려줌",
          "speaker_id": "1",
          "time": "20210828 13:17"
        },
        {
          "id": "MDRW2100020834.1.41",
          "form": "ㅇㅏ엠버는",
          "original_form": "ㅇㅏ엠버는",
          "speaker_id": "2",
          "time": "20210828 13:17"
        },
        {
          "id": "MDRW2100020834.1.42",
          "form": "걍 3성활이나",
          "original_form": "걍 3성활이나",
          "speaker_id": "2",
          "time": "20210828 13:17"
        },
        {
          "id": "MDRW2100020834.1.43",
          "form": "써",
          "original_form": "써",
          "speaker_id": "2",
          "time": "20210828 13:17"
        },
        {
          "id": "MDRW2100020834.1.44",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:17"
        },
        {
          "id": "MDRW2100020834.1.45",
          "form": "가차없네",
          "original_form": "가차없네",
          "speaker_id": "1",
          "time": "20210828 13:17"
        },
        {
          "id": "MDRW2100020834.1.46",
          "form": "걔는 진짜 거대상향을 받ㅈㅣ않는 한",
          "original_form": "걔는 진짜 거대상향을 받ㅈㅣ않는 한",
          "speaker_id": "2",
          "time": "20210828 13:17"
        },
        {
          "id": "MDRW2100020834.1.47",
          "form": "쓸일없을것같아",
          "original_form": "쓸일없을것같아",
          "speaker_id": "2",
          "time": "20210828 13:17"
        },
        {
          "id": "MDRW2100020834.1.48",
          "form": "기본지급 캐릭터라",
          "original_form": "기본지급 캐릭터라",
          "speaker_id": "1",
          "time": "20210828 13:18"
        },
        {
          "id": "MDRW2100020834.1.49",
          "form": "상향도 안할듯",
          "original_form": "상향도 안할듯",
          "speaker_id": "1",
          "time": "20210828 13:18"
        },
        {
          "id": "MDRW2100020834.1.50",
          "form": "여태까지 밸런스 조정한거 종려뿐이었오",
          "original_form": "여태까지 밸런스 조정한거 종려뿐이었오",
          "speaker_id": "1",
          "time": "20210828 13:18"
        },
        {
          "id": "MDRW2100020834.1.51",
          "form": "ㅇㅇ그래서",
          "original_form": "ㅇㅇ그래서",
          "speaker_id": "2",
          "time": "20210828 13:18"
        },
        {
          "id": "MDRW2100020834.1.52",
          "form": "가차없는 플레이어....",
          "original_form": "가차없는 플레이어....",
          "speaker_id": "1",
