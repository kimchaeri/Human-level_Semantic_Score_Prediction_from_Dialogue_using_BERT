      "utterance": [
        {
          "id": "MDRW2100005787.1.1",
          "form": "전 방금 브런치 먹었어용",
          "original_form": "전 방금 브런치 먹었어용",
          "speaker_id": "1",
          "time": "20210907 12:01"
        },
        {
          "id": "MDRW2100005787.1.2",
          "form": "저도 늦은 아침겸 점심 먹었네요 방금",
          "original_form": "저도 늦은 아침겸 점심 먹었네요 방금",
          "speaker_id": "2",
          "time": "20210907 12:01"
        },
        {
          "id": "MDRW2100005787.1.3",
          "form": "이제 뭐하시게요?",
          "original_form": "이제 뭐하시게요?",
          "speaker_id": "2",
          "time": "20210907 12:02"
        },
        {
          "id": "MDRW2100005787.1.4",
          "form": "전 이제 책 읽으려구용 혹시 책 좋아하세요?",
          "original_form": "전 이제 책 읽으려구용 혹시 책 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 12:02"
        },
        {
          "id": "MDRW2100005787.1.5",
          "form": "네네 저 책 읽는거 좋아해용",
          "original_form": "네네 저 책 읽는거 좋아해용",
          "speaker_id": "2",
          "time": "20210907 12:02"
        },
        {
          "id": "MDRW2100005787.1.6",
          "form": "무슨 책 읽으세요??",
          "original_form": "무슨 책 읽으세요??",
          "speaker_id": "2",
          "time": "20210907 12:02"
        },
        {
          "id": "MDRW2100005787.1.7",
          "form": "심리 관련 책 읽고 있어요",
          "original_form": "심리 관련 책 읽고 있어요",
          "speaker_id": "1",
          "time": "20210907 12:03"
        },
        {
          "id": "MDRW2100005787.1.8",
          "form": "흥미롭더라구요",
          "original_form": "흥미롭더라구요",
          "speaker_id": "1",
          "time": "20210907 12:03"
        },
        {
          "id": "MDRW2100005787.1.9",
          "form": "아하~ 심리 관련 책 재밌죠! 사람들을 더욱 잘 이해할 수 있는 느낌이에요",
          "original_form": "아하~ 심리 관련 책 재밌죠! 사람들을 더욱 잘 이해할 수 있는 느낌이에요",
          "speaker_id": "2",
          "time": "20210907 12:04"
        },
        {
          "id": "MDRW2100005787.1.10",
          "form": "맞아요 저와 다른 사람 이해하는데 큰 도움이 됩니다",
          "original_form": "맞아요 저와 다른 사람 이해하는데 큰 도움이 됩니다",
          "speaker_id": "1",
          "time": "20210907 12:04"
        },
        {
          "id": "MDRW2100005787.1.11",
          "form": "저는 요즘 시집 읽고 있오용 ㅎㅎ",
          "original_form": "저는 요즘 시집 읽고 있오용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 12:05"
        },
        {
          "id": "MDRW2100005787.1.12",
          "form": "우와 시도 좋죠 문학도 정말 무난하게 읽는 분야 같아요",
          "original_form": "우와 시도 좋죠 문학도 정말 무난하게 읽는 분야 같아요",
          "speaker_id": "1",
          "time": "20210907 12:05"
        },
        {
          "id": "MDRW2100005787.1.13",
          "form": "네에 맞아용~ 제가 좋아하는 작가님이 신작을 내셔서 그거 보구 있네용",
          "original_form": "네에 맞아용~ 제가 좋아하는 작가님이 신작을 내셔서 그거 보구 있네용",
          "speaker_id": "2",
          "time": "20210907 12:06"
        },
        {
          "id": "MDRW2100005787.1.14",
          "form": "좋습니다ㅎㅎ 잡지는 읽으세요?",
          "original_form": "좋습니다ㅎㅎ 잡지는 읽으세요?",
          "speaker_id": "1",
          "time": "20210907 12:09"
        },
        {
          "id": "MDRW2100005787.1.15",
          "form": "네 저는 보그 정기구독해요",
          "original_form": "네 저는 보그 정기구독해요",
          "speaker_id": "2",
          "time": "20210907 12:09"
        },
        {
          "id": "MDRW2100005787.1.16",
          "form": "아하 그렇군요 전 잡지는 그닥 안 읽네요ㅎㅎ",
          "original_form": "아하 그렇군요 전 잡지는 그닥 안 읽네요ㅎㅎ",
          "speaker_id": "1",
