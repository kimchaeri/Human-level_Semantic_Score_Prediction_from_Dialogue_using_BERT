        {
          "id": "MDRW2100008140.1.1",
          "form": "화장품 뭐쓰세요",
          "original_form": "화장품 뭐쓰세요",
          "speaker_id": "1",
          "time": "20210822 13:31"
        },
        {
          "id": "MDRW2100008140.1.2",
          "form": "저는 로드샵 화장품 애용해요",
          "original_form": "저는 로드샵 화장품 애용해요",
          "speaker_id": "2",
          "time": "20210822 13:32"
        },
        {
          "id": "MDRW2100008140.1.3",
          "form": "화장품 어디서 사세요",
          "original_form": "화장품 어디서 사세요",
          "speaker_id": "2",
          "time": "20210822 13:32"
        },
        {
          "id": "MDRW2100008140.1.4",
          "form": "전요새 화장품안사요",
          "original_form": "전요새 화장품안사요",
          "speaker_id": "1",
          "time": "20210822 13:32"
        },
        {
          "id": "MDRW2100008140.1.5",
          "form": "코로나라서",
          "original_form": "코로나라서",
          "speaker_id": "1",
          "time": "20210822 13:32"
        },
        {
          "id": "MDRW2100008140.1.6",
          "form": "화장안해요",
          "original_form": "화장안해요",
          "speaker_id": "1",
          "time": "20210822 13:32"
        },
        {
          "id": "MDRW2100008140.1.7",
          "form": "그렇군요 코로나라서 다이어트 하기도 쉽지 않네요",
          "original_form": "그렇군요 코로나라서 다이어트 하기도 쉽지 않네요",
          "speaker_id": "2",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100008140.1.8",
          "form": "나가서 운동하는 것도 제한되고",
          "original_form": "나가서 운동하는 것도 제한되고",
          "speaker_id": "2",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100008140.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100008140.1.10",
          "form": "다이어트하시나요",
          "original_form": "다이어트하시나요",
          "speaker_id": "1",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100008140.1.11",
          "form": "아니오 안해요",
          "original_form": "아니오 안해요",
          "speaker_id": "2",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100008140.1.12",
          "form": "하지만 건강을 위해서는 해야겠죠",
          "original_form": "하지만 건강을 위해서는 해야겠죠",
          "speaker_id": "2",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100008140.1.13",
          "form": "다이어트 하시나요",
          "original_form": "다이어트 하시나요",
          "speaker_id": "2",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100008140.1.14",
          "form": "아니요 예요",
          "original_form": "아니요 예요",
          "speaker_id": "1",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100008140.1.15",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100008140.1.16",
          "form": "다이어트는 안해요",
          "original_form": "다이어트는 안해요",
          "speaker_id": "1",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100008140.1.17",
          "form": "그렇군요 운동은 좋아하세요?",
          "original_form": "그렇군요 운동은 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008140.1.18",
          "form": "전 운동 안 좋아해요",
          "original_form": "전 운동 안 좋아해요",
          "speaker_id": "2",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008140.1.19",
          "form": "저두요",
          "original_form": "저두요",
