      "utterance": [
        {
          "id": "MDRW2100046038.1.1",
          "form": "중고 거래도 하시나요",
          "original_form": "중고 거래도 하시나요",
          "speaker_id": "2",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100046038.1.2",
          "form": "당근 가끔 판매요",
          "original_form": "당근 가끔 판매요",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046038.1.3",
          "form": "어떤 물건을 구매하시나요",
          "original_form": "어떤 물건을 구매하시나요",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046038.1.4",
          "form": "요즘은 옛날씨디나 테이프 내놔요ㅋ",
          "original_form": "요즘은 옛날씨디나 테이프 내놔요ㅋ",
          "speaker_id": "1",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046038.1.5",
          "form": "복고 상품이네요",
          "original_form": "복고 상품이네요",
          "speaker_id": "2",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046038.1.6",
          "form": "뭔테이프가 그리많은지ㅋㅋ",
          "original_form": "뭔테이프가 그리많은지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046038.1.7",
          "form": "복고상품 그립네요",
          "original_form": "복고상품 그립네요",
          "speaker_id": "2",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046038.1.8",
          "form": "한번 당근에 가봐야 겠어요",
          "original_form": "한번 당근에 가봐야 겠어요",
          "speaker_id": "2",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046038.1.9",
          "form": "괜찬은거 많이 나오긴 하더라고요",
          "original_form": "괜찬은거 많이 나오긴 하더라고요",
          "speaker_id": "1",
          "time": "20210908 09:59"
        },
        {
          "id": "MDRW2100046038.1.10",
          "form": "먼저 아이쇼핑부터 해봐야 겠어요",
          "original_form": "먼저 아이쇼핑부터 해봐야 겠어요",
          "speaker_id": "2",
          "time": "20210908 10:02"
        },
        {
          "id": "MDRW2100046038.1.11",
          "form": "네 저도 안사도 보고 잇을때 많아요ㅋ",
          "original_form": "네 저도 안사도 보고 잇을때 많아요ㅋ",
          "speaker_id": "1",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100046038.1.12",
          "form": "보고 저렴하게 살수가 있어서 좋은곳 같긴해요",
          "original_form": "보고 저렴하게 살수가 있어서 좋은곳 같긴해요",
          "speaker_id": "2",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100046038.1.13",
          "form": "네 사기도 거의없어서 갠찬아요",
          "original_form": "네 사기도 거의없어서 갠찬아요",
          "speaker_id": "1",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100046038.1.14",
          "form": "참 좋은것 같아요",
          "original_form": "참 좋은것 같아요",
          "speaker_id": "2",
          "time": "20210908 10:16"
        },
        {
          "id": "MDRW2100046038.1.15",
          "form": "요즘은 당근도 무인택배함 이용한대요",
          "original_form": "요즘은 당근도 무인택배함 이용한대요",
          "speaker_id": "1",
          "time": "20210908 10:17"
        }
      ]
