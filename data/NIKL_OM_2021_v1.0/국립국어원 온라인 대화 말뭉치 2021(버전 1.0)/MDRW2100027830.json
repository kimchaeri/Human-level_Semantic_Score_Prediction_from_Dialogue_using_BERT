      "utterance": [
        {
          "id": "MDRW2100027830.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100027830.1.2",
          "form": "보시는 유투브 있으신가요",
          "original_form": "보시는 유투브 있으신가요",
          "speaker_id": "2",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100027830.1.3",
          "form": "자주 보던거는",
          "original_form": "자주 보던거는",
          "speaker_id": "1",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100027830.1.4",
          "form": "좋좋소요 ㅎㅎ",
          "original_form": "좋좋소요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 15:30"
        },
        {
          "id": "MDRW2100027830.1.5",
          "form": "오 그 감독",
          "original_form": "오 그 감독",
          "speaker_id": "2",
          "time": "20210902 15:34"
        },
        {
          "id": "MDRW2100027830.1.6",
          "form": "누군지 아세요?",
          "original_form": "누군지 아세요?",
          "speaker_id": "2",
          "time": "20210902 15:34"
        },
        {
          "id": "MDRW2100027830.1.7",
          "form": "빠니보틀이요?ㅋㅋ",
          "original_form": "빠니보틀이요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100027830.1.8",
          "form": "맞아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100027830.1.9",
          "form": "그거는 안봤는데",
          "original_form": "그거는 안봤는데",
          "speaker_id": "2",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100027830.1.10",
          "form": "여행그건 거의 다봐가요",
          "original_form": "여행그건 거의 다봐가요",
          "speaker_id": "2",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100027830.1.11",
          "form": "너무 재밌어요 ㅠㅠ",
          "original_form": "너무 재밌어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 15:43"
        },
        {
          "id": "MDRW2100027830.1.12",
          "form": "좋좋소 보세요",
          "original_form": "좋좋소 보세요",
          "speaker_id": "1",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100027830.1.13",
          "form": "강추에요",
          "original_form": "강추에요",
          "speaker_id": "1",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100027830.1.14",
          "form": "평이 좋더라구요",
          "original_form": "평이 좋더라구요",
          "speaker_id": "2",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100027830.1.15",
          "form": "빠니보틀 여행은 안보셨을까요?",
          "original_form": "빠니보틀 여행은 안보셨을까요?",
          "speaker_id": "2",
          "time": "20210902 17:56"
        },
        {
          "id": "MDRW2100027830.1.16",
          "form": "봤죠 ㅋㅋ 처음에 그거 보다",
          "original_form": "봤죠 ㅋㅋ 처음에 그거 보다",
          "speaker_id": "1",
          "time": "20210902 17:57"
        },
        {
          "id": "MDRW2100027830.1.17",
          "form": "좋좋소 간거에요",
          "original_form": "좋좋소 간거에요",
          "speaker_id": "1",
          "time": "20210902 17:57"
        },
        {
          "id": "MDRW2100027830.1.18",
          "form": "그렇게 혼자 말도 잘하고 여행잘다니고 부러워요 ㅋㅋ",
          "original_form": "그렇게 혼자 말도 잘하고 여행잘다니고 부러워요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:58"
        },
        {
          "id": "MDRW2100027830.1.19",
          "form": "ㅋㅋㅋㅋㅋ 가면 쉽죠 사실",
          "original_form": "ㅋㅋㅋㅋㅋ 가면 쉽죠 사실",
          "speaker_id": "1",
          "time": "20210902 18:00"
        },
        {
          "id": "MDRW2100027830.1.20",
          "form": "근데 못감ㅋㅋㅋ",
          "original_form": "근데 못감ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:00"
        },
        {
          "id": "MDRW2100027830.1.21",
          "form": "이 코로나",
          "original_form": "이 코로나",
          "speaker_id": "2",
          "time": "20210902 18:03"
        },
        {
          "id": "MDRW2100027830.1.22",
          "form": "저는 혼자 여행 못가요 ㅠㅠ",
          "original_form": "저는 혼자 여행 못가요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 18:03"
        },
        {
          "id": "MDRW2100027830.1.23",
          "form": "부끄럼쟁이",
          "original_form": "부끄럼쟁이",
          "speaker_id": "2",
          "time": "20210902 18:03"
        },
        {
          "id": "MDRW2100027830.1.24",
          "form": "ㅋㅋ말 잘 못거시나요",
          "original_form": "ㅋㅋ말 잘 못거시나요",
          "speaker_id": "1",
          "time": "20210902 18:22"
        },
        {
          "id": "MDRW2100027830.1.25",
          "form": "ㅋㅋㅋ 맞아요 혼밥도 쩜 부끄럽",
          "original_form": "ㅋㅋㅋ 맞아요 혼밥도 쩜 부끄럽",
          "speaker_id": "2",
          "time": "20210902 18:25"
        },
        {
          "id": "MDRW2100027830.1.26",
          "form": "혼영은 잘해요 ㅋㅋㅋㅋ",
          "original_form": "혼영은 잘해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:25"
        },
        {
          "id": "MDRW2100027830.1.27",
          "form": "빠니보틀 같은사람하고 가야한다는 ㅋㅋㅋ",
          "original_form": "빠니보틀 같은사람하고 가야한다는 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:25"
        },
        {
          "id": "MDRW2100027830.1.28",
          "form": "새로 여행 시작하셨데요",
          "original_form": "새로 여행 시작하셨데요",
