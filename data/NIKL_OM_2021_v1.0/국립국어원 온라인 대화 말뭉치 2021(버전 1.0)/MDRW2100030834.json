      "utterance": [
        {
          "id": "MDRW2100030834.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210904 13:12"
        },
        {
          "id": "MDRW2100030834.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100030834.1.3",
          "form": "어떤일을 하시나요?",
          "original_form": "어떤일을 하시나요?",
          "speaker_id": "2",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100030834.1.4",
          "form": "저는 보건쪽 일을 해요",
          "original_form": "저는 보건쪽 일을 해요",
          "speaker_id": "1",
          "time": "20210904 22:00"
        },
        {
          "id": "MDRW2100030834.1.5",
          "form": "오늘 날씨가 좋죠",
          "original_form": "오늘 날씨가 좋죠",
          "speaker_id": "1",
          "time": "20210904 22:00"
        },
        {
          "id": "MDRW2100030834.1.6",
          "form": "네 날씨 너무 좋더라고요 선선하고 바람불고",
          "original_form": "네 날씨 너무 좋더라고요 선선하고 바람불고",
          "speaker_id": "2",
          "time": "20210905 00:20"
        },
        {
          "id": "MDRW2100030834.1.7",
          "form": "보건쪽이면 코로나때문에 많이 바쁘고 힘드시겠어요",
          "original_form": "보건쪽이면 코로나때문에 많이 바쁘고 힘드시겠어요",
          "speaker_id": "2",
          "time": "20210905 00:20"
        },
        {
          "id": "MDRW2100030834.1.8",
          "form": "ㅎㅎ맞아요ㅠ 검사도자주하고자주당하고..",
          "original_form": "ㅎㅎ맞아요ㅠ 검사도자주하고자주당하고..",
          "speaker_id": "1",
          "time": "20210905 10:46"
        },
        {
          "id": "MDRW2100030834.1.9",
          "form": "어떤계열에계세요?",
          "original_form": "어떤계열에계세요?",
          "speaker_id": "1",
          "time": "20210905 10:46"
        },
        {
          "id": "MDRW2100030834.1.10",
          "form": "저는 발전소에서 일해쇼",
          "original_form": "저는 발전소에서 일해쇼",
          "speaker_id": "2",
          "time": "20210905 11:06"
        },
        {
          "id": "MDRW2100030834.1.11",
          "form": "앗 발전소 멋있어요ㅎㅎ",
          "original_form": "앗 발전소 멋있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 12:27"
        },
        {
          "id": "MDRW2100030834.1.12",
          "form": "오늘일안가니까좋네요",
          "original_form": "오늘일안가니까좋네요",
          "speaker_id": "1",
          "time": "20210905 12:27"
        },
        {
          "id": "MDRW2100030834.1.13",
          "form": "일  안하실땐 주로 뭐하세요?",
          "original_form": "일  안하실땐 주로 뭐하세요?",
          "speaker_id": "2",
          "time": "20210905 13:38"
        },
        {
          "id": "MDRW2100030834.1.14",
          "form": "저는지인들만나요!",
          "original_form": "저는지인들만나요!",
          "speaker_id": "1",
          "time": "20210905 20:17"
        },
        {
          "id": "MDRW2100030834.1.15",
          "form": "지인들 만나면 뭐해요?",
          "original_form": "지인들 만나면 뭐해요?",
          "speaker_id": "2",
          "time": "20210905 20:21"
        },
        {
          "id": "MDRW2100030834.1.16",
          "form": "맛집 간다던가 카페 간다던가 하시나요?",
          "original_form": "맛집 간다던가 카페 간다던가 하시나요?",
          "speaker_id": "2",
          "time": "20210905 20:22"
        },
        {
          "id": "MDRW2100030834.1.17",
          "form": "ㅎㅎ둘다가요",
          "original_form": "ㅎㅎ둘다가요",
          "speaker_id": "1",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100030834.1.18",
          "form": "내일 출근해야하는데",
          "original_form": "내일 출근해야하는데",
          "speaker_id": "1",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100030834.1.19",
          "form": "월요병이오려고해요ㅎㅎ",
          "original_form": "월요병이오려고해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100030834.1.20",
          "form": "저도 출근하기 너무 싫으네요",
          "original_form": "저도 출근하기 너무 싫으네요",
          "speaker_id": "2",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100030834.1.21",
          "form": "출근 준비는 다 해놨지만",
          "original_form": "출근 준비는 다 해놨지만",
          "speaker_id": "2",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100030834.1.22",
          "form": "주말은 잘 보내셨어요?",
          "original_form": "주말은 잘 보내셨어요?",
          "speaker_id": "2",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100030834.1.23",
          "form": "흡ㅠㅜ저도..",
          "original_form": "흡ㅠㅜ저도..",
          "speaker_id": "1",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100030834.1.24",
          "form": "주말행복했어요..",
          "original_form": "주말행복했어요..",
          "speaker_id": "1",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100030834.1.25",
          "form": "주말잘쉬셨나요?",
          "original_form": "주말잘쉬셨나요?",
          "speaker_id": "1",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100030834.1.26",
          "form": "네 어제는 와이프랑 경주 황리단길 다녀오고",
          "original_form": "네 어제는 와이프랑 경주 황리단길 다녀오고",
          "speaker_id": "2",
          "time": "20210905 21:17"
        },
        {
          "id": "MDRW2100030834.1.27",
          "form": "오늘은 맛집 다녀왔어요",
          "original_form": "오늘은 맛집 다녀왔어요",
          "speaker_id": "2",
          "time": "20210905 21:17"
        },
        {
          "id": "MDRW2100030834.1.28",
          "form": "어제는 아내랑 경주황리단길 나들이하고",
          "original_form": "어제는 아내랑 경주황리단길 나들이하고",
          "speaker_id": "2",
          "time": "20210905 21:18"
        },
        {
          "id": "MDRW2100030834.1.29",
          "form": "오늘은 맛집가서 외식했어요",
          "original_form": "오늘은 맛집가서 외식했어요",
          "speaker_id": "2",
          "time": "20210905 21:18"
        },
        {
          "id": "MDRW2100030834.1.30",
          "form": "즐거웠습니다",
          "original_form": "즐거웠습니다",
          "speaker_id": "2",
          "time": "20210905 21:18"
        }
      ]
    }
  ]
