      "utterance": [
        {
          "id": "MDRW2100028356.1.1",
          "form": "나는 지금 티비 켜 놓고 보고 있거든요",
          "original_form": "나는 지금 티비 켜 놓고 보고 있거든요",
          "speaker_id": "1",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028356.1.2",
          "form": "그러시군요 재밌나요?",
          "original_form": "그러시군요 재밌나요?",
          "speaker_id": "2",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028356.1.3",
          "form": "그냥 늘 틀어 놓는 거 같아요 딱히 재미있지 않더라도",
          "original_form": "그냥 늘 틀어 놓는 거 같아요 딱히 재미있지 않더라도",
          "speaker_id": "1",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028356.1.4",
          "form": "어머님들이 그러시더라고요",
          "original_form": "어머님들이 그러시더라고요",
          "speaker_id": "2",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028356.1.5",
          "form": "맞아요 나도 내 친구들도 그래요",
          "original_form": "맞아요 나도 내 친구들도 그래요",
          "speaker_id": "1",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100028356.1.6",
          "form": "아침에는 아침마당 이런 거 항상 틀어 놓고",
          "original_form": "아침에는 아침마당 이런 거 항상 틀어 놓고",
          "speaker_id": "2",
          "time": "20210902 19:21"
        },
        {
          "id": "MDRW2100028356.1.7",
          "form": "저녁 준비할 때는 생생정보통",
          "original_form": "저녁 준비할 때는 생생정보통",
          "speaker_id": "2",
          "time": "20210902 19:21"
        },
        {
          "id": "MDRW2100028356.1.8",
          "form": "잘 아네요",
          "original_form": "잘 아네요",
          "speaker_id": "1",
          "time": "20210902 19:21"
        },
        {
          "id": "MDRW2100028356.1.9",
          "form": "항상 맛집 정보 나오잖아요",
          "original_form": "항상 맛집 정보 나오잖아요",
          "speaker_id": "2",
          "time": "20210902 19:22"
        },
        {
          "id": "MDRW2100028356.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210902 19:22"
        },
        {
          "id": "MDRW2100028356.1.11",
          "form": "근데 그게 과장 광고가 많다고 그러더라고요",
          "original_form": "근데 그게 과장 광고가 많다고 그러더라고요",
          "speaker_id": "2",
          "time": "20210902 19:22"
        },
        {
          "id": "MDRW2100028356.1.12",
          "form": "암튼 지금 보고 있는 일일드라마",
          "original_form": "암튼 지금 보고 있는 일일드라마",
          "speaker_id": "1",
          "time": "20210902 19:22"
        },
        {
          "id": "MDRW2100028356.1.13",
          "form": "참 웃겨요 저렇게 쓰려면 나도 쓰겠어",
          "original_form": "참 웃겨요 저렇게 쓰려면 나도 쓰겠어",
          "speaker_id": "1",
          "time": "20210902 19:22"
        },
        {
          "id": "MDRW2100028356.1.14",
          "form": "배신하고 복수하고",
          "original_form": "배신하고 복수하고",
          "speaker_id": "2",
          "time": "20210902 19:23"
        },
        {
          "id": "MDRW2100028356.1.15",
          "form": "출생의 비밀 있고",
          "original_form": "출생의 비밀 있고",
