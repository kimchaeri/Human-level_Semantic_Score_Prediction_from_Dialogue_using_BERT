      "utterance": [
        {
          "id": "MDRW2100002883.1.1",
          "form": "스포츠도 좋아하시나요",
          "original_form": "스포츠도 좋아하시나요",
          "speaker_id": "1",
          "time": "20210820 16:14"
        },
        {
          "id": "MDRW2100002883.1.2",
          "form": "네 야구 좋아해요 ㅎㅎ",
          "original_form": "네 야구 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 16:30"
        },
        {
          "id": "MDRW2100002883.1.3",
          "form": "으아 이번 올림픽 야구ㅠㅠ",
          "original_form": "으아 이번 올림픽 야구ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 16:31"
        },
        {
          "id": "MDRW2100002883.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ전 못할거같아서 하나도 안봤아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ전 못할거같아서 하나도 안봤아요",
          "speaker_id": "2",
          "time": "20210820 16:36"
        },
        {
          "id": "MDRW2100002883.1.5",
          "form": "ㅋㅋㅋㅋㅋ 느낌이 오셨군요",
          "original_form": "ㅋㅋㅋㅋㅋ 느낌이 오셨군요",
          "speaker_id": "1",
          "time": "20210820 17:37"
        },
        {
          "id": "MDRW2100002883.1.6",
          "form": "네 ㅋㅋ   메이저리그 보는거 좋아해요",
          "original_form": "네 ㅋㅋ   메이저리그 보는거 좋아해요",
          "speaker_id": "2",
          "time": "20210820 17:42"
        },
        {
          "id": "MDRW2100002883.1.7",
          "form": "좋아하는.운동 있어요?",
          "original_form": "좋아하는.운동 있어요?",
          "speaker_id": "2",
          "time": "20210820 17:43"
        },
        {
          "id": "MDRW2100002883.1.8",
          "form": "저도 메이저리그",
          "original_form": "저도 메이저리그",
          "speaker_id": "1",
          "time": "20210820 17:43"
        },
        {
          "id": "MDRW2100002883.1.9",
          "form": "오타니ㅎㅎ",
          "original_form": "오타니ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 17:43"
        },
        {
          "id": "MDRW2100002883.1.10",
          "form": "오  저는 보스턴 팬이에요 ㅎㅎ",
          "original_form": "오  저는 보스턴 팬이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 18:16"
        },
        {
          "id": "MDRW2100002883.1.11",
          "form": "와 뭔가 오랜 팬인듯해요",
          "original_form": "와 뭔가 오랜 팬인듯해요",
          "speaker_id": "1",
          "time": "20210820 18:19"
        },
        {
          "id": "MDRW2100002883.1.12",
          "form": "네 오래됐어요 2001년 즈음부터 좋아했어요",
          "original_form": "네 오래됐어요 2001년 즈음부터 좋아했어요",
          "speaker_id": "2",
          "time": "20210820 18:29"
        },
        {
          "id": "MDRW2100002883.1.13",
          "form": "어느팀 팬이에요?",
          "original_form": "어느팀 팬이에요?",
          "speaker_id": "2",
          "time": "20210820 18:29"
        },
        {
          "id": "MDRW2100002883.1.14",
          "form": "저는 그냥 한국인 응원해요ㅎㅎ 사실 메이저리그 팬도 아닌셈이죠",
          "original_form": "저는 그냥 한국인 응원해요ㅎㅎ 사실 메이저리그 팬도 아닌셈이죠",
          "speaker_id": "1",
          "time": "20210820 18:40"
        },
        {
          "id": "MDRW2100002883.1.15",
          "form": "근데 la그 타자 멋있었는데 괜히 세레모니로 부상당해서ㅠ",
          "original_form": "근데 la그 타자 멋있었는데 괜히 세레모니로 부상당해서ㅠ",
          "speaker_id": "1",
          "time": "20210820 18:40"
        },
        {
          "id": "MDRW2100002883.1.16",
          "form": "아 ㅎㅎ",
          "original_form": "아 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 20:18"
        },
        {
          "id": "MDRW2100002883.1.17",
          "form": "그래도 걔들은 그것도 경기에 일부분이라고 생각해서",
          "original_form": "그래도 걔들은 그것도 경기에 일부분이라고 생각해서",
          "speaker_id": "2",
          "time": "20210820 20:18"
        },
        {
          "id": "MDRW2100002883.1.18",
          "form": "많이들 하더라고요",
          "original_form": "많이들 하더라고요",
          "speaker_id": "2",
          "time": "20210820 20:18"
        },
        {
          "id": "MDRW2100002883.1.19",
          "form": "보스턴이 저주를 깼을때 기쁘셨겠어요",
          "original_form": "보스턴이 저주를 깼을때 기쁘셨겠어요",
          "speaker_id": "1",
          "time": "20210820 20:37"
        },
        {
          "id": "MDRW2100002883.1.20",
          "form": "네 엄청 좋았어요 ㅎㅎ",
          "original_form": "네 엄청 좋았어요 ㅎㅎ",
          "speaker_id": "2",
