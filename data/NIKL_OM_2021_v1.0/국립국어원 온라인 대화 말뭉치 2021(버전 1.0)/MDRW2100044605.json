      "utterance": [
        {
          "id": "MDRW2100044605.1.1",
          "form": "내일 약속 잊지 않았겠지",
          "original_form": "내일 약속 잊지 않았겠지",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044605.1.2",
          "form": "ㅇㅇ오늘로바꾸고싶었는데 참았다ㅋㅋ",
          "original_form": "ㅇㅇ오늘로바꾸고싶었는데 참았다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044605.1.3",
          "form": "빨리먹고싶어서ㅋㅋ",
          "original_form": "빨리먹고싶어서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044605.1.4",
          "form": "내일",
          "original_form": "내일",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044605.1.5",
          "form": "시켜놓고 있어 ㅋㅋㅋ",
          "original_form": "시켜놓고 있어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044605.1.6",
          "form": "헐 메뉴 텔레로 보내줘",
          "original_form": "헐 메뉴 텔레로 보내줘",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044605.1.7",
          "form": "피자는 와서 정할꺼?",
          "original_form": "피자는 와서 정할꺼?",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044605.1.8",
          "form": "메뉴 나는 해산믈 올리브",
          "original_form": "메뉴 나는 해산믈 올리브",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044605.1.9",
          "form": "피자 3종류밖에없던데",
          "original_form": "피자 3종류밖에없던데",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044605.1.10",
          "form": "고르곤졸라 페퍼로니 샐러드",
          "original_form": "고르곤졸라 페퍼로니 샐러드",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044605.1.11",
          "form": "뭐가마음에드니",
          "original_form": "뭐가마음에드니",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044605.1.12",
          "form": "글쎄......딱땡기는건 없네 너는???ㅋㅋ",
          "original_form": "글쎄......딱땡기는건 없네 너는???ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044605.1.13",
          "form": "난 해산물크림파스타",
          "original_form": "난 해산물크림파스타",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044605.1.14",
          "form": "그럼 파스타만먹어?",
          "original_form": "그럼 파스타만먹어?",
          "speaker_id": "1",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044605.1.15",
          "form": "아니피자도먹어보자ㅋㅋ얇은거니까ㅋㅋ",
          "original_form": "아니피자도먹어보자ㅋㅋ얇은거니까ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044605.1.16",
          "form": "너다니면서 먹고싶었던 피자없어??",
          "original_form": "너다니면서 먹고싶었던 피자없어??",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044605.1.17",
          "form": "ㅋㅋㅋ딱히 생각안해봤어 ㅋㅋ",
          "original_form": "ㅋㅋㅋ딱히 생각안해봤어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044605.1.18",
          "form": "샐러드피자어때???ㅋㅋ",
          "original_form": "샐러드피자어때???ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044605.1.19",
          "form": "페퍼로니아님 그게 좋겠다고 생각했어 ㅋㅋ",
          "original_form": "페퍼로니아님 그게 좋겠다고 생각했어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044605.1.20",
          "form": "그럼 그걸로하잨ㄱㅋㅋㄱ 샐러드피자안먹은지 십년된듯ㅋㅋ",
          "original_form": "그럼 그걸로하잨ㄱㅋㅋㄱ 샐러드피자안먹은지 십년된듯ㅋㅋ",
