      "utterance": [
        {
          "id": "MDRW2100040030.1.1",
          "form": "반려동물 키워 보신 적 있어요?",
          "original_form": "반려동물 키워 보신 적 있어요?",
          "speaker_id": "1",
          "time": "20210904 00:55"
        },
        {
          "id": "MDRW2100040030.1.2",
          "form": "네지금도 강아지 키워욪ㅎ",
          "original_form": "네지금도 강아지 키워욪ㅎ",
          "speaker_id": "2",
          "time": "20210904 00:57"
        },
        {
          "id": "MDRW2100040030.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210904 00:58"
        },
        {
          "id": "MDRW2100040030.1.4",
          "form": "강아지의는 안 키워 봣고 예전에 어릴 때 청거북이 켜 봤어요",
          "original_form": "강아지의는 안 키워 봣고 예전에 어릴 때 청거북이 켜 봤어요",
          "speaker_id": "1",
          "time": "20210904 00:58"
        },
        {
          "id": "MDRW2100040030.1.5",
          "form": "청거북이 새롭네요",
          "original_form": "청거북이 새롭네요",
          "speaker_id": "2",
          "time": "20210904 00:59"
        },
        {
          "id": "MDRW2100040030.1.6",
          "form": "크기가 어떤가요?",
          "original_form": "크기가 어떤가요?",
          "speaker_id": "2",
          "time": "20210904 00:59"
        },
        {
          "id": "MDRW2100040030.1.7",
          "form": "아기들 손만 했어요",
          "original_form": "아기들 손만 했어요",
          "speaker_id": "1",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100040030.1.8",
          "form": "되게 귀여웠어요",
          "original_form": "되게 귀여웠어요",
          "speaker_id": "1",
          "time": "20210904 01:00"
        },
        {
          "id": "MDRW2100040030.1.9",
          "form": "아이고 검색하니 넘 귀엽ㅜㅜ",
          "original_form": "아이고 검색하니 넘 귀엽ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 01:05"
        },
        {
          "id": "MDRW2100040030.1.10",
          "form": "내 엄청 귀여웠어요 그런데 거기서 밥 많이 주면 또 엄청나게 크더라고요",
          "original_form": "내 엄청 귀여웠어요 그런데 거기서 밥 많이 주면 또 엄청나게 크더라고요",
          "speaker_id": "1",
          "time": "20210904 01:06"
        },
        {
          "id": "MDRW2100040030.1.11",
          "form": "아기들 손 사이즈가 성체인가요? 아님 그보다 더?",
          "original_form": "아기들 손 사이즈가 성체인가요? 아님 그보다 더?",
          "speaker_id": "2",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100040030.1.12",
          "form": "성채는 아니고",
          "original_form": "성채는 아니고",
          "speaker_id": "1",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100040030.1.13",
          "form": "아기 거북이 였어요",
          "original_form": "아기 거북이 였어요",
          "speaker_id": "1",
          "time": "20210904 01:08"
        },
        {
          "id": "MDRW2100040030.1.14",
          "form": "이름이 뭐였나요?",
          "original_form": "이름이 뭐였나요?",
          "speaker_id": "2",
          "time": "20210904 01:09"
        },
        {
          "id": "MDRW2100040030.1.15",
          "form": "이름 없이 그냥 거북이었어요",
          "original_form": "이름 없이 그냥 거북이었어요",
          "speaker_id": "1",
          "time": "20210904 01:10"
        },
        {
          "id": "MDRW2100040030.1.16",
          "form": "아ㅋㅋ 그냥 거북아 불렀군요ㅎ",
          "original_form": "아ㅋㅋ 그냥 거북아 불렀군요ㅎ",
          "speaker_id": "2",
          "time": "20210904 01:11"
        },
        {
          "id": "MDRW2100040030.1.17",
          "form": "거북이는 뭐 먹고 사나요",
          "original_form": "거북이는 뭐 먹고 사나요",
          "speaker_id": "2",
          "time": "20210904 01:11"
        },
        {
          "id": "MDRW2100040030.1.18",
          "form": "사료랑 돼지고기가 섞어서 줬어요",
          "original_form": "사료랑 돼지고기가 섞어서 줬어요",
          "speaker_id": "1",
          "time": "20210904 01:12"
        },
        {
          "id": "MDRW2100040030.1.19",
          "form": "주로 사료요",
          "original_form": "주로 사료요",
          "speaker_id": "1",
          "time": "20210904 01:12"
        }
      ]
    }
