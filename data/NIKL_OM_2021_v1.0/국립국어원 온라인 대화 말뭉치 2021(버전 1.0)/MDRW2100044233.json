        {
          "id": "MDRW2100044233.1.1",
          "form": "카지노 있는 나라 가셨어요?",
          "original_form": "카지노 있는 나라 가셨어요?",
          "speaker_id": "2",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100044233.1.2",
          "form": "호",
          "original_form": "호",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044233.1.3",
          "form": "텔",
          "original_form": "텔",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044233.1.4",
          "form": "에 카지노가 있기도 하던데요",
          "original_form": "에 카지노가 있기도 하던데요",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044233.1.5",
          "form": "아 맞아요 ㅋㅋㅋㅋ",
          "original_form": "아 맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044233.1.6",
          "form": "안들어가봤어요",
          "original_form": "안들어가봤어요",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044233.1.7",
          "form": "국내요??",
          "original_form": "국내요??",
          "speaker_id": "2",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044233.1.8",
          "form": "네~",
          "original_form": "네~",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044233.1.9",
          "form": "지난번에 어디 갔는데 카지노가 있더라고요",
          "original_form": "지난번에 어디 갔는데 카지노가 있더라고요",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044233.1.10",
          "form": "그런건 보통 외국인 전용이지 않나요??",
          "original_form": "그런건 보통 외국인 전용이지 않나요??",
          "speaker_id": "2",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044233.1.11",
          "form": "일단 애들이 있어서 못 들어가기도 했고요",
          "original_form": "일단 애들이 있어서 못 들어가기도 했고요",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044233.1.12",
          "form": "아 가족여행이면 그렇겠네요",
          "original_form": "아 가족여행이면 그렇겠네요",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044233.1.13",
          "form": "마카오도 가고 싶으시겠오요",
          "original_form": "마카오도 가고 싶으시겠오요",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044233.1.14",
          "form": "네 마카오도 코로나 나아지면 가고싶은 곳 중 하나예요",
          "original_form": "네 마카오도 코로나 나아지면 가고싶은 곳 중 하나예요",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044233.1.15",
          "form": "가보셨나요?",
          "original_form": "가보셨나요?",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044233.1.16",
          "form": "아뇨 생각도 못했어요",
          "original_form": "아뇨 생각도 못했어요",
          "speaker_id": "1",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044233.1.17",
          "form": "보통 홍콩이랑 묶어서 가더라구요",
          "original_form": "보통 홍콩이랑 묶어서 가더라구요",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044233.1.18",
          "form": "저 홍콩도 가려다 못갔는데요",
          "original_form": "저 홍콩도 가려다 못갔는데요",
          "speaker_id": "1",
          "time": "20210907 22:07"
        }
      ]
