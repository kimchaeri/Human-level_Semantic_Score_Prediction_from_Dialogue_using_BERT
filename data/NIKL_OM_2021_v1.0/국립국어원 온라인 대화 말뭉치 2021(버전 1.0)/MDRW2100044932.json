        },
        {
          "id": "MDRW2100044932.1.2",
          "form": "독재가 저지경이니",
          "original_form": "독재가 저지경이니",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044932.1.3",
          "form": "언제 한번 큰거 올 듯",
          "original_form": "언제 한번 큰거 올 듯",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044932.1.4",
          "form": "그",
          "original_form": "그",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044932.1.5",
          "form": "제가본영상에서 말하길",
          "original_form": "제가본영상에서 말하길",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044932.1.6",
          "form": "우리나라 약간 호남영남 싸우듯",
          "original_form": "우리나라 약간 호남영남 싸우듯",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044932.1.7",
          "form": "정치가 상하이파 북경파 나눠있는데",
          "original_form": "정치가 상하이파 북경파 나눠있는데",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044932.1.8",
          "form": "상하이파가 돈줄이고",
          "original_form": "상하이파가 돈줄이고",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044932.1.9",
          "form": "ㅇㅇㅇ 저 그거 짤로 본거같은데",
          "original_form": "ㅇㅇㅇ 저 그거 짤로 본거같은데",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044932.1.10",
          "form": "적통정치계열이 북경파라고하더라고",
          "original_form": "적통정치계열이 북경파라고하더라고",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044932.1.11",
          "form": "충칭쪽 있고",
          "original_form": "충칭쪽 있고",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044932.1.12",
          "form": "홍콩쪽 있고",
          "original_form": "홍콩쪽 있고",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044932.1.13",
          "form": "그거 아닌가여",
          "original_form": "그거 아닌가여",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044932.1.14",
          "form": "근데 지금 홍코오진이유가",
          "original_form": "근데 지금 홍코오진이유가",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044932.1.15",
          "form": "상해쪽 있고",
          "original_form": "상해쪽 있고",
          "speaker_id": "2",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044932.1.16",
          "form": "네네 그거맞을거에여",
          "original_form": "네네 그거맞을거에여",
          "speaker_id": "1",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044932.1.17",
          "form": "홍콩 조진이유가",
          "original_form": "홍콩 조진이유가",
          "speaker_id": "1",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044932.1.18",
          "form": "광둥세력 결집 막는다고",
          "original_form": "광둥세력 결집 막는다고",
          "speaker_id": "1",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044932.1.19",
          "form": "충칭이랑",
          "original_form": "충칭이랑",
          "speaker_id": "1",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044932.1.20",
          "form": "홍콩 찍어눌러야",
          "original_form": "홍콩 찍어눌러야",
          "speaker_id": "2",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044932.1.21",
          "form": "근데 ㅋㅋㅋㅋㅋㅋ아이렇게가다보면",
          "original_form": "근데 ㅋㅋㅋㅋㅋㅋ아이렇게가다보면",
          "speaker_id": "1",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044932.1.22",
          "form": "홍콩이랑 가까운 광둥쪽",
          "original_form": "홍콩이랑 가까운 광둥쪽",
          "speaker_id": "2",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044932.1.23",
          "form": "핑핑이 금방 무너지지 않을까",
          "original_form": "핑핑이 금방 무너지지 않을까",
          "speaker_id": "1",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044932.1.24",
          "form": "뒤집히는거 막는다고",
          "original_form": "뒤집히는거 막는다고",
          "speaker_id": "2",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044932.1.25",
          "form": "그러던데",
          "original_form": "그러던데",
          "speaker_id": "2",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044932.1.26",
          "form": "어우",
          "original_form": "어우",
          "speaker_id": "2",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044932.1.27",
          "form": "또 서버 이상한거",
          "original_form": "또 서버 이상한거",
          "speaker_id": "2",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044932.1.28",
          "form": "가",
          "original_form": "가",
          "speaker_id": "2",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044932.1.29",
          "form": "지금 독재도",
          "original_form": "지금 독재도",
          "speaker_id": "1",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044932.1.30",
          "form": "너무심각한 독재라",
          "original_form": "너무심각한 독재라",
          "speaker_id": "1",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100044932.1.31",
          "form": "금방 무너졌으면...",
          "original_form": "금방 무너졌으면...",
          "speaker_id": "2",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044932.1.32",
          "form": "아마 상해파랑 다른파들도",
          "original_form": "아마 상해파랑 다른파들도",
          "speaker_id": "1",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044932.1.33",
          "form": "가만히 있지 못할것...",
          "original_form": "가만히 있지 못할것...",
          "speaker_id": "1",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044932.1.34",
          "form": "하지만 저는 시진핑핑이가",
          "original_form": "하지만 저는 시진핑핑이가",
          "speaker_id": "1",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044932.1.35",
          "form": "일단 둘기회수는 좀 하고...",
          "original_form": "일단 둘기회수는 좀 하고...",
          "speaker_id": "1",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044932.1.36",
          "form": "무너졌으면...",
          "original_form": "무너졌으면...",
          "speaker_id": "1",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044932.1.37",
          "form": "머지",
          "original_form": "머지",
          "speaker_id": "1",
