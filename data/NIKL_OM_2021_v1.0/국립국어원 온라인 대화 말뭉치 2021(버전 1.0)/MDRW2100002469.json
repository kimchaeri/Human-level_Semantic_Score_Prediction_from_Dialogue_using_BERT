        {
          "id": "MDRW2100002469.1.1",
          "form": "민트초코 좋아하시나봐요",
          "original_form": "민트초코 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002469.1.2",
          "form": "스벅 민트초코 마셔보셨어요?",
          "original_form": "스벅 민트초코 마셔보셨어요?",
          "speaker_id": "1",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002469.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002469.1.4",
          "form": "스벅껀",
          "original_form": "스벅껀",
          "speaker_id": "2",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002469.1.5",
          "form": "아직 못마셔봤네요",
          "original_form": "아직 못마셔봤네요",
          "speaker_id": "2",
          "time": "20210820 12:41"
        },
        {
          "id": "MDRW2100002469.1.6",
          "form": "저두여ㅋㅋㅋ",
          "original_form": "저두여ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002469.1.7",
          "form": "기회되면",
          "original_form": "기회되면",
          "speaker_id": "2",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002469.1.8",
          "form": "마셔보고싶어요",
          "original_form": "마셔보고싶어요",
          "speaker_id": "2",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002469.1.9",
          "form": "맛있다던데",
          "original_form": "맛있다던데",
          "speaker_id": "2",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002469.1.10",
          "form": "저는 저번에던킨거마셔봤는데",
          "original_form": "저는 저번에던킨거마셔봤는데",
          "speaker_id": "1",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002469.1.11",
          "form": "맛있더라구용",
          "original_form": "맛있더라구용",
          "speaker_id": "1",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002469.1.12",
          "form": "오 저도 던킨꺼",
          "original_form": "오 저도 던킨꺼",
          "speaker_id": "2",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002469.1.13",
          "form": "마셔봤어요",
          "original_form": "마셔봤어요",
          "speaker_id": "2",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002469.1.14",
          "form": "기라델리어쩌구",
          "original_form": "기라델리어쩌구",
          "speaker_id": "2",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002469.1.15",
          "form": "맛있더라구요",
          "original_form": "맛있더라구요",
          "speaker_id": "2",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002469.1.16",
          "form": "앗 저두요 같은거 마셨네요",
          "original_form": "앗 저두요 같은거 마셨네요",
          "speaker_id": "1",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002469.1.17",
          "form": "ㅋㅋ 행사해서",
          "original_form": "ㅋㅋ 행사해서",
          "speaker_id": "2",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002469.1.18",
          "form": "별로 기대안했는데",
          "original_form": "별로 기대안했는데",
          "speaker_id": "1",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002469.1.19",
          "form": "함 먹어봤어요",
          "original_form": "함 먹어봤어요",
          "speaker_id": "2",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002469.1.20",
          "form": "생각보다 맛있어서",
          "original_form": "생각보다 맛있어서",
          "speaker_id": "1",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002469.1.21",
          "form": "만족헀어요ㅎㅎ",
          "original_form": "만족헀어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002469.1.22",
          "form": "그쵸 저도",
          "original_form": "그쵸 저도",
          "speaker_id": "2",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002469.1.23",
          "form": "기대안했는데",
          "original_form": "기대안했는데",
          "speaker_id": "2",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002469.1.24",
          "form": "맛있더라구요",
          "original_form": "맛있더라구요",
          "speaker_id": "2",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002469.1.25",
          "form": "스벅꺼도 사라지기전에",
