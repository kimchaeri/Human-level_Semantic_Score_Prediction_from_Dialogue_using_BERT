      "utterance": [
        {
          "id": "MDRW2100046077.1.1",
          "form": "아쉽게도 오이는 안들어갔던거같아요",
          "original_form": "아쉽게도 오이는 안들어갔던거같아요",
          "speaker_id": "1",
          "time": "20210908 10:07"
        },
        {
          "id": "MDRW2100046077.1.2",
          "form": "오이 정말 좋아하는데",
          "original_form": "오이 정말 좋아하는데",
          "speaker_id": "1",
          "time": "20210908 10:07"
        },
        {
          "id": "MDRW2100046077.1.3",
          "form": "앗.. 초록색에 무언가..",
          "original_form": "앗.. 초록색에 무언가..",
          "speaker_id": "2",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100046077.1.4",
          "form": "맛이 달달한가요?",
          "original_form": "맛이 달달한가요?",
          "speaker_id": "2",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100046077.1.5",
          "form": "아니요~!",
          "original_form": "아니요~!",
          "speaker_id": "1",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100046077.1.6",
          "form": "이 럴 수 가",
          "original_form": "이 럴 수 가",
          "speaker_id": "2",
          "time": "20210908 10:10"
        },
        {
          "id": "MDRW2100046077.1.7",
          "form": "아 진짜",
          "original_form": "아 진짜",
          "speaker_id": "1",
          "time": "20210908 10:10"
        },
        {
          "id": "MDRW2100046077.1.8",
          "form": "간단한데..",
          "original_form": "간단한데..",
          "speaker_id": "1",
          "time": "20210908 10:10"
        },
        {
          "id": "MDRW2100046077.1.9",
          "form": "초록색에..안 달고",
          "original_form": "초록색에..안 달고",
          "speaker_id": "2",
          "time": "20210908 10:10"
        },
        {
          "id": "MDRW2100046077.1.10",
          "form": "단호박 드세요?",
          "original_form": "단호박 드세요?",
          "speaker_id": "2",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100046077.1.11",
          "form": "아니다 단호박은 달잖어",
          "original_form": "아니다 단호박은 달잖어",
          "speaker_id": "2",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100046077.1.12",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100046077.1.13",
          "form": "딱 초록색 하나로 한 끼를 때우나요?",
          "original_form": "딱 초록색 하나로 한 끼를 때우나요?",
          "speaker_id": "2",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100046077.1.14",
          "form": "ㅋㅋㄱㅋㅋㄱㄱㅋㄱㅋㄱㅋㄱㅋㄴㅔ에..",
          "original_form": "ㅋㅋㄱㅋㅋㄱㄱㅋㄱㅋㄱㅋㄱㅋㄴㅔ에..",
          "speaker_id": "1",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100046077.1.15",
          "form": "고기도 들어가긴해요 메인은아니지ㅣ마느...",
          "original_form": "고기도 들어가긴해요 메인은아니지ㅣ마느...",
          "speaker_id": "1",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100046077.1.16",
          "form": "소스가 들어가나요?",
          "original_form": "소스가 들어가나요?",
          "speaker_id": "2",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100046077.1.17",
          "form": "들어가기두, 안들어가기두 해요",
          "original_form": "들어가기두, 안들어가기두 해요",
          "speaker_id": "1",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100046077.1.18",
          "form": "소스는 선택이군요!",
          "original_form": "소스는 선택이군요!",
          "speaker_id": "2",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100046077.1.19",
          "form": "혹시 혹시",
          "original_form": "혹시 혹시",
          "speaker_id": "2",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100046077.1.20",
          "form": "잎채소인가요?",
          "original_form": "잎채소인가요?",
          "speaker_id": "2",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100046077.1.21",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "1",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100046077.1.22",
          "form": "아! 정답",
          "original_form": "아! 정답",
          "speaker_id": "2",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100046077.1.23",
          "form": "샐러드!!!!",
          "original_form": "샐러드!!!!",
          "speaker_id": "2",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100046077.1.24",
          "form": "정답이에요!!",
          "original_form": "정답이에요!!",
          "speaker_id": "1",
          "time": "20210908 10:15"
        },
        {
          "id": "MDRW2100046077.1.25",
          "form": "정말 간단했져?",
          "original_form": "정말 간단했져?",
          "speaker_id": "1",
          "time": "20210908 10:15"
        },
        {
          "id": "MDRW2100046077.1.26",
          "form": "야호~~~ 맞췄다",
          "original_form": "야호~~~ 맞췄다",
          "speaker_id": "2",
          "time": "20210908 10:16"
        },
        {
          "id": "MDRW2100046077.1.27",
          "form": "축하드려여",
          "original_form": "축하드려여",
          "speaker_id": "1",
          "time": "20210908 10:16"
        },
        {
          "id": "MDRW2100046077.1.28",
          "form": "돗자리 새로 안사셔도 될 것 같아요",
          "original_form": "돗자리 새로 안사셔도 될 것 같아요",
          "speaker_id": "1",
          "time": "20210908 10:16"
        },
        {
          "id": "MDRW2100046077.1.29",
          "form": "ㅋㅋㅋ고마버요 고마버요",
          "original_form": "ㅋㅋㅋ고마버요 고마버요",
          "speaker_id": "2",
          "time": "20210908 10:16"
        },
        {
          "id": "MDRW2100046077.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ돗자리 사수 성공",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ돗자리 사수 성공",
          "speaker_id": "2",
