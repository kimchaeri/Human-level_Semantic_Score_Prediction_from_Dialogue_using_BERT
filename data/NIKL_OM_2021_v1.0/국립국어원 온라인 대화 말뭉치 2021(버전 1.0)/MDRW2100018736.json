        },
        {
          "id": "MDRW2100018736.1.2",
          "form": "분명 어제까지는 자료 찾아보고 재밌었는데",
          "original_form": "분명 어제까지는 자료 찾아보고 재밌었는데",
          "speaker_id": "2",
          "time": "20210827 11:36"
        },
        {
          "id": "MDRW2100018736.1.3",
          "form": "오늘은 아니야",
          "original_form": "오늘은 아니야",
          "speaker_id": "2",
          "time": "20210827 11:36"
        },
        {
          "id": "MDRW2100018736.1.4",
          "form": "오늘은 망했어",
          "original_form": "오늘은 망했어",
          "speaker_id": "2",
          "time": "20210827 11:36"
        },
        {
          "id": "MDRW2100018736.1.5",
          "form": "? 뭐가?",
          "original_form": "? 뭐가?",
          "speaker_id": "1",
          "time": "20210827 11:37"
        },
        {
          "id": "MDRW2100018736.1.6",
          "form": "오늘수강신청아님?",
          "original_form": "오늘수강신청아님?",
          "speaker_id": "1",
          "time": "20210827 11:37"
        },
        {
          "id": "MDRW2100018736.1.7",
          "form": "어",
          "original_form": "어",
          "speaker_id": "2",
          "time": "20210827 11:37"
        },
        {
          "id": "MDRW2100018736.1.8",
          "form": "망했어",
          "original_form": "망했어",
          "speaker_id": "2",
          "time": "20210827 11:37"
        },
        {
          "id": "MDRW2100018736.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:37"
        },
        {
          "id": "MDRW2100018736.1.10",
          "form": "널마나?",
          "original_form": "널마나?",
          "speaker_id": "1",
          "time": "20210827 11:37"
        },
        {
          "id": "MDRW2100018736.1.11",
          "form": "잡긴잡았음?",
          "original_form": "잡긴잡았음?",
          "speaker_id": "1",
          "time": "20210827 11:37"
        },
        {
          "id": "MDRW2100018736.1.12",
          "form": "한번 더 휴학하고 싶을 정도로",
          "original_form": "한번 더 휴학하고 싶을 정도로",
          "speaker_id": "2",
          "time": "20210827 11:37"
        },
        {
          "id": "MDRW2100018736.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ필요한건잡았어,",
          "original_form": "ㅋㅋㅋㅋㅋㅋ필요한건잡았어,",
          "speaker_id": "1",
          "time": "20210827 11:37"
        },
        {
          "id": "MDRW2100018736.1.14",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210827 11:37"
        },
        {
          "id": "MDRW2100018736.1.15",
          "form": "다른 걸로 잡긴 잡았는데 하나가 안 잡힘",
          "original_form": "다른 걸로 잡긴 잡았는데 하나가 안 잡힘",
          "speaker_id": "2",
          "time": "20210827 11:38"
        },
        {
          "id": "MDRW2100018736.1.16",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210827 11:38"
        },
        {
          "id": "MDRW2100018736.1.17",
          "form": "전공을 5개 놓친게 말이 되냐",
          "original_form": "전공을 5개 놓친게 말이 되냐",
          "speaker_id": "2",
          "time": "20210827 11:38"
        },
        {
          "id": "MDRW2100018736.1.18",
          "form": "??그정도면",
          "original_form": "??그정도면",
          "speaker_id": "1",
          "time": "20210827 11:38"
        },
        {
          "id": "MDRW2100018736.1.19",
          "form": "하나도못잡은거아님?",
          "original_form": "하나도못잡은거아님?",
          "speaker_id": "1",
          "time": "20210827 11:38"
        },
        {
          "id": "MDRW2100018736.1.20",
          "form": "수강신청을어떻게했길래",
          "original_form": "수강신청을어떻게했길래",
          "speaker_id": "1",
          "time": "20210827 11:38"
        },
        {
          "id": "MDRW2100018736.1.21",
          "form": "아니다 교양 하나 전공 4갸 놓침",
          "original_form": "아니다 교양 하나 전공 4갸 놓침",
          "speaker_id": "2",
          "time": "20210827 11:39"
        },
        {
          "id": "MDRW2100018736.1.22",
          "form": "그니까 교필에서 시간을 너무 많이 잡아먹어서",
          "original_form": "그니까 교필에서 시간을 너무 많이 잡아먹어서",
          "speaker_id": "2",
          "time": "20210827 11:39"
        },
        {
          "id": "MDRW2100018736.1.23",
          "form": "망함",
          "original_form": "망함",
          "speaker_id": "2",
          "time": "20210827 11:39"
        },
        {
          "id": "MDRW2100018736.1.24",
          "form": "그러면지그몇학점잡았는데",
          "original_form": "그러면지그몇학점잡았는데",
          "speaker_id": "1",
          "time": "20210827 11:39"
        },
        {
          "id": "MDRW2100018736.1.25",
          "form": "지금 18학점",
          "original_form": "지금 18학점",
          "speaker_id": "2",
          "time": "20210827 11:40"
        },
        {
          "id": "MDRW2100018736.1.26",
          "form": "하나만 더 잡ㅇ면 됨",
          "original_form": "하나만 더 잡ㅇ면 됨",
          "speaker_id": "2",
          "time": "20210827 11:40"
        },
        {
          "id": "MDRW2100018736.1.27",
          "form": "20학점이 최대임",
          "original_form": "20학점이 최대임",
          "speaker_id": "2",
          "time": "20210827 11:40"
        },
        {
          "id": "MDRW2100018736.1.28",
          "form": "아아 근데 전공은 인원 어느정도 맞게 나오지 않음?",
          "original_form": "아아 근데 전공은 인원 어느정도 맞게 나오지 않음?",
          "speaker_id": "1",
          "time": "20210827 11:40"
        },
        {
          "id": "MDRW2100018736.1.29",
          "form": "어 근데 1교시느 못하겠어",
          "original_form": "어 근데 1교시느 못하겠어",
          "speaker_id": "2",
          "time": "20210827 11:41"
        },
        {
          "id": "MDRW2100018736.1.30",
          "form": "1교시 밖에 안남은게 말이되냐",
          "original_form": "1교시 밖에 안남은게 말이되냐",
          "speaker_id": "2",
          "time": "20210827 11:41"
        },
        {
          "id": "MDRW2100018736.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ아",
          "speaker_id": "1",
          "time": "20210827 11:41"
        },
        {
          "id": "MDRW2100018736.1.32",
          "form": "나 지금 시간표 진심 뒤죽박죽임",
          "original_form": "나 지금 시간표 진심 뒤죽박죽임",
          "speaker_id": "2",
          "time": "20210827 11:41"
        },
        {
          "id": "MDRW2100018736.1.33",
          "form": "대충어떤데",
          "original_form": "대충어떤데",
          "speaker_id": "1",
          "time": "20210827 11:42"
        }
      ]
