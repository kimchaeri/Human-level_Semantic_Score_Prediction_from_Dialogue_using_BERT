      "utterance": [
        {
          "id": "MDRW2100003446.1.1",
          "form": "나도 그래. 털갈이 하는 것도 아닌데 숭숭",
          "original_form": "나도 그래. 털갈이 하는 것도 아닌데 숭숭",
          "speaker_id": "2",
          "time": "20210820 23:12"
        },
        {
          "id": "MDRW2100003446.1.2",
          "form": "머리 감으면 하수구가 막힐 지경이야ㅠ 머리를 짧게 잘라야 하나",
          "original_form": "머리 감으면 하수구가 막힐 지경이야ㅠ 머리를 짧게 잘라야 하나",
          "speaker_id": "1",
          "time": "20210820 23:13"
        },
        {
          "id": "MDRW2100003446.1.3",
          "form": "탈모 제품 좀 써봐",
          "original_form": "탈모 제품 좀 써봐",
          "speaker_id": "2",
          "time": "20210820 23:13"
        },
        {
          "id": "MDRW2100003446.1.4",
          "form": "지금 쓰는 것도 탈모 방지인데 ㅠ",
          "original_form": "지금 쓰는 것도 탈모 방지인데 ㅠ",
          "speaker_id": "1",
          "time": "20210820 23:13"
        },
        {
          "id": "MDRW2100003446.1.5",
          "form": "빗으로 마사지도 좀 하고",
          "original_form": "빗으로 마사지도 좀 하고",
          "speaker_id": "2",
          "time": "20210820 23:13"
        },
        {
          "id": "MDRW2100003446.1.6",
          "form": "빗으로 빗으면 더 빠지는 것 같아 ㅋㅋㅋ",
          "original_form": "빗으로 빗으면 더 빠지는 것 같아 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 23:13"
        },
        {
          "id": "MDRW2100003446.1.7",
          "form": "하긴 안방에 머리카락 많이 떨어져있긴 하더라",
          "original_form": "하긴 안방에 머리카락 많이 떨어져있긴 하더라",
          "speaker_id": "2",
          "time": "20210820 23:14"
        },
        {
          "id": "MDRW2100003446.1.8",
          "form": "내가 청소기로 수시로 없애는데도 그 정도야",
          "original_form": "내가 청소기로 수시로 없애는데도 그 정도야",
          "speaker_id": "1",
          "time": "20210820 23:14"
        },
        {
          "id": "MDRW2100003446.1.9",
          "form": "요즘 스트레스 받아서 더 그런가 ㅠ",
          "original_form": "요즘 스트레스 받아서 더 그런가 ㅠ",
          "speaker_id": "2",
          "time": "20210820 23:14"
        },
        {
          "id": "MDRW2100003446.1.10",
          "form": "ㅎㅎ 그럴지도",
          "original_form": "ㅎㅎ 그럴지도",
          "speaker_id": "1",
          "time": "20210820 23:14"
        },
        {
          "id": "MDRW2100003446.1.11",
          "form": "코로나 없어져야 탈모도 사라지나",
          "original_form": "코로나 없어져야 탈모도 사라지나",
          "speaker_id": "1",
          "time": "20210820 23:14"
        },
        {
          "id": "MDRW2100003446.1.12",
          "form": "ㅎㅎ 그럴건가보다",
          "original_form": "ㅎㅎ 그럴건가보다",
          "speaker_id": "2",
          "time": "20210820 23:14"
        },
        {
          "id": "MDRW2100003446.1.13",
          "form": "나이 들수록 머리숱이 줄어들어 슬퍼",
          "original_form": "나이 들수록 머리숱이 줄어들어 슬퍼",
          "speaker_id": "1",
          "time": "20210820 23:15"
        },
        {
          "id": "MDRW2100003446.1.14",
          "form": "겉으로는 티안나",
          "original_form": "겉으로는 티안나",
          "speaker_id": "2",
          "time": "20210820 23:15"
        },
        {
          "id": "MDRW2100003446.1.15",
          "form": "아이낳고 왕창 빠지더니 회복될 줄 알았는데 다시 안돌아오네 ㅠㅠ",
          "original_form": "아이낳고 왕창 빠지더니 회복될 줄 알았는데 다시 안돌아오네 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 23:15"
        },
        {
          "id": "MDRW2100003446.1.16",
          "form": "토닥토닥.. 힘내",
          "original_form": "토닥토닥.. 힘내",
