      "utterance": [
        {
          "id": "MDRW2100031343.1.1",
          "form": "혹시 건강 관리 하시나용?",
          "original_form": "혹시 건강 관리 하시나용?",
          "speaker_id": "2",
          "time": "20210904 22:36"
        },
        {
          "id": "MDRW2100031343.1.2",
          "form": "가끔 운동을 해요",
          "original_form": "가끔 운동을 해요",
          "speaker_id": "1",
          "time": "20210904 22:57"
        },
        {
          "id": "MDRW2100031343.1.3",
          "form": "무슨운동하세요?",
          "original_form": "무슨운동하세요?",
          "speaker_id": "2",
          "time": "20210904 23:07"
        },
        {
          "id": "MDRW2100031343.1.4",
          "form": "저는 슬로우버피 해요",
          "original_form": "저는 슬로우버피 해요",
          "speaker_id": "1",
          "time": "20210904 23:07"
        },
        {
          "id": "MDRW2100031343.1.5",
          "form": "하루에 몇개할까요?",
          "original_form": "하루에 몇개할까요?",
          "speaker_id": "1",
          "time": "20210904 23:07"
        },
        {
          "id": "MDRW2100031343.1.6",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031343.1.7",
          "form": "대단하네요",
          "original_form": "대단하네요",
          "speaker_id": "2",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031343.1.8",
          "form": "전 그냥 제자리 걷기도 힘들던데",
          "original_form": "전 그냥 제자리 걷기도 힘들던데",
          "speaker_id": "2",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031343.1.9",
          "form": "아 감사해요ㅋㅋㅋ",
          "original_form": "아 감사해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031343.1.10",
          "form": "몇개하세요?  100개?",
          "original_form": "몇개하세요?  100개?",
          "speaker_id": "2",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031343.1.11",
          "form": "아 근데 저도 자주는 안해요",
          "original_form": "아 근데 저도 자주는 안해요",
          "speaker_id": "1",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031343.1.12",
          "form": "네 100개정도요....유튜브 보고 따라해서요",
          "original_form": "네 100개정도요....유튜브 보고 따라해서요",
          "speaker_id": "1",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031343.1.13",
          "form": "다들 스쿼트가 좋다던데요",
          "original_form": "다들 스쿼트가 좋다던데요",
          "speaker_id": "1",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100031343.1.14",
          "form": "아 저는 그냥 산책처럼 걷는 것도 좋아해요",
          "original_form": "아 저는 그냥 산책처럼 걷는 것도 좋아해요",
          "speaker_id": "1",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031343.1.15",
          "form": "산책 자주 하시나요?",
          "original_form": "산책 자주 하시나요?",
          "speaker_id": "1",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031343.1.16",
          "form": "100개 ",
          "original_form": "100개 ",
          "speaker_id": "2",
          "time": "20210905 11:07"
        },
        {
          "id": "MDRW2100031343.1.17",
          "form": "스쿼트는 예전엔 5개도 힘들었는데 요즘은 20개정도까지 하는거같아요 즈질체력이라",
          "original_form": "스쿼트는 예전엔 5개도 힘들었는데 요즘은 20개정도까지 하는거같아요 즈질체력이라",
          "speaker_id": "2",
          "time": "20210905 11:07"
        },
        {
          "id": "MDRW2100031343.1.18",
          "form": "산책 자주안해요 ㅎ",
          "original_form": "산책 자주안해요 ㅎ",
          "speaker_id": "2",
          "time": "20210905 11:07"
        },
        {
          "id": "MDRW2100031343.1.19",
          "form": "그냥 소화 안되면 집에서 제자리걸음 하는정도?",
          "original_form": "그냥 소화 안되면 집에서 제자리걸음 하는정도?",
          "speaker_id": "2",
          "time": "20210905 11:07"
        },
        {
          "id": "MDRW2100031343.1.20",
          "form": "오 제자리걸음도 은근 운동 되던데요",
          "original_form": "오 제자리걸음도 은근 운동 되던데요",
          "speaker_id": "1",
          "time": "20210905 11:33"
        },
        {
          "id": "MDRW2100031343.1.21",
          "form": "막 30분씩 걷고 그러기는 힘들지만요ㅠㅠㅠ",
          "original_form": "막 30분씩 걷고 그러기는 힘들지만요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 11:33"
        },
        {
          "id": "MDRW2100031343.1.22",
          "form": "되기는해요 ㅎ 엉망으로 걸어서 발목이 아프지만요",
          "original_form": "되기는해요 ㅎ 엉망으로 걸어서 발목이 아프지만요",
          "speaker_id": "2",
          "time": "20210905 11:40"
        },
        {
          "id": "MDRW2100031343.1.23",
          "form": "이것도 신발 신고해야겠더라고요",
          "original_form": "이것도 신발 신고해야겠더라고요",
          "speaker_id": "2",
          "time": "20210905 11:40"
        },
        {
          "id": "MDRW2100031343.1.24",
          "form": "아 맞아요 런닝화 신으라고 하더라구요...",
          "original_form": "아 맞아요 런닝화 신으라고 하더라구요...",
          "speaker_id": "1",
          "time": "20210905 16:09"
        },
        {
          "id": "MDRW2100031343.1.25",
          "form": "네네 어제 좀 하고 무릎이...쑤시더라고요",
          "original_form": "네네 어제 좀 하고 무릎이...쑤시더라고요",
          "speaker_id": "2",
          "time": "20210905 21:00"
        },
        {
          "id": "MDRW2100031343.1.26",
          "form": "ㅋㅋㅋㅋ혹시 집에서 하시다면 신발신고 하세요ㅠ",
          "original_form": "ㅋㅋㅋㅋ혹시 집에서 하시다면 신발신고 하세요ㅠ",
          "speaker_id": "2",
          "time": "20210905 21:00"
        },
        {
          "id": "MDRW2100031343.1.27",
          "form": "헐 네",
          "original_form": "헐 네",
          "speaker_id": "1",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100031343.1.28",
          "form": "신발 장만해야겠어요",
