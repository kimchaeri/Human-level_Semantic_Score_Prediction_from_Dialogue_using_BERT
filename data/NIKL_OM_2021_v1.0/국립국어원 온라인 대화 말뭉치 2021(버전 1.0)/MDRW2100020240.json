        {
          "id": "MDRW2100020240.1.1",
          "form": "재택근무 하시나요?",
          "original_form": "재택근무 하시나요?",
          "speaker_id": "1",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020240.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210828 06:33"
        },
        {
          "id": "MDRW2100020240.1.3",
          "form": "그러시구나 저도 안해요",
          "original_form": "그러시구나 저도 안해요",
          "speaker_id": "1",
          "time": "20210828 09:16"
        },
        {
          "id": "MDRW2100020240.1.4",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210828 09:22"
        },
        {
          "id": "MDRW2100020240.1.5",
          "form": "요즘도 주택근무하는 회사가 꽤 있더라구요",
          "original_form": "요즘도 주택근무하는 회사가 꽤 있더라구요",
          "speaker_id": "2",
          "time": "20210828 09:22"
        },
        {
          "id": "MDRW2100020240.1.6",
          "form": "예전에 했었는데 힘들었던",
          "original_form": "예전에 했었는데 힘들었던",
          "speaker_id": "1",
          "time": "20210828 09:28"
        },
        {
          "id": "MDRW2100020240.1.7",
          "form": "생각보다 힘들다고하더라구요",
          "original_form": "생각보다 힘들다고하더라구요",
          "speaker_id": "2",
          "time": "20210828 19:08"
        },
        {
          "id": "MDRW2100020240.1.8",
          "form": "저는 되게 편할거라 생각해서 놀랐어요",
          "original_form": "저는 되게 편할거라 생각해서 놀랐어요",
          "speaker_id": "2",
          "time": "20210828 19:08"
        },
        {
          "id": "MDRW2100020240.1.9",
          "form": "집안일과같이ㅠ",
          "original_form": "집안일과같이ㅠ",
          "speaker_id": "1",
          "time": "20210828 22:00"
        },
        {
          "id": "MDRW2100020240.1.10",
          "form": "으윽ㅠ 그게 힘들군요",
          "original_form": "으윽ㅠ 그게 힘들군요",
          "speaker_id": "2",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100020240.1.11",
          "form": "그래서 별로더라구요",
          "original_form": "그래서 별로더라구요",
          "speaker_id": "1",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100020240.1.12",
          "form": "확실히 그렇네요",
          "original_form": "확실히 그렇네요",
          "speaker_id": "2",
          "time": "20210828 23:44"
        },
        {
          "id": "MDRW2100020240.1.13",
          "form": "집이면 집중도 잘안될거같아요",
          "original_form": "집이면 집중도 잘안될거같아요",
          "speaker_id": "2",
          "time": "20210828 23:44"
        },
        {
          "id": "MDRW2100020240.1.14",
          "form": "네 일할게 잔뜩ㅠ",
          "original_form": "네 일할게 잔뜩ㅠ",
          "speaker_id": "1",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100020240.1.15",
          "form": "일에 일인거네요",
          "original_form": "일에 일인거네요",
          "speaker_id": "2",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100020240.1.16",
          "form": "으윽ㅠ",
          "original_form": "으윽ㅠ",
          "speaker_id": "2",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100020240.1.17",
          "form": "아무도 없어야",
          "original_form": "아무도 없어야",
