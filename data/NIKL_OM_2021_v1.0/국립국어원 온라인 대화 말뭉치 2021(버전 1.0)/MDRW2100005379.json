      "utterance": [
        {
          "id": "MDRW2100005379.1.1",
          "form": "아침 저녁 선선하네요",
          "original_form": "아침 저녁 선선하네요",
          "speaker_id": "2",
          "time": "20210821 23:08"
        },
        {
          "id": "MDRW2100005379.1.2",
          "form": "비가 오니 좀 시원해졌어요",
          "original_form": "비가 오니 좀 시원해졌어요",
          "speaker_id": "1",
          "time": "20210821 23:09"
        },
        {
          "id": "MDRW2100005379.1.3",
          "form": "절기는 과학이네요",
          "original_form": "절기는 과학이네요",
          "speaker_id": "1",
          "time": "20210821 23:10"
        },
        {
          "id": "MDRW2100005379.1.4",
          "form": "그러네요. 음 저는 여름이 싫어요",
          "original_form": "그러네요. 음 저는 여름이 싫어요",
          "speaker_id": "2",
          "time": "20210821 23:10"
        },
        {
          "id": "MDRW2100005379.1.5",
          "form": "저두요 점점 더워져서 힘들죠",
          "original_form": "저두요 점점 더워져서 힘들죠",
          "speaker_id": "1",
          "time": "20210821 23:10"
        },
        {
          "id": "MDRW2100005379.1.6",
          "form": "봄가을이 짧아 아쉬워요",
          "original_form": "봄가을이 짧아 아쉬워요",
          "speaker_id": "1",
          "time": "20210821 23:10"
        },
        {
          "id": "MDRW2100005379.1.7",
          "form": "시월까지는 더울 것 같아요",
          "original_form": "시월까지는 더울 것 같아요",
          "speaker_id": "1",
          "time": "20210821 23:11"
        },
        {
          "id": "MDRW2100005379.1.8",
          "form": "ㅜㅜ 그럴리가요 곧 선선해집니당",
          "original_form": "ㅜㅜ 그럴리가요 곧 선선해집니당",
          "speaker_id": "2",
          "time": "20210821 23:26"
        },
        {
          "id": "MDRW2100005379.1.9",
          "form": "넹 그럴거예요 환절기 때 감기 조심하세요",
          "original_form": "넹 그럴거예요 환절기 때 감기 조심하세요",
          "speaker_id": "1",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005379.1.10",
          "form": "환절기에도 마스크는 필수",
          "original_form": "환절기에도 마스크는 필수",
          "speaker_id": "2",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005379.1.11",
          "form": "마스크 쓰니 감기는 덜 걸렸어요",
          "original_form": "마스크 쓰니 감기는 덜 걸렸어요",
          "speaker_id": "1",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005379.1.12",
          "form": "겨울은 올까요",
          "original_form": "겨울은 올까요",
          "speaker_id": "2",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005379.1.13",
          "form": "상추가 써지면 여름이 간거래요",
          "original_form": "상추가 써지면 여름이 간거래요",
          "speaker_id": "1",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005379.1.14",
          "form": "겨울은 생각보다는 빨리 올 것 같아요",
          "original_form": "겨울은 생각보다는 빨리 올 것 같아요",
          "speaker_id": "1",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005379.1.15",
          "form": "오늘 지진이 있었대요",
          "original_form": "오늘 지진이 있었대요",
          "speaker_id": "1",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005379.1.16",
          "form": "미세먼지는 없었어요",
          "original_form": "미세먼지는 없었어요",
          "speaker_id": "2",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100005379.1.17",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100005379.1.18",
          "form": "일기예보가 다 맞진 않네요",
          "original_form": "일기예보가 다 맞진 않네요",
          "speaker_id": "1",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100005379.1.19",
          "form": "전 아침마다 날씨 확인하거든요",
          "original_form": "전 아침마다 날씨 확인하거든요",
          "speaker_id": "1",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100005379.1.20",
          "form": "아하 날씨 곧 좋아지겠죠",
          "original_form": "아하 날씨 곧 좋아지겠죠",
          "speaker_id": "2",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100005379.1.21",
          "form": "가을이네요",
