        {
          "id": "MDRW2100006401.1.1",
          "form": "뼈닭과 순살중에 어떤거 선호하세요",
          "original_form": "뼈닭과 순살중에 어떤거 선호하세요",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100006401.1.2",
          "form": "뼈 있는 거요!",
          "original_form": "뼈 있는 거요!",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100006401.1.3",
          "form": "가장최애치킨 브랜드는여?",
          "original_form": "가장최애치킨 브랜드는여?",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100006401.1.4",
          "form": "kfc! 너무 맛있어요....",
          "original_form": "kfc! 너무 맛있어요....",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100006401.1.5",
          "form": "오 그 약간매콤한 그거드세요?",
          "original_form": "오 그 약간매콤한 그거드세요?",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100006401.1.6",
          "form": "네 그거 너무 좋아요...",
          "original_form": "네 그거 너무 좋아요...",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100006401.1.7",
          "form": "부위는 어떤부위좋아하세요",
          "original_form": "부위는 어떤부위좋아하세요",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100006401.1.8",
          "form": "당연히 다리 젤 좋아합니다!",
          "original_form": "당연히 다리 젤 좋아합니다!",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100006401.1.9",
          "form": "아 목도 좋아해요",
          "original_form": "아 목도 좋아해요",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100006401.1.10",
          "form": "ㅋㅋㅋ저도 다리와날개좋아해서 보통 콤보먹어요",
          "original_form": "ㅋㅋㅋ저도 다리와날개좋아해서 보통 콤보먹어요",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100006401.1.11",
          "form": "후라이드파신가여",
          "original_form": "후라이드파신가여",
