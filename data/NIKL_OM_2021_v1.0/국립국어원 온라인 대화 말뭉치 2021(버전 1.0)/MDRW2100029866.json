        },
        {
          "id": "MDRW2100029866.1.3",
          "form": "name3이 꺼 내가 알려줄 수 있어ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "&name3&이 꺼 내가 알려줄 수 있어ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:39"
        },
        {
          "id": "MDRW2100029866.1.4",
          "form": "그치만 이영도 글 잘 쓰는데....",
          "original_form": "그치만 이영도 글 잘 쓰는데....",
          "speaker_id": "1",
          "time": "20210903 15:39"
        },
        {
          "id": "MDRW2100029866.1.5",
          "form": "감나무 드립만 봤음ㅋㅋㅋ",
          "original_form": "감나무 드립만 봤음ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:51"
        },
        {
          "id": "MDRW2100029866.1.6",
          "form": "한번씩은 보더라",
          "original_form": "한번씩은 보더라",
          "speaker_id": "2",
          "time": "20210903 15:51"
        },
        {
          "id": "MDRW2100029866.1.7",
          "form": "신간을 잘 안 내기 때문에....",
          "original_form": "신간을 잘 안 내기 때문에....",
          "speaker_id": "1",
          "time": "20210903 15:52"
        },
        {
          "id": "MDRW2100029866.1.8",
          "form": "작년인가에 시타의 장인가?",
          "original_form": "작년인가에 시타의 장인가?",
          "speaker_id": "1",
          "time": "20210903 15:52"
        },
        {
          "id": "MDRW2100029866.1.9",
          "form": "내긴 했지만",
          "original_form": "내긴 했지만",
          "speaker_id": "1",
          "time": "20210903 15:52"
        },
        {
          "id": "MDRW2100029866.1.10",
          "form": "오 내긴냈구나",
          "original_form": "오 내긴냈구나",
          "speaker_id": "2",
          "time": "20210903 15:55"
        },
        {
          "id": "MDRW2100029866.1.11",
          "form": "특정작가거 챙겨본건",
          "original_form": "특정작가거 챙겨본건",
          "speaker_id": "2",
          "time": "20210903 15:55"
        },
        {
          "id": "MDRW2100029866.1.12",
          "form": "베르베르 책 뿐일듯",
          "original_form": "베르베르 책 뿐일듯",
          "speaker_id": "2",
          "time": "20210903 15:55"
        },
        {
          "id": "MDRW2100029866.1.13",
          "form": "탐라의 이영도 팬들이 축배를 들어서",
          "original_form": "탐라의 이영도 팬들이 축배를 들어서",
          "speaker_id": "1",
          "time": "20210903 15:56"
        },
        {
          "id": "MDRW2100029866.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
