      "utterance": [
        {
          "id": "MDRW2100007074.1.1",
          "form": "곰표 맥주",
          "original_form": "곰표 맥주",
          "speaker_id": "1",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007074.1.2",
          "form": "드셔보셨나요",
          "original_form": "드셔보셨나요",
          "speaker_id": "1",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007074.1.3",
          "form": "먹어봤는데",
          "original_form": "먹어봤는데",
          "speaker_id": "2",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007074.1.4",
          "form": "일반 맥주랑 다른 점을 모르겟어요",
          "original_form": "일반 맥주랑 다른 점을 모르겟어요",
          "speaker_id": "2",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007074.1.5",
          "form": "아저는",
          "original_form": "아저는",
          "speaker_id": "1",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007074.1.6",
          "form": "한번도안먹어봤어요",
          "original_form": "한번도안먹어봤어요",
          "speaker_id": "1",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007074.1.7",
          "form": "비싼가요?",
          "original_form": "비싼가요?",
          "speaker_id": "1",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007074.1.8",
          "form": "글캐 비싸진 않앗던거같앙ㅅ",
          "original_form": "글캐 비싸진 않앗던거같앙ㅅ",
          "speaker_id": "2",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007074.1.9",
          "form": "곰표 패딩도 나온거 아심??",
          "original_form": "곰표 패딩도 나온거 아심??",
          "speaker_id": "2",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007074.1.10",
          "form": "그건예전에나왔어요...",
          "original_form": "그건예전에나왔어요...",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007074.1.11",
          "form": "콜라보마켓팅에데해잘모르시네요...",
          "original_form": "콜라보마켓팅에데해잘모르시네요...",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007074.1.12",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007074.1.13",
          "form": "곰표패딩",
          "original_form": "곰표패딩",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007074.1.14",
          "form": "전소미도 입엇다",
          "original_form": "전소미도 입엇다",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007074.1.15",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007074.1.16",
          "form": "이얘기가 하고싶엇음",
          "original_form": "이얘기가 하고싶엇음",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007074.1.17",
          "form": "네...",
          "original_form": "네...",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007074.1.18",
          "form": "그리고 곰표패딩은",
          "original_form": "그리고 곰표패딩은",
          "speaker_id": "1",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007074.1.19",
          "form": "연예인선정에 어려움을겪나보네요",
          "original_form": "연예인선정에 어려움을겪나보네요",
          "speaker_id": "1",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007074.1.20",
          "form": "아니 전소미가",
          "original_form": "아니 전소미가",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007074.1.21",
          "form": "자기가 사서 입은거임",
          "original_form": "자기가 사서 입은거임",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007074.1.22",
          "form": "곰표 패딩 = 인싸들의 상징",
          "original_form": "곰표 패딩 = 인싸들의 상징",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007074.1.23",
          "form": "전소미 = 인써",
          "original_form": "전소미 = 인써",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007074.1.24",
          "form": "인싸",
          "original_form": "인싸",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007074.1.25",
          "form": "아...근데곰표패딩이",
          "original_form": "아...근데곰표패딩이",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007074.1.26",
          "form": "전소미가입으니까",
          "original_form": "전소미가입으니까",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007074.1.27",
          "form": "아니세요 아무튼 콜라보마케팅은참",
          "original_form": "아니세요 아무튼 콜라보마케팅은참",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007074.1.28",
          "form": "이제그만햏음해요",
          "original_form": "이제그만햏음해요",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007074.1.29",
          "form": "먹는 것처럼나온건",
          "original_form": "먹는 것처럼나온건",
          "speaker_id": "1",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007074.1.30",
          "form": "애들이헷갈릴까걱정이에요",
          "original_form": "애들이헷갈릴까걱정이에요",
          "speaker_id": "1",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007074.1.31",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007074.1.32",
          "form": "애들이 헷갈려서",
          "original_form": "애들이 헷갈려서",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007074.1.33",
          "form": "곰표 밀가루 대신",
          "original_form": "곰표 밀가루 대신",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007074.1.34",
          "form": "곰표 맥주를 사면",
          "original_form": "곰표 맥주를 사면",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007074.1.35",
          "form": "큰일이지않겠어요?",
          "original_form": "큰일이지않겠어요?",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007074.1.36",
          "form": "그건아니지만",
          "original_form": "그건아니지만",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007074.1.37",
          "form": "마시는 마커",
          "original_form": "마시는 마커",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007074.1.38",
          "form": "이런거는",
          "original_form": "이런거는",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007074.1.39",
          "form": "위험할거같아요",
          "original_form": "위험할거같아요",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007074.1.40",
          "form": "앗 그쵸그쵸",
          "original_form": "앗 그쵸그쵸",
          "speaker_id": "2",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007074.1.41",
          "form": "마시는 마커는...정말 위험 한 것 같아요",
          "original_form": "마시는 마커는...정말 위험 한 것 같아요",
          "speaker_id": "2",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007074.1.42",
          "form": "마커는진짜",
          "original_form": "마커는진짜",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007074.1.43",
          "form": "아무튼위험함을피해야",
          "original_form": "아무튼위험함을피해야",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007074.1.44",
          "form": "할거같아요",
          "original_form": "할거같아요",
          "speaker_id": "1",
          "time": "20210822 00:56"
        }
      ]
    }
  ]
