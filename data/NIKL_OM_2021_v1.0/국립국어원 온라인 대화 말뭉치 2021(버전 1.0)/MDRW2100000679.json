      "utterance": [
        {
          "id": "MDRW2100000679.1.1",
          "form": "재택근무 넘 ..좋아",
          "original_form": "재택근무 넘 ..좋아",
          "speaker_id": "1",
          "time": "20210819 17:35"
        },
        {
          "id": "MDRW2100000679.1.2",
          "form": "맞아~ 출퇴근의 에너지를 아낄 수 있어서 너무 좋다~",
          "original_form": "맞아~ 출퇴근의 에너지를 아낄 수 있어서 너무 좋다~",
          "speaker_id": "2",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000679.1.3",
          "form": "요즘같은 시국에 안전하기도 하고~",
          "original_form": "요즘같은 시국에 안전하기도 하고~",
          "speaker_id": "2",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000679.1.4",
          "form": "맞아용 넘 안전해~",
          "original_form": "맞아용 넘 안전해~",
          "speaker_id": "1",
          "time": "20210819 17:38"
        },
        {
          "id": "MDRW2100000679.1.5",
          "form": "시간 절약 굿~",
          "original_form": "시간 절약 굿~",
          "speaker_id": "1",
          "time": "20210819 17:38"
        },
        {
          "id": "MDRW2100000679.1.6",
          "form": "웅웅~ 통근시간만 3~4시간인데,",
          "original_form": "웅웅~ 통근시간만 3~4시간인데,",
          "speaker_id": "2",
          "time": "20210819 17:40"
        },
        {
          "id": "MDRW2100000679.1.7",
          "form": "3~4시간이 생겨서 넘나 좋다~",
          "original_form": "3~4시간이 생겨서 넘나 좋다~",
          "speaker_id": "2",
          "time": "20210819 17:40"
        },
        {
          "id": "MDRW2100000679.1.8",
          "form": "마자마자! 통근시간 넘 아까워~",
          "original_form": "마자마자! 통근시간 넘 아까워~",
          "speaker_id": "1",
          "time": "20210819 17:41"
        },
        {
          "id": "MDRW2100000679.1.9",
          "form": "웅~ 진짜 사람들한테 낑겨서 시간낭비..ㅠㅠ",
          "original_form": "웅~ 진짜 사람들한테 낑겨서 시간낭비..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 17:45"
        },
        {
          "id": "MDRW2100000679.1.10",
          "form": "마자 ㅜㅜ",
          "original_form": "마자 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210819 17:47"
        },
        {
          "id": "MDRW2100000679.1.11",
          "form": "지금처럼 일주일에 한번정도만 출근하면 딱 좋을듯,,",
          "original_form": "지금처럼 일주일에 한번정도만 출근하면 딱 좋을듯,,",
          "speaker_id": "1",
          "time": "20210819 17:48"
        },
        {
          "id": "MDRW2100000679.1.12",
          "form": "웅웅~ 막 모여서 할 일도 별로 없는데,",
          "original_form": "웅웅~ 막 모여서 할 일도 별로 없는데,",
          "speaker_id": "2",
          "time": "20210819 17:50"
        },
        {
          "id": "MDRW2100000679.1.13",
          "form": "회의도 화상으로도 충분히 잘 진행되고 있고~",
          "original_form": "회의도 화상으로도 충분히 잘 진행되고 있고~",
          "speaker_id": "2",
          "time": "20210819 17:50"
        },
        {
          "id": "MDRW2100000679.1.14",
          "form": "앞으로도 계속 재택 쭉 가능할텐데~",
          "original_form": "앞으로도 계속 재택 쭉 가능할텐데~",
          "speaker_id": "2",
          "time": "20210819 17:50"
        },
        {
          "id": "MDRW2100000679.1.15",
          "form": "글겡~~",
          "original_form": "글겡~~",
          "speaker_id": "1",
          "time": "20210819 17:51"
        },
        {
          "id": "MDRW2100000679.1.16",
          "form": "이대로 쭉..가길~ 코로나 없어져도 ㅋㅋㅋ",
          "original_form": "이대로 쭉..가길~ 코로나 없어져도 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:51"
        },
        {
          "id": "MDRW2100000679.1.17",
          "form": "웅웅~ 재택의 순기능을 제발 알아주시길!!",
          "original_form": "웅웅~ 재택의 순기능을 제발 알아주시길!!",
          "speaker_id": "2",
          "time": "20210819 17:52"
        },
        {
          "id": "MDRW2100000679.1.18",
          "form": "마잣!!",
          "original_form": "마잣!!",
          "speaker_id": "1",
          "time": "20210819 17:52"
        },
        {
          "id": "MDRW2100000679.1.19",
          "form": "직원들의 입장에서도 생각해주시길!!",
          "original_form": "직원들의 입장에서도 생각해주시길!!",
