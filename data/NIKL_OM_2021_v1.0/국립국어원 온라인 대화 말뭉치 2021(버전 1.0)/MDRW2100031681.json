      "utterance": [
        {
          "id": "MDRW2100031681.1.1",
          "form": "활용 잘 해서",
          "original_form": "활용 잘 해서",
          "speaker_id": "2",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100031681.1.2",
          "form": "디자인하면",
          "original_form": "디자인하면",
          "speaker_id": "2",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100031681.1.3",
          "form": "훨씬 이쁠것 같은데",
          "original_form": "훨씬 이쁠것 같은데",
          "speaker_id": "2",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100031681.1.4",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100031681.1.5",
          "form": "경주처럼은 아니더라도",
          "original_form": "경주처럼은 아니더라도",
          "speaker_id": "2",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100031681.1.6",
          "form": "이제 고성장시기 끝났으니까",
          "original_form": "이제 고성장시기 끝났으니까",
          "speaker_id": "1",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100031681.1.7",
          "form": "그런 느낌만 가져와서 디자인 하면 훨씬 좋을것 같아",
          "original_form": "그런 느낌만 가져와서 디자인 하면 훨씬 좋을것 같아",
          "speaker_id": "2",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100031681.1.8",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100031681.1.9",
          "form": "미관도 좀 신경써줫으면",
          "original_form": "미관도 좀 신경써줫으면",
          "speaker_id": "1",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100031681.1.10",
          "form": "긍가",
          "original_form": "긍가",
          "speaker_id": "1",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100031681.1.11",
          "form": "외국인이든 타지역 사람들이든",
          "original_form": "외국인이든 타지역 사람들이든",
          "speaker_id": "2",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100031681.1.12",
          "form": "오고 싶은 그런 것들을 만들어야 함",
          "original_form": "오고 싶은 그런 것들을 만들어야 함",
          "speaker_id": "2",
          "time": "20210904 23:33"
        },
        {
          "id": "MDRW2100031681.1.13",
          "form": "맞음",
          "original_form": "맞음",
          "speaker_id": "1",
          "time": "20210904 23:33"
        },
        {
          "id": "MDRW2100031681.1.14",
          "form": "어디 명소나 자연풍경 찾아가야 예쁜거말고",
          "original_form": "어디 명소나 자연풍경 찾아가야 예쁜거말고",
          "speaker_id": "1",
          "time": "20210904 23:33"
        },
        {
          "id": "MDRW2100031681.1.15",
          "form": "걍 길거리가 예뻐야함",
          "original_form": "걍 길거리가 예뻐야함",
          "speaker_id": "1",
          "time": "20210904 23:33"
        },
        {
          "id": "MDRW2100031681.1.16",
          "form": "맞아맞아맞",
          "original_form": "맞아맞아맞",
          "speaker_id": "2",
          "time": "20210904 23:33"
        },
        {
          "id": "MDRW2100031681.1.17",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210904 23:33"
        },
        {
          "id": "MDRW2100031681.1.18",
          "form": "진짜 이쁜 자연경관 있으면",
          "original_form": "진짜 이쁜 자연경관 있으면",
          "speaker_id": "2",
          "time": "20210904 23:33"
        },
        {
          "id": "MDRW2100031681.1.19",
          "form": "유럽에 gdp낮은 국가들은 ㄹㅇ 관광업으로 먹고살잖아",
          "original_form": "유럽에 gdp낮은 국가들은 ㄹㅇ 관광업으로 먹고살잖아",
          "speaker_id": "1",
          "time": "20210904 23:33"
        },
        {
          "id": "MDRW2100031681.1.20",
          "form": "거기에 카페 이것저것 생기고",
          "original_form": "거기에 카페 이것저것 생기고",
          "speaker_id": "2",
          "time": "20210904 23:34"
        },
        {
          "id": "MDRW2100031681.1.21",
          "form": "그러면서 또 미관상 안좋아지고",
          "original_form": "그러면서 또 미관상 안좋아지고",
          "speaker_id": "2",
          "time": "20210904 23:34"
        },
        {
          "id": "MDRW2100031681.1.22",
          "form": "맞음ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞음ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:34"
        },
        {
          "id": "MDRW2100031681.1.23",
