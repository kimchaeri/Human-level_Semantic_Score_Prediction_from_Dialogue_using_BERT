        },
        {
          "id": "MDRW2100042585.1.2",
          "form": "나 뭐 쫌 샀어",
          "original_form": "나 뭐 쫌 샀어",
          "speaker_id": "1",
          "time": "20210906 23:05"
        },
        {
          "id": "MDRW2100042585.1.3",
          "form": "오늘은 또 무얼 샀니",
          "original_form": "오늘은 또 무얼 샀니",
          "speaker_id": "2",
          "time": "20210906 23:05"
        },
        {
          "id": "MDRW2100042585.1.4",
          "form": "오늘은 과자를 샀어",
          "original_form": "오늘은 과자를 샀어",
          "speaker_id": "1",
          "time": "20210906 23:05"
        },
        {
          "id": "MDRW2100042585.1.5",
          "form": "너 과자 안 좋아하잖아",
          "original_form": "너 과자 안 좋아하잖아",
          "speaker_id": "2",
          "time": "20210906 23:06"
        },
        {
          "id": "MDRW2100042585.1.6",
          "form": "그건 살 찔까봐 안 먹는 거죠",
          "original_form": "그건 살 찔까봐 안 먹는 거죠",
          "speaker_id": "1",
          "time": "20210906 23:06"
        },
        {
          "id": "MDRW2100042585.1.7",
          "form": "오늘 산 건 몸에 좋은가 보네",
          "original_form": "오늘 산 건 몸에 좋은가 보네",
          "speaker_id": "2",
          "time": "20210906 23:06"
        },
        {
          "id": "MDRW2100042585.1.8",
          "form": "일단 설탕을 안 썼어",
          "original_form": "일단 설탕을 안 썼어",
          "speaker_id": "1",
          "time": "20210906 23:06"
        },
        {
          "id": "MDRW2100042585.1.9",
          "form": "맞아 당분을 조심해야지",
          "original_form": "맞아 당분을 조심해야지",
          "speaker_id": "2",
          "time": "20210906 23:06"
        },
        {
          "id": "MDRW2100042585.1.10",
          "form": "그리고 밀가루 대신 아몬드 가루로 만들었어",
          "original_form": "그리고 밀가루 대신 아몬드 가루로 만들었어",
          "speaker_id": "1",
          "time": "20210906 23:06"
        },
        {
          "id": "MDRW2100042585.1.11",
          "form": "그건 진짜 건강하겠다",
          "original_form": "그건 진짜 건강하겠다",
          "speaker_id": "2",
          "time": "20210906 23:07"
        },
        {
          "id": "MDRW2100042585.1.12",
          "form": "심지어 일반 쿠키랑 똑같아",
          "original_form": "심지어 일반 쿠키랑 똑같아",
          "speaker_id": "1",
          "time": "20210906 23:07"
        },
        {
          "id": "MDRW2100042585.1.13",
          "form": "이름 좀 말해봐",
          "original_form": "이름 좀 말해봐",
          "speaker_id": "2",
          "time": "20210906 23:07"
        },
        {
          "id": "MDRW2100042585.1.14",
          "form": "닥터 밀로의 초코칩 쿠키",
          "original_form": "닥터 밀로의 초코칩 쿠키",
          "speaker_id": "1",
          "time": "20210906 23:07"
        },
        {
          "id": "MDRW2100042585.1.15",
          "form": "나도 다음에 도전해볼게",
