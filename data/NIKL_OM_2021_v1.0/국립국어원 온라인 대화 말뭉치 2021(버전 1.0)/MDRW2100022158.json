      "utterance": [
        {
          "id": "MDRW2100022158.1.1",
          "form": "name1은 결혼하셨나요?",
          "original_form": "&name1&은 결혼하셨나요?",
          "speaker_id": "2",
          "time": "20210829 09:52"
        },
        {
          "id": "MDRW2100022158.1.2",
          "form": "아휴 아니요!",
          "original_form": "아휴 아니요!",
          "speaker_id": "1",
          "time": "20210829 09:52"
        },
        {
          "id": "MDRW2100022158.1.3",
          "form": "결혼은 커녕",
          "original_form": "결혼은 커녕",
          "speaker_id": "1",
          "time": "20210829 09:52"
        },
        {
          "id": "MDRW2100022158.1.4",
          "form": "연애 생각도 없어요",
          "original_form": "연애 생각도 없어요",
          "speaker_id": "1",
          "time": "20210829 09:53"
        },
        {
          "id": "MDRW2100022158.1.5",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "2",
          "time": "20210829 09:53"
        },
        {
          "id": "MDRW2100022158.1.6",
          "form": "근데 요즘 젊은 여자들 다 그렇다고 하더라구요",
          "original_form": "근데 요즘 젊은 여자들 다 그렇다고 하더라구요",
          "speaker_id": "2",
          "time": "20210829 09:53"
        },
        {
          "id": "MDRW2100022158.1.7",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210829 09:53"
        },
        {
          "id": "MDRW2100022158.1.8",
          "form": "요즘 남자들 보면 결혼하고싶은 생각이 싹 사라져요",
          "original_form": "요즘 남자들 보면 결혼하고싶은 생각이 싹 사라져요",
          "speaker_id": "1",
          "time": "20210829 09:53"
        },
        {
          "id": "MDRW2100022158.1.9",
          "form": "그리고 결혼을 하기엔",
          "original_form": "그리고 결혼을 하기엔",
          "speaker_id": "1",
          "time": "20210829 09:53"
        },
        {
          "id": "MDRW2100022158.1.10",
          "form": "여자들이 너무 많은 걸 알아버렸어요",
          "original_form": "여자들이 너무 많은 걸 알아버렸어요",
          "speaker_id": "1",
          "time": "20210829 09:53"
        },
        {
          "id": "MDRW2100022158.1.11",
          "form": "어떤 것을요?",
          "original_form": "어떤 것을요?",
          "speaker_id": "2",
          "time": "20210829 09:53"
        },
        {
          "id": "MDRW2100022158.1.12",
          "form": "음.... 결혼을 하면",
          "original_form": "음.... 결혼을 하면",
          "speaker_id": "1",
          "time": "20210829 09:53"
        },
        {
          "id": "MDRW2100022158.1.13",
          "form": "일단 남자가 한 여자만을 바라보면서 사는 게 아니고",
          "original_form": "일단 남자가 한 여자만을 바라보면서 사는 게 아니고",
          "speaker_id": "1",
          "time": "20210829 09:54"
        },
        {
          "id": "MDRW2100022158.1.14",
          "form": "다른 여자랑 바람을 피든",
          "original_form": "다른 여자랑 바람을 피든",
          "speaker_id": "1",
          "time": "20210829 09:54"
        },
        {
          "id": "MDRW2100022158.1.15",
          "form": "밖에서 비도덕적인 성 매매 행위를 하든",
          "original_form": "밖에서 비도덕적인 성 매매 행위를 하든",
          "speaker_id": "1",
          "time": "20210829 09:54"
        },
        {
          "id": "MDRW2100022158.1.16",
          "form": "네....",
          "original_form": "네....",
          "speaker_id": "2",
          "time": "20210829 09:54"
        },
        {
          "id": "MDRW2100022158.1.17",
          "form": "자식들 때문에 참고 사는 경우도 많고",
          "original_form": "자식들 때문에 참고 사는 경우도 많고",
          "speaker_id": "1",
          "time": "20210829 09:54"
        },
        {
          "id": "MDRW2100022158.1.18",
          "form": "가사 노동 대부분 여자가 하고",
          "original_form": "가사 노동 대부분 여자가 하고",
          "speaker_id": "1",
          "time": "20210829 09:54"
        },
        {
          "id": "MDRW2100022158.1.19",
          "form": "아이가 생겨도",
          "original_form": "아이가 생겨도",
          "speaker_id": "1",
          "time": "20210829 09:54"
        },
        {
          "id": "MDRW2100022158.1.20",
          "form": "육아는 독박으로 여자가 하잖아요",
          "original_form": "육아는 독박으로 여자가 하잖아요",
          "speaker_id": "1",
          "time": "20210829 09:54"
        },
        {
          "id": "MDRW2100022158.1.21",
          "form": "다 맞는 말이에요",
          "original_form": "다 맞는 말이에요",
          "speaker_id": "2",
          "time": "20210829 09:54"
        },
        {
          "id": "MDRW2100022158.1.22",
          "form": "저희 세대는",
          "original_form": "저희 세대는",
          "speaker_id": "2",
          "time": "20210829 09:55"
        },
        {
          "id": "MDRW2100022158.1.23",
          "form": "그냥 그런걸 다",
          "original_form": "그냥 그런걸 다",
          "speaker_id": "2",
          "time": "20210829 09:55"
        },
        {
          "id": "MDRW2100022158.1.24",
          "form": "당연하게 여겼어요",
          "original_form": "당연하게 여겼어요",
          "speaker_id": "2",
          "time": "20210829 09:55"
        },
        {
          "id": "MDRW2100022158.1.25",
          "form": "맞아요 저희 엄마도",
          "original_form": "맞아요 저희 엄마도",
          "speaker_id": "1",
          "time": "20210829 09:55"
        },
        {
          "id": "MDRW2100022158.1.26",
          "form": "아빠 때문에 그렇게 고생했으면서",
          "original_form": "아빠 때문에 그렇게 고생했으면서",
          "speaker_id": "1",
          "time": "20210829 09:55"
        },
        {
          "id": "MDRW2100022158.1.27",
          "form": "저한테는 결혼하라고 매일 난리예요",
          "original_form": "저한테는 결혼하라고 매일 난리예요",
          "speaker_id": "1",
          "time": "20210829 09:55"
        },
        {
          "id": "MDRW2100022158.1.28",
          "form": "모순적이네요",
          "original_form": "모순적이네요",
          "speaker_id": "2",
          "time": "20210829 09:55"
        },
        {
          "id": "MDRW2100022158.1.29",
          "form": "하지만 엄마한테는",
          "original_form": "하지만 엄마한테는",
          "speaker_id": "2",
          "time": "20210829 09:55"
        },
        {
          "id": "MDRW2100022158.1.30",
          "form": "아직 희망이 있어서 그럴지도 몰라요",
          "original_form": "아직 희망이 있어서 그럴지도 몰라요",
          "speaker_id": "2",
          "time": "20210829 09:55"
        },
        {
          "id": "MDRW2100022158.1.31",
          "form": "어떤 희망이요???",
          "original_form": "어떤 희망이요???",
          "speaker_id": "1",
          "time": "20210829 09:55"
        },
        {
          "id": "MDRW2100022158.1.32",
          "form": "딸이 만나는 남자는",
          "original_form": "딸이 만나는 남자는",
          "speaker_id": "2",
          "time": "20210829 09:55"
        },
        {
          "id": "MDRW2100022158.1.33",
          "form": "괜찮을지도 모른다는 희망이요",
          "original_form": "괜찮을지도 모른다는 희망이요",
          "speaker_id": "2",
          "time": "20210829 09:56"
        },
        {
          "id": "MDRW2100022158.1.34",
          "form": "에휴 그럴리가요",
          "original_form": "에휴 그럴리가요",
          "speaker_id": "1",
          "time": "20210829 09:56"
        },
        {
          "id": "MDRW2100022158.1.35",
          "form": "2030 남자들은 글렀어요",
          "original_form": "2030 남자들은 글렀어요",
          "speaker_id": "1",
          "time": "20210829 09:56"
        },
        {
          "id": "MDRW2100022158.1.36",
          "form": "신남성연대 이런 것도 만들고...",
          "original_form": "신남성연대 이런 것도 만들고...",
          "speaker_id": "1",
          "time": "20210829 09:56"
        },
        {
          "id": "MDRW2100022158.1.37",
          "form": "저도 그거 우리 딸이 얘기해줘서 알았어요",
          "original_form": "저도 그거 우리 딸이 얘기해줘서 알았어요",
          "speaker_id": "2",
          "time": "20210829 09:56"
        },
        {
          "id": "MDRW2100022158.1.38",
          "form": "심각하더라구요",
          "original_form": "심각하더라구요",
          "speaker_id": "2",
          "time": "20210829 09:56"
        }
      ]
    }
  ]
