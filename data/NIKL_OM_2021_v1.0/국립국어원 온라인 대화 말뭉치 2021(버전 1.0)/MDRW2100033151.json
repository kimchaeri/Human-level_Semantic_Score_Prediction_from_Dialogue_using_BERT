      "utterance": [
        {
          "id": "MDRW2100033151.1.1",
          "form": "안녕하세용 반려동물 키우시나요?",
          "original_form": "안녕하세용 반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210906 17:40"
        },
        {
          "id": "MDRW2100033151.1.2",
          "form": "예전에 키웠는데 지금은 안키워요",
          "original_form": "예전에 키웠는데 지금은 안키워요",
          "speaker_id": "2",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100033151.1.3",
          "form": "name1님은 반려동물과 함께 사시나요?",
          "original_form": "&name1&님은 반려동물과 함께 사시나요?",
          "speaker_id": "2",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100033151.1.4",
          "form": "네 저는 열대어와 함께 살아요",
          "original_form": "네 저는 열대어와 함께 살아요",
          "speaker_id": "1",
          "time": "20210906 17:42"
        },
        {
          "id": "MDRW2100033151.1.5",
          "form": "사진을 못 받는게 아쉽네요 몇마리 키우세요?",
          "original_form": "사진을 못 받는게 아쉽네요 몇마리 키우세요?",
          "speaker_id": "2",
          "time": "20210906 17:43"
        },
        {
          "id": "MDRW2100033151.1.6",
          "form": "20마리 좀 넘는 것 같아요",
          "original_form": "20마리 좀 넘는 것 같아요",
          "speaker_id": "1",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100033151.1.7",
          "form": "너무 활발하게 움직여서 정확한 수를 셀 수가 없어요",
          "original_form": "너무 활발하게 움직여서 정확한 수를 셀 수가 없어요",
          "speaker_id": "1",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100033151.1.8",
          "form": "와우 스무마리나요",
          "original_form": "와우 스무마리나요",
          "speaker_id": "2",
          "time": "20210906 17:45"
        },
        {
          "id": "MDRW2100033151.1.9",
          "form": "저는 열대어하면 구피밖에 모르는데,,",
          "original_form": "저는 열대어하면 구피밖에 모르는데,,",
          "speaker_id": "2",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100033151.1.10",
          "form": "어떤 종 키우세요?",
          "original_form": "어떤 종 키우세요?",
          "speaker_id": "2",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100033151.1.11",
          "form": "구피랑 풍선몰리, 엔젤피쉬, 네온테트라 키우고 있어요",
          "original_form": "구피랑 풍선몰리, 엔젤피쉬, 네온테트라 키우고 있어요",
          "speaker_id": "1",
          "time": "20210906 17:48"
        },
        {
          "id": "MDRW2100033151.1.12",
          "form": "멋지다 보니까 열대어도 손 많이 가더라고요",
          "original_form": "멋지다 보니까 열대어도 손 많이 가더라고요",
          "speaker_id": "2",
          "time": "20210906 17:50"
        },
        {
          "id": "MDRW2100033151.1.13",
          "form": "직접 스무마리나 챙기시려면 시간 많이 쏟아야겠어요",
          "original_form": "직접 스무마리나 챙기시려면 시간 많이 쏟아야겠어요",
          "speaker_id": "2",
          "time": "20210906 17:51"
        },
        {
          "id": "MDRW2100033151.1.14",
          "form": "1~2주에 한번 부분환수만 해주면 되는거라 생각보다 괜찮아요!",
          "original_form": "1~2주에 한번 부분환수만 해주면 되는거라 생각보다 괜찮아요!",
          "speaker_id": "1",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100033151.1.15",
          "form": "처음엔 이것저것 챙길 것도 많고 환수도 힘든데 적응하면 쉬워요",
          "original_form": "처음엔 이것저것 챙길 것도 많고 환수도 힘든데 적응하면 쉬워요",
          "speaker_id": "1",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100033151.1.16",
          "form": "오 글쿠낭 완전 물 다 가는건 안하나요?",
          "original_form": "오 글쿠낭 완전 물 다 가는건 안하나요?",
          "speaker_id": "2",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100033151.1.17",
          "form": "환수는 완전 몰라서ㅋㅋㅋㅋㅜ",
          "original_form": "환수는 완전 몰라서ㅋㅋㅋㅋㅜ",
          "speaker_id": "2",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100033151.1.18",
          "form": "전체환수는 보통 한두달에 한번 해요!!",
          "original_form": "전체환수는 보통 한두달에 한번 해요!!",
          "speaker_id": "1",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100033151.1.19",
          "form": "자주하면 오히려 안좋아서ㅠㅠ",
          "original_form": "자주하면 오히려 안좋아서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100033151.1.20",
          "form": "새 물에 적응하는 것도 은근 스트레스라..",
          "original_form": "새 물에 적응하는 것도 은근 스트레스라..",
          "speaker_id": "1",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100033151.1.21",
          "form": "헐 그래요?? 신기하다 스트레스 받는군여,,",
          "original_form": "헐 그래요?? 신기하다 스트레스 받는군여,,",
          "speaker_id": "2",
          "time": "20210906 18:21"
        },
        {
          "id": "MDRW2100033151.1.22",
          "form": "네ㅠㅠ 그래서 환수한 물에 적응 못하고 용궁으로 떠나는 경우도 있어요..",
          "original_form": "네ㅠㅠ 그래서 환수한 물에 적응 못하고 용궁으로 떠나는 경우도 있어요..",
          "speaker_id": "1",
          "time": "20210906 18:23"
        },
        {
          "id": "MDRW2100033151.1.23",
          "form": "헉 아이고ㅜㅠㅠ",
          "original_form": "헉 아이고ㅜㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 18:24"
        },
        {
          "id": "MDRW2100033151.1.24",
          "form": "용궁에서 잘 지내길ㅜ",
          "original_form": "용궁에서 잘 지내길ㅜ",
          "speaker_id": "2",
          "time": "20210906 18:25"
        },
        {
          "id": "MDRW2100033151.1.25",
          "form": "열대어들이 예민한 경우가 많아서 환경이 급작스럽게 변하면 안 좋더라구요..",
          "original_form": "열대어들이 예민한 경우가 많아서 환경이 급작스럽게 변하면 안 좋더라구요..",
          "speaker_id": "1",
          "time": "20210906 18:26"
        }
      ]
