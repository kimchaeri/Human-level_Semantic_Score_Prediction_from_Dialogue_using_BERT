      "utterance": [
        {
          "id": "MDRW2100021593.1.1",
          "form": "네 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:45"
        },
        {
          "id": "MDRW2100021593.1.2",
          "form": "신선해요?",
          "original_form": "신선해요?",
          "speaker_id": "2",
          "time": "20210828 20:45"
        },
        {
          "id": "MDRW2100021593.1.3",
          "form": "네 제가 지금 서울에 살고 있어서 ㅎㅎ",
          "original_form": "네 제가 지금 서울에 살고 있어서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 20:50"
        },
        {
          "id": "MDRW2100021593.1.4",
          "form": "계획을 2년동안이나 세웠다고 하니 신기해요!",
          "original_form": "계획을 2년동안이나 세웠다고 하니 신기해요!",
          "speaker_id": "1",
          "time": "20210828 20:50"
        },
        {
          "id": "MDRW2100021593.1.5",
          "form": "아 여름방학때 여기가야지 하면 코로나",
          "original_form": "아 여름방학때 여기가야지 하면 코로나",
          "speaker_id": "2",
          "time": "20210828 20:52"
        },
        {
          "id": "MDRW2100021593.1.6",
          "form": "겨울 방학때 여기가야지 하면 코로나",
          "original_form": "겨울 방학때 여기가야지 하면 코로나",
          "speaker_id": "2",
          "time": "20210828 20:52"
        },
        {
          "id": "MDRW2100021593.1.7",
          "form": "이게 2년째 반복이라서요 ㅋㅋㅋ",
          "original_form": "이게 2년째 반복이라서요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:52"
        },
        {
          "id": "MDRW2100021593.1.8",
          "form": "헉 그래서 2년째구나",
          "original_form": "헉 그래서 2년째구나",
          "speaker_id": "1",
          "time": "20210828 20:55"
        },
        {
          "id": "MDRW2100021593.1.9",
          "form": "서울 어디 구경해보고 싶어요?",
          "original_form": "서울 어디 구경해보고 싶어요?",
          "speaker_id": "1",
          "time": "20210828 20:55"
        },
        {
          "id": "MDRW2100021593.1.10",
          "form": "사실 전 연애가 서울 사람과 한 6년동안 장거리연애여서",
          "original_form": "사실 전 연애가 서울 사람과 한 6년동안 장거리연애여서",
          "speaker_id": "2",
          "time": "20210828 20:56"
        },
        {
          "id": "MDRW2100021593.1.11",
          "form": "가볼만한덴 다 가봤고ㅋㅋㅋ",
          "original_form": "가볼만한덴 다 가봤고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100021593.1.12",
          "form": "전시회랑 박물관 이런데 가려구요",
          "original_form": "전시회랑 박물관 이런데 가려구요",
          "speaker_id": "2",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100021593.1.13",
          "form": "앗 진짜요?",
          "original_form": "앗 진짜요?",
          "speaker_id": "1",
          "time": "20210828 20:58"
        },
        {
          "id": "MDRW2100021593.1.14",
          "form": "그럼 예술의 전당도 가보셨어요?",
          "original_form": "그럼 예술의 전당도 가보셨어요?",
          "speaker_id": "1",
          "time": "20210828 20:58"
        },
        {
          "id": "MDRW2100021593.1.15",
          "form": "넹ㅋㅋㅋ 한번씩 전시회 간다고 서울 종종 갔어요",
          "original_form": "넹ㅋㅋㅋ 한번씩 전시회 간다고 서울 종종 갔어요",
          "speaker_id": "2",
          "time": "20210828 20:59"
        },
        {
          "id": "MDRW2100021593.1.16",
          "form": "근데 코로나땜에 놓친 전시가 많아서 아쉬워요",
          "original_form": "근데 코로나땜에 놓친 전시가 많아서 아쉬워요",
          "speaker_id": "2",
          "time": "20210828 20:59"
        },
        {
          "id": "MDRW2100021593.1.17",
          "form": "ㅎㅎ저도 코로나일 때 작년에 전시회는 종종 봤었네요",
          "original_form": "ㅎㅎ저도 코로나일 때 작년에 전시회는 종종 봤었네요",
          "speaker_id": "1",
          "time": "20210828 21:03"
        },
        {
          "id": "MDRW2100021593.1.18",
          "form": "박물관은 여러군데 있던데 어디 가고 싶으세요?",
          "original_form": "박물관은 여러군데 있던데 어디 가고 싶으세요?",
          "speaker_id": "1",
          "time": "20210828 21:03"
        },
        {
          "id": "MDRW2100021593.1.19",
          "form": "중앙박물관이요!",
          "original_form": "중앙박물관이요!",
          "speaker_id": "2",
          "time": "20210828 21:04"
        },
        {
          "id": "MDRW2100021593.1.20",
          "form": "이건희 소장품들 좀 보러가고싶었는데 ㅋㅋㅋ",
          "original_form": "이건희 소장품들 좀 보러가고싶었는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:04"
        },
        {
          "id": "MDRW2100021593.1.21",
          "form": "앗 이건희 소장품들이 거기에 있어요?",
          "original_form": "앗 이건희 소장품들이 거기에 있어요?",
          "speaker_id": "1",
          "time": "20210828 21:09"
        },
        {
          "id": "MDRW2100021593.1.22",
          "form": "그 미술작품들이요?",
          "original_form": "그 미술작품들이요?",
          "speaker_id": "1",
          "time": "20210828 21:09"
        },
        {
          "id": "MDRW2100021593.1.23",
          "form": "네네 특별전시로 하고있어요ㅋㅋㅋ",
          "original_form": "네네 특별전시로 하고있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:09"
        },
        {
          "id": "MDRW2100021593.1.24",
          "form": "나중에 이건희 미술관 지으면 거기로 가겠지만",
          "original_form": "나중에 이건희 미술관 지으면 거기로 가겠지만",
          "speaker_id": "2",
          "time": "20210828 21:10"
        },
        {
          "id": "MDRW2100021593.1.25",
          "form": "이건희 미술관은 어디로 지은데요?",
          "original_form": "이건희 미술관은 어디로 지은데요?",
          "speaker_id": "1",
          "time": "20210828 21:11"
        },
        {
          "id": "MDRW2100021593.1.26",
          "form": "나중에 저도 거기로 미술여행 떠나봐야겠어요~",
          "original_form": "나중에 저도 거기로 미술여행 떠나봐야겠어요~",
          "speaker_id": "1",
          "time": "20210828 21:11"
        },
        {
          "id": "MDRW2100021593.1.27",
          "form": "뭐 여기저기 서로 유치해달라고 싸우더니",
          "original_form": "뭐 여기저기 서로 유치해달라고 싸우더니",
          "speaker_id": "2",
          "time": "20210828 21:11"
        },
        {
          "id": "MDRW2100021593.1.28",
          "form": "결국 서울에 생긴다고 들었어여 ㅋㅋㅋ",
          "original_form": "결국 서울에 생긴다고 들었어여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:11"
        },
        {
          "id": "MDRW2100021593.1.29",
          "form": "낯선동네라 이름은 첨 듣는 동네였어요",
          "original_form": "낯선동네라 이름은 첨 듣는 동네였어요",
          "speaker_id": "2",
          "time": "20210828 21:11"
        }
      ]
