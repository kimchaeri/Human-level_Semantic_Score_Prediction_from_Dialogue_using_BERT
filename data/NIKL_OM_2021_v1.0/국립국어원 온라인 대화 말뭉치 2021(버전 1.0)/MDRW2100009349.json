      "utterance": [
        {
          "id": "MDRW2100009349.1.1",
          "form": "맛블로그 하세요",
          "original_form": "맛블로그 하세요",
          "speaker_id": "2",
          "time": "20210822 22:52"
        },
        {
          "id": "MDRW2100009349.1.2",
          "form": "맛집 정보가 아까워요",
          "original_form": "맛집 정보가 아까워요",
          "speaker_id": "2",
          "time": "20210822 22:52"
        },
        {
          "id": "MDRW2100009349.1.3",
          "form": "맛집과 곁들인 잼에 쿠폰정보까지",
          "original_form": "맛집과 곁들인 잼에 쿠폰정보까지",
          "speaker_id": "2",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100009349.1.4",
          "form": "아니 그정도의 열정은 없달까 ㅋㅋ",
          "original_form": "아니 그정도의 열정은 없달까 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:55"
        },
        {
          "id": "MDRW2100009349.1.5",
          "form": "다알게 되서 넘 좋았어요",
          "original_form": "다알게 되서 넘 좋았어요",
          "speaker_id": "2",
          "time": "20210822 22:56"
        },
        {
          "id": "MDRW2100009349.1.6",
          "form": "저 버거킹 깔고 이제 앱에서ㅠ시키겠어요",
          "original_form": "저 버거킹 깔고 이제 앱에서ㅠ시키겠어요",
          "speaker_id": "2",
          "time": "20210822 22:56"
        },
        {
          "id": "MDRW2100009349.1.7",
          "form": "사진 찍는거도 귀찮고",
          "original_form": "사진 찍는거도 귀찮고",
          "speaker_id": "1",
          "time": "20210822 22:56"
        },
        {
          "id": "MDRW2100009349.1.8",
          "form": "저도 사진은 ㅡㅡ싫으네요",
          "original_form": "저도 사진은 ㅡㅡ싫으네요",
          "speaker_id": "2",
          "time": "20210822 22:56"
        },
        {
          "id": "MDRW2100009349.1.9",
          "form": "딜리버리 할인할때도있어요 얼마이상시키면",
          "original_form": "딜리버리 할인할때도있어요 얼마이상시키면",
          "speaker_id": "1",
          "time": "20210822 22:56"
        },
        {
          "id": "MDRW2100009349.1.10",
          "form": "햄버거는 못끊어서요 ㅋㅋ",
          "original_form": "햄버거는 못끊어서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:56"
        },
        {
          "id": "MDRW2100009349.1.11",
          "form": "저희는 3세트 이상이라 참곺하께요",
          "original_form": "저희는 3세트 이상이라 참곺하께요",
          "speaker_id": "2",
          "time": "20210822 22:57"
        },
        {
          "id": "MDRW2100009349.1.12",
          "form": "할께요",
          "original_form": "할께요",
          "speaker_id": "2",
          "time": "20210822 22:57"
        },
        {
          "id": "MDRW2100009349.1.13",
          "form": "그리고 좀 맛집블로그 이런건 미식가들이하시는건가 싶기도하고 ㅋㅋ",
          "original_form": "그리고 좀 맛집블로그 이런건 미식가들이하시는건가 싶기도하고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:59"
        },
        {
          "id": "MDRW2100009349.1.14",
          "form": "에이 미식사세요",
          "original_form": "에이 미식사세요",
          "speaker_id": "2",
          "time": "20210822 23:00"
        },
        {
          "id": "MDRW2100009349.1.15",
          "form": "가세요",
          "original_form": "가세요",
          "speaker_id": "2",
          "time": "20210822 23:01"
        },
        {
          "id": "MDRW2100009349.1.16",
          "form": "전 맛도 몰라요",
          "original_form": "전 맛도 몰라요",
          "speaker_id": "2",
          "time": "20210822 23:01"
        },
        {
          "id": "MDRW2100009349.1.17",
          "form": "다맛있다 주의라",
          "original_form": "다맛있다 주의라",
          "speaker_id": "2",
          "time": "20210822 23:01"
        },
        {
          "id": "MDRW2100009349.1.18",
          "form": "전 좀 가격비교 이런거 좋아해요",
          "original_form": "전 좀 가격비교 이런거 좋아해요",
          "speaker_id": "1",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009349.1.19",
          "form": "짜잘하게",
          "original_form": "짜잘하게",
          "speaker_id": "1",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009349.1.20",
          "form": "그래야해요 맛집도 가격 중요하죠",
          "original_form": "그래야해요 맛집도 가격 중요하죠",
          "speaker_id": "2",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009349.1.21",
          "form": "그런의미에서 스벅은 좀 ㅋㅋ",
          "original_form": "그런의미에서 스벅은 좀 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009349.1.22",
          "form": "제입장에선 양아치같아요",
          "original_form": "제입장에선 양아치같아요",
          "speaker_id": "1",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009349.1.23",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009349.1.24",
          "form": "저도 공짜라 먹어요 ㅋㅋ",
          "original_form": "저도 공짜라 먹어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009349.1.25",
          "form": "사실 커피의 맛이란게 그렇게 뭐 대단히 다른감 싶어요",
          "original_form": "사실 커피의 맛이란게 그렇게 뭐 대단히 다른감 싶어요",
          "speaker_id": "1",
