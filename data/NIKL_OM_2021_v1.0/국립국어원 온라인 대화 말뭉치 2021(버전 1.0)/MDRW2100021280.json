      "utterance": [
        {
          "id": "MDRW2100021280.1.1",
          "form": "과제 할 시즌 아니라요??",
          "original_form": "과제 할 시즌 아니라요??",
          "speaker_id": "2",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100021280.1.2",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100021280.1.3",
          "form": "걍 갑시다 고고 ㅋㅋㅋㅋ",
          "original_form": "걍 갑시다 고고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100021280.1.4",
          "form": "미치긋네ㅋㅋ",
          "original_form": "미치긋네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100021280.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ과제끝나면 기말 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ과제끝나면 기말 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100021280.1.6",
          "form": "ㅡㅡ",
          "original_form": "ㅡㅡ",
          "speaker_id": "1",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100021280.1.7",
          "form": "책 많이 읽히란다",
          "original_form": "책 많이 읽히란다",
          "speaker_id": "1",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100021280.1.8",
          "form": "학업스트레스ㅋ",
          "original_form": "학업스트레스ㅋ",
          "speaker_id": "1",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100021280.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ책읽는것도 스트레스 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ책읽는것도 스트레스 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100021280.1.10",
          "form": "난 요즘 그 학습지에있는 지문이라도 단디.읽으라캐요ㅠㅋㅋㅋㅋㅋㅋ",
          "original_form": "난 요즘 그 학습지에있는 지문이라도 단디.읽으라캐요ㅠㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100021280.1.11",
          "form": "잘하고있네",
          "original_form": "잘하고있네",
          "speaker_id": "1",
          "time": "20210828 17:18"
        },
        {
          "id": "MDRW2100021280.1.12",
          "form": "7살에 평생 읽을 책을 다읽은듯..",
          "original_form": "7살에 평생 읽을 책을 다읽은듯..",
          "speaker_id": "2",
          "time": "20210828 17:18"
        },
        {
          "id": "MDRW2100021280.1.13",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 17:18"
        },
        {
          "id": "MDRW2100021280.1.14",
          "form": "그뒤로는 질렸나보ㅏ여 ㅋㅋㅋㅋㅋ",
          "original_form": "그뒤로는 질렸나보ㅏ여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:18"
        },
        {
          "id": "MDRW2100021280.1.15",
          "form": "우리도책읽자",
          "original_form": "우리도책읽자",
          "speaker_id": "1",
          "time": "20210828 17:18"
        },
        {
          "id": "MDRW2100021280.1.16",
          "form": "일주일에한권씩",
          "original_form": "일주일에한권씩",
          "speaker_id": "1",
          "time": "20210828 17:18"
        },
        {
          "id": "MDRW2100021280.1.17",
          "form": "안그래도 책에대해서 얘기하면..",
          "original_form": "안그래도 책에대해서 얘기하면..",
          "speaker_id": "2",
          "time": "20210828 17:19"
        },
        {
          "id": "MDRW2100021280.1.18",
          "form": "애들 책 읽어주는것밖에;;",
          "original_form": "애들 책 읽어주는것밖에;;",
          "speaker_id": "2",
          "time": "20210828 17:19"
        },
        {
          "id": "MDRW2100021280.1.19",
          "form": "그래 니는 독서를 하고있었구나ㅋ",
          "original_form": "그래 니는 독서를 하고있었구나ㅋ",
          "speaker_id": "1",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100021280.1.20",
          "form": "그쵸~언니네 애들은 각자 읽으니 ㅋㅋㅋㅋ",
          "original_form": "그쵸~언니네 애들은 각자 읽으니 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:35"
        },
        {
          "id": "MDRW2100021280.1.21",
          "form": "언니 그래도 자격증 공부하니깐여 ㅋㅋㅋ",
          "original_form": "언니 그래도 자격증 공부하니깐여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:35"
        },
        {
          "id": "MDRW2100021280.1.22",
          "form": "자격증공부에 책읽기는 엄써ㅋ",
          "original_form": "자격증공부에 책읽기는 엄써ㅋ",
          "speaker_id": "1",
          "time": "20210828 17:36"
        },
        {
          "id": "MDRW2100021280.1.23",
          "form": "월요일 은근 월요병있다나는",
          "original_form": "월요일 은근 월요병있다나는",
          "speaker_id": "1",
          "time": "20210828 17:37"
        },
        {
          "id": "MDRW2100021280.1.24",
          "form": "언니신랑도 있잖아여 ㅋㅋㅋㅋ",
          "original_form": "언니신랑도 있잖아여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:45"
        },
        {
          "id": "MDRW2100021280.1.25",
          "form": "부창부수 ㅋㅋㅋㅋ",
          "original_form": "부창부수 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:45"
        },
        {
          "id": "MDRW2100021280.1.26",
          "form": "모두에게있군ㅋㅋ",
          "original_form": "모두에게있군ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:52"
        },
        {
          "id": "MDRW2100021280.1.27",
          "form": "첫째학교선생님 바뀐단다ㅡㅡ",
          "original_form": "첫째학교선생님 바뀐단다ㅡㅡ",
          "speaker_id": "1",
          "time": "20210828 17:52"
        },
        {
          "id": "MDRW2100021280.1.28",
          "form": "시어머니아프다고",
          "original_form": "시어머니아프다고",
          "speaker_id": "1",
          "time": "20210828 17:52"
        },
        {
          "id": "MDRW2100021280.1.29",
          "form": "학기 중에 맡길 수가 있어요",
          "original_form": "학기 중에 맡길 수가 있어요",
          "speaker_id": "2",
          "time": "20210828 17:53"
        },
        {
          "id": "MDRW2100021280.1.30",
          "form": "바뀔 수가",
          "original_form": "바뀔 수가",
          "speaker_id": "2",
          "time": "20210828 17:53"
        },
        {
          "id": "MDRW2100021280.1.31",
