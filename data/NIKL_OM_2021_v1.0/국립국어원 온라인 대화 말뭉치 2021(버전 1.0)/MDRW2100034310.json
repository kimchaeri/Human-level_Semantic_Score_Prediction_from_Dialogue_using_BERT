      "utterance": [
        {
          "id": "MDRW2100034310.1.1",
          "form": "name2님은 먹는 거 좋아하세요!?",
          "original_form": "&name2&님은 먹는 거 좋아하세요!?",
          "speaker_id": "1",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100034310.1.2",
          "form": "먹는 낙으로 살아요 ㅋㅋㅋ",
          "original_form": "먹는 낙으로 살아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100034310.1.3",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100034310.1.4",
          "form": "네!! 저 특히 떡볶이 엄청 좋아해요",
          "original_form": "네!! 저 특히 떡볶이 엄청 좋아해요",
          "speaker_id": "1",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100034310.1.5",
          "form": "오어디거요??",
          "original_form": "오어디거요??",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100034310.1.6",
          "form": "저 태리로제 떡볶이 왕 좋아하고!",
          "original_form": "저 태리로제 떡볶이 왕 좋아하고!",
          "speaker_id": "1",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100034310.1.7",
          "form": "응급실 떡볶이 좋아해요 ㅎㅎ!!!!!!!!",
          "original_form": "응급실 떡볶이 좋아해요 ㅎㅎ!!!!!!!!",
          "speaker_id": "1",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100034310.1.8",
          "form": "태리로제 인기 많더라구요!",
          "original_form": "태리로제 인기 많더라구요!",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100034310.1.9",
          "form": "name2님도 좋아하시나요 ㅎ.ㅎ ?",
          "original_form": "&name2&님도 좋아하시나요 ㅎ.ㅎ ?",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100034310.1.10",
          "form": "맞어요 ㅎㅎ",
          "original_form": "맞어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100034310.1.11",
          "form": "태리는 못먹어봤어요 ㅜㅜ",
          "original_form": "태리는 못먹어봤어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100034310.1.12",
          "form": "헛 그럼 어디 거 자주 드세요 ~?",
          "original_form": "헛 그럼 어디 거 자주 드세요 ~?",
          "speaker_id": "1",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100034310.1.13",
          "form": "저는 신전이요!",
          "original_form": "저는 신전이요!",
          "speaker_id": "2",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100034310.1.14",
          "form": "드셔보셨어요?",
          "original_form": "드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100034310.1.15",
          "form": "헐 네!! 저 신전 좋아해요",
          "original_form": "헐 네!! 저 신전 좋아해요",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100034310.1.16",
          "form": "자취할 때 다른 집은 떡볶이가 너무 많아서 신전으로 자주 먹었거든요 ㅎㅎ",
          "original_form": "자취할 때 다른 집은 떡볶이가 너무 많아서 신전으로 자주 먹었거든요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100034310.1.17",
          "form": "오맞아요 ㅜㅜ",
          "original_form": "오맞아요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100034310.1.18",
          "form": "1인용으로 딱 좋아요 다른데는 너무 많죠",
          "original_form": "1인용으로 딱 좋아요 다른데는 너무 많죠",
          "speaker_id": "2",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100034310.1.19",
          "form": "네... 게다가 사이드도 먹을 수 있어서 너무 좋아요!",
          "original_form": "네... 게다가 사이드도 먹을 수 있어서 너무 좋아요!",
          "speaker_id": "1",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100034310.1.20",
          "form": "딴 곳은 배불러서 못 먹거든요... ㅎㅎ",
          "original_form": "딴 곳은 배불러서 못 먹거든요... ㅎㅎ",
