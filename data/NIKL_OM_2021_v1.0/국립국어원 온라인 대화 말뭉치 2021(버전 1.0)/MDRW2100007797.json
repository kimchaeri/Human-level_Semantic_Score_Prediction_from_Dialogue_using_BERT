      "utterance": [
        {
          "id": "MDRW2100007797.1.1",
          "form": "저는 먹방보면서",
          "original_form": "저는 먹방보면서",
          "speaker_id": "2",
          "time": "20210822 10:52"
        },
        {
          "id": "MDRW2100007797.1.2",
          "form": "대리만족해요",
          "original_form": "대리만족해요",
          "speaker_id": "2",
          "time": "20210822 10:52"
        },
        {
          "id": "MDRW2100007797.1.3",
          "form": "먹방좋아하시는구사",
          "original_form": "먹방좋아하시는구사",
          "speaker_id": "1",
          "time": "20210822 10:53"
        },
        {
          "id": "MDRW2100007797.1.4",
          "form": "저는입이짧아서많이못먹는데",
          "original_form": "저는입이짧아서많이못먹는데",
          "speaker_id": "2",
          "time": "20210822 10:53"
        },
        {
          "id": "MDRW2100007797.1.5",
          "form": "엄청잘먹더라구요",
          "original_form": "엄청잘먹더라구요",
          "speaker_id": "2",
          "time": "20210822 10:53"
        },
        {
          "id": "MDRW2100007797.1.6",
          "form": "주로 어떤 먹방보세요",
          "original_form": "주로 어떤 먹방보세요",
          "speaker_id": "1",
          "time": "20210822 10:53"
        },
        {
          "id": "MDRW2100007797.1.7",
          "form": "그날그날먹고싶은음식검색해서",
          "original_form": "그날그날먹고싶은음식검색해서",
          "speaker_id": "2",
          "time": "20210822 10:53"
        },
        {
          "id": "MDRW2100007797.1.8",
          "form": "봐요ㅋㅋ",
          "original_form": "봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:53"
        },
        {
          "id": "MDRW2100007797.1.9",
          "form": "최근에 본 먹방은 무엇인가요",
          "original_form": "최근에 본 먹방은 무엇인가요",
          "speaker_id": "1",
          "time": "20210822 10:53"
        },
        {
          "id": "MDRW2100007797.1.10",
          "form": "옛날통닭이요",
          "original_form": "옛날통닭이요",
          "speaker_id": "2",
          "time": "20210822 10:54"
        },
        {
          "id": "MDRW2100007797.1.11",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "1",
          "time": "20210822 10:54"
        },
        {
          "id": "MDRW2100007797.1.12",
          "form": "약간 바삭한 소리나는것도 좋고",
          "original_form": "약간 바삭한 소리나는것도 좋고",
          "speaker_id": "1",
          "time": "20210822 10:54"
        },
        {
          "id": "MDRW2100007797.1.13",
          "form": "옛날 통닭하면 통으로 튀긴거잖아요",
          "original_form": "옛날 통닭하면 통으로 튀긴거잖아요",
          "speaker_id": "1",
          "time": "20210822 10:54"
        },
        {
          "id": "MDRW2100007797.1.14",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210822 10:54"
        },
        {
          "id": "MDRW2100007797.1.15",
          "form": "유튜브에는잘려서나온것도있더라구요",
          "original_form": "유튜브에는잘려서나온것도있더라구요",
          "speaker_id": "2",
          "time": "20210822 10:55"
        },
        {
          "id": "MDRW2100007797.1.16",
          "form": "어때요 그 날도 대리만족이 되셨나요",
          "original_form": "어때요 그 날도 대리만족이 되셨나요",
          "speaker_id": "1",
          "time": "20210822 10:55"
        },
        {
          "id": "MDRW2100007797.1.17",
          "form": "네 당연하죠",
          "original_form": "네 당연하죠",
          "speaker_id": "2",
          "time": "20210822 10:55"
        },
        {
          "id": "MDRW2100007797.1.18",
          "form": "유투브한번들어가면 개미지옥이예요",
          "original_form": "유투브한번들어가면 개미지옥이예요",
          "speaker_id": "2",
          "time": "20210822 10:55"
        },
        {
          "id": "MDRW2100007797.1.19",
          "form": "알고리즘으로계속떠서",
          "original_form": "알고리즘으로계속떠서",
          "speaker_id": "2",
          "time": "20210822 10:55"
        },
        {
          "id": "MDRW2100007797.1.20",
          "form": "맞아요 진짜 다양하게 쏟아져 나오는거같아요",
          "original_form": "맞아요 진짜 다양하게 쏟아져 나오는거같아요",
          "speaker_id": "1",
          "time": "20210822 10:56"
        },
        {
          "id": "MDRW2100007797.1.21",
          "form": "티비가필요없어요 액기스만보는게더재밌어요",
          "original_form": "티비가필요없어요 액기스만보는게더재밌어요",
          "speaker_id": "2",
          "time": "20210822 10:56"
        }
      ]
