      "utterance": [
        {
          "id": "MDRW2100017119.1.1",
          "form": "새우튀김 정말 좋아해요",
          "original_form": "새우튀김 정말 좋아해요",
          "speaker_id": "2",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100017119.1.2",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100017119.1.3",
          "form": "새우튀김 좋아하시는구나",
          "original_form": "새우튀김 좋아하시는구나",
          "speaker_id": "1",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100017119.1.4",
          "form": "떡볶이 좋아하세요?",
          "original_form": "떡볶이 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100017119.1.5",
          "form": "국물을 좋아해요",
          "original_form": "국물을 좋아해요",
          "speaker_id": "2",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100017119.1.6",
          "form": "떡은 잘 안먹어요",
          "original_form": "떡은 잘 안먹어요",
          "speaker_id": "2",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100017119.1.7",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100017119.1.8",
          "form": "저는 떡은 별로고",
          "original_form": "저는 떡은 별로고",
          "speaker_id": "1",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100017119.1.9",
          "form": "요즘 로제떡볶이가 대세더라구요",
          "original_form": "요즘 로제떡볶이가 대세더라구요",
          "speaker_id": "2",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100017119.1.10",
          "form": "어묵을 좋아해요",
          "original_form": "어묵을 좋아해요",
          "speaker_id": "1",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100017119.1.11",
          "form": "아아 저도 어묵파!",
          "original_form": "아아 저도 어묵파!",
          "speaker_id": "2",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100017119.1.12",
          "form": "어 통했네요!",
          "original_form": "어 통했네요!",
          "speaker_id": "1",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017119.1.13",
          "form": "부산어묵....",
          "original_form": "부산어묵....",
          "speaker_id": "2",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017119.1.14",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017119.1.15",
          "form": "고래사어묵 드셔보셨어요?",
          "original_form": "고래사어묵 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017119.1.16",
          "form": "과자도 어묵? 말린 거?",
          "original_form": "과자도 어묵? 말린 거?",
          "speaker_id": "2",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017119.1.17",
          "form": "오 처음 들어봐요",
          "original_form": "오 처음 들어봐요",
          "speaker_id": "2",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017119.1.18",
          "form": "고래사어묵이 뭐예요?",
          "original_form": "고래사어묵이 뭐예요?",
          "speaker_id": "2",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017119.1.19",
          "form": "약간 핫바같은 느낌인데 맛있어요 언제 한번 드셔보세요",
          "original_form": "약간 핫바같은 느낌인데 맛있어요 언제 한번 드셔보세요",
          "speaker_id": "1",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017119.1.20",
          "form": "오오 그럴게요",
          "original_form": "오오 그럴게요",
          "speaker_id": "2",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017119.1.21",
          "form": "단점은.. 어묵치고 너무 비사다는거",
          "original_form": "단점은.. 어묵치고 너무 비사다는거",
          "speaker_id": "1",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017119.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ새우튀김 느낌이네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ새우튀김 느낌이네요",
          "speaker_id": "2",
          "time": "20210826 17:32"
        },
        {
          "id": "MDRW2100017119.1.23",
          "form": "맛있으면 다 비싸더라구요",
          "original_form": "맛있으면 다 비싸더라구요",
          "speaker_id": "2",
          "time": "20210826 17:32"
        },
        {
          "id": "MDRW2100017119.1.24",
          "form": "그렇게 되는건가요ㅋㅋ",
          "original_form": "그렇게 되는건가요ㅋㅋ",
          "speaker_id": "1",
