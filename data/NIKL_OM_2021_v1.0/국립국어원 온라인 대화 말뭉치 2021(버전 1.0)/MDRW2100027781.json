      "utterance": [
        {
          "id": "MDRW2100027781.1.1",
          "form": "안녕~ 음식 뭐 좋아해?",
          "original_form": "안녕~ 음식 뭐 좋아해?",
          "speaker_id": "1",
          "time": "20210902 15:02"
        },
        {
          "id": "MDRW2100027781.1.2",
          "form": "나는 치킨!",
          "original_form": "나는 치킨!",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027781.1.3",
          "form": "너는 뭐 좋아해?",
          "original_form": "너는 뭐 좋아해?",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027781.1.4",
          "form": "오 나는 피자파당",
          "original_form": "오 나는 피자파당",
          "speaker_id": "1",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027781.1.5",
          "form": "치킨은 잘 안머거",
          "original_form": "치킨은 잘 안머거",
          "speaker_id": "1",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027781.1.6",
          "form": "피자파랑 치킨파 은근 갈리더라구",
          "original_form": "피자파랑 치킨파 은근 갈리더라구",
          "speaker_id": "2",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100027781.1.7",
          "form": "근데 사실 난 둘 다 좋아해 ㅋㅋㅋ",
          "original_form": "근데 사실 난 둘 다 좋아해 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100027781.1.8",
          "form": "구징 나는 그래서 치킨 시키면 많이 양보해 ㅋㅋㅋ",
          "original_form": "구징 나는 그래서 치킨 시키면 많이 양보해 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100027781.1.9",
          "form": "오오 둘다 잘 먹는구나",
          "original_form": "오오 둘다 잘 먹는구나",
          "speaker_id": "1",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100027781.1.10",
          "form": "닭다리 vs 퍽퍽살 ?!!",
          "original_form": "닭다리 vs 퍽퍽살 ?!!",
          "speaker_id": "2",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100027781.1.11",
          "form": "으으음 둘다 안먹지만 퍽퍽살?!",
          "original_form": "으으음 둘다 안먹지만 퍽퍽살?!",
          "speaker_id": "1",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100027781.1.12",
          "form": "너는??",
          "original_form": "너는??",
          "speaker_id": "1",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100027781.1.13",
          "form": "헐 나두 퍽퍽살",
          "original_form": "헐 나두 퍽퍽살",
          "speaker_id": "2",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100027781.1.14",
          "form": "난 퍽퍽한거 진짜 좋아하거든",
          "original_form": "난 퍽퍽한거 진짜 좋아하거든",
          "speaker_id": "2",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100027781.1.15",
          "form": "헐 완전 신기",
          "original_form": "헐 완전 신기",
          "speaker_id": "1",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100027781.1.16",
          "form": "퍽퍽파였구나",
          "original_form": "퍽퍽파였구나",
          "speaker_id": "1",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100027781.1.17",
          "form": "그래서 다이어트 안 하는데 종종 닭가슴살 팩 먹어..ㅋㅋㅋ",
          "original_form": "그래서 다이어트 안 하는데 종종 닭가슴살 팩 먹어..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100027781.1.18",
          "form": "오랜만에 보는 퍽퍽파",
          "original_form": "오랜만에 보는 퍽퍽파",
          "speaker_id": "1",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100027781.1.19",
          "form": "앜ㅋㅋㅋ 좋다좋다",
          "original_form": "앜ㅋㅋㅋ 좋다좋다",
          "speaker_id": "1",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100027781.1.20",
          "form": "무슨 피자 제일 좋아해?",
          "original_form": "무슨 피자 제일 좋아해?",
          "speaker_id": "2",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027781.1.21",
          "form": "나는 포테이토!!",
          "original_form": "나는 포테이토!!",
          "speaker_id": "1",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027781.1.22",
          "form": "감자가 너무 좋아",
          "original_form": "감자가 너무 좋아",
          "speaker_id": "1",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027781.1.23",
          "form": "허얼 포테이토 좋지",
          "original_form": "허얼 포테이토 좋지",
          "speaker_id": "2",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027781.1.24",
          "form": "베이컨포테이토",
          "original_form": "베이컨포테이토",
          "speaker_id": "2",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027781.1.25",
          "form": "감자음식 다 좋아해",
          "original_form": "감자음식 다 좋아해",
          "speaker_id": "1",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027781.1.26",
          "form": "난 고구마피자도 좋아해",
          "original_form": "난 고구마피자도 좋아해",
          "speaker_id": "2",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027781.1.27",
          "form": "넘 좋징",
          "original_form": "넘 좋징",
          "speaker_id": "1",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027781.1.28",
          "form": "구황작물 진짜 맛있어",
          "original_form": "구황작물 진짜 맛있어",
          "speaker_id": "2",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027781.1.29",
          "form": "고구마도 맛나 ㅠㅠㅠ",
          "original_form": "고구마도 맛나 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027781.1.30",
          "form": "앜ㅋㅋㅋㅋ 구황작물 오랜만에 듣는 단어당",
          "original_form": "앜ㅋㅋㅋㅋ 구황작물 오랜만에 듣는 단어당",
          "speaker_id": "1",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027781.1.31",
          "form": "마자... 고구마크러스트",
          "original_form": "마자... 고구마크러스트",
          "speaker_id": "2",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027781.1.32",
          "form": "진짜 맛있어!!!",
          "original_form": "진짜 맛있어!!!",
          "speaker_id": "2",
          "time": "20210902 15:08"
        },
        {
          "id": "MDRW2100027781.1.33",
          "form": "주로 어떤 피자 브랜드 즐겨 먹어?",
          "original_form": "주로 어떤 피자 브랜드 즐겨 먹어?",
          "speaker_id": "2",
          "time": "20210902 15:08"
        },
        {
          "id": "MDRW2100027781.1.34",
          "form": "고구마는 달달해서 너무 좋아 우유랑 찰떡이얌",
          "original_form": "고구마는 달달해서 너무 좋아 우유랑 찰떡이얌",
          "speaker_id": "1",
          "time": "20210902 15:08"
        },
        {
          "id": "MDRW2100027781.1.35",
          "form": "나 피자는 다 좋아해 ㅋㅋㅋㅋ",
          "original_form": "나 피자는 다 좋아해 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:08"
        },
        {
          "id": "MDRW2100027781.1.36",
          "form": "그 뭐지 유민상 어록 중에 그런거 있어",
          "original_form": "그 뭐지 유민상 어록 중에 그런거 있어",
          "speaker_id": "1",
          "time": "20210902 15:08"
        },
        {
          "id": "MDRW2100027781.1.37",
          "form": "브랜드 따지지 않고 가까운걸 먹는다",
          "original_form": "브랜드 따지지 않고 가까운걸 먹는다",
          "speaker_id": "1",
          "time": "20210902 15:08"
        },
        {
          "id": "MDRW2100027781.1.38",
          "form": "우와.. 역시 배운사람은 다르네",
          "original_form": "우와.. 역시 배운사람은 다르네",
          "speaker_id": "2",
          "time": "20210902 15:09"
        },
        {
          "id": "MDRW2100027781.1.39",
          "form": "대박이지 ㅋㅋㅋㅋ 넘 웃겨",
          "original_form": "대박이지 ㅋㅋㅋㅋ 넘 웃겨",
          "speaker_id": "1",
          "time": "20210902 15:09"
        },
        {
          "id": "MDRW2100027781.1.40",
          "form": "나도 동감해 ㅋㅋㅋ 가까운 피자집이 짱!",
          "original_form": "나도 동감해 ㅋㅋㅋ 가까운 피자집이 짱!",
          "speaker_id": "1",
          "time": "20210902 15:09"
        },
        {
          "id": "MDRW2100027781.1.41",
