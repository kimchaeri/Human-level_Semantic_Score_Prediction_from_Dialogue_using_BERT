      "utterance": [
        {
          "id": "MDRW2100032489.1.1",
          "form": "책 읽는거 좋아하세요?",
          "original_form": "책 읽는거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 17:08"
        },
        {
          "id": "MDRW2100032489.1.2",
          "form": "네네 좋아해요.. 요즘은 이북이나 웹소서을 주로 봐요 ㅎ으",
          "original_form": "네네 좋아해요.. 요즘은 이북이나 웹소서을 주로 봐요 ㅎ으",
          "speaker_id": "1",
          "time": "20210907 17:53"
        },
        {
          "id": "MDRW2100032489.1.3",
          "form": "오오 무슨 장르 읽으세요?",
          "original_form": "오오 무슨 장르 읽으세요?",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100032489.1.4",
          "form": "로맨스랑 판타지요",
          "original_form": "로맨스랑 판타지요",
          "speaker_id": "1",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100032489.1.5",
          "form": "오오 전 웹툰은 많이보는데 소설은 안읽어봣어요",
          "original_form": "오오 전 웹툰은 많이보는데 소설은 안읽어봣어요",
          "speaker_id": "2",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100032489.1.6",
          "form": "웹툰은 어떤거보세요?",
          "original_form": "웹툰은 어떤거보세요?",
          "speaker_id": "1",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100032489.1.7",
          "form": "저 되게 많이 봐여 ㅋㅋㅋ",
          "original_form": "저 되게 많이 봐여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100032489.1.8",
          "form": "네이버 웹툰만 봐여",
          "original_form": "네이버 웹툰만 봐여",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100032489.1.9",
          "form": "로맨스를 보시나요?",
          "original_form": "로맨스를 보시나요?",
          "speaker_id": "1",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100032489.1.10",
          "form": "판타지를 보시나요?",
          "original_form": "판타지를 보시나요?",
          "speaker_id": "1",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100032489.1.11",
          "form": "저는 로맨스 위주로 봐요",
          "original_form": "저는 로맨스 위주로 봐요",
          "speaker_id": "2",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100032489.1.12",
          "form": "고래별 보셧나요?",
          "original_form": "고래별 보셧나요?",
          "speaker_id": "1",
          "time": "20210908 01:55"
        },
        {
          "id": "MDRW2100032489.1.13",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100032489.1.14",
          "form": "저 지금 그거 정주행 하고잇어요",
          "original_form": "저 지금 그거 정주행 하고잇어요",
          "speaker_id": "2",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100032489.1.15",
          "form": "한 60화 보는중입니당",
          "original_form": "한 60화 보는중입니당",
          "speaker_id": "2",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100032489.1.16",
          "form": "재밌죠..?",
          "original_form": "재밌죠..?",
          "speaker_id": "1",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100032489.1.17",
          "form": "아직 보시는 중이군요ㅠㅠㅠㅠ",
          "original_form": "아직 보시는 중이군요ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100032489.1.18",
          "form": "네네 스토리가 너무 흥미진진해요",
          "original_form": "네네 스토리가 너무 흥미진진해요",
          "speaker_id": "2",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100032489.1.19",
          "form": "친구가 추천해줘서 봐야지하다가",
          "original_form": "친구가 추천해줘서 봐야지하다가",
          "speaker_id": "2",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100032489.1.20",
          "form": "유료화 곧 된다길애 얼른 보고잇어요",
          "original_form": "유료화 곧 된다길애 얼른 보고잇어요",
          "speaker_id": "2",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100032489.1.21",
          "form": "아껴서 그치만 유료화전에 다 보세요",
          "original_form": "아껴서 그치만 유료화전에 다 보세요",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100032489.1.22",
          "form": "슬프지만 재밌고 플롯도 그림도 좋아요",
          "original_form": "슬프지만 재밌고 플롯도 그림도 좋아요",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100032489.1.23",
          "form": "작가님이 표현력이 대단하시더라구요",
          "original_form": "작가님이 표현력이 대단하시더라구요",
          "speaker_id": "2",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100032489.1.24",
          "form": "네 진짜 있을것같이 잘 구성하셨어요.. 인물하나하나에 다 애정이 있으시고요",
