        {
          "id": "MDRW2100014317.1.1",
          "form": "좋아하는 옷 있나요??",
          "original_form": "좋아하는 옷 있나요??",
          "speaker_id": "2",
          "time": "20210825 19:11"
        },
        {
          "id": "MDRW2100014317.1.2",
          "form": "와이드팬츠요!",
          "original_form": "와이드팬츠요!",
          "speaker_id": "1",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100014317.1.3",
          "form": "오 저도 좋아해여",
          "original_form": "오 저도 좋아해여",
          "speaker_id": "2",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100014317.1.4",
          "form": "원피스도 좋아해요!",
          "original_form": "원피스도 좋아해요!",
          "speaker_id": "1",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014317.1.5",
          "form": "ㅋㅋㅋ ㅋㅋㅋ 전 치마는 잘 안입어요",
          "original_form": "ㅋㅋㅋ ㅋㅋㅋ 전 치마는 잘 안입어요",
          "speaker_id": "2",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014317.1.6",
          "form": "잘 입으세요?",
          "original_form": "잘 입으세요?",
          "speaker_id": "2",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014317.1.7",
          "form": "네네 원피스는 편하더라구용",
          "original_form": "네네 원피스는 편하더라구용",
          "speaker_id": "1",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014317.1.8",
          "form": "밑이 뚫려서",
          "original_form": "밑이 뚫려서",
          "speaker_id": "2",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014317.1.9",
          "form": "편하긴하죠 ㅎ",
          "original_form": "편하긴하죠 ㅎ",
          "speaker_id": "2",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014317.1.10",
          "form": "청바지도 잘입으세요?",
          "original_form": "청바지도 잘입으세요?",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014317.1.11",
          "form": "네 ㅋㅋㅋㅋ 근데",
          "original_form": "네 ㅋㅋㅋㅋ 근데",
          "speaker_id": "2",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014317.1.12",
          "form": "통 큰거만요",
          "original_form": "통 큰거만요",
          "speaker_id": "2",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014317.1.13",
          "form": "슬림한건 안좋아하세용?",
          "original_form": "슬림한건 안좋아하세용?",
          "speaker_id": "1",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014317.1.14",
          "form": "네 불편해여 ㅠㅠ",
          "original_form": "네 불편해여 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014317.1.15",
          "form": "잘 입으세요?",
          "original_form": "잘 입으세요?",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014317.1.16",
          "form": "네네 이것저것 코디하면서 입긴해요 ㅋㅋ",
          "original_form": "네네 이것저것 코디하면서 입긴해요 ㅋㅋ",
