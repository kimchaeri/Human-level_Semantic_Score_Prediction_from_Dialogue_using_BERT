      "utterance": [
        {
          "id": "MDRW2100003829.1.1",
          "form": "그래서 저녁은 치킨이야?",
          "original_form": "그래서 저녁은 치킨이야?",
          "speaker_id": "2",
          "time": "20210821 12:39"
        },
        {
          "id": "MDRW2100003829.1.2",
          "form": "응 ㅋㅋㅋ",
          "original_form": "응 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 12:39"
        },
        {
          "id": "MDRW2100003829.1.3",
          "form": "치킨 시켜야지",
          "original_form": "치킨 시켜야지",
          "speaker_id": "1",
          "time": "20210821 12:39"
        },
        {
          "id": "MDRW2100003829.1.4",
          "form": "무슨 치킨 먹을 건데",
          "original_form": "무슨 치킨 먹을 건데",
          "speaker_id": "2",
          "time": "20210821 12:39"
        },
        {
          "id": "MDRW2100003829.1.5",
          "form": "오늘은 처갓집이 땡긴다",
          "original_form": "오늘은 처갓집이 땡긴다",
          "speaker_id": "1",
          "time": "20210821 12:40"
        },
        {
          "id": "MDRW2100003829.1.6",
          "form": "처갓집 어릴 때 많이 먹었는데",
          "original_form": "처갓집 어릴 때 많이 먹었는데",
          "speaker_id": "2",
          "time": "20210821 12:40"
        },
        {
          "id": "MDRW2100003829.1.7",
          "form": "처갓집 슈프림 완전 맛있어",
          "original_form": "처갓집 슈프림 완전 맛있어",
          "speaker_id": "1",
          "time": "20210821 12:40"
        },
        {
          "id": "MDRW2100003829.1.8",
          "form": "그건 안먹어봤어",
          "original_form": "그건 안먹어봤어",
          "speaker_id": "2",
          "time": "20210821 12:40"
        },
        {
          "id": "MDRW2100003829.1.9",
          "form": "꼭 먹어봐",
          "original_form": "꼭 먹어봐",
          "speaker_id": "1",
          "time": "20210821 12:40"
        },
        {
          "id": "MDRW2100003829.1.10",
          "form": "그래야겠다",
          "original_form": "그래야겠다",
          "speaker_id": "2",
          "time": "20210821 12:40"
        },
        {
          "id": "MDRW2100003829.1.11",
          "form": "진짜 진짜 맛있어",
          "original_form": "진짜 진짜 맛있어",
          "speaker_id": "1",
          "time": "20210821 12:41"
        },
        {
          "id": "MDRW2100003829.1.12",
          "form": "집 근처에는 없어서 못먹었어",
          "original_form": "집 근처에는 없어서 못먹었어",
          "speaker_id": "2",
          "time": "20210821 12:41"
        },
        {
          "id": "MDRW2100003829.1.13",
          "form": "근데 좀 먹다보면 느끼해서",
          "original_form": "근데 좀 먹다보면 느끼해서",
          "speaker_id": "1",
          "time": "20210821 12:41"
        },
        {
          "id": "MDRW2100003829.1.14",
          "form": "느끼해?",
          "original_form": "느끼해?",
          "speaker_id": "2",
          "time": "20210821 12:41"
        },
        {
          "id": "MDRW2100003829.1.15",
          "form": "위에 하얀 소스가 먹다보면 좀 느끼",
          "original_form": "위에 하얀 소스가 먹다보면 좀 느끼",
