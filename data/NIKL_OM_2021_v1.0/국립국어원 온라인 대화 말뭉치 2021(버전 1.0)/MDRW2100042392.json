        {
          "id": "MDRW2100042392.1.1",
          "form": "자체발광그녀 출연진은 별로였나",
          "original_form": "자체발광그녀 출연진은 별로였나",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042392.1.2",
          "form": "누가 나왔어요?",
          "original_form": "누가 나왔어요?",
          "speaker_id": "1",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042392.1.3",
          "form": "KBS드라마채널이라서 그런지 0.7%",
          "original_form": "KBS드라마채널이라서 그런지 0.7%",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042392.1.4",
          "form": "소이현 드라마는 확 뜬건 없는거 같아요",
          "original_form": "소이현 드라마는 확 뜬건 없는거 같아요",
          "speaker_id": "1",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042392.1.5",
          "form": "소이현 박광현 김형준 강세정",
          "original_form": "소이현 박광현 김형준 강세정",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042392.1.6",
          "form": "헐 0.7  이거 애국가 시청률",
          "original_form": "헐 0.7  이거 애국가 시청률",
          "speaker_id": "1",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042392.1.7",
          "form": "심하게 저조한 시청률이다",
          "original_form": "심하게 저조한 시청률이다",
          "speaker_id": "2",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042392.1.8",
          "form": "어찌 1프로도 안되죠? ㅋ",
          "original_form": "어찌 1프로도 안되죠? ㅋ",
          "speaker_id": "1",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042392.1.9",
          "form": "그래서 그런지 해피엔딩에서 조연으로",
          "original_form": "그래서 그런지 해피엔딩에서 조연으로",
          "speaker_id": "2",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042392.1.10",
          "form": "주연에서 조연으로ㅋ",
          "original_form": "주연에서 조연으로ㅋ",
          "speaker_id": "1",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042392.1.11",
          "form": "이 드라마도 시청률은 1.7%",
          "original_form": "이 드라마도 시청률은 1.7%",
          "speaker_id": "2",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042392.1.12",
          "form": "근데 이드라마도 기억에 없음",
