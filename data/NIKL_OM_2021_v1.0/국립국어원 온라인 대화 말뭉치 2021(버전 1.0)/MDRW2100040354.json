      "utterance": [
        {
          "id": "MDRW2100040354.1.1",
          "form": "집에 무슨 아이스크림 있나요!",
          "original_form": "집에 무슨 아이스크림 있나요!",
          "speaker_id": "2",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100040354.1.2",
          "form": "사러가야해요!!!! ㅜㅜㅜㅜㅜ",
          "original_form": "사러가야해요!!!! ㅜㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100040354.1.3",
          "form": "그래서때릴수가없어여..",
          "original_form": "그래서때릴수가없어여..",
          "speaker_id": "1",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100040354.1.4",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "2",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100040354.1.5",
          "form": "보니까 요기요는 홈플러스도 배달하던데",
          "original_form": "보니까 요기요는 홈플러스도 배달하던데",
          "speaker_id": "2",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100040354.1.6",
          "form": "아님 편의점이나?",
          "original_form": "아님 편의점이나?",
          "speaker_id": "2",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100040354.1.7",
          "form": "이렇게 사람이 안 움직이는 대지가 되어갑니다ㅎㅎ",
          "original_form": "이렇게 사람이 안 움직이는 대지가 되어갑니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100040354.1.8",
          "form": "와 요기요홈플도배달해요?!",
          "original_form": "와 요기요홈플도배달해요?!",
          "speaker_id": "1",
          "time": "20210904 17:00"
        },
        {
          "id": "MDRW2100040354.1.9",
          "form": "요새진짜배달너무잘되어있어서",
          "original_form": "요새진짜배달너무잘되어있어서",
          "speaker_id": "1",
          "time": "20210904 17:00"
        },
        {
          "id": "MDRW2100040354.1.10",
          "form": "나가기가싫습니당..",
          "original_form": "나가기가싫습니당..",
          "speaker_id": "1",
          "time": "20210904 17:00"
        },
        {
          "id": "MDRW2100040354.1.11",
          "form": "ㅎㅎ 문명의 이기를 누릴 줄 알아야죠",
          "original_form": "ㅎㅎ 문명의 이기를 누릴 줄 알아야죠",
          "speaker_id": "2",
          "time": "20210904 17:01"
        },
        {
          "id": "MDRW2100040354.1.12",
          "form": "씻기싫으면 집앞도안나가서 시켜먹곤하는데ㅋㅋㄱㅋ\n배달하시는분 눈배리게하는",
          "original_form": "씻기싫으면 집앞도안나가서 시켜먹곤하는데ㅋㅋㄱㅋ\n배달하시는분 눈배리게하는",
          "speaker_id": "1",
          "time": "20210904 17:04"
        },
        {
          "id": "MDRW2100040354.1.13",
          "form": "ㅋㅋㅈㄱㅈㄱㅈㄱㅈㄱ",
          "original_form": "ㅋㅋㅈㄱㅈㄱㅈㄱㅈㄱ",
          "speaker_id": "1",
          "time": "20210904 17:04"
        },
        {
          "id": "MDRW2100040354.1.14",
          "form": "요샌 그냥 문앞에 두는게 기본이 돼서 편하쥬ㅎ",
          "original_form": "요샌 그냥 문앞에 두는게 기본이 돼서 편하쥬ㅎ",
          "speaker_id": "2",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100040354.1.15",
          "form": "아 맞아요ㅋㅋㅋ그거좋아요ㅋㅋㅋㅋ\n근데 문앞에둬달라구하는데 못보시고 요청사항 못보시고 기다리시는분도 가끔계세ㅣ요ㅋㅋㅋ\n서로놀람",
          "original_form": "아 맞아요ㅋㅋㅋ그거좋아요ㅋㅋㅋㅋ\n근데 문앞에둬달라구하는데 못보시고 요청사항 못보시고 기다리시는분도 가끔계세ㅣ요ㅋㅋㅋ\n서로놀람",
          "speaker_id": "1",
          "time": "20210904 17:07"
        },
        {
          "id": "MDRW2100040354.1.16",
          "form": "아 그거 넘 싫어요ㅜㅇㅜ",
          "original_form": "아 그거 넘 싫어요ㅜㅇㅜ",
          "speaker_id": "2",
          "time": "20210904 17:08"
        },
        {
          "id": "MDRW2100040354.1.17",
          "form": "근데 문앞에 두고가니",
          "original_form": "근데 문앞에 두고가니",
          "speaker_id": "2",
          "time": "20210904 17:09"
        },
        {
          "id": "MDRW2100040354.1.18",
          "form": "오배송이 좀 늘긴 했어요ㅜ",
          "original_form": "오배송이 좀 늘긴 했어요ㅜ",
          "speaker_id": "2",
          "time": "20210904 17:10"
        },
        {
          "id": "MDRW2100040354.1.19",
          "form": "어제도 광어멍게새우튀김 혼술세트 시켰더니",
          "original_form": "어제도 광어멍게새우튀김 혼술세트 시켰더니",
          "speaker_id": "2",
          "time": "20210904 17:11"
        },
        {
          "id": "MDRW2100040354.1.20",
          "form": "딴집 배달 가서ㅜㅜ",
          "original_form": "딴집 배달 가서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 17:11"
        },
        {
          "id": "MDRW2100040354.1.21",
          "form": "헐  어ㄸㆍㅎ게찾으셨어요??!",
          "original_form": "헐  어ㄸㆍㅎ게찾으셨어요??!",
          "speaker_id": "1",
          "time": "20210904 17:12"
        },
        {
          "id": "MDRW2100040354.1.22",
          "form": "인ㅌㆍ넷보면\n그걸그냥먹어버려서 막 싸움난",
          "original_form": "인ㅌㆍ넷보면\n그걸그냥먹어버려서 막 싸움난",
          "speaker_id": "1",
          "time": "20210904 17:13"
        },
        {
          "id": "MDRW2100040354.1.23",
          "form": "그런것도봤어요",
          "original_form": "그런것도봤어요",
          "speaker_id": "1",
          "time": "20210904 17:13"
        },
        {
          "id": "MDRW2100040354.1.24",
          "form": "근데 배민1으로 배달한거라",
          "original_form": "근데 배민1으로 배달한거라",
          "speaker_id": "2",
          "time": "20210904 17:13"
        },
        {
          "id": "MDRW2100040354.1.25",
          "form": "문자가 아니라 배민앱으로 연락이와요ㅜ",
          "original_form": "문자가 아니라 배민앱으로 연락이와요ㅜ",
          "speaker_id": "2",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100040354.1.26",
          "form": "도착사진도",
          "original_form": "도착사진도",
          "speaker_id": "2",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100040354.1.27",
          "form": "도착사진이  우리집 대문이 아니라 라이더에 연락해야 하는데",
          "original_form": "도착사진이  우리집 대문이 아니라 라이더에 연락해야 하는데",
          "speaker_id": "2",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100040354.1.28",
          "form": "연락처가 안ㅋ보ㅋ 임",
          "original_form": "연락처가 안ㅋ보ㅋ 임",
          "speaker_id": "2",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100040354.1.29",
          "form": "세상에ㅋㅋㅋㅋㅋ",
          "original_form": "세상에ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100040354.1.30",
          "form": "가게에 전화하니 자기네도 모르니 배민에 물어봐야 한다고ㅎ",
          "original_form": "가게에 전화하니 자기네도 모르니 배민에 물어봐야 한다고ㅎ",
          "speaker_id": "2",
          "time": "20210904 17:15"
        },
        {
          "id": "MDRW2100040354.1.31",
          "form": "배민 실시간 상담 채팅 15분만에 연결됐네욪ㅎ",
          "original_form": "배민 실시간 상담 채팅 15분만에 연결됐네욪ㅎ",
          "speaker_id": "2",
          "time": "20210904 17:15"
        },
        {
          "id": "MDRW2100040354.1.32",
          "form": "와..진짜답답하셨겠는데요",
          "original_form": "와..진짜답답하셨겠는데요",
          "speaker_id": "1",
          "time": "20210904 17:15"
        },
        {
          "id": "MDRW2100040354.1.33",
          "form": "배민답변짱느리다던데",
          "original_form": "배민답변짱느리다던데",
          "speaker_id": "1",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100040354.1.34",
          "form": "ㅎㅎ 재조리해 다시 준다는데",
          "original_form": "ㅎㅎ 재조리해 다시 준다는데",
          "speaker_id": "2",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100040354.1.35",
          "form": "오배송된 거 찾아서 갖고와도 된다고 그래도",
          "original_form": "오배송된 거 찾아서 갖고와도 된다고 그래도",
          "speaker_id": "2",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100040354.1.36",
          "form": "라이더랑 연결 안된대요",
          "original_form": "라이더랑 연결 안된대요",
          "speaker_id": "2",
          "time": "20210904 17:16"
        }
      ]
