        {
          "id": "MDRW2100030343.1.1",
          "form": "칼국수랑 만두랑 세트 줘",
          "original_form": "칼국수랑 만두랑 세트 줘",
          "speaker_id": "2",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100030343.1.2",
          "form": "죠",
          "original_form": "죠",
          "speaker_id": "2",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100030343.1.3",
          "form": "김밥이랑 먹어도 맛있죠",
          "original_form": "김밥이랑 먹어도 맛있죠",
          "speaker_id": "1",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100030343.1.4",
          "form": "항아리수제비",
          "original_form": "항아리수제비",
          "speaker_id": "2",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100030343.1.5",
          "form": "쫄깃한 수제비가 맛잇나요",
          "original_form": "쫄깃한 수제비가 맛잇나요",
          "speaker_id": "2",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100030343.1.6",
          "form": "조선 칼국수 안 간지 오래 됐네요",
          "original_form": "조선 칼국수 안 간지 오래 됐네요",
          "speaker_id": "1",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100030343.1.7",
          "form": "부드러운 수제비가 맛잇나요",
          "original_form": "부드러운 수제비가 맛잇나요",
          "speaker_id": "2",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100030343.1.8",
          "form": "남포 수제비 아시나요",
          "original_form": "남포 수제비 아시나요",
          "speaker_id": "1",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100030343.1.9",
          "form": "매우 얇은 피인데 정말 맛있어요",
          "original_form": "매우 얇은 피인데 정말 맛있어요",
          "speaker_id": "1",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100030343.1.10",
          "form": "얼큰한 짬뽕 수제비 맛있어요",
          "original_form": "얼큰한 짬뽕 수제비 맛있어요",
          "speaker_id": "1",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100030343.1.11",
          "form": "회사 근처여서 한 번씩 갔어요",
          "original_form": "회사 근처여서 한 번씩 갔어요",
          "speaker_id": "1",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100030343.1.12",
          "form": "짬뽕은 다 맛있죠",
          "original_form": "짬뽕은 다 맛있죠",
          "speaker_id": "2",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100030343.1.13",
          "form": "외식을 많이 하나 봐요",
          "original_form": "외식을 많이 하나 봐요",
          "speaker_id": "2",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100030343.1.14",
          "form": "얇은티인데 수제비 안 좋아하는데도 진짜 맛있더라구요",
          "original_form": "얇은티인데 수제비 안 좋아하는데도 진짜 맛있더라구요",
          "speaker_id": "1",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100030343.1.15",
          "form": "밀가루는 다 맛있죠",
          "original_form": "밀가루는 다 맛있죠",
          "speaker_id": "2",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100030343.1.16",
          "form": "회사에서 바로 일하러 갈 때는 자주 사 먹었죠",
          "original_form": "회사에서 바로 일하러 갈 때는 자주 사 먹었죠",
          "speaker_id": "1",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100030343.1.17",
          "form": "일본식 김치 우동 집도 있어요",
          "original_form": "일본식 김치 우동 집도 있어요",
          "speaker_id": "1",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100030343.1.18",
          "form": "혼자 가나요",
          "original_form": "혼자 가나요",
          "speaker_id": "2",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100030343.1.19",
          "form": "정말 맛있어요",
          "original_form": "정말 맛있어요",
          "speaker_id": "1",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100030343.1.20",
          "form": "일본식은 달라요",
          "original_form": "일본식은 달라요",
          "speaker_id": "2",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100030343.1.21",
          "form": "주변 사람이랑 같이 가요",
          "original_form": "주변 사람이랑 같이 가요",
          "speaker_id": "1",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100030343.1.22",
          "form": "비빔밥 맛있는 것도 있어요",
          "original_form": "비빔밥 맛있는 것도 있어요",
