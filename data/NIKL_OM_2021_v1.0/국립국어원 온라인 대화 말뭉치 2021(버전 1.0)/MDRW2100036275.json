        },
        {
          "id": "MDRW2100036275.1.3",
          "form": "저녁 드셧나용",
          "original_form": "저녁 드셧나용",
          "speaker_id": "2",
          "time": "20210830 20:41"
        },
        {
          "id": "MDRW2100036275.1.4",
          "form": "네 전 먹었어요 name2님은 드셨나요?",
          "original_form": "네 전 먹었어요 &name2&님은 드셨나요?",
          "speaker_id": "1",
          "time": "20210830 20:42"
        },
        {
          "id": "MDRW2100036275.1.5",
          "form": "네! 방금 먹고 왔습니다 ㅋㅋㅋ",
          "original_form": "네! 방금 먹고 왔습니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:43"
        },
        {
          "id": "MDRW2100036275.1.6",
          "form": "뭐 드셨어요? 전 그냥 집밥이요 ㅋㅋ",
          "original_form": "뭐 드셨어요? 전 그냥 집밥이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:43"
        },
        {
          "id": "MDRW2100036275.1.7",
          "form": "저는 김치찌개요ㅋㅋㅋ",
          "original_form": "저는 김치찌개요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:44"
        },
        {
          "id": "MDRW2100036275.1.8",
          "form": "좋아하는 음식 있으신가요?",
          "original_form": "좋아하는 음식 있으신가요?",
          "speaker_id": "1",
          "time": "20210830 20:44"
        },
        {
          "id": "MDRW2100036275.1.9",
          "form": "저 냉면 회 김밥이요 ㅋㅋ",
          "original_form": "저 냉면 회 김밥이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:46"
        },
        {
          "id": "MDRW2100036275.1.10",
          "form": "진짜 저 세 음식은 평생 저거만 먹으라구 해도 먹을 수 있을 것 같아요",
          "original_form": "진짜 저 세 음식은 평생 저거만 먹으라구 해도 먹을 수 있을 것 같아요",
          "speaker_id": "2",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100036275.1.11",
          "form": "name1님은 좋아하는 음식 잇으신가요?",
          "original_form": "&name1&님은 좋아하는 음식 잇으신가요?",
          "speaker_id": "2",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100036275.1.12",
          "form": "저는 떡볶이요",
          "original_form": "저는 떡볶이요",
          "speaker_id": "1",
          "time": "20210830 20:48"
        },
        {
          "id": "MDRW2100036275.1.13",
          "form": "김밥은 특히 좋아하시는 종류가 있으신가요??",
          "original_form": "김밥은 특히 좋아하시는 종류가 있으신가요??",
          "speaker_id": "1",
          "time": "20210830 20:48"
        },
        {
          "id": "MDRW2100036275.1.14",
          "form": "떡볶이 좋죠!!! 저 어제두 먹었어요 ㅋㅋㅋ",
          "original_form": "떡볶이 좋죠!!! 저 어제두 먹었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:49"
        },
        {
          "id": "MDRW2100036275.1.15",
          "form": "김밥이라면 다 좋아하는 거 같아요 진짜",
          "original_form": "김밥이라면 다 좋아하는 거 같아요 진짜",
          "speaker_id": "2",
          "time": "20210830 20:49"
        },
        {
          "id": "MDRW2100036275.1.16",
          "form": "매운 거 못 먹어서 땡초김밥 이런고만 아니면 다 좋아해요",
          "original_form": "매운 거 못 먹어서 땡초김밥 이런고만 아니면 다 좋아해요",
          "speaker_id": "2",
          "time": "20210830 20:49"
        },
        {
          "id": "MDRW2100036275.1.17",
          "form": "그냥 김에 밥만 말아두 맛있는 것 같아요",
          "original_form": "그냥 김에 밥만 말아두 맛있는 것 같아요",
          "speaker_id": "2",
          "time": "20210830 20:50"
