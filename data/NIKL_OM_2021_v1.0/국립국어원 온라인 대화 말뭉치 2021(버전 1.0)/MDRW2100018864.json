      "utterance": [
        {
          "id": "MDRW2100018864.1.1",
          "form": "돈 많이 벌어서 쇼핑해야하는데요 ㅎㅎ",
          "original_form": "돈 많이 벌어서 쇼핑해야하는데요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100018864.1.2",
          "form": "ㅋㅋ많이 버셔야죠",
          "original_form": "ㅋㅋ많이 버셔야죠",
          "speaker_id": "1",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100018864.1.3",
          "form": "소비하기 위해 벌죠 ㅋㅋ",
          "original_form": "소비하기 위해 벌죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100018864.1.4",
          "form": "ㅋㅋ그래도 나름 버는데",
          "original_form": "ㅋㅋ그래도 나름 버는데",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100018864.1.5",
          "form": "집이 너무 비싸네요ㅎㅎ",
          "original_form": "집이 너무 비싸네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100018864.1.6",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100018864.1.7",
          "form": "집은 사려면 오르고",
          "original_form": "집은 사려면 오르고",
          "speaker_id": "1",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100018864.1.8",
          "form": "오르고",
          "original_form": "오르고",
          "speaker_id": "1",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100018864.1.9",
          "form": "끝도 없이 오르죠",
          "original_form": "끝도 없이 오르죠",
          "speaker_id": "1",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100018864.1.10",
          "form": "사고 싶은 것중 젤비싸죠",
          "original_form": "사고 싶은 것중 젤비싸죠",
          "speaker_id": "1",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100018864.1.11",
          "form": "대출도 막히고 결국 원하는집은 네집 아니죠 은행꺼죠",
          "original_form": "대출도 막히고 결국 원하는집은 네집 아니죠 은행꺼죠",
          "speaker_id": "1",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100018864.1.12",
          "form": "내집",
          "original_form": "내집",
          "speaker_id": "1",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100018864.1.13",
          "form": "아니죠",
          "original_form": "아니죠",
          "speaker_id": "1",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100018864.1.14",
          "form": "ㅋㅋ그르게요~~",
          "original_form": "ㅋㅋ그르게요~~",
          "speaker_id": "2",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100018864.1.15",
          "form": "어 근데 에디그램은",
          "original_form": "어 근데 에디그램은",
          "speaker_id": "2",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100018864.1.16",
          "form": "앱은 없는건가요",
          "original_form": "앱은 없는건가요",
          "speaker_id": "2",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100018864.1.17",
          "form": "전 앱아니고 그냥 바로가기로해요",
          "original_form": "전 앱아니고 그냥 바로가기로해요",
          "speaker_id": "1",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100018864.1.18",
          "form": "바탕화면에요 ㅎㅎ",
          "original_form": "바탕화면에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100018864.1.19",
          "form": "아 그렇게 해야하는구나 ㅋㅋ 알겠습니다",
          "original_form": "아 그렇게 해야하는구나 ㅋㅋ 알겠습니다",
          "speaker_id": "2",
          "time": "20210827 16:45"
        },
        {
          "id": "MDRW2100018864.1.20",
          "form": "하다가 1000원모이면 다음등금 사서 25개보면 매일 525 모여요 ㅎㅎ",
          "original_form": "하다가 1000원모이면 다음등금 사서 25개보면 매일 525 모여요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 16:45"
        },
        {
          "id": "MDRW2100018864.1.21",
          "form": "돈 벌어서 쇼핑해야지...",
          "original_form": "돈 벌어서 쇼핑해야지...",
          "speaker_id": "2",
          "time": "20210827 16:45"
        },
        {
          "id": "MDRW2100018864.1.22",
          "form": "네 그러세요 홧팅입니다",
          "original_form": "네 그러세요 홧팅입니다",
          "speaker_id": "1",
          "time": "20210827 16:45"
        },
        {
          "id": "MDRW2100018864.1.23",
          "form": "뭐 조그마한거 쇼핑할정도는 모이죠",
          "original_form": "뭐 조그마한거 쇼핑할정도는 모이죠",
          "speaker_id": "1",
          "time": "20210827 16:45"
        },
        {
          "id": "MDRW2100018864.1.24",
          "form": "주로 생필품은 아내분이 쇼핑하세요?",
          "original_form": "주로 생필품은 아내분이 쇼핑하세요?",
          "speaker_id": "1",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100018864.1.25",
          "form": "비누 화장지 세제 샴푸른 티몬 위메프 세일할때 젤 싼거 같아요 ㅎㅎ",
          "original_form": "비누 화장지 세제 샴푸른 티몬 위메프 세일할때 젤 싼거 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 16:52"
        },
        {
          "id": "MDRW2100018864.1.26",
          "form": "음 같이 해요 ㅎㅎ",
          "original_form": "음 같이 해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 18:38"
        },
        {
          "id": "MDRW2100018864.1.27",
          "form": "카드 실적 채우려고 ㅎㅎ",
          "original_form": "카드 실적 채우려고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 18:39"
        },
        {
          "id": "MDRW2100018864.1.28",
          "form": "아 그렇죠",
          "original_form": "아 그렇죠",
          "speaker_id": "1",
          "time": "20210827 18:39"
        },
        {
          "id": "MDRW2100018864.1.29",
          "form": "요새 실적 때문에 다들 그래요",
          "original_form": "요새 실적 때문에 다들 그래요",
          "speaker_id": "1",
          "time": "20210827 18:40"
        },
        {
          "id": "MDRW2100018864.1.30",
          "form": "네 코로나때문에 카드도 하나 잘랐어요ㅋㅋ",
          "original_form": "네 코로나때문에 카드도 하나 잘랐어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:40"
        },
        {
          "id": "MDRW2100018864.1.31",
          "form": "그래야 해요 마구쓰다 ㅋㅋ폭탄맞죠",
          "original_form": "그래야 해요 마구쓰다 ㅋㅋ폭탄맞죠",
          "speaker_id": "1",
          "time": "20210827 18:41"
        },
        {
          "id": "MDRW2100018864.1.32",
          "form": "전 제꺼만 몰빵해서 써요",
          "original_form": "전 제꺼만 몰빵해서 써요",
