      "utterance": [
        {
          "id": "MDRW2100046177.1.1",
          "form": "배달음식 자주 드시나요",
          "original_form": "배달음식 자주 드시나요",
          "speaker_id": "2",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100046177.1.2",
          "form": "일주일에 한 번은 시켜 먹어용",
          "original_form": "일주일에 한 번은 시켜 먹어용",
          "speaker_id": "1",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046177.1.3",
          "form": "어떤것 시켜 드시나요",
          "original_form": "어떤것 시켜 드시나요",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046177.1.4",
          "form": "주로 치킨 시켜먹어요",
          "original_form": "주로 치킨 시켜먹어요",
          "speaker_id": "1",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046177.1.5",
          "form": "저도 치킨 피자가 주로예요",
          "original_form": "저도 치킨 피자가 주로예요",
          "speaker_id": "2",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100046177.1.6",
          "form": "어디 피자 시키시나요?",
          "original_form": "어디 피자 시키시나요?",
          "speaker_id": "1",
          "time": "20210908 11:08"
        },
        {
          "id": "MDRW2100046177.1.7",
          "form": "치킨bhc  피자 파파존스",
          "original_form": "치킨bhc  피자 파파존스",
          "speaker_id": "2",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100046177.1.8",
          "form": "파파존스에서 뭐가 제일 맛있어?",
          "original_form": "파파존스에서 뭐가 제일 맛있어?",
          "speaker_id": "1",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046177.1.9",
          "form": "전 고기류가 맛나요",
          "original_form": "전 고기류가 맛나요",
          "speaker_id": "2",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046177.1.10",
          "form": "오 파파존스는 사이드 메뉴 맛집이라던데",
          "original_form": "오 파파존스는 사이드 메뉴 맛집이라던데",
          "speaker_id": "1",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046177.1.11",
          "form": "사이드 메뉴는 파스타만 먹어봣어요",
          "original_form": "사이드 메뉴는 파스타만 먹어봣어요",
          "speaker_id": "2",
          "time": "20210908 11:19"
        },
