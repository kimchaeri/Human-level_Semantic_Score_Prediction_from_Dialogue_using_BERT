      "utterance": [
        {
          "id": "MDRW2100028997.1.1",
          "form": "넷플릭스 보는거 있으세요?",
          "original_form": "넷플릭스 보는거 있으세요?",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028997.1.2",
          "form": "저는넷플릭흐안봐요 ㅠㅠ",
          "original_form": "저는넷플릭흐안봐요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028997.1.3",
          "form": "재밋게본 드라마 어떤거세요??",
          "original_form": "재밋게본 드라마 어떤거세요??",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028997.1.4",
          "form": "저음..",
          "original_form": "저음..",
          "speaker_id": "2",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028997.1.5",
          "form": "지금은 안보는데",
          "original_form": "지금은 안보는데",
          "speaker_id": "2",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028997.1.6",
          "form": "펜트하우스 잼게 본거같아요",
          "original_form": "펜트하우스 잼게 본거같아요",
          "speaker_id": "2",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028997.1.7",
          "form": "3은 별루더라그요",
          "original_form": "3은 별루더라그요",
          "speaker_id": "2",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028997.1.8",
          "form": "오 맞아요 ㅋㅋㅋㅋㅋㅋ 2까지는 재밌었는데",
          "original_form": "오 맞아요 ㅋㅋㅋㅋㅋㅋ 2까지는 재밌었는데",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028997.1.9",
          "form": "낼 마지막이죠??",
          "original_form": "낼 마지막이죠??",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028997.1.10",
          "form": "모르겠어요 ㅋㅋㅋㅋ",
          "original_form": "모르겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 06:39"
        },
        {
          "id": "MDRW2100028997.1.11",
          "form": "결말 너무 궁금해요 ㅋㅋㅋ",
          "original_form": "결말 너무 궁금해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:03"
        },
        {
          "id": "MDRW2100028997.1.12",
          "form": "드디어 드라마 하나가 끝나네요",
          "original_form": "드디어 드라마 하나가 끝나네요",
          "speaker_id": "1",
          "time": "20210903 14:03"
        },
        {
          "id": "MDRW2100028997.1.13",
          "form": "저도요!!순옥킴 꺼자나요!!ㅋㅋ",
          "original_form": "저도요!!순옥킴 꺼자나요!!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:58"
        },
        {
          "id": "MDRW2100028997.1.14",
          "form": "저 어제 마지막회인줄 알앗는데",
          "original_form": "저 어제 마지막회인줄 알앗는데",
          "speaker_id": "1",
          "time": "20210904 21:36"
        },
        {
          "id": "MDRW2100028997.1.15",
          "form": "담주에 또한데여 ㅋㅋㅋ",
          "original_form": "담주에 또한데여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:36"
        },
        {
          "id": "MDRW2100028997.1.16",
          "form": "헐랭 3은아예못봤어요어떤가요",
          "original_form": "헐랭 3은아예못봤어요어떤가요",
