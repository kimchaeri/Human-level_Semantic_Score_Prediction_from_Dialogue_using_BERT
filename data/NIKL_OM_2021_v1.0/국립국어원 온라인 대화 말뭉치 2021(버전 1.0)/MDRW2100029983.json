        {
          "id": "MDRW2100029983.1.1",
          "form": "여기는 오늘 맑았어요",
          "original_form": "여기는 오늘 맑았어요",
          "speaker_id": "2",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100029983.1.2",
          "form": "더웠나요?",
          "original_form": "더웠나요?",
          "speaker_id": "1",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100029983.1.3",
          "form": "네 낮에 조금요",
          "original_form": "네 낮에 조금요",
          "speaker_id": "2",
          "time": "20210903 17:55"
        },
        {
          "id": "MDRW2100029983.1.4",
          "form": "그곳은 흐렸나요?",
          "original_form": "그곳은 흐렸나요?",
          "speaker_id": "2",
          "time": "20210903 17:55"
        },
        {
          "id": "MDRW2100029983.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210903 17:56"
        },
        {
          "id": "MDRW2100029983.1.6",
          "form": "흐렸으면 덥지는 않았겠어요",
          "original_form": "흐렸으면 덥지는 않았겠어요",
          "speaker_id": "2",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100029983.1.7",
          "form": "선선하죠",
          "original_form": "선선하죠",
          "speaker_id": "1",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100029983.1.8",
          "form": "선선하니 딱 좋은 날씨네요",
          "original_form": "선선하니 딱 좋은 날씨네요",
          "speaker_id": "2",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100029983.1.9",
          "form": "맞아여",
          "original_form": "맞아여",
          "speaker_id": "1",
          "time": "20210903 18:55"
        },
        {
          "id": "MDRW2100029983.1.10",
          "form": "어느 계절 좋아하세여?",
          "original_form": "어느 계절 좋아하세여?",
          "speaker_id": "2",
          "time": "20210903 19:01"
