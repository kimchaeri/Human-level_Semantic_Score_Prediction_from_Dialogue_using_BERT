      "utterance": [
        {
          "id": "MDRW2100016596.1.1",
          "form": "레시피 어디서 주로 보세요",
          "original_form": "레시피 어디서 주로 보세요",
          "speaker_id": "2",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100016596.1.2",
          "form": "그때그때 인터넷서 검색해요",
          "original_form": "그때그때 인터넷서 검색해요",
          "speaker_id": "1",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100016596.1.3",
          "form": "사실 요리를 잘 안해서ㅎ",
          "original_form": "사실 요리를 잘 안해서ㅎ",
          "speaker_id": "1",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100016596.1.4",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100016596.1.5",
          "form": "저도 그때그때 유투브나",
          "original_form": "저도 그때그때 유투브나",
          "speaker_id": "2",
          "time": "20210826 14:43"
        },
        {
          "id": "MDRW2100016596.1.6",
          "form": "만개의 레시피라는 어플 좋아요",
          "original_form": "만개의 레시피라는 어플 좋아요",
          "speaker_id": "2",
          "time": "20210826 14:43"
        },
        {
          "id": "MDRW2100016596.1.7",
          "form": "만개의 레시피?",
          "original_form": "만개의 레시피?",
          "speaker_id": "1",
          "time": "20210826 15:06"
        },
        {
          "id": "MDRW2100016596.1.8",
          "form": "이름만 들어도 무척 유용해보여요",
          "original_form": "이름만 들어도 무척 유용해보여요",
          "speaker_id": "1",
          "time": "20210826 15:06"
        },
        {
          "id": "MDRW2100016596.1.9",
          "form": "네 재료나 메뉴치면 레시피. 쫙 나와요",
          "original_form": "네 재료나 메뉴치면 레시피. 쫙 나와요",
          "speaker_id": "2",
          "time": "20210826 15:07"
        },
        {
          "id": "MDRW2100016596.1.10",
          "form": "오오 그 앱으로 어떤 요리를 해보셨어요?",
          "original_form": "오오 그 앱으로 어떤 요리를 해보셨어요?",
          "speaker_id": "1",
          "time": "20210826 15:08"
        },
        {
          "id": "MDRW2100016596.1.11",
          "form": "제육볶음. 단호박에그슬럿 같은거 해봤어요",
          "original_form": "제육볶음. 단호박에그슬럿 같은거 해봤어요",
          "speaker_id": "2",
          "time": "20210826 15:09"
        },
        {
          "id": "MDRW2100016596.1.12",
          "form": "요똥이라 간단한거ㅎ",
          "original_form": "요똥이라 간단한거ㅎ",
          "speaker_id": "2",
          "time": "20210826 15:09"
        },
        {
          "id": "MDRW2100016596.1.13",
          "form": "단호박을 그릇처럼 쓴 건가요?",
          "original_form": "단호박을 그릇처럼 쓴 건가요?",
          "speaker_id": "1",
          "time": "20210826 15:09"
        },
        {
          "id": "MDRW2100016596.1.14",
          "form": "제눈에 요잘알인데요ㅎ",
          "original_form": "제눈에 요잘알인데요ㅎ",
          "speaker_id": "1",
          "time": "20210826 15:10"
        },
        {
          "id": "MDRW2100016596.1.15",
          "form": "오 맞아요 단호박 파내서 달걀이랑 치즈만 넣으면 끝이에요",
          "original_form": "오 맞아요 단호박 파내서 달걀이랑 치즈만 넣으면 끝이에요",
          "speaker_id": "2",
          "time": "20210826 15:10"
        },
        {
          "id": "MDRW2100016596.1.16",
          "form": "간식으로 좋은 것 같아요",
          "original_form": "간식으로 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210826 15:10"
        },
        {
          "id": "MDRW2100016596.1.17",
          "form": "한번 만들어보세요ㅎㅎ",
          "original_form": "한번 만들어보세요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100016596.1.18",
          "form": "ㅎㅎㅎ 아니 듣기만해도 어려운데요",
          "original_form": "ㅎㅎㅎ 아니 듣기만해도 어려운데요",
          "speaker_id": "1",
          "time": "20210826 15:14"
        },
        {
          "id": "MDRW2100016596.1.19",
          "form": "호박은 애호박 부쳐먹는거가 제 한계인데",
          "original_form": "호박은 애호박 부쳐먹는거가 제 한계인데",
          "speaker_id": "1",
          "time": "20210826 15:14"
        },
        {
          "id": "MDRW2100016596.1.20",
          "form": "치즈는 피자치즈 넣는 거죠?",
          "original_form": "치즈는 피자치즈 넣는 거죠?",
          "speaker_id": "1",
          "time": "20210826 15:14"
        },
        {
          "id": "MDRW2100016596.1.21",
          "form": "오 애호박전인가요?ㅎ",
          "original_form": "오 애호박전인가요?ㅎ",
          "speaker_id": "2",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100016596.1.22",
          "form": "네 모짜렐라치즈에요",
          "original_form": "네 모짜렐라치즈에요",
          "speaker_id": "2",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100016596.1.23",
          "form": "네 심지어 귀찮다고 아무것도 안입히고 그냥 부치지요",
          "original_form": "네 심지어 귀찮다고 아무것도 안입히고 그냥 부치지요",
          "speaker_id": "1",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100016596.1.24",
          "form": "모짜렐라치즈 이름도 생각 안나서 피자치즈라고 말하는 이 요알못ㅜ",
          "original_form": "모짜렐라치즈 이름도 생각 안나서 피자치즈라고 말하는 이 요알못ㅜ",
          "speaker_id": "1",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016596.1.25",
          "form": "헉 그래도 부쳐지나봐요ㅋㅋ",
          "original_form": "헉 그래도 부쳐지나봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:16"
        }
      ]
    }
