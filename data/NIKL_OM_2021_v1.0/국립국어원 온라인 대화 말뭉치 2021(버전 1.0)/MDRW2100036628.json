      "utterance": [
        {
          "id": "MDRW2100036628.1.1",
          "form": "오호 청정원몰에서 김치사기...",
          "original_form": "오호 청정원몰에서 김치사기...",
          "speaker_id": "1",
          "time": "20210831 02:41"
        },
        {
          "id": "MDRW2100036628.1.2",
          "form": "열무김치 좋아하세요??",
          "original_form": "열무김치 좋아하세요??",
          "speaker_id": "1",
          "time": "20210831 02:41"
        },
        {
          "id": "MDRW2100036628.1.3",
          "form": "할인많이때릴때사세요",
          "original_form": "할인많이때릴때사세요",
          "speaker_id": "2",
          "time": "20210831 02:42"
        },
        {
          "id": "MDRW2100036628.1.4",
          "form": "넹넹!!",
          "original_form": "넹넹!!",
          "speaker_id": "1",
          "time": "20210831 02:42"
        },
        {
          "id": "MDRW2100036628.1.5",
          "form": "네 열무김치 국물넘좋아해요",
          "original_form": "네 열무김치 국물넘좋아해요",
          "speaker_id": "2",
          "time": "20210831 02:42"
        },
        {
          "id": "MDRW2100036628.1.6",
          "form": "제돈주고는 비싸서ㅋㅋㅋㅋ",
          "original_form": "제돈주고는 비싸서ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 02:42"
        },
        {
          "id": "MDRW2100036628.1.7",
          "form": "시원시원 여름에 없음안됩니다",
          "original_form": "시원시원 여름에 없음안됩니다",
          "speaker_id": "2",
          "time": "20210831 02:42"
        },
        {
          "id": "MDRW2100036628.1.8",
          "form": "크으 열무김치는 소면으로 비벼먹어도 맛있고",
          "original_form": "크으 열무김치는 소면으로 비벼먹어도 맛있고",
          "speaker_id": "1",
          "time": "20210831 02:42"
        },
        {
          "id": "MDRW2100036628.1.9",
          "form": "비빔밥에 넣어먹어도 맛있고",
          "original_form": "비빔밥에 넣어먹어도 맛있고",
          "speaker_id": "1",
          "time": "20210831 02:42"
        },
        {
          "id": "MDRW2100036628.1.10",
          "form": "맞아요 비빔면많이해먹었어요",
          "original_form": "맞아요 비빔면많이해먹었어요",
          "speaker_id": "2",
          "time": "20210831 02:42"
        },
        {
          "id": "MDRW2100036628.1.11",
          "form": "열무잘골라야되는게 질긴애들은",
          "original_form": "열무잘골라야되는게 질긴애들은",
          "speaker_id": "2",
          "time": "20210831 02:43"
        },
        {
          "id": "MDRW2100036628.1.12",
          "form": "영못먹겠더라구여",
          "original_form": "영못먹겠더라구여",
          "speaker_id": "2",
          "time": "20210831 02:43"
        },
        {
          "id": "MDRW2100036628.1.13",
          "form": "골뱅이넣고 열무넣고 참기름 넣고 비빔면 해서먹으면 맛있더라구요",
          "original_form": "골뱅이넣고 열무넣고 참기름 넣고 비빔면 해서먹으면 맛있더라구요",
          "speaker_id": "1",
          "time": "20210831 02:43"
        },
        {
          "id": "MDRW2100036628.1.14",
          "form": "크으 진짜 파는거 저리가라죠",
          "original_form": "크으 진짜 파는거 저리가라죠",
          "speaker_id": "2",
          "time": "20210831 02:43"
        },
        {
          "id": "MDRW2100036628.1.15",
          "form": "아 맞아요 열무 줄기가 억센 친구들이 있어요ㅋㅋㅋㅋ",
          "original_form": "아 맞아요 열무 줄기가 억센 친구들이 있어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 02:43"
        },
        {
          "id": "MDRW2100036628.1.16",
          "form": "네네 유독억센애들",
          "original_form": "네네 유독억센애들",
          "speaker_id": "2",
          "time": "20210831 02:43"
        },
        {
          "id": "MDRW2100036628.1.17",
          "form": "그런것들은 주로 잘게 다져서 비빔밥에 넣어먹어요ㅋㅋㅋ",
          "original_form": "그런것들은 주로 잘게 다져서 비빔밥에 넣어먹어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 02:43"
        }
      ]
