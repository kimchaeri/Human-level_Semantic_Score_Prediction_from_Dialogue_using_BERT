      "utterance": [
        {
          "id": "MDRW2100022724.1.1",
          "form": "결혼하셨나요?",
          "original_form": "결혼하셨나요?",
          "speaker_id": "2",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100022724.1.2",
          "form": "사별이랄까요",
          "original_form": "사별이랄까요",
          "speaker_id": "1",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100022724.1.3",
          "form": "저런..그러면 새로운 연애를 하고싶으신가요?",
          "original_form": "저런..그러면 새로운 연애를 하고싶으신가요?",
          "speaker_id": "2",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100022724.1.4",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022724.1.5",
          "form": "저 모쏠입니다",
          "original_form": "저 모쏠입니다",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022724.1.6",
          "form": "그러시구나 결혼계획은 있으신가요?",
          "original_form": "그러시구나 결혼계획은 있으신가요?",
          "speaker_id": "2",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022724.1.7",
          "form": "전 결혼할생각이없어서요",
          "original_form": "전 결혼할생각이없어서요",
          "speaker_id": "2",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022724.1.8",
          "form": "비혼주의자에요^^",
          "original_form": "비혼주의자에요^^",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022724.1.9",
          "form": "아 저랑 같네요",
          "original_form": "아 저랑 같네요",
          "speaker_id": "2",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022724.1.10",
          "form": "결혼관이 저희가",
          "original_form": "결혼관이 저희가",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022724.1.11",
          "form": "꼭맞네요",
          "original_form": "꼭맞네요",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022724.1.12",
          "form": "그냥 저희둘이 할까요?",
          "original_form": "그냥 저희둘이 할까요?",
          "speaker_id": "1",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022724.1.13",
          "form": "네 둘이 해서",
          "original_form": "네 둘이 해서",
          "speaker_id": "2",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022724.1.14",
          "form": "축의금 걷어요",
          "original_form": "축의금 걷어요",
          "speaker_id": "2",
          "time": "20210829 17:29"
        },
        {
          "id": "MDRW2100022724.1.15",
          "form": "부모님이 뿌린게 많아서 식은 올리고싶네요",
          "original_form": "부모님이 뿌린게 많아서 식은 올리고싶네요",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022724.1.16",
          "form": "네 참석했던 축의금들이",
          "original_form": "네 참석했던 축의금들이",
          "speaker_id": "1",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022724.1.17",
          "form": "아까워죽겠어요",
          "original_form": "아까워죽겠어요",
          "speaker_id": "1",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022724.1.18",
          "form": "부모님 돈,,,",
          "original_form": "부모님 돈,,,",
          "speaker_id": "1",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022724.1.19",
          "form": "축의금 아껴서 카카오주식이나사지..",
          "original_form": "축의금 아껴서 카카오주식이나사지..",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022724.1.20",
          "form": "아무튼 결혼은 안하고 식만 올리고 싶어요",
          "original_form": "아무튼 결혼은 안하고 식만 올리고 싶어요",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022724.1.21",
          "form": "비혼 쉐어하우스 생기면",
          "original_form": "비혼 쉐어하우스 생기면",
          "speaker_id": "1",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100022724.1.22",
          "form": "들어갈래요",
          "original_form": "들어갈래요",
          "speaker_id": "1",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100022724.1.23",
          "form": "비혼 쉐어하우스 아이디어가 좋네요",
          "original_form": "비혼 쉐어하우스 아이디어가 좋네요",
          "speaker_id": "2",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100022724.1.24",
          "form": "맥주나마시는거죠",
          "original_form": "맥주나마시는거죠",
