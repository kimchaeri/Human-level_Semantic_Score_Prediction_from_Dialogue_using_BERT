      "utterance": [
        {
          "id": "MDRW2100006185.1.1",
          "form": "저녁매뉴 뭐드셨나요",
          "original_form": "저녁매뉴 뭐드셨나요",
          "speaker_id": "2",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100006185.1.2",
          "form": "결국 고등어에 밥 먹었답니다",
          "original_form": "결국 고등어에 밥 먹었답니다",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100006185.1.3",
          "form": "고등어는 생물인가요?아님 포장된것??",
          "original_form": "고등어는 생물인가요?아님 포장된것??",
          "speaker_id": "2",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100006185.1.4",
          "form": "냉동된거요",
          "original_form": "냉동된거요",
          "speaker_id": "1",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100006185.1.5",
          "form": "전에 생물 샀다가",
          "original_form": "전에 생물 샀다가",
          "speaker_id": "1",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100006185.1.6",
          "form": "손질을 어설프게 했던",
          "original_form": "손질을 어설프게 했던",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100006185.1.7",
          "form": "안타까운 기억이 있는지라",
          "original_form": "안타까운 기억이 있는지라",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100006185.1.8",
          "form": "저도 고등어밥상?이라는.손질고등어 사먹는데 괜찮더라구요",
          "original_form": "저도 고등어밥상?이라는.손질고등어 사먹는데 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100006185.1.9",
          "form": "아 그러고보니",
          "original_form": "아 그러고보니",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100006185.1.10",
          "form": "손질 생선 많이 생겼더라고요",
          "original_form": "손질 생선 많이 생겼더라고요",
          "speaker_id": "1",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100006185.1.11",
          "form": "네 저건 가시도 발라져있어요...ㅎ",
          "original_form": "네 저건 가시도 발라져있어요...ㅎ",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100006185.1.12",
          "form": "여튼",
          "original_form": "여튼",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006185.1.13",
          "form": "이번에 먹은건",
          "original_form": "이번에 먹은건",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006185.1.14",
          "form": "냉동 노르웨이 고등어인지라",
          "original_form": "냉동 노르웨이 고등어인지라",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006185.1.15",
          "form": "그러셨군요 고등어 맛있죠..",
          "original_form": "그러셨군요 고등어 맛있죠..",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100006185.1.16",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100006185.1.17",
          "form": "꽁치와 함께",
          "original_form": "꽁치와 함께",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100006185.1.18",
          "form": "구워먹기 좋음",
          "original_form": "구워먹기 좋음",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100006185.1.19",
          "form": "은",
          "original_form": "은",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100006185.1.20",
          "form": "꽁치는 안먹은지 오래네요",
          "original_form": "꽁치는 안먹은지 오래네요",
          "speaker_id": "2",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100006185.1.21",
          "form": "참치는요?",
          "original_form": "참치는요?",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100006185.1.22",
          "form": "참치도요..ㅎㅎ\r\n갈치.삼치.고등어를 자주 먹네요",
