      "utterance": [
        {
          "id": "MDRW2100033234.1.1",
          "form": "덕분에편하게다녔죠",
          "original_form": "덕분에편하게다녔죠",
          "speaker_id": "2",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100033234.1.2",
          "form": "시드니갔는데 신기하더라구요",
          "original_form": "시드니갔는데 신기하더라구요",
          "speaker_id": "2",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100033234.1.3",
          "form": "오페라하우스같은거ㅋㅋ",
          "original_form": "오페라하우스같은거ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100033234.1.4",
          "form": "아 진짜 너무 좋았을거같아요",
          "original_form": "아 진짜 너무 좋았을거같아요",
          "speaker_id": "1",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100033234.1.5",
          "form": "또 가고싶으세요?",
          "original_form": "또 가고싶으세요?",
          "speaker_id": "1",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100033234.1.6",
          "form": "호주 다른쪽은 가고싶어요!",
          "original_form": "호주 다른쪽은 가고싶어요!",
          "speaker_id": "2",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100033234.1.7",
          "form": "여행 기억에 남는곳 있으셨나요?",
          "original_form": "여행 기억에 남는곳 있으셨나요?",
          "speaker_id": "2",
          "time": "20210906 20:30"
        },
        {
          "id": "MDRW2100033234.1.8",
          "form": "전 캐나다? ㅋㅋ",
          "original_form": "전 캐나다? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:31"
        },
        {
          "id": "MDRW2100033234.1.9",
          "form": "우와ㅏㅏ",
          "original_form": "우와ㅏㅏ",
          "speaker_id": "2",
          "time": "20210906 20:31"
        },
        {
          "id": "MDRW2100033234.1.10",
          "form": "워킹으로 갔는데ㅋㅋ 금방 왔어요ㅋㅋ",
          "original_form": "워킹으로 갔는데ㅋㅋ 금방 왔어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:31"
        },
        {
          "id": "MDRW2100033234.1.11",
          "form": "어느계절에가셨어요?",
          "original_form": "어느계절에가셨어요?",
          "speaker_id": "2",
          "time": "20210906 20:31"
        },
        {
          "id": "MDRW2100033234.1.12",
          "form": "한국에선 봄이었는데 거긴 겨울...",
          "original_form": "한국에선 봄이었는데 거긴 겨울...",
          "speaker_id": "1",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100033234.1.13",
          "form": "5월까지 눈오더라고요",
          "original_form": "5월까지 눈오더라고요",
          "speaker_id": "1",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100033234.1.14",
          "form": "우와ㄷㄷㄷ",
          "original_form": "우와ㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210906 20:33"
        },
        {
          "id": "MDRW2100033234.1.15",
          "form": "많이추운가요?",
          "original_form": "많이추운가요?",
          "speaker_id": "2",
          "time": "20210906 20:33"
        },
        {
          "id": "MDRW2100033234.1.16",
          "form": "캐나다도 넓어서 지역마다 다른데 제가 간곳은 춥더라고요ㅜㅠ",
          "original_form": "캐나다도 넓어서 지역마다 다른데 제가 간곳은 춥더라고요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210906 20:35"
        },
        {
          "id": "MDRW2100033234.1.17",
          "form": "워킹도 호주가 좋은거같아요",
          "original_form": "워킹도 호주가 좋은거같아요",
          "speaker_id": "1",
          "time": "20210906 20:35"
        },
        {
          "id": "MDRW2100033234.1.18",
          "form": "친구보면 잘사는거같아요",
          "original_form": "친구보면 잘사는거같아요",
          "speaker_id": "2",
          "time": "20210906 20:36"
        },
        {
          "id": "MDRW2100033234.1.19",
          "form": "힘들긴하겠지만ㅜㅜ",
          "original_form": "힘들긴하겠지만ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 20:36"
        },
        {
          "id": "MDRW2100033234.1.20",
          "form": "캐나다는 혼자다니기 괜찮나요?",
          "original_form": "캐나다는 혼자다니기 괜찮나요?",
          "speaker_id": "2",
          "time": "20210906 20:36"
        },
        {
          "id": "MDRW2100033234.1.21",
          "form": "네 완전 안전 물론 밤엔 위험하죠",
          "original_form": "네 완전 안전 물론 밤엔 위험하죠",
          "speaker_id": "1",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100033234.1.22",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210906 20:39"
        },
        {
          "id": "MDRW2100033234.1.23",
          "form": "호주는 사람이없어서그런가 해만지면무섭더라구요ㅜㅜ",
          "original_form": "호주는 사람이없어서그런가 해만지면무섭더라구요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 20:39"
        },
        {
          "id": "MDRW2100033234.1.24",
          "form": "캐나다 깡촌으로 가서 저도 도보하는 사람은 만난적이 별로 없었어요",
          "original_form": "캐나다 깡촌으로 가서 저도 도보하는 사람은 만난적이 별로 없었어요",
