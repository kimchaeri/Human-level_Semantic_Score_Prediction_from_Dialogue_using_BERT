      "utterance": [
        {
          "id": "MDRW2100029631.1.1",
          "form": "족제비 키우면 안좋지 않나",
          "original_form": "족제비 키우면 안좋지 않나",
          "speaker_id": "2",
          "time": "20210903 11:04"
        },
        {
          "id": "MDRW2100029631.1.2",
          "form": "나중에 사진 보여주렴",
          "original_form": "나중에 사진 보여주렴",
          "speaker_id": "2",
          "time": "20210903 11:05"
        },
        {
          "id": "MDRW2100029631.1.3",
          "form": "제가 키우는건 아니지만 ㅎㅎ 네",
          "original_form": "제가 키우는건 아니지만 ㅎㅎ 네",
          "speaker_id": "1",
          "time": "20210903 11:05"
        },
        {
          "id": "MDRW2100029631.1.4",
          "form": "카톡으로 사진 보냈어요",
          "original_form": "카톡으로 사진 보냈어요",
          "speaker_id": "1",
          "time": "20210903 11:05"
        },
        {
          "id": "MDRW2100029631.1.5",
          "form": "귀엽다",
          "original_form": "귀엽다",
          "speaker_id": "2",
          "time": "20210903 11:05"
        },
        {
          "id": "MDRW2100029631.1.6",
          "form": "털동물이네 까무잡잡하고",
          "original_form": "털동물이네 까무잡잡하고",
          "speaker_id": "2",
          "time": "20210903 11:05"
        },
        {
          "id": "MDRW2100029631.1.7",
          "form": "되게 길다 야",
          "original_form": "되게 길다 야",
          "speaker_id": "2",
          "time": "20210903 11:05"
        },
        {
          "id": "MDRW2100029631.1.8",
          "form": "해외 출신이라고 들었어요 토종이랑은 좀 다르대요",
          "original_form": "해외 출신이라고 들었어요 토종이랑은 좀 다르대요",
          "speaker_id": "1",
          "time": "20210903 11:05"
        },
        {
          "id": "MDRW2100029631.1.9",
          "form": "물건너 온 애구나",
          "original_form": "물건너 온 애구나",
          "speaker_id": "2",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029631.1.10",
          "form": "귀엽네",
          "original_form": "귀엽네",
          "speaker_id": "2",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029631.1.11",
          "form": "족제비는 뭐먹고 산다니?",
          "original_form": "족제비는 뭐먹고 산다니?",
          "speaker_id": "2",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029631.1.12",
          "form": "글쎄요 야채 먹지 않을까 싶은데",
          "original_form": "글쎄요 야채 먹지 않을까 싶은데",
          "speaker_id": "1",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029631.1.13",
          "form": "벌레는 안먹을거같아요 그언니가 말한적 없어서",
          "original_form": "벌레는 안먹을거같아요 그언니가 말한적 없어서",
          "speaker_id": "1",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029631.1.14",
          "form": "사료 먹지 않을까요?",
          "original_form": "사료 먹지 않을까요?",
          "speaker_id": "1",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029631.1.15",
          "form": "사료만 먹고 사는것도 고생이야",
          "original_form": "사료만 먹고 사는것도 고생이야",
          "speaker_id": "2",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029631.1.16",
          "form": "간식같은것도 주려나...",
          "original_form": "간식같은것도 주려나...",
          "speaker_id": "1",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029631.1.17",
          "form": "가족같이 아끼긴 하더라고요",
          "original_form": "가족같이 아끼긴 하더라고요",
          "speaker_id": "1",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029631.1.18",
          "form": "그 우유도 그렇고 정주면 가족되나봐요",
          "original_form": "그 우유도 그렇고 정주면 가족되나봐요",
          "speaker_id": "1",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029631.1.19",
          "form": "너도 병아리 죽었을때 엄청 울었잖아",
          "original_form": "너도 병아리 죽었을때 엄청 울었잖아",
          "speaker_id": "2",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029631.1.20",
          "form": "그건 제가 잘못했기도 했고 ㅠ",
          "original_form": "그건 제가 잘못했기도 했고 ㅠ",
          "speaker_id": "1",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029631.1.21",
          "form": "그뒤로 동물 안키우잖아요",
          "original_form": "그뒤로 동물 안키우잖아요",
          "speaker_id": "1",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029631.1.22",
          "form": "뭔가 죽일거같아서 무섭더라구요",
          "original_form": "뭔가 죽일거같아서 무섭더라구요",
          "speaker_id": "1",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029631.1.23",
          "form": "말도 못하는 애들이니 키우기 어렵지",
          "original_form": "말도 못하는 애들이니 키우기 어렵지",
          "speaker_id": "2",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029631.1.24",
          "form": "너두 말 안듣는데",
          "original_form": "너두 말 안듣는데",
          "speaker_id": "2",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029631.1.25",
          "form": "저는 그래도 개보단 낫다구요 ㅋㅋ",
          "original_form": "저는 그래도 개보단 낫다구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100029631.1.26",
          "form": "개가 말 더 잘 듣겠다",
          "original_form": "개가 말 더 잘 듣겠다",
          "speaker_id": "2",
          "time": "20210903 11:09"
        }
      ]
    }
  ]
