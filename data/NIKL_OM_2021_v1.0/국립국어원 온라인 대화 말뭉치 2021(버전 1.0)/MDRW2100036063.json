      "utterance": [
        {
          "id": "MDRW2100036063.1.1",
          "form": "취업하기 쉽지않네요",
          "original_form": "취업하기 쉽지않네요",
          "speaker_id": "2",
          "time": "20210830 15:43"
        },
        {
          "id": "MDRW2100036063.1.2",
          "form": "취업진짜 쉽지않죠,,",
          "original_form": "취업진짜 쉽지않죠,,",
          "speaker_id": "1",
          "time": "20210830 17:43"
        },
        {
          "id": "MDRW2100036063.1.3",
          "form": "일하고 계시나요?",
          "original_form": "일하고 계시나요?",
          "speaker_id": "2",
          "time": "20210830 18:13"
        },
        {
          "id": "MDRW2100036063.1.4",
          "form": "저는 이번달에 퇴사해서 쉬고있어요!",
          "original_form": "저는 이번달에 퇴사해서 쉬고있어요!",
          "speaker_id": "1",
          "time": "20210830 18:13"
        },
        {
          "id": "MDRW2100036063.1.5",
          "form": "오! 실업 급여 타시면서요?!",
          "original_form": "오! 실업 급여 타시면서요?!",
          "speaker_id": "2",
          "time": "20210830 18:17"
        },
        {
          "id": "MDRW2100036063.1.6",
          "form": "네!  이번달이 실업급여 받는 첫달이였어요ㅎㅎ",
          "original_form": "네!  이번달이 실업급여 받는 첫달이였어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 18:27"
        },
        {
          "id": "MDRW2100036063.1.7",
          "form": "꺄 대박대박",
          "original_form": "꺄 대박대박",
          "speaker_id": "2",
          "time": "20210830 18:37"
        },
        {
          "id": "MDRW2100036063.1.8",
          "form": "저는 실업급여 받아보면 일하기 싫더라구요ㅋㅋ",
          "original_form": "저는 실업급여 받아보면 일하기 싫더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:37"
        },
        {
          "id": "MDRW2100036063.1.9",
          "form": "어 맞아요 지금 벌써 갓수의삶에 눈이 돌아있어요ㅋㅋㅋㄱㅋ",
          "original_form": "어 맞아요 지금 벌써 갓수의삶에 눈이 돌아있어요ㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210830 18:42"
        },
        {
          "id": "MDRW2100036063.1.10",
          "form": "스펙쌓아야되는데 토익하기싫어요,,",
          "original_form": "스펙쌓아야되는데 토익하기싫어요,,",
          "speaker_id": "1",
          "time": "20210830 18:42"
        },
        {
          "id": "MDRW2100036063.1.11",
          "form": "ㅋㅋㅋ진짜 글러먹은 사람 되기 딱 좋아요 기쁘고ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ진짜 글러먹은 사람 되기 딱 좋아요 기쁘고ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100036063.1.12",
          "form": "ㅋㄱㅋㅋㅋㄱㅋ눈빛이 어딘가모르게 탁해지고있어요",
          "original_form": "ㅋㄱㅋㅋㅋㄱㅋ눈빛이 어딘가모르게 탁해지고있어요",
          "speaker_id": "1",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100036063.1.13",
          "form": "아... 저두요..  실생활에 쓰이는것도 아닌데.. 너무 많이 차지하잖아요옷!",
          "original_form": "아... 저두요..  실생활에 쓰이는것도 아닌데.. 너무 많이 차지하잖아요옷!",
          "speaker_id": "2",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100036063.1.14",
          "form": "ㅋㅋㅋㅋ그쵸그쵸ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ그쵸그쵸ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100036063.1.15",
          "form": "일하면서 영어 한번도안썼는데 토익이 왜필요한지",
          "original_form": "일하면서 영어 한번도안썼는데 토익이 왜필요한지",
          "speaker_id": "1",
          "time": "20210830 18:47"
        },
        {
          "id": "MDRW2100036063.1.16",
          "form": "그니까요ㅋㅋㅋ 번역하는 일 있음 다 외주주고 그러는데",
          "original_form": "그니까요ㅋㅋㅋ 번역하는 일 있음 다 외주주고 그러는데",
          "speaker_id": "2",
          "time": "20210830 18:54"
        },
        {
          "id": "MDRW2100036063.1.17",
          "form": "도대체 왜 필요한거죠",
          "original_form": "도대체 왜 필요한거죠",
          "speaker_id": "2",
          "time": "20210830 18:54"
        },
        {
          "id": "MDRW2100036063.1.18",
          "form": "토익으로 성실함을 본대요,,ㅋㅋㅋ",
          "original_form": "토익으로 성실함을 본대요,,ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:57"
        },
