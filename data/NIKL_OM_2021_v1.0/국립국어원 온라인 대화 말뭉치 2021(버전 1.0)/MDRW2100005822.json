        },
        {
          "id": "MDRW2100005822.1.2",
          "form": "상호대차 5권까지 아냐?",
          "original_form": "상호대차 5권까지 아냐?",
          "speaker_id": "2",
          "time": "20210907 13:25"
        },
        {
          "id": "MDRW2100005822.1.3",
          "form": "한권만 반납하면 한권만 들고갈수 있나..?",
          "original_form": "한권만 반납하면 한권만 들고갈수 있나..?",
          "speaker_id": "2",
          "time": "20210907 13:25"
        },
        {
          "id": "MDRW2100005822.1.4",
          "form": "이미 5권 빌렸다가 2권 반납하고 1권 빌림",
          "original_form": "이미 5권 빌렸다가 2권 반납하고 1권 빌림",
          "speaker_id": "1",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100005822.1.5",
          "form": "그러니까 1권만 반납하고 2권 빌릴 수는 있을텐데",
          "original_form": "그러니까 1권만 반납하고 2권 빌릴 수는 있을텐데",
          "speaker_id": "1",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100005822.1.6",
          "form": "내일 또 가야하니까~",
          "original_form": "내일 또 가야하니까~",
          "speaker_id": "1",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100005822.1.7",
          "form": "연장은 안돼?",
          "original_form": "연장은 안돼?",
          "speaker_id": "2",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100005822.1.8",
          "form": "한번에 알려줘라~",
          "original_form": "한번에 알려줘라~",
          "speaker_id": "2",
          "time": "20210907 13:27"
        },
        {
          "id": "MDRW2100005822.1.9",
          "form": "이미 연장된 상태",
          "original_form": "이미 연장된 상태",
          "speaker_id": "1",
          "time": "20210907 13:27"
        },
        {
          "id": "MDRW2100005822.1.10",
          "form": "b시는 요즘 대출할때 그냥 3주 찍어서 주더라고",
          "original_form": "b시는 요즘 대출할때 그냥 3주 찍어서 주더라고",
          "speaker_id": "1",
          "time": "20210907 13:27"
        },
        {
          "id": "MDRW2100005822.1.11",
          "form": "자동 연장",
          "original_form": "자동 연장",
          "speaker_id": "1",
          "time": "20210907 13:27"
        },
        {
          "id": "MDRW2100005822.1.12",
          "form": "글쿠먼",
          "original_form": "글쿠먼",
          "speaker_id": "2",
          "time": "20210907 13:28"
        },
        {
          "id": "MDRW2100005822.1.13",
          "form": "낼 반납해야하는거 뭐뭔데?",
          "original_form": "낼 반납해야하는거 뭐뭔데?",
          "speaker_id": "2",
          "time": "20210907 13:28"
        },
        {
          "id": "MDRW2100005822.1.14",
          "form": "책읽기좋아하는사람.. 그거",
          "original_form": "책읽기좋아하는사람.. 그거",
          "speaker_id": "1",
          "time": "20210907 13:30"
        },
        {
          "id": "MDRW2100005822.1.15",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "2",
          "time": "20210907 13:35"
        },
        {
          "id": "MDRW2100005822.1.16",
          "form": "번거롭게 됐구먼 알았음 아침에 챙겨줬을텐데",
          "original_form": "번거롭게 됐구먼 알았음 아침에 챙겨줬을텐데",
          "speaker_id": "2",
          "time": "20210907 13:35"
        },
        {
          "id": "MDRW2100005822.1.17",
          "form": "응 나도 오늘 아침엔 깜빡함",
          "original_form": "응 나도 오늘 아침엔 깜빡함",
