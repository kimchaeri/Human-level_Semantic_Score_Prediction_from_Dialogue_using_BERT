      "utterance": [
        {
          "id": "MDRW2100034380.1.1",
          "form": "게임 하시는거 있으세요?",
          "original_form": "게임 하시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210828 01:46"
        },
        {
          "id": "MDRW2100034380.1.2",
          "form": "디즈니 매직킹덤이라는 모바일 게임 하나 하고있어요",
          "original_form": "디즈니 매직킹덤이라는 모바일 게임 하나 하고있어요",
          "speaker_id": "2",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100034380.1.3",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100034380.1.4",
          "form": "엄청 좋아하진 않지만 몇몇개 돌아가면서 하는편이에용ㅋㄱㅋ",
          "original_form": "엄청 좋아하진 않지만 몇몇개 돌아가면서 하는편이에용ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100034380.1.5",
          "form": "매직킹덤 저도 했었는데 반갑네요!!",
          "original_form": "매직킹덤 저도 했었는데 반갑네요!!",
          "speaker_id": "1",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100034380.1.6",
          "form": "지금은 안하시나요?!",
          "original_form": "지금은 안하시나요?!",
          "speaker_id": "2",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100034380.1.7",
          "form": "지금 달마시안 데려오기 시작했는데요ㅠ",
          "original_form": "지금 달마시안 데려오기 시작했는데요ㅠ",
          "speaker_id": "2",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100034380.1.8",
          "form": "지금은 안해요ㅜㅠ 너무 현질유도도 심하고 짜져서ㅠㅜ 지치더라구요\n디즈니 좋아해서 캐릭터들도 열심히 모으고했는데ㅠ",
          "original_form": "지금은 안해요ㅜㅠ 너무 현질유도도 심하고 짜져서ㅠㅜ 지치더라구요\n디즈니 좋아해서 캐릭터들도 열심히 모으고했는데ㅠ",
          "speaker_id": "1",
          "time": "20210828 14:57"
        },
        {
          "id": "MDRW2100034380.1.9",
          "form": "달마시안 너무 귀엽겠어용ㅎㅎ",
          "original_form": "달마시안 너무 귀엽겠어용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 14:57"
        },
        {
          "id": "MDRW2100034380.1.10",
          "form": "오랜만에 이벤트 할 맛이 난답니다..",
          "original_form": "오랜만에 이벤트 할 맛이 난답니다..",
          "speaker_id": "2",
          "time": "20210828 15:02"
        },
        {
          "id": "MDRW2100034380.1.11",
          "form": "현질유도 심하긴하죠 진짜",
          "original_form": "현질유도 심하긴하죠 진짜",
          "speaker_id": "2",
          "time": "20210828 15:02"
        },
        {
          "id": "MDRW2100034380.1.12",
          "form": "점점 신입유저들이 시작하기 어려워지는 것 같아요",
          "original_form": "점점 신입유저들이 시작하기 어려워지는 것 같아요",
          "speaker_id": "2",
          "time": "20210828 15:03"
        },
        {
          "id": "MDRW2100034380.1.13",
          "form": "네네 조금만 안해도 따라잡기 힘들고 계속 그것만 붙잡고있게되는것같아요ㅠ",
          "original_form": "네네 조금만 안해도 따라잡기 힘들고 계속 그것만 붙잡고있게되는것같아요ㅠ",
          "speaker_id": "1",
          "time": "20210828 15:05"
        },
        {
          "id": "MDRW2100034380.1.14",
          "form": "요즘은 어떤 게임 하세요?",
          "original_form": "요즘은 어떤 게임 하세요?",
          "speaker_id": "2",
          "time": "20210828 15:08"
        },
        {
          "id": "MDRW2100034380.1.15",
          "form": "요즘은 꿈의집 하고있어요!!",
          "original_form": "요즘은 꿈의집 하고있어요!!",
          "speaker_id": "1",
          "time": "20210828 15:12"
        },
        {
          "id": "MDRW2100034380.1.16",
          "form": "꿈의집 해보셨어요?",
          "original_form": "꿈의집 해보셨어요?",
          "speaker_id": "1",
          "time": "20210828 15:12"
        },
        {
          "id": "MDRW2100034380.1.17",
          "form": "아 진짜 열심히 했었는데",
          "original_form": "아 진짜 열심히 했었는데",
          "speaker_id": "2",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100034380.1.18",
          "form": "그 집사 이름 뭐였죠? 저스틴?",
          "original_form": "그 집사 이름 뭐였죠? 저스틴?",
          "speaker_id": "2",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100034380.1.19",
          "form": "너무 세세하게 일 시키는 걸로 늘길래 하차했어요 ㅎㅎ",
          "original_form": "너무 세세하게 일 시키는 걸로 늘길래 하차했어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 15:13"
        },
        {
          "id": "MDRW2100034380.1.20",
          "form": "앜ㅋㅋㄱㄱㅋㄱㅋ오스틴?",
          "original_form": "앜ㅋㅋㄱㄱㅋㄱㅋ오스틴?",
          "speaker_id": "1",
          "time": "20210828 15:14"
        },
        {
          "id": "MDRW2100034380.1.21",
          "form": "전 그래서 집안고치고ㅋㅋㅋㅋ",
          "original_form": "전 그래서 집안고치고ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:14"
        },
        {
          "id": "MDRW2100034380.1.22",
          "form": "레벨만 클리어해옄ㅋㅋㅋ",
          "original_form": "레벨만 클리어해옄ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:14"
        },
        {
          "id": "MDRW2100034380.1.23",
          "form": "귀찮아섴ㄱㅋ",
          "original_form": "귀찮아섴ㄱㅋ",
          "speaker_id": "1",
          "time": "20210828 15:14"
        },
        {
          "id": "MDRW2100034380.1.24",
          "form": "진짜 오만거 다 시키지않아요?",
          "original_form": "진짜 오만거 다 시키지않아요?",
          "speaker_id": "2",
          "time": "20210828 15:16"
        },
        {
          "id": "MDRW2100034380.1.25",
          "form": "집사놈이 편지도 받아달라 그러고 ㅋㅋㅋㅋ",
          "original_form": "집사놈이 편지도 받아달라 그러고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:16"
        },
        {
          "id": "MDRW2100034380.1.26",
          "form": "으아 맞아요",
          "original_form": "으아 맞아요",
          "speaker_id": "1",
          "time": "20210828 15:17"
        },
        {
          "id": "MDRW2100034380.1.27",
          "form": "안궁금한데 자꾸 말하고ㅋㅋㄱㄲ",
          "original_form": "안궁금한데 자꾸 말하고ㅋㅋㄱㄲ",
          "speaker_id": "1",
          "time": "20210828 15:17"
        },
        {
          "id": "MDRW2100034380.1.28",
          "form": "제발 말하지 말아줘...",
          "original_form": "제발 말하지 말아줘...",
