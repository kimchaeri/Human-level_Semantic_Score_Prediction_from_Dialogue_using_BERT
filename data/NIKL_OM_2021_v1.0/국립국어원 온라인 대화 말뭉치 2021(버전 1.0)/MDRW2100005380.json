      "utterance": [
        {
          "id": "MDRW2100005380.1.1",
          "form": "차달남은 광고 엄청 길다 기다리다 지치겠다",
          "original_form": "차달남은 광고 엄청 길다 기다리다 지치겠다",
          "speaker_id": "2",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005380.1.2",
          "form": "ㅋㅋ 한 15~20분 할껄",
          "original_form": "ㅋㅋ 한 15~20분 할껄",
          "speaker_id": "1",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005380.1.3",
          "form": "엄청 긴거 같다 안보던 광고 덕분에 보겠다",
          "original_form": "엄청 긴거 같다 안보던 광고 덕분에 보겠다",
          "speaker_id": "2",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005380.1.4",
          "form": "다른데 봤다가 돌려",
          "original_form": "다른데 봤다가 돌려",
          "speaker_id": "1",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005380.1.5",
          "form": "채널돌리기 귀찮다",
          "original_form": "채널돌리기 귀찮다",
          "speaker_id": "2",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005380.1.6",
          "form": "그것도 귀찮아?  광고 보는거 보다 낫겠다",
          "original_form": "그것도 귀찮아?  광고 보는거 보다 낫겠다",
          "speaker_id": "1",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005380.1.7",
          "form": "ㅋㅋ 다른데 틀어도 재밌는것도 없는데뭐",
          "original_form": "ㅋㅋ 다른데 틀어도 재밌는것도 없는데뭐",
          "speaker_id": "2",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005380.1.8",
          "form": "이제 한다",
          "original_form": "이제 한다",
          "speaker_id": "2",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005380.1.9",
          "form": "뉴스 보다가 계속 채널 돌림",
          "original_form": "뉴스 보다가 계속 채널 돌림",
          "speaker_id": "1",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005380.1.10",
          "form": "이제 뭐함?",
          "original_form": "이제 뭐함?",
          "speaker_id": "1",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005380.1.11",
          "form": "기이한 동물 사건이란다 재밌겠다 안본거라",
          "original_form": "기이한 동물 사건이란다 재밌겠다 안본거라",
          "speaker_id": "2",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005380.1.12",
          "form": "매번 할 꺼리가 생긴다는게 신기하다",
          "original_form": "매번 할 꺼리가 생긴다는게 신기하다",
          "speaker_id": "1",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005380.1.13",
          "form": "11위 매장을 휩쓸어버린 동물이래",
          "original_form": "11위 매장을 휩쓸어버린 동물이래",
          "speaker_id": "2",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005380.1.14",
          "form": "어떤 동물이야?",
          "original_form": "어떤 동물이야?",
          "speaker_id": "1",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005380.1.15",
          "form": "편의점에 나타나서 사고를 쳤나보다",
          "original_form": "편의점에 나타나서 사고를 쳤나보다",
          "speaker_id": "2",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005380.1.16",
          "form": "헉 엄청 큰 도마뱀이다",
          "original_form": "헉 엄청 큰 도마뱀이다",
          "speaker_id": "2",
          "time": "20210821 22:12"
        }
      ]
