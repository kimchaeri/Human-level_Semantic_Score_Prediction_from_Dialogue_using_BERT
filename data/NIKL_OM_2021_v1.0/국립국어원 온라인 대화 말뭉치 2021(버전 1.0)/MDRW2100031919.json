      "utterance": [
        {
          "id": "MDRW2100031919.1.1",
          "form": "전 그래서 그러지말고 이벤트해서 받은 스벅으로 사줘요ㅎ 7백원이면 되니깐ㅎ",
          "original_form": "전 그래서 그러지말고 이벤트해서 받은 스벅으로 사줘요ㅎ 7백원이면 되니깐ㅎ",
          "speaker_id": "1",
          "time": "20210905 08:39"
        },
        {
          "id": "MDRW2100031919.1.2",
          "form": "다이어트?ㅎ",
          "original_form": "다이어트?ㅎ",
          "speaker_id": "1",
          "time": "20210905 08:40"
        },
        {
          "id": "MDRW2100031919.1.3",
          "form": "항상하시죠?",
          "original_form": "항상하시죠?",
          "speaker_id": "1",
          "time": "20210905 08:40"
        },
        {
          "id": "MDRW2100031919.1.4",
          "form": "ㅋㅋ 매일ㅋㅋ 마음으롴ㅋ",
          "original_form": "ㅋㅋ 매일ㅋㅋ 마음으롴ㅋ",
          "speaker_id": "2",
          "time": "20210905 08:41"
        },
        {
          "id": "MDRW2100031919.1.5",
          "form": "음식 가려드시나요?",
          "original_form": "음식 가려드시나요?",
          "speaker_id": "2",
          "time": "20210905 08:41"
        },
        {
          "id": "MDRW2100031919.1.6",
          "form": "ㅎㅎ 마음으로만 하면 절반성공이내요ㅎ",
          "original_form": "ㅎㅎ 마음으로만 하면 절반성공이내요ㅎ",
          "speaker_id": "1",
          "time": "20210905 08:41"
        },
        {
          "id": "MDRW2100031919.1.7",
          "form": "그렇게 가리진 않아요 ㅡㅎ",
          "original_form": "그렇게 가리진 않아요 ㅡㅎ",
          "speaker_id": "1",
          "time": "20210905 08:41"
        },
        {
          "id": "MDRW2100031919.1.8",
          "form": "고기만 좋아하고 야채는 별로 안좋아하는정도?",
          "original_form": "고기만 좋아하고 야채는 별로 안좋아하는정도?",
          "speaker_id": "1",
          "time": "20210905 08:41"
        },
        {
          "id": "MDRW2100031919.1.9",
          "form": "ㅋㅋ샤브샤브로 드시면 야채흡입가능해요!",
          "original_form": "ㅋㅋ샤브샤브로 드시면 야채흡입가능해요!",
          "speaker_id": "2",
          "time": "20210905 08:42"
        },
        {
          "id": "MDRW2100031919.1.10",
          "form": "베이컨이나 햄 소세지 건강에 안좋다고ㅜ",
          "original_form": "베이컨이나 햄 소세지 건강에 안좋다고ㅜ",
          "speaker_id": "2",
          "time": "20210905 08:42"
        },
        {
          "id": "MDRW2100031919.1.11",
          "form": "하지만 다들 먹고 살겠죠..",
          "original_form": "하지만 다들 먹고 살겠죠..",
          "speaker_id": "2",
          "time": "20210905 08:43"
        },
        {
          "id": "MDRW2100031919.1.12",
          "form": "ㅎㅎ 건강에 안좋아도",
          "original_form": "ㅎㅎ 건강에 안좋아도",
          "speaker_id": "1",
          "time": "20210905 08:43"
        },
        {
          "id": "MDRW2100031919.1.13",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210905 08:43"
        },
        {
          "id": "MDRW2100031919.1.14",
          "form": "맛만있으면 되는주의라",
          "original_form": "맛만있으면 되는주의라",
          "speaker_id": "1",
          "time": "20210905 08:43"
        },
        {
          "id": "MDRW2100031919.1.15",
          "form": "남들이 뭐라해도 맛있게 먹고 살다가 죽을래요ㅎㅎㅎ",
          "original_form": "남들이 뭐라해도 맛있게 먹고 살다가 죽을래요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 08:43"
        },
        {
          "id": "MDRW2100031919.1.16",
          "form": "부모님과 함께 살아서 건강식으로 먹고싶은데",
          "original_form": "부모님과 함께 살아서 건강식으로 먹고싶은데",
          "speaker_id": "2",
          "time": "20210905 08:43"
        },
        {
          "id": "MDRW2100031919.1.17",
          "form": "ㅋㅋ저걸 포기하곤 너무 먹고싶어요!!",
          "original_form": "ㅋㅋ저걸 포기하곤 너무 먹고싶어요!!",
          "speaker_id": "2",
          "time": "20210905 08:43"
        },
        {
          "id": "MDRW2100031919.1.18",
          "form": "ㅎㅎ 그렇죠 ㅡㅎ",
          "original_form": "ㅎㅎ 그렇죠 ㅡㅎ",
          "speaker_id": "1",
          "time": "20210905 08:44"
        },
        {
          "id": "MDRW2100031919.1.19",
          "form": "전 인스턴트 요리전문가에요ㅎㅎㅎ",
          "original_form": "전 인스턴트 요리전문가에요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 08:44"
        },
        {
          "id": "MDRW2100031919.1.20",
          "form": "ㅋㅋ 마음만 다이어트시군요",
          "original_form": "ㅋㅋ 마음만 다이어트시군요",
          "speaker_id": "2",
          "time": "20210905 08:44"
        },
        {
          "id": "MDRW2100031919.1.21",
          "form": "매일 닭가슴살에 대기업의 도움을 받아 만들죠ㅎㅎ",
          "original_form": "매일 닭가슴살에 대기업의 도움을 받아 만들죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 08:44"
        },
        {
          "id": "MDRW2100031919.1.22",
          "form": "단백질은 건강식이니까요!",
          "original_form": "단백질은 건강식이니까요!",
          "speaker_id": "2",
          "time": "20210905 08:45"
        },
        {
          "id": "MDRW2100031919.1.23",
          "form": "건강하게 살찌고있는중이에요ㅎㅎㅎㅎ",
          "original_form": "건강하게 살찌고있는중이에요ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 08:45"
        },
        {
          "id": "MDRW2100031919.1.24",
          "form": "보통 그럼 뭐많이드세요? 반찬이라든지 아니면 메인?",
          "original_form": "보통 그럼 뭐많이드세요? 반찬이라든지 아니면 메인?",
