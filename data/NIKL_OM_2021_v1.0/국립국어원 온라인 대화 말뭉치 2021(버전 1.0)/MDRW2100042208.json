        {
          "id": "MDRW2100042208.1.1",
          "form": "알죠알죠",
          "original_form": "알죠알죠",
          "speaker_id": "2",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042208.1.2",
          "form": "아니그럼",
          "original_form": "아니그럼",
          "speaker_id": "1",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042208.1.3",
          "form": "진짜 배달음식",
          "original_form": "진짜 배달음식",
          "speaker_id": "1",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042208.1.4",
          "form": "자주 먹으면",
          "original_form": "자주 먹으면",
          "speaker_id": "1",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042208.1.5",
          "form": "보고 저는 제 눈이 이상하다고 생각했어요",
          "original_form": "보고 저는 제 눈이 이상하다고 생각했어요",
          "speaker_id": "2",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042208.1.6",
          "form": "진짜 건강이 골로 가겠네요?",
          "original_form": "진짜 건강이 골로 가겠네요?",
          "speaker_id": "1",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042208.1.7",
          "form": "26000원이라는 줄 알았어요",
          "original_form": "26000원이라는 줄 알았어요",
          "speaker_id": "1",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042208.1.8",
          "form": "2600 칼로리",
          "original_form": "2600 칼로리",
          "speaker_id": "1",
          "time": "20210906 20:38"
        },
        {
          "id": "MDRW2100042208.1.9",
          "form": "제 기초대사량이 1300이 안될텐데",
          "original_form": "제 기초대사량이 1300이 안될텐데",
          "speaker_id": "1",
          "time": "20210906 20:38"
        },
        {
          "id": "MDRW2100042208.1.10",
          "form": "로제 떡볶이는 5번 생각하고 먹기..",
          "original_form": "로제 떡볶이는 5번 생각하고 먹기..",
          "speaker_id": "2",
          "time": "20210906 20:39"
        },
        {
          "id": "MDRW2100042208.1.11",
          "form": "다른거 먹어요 우리 ㅠ",
          "original_form": "다른거 먹어요 우리 ㅠ",
          "speaker_id": "2",
          "time": "20210906 20:39"
        },
        {
          "id": "MDRW2100042208.1.12",
          "form": "떡볶이가 제일 살찐다고..흑",
          "original_form": "떡볶이가 제일 살찐다고..흑",
          "speaker_id": "2",
          "time": "20210906 20:39"
        },
        {
          "id": "MDRW2100042208.1.13",
          "form": "하 맞아요",
          "original_form": "하 맞아요",
          "speaker_id": "1",
          "time": "20210906 20:43"
        },
        {
          "id": "MDRW2100042208.1.14",
          "form": "진짜 떡볶이가",
          "original_form": "진짜 떡볶이가",
          "speaker_id": "1",
          "time": "20210906 20:43"
        },
        {
          "id": "MDRW2100042208.1.15",
          "form": "가장 빠르고 정확하게 살찔 수 있는 음식이라는 것을",
          "original_form": "가장 빠르고 정확하게 살찔 수 있는 음식이라는 것을",
          "speaker_id": "1",
          "time": "20210906 20:43"
        },
        {
          "id": "MDRW2100042208.1.16",
          "form": "제가 직접 느꼈거든요!!!",
          "original_form": "제가 직접 느꼈거든요!!!",
          "speaker_id": "1",
          "time": "20210906 20:43"
        },
        {
          "id": "MDRW2100042208.1.17",
          "form": "진짜 차라리 치킨이 나은 것 같아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "진짜 차라리 치킨이 나은 것 같아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:43"
        },
        {
          "id": "MDRW2100042208.1.18",
          "form": "차라리 치킨을 먹읍시다 ㅋㅋㅋ",
          "original_form": "차라리 치킨을 먹읍시다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:46"
        },
        {
          "id": "MDRW2100042208.1.19",
          "form": "다이어트하는사람들도 치킨은 튀김옷을 벗기고 먹는다고 하더라구요!",
          "original_form": "다이어트하는사람들도 치킨은 튀김옷을 벗기고 먹는다고 하더라구요!",
          "speaker_id": "2",
          "time": "20210906 20:46"
        },
        {
          "id": "MDRW2100042208.1.20",
          "form": "하지만 튀김옷은 진짜",
          "original_form": "하지만 튀김옷은 진짜",
          "speaker_id": "1",
          "time": "20210906 20:50"
        },
        {
          "id": "MDRW2100042208.1.21",
          "form": "튀김옷이 진짠데 (?)",
          "original_form": "튀김옷이 진짠데 (?)",
          "speaker_id": "1",
          "time": "20210906 20:50"
        },
        {
          "id": "MDRW2100042208.1.22",
          "form": "아니 너무 심각해요 진짜 ㅋㅋㅋ 적당히 막아야지",
          "original_form": "아니 너무 심각해요 진짜 ㅋㅋㅋ 적당히 막아야지",
          "speaker_id": "2",
          "time": "20210906 20:50"
        },
        {
          "id": "MDRW2100042208.1.23",
          "form": "저희는 그냥 먹고 다이어트 하는 뷴들만요!ㅋㅋㅋ",
          "original_form": "저희는 그냥 먹고 다이어트 하는 뷴들만요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:50"
        },
        {
          "id": "MDRW2100042208.1.24",
          "form": "아아아아아",
          "original_form": "아아아아아",
          "speaker_id": "1",
          "time": "20210906 20:50"
        },
        {
          "id": "MDRW2100042208.1.25",
          "form": "아 웃기다 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 웃기다 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:50"
        },
        {
          "id": "MDRW2100042208.1.26",
          "form": "네네 안심하고 먹을게요",
          "original_form": "네네 안심하고 먹을게요",
          "speaker_id": "1",
          "time": "20210906 20:50"
        },
        {
          "id": "MDRW2100042208.1.27",
          "form": "와 근데 떡볶이....",
          "original_form": "와 근데 떡볶이....",
          "speaker_id": "1",
          "time": "20210906 20:51"
        },
        {
          "id": "MDRW2100042208.1.28",
          "form": "진짜 무서운 녀석이네요....",
          "original_form": "진짜 무서운 녀석이네요....",
          "speaker_id": "1",
          "time": "20210906 20:51"
        },
        {
          "id": "MDRW2100042208.1.29",
          "form": "떡볶이 싫어하는 사람 부럽다...",
          "original_form": "떡볶이 싫어하는 사람 부럽다...",
          "speaker_id": "1",
          "time": "20210906 20:51"
        },
        {
          "id": "MDRW2100042208.1.30",
          "form": "로제 말고 일반 떡볶이로 타협해요 저희..",
          "original_form": "로제 말고 일반 떡볶이로 타협해요 저희..",
          "speaker_id": "2",
          "time": "20210906 20:53"
        },
        {
          "id": "MDRW2100042208.1.31",
          "form": "로제는 진짜 가끔만 먹어요",
          "original_form": "로제는 진짜 가끔만 먹어요",
          "speaker_id": "2",
          "time": "20210906 20:53"
        },
        {
          "id": "MDRW2100042208.1.32",
          "form": "그..그래야겠어요",
          "original_form": "그..그래야겠어요",
          "speaker_id": "1",
          "time": "20210906 20:53"
        },
        {
          "id": "MDRW2100042208.1.33",
          "form": "로제야.....",
          "original_form": "로제야.....",
          "speaker_id": "1",
          "time": "20210906 20:53"
        },
        {
          "id": "MDRW2100042208.1.34",
          "form": "저정도 컬로리면 아마 몸에도 무리 올 것 같아요 ㅋㅋ",
          "original_form": "저정도 컬로리면 아마 몸에도 무리 올 것 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:53"
        },
        {
          "id": "MDRW2100042208.1.35",
          "form": "아니그러니까요",
          "original_form": "아니그러니까요",
          "speaker_id": "1",
          "time": "20210906 20:54"
        },
        {
          "id": "MDRW2100042208.1.36",
          "form": "젊은 사람들 위암 많이 걸린다면서요 ㅠㅠ",
          "original_form": "젊은 사람들 위암 많이 걸린다면서요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 20:54"
        },
        {
          "id": "MDRW2100042208.1.37",
          "form": "왜냐면 저희는 저거만 먹지 않으니까!!",
          "original_form": "왜냐면 저희는 저거만 먹지 않으니까!!",
