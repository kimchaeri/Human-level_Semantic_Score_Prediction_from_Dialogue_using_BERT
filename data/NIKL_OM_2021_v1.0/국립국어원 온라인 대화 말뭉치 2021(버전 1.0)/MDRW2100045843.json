        },
        {
          "id": "MDRW2100045843.1.3",
          "form": "애슐리",
          "original_form": "애슐리",
          "speaker_id": "2",
          "time": "20210908 07:58"
        },
        {
          "id": "MDRW2100045843.1.4",
          "form": "애슐리 마늘 컨셉일 때 진짜 맛있는데",
          "original_form": "애슐리 마늘 컨셉일 때 진짜 맛있는데",
          "speaker_id": "1",
          "time": "20210908 07:58"
        },
        {
          "id": "MDRW2100045843.1.5",
          "form": "어떤ㅁ음식이 있어",
          "original_form": "어떤ㅁ음식이 있어",
          "speaker_id": "2",
          "time": "20210908 07:58"
        },
        {
          "id": "MDRW2100045843.1.6",
          "form": "그냥 온통 마늘이야",
          "original_form": "그냥 온통 마늘이야",
          "speaker_id": "1",
          "time": "20210908 07:58"
        },
        {
          "id": "MDRW2100045843.1.7",
          "form": "볶음밥같은거?",
          "original_form": "볶음밥같은거?",
          "speaker_id": "2",
          "time": "20210908 07:59"
        },
        {
          "id": "MDRW2100045843.1.8",
          "form": "맞아. 마늘 스테이크도 있고",
          "original_form": "맞아. 마늘 스테이크도 있고",
          "speaker_id": "1",
          "time": "20210908 07:59"
        },
        {
          "id": "MDRW2100045843.1.9",
          "form": "나는 민트 페스티벌일 때 맛있게 먹음",
          "original_form": "나는 민트 페스티벌일 때 맛있게 먹음",
          "speaker_id": "2",
          "time": "20210908 07:59"
        },
        {
          "id": "MDRW2100045843.1.10",
          "form": "어떤 민트 음식이 있어",
          "original_form": "어떤 민트 음식이 있어",
          "speaker_id": "1",
          "time": "20210908 07:59"
        },
        {
          "id": "MDRW2100045843.1.11",
          "form": "일단 민트 와플",
          "original_form": "일단 민트 와플",
          "speaker_id": "2",
          "time": "20210908 07:59"
        },
        {
          "id": "MDRW2100045843.1.12",
          "form": "와 듣기만 해도 최곤데",
          "original_form": "와 듣기만 해도 최곤데",
          "speaker_id": "1",
          "time": "20210908 07:59"
        },
