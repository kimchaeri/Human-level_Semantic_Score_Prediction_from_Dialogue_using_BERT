        {
          "id": "MDRW2100020233.1.1",
          "form": "게임 종종 하세요?",
          "original_form": "게임 종종 하세요?",
          "speaker_id": "1",
          "time": "20210828 00:32"
        },
        {
          "id": "MDRW2100020233.1.2",
          "form": "예전엔 많이 했었어요",
          "original_form": "예전엔 많이 했었어요",
          "speaker_id": "2",
          "time": "20210828 00:33"
        },
        {
          "id": "MDRW2100020233.1.3",
          "form": "어떤 거 하셨어요?",
          "original_form": "어떤 거 하셨어요?",
          "speaker_id": "1",
          "time": "20210828 00:35"
        },
        {
          "id": "MDRW2100020233.1.4",
          "form": "큐플레이..",
          "original_form": "큐플레이..",
          "speaker_id": "2",
          "time": "20210828 00:35"
        },
        {
          "id": "MDRW2100020233.1.5",
          "form": "크레이지아케이드",
          "original_form": "크레이지아케이드",
          "speaker_id": "2",
          "time": "20210828 00:35"
        },
        {
          "id": "MDRW2100020233.1.6",
          "form": "알투비트",
          "original_form": "알투비트",
          "speaker_id": "2",
          "time": "20210828 00:35"
        },
        {
          "id": "MDRW2100020233.1.7",
          "form": "ㅋㅋ 뭔가 추억의 이름들이네요",
          "original_form": "ㅋㅋ 뭔가 추억의 이름들이네요",
          "speaker_id": "1",
          "time": "20210828 00:37"
        },
        {
          "id": "MDRW2100020233.1.8",
          "form": "테일즈위버",
          "original_form": "테일즈위버",
          "speaker_id": "2",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100020233.1.9",
          "form": "테트리스",
          "original_form": "테트리스",
          "speaker_id": "2",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100020233.1.10",
          "form": "맞고..ㅋㅋㅋ요런거",
          "original_form": "맞고..ㅋㅋㅋ요런거",
          "speaker_id": "2",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100020233.1.11",
          "form": "다양하네요ㅋㅋ 요즘엔 전혀 안 하세요?",
          "original_form": "다양하네요ㅋㅋ 요즘엔 전혀 안 하세요?",
          "speaker_id": "1",
          "time": "20210828 00:39"
        },
        {
          "id": "MDRW2100020233.1.12",
          "form": "네 ㅋㅋ요즘은 딱히",
          "original_form": "네 ㅋㅋ요즘은 딱히",
          "speaker_id": "2",
          "time": "20210828 00:41"
        },
        {
          "id": "MDRW2100020233.1.13",
          "form": "재밌는게 없네요",
          "original_form": "재밌는게 없네요",
          "speaker_id": "2",
          "time": "20210828 00:41"
        },
        {
          "id": "MDRW2100020233.1.14",
          "form": "스마트폰게임은 \n캔디크러쉬랑 레스토랑카페?? 운영하는거 그런거",
          "original_form": "스마트폰게임은 \n캔디크러쉬랑 레스토랑카페?? 운영하는거 그런거",
          "speaker_id": "2",
          "time": "20210828 00:41"
        },
        {
          "id": "MDRW2100020233.1.15",
          "form": "저도 요즘은 가끔 프렌즈타운 하는 정도예요",
          "original_form": "저도 요즘은 가끔 프렌즈타운 하는 정도예요",
          "speaker_id": "1",
          "time": "20210828 00:42"
        },
        {
          "id": "MDRW2100020233.1.16",
          "form": "카톡게임일까요 들어본거같아요",
          "original_form": "카톡게임일까요 들어본거같아요",
          "speaker_id": "2",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020233.1.17",
          "form": "퍼즐 게임? 종류에요ㅋㅋ",
          "original_form": "퍼즐 게임? 종류에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:44"
        },
        {
          "id": "MDRW2100020233.1.18",
          "form": "캔디크러쉬랑 비슷한 느낌",
          "original_form": "캔디크러쉬랑 비슷한 느낌",
          "speaker_id": "1",
          "time": "20210828 00:45"
        },
        {
          "id": "MDRW2100020233.1.19",
          "form": "아 ㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:45"
        },
        {
          "id": "MDRW2100020233.1.20",
          "form": "어쩌다 가끔 하긴 해도 하다보면 계속하게 되더라구요",
          "original_form": "어쩌다 가끔 하긴 해도 하다보면 계속하게 되더라구요",
          "speaker_id": "1",
          "time": "20210828 00:47"
        },
        {
          "id": "MDRW2100020233.1.21",
          "form": "중독인것같아요 ㅋㅋ",
