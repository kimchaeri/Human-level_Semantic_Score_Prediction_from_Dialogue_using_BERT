      "utterance": [
        {
          "id": "MDRW2100014948.1.1",
          "form": "공연 좋아하시나요??",
          "original_form": "공연 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014948.1.2",
          "form": "공연은 보러간적이 없어요",
          "original_form": "공연은 보러간적이 없어요",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014948.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014948.1.4",
          "form": "저는 뮤지컬 콘서트 좋아해요 ㅎㅎ",
          "original_form": "저는 뮤지컬 콘서트 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014948.1.5",
          "form": "뮤지컬 어떤극 최근에 보셨나요",
          "original_form": "뮤지컬 어떤극 최근에 보셨나요",
          "speaker_id": "2",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014948.1.6",
          "form": "저 최근에 레드북 이요!",
          "original_form": "저 최근에 레드북 이요!",
          "speaker_id": "1",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014948.1.7",
          "form": "들어보셨나요?",
          "original_form": "들어보셨나요?",
          "speaker_id": "1",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014948.1.8",
          "form": "내용은 모르는데 차지연씨 나오는 극 아닌가요?",
          "original_form": "내용은 모르는데 차지연씨 나오는 극 아닌가요?",
          "speaker_id": "2",
          "time": "20210825 21:25"
        },
        {
          "id": "MDRW2100014948.1.9",
          "form": "네 맞아요!",
          "original_form": "네 맞아요!",
          "speaker_id": "1",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014948.1.10",
          "form": "차지연씨 팬이신가요??",
          "original_form": "차지연씨 팬이신가요??",
          "speaker_id": "1",
          "time": "20210825 21:27"
        },
        {
          "id": "MDRW2100014948.1.11",
          "form": "팬이라고 말하기엔 너무 소소하구요 한번 차지연씨가 하시던 레베카 영상보구 너무 인상깊어서 기억이나요",
          "original_form": "팬이라고 말하기엔 너무 소소하구요 한번 차지연씨가 하시던 레베카 영상보구 너무 인상깊어서 기억이나요",
          "speaker_id": "2",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014948.1.12",
          "form": "레베카 노래 대박 ㅠ 언젠가 라이브로 들어보고 싶어요",
          "original_form": "레베카 노래 대박 ㅠ 언젠가 라이브로 들어보고 싶어요",
          "speaker_id": "1",
          "time": "20210825 21:29"
        },
        {
          "id": "MDRW2100014948.1.13",
          "form": "가능만하다면 옥주현씨의 레베카도",
          "original_form": "가능만하다면 옥주현씨의 레베카도",
          "speaker_id": "2",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014948.1.14",
          "form": "차지연 님의 레베카도",
          "original_form": "차지연 님의 레베카도",
          "speaker_id": "2",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014948.1.15",
          "form": "보고싶어요....",
          "original_form": "보고싶어요....",
          "speaker_id": "2",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100014948.1.16",
          "form": "자랑은 아니지만 옥주현님 레베카를 보았습니다",
          "original_form": "자랑은 아니지만 옥주현님 레베카를 보았습니다",
          "speaker_id": "1",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014948.1.17",
          "form": "혼자 마이크 다른 거 쓰시더라구요 성량이",
          "original_form": "혼자 마이크 다른 거 쓰시더라구요 성량이",
          "speaker_id": "1",
          "time": "20210825 21:31"
        },
        {
          "id": "MDRW2100014948.1.18",
          "form": "와......",
          "original_form": "와......",
          "speaker_id": "2",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014948.1.19",
          "form": "너무 부러워요ㅠㅠㅠㅠ",
          "original_form": "너무 부러워요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014948.1.20",
          "form": "성량 영상으로봐도 넘사셨는데",
          "original_form": "성량 영상으로봐도 넘사셨는데",
          "speaker_id": "2",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014948.1.21",
          "form": "실제로들으면 더해요?ㅠㅠㅠㅜ",
          "original_form": "실제로들으면 더해요?ㅠㅠㅠㅜ",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014948.1.22",
          "form": "다른 내용이나 노래는 기억이 안나고",
          "original_form": "다른 내용이나 노래는 기억이 안나고",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014948.1.23",
          "form": "옥주현님의 레베카만 맴돌아요",
          "original_form": "옥주현님의 레베카만 맴돌아요",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014948.1.24",
          "form": "첫 등장때부터 소름",
          "original_form": "첫 등장때부터 소름",
          "speaker_id": "1",
          "time": "20210825 21:33"
        }
      ]
    }
