      "utterance": [
        {
          "id": "MDRW2100016237.1.1",
          "form": "네 좋네옷",
          "original_form": "네 좋네옷",
          "speaker_id": "2",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016237.1.2",
          "form": "간만에 비가 안오네요",
          "original_form": "간만에 비가 안오네요",
          "speaker_id": "2",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016237.1.3",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016237.1.4",
          "form": "그래서 조깅도 했어요",
          "original_form": "그래서 조깅도 했어요",
          "speaker_id": "1",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100016237.1.5",
          "form": "잘하셨네요",
          "original_form": "잘하셨네요",
          "speaker_id": "2",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016237.1.6",
          "form": "어떤 계절 제일 좋아하세요?",
          "original_form": "어떤 계절 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016237.1.7",
          "form": "맞춰보세요",
          "original_form": "맞춰보세요",
          "speaker_id": "1",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016237.1.8",
          "form": "음 봄이요",
          "original_form": "음 봄이요",
          "speaker_id": "2",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016237.1.9",
          "form": "님은 봄인가요?",
          "original_form": "님은 봄인가요?",
          "speaker_id": "1",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016237.1.10",
          "form": "헐 아닌데요",
          "original_form": "헐 아닌데요",
          "speaker_id": "2",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016237.1.11",
          "form": "정답입니다라락",
          "original_form": "정답입니다라락",
          "speaker_id": "1",
          "time": "20210826 11:16"
        },
        {
          "id": "MDRW2100016237.1.12",
          "form": "여름이죠",
          "original_form": "여름이죠",
          "speaker_id": "1",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016237.1.13",
          "form": "그럴 줄 알았어요",
          "original_form": "그럴 줄 알았어요",
          "speaker_id": "2",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016237.1.14",
          "form": "헐 아닌데요",
          "original_form": "헐 아닌데요",
          "speaker_id": "2",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016237.1.15",
          "form": "여름 제일 싫어해요",
          "original_form": "여름 제일 싫어해요",
          "speaker_id": "2",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016237.1.16",
          "form": "저는 겨울이 제일 좋아요",
          "original_form": "저는 겨울이 제일 좋아요",
          "speaker_id": "2",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016237.1.17",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "1",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016237.1.18",
          "form": "전 싫은데",
          "original_form": "전 싫은데",
          "speaker_id": "1",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016237.1.19",
          "form": "날씨는 가을이 좋지만 모기가 많거든요",
          "original_form": "날씨는 가을이 좋지만 모기가 많거든요",
          "speaker_id": "2",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016237.1.20",
          "form": "겨울 왜 좋아하세요",
          "original_form": "겨울 왜 좋아하세요",
          "speaker_id": "1",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016237.1.21",
          "form": "봄 좋아하시니",
          "original_form": "봄 좋아하시니",
          "speaker_id": "2",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016237.1.22",
          "form": "그라요??",
          "original_form": "그라요??",
          "speaker_id": "1",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016237.1.23",
          "form": "겨울이 싫으실 수 있죠",
          "original_form": "겨울이 싫으실 수 있죠",
          "speaker_id": "2",
          "time": "20210826 11:17"
        },
        {
          "id": "MDRW2100016237.1.24",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016237.1.25",
          "form": "겨울 포근해요",
          "original_form": "겨울 포근해요",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016237.1.26",
          "form": "네??",
          "original_form": "네??",
          "speaker_id": "1",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016237.1.27",
          "form": "개추운대요",
          "original_form": "개추운대요",
          "speaker_id": "1",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016237.1.28",
          "form": "이런 제 갬성을 이해하지 못하시는군요",
          "original_form": "이런 제 갬성을 이해하지 못하시는군요",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016237.1.29",
          "form": "춥지만",
          "original_form": "춥지만",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016237.1.30",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016237.1.31",
          "form": "꺼입을 수 있잖아요",
          "original_form": "꺼입을 수 있잖아요",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016237.1.32",
          "form": "겨",
          "original_form": "겨",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016237.1.33",
          "form": "껴",
          "original_form": "껴",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016237.1.34",
          "form": "으 껴압어도 갸추워요",
          "original_form": "으 껴압어도 갸추워요",
          "speaker_id": "1",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016237.1.35",
          "form": "가을에도 껴입을수있엉ㅅ",
          "original_form": "가을에도 껴입을수있엉ㅅ",
          "speaker_id": "1",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016237.1.36",
          "form": "님이 추위를 잘 타시나보네요",
          "original_form": "님이 추위를 잘 타시나보네요",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016237.1.37",
          "form": "너무 껴입어서 불편햐요 전",
          "original_form": "너무 껴입어서 불편햐요 전",
          "speaker_id": "1",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016237.1.38",
          "form": "가을에는 한계가 있어요",
          "original_form": "가을에는 한계가 있어요",
          "speaker_id": "2",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016237.1.39",
          "form": "네다섯 겹 입잖아요",
          "original_form": "네다섯 겹 입잖아요",
          "speaker_id": "1",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016237.1.40",
          "form": "그러는거 좋아하세요(?",
          "original_form": "그러는거 좋아하세요(?",
          "speaker_id": "1",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016237.1.41",
          "form": "그건 겨울인데요",
          "original_form": "그건 겨울인데요",
          "speaker_id": "2",
          "time": "20210826 11:19"
        },
        {
          "id": "MDRW2100016237.1.42",
          "form": "네 전 좋아해요",
          "original_form": "네 전 좋아해요",
          "speaker_id": "2",
          "time": "20210826 11:19"
        },
        {
          "id": "MDRW2100016237.1.43",
          "form": "하나씩 벗는 재미가 있거든요",
          "original_form": "하나씩 벗는 재미가 있거든요",
          "speaker_id": "2",
          "time": "20210826 11:19"
        },
        {
          "id": "MDRW2100016237.1.44",
          "form": "학교 다닐 때도 최소 6겹씩 입고 다녔어요",
          "original_form": "학교 다닐 때도 최소 6겹씩 입고 다녔어요",
          "speaker_id": "2",
          "time": "20210826 11:19"
        },
        {
          "id": "MDRW2100016237.1.45",
          "form": "어딜 가서 벗 으세요?",
          "original_form": "어딜 가서 벗 으세요?",
          "speaker_id": "1",
          "time": "20210826 11:19"
        },
        {
          "id": "MDRW2100016237.1.46",
          "form": "전 추워서 못 벗 눈데",
          "original_form": "전 추워서 못 벗 눈데",
          "speaker_id": "1",
          "time": "20210826 11:19"
        },
        {
          "id": "MDRW2100016237.1.47",
          "form": "내복 - 목티 - 셔츠 - 조끼 - 맨투맨 - 패딩조끼 - 롱패디우",
          "original_form": "내복 - 목티 - 셔츠 - 조끼 - 맨투맨 - 패딩조끼 - 롱패디우",
          "speaker_id": "2",
          "time": "20210826 11:20"
        },
        {
          "id": "MDRW2100016237.1.48",
          "form": "이렇게요",
          "original_form": "이렇게요",
          "speaker_id": "2",
          "time": "20210826 11:20"
        },
        {
          "id": "MDRW2100016237.1.49",
          "form": "띠용",
          "original_form": "띠용",
          "speaker_id": "2",
          "time": "20210826 11:20"
        },
        {
          "id": "MDRW2100016237.1.50",
          "form": "저랑 비슷하넹ㅋㅋ",
          "original_form": "저랑 비슷하넹ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:20"
        }
      ]
    }
