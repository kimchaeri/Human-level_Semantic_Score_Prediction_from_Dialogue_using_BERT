      "utterance": [
        {
          "id": "MDRW2100044827.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044827.1.2",
          "form": "그거 시상식이라 마스크 쓰고 있는 거였는뎈ㅋㅋㅋㅋ",
          "original_form": "그거 시상식이라 마스크 쓰고 있는 거였는뎈ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044827.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044827.1.4",
          "form": "난 러블리즈 잘 몰랐는데",
          "original_form": "난 러블리즈 잘 몰랐는데",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044827.1.5",
          "form": "이미주 넘 웃긴 사람임",
          "original_form": "이미주 넘 웃긴 사람임",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044827.1.6",
          "form": "ㅁㅈㅁㅈㅋㅋㅋㅋㅋㅋ퀸덤으로 얼굴만 알았는뎈ㅋㅋㅋㅋ",
          "original_form": "ㅁㅈㅁㅈㅋㅋㅋㅋㅋㅋ퀸덤으로 얼굴만 알았는뎈ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044827.1.7",
          "form": "깨발랄이란 단어를 인간으로 만든게 이미주 일것...",
          "original_form": "깨발랄이란 단어를 인간으로 만든게 이미주 일것...",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044827.1.8",
          "form": "그냥 발랄 아니고 깨발랄...ㅋㅋㅋㅋㅋ",
          "original_form": "그냥 발랄 아니고 깨발랄...ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044827.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ이번에 보니까 놀면 머하니에도 나오더만ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ이번에 보니까 놀면 머하니에도 나오더만ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044827.1.10",
          "form": "맞아ㅋㅋㅋ 깜짝카메라도 당함",
          "original_form": "맞아ㅋㅋㅋ 깜짝카메라도 당함",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044827.1.11",
          "form": "출연진 모두를 공평하게 다 속이는 제작진들",
          "original_form": "출연진 모두를 공평하게 다 속이는 제작진들",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044827.1.12",
          "form": "장학퀴즈돜ㅋㅋㅋㅋ 넘 웃길 것 같음",
          "original_form": "장학퀴즈돜ㅋㅋㅋㅋ 넘 웃길 것 같음",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044827.1.13",
          "form": "저번주 마지막에ㅋㅋㅋ",
          "original_form": "저번주 마지막에ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044827.1.14",
          "form": "유재석이 엠씨보니깐 억울한 하하가 문제 내봤는데",
          "original_form": "유재석이 엠씨보니깐 억울한 하하가 문제 내봤는데",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044827.1.15",
          "form": "유재석이 계속 맞추니까 비속어날림",
          "original_form": "유재석이 계속 맞추니까 비속어날림",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044827.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ유재석은ㅋㅋㅋㅋ점점 더 모르는게 없는 것 같음ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ유재석은ㅋㅋㅋㅋ점점 더 모르는게 없는 것 같음ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044827.1.17",
          "form": "형은...변했어요!",
          "original_form": "형은...변했어요!",
          "speaker_id": "1",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044827.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044827.1.19",
          "form": "점점 진화하는 국민 엠씨ㅋㅋㅋㅋㅋ",
          "original_form": "점점 진화하는 국민 엠씨ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044827.1.20",
          "form": "그러니까 계속 톱에 있겠지ㅋㅋ",
          "original_form": "그러니까 계속 톱에 있겠지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044827.1.21",
          "form": "방송 계속 하면 좋겠다",
          "original_form": "방송 계속 하면 좋겠다",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044827.1.22",
          "form": "좋짘ㅋㅋㅋㅋㅋㅋ 방송 계속하려구 운동도 하고 직업에 진심인 사람이라 좋은 듯",
