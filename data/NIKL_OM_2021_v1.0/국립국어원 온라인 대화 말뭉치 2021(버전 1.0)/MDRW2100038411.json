      "utterance": [
        {
          "id": "MDRW2100038411.1.1",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "1",
          "time": "20210902 12:34"
        },
        {
          "id": "MDRW2100038411.1.2",
          "form": "네 먹었어요",
          "original_form": "네 먹었어요",
          "speaker_id": "2",
          "time": "20210902 12:34"
        },
        {
          "id": "MDRW2100038411.1.3",
          "form": "뭐 드셨나요?",
          "original_form": "뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210902 12:35"
        },
        {
          "id": "MDRW2100038411.1.4",
          "form": "전 빵 먹었어요 크림소보루요",
          "original_form": "전 빵 먹었어요 크림소보루요",
          "speaker_id": "1",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100038411.1.5",
          "form": "맛있었겠네요",
          "original_form": "맛있었겠네요",
          "speaker_id": "2",
          "time": "20210902 15:24"
        },
        {
          "id": "MDRW2100038411.1.6",
          "form": "커피도 드셨나요?",
          "original_form": "커피도 드셨나요?",
          "speaker_id": "2",
          "time": "20210902 15:24"
        },
        {
          "id": "MDRW2100038411.1.7",
          "form": "아뇨ㅋㅋ 커피는 몸에 잘 안 받더라고요",
          "original_form": "아뇨ㅋㅋ 커피는 몸에 잘 안 받더라고요",
          "speaker_id": "1",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100038411.1.8",
          "form": "저는 커피 자주 마셔서요ㅋㅋ",
          "original_form": "저는 커피 자주 마셔서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:10"
        },
        {
          "id": "MDRW2100038411.1.9",
          "form": "음료는 잘 안드시나요?",
          "original_form": "음료는 잘 안드시나요?",
          "speaker_id": "2",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100038411.1.10",
          "form": "카페가면 보통 차 마셔요 음료도 좋은데 자주 먹으면 건강에 안 좋으니까요ㅋㅋ",
          "original_form": "카페가면 보통 차 마셔요 음료도 좋은데 자주 먹으면 건강에 안 좋으니까요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100038411.1.11",
          "form": "물이 제일 좋다곤 하더라구요. 그래도 맛있는거 찾게 되요ㅎ",
          "original_form": "물이 제일 좋다곤 하더라구요. 그래도 맛있는거 찾게 되요ㅎ",
          "speaker_id": "2",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100038411.1.12",
          "form": "맞아요ㅋㅋ 자주 가는 카페 있으세요?",
          "original_form": "맞아요ㅋㅋ 자주 가는 카페 있으세요?",
          "speaker_id": "1",
          "time": "20210903 08:59"
        },
        {
          "id": "MDRW2100038411.1.13",
          "form": "이디야 가까워서 자주가요",
          "original_form": "이디야 가까워서 자주가요",
          "speaker_id": "2",
          "time": "20210903 09:02"
        },
        {
          "id": "MDRW2100038411.1.14",
          "form": "백다방도요",
          "original_form": "백다방도요",
          "speaker_id": "2",
          "time": "20210903 09:02"
        },
        {
          "id": "MDRW2100038411.1.15",
          "form": "저 이디야에서 와플 먹은적 있었는데 되게 맛있었어요",
          "original_form": "저 이디야에서 와플 먹은적 있었는데 되게 맛있었어요",
          "speaker_id": "1",
          "time": "20210903 09:03"
        },
        {
          "id": "MDRW2100038411.1.16",
          "form": "name2 님은 디저트 좋아하세요?",
          "original_form": "&name2& 님은 디저트 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 09:03"
        },
        {
          "id": "MDRW2100038411.1.17",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210903 09:03"
        },
        {
          "id": "MDRW2100038411.1.18",
          "form": "크로플 맛있더라구요",
          "original_form": "크로플 맛있더라구요",
          "speaker_id": "2",
          "time": "20210903 09:04"
        },
        {
          "id": "MDRW2100038411.1.19",
          "form": "맞아요 크로플 셀렉토에서 파는 거 맛있어요",
          "original_form": "맞아요 크로플 셀렉토에서 파는 거 맛있어요",
          "speaker_id": "1",
          "time": "20210903 09:05"
        },
        {
          "id": "MDRW2100038411.1.20",
          "form": "셀렉토는 자주 안가봐서요. 한번 먹어봐야겠네요",
          "original_form": "셀렉토는 자주 안가봐서요. 한번 먹어봐야겠네요",
          "speaker_id": "2",
          "time": "20210903 09:08"
        }
      ]
