      "utterance": [
        {
          "id": "MDRW2100024048.1.1",
          "form": "여행가기전에 미용하실거 있나요?",
          "original_form": "여행가기전에 미용하실거 있나요?",
          "speaker_id": "1",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100024048.1.2",
          "form": "여자분들은 페디많이 하던데요",
          "original_form": "여자분들은 페디많이 하던데요",
          "speaker_id": "1",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100024048.1.3",
          "form": "셀프염색 할 때되서 조만간 할 예정인데",
          "original_form": "셀프염색 할 때되서 조만간 할 예정인데",
          "speaker_id": "2",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100024048.1.4",
          "form": "여행다녀와서 할거같아요 ㅋㅋㅋ",
          "original_form": "여행다녀와서 할거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100024048.1.5",
          "form": "name1님은 따로 건강관리 하시는거 있으세요?",
          "original_form": "&name1&님은 따로 건강관리 하시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100024048.1.6",
          "form": "집에서 홈트 하고있서요",
          "original_form": "집에서 홈트 하고있서요",
          "speaker_id": "1",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100024048.1.7",
          "form": "꾸준히해야되는데",
          "original_form": "꾸준히해야되는데",
          "speaker_id": "1",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100024048.1.8",
          "form": "술한잔 하면 빼먹고해서 아랫배가 뽈록하네여 ㅋㅋㅋ",
          "original_form": "술한잔 하면 빼먹고해서 아랫배가 뽈록하네여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100024048.1.9",
          "form": "염색을 셀프로 하시나요 대단대단",
          "original_form": "염색을 셀프로 하시나요 대단대단",
          "speaker_id": "1",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100024048.1.10",
          "form": "ㅋㅋㅋ술한잔 자주하게되지않나요",
          "original_form": "ㅋㅋㅋ술한잔 자주하게되지않나요",
          "speaker_id": "2",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100024048.1.11",
          "form": "전 뒤늦게 알콜맛을 알앗네요",
          "original_form": "전 뒤늦게 알콜맛을 알앗네요",
          "speaker_id": "2",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100024048.1.12",
          "form": "건강을 해치고있습니다 ㅋㅋ",
          "original_form": "건강을 해치고있습니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100024048.1.13",
          "form": "그러게말입니다",
          "original_form": "그러게말입니다",
          "speaker_id": "2",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100024048.1.14",
          "form": "언제 알콜홀릭 되셧나요",
          "original_form": "언제 알콜홀릭 되셧나요",
          "speaker_id": "1",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100024048.1.15",
          "form": "매우최근에요 .. 맛있어요",
          "original_form": "매우최근에요 .. 맛있어요",
          "speaker_id": "2",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100024048.1.16",
          "form": "셀프로 해도 잘 나와서",
          "original_form": "셀프로 해도 잘 나와서",
          "speaker_id": "2",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100024048.1.17",
          "form": "주종은 무엇일까요?",
          "original_form": "주종은 무엇일까요?",
          "speaker_id": "1",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100024048.1.18",
          "form": "계속 셀프로하게되요 염색 ㅋㅋ",
          "original_form": "계속 셀프로하게되요 염색 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100024048.1.19",
          "form": "긴머리인가요?",
          "original_form": "긴머리인가요?",
          "speaker_id": "1",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100024048.1.20",
          "form": "스포츠형인가요?",
          "original_form": "스포츠형인가요?",
          "speaker_id": "1",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100024048.1.21",
          "form": "전 단게 좋아요 화이트와인 칵테일 맥주요 ㅎㅎ 아 요즘 하이볼맛있어요!",
          "original_form": "전 단게 좋아요 화이트와인 칵테일 맥주요 ㅎㅎ 아 요즘 하이볼맛있어요!",
          "speaker_id": "2",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100024048.1.22",
          "form": "긴머리에요~",
          "original_form": "긴머리에요~",
          "speaker_id": "2",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100024048.1.23",
          "form": "오 고급지시네요",
          "original_form": "오 고급지시네요",
          "speaker_id": "1",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100024048.1.24",
          "form": "고급은 아닙니다.. ㅋㅋㅋㅋㅋ",
          "original_form": "고급은 아닙니다.. ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100024048.1.25",
          "form": "긴머리 잘어울리는 분은 이쁘다고합니다",
          "original_form": "긴머리 잘어울리는 분은 이쁘다고합니다",
          "speaker_id": "1",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100024048.1.26",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100024048.1.27",
          "form": "감사합니다 ㅋㅋㅋㅋ",
          "original_form": "감사합니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100024048.1.28",
          "form": "찰랑찰랑 한번 해주세요",
          "original_form": "찰랑찰랑 한번 해주세요",
          "speaker_id": "1",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100024048.1.29",
          "form": "얼굴볼수없으니 넙죽받아야지",
          "original_form": "얼굴볼수없으니 넙죽받아야지",
          "speaker_id": "2",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100024048.1.30",
          "form": "앜ㅋ네 ㅋㅋㅋ",
          "original_form": "앜ㅋ네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:42"
        },
        {
          "id": "MDRW2100024048.1.31",
          "form": "염색 색깔은 어떤색을 선호하시나요?",
          "original_form": "염색 색깔은 어떤색을 선호하시나요?",
          "speaker_id": "1",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100024048.1.32",
          "form": "이번에는",
          "original_form": "이번에는",
          "speaker_id": "2",
          "time": "20210830 21:44"
        },
        {
          "id": "MDRW2100024048.1.33",
          "form": "지금제가 밝은갈색인데 이번에 할땐 코발트블루라고..",
          "original_form": "지금제가 밝은갈색인데 이번에 할땐 코발트블루라고..",
          "speaker_id": "2",
          "time": "20210830 21:44"
        },
        {
          "id": "MDRW2100024048.1.34",
          "form": "푸른빛나는 어두운 색으로 하려구요~~",
          "original_form": "푸른빛나는 어두운 색으로 하려구요~~",
          "speaker_id": "2",
          "time": "20210830 21:44"
        },
        {
          "id": "MDRW2100024048.1.35",
          "form": "와우 고급지네요",
          "original_form": "와우 고급지네요",
          "speaker_id": "1",
          "time": "20210830 21:44"
        },
        {
          "id": "MDRW2100024048.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:44"
        },
        {
          "id": "MDRW2100024048.1.37",
          "form": "빛나실겁니다",
          "original_form": "빛나실겁니다",
          "speaker_id": "1",
          "time": "20210830 21:44"
        },
        {
          "id": "MDRW2100024048.1.38",
          "form": "와 감사합니다~ 잘어울릴지 생각안하고 그냥 하고싶은걸로 해요 ㅎㅎ",
          "original_form": "와 감사합니다~ 잘어울릴지 생각안하고 그냥 하고싶은걸로 해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100024048.1.39",
          "form": "직장에선 머리 색으로 간섭하지 않나보네요",
          "original_form": "직장에선 머리 색으로 간섭하지 않나보네요",
          "speaker_id": "1",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100024048.1.40",
          "form": "인물 좋아서 괜찮을것 같네요",
          "original_form": "인물 좋아서 괜찮을것 같네요",
          "speaker_id": "1",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100024048.1.41",
          "form": "아 네네 직장 자유로운편이에요  복장도 완전 캐주얼하게 다닙니다 ㅎㅎ",
          "original_form": "아 네네 직장 자유로운편이에요  복장도 완전 캐주얼하게 다닙니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100024048.1.42",
          "form": "ㅋㅋㅋ칭찬에 후하시네요",
          "original_form": "ㅋㅋㅋ칭찬에 후하시네요",
          "speaker_id": "2",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100024048.1.43",
          "form": "직장룩이 어떻게 되시나요?",
          "original_form": "직장룩이 어떻게 되시나요?",
          "speaker_id": "2",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024048.1.44",
          "form": "대게 그런분들이 예쁘시더라구요 살아보니 그러더라구요",
          "original_form": "대게 그런분들이 예쁘시더라구요 살아보니 그러더라구요",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024048.1.45",
          "form": "저도 딱히 간섭없어요",
          "original_form": "저도 딱히 간섭없어요",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024048.1.46",
          "form": "반바지빼고 쌉가능 ㅋㅋ",
          "original_form": "반바지빼고 쌉가능 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024048.1.47",
          "form": "운동화 단화 구두 신고싶은거 신고 갑니다",
          "original_form": "운동화 단화 구두 신고싶은거 신고 갑니다",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024048.1.48",
          "form": "간혹 쓰리빠신고 갈때도 있어요",
          "original_form": "간혹 쓰리빠신고 갈때도 있어요",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024048.1.49",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024048.1.50",
          "form": "아기아빠도 쌉가능이라는 말을 쓰시는군요.......",
          "original_form": "아기아빠도 쌉가능이라는 말을 쓰시는군요.......",
          "speaker_id": "2",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024048.1.51",
          "form": "쓰레빠요...?",
          "original_form": "쓰레빠요...?",
          "speaker_id": "2",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024048.1.52",
