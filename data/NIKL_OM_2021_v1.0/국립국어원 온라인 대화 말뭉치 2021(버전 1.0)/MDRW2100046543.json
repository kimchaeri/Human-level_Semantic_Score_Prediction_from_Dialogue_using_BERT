        },
        {
          "id": "MDRW2100046543.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211028 15:55"
        },
        {
          "id": "MDRW2100046543.1.3",
          "form": "맞춤법 잘 아시나요?",
          "original_form": "맞춤법 잘 아시나요?",
          "speaker_id": "2",
          "time": "20211028 15:55"
        },
        {
          "id": "MDRW2100046543.1.4",
          "form": "아니요 요즘 많이 헷갈려요",
          "original_form": "아니요 요즘 많이 헷갈려요",
          "speaker_id": "1",
          "time": "20211028 15:55"
        },
        {
          "id": "MDRW2100046543.1.5",
          "form": "맞춤범에 맞춰서 안쓰다보니",
          "original_form": "맞춤범에 맞춰서 안쓰다보니",
          "speaker_id": "1",
          "time": "20211028 15:56"
        },
        {
          "id": "MDRW2100046543.1.6",
          "form": "카톡 생긴이후로",
          "original_form": "카톡 생긴이후로",
          "speaker_id": "2",
          "time": "20211028 15:56"
        },
        {
          "id": "MDRW2100046543.1.7",
          "form": "맞춤법 신경 안쓰고",
          "original_form": "맞춤법 신경 안쓰고",
          "speaker_id": "2",
          "time": "20211028 15:56"
        },
        {
          "id": "MDRW2100046543.1.8",
          "form": "막 쓰다보니까 더 헷갈려지는 것 같아요 ㅋㅋㅋ",
          "original_form": "막 쓰다보니까 더 헷갈려지는 것 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211028 15:56"
        },
        {
          "id": "MDRW2100046543.1.9",
          "form": "맞아요 국어 공부 다시 해야 할듯합니다",
          "original_form": "맞아요 국어 공부 다시 해야 할듯합니다",
          "speaker_id": "1",
          "time": "20211028 15:57"
        },
        {
          "id": "MDRW2100046543.1.10",
          "form": "ㅜ",
          "original_form": "ㅜ",
          "speaker_id": "1",
          "time": "20211028 15:57"
        },
        {
          "id": "MDRW2100046543.1.11",
          "form": "네이버 찾아보게 돼요",
          "original_form": "네이버 찾아보게 돼요",
          "speaker_id": "1",
          "time": "20211028 15:57"
        },
        {
          "id": "MDRW2100046543.1.12",
          "form": "하지만 다음에 쓰려면 또 헷갈리고 심각합니다",
          "original_form": "하지만 다음에 쓰려면 또 헷갈리고 심각합니다",
          "speaker_id": "1",
          "time": "20211028 15:57"
        },
        {
          "id": "MDRW2100046543.1.13",
          "form": "맞아요 ㅠㅠ 저도 헷갈리는 건 항상 헷갈리더라구요 ㅋㅋ",
          "original_form": "맞아요 ㅠㅠ 저도 헷갈리는 건 항상 헷갈리더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211028 15:58"
        },
        {
          "id": "MDRW2100046543.1.14",
          "form": "대책이 필요 한 듯해요",
          "original_form": "대책이 필요 한 듯해요",
          "speaker_id": "1",
          "time": "20211028 15:58"
        },
        {
          "id": "MDRW2100046543.1.15",
          "form": "매번 검색해서 쓰고 그래요 ㅋㅋㅋ",
          "original_form": "매번 검색해서 쓰고 그래요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211028 15:58"
        },
        {
          "id": "MDRW2100046543.1.16",
          "form": "유투브같은 플랫폼에서 신조어도 엄청 생겨나잖아요",
          "original_form": "유투브같은 플랫폼에서 신조어도 엄청 생겨나잖아요",
          "speaker_id": "2",
          "time": "20211028 15:58"
        },
        {
          "id": "MDRW2100046543.1.17",
          "form": "저도요 하지만 신조어도 못 따라가고 맞춤법도 모르고",
          "original_form": "저도요 하지만 신조어도 못 따라가고 맞춤법도 모르고",
          "speaker_id": "1",
          "time": "20211028 15:59"
        },
        {
          "id": "MDRW2100046543.1.18",
          "form": "이게 무슨 일인지 ㅋㅋㅋ",
          "original_form": "이게 무슨 일인지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211028 15:59"
        },
        {
          "id": "MDRW2100046543.1.19",
          "form": "ㅋㅋㅋ 저랑 비슷하시네요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 저랑 비슷하시네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211028 15:59"
        },
        {
          "id": "MDRW2100046543.1.20",
          "form": "애매합니다",
          "original_form": "애매합니다",
