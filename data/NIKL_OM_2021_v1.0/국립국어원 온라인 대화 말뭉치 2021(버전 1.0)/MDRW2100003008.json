      "utterance": [
        {
          "id": "MDRW2100003008.1.1",
          "form": "요즘 학교 동아리 운영중인거 같아?",
          "original_form": "요즘 학교 동아리 운영중인거 같아?",
          "speaker_id": "1",
          "time": "20210820 17:33"
        },
        {
          "id": "MDRW2100003008.1.2",
          "form": "어 진짜?",
          "original_form": "어 진짜?",
          "speaker_id": "2",
          "time": "20210820 17:38"
        },
        {
          "id": "MDRW2100003008.1.3",
          "form": "근데 여러명에서 못하지 않아?",
          "original_form": "근데 여러명에서 못하지 않아?",
          "speaker_id": "2",
          "time": "20210820 17:39"
        },
        {
          "id": "MDRW2100003008.1.4",
          "form": "우리는 운영중인데",
          "original_form": "우리는 운영중인데",
          "speaker_id": "1",
          "time": "20210820 17:39"
        },
        {
          "id": "MDRW2100003008.1.5",
          "form": "비대면으로 만나",
          "original_form": "비대면으로 만나",
          "speaker_id": "1",
          "time": "20210820 17:39"
        },
        {
          "id": "MDRW2100003008.1.6",
          "form": "zoom 같은걸로",
          "original_form": "zoom 같은걸로",
          "speaker_id": "1",
          "time": "20210820 17:40"
        },
        {
          "id": "MDRW2100003008.1.7",
          "form": "아하 비대면으로 하는구나",
          "original_form": "아하 비대면으로 하는구나",
          "speaker_id": "2",
          "time": "20210820 17:40"
        },
        {
          "id": "MDRW2100003008.1.8",
          "form": "난 동아리가 운동부라서",
          "original_form": "난 동아리가 운동부라서",
          "speaker_id": "2",
          "time": "20210820 17:40"
        },
        {
          "id": "MDRW2100003008.1.9",
          "form": "생각도 못했어",
          "original_form": "생각도 못했어",
          "speaker_id": "2",
          "time": "20210820 17:40"
        },
        {
          "id": "MDRW2100003008.1.10",
          "form": "연락은 해?",
          "original_form": "연락은 해?",
          "speaker_id": "1",
          "time": "20210820 18:38"
        },
        {
          "id": "MDRW2100003008.1.11",
          "form": "안한지 좀 됐지",
          "original_form": "안한지 좀 됐지",
          "speaker_id": "2",
          "time": "20210820 18:41"
        },
        {
          "id": "MDRW2100003008.1.12",
          "form": "하긴 운동 동아리면 그럴만 해",
          "original_form": "하긴 운동 동아리면 그럴만 해",
          "speaker_id": "1",
          "time": "20210820 19:02"
        },
        {
          "id": "MDRW2100003008.1.13",
          "form": "코로나 때문에 새학기 시작해도",
          "original_form": "코로나 때문에 새학기 시작해도",
          "speaker_id": "2",
          "time": "20210820 19:18"
        },
        {
          "id": "MDRW2100003008.1.14",
          "form": "잘 못볼것 같아서 아쉽네",
          "original_form": "잘 못볼것 같아서 아쉽네",
          "speaker_id": "2",
          "time": "20210820 19:18"
        },
        {
          "id": "MDRW2100003008.1.15",
          "form": "2학기에는 코로나 나아져서 만났으면 좋겠다",
          "original_form": "2학기에는 코로나 나아져서 만났으면 좋겠다",
          "speaker_id": "1",
          "time": "20210820 19:20"
        },
        {
          "id": "MDRW2100003008.1.16",
          "form": "그러게 제발 그랬으면ㅠㅠ",
          "original_form": "그러게 제발 그랬으면ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 19:23"
        },
        {
          "id": "MDRW2100003008.1.17",
          "form": "우리학교 운동 동아리는 4인씩 모여서 등산하나보더라",
          "original_form": "우리학교 운동 동아리는 4인씩 모여서 등산하나보더라",
          "speaker_id": "1",
          "time": "20210820 19:34"
        },
        {
          "id": "MDRW2100003008.1.18",
          "form": "와 등산 대단하네",
          "original_form": "와 등산 대단하네",
          "speaker_id": "2",
          "time": "20210820 21:27"
        },
        {
          "id": "MDRW2100003008.1.19",
          "form": "나도 등산동아리라는 말 듣고 대단하다고 그랬어",
          "original_form": "나도 등산동아리라는 말 듣고 대단하다고 그랬어",
          "speaker_id": "1",
          "time": "20210820 23:07"
        }
      ]
