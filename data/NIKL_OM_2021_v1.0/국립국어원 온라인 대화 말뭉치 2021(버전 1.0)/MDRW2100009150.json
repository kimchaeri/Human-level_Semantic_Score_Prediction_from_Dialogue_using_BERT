      "utterance": [
        {
          "id": "MDRW2100009150.1.1",
          "form": "지금은 코로나로 못하시죠?",
          "original_form": "지금은 코로나로 못하시죠?",
          "speaker_id": "2",
          "time": "20210822 21:46"
        },
        {
          "id": "MDRW2100009150.1.2",
          "form": "많은것들이 중단됐어요",
          "original_form": "많은것들이 중단됐어요",
          "speaker_id": "2",
          "time": "20210822 21:47"
        },
        {
          "id": "MDRW2100009150.1.3",
          "form": "name2님이 봉사를 다양하게하셔서 봉사로만 대화가 차네요ㅋㅋㅋㅋ",
          "original_form": "&name2&님이 봉사를 다양하게하셔서 봉사로만 대화가 차네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:47"
        },
        {
          "id": "MDRW2100009150.1.4",
          "form": "아니예요 ㅜㅜ",
          "original_form": "아니예요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 21:47"
        },
        {
          "id": "MDRW2100009150.1.5",
          "form": "네 인도는 다니면 위험하죠 코로나걸려요",
          "original_form": "네 인도는 다니면 위험하죠 코로나걸려요",
          "speaker_id": "1",
          "time": "20210822 21:47"
        },
        {
          "id": "MDRW2100009150.1.6",
          "form": "그렇죠 진짜 안타까워요",
          "original_form": "그렇죠 진짜 안타까워요",
          "speaker_id": "2",
          "time": "20210822 21:47"
        },
        {
          "id": "MDRW2100009150.1.7",
          "form": "불가촉천민분들은 그냥 마스크안끼고 사셔서",
          "original_form": "불가촉천민분들은 그냥 마스크안끼고 사셔서",
          "speaker_id": "1",
          "time": "20210822 21:47"
        },
        {
          "id": "MDRW2100009150.1.8",
          "form": "뉴스 볼때마다 치료도 못받고 ㅜㅜ",
          "original_form": "뉴스 볼때마다 치료도 못받고 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 21:47"
        },
        {
          "id": "MDRW2100009150.1.9",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210822 21:48"
        },
        {
          "id": "MDRW2100009150.1.10",
          "form": "그냥산대요",
          "original_form": "그냥산대요",
          "speaker_id": "1",
          "time": "20210822 21:48"
        },
        {
          "id": "MDRW2100009150.1.11",
          "form": "거긴 구할수도 앖고 그냥 그런 분위기던데",
          "original_form": "거긴 구할수도 앖고 그냥 그런 분위기던데",
          "speaker_id": "2",
          "time": "20210822 21:48"
        },
        {
          "id": "MDRW2100009150.1.12",
          "form": "안타까워요 제가 아는 인도가족분들 모두 코로나 걸렸는데 아버님쪽이 돌아가셨더라구요",
          "original_form": "안타까워요 제가 아는 인도가족분들 모두 코로나 걸렸는데 아버님쪽이 돌아가셨더라구요",
          "speaker_id": "1",
          "time": "20210822 21:48"
        },
        {
          "id": "MDRW2100009150.1.13",
          "form": "안타깝더래요ㅜ",
          "original_form": "안타깝더래요ㅜ",
          "speaker_id": "1",
          "time": "20210822 21:48"
        },
        {
          "id": "MDRW2100009150.1.14",
          "form": "세상에 산소도 못구해서 난리라던데",
          "original_form": "세상에 산소도 못구해서 난리라던데",
          "speaker_id": "2",
          "time": "20210822 21:49"
        },
        {
          "id": "MDRW2100009150.1.15",
          "form": "그렇죠 아는분이 그리되니",
          "original_form": "그렇죠 아는분이 그리되니",
          "speaker_id": "2",
          "time": "20210822 21:49"
        },
        {
          "id": "MDRW2100009150.1.16",
          "form": "화이트 타이거 영화를 봤는데",
          "original_form": "화이트 타이거 영화를 봤는데",
          "speaker_id": "2",
          "time": "20210822 21:49"
        },
        {
          "id": "MDRW2100009150.1.17",
          "form": "계급에 따라 진짜 삶이 나락으로 가더라구요",
          "original_form": "계급에 따라 진짜 삶이 나락으로 가더라구요",
          "speaker_id": "2",
          "time": "20210822 21:49"
        },
        {
          "id": "MDRW2100009150.1.18",
          "form": "자신이 어떤 취급을 당하는지도 모르고 무시당하고",
          "original_form": "자신이 어떤 취급을 당하는지도 모르고 무시당하고",
          "speaker_id": "2",
          "time": "20210822 21:50"
        },
        {
          "id": "MDRW2100009150.1.19",
          "form": "당연히 살던데",
          "original_form": "당연히 살던데",
          "speaker_id": "2",
          "time": "20210822 21:50"
        },
        {
          "id": "MDRW2100009150.1.20",
          "form": "밑에 분들은 봉사하는분 도움 없다면",
          "original_form": "밑에 분들은 봉사하는분 도움 없다면",
          "speaker_id": "2",
          "time": "20210822 21:50"
        },
        {
          "id": "MDRW2100009150.1.21",
          "form": "이겨낼 방법 없겠어요",
          "original_form": "이겨낼 방법 없겠어요",
          "speaker_id": "2",
          "time": "20210822 21:51"
        },
        {
          "id": "MDRW2100009150.1.22",
          "form": "거기 천민마을은 다들 비슷하게 살아서 비교할게없어서 그냥 행복하게사는데도 많아요",
          "original_form": "거기 천민마을은 다들 비슷하게 살아서 비교할게없어서 그냥 행복하게사는데도 많아요",
          "speaker_id": "1",
          "time": "20210822 21:51"
        },
        {
          "id": "MDRW2100009150.1.23",
          "form": "네 전혀 모르는거 같아요",
          "original_form": "네 전혀 모르는거 같아요",
          "speaker_id": "2",
          "time": "20210822 21:51"
        },
        {
          "id": "MDRW2100009150.1.24",
          "form": "근데 빈부격차가 심해요 정말",
          "original_form": "근데 빈부격차가 심해요 정말",
          "speaker_id": "1",
          "time": "20210822 21:51"
        },
        {
          "id": "MDRW2100009150.1.25",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210822 21:51"
        },
        {
          "id": "MDRW2100009150.1.26",
          "form": "봉사를 떠나서 어찌 지내셨나요?",
          "original_form": "봉사를 떠나서 어찌 지내셨나요?",
          "speaker_id": "2",
          "time": "20210822 21:52"
        },
        {
          "id": "MDRW2100009150.1.27",
          "form": "정부에서 법을많이 고쳤는데도 그 제도를 받아먹으려면",
          "original_form": "정부에서 법을많이 고쳤는데도 그 제도를 받아먹으려면",
          "speaker_id": "1",
          "time": "20210822 21:52"
        },
        {
          "id": "MDRW2100009150.1.28",
          "form": "글을알고 찾아먹어야하거든요",
          "original_form": "글을알고 찾아먹어야하거든요",
          "speaker_id": "1",
          "time": "20210822 21:52"
        },
        {
          "id": "MDRW2100009150.1.29",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210822 21:52"
        },
        {
          "id": "MDRW2100009150.1.30",
          "form": "그런거는 똑똑한봉사자가 찾아서 지원해야죵",
          "original_form": "그런거는 똑똑한봉사자가 찾아서 지원해야죵",
          "speaker_id": "1",
          "time": "20210822 21:52"
        },
        {
          "id": "MDRW2100009150.1.31",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210822 21:52"
        },
        {
          "id": "MDRW2100009150.1.32",
          "form": "참 좋은일 하셨어요",
          "original_form": "참 좋은일 하셨어요",
          "speaker_id": "2",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009150.1.33",
          "form": "봉사 끝나고는 한국와서 코로나를피해 대충살고있네요",
          "original_form": "봉사 끝나고는 한국와서 코로나를피해 대충살고있네요",
          "speaker_id": "1",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009150.1.34",
          "form": "칭찬합니다",
          "original_form": "칭찬합니다",
          "speaker_id": "2",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009150.1.35",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009150.1.36",
          "form": "하지만 절대 못할일이죠",
          "original_form": "하지만 절대 못할일이죠",
          "speaker_id": "2",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009150.1.37",
          "form": "name2님이야말로 좋은일많이 하시네요",
          "original_form": "&name2&님이야말로 좋은일많이 하시네요",
          "speaker_id": "1",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009150.1.38",
          "form": "전 하루만에 도망업니다",
          "original_form": "전 하루만에 도망업니다",
          "speaker_id": "2",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009150.1.39",
          "form": "옵니다",
          "original_form": "옵니다",
          "speaker_id": "2",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009150.1.40",
          "form": "아니예요",
          "original_form": "아니예요",
          "speaker_id": "2",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009150.1.41",
          "form": "절대는아니고 봉사자 종종 오셔서 자기 하는만큼 돠주고 가셨어요",
