      "utterance": [
        {
          "id": "MDRW2100031720.1.1",
          "form": "필라테스도 근육통이 있나요??",
          "original_form": "필라테스도 근육통이 있나요??",
          "speaker_id": "2",
          "time": "20210904 23:49"
        },
        {
          "id": "MDRW2100031720.1.2",
          "form": "없지 않아요! 운동인걸요",
          "original_form": "없지 않아요! 운동인걸요",
          "speaker_id": "1",
          "time": "20210904 23:49"
        },
        {
          "id": "MDRW2100031720.1.3",
          "form": "하면할수록 점점 없어지긴해요!",
          "original_form": "하면할수록 점점 없어지긴해요!",
          "speaker_id": "1",
          "time": "20210904 23:49"
        },
        {
          "id": "MDRW2100031720.1.4",
          "form": "확실히 그렇군요,,",
          "original_form": "확실히 그렇군요,,",
          "speaker_id": "2",
          "time": "20210904 23:50"
        },
        {
          "id": "MDRW2100031720.1.5",
          "form": "약간 레깅스 입는게 부담스러워요 ..ㅎ",
          "original_form": "약간 레깅스 입는게 부담스러워요 ..ㅎ",
          "speaker_id": "2",
          "time": "20210904 23:51"
        },
        {
          "id": "MDRW2100031720.1.6",
          "form": "부끄",
          "original_form": "부끄",
          "speaker_id": "2",
          "time": "20210904 23:51"
        },
        {
          "id": "MDRW2100031720.1.7",
          "form": "레깅스에 바지 반바지를 하나 더 입으시면 돼요!",
          "original_form": "레깅스에 바지 반바지를 하나 더 입으시면 돼요!",
          "speaker_id": "1",
          "time": "20210904 23:51"
        },
        {
          "id": "MDRW2100031720.1.8",
          "form": "아니면 상의를 좀 긴걸 입고 하기도 해요",
          "original_form": "아니면 상의를 좀 긴걸 입고 하기도 해요",
          "speaker_id": "1",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100031720.1.9",
          "form": "오오! 위에 반바지 다른 사람들도 많이 입으시나요??",
          "original_form": "오오! 위에 반바지 다른 사람들도 많이 입으시나요??",
          "speaker_id": "2",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100031720.1.10",
          "form": "네 많이들 입으세요!",
          "original_form": "네 많이들 입으세요!",
          "speaker_id": "1",
          "time": "20210904 23:55"
        },
        {
          "id": "MDRW2100031720.1.11",
          "form": "이제 붙는 옷을 주로 입는 이유갸\r\n아까 말했듯이 본인 자세를 잘 보기 위해서에요",
          "original_form": "이제 붙는 옷을 주로 입는 이유갸\r\n아까 말했듯이 본인 자세를 잘 보기 위해서에요",
          "speaker_id": "1",
          "time": "20210904 23:55"
        },
        {
          "id": "MDRW2100031720.1.12",
          "form": "그치만 민망한건 싫으시다면 반바지 당연히 입어도 되는걸요 ㅋㅋ",
          "original_form": "그치만 민망한건 싫으시다면 반바지 당연히 입어도 되는걸요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:56"
        },
        {
          "id": "MDRW2100031720.1.13",
          "form": "우와,,ㅎㅎ 그렇군요",
          "original_form": "우와,,ㅎㅎ 그렇군요",
          "speaker_id": "2",
          "time": "20210904 23:56"
        },
        {
          "id": "MDRW2100031720.1.14",
          "form": "부담을 덜었네요!",
          "original_form": "부담을 덜었네요!",
          "speaker_id": "2",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100031720.1.15",
          "form": "감사해요 ㅎㅎ",
          "original_form": "감사해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100031720.1.16",
          "form": "관리는 평생 숙제인거 같아요..ㅎ",
          "original_form": "관리는 평생 숙제인거 같아요..ㅎ",
          "speaker_id": "2",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100031720.1.17",
          "form": "정말 어려운 숙제에요",
          "original_form": "정말 어려운 숙제에요",
          "speaker_id": "1",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100031720.1.18",
          "form": "식습관도 평생 할 식습관을 들여야 한다고 하는데",
          "original_form": "식습관도 평생 할 식습관을 들여야 한다고 하는데",
          "speaker_id": "1",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100031720.1.19",
          "form": "설탕.. 그거 어떻게 안 먹는거죠?",
          "original_form": "설탕.. 그거 어떻게 안 먹는거죠?",
          "speaker_id": "1",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100031720.1.20",
          "form": "마자요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "마자요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:01"
        },
        {
          "id": "MDRW2100031720.1.21",
          "form": "저는 밀가루도 못 끊어요",
          "original_form": "저는 밀가루도 못 끊어요",
          "speaker_id": "2",
          "time": "20210905 00:01"
        },
        {
          "id": "MDRW2100031720.1.22",
          "form": "ㅋㅋㅋㅋ 그걸 끊으면 인생은 무슨낙에 사나요 ㅠ",
          "original_form": "ㅋㅋㅋㅋ 그걸 끊으면 인생은 무슨낙에 사나요 ㅠ",
          "speaker_id": "1",
          "time": "20210905 00:02"
        },
        {
          "id": "MDRW2100031720.1.23",
          "form": "차라리 먹고 좀 일찍 주글래여,,ㅎ",
          "original_form": "차라리 먹고 좀 일찍 주글래여,,ㅎ",
          "speaker_id": "2",
          "time": "20210905 00:03"
        },
        {
          "id": "MDRW2100031720.1.24",
          "form": "설탕을 끊는다..?!?!",
          "original_form": "설탕을 끊는다..?!?!",
          "speaker_id": "2",
          "time": "20210905 00:03"
        },
        {
          "id": "MDRW2100031720.1.25",
          "form": "말도안되요",
          "original_form": "말도안되요",
          "speaker_id": "2",
          "time": "20210905 00:03"
        },
        {
          "id": "MDRW2100031720.1.26",
          "form": "밀가루,,, 쌀보다 많이먹을 때도 많아요 ㅋ큐ㅠㅠ",
          "original_form": "밀가루,,, 쌀보다 많이먹을 때도 많아요 ㅋ큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 00:04"
        },
        {
          "id": "MDRW2100031720.1.27",
          "form": "쌀보다 밀가루를 더 자주 먹는 것 같기도 해요 ㅋㅋ",
          "original_form": "쌀보다 밀가루를 더 자주 먹는 것 같기도 해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:04"
        },
        {
          "id": "MDRW2100031720.1.28",
          "form": "앜ㅋㅋ 저만 그런게 아니군욬ㅋㅋㅋ",
          "original_form": "앜ㅋㅋ 저만 그런게 아니군욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:04"
        },
        {
          "id": "MDRW2100031720.1.29",
          "form": "야식은 왜또 그리 맛있는지",
          "original_form": "야식은 왜또 그리 맛있는지",
          "speaker_id": "2",
