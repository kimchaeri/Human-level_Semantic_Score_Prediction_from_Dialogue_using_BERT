      "utterance": [
        {
          "id": "MDRW2100009232.1.1",
          "form": "직장인이세요?",
          "original_form": "직장인이세요?",
          "speaker_id": "1",
          "time": "20210822 22:15"
        },
        {
          "id": "MDRW2100009232.1.2",
          "form": "비노동인구에요.. ㅋㅋㅋ",
          "original_form": "비노동인구에요.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009232.1.3",
          "form": "백수요..",
          "original_form": "백수요..",
          "speaker_id": "2",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009232.1.4",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "2",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009232.1.5",
          "form": "ㅋㅋㅋㅋㅋ 부럽네요",
          "original_form": "ㅋㅋㅋㅋㅋ 부럽네요",
          "speaker_id": "1",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009232.1.6",
          "form": "직장인이요",
          "original_form": "직장인이요",
          "speaker_id": "1",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009232.1.7",
          "form": "낼 출근..ㅋㅋㅋㅋㅋ",
          "original_form": "낼 출근..ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:17"
        },
        {
          "id": "MDRW2100009232.1.8",
          "form": "부럽다..",
          "original_form": "부럽다..",
          "speaker_id": "2",
          "time": "20210822 22:17"
        },
        {
          "id": "MDRW2100009232.1.9",
          "form": "허 ..잠시 쉬고계신건가요?",
          "original_form": "허 ..잠시 쉬고계신건가요?",
          "speaker_id": "1",
          "time": "20210822 22:17"
        },
        {
          "id": "MDRW2100009232.1.10",
          "form": "오래 쉬었지요..ㅋㅋㅋ \r\n이런저런 사정으로..",
          "original_form": "오래 쉬었지요..ㅋㅋㅋ \r\n이런저런 사정으로..",
          "speaker_id": "2",
          "time": "20210822 22:18"
        },
        {
          "id": "MDRW2100009232.1.11",
          "form": "어떤 직종이세요?",
          "original_form": "어떤 직종이세요?",
          "speaker_id": "2",
          "time": "20210822 22:18"
        },
        {
          "id": "MDRW2100009232.1.12",
          "form": "저는 치과다닙니다 치위생사예요 ㅎㅎ",
          "original_form": "저는 치과다닙니다 치위생사예요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:18"
        },
        {
          "id": "MDRW2100009232.1.13",
          "form": "원래는 무슨일 하셨어요?",
          "original_form": "원래는 무슨일 하셨어요?",
          "speaker_id": "1",
          "time": "20210822 22:18"
        },
        {
          "id": "MDRW2100009232.1.14",
          "form": "무서우신 분이시네요.. \r\n저는 반도체회사 다니다.. \r\n급 이벤트쪽 일도하고.. \r\n급 유통쪽 일도하고..",
          "original_form": "무서우신 분이시네요.. \r\n저는 반도체회사 다니다.. \r\n급 이벤트쪽 일도하고.. \r\n급 유통쪽 일도하고..",
          "speaker_id": "2",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100009232.1.15",
          "form": "치과면.. 출근하는거 매일매일 안무서운가요?",
          "original_form": "치과면.. 출근하는거 매일매일 안무서운가요?",
          "speaker_id": "2",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100009232.1.16",
          "form": "왜 매일 무서워용?",
          "original_form": "왜 매일 무서워용?",
          "speaker_id": "1",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009232.1.17",
          "form": "소리랑.. 냄세.. 그리고 카드값.. ㅋㅋㅋ",
          "original_form": "소리랑.. 냄세.. 그리고 카드값.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009232.1.18",
          "form": "나이먹어도 치과는 무서움..",
          "original_form": "나이먹어도 치과는 무서움..",
          "speaker_id": "2",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009232.1.19",
          "form": "엥 카드값은 무슨의미예용??",
          "original_form": "엥 카드값은 무슨의미예용??",
          "speaker_id": "1",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009232.1.20",
          "form": "치과 비싸 잖아요..",
          "original_form": "치과 비싸 잖아요..",
          "speaker_id": "2",
          "time": "20210822 22:21"
        },
        {
          "id": "MDRW2100009232.1.21",
          "form": "저 선천적으로 모가 안좋다고.. 충치가 엄청 잘생겨서.. 동네치과 먹여 살리고잇어요",
          "original_form": "저 선천적으로 모가 안좋다고.. 충치가 엄청 잘생겨서.. 동네치과 먹여 살리고잇어요",
          "speaker_id": "2",
          "time": "20210822 22:21"
        },
        {
          "id": "MDRW2100009232.1.22",
          "form": "아ㅋㅋㅋㅋ 그돈 결제를 제가합니다 ..",
          "original_form": "아ㅋㅋㅋㅋ 그돈 결제를 제가합니다 ..",
          "speaker_id": "1",
