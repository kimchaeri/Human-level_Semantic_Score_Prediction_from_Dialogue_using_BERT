      "utterance": [
        {
          "id": "MDRW2100029412.1.1",
          "form": "지금 날씨는 어떤가요?",
          "original_form": "지금 날씨는 어떤가요?",
          "speaker_id": "2",
          "time": "20210903 07:24"
        },
        {
          "id": "MDRW2100029412.1.2",
          "form": "여기는 해떴어요~거기는요?",
          "original_form": "여기는 해떴어요~거기는요?",
          "speaker_id": "1",
          "time": "20210903 07:24"
        },
        {
          "id": "MDRW2100029412.1.3",
          "form": "여기는 가는 비가 오네요",
          "original_form": "여기는 가는 비가 오네요",
          "speaker_id": "2",
          "time": "20210903 07:25"
        },
        {
          "id": "MDRW2100029412.1.4",
          "form": "아직까지 어둑해요",
          "original_form": "아직까지 어둑해요",
          "speaker_id": "2",
          "time": "20210903 07:25"
        },
        {
          "id": "MDRW2100029412.1.5",
          "form": "아 그렇군요 흐리겠어요",
          "original_form": "아 그렇군요 흐리겠어요",
          "speaker_id": "1",
          "time": "20210903 07:25"
        },
        {
          "id": "MDRW2100029412.1.6",
          "form": "계속 비가 왔나요?",
          "original_form": "계속 비가 왔나요?",
          "speaker_id": "1",
          "time": "20210903 07:26"
        },
        {
          "id": "MDRW2100029412.1.7",
          "form": "3일 정도 비가 내리고 그치기를 반복하네요",
          "original_form": "3일 정도 비가 내리고 그치기를 반복하네요",
          "speaker_id": "2",
          "time": "20210903 07:26"
        },
        {
          "id": "MDRW2100029412.1.8",
          "form": "여기도 그랬는데ㅜ지금은 밝아요~모처럼 해네요",
          "original_form": "여기도 그랬는데ㅜ지금은 밝아요~모처럼 해네요",
          "speaker_id": "1",
          "time": "20210903 07:27"
        },
        {
          "id": "MDRW2100029412.1.9",
          "form": "기쁜소식이네요",
          "original_form": "기쁜소식이네요",
          "speaker_id": "2",
          "time": "20210903 07:27"
        },
        {
          "id": "MDRW2100029412.1.10",
          "form": "맑은 날씨는 웃음짓게 만들죠",
          "original_form": "맑은 날씨는 웃음짓게 만들죠",
          "speaker_id": "2",
          "time": "20210903 07:28"
        },
        {
          "id": "MDRW2100029412.1.11",
          "form": "맑은 날 좋아하세요?",
          "original_form": "맑은 날 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 07:28"
        },
        {
          "id": "MDRW2100029412.1.12",
          "form": "너무 뜨겁지만 않다면요ㅎㅎ",
          "original_form": "너무 뜨겁지만 않다면요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 07:28"
        },
        {
          "id": "MDRW2100029412.1.13",
          "form": "좋아하시는군요~",
          "original_form": "좋아하시는군요~",
          "speaker_id": "1",
          "time": "20210903 07:28"
        },
        {
          "id": "MDRW2100029412.1.14",
          "form": "너무 더운것도ㅜ",
          "original_form": "너무 더운것도ㅜ",
          "speaker_id": "1",
          "time": "20210903 07:28"
        },
        {
          "id": "MDRW2100029412.1.15",
          "form": "비오는 날은요?",
          "original_form": "비오는 날은요?",
          "speaker_id": "1",
          "time": "20210903 07:28"
        },
        {
          "id": "MDRW2100029412.1.16",
          "form": "집에서 빗소리듣는걸 좋아해요",
          "original_form": "집에서 빗소리듣는걸 좋아해요",
          "speaker_id": "2",
          "time": "20210903 07:30"
        },
        {
          "id": "MDRW2100029412.1.17",
          "form": "밖에 다녀야하는 날은 싫지만요ㅎ",
          "original_form": "밖에 다녀야하는 날은 싫지만요ㅎ",
          "speaker_id": "2",
          "time": "20210903 07:30"
        },
        {
          "id": "MDRW2100029412.1.18",
          "form": "맞아요 빗소리 좋아요~어떤날 가장 좋아하세요?",
          "original_form": "맞아요 빗소리 좋아요~어떤날 가장 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 07:32"
        },
        {
          "id": "MDRW2100029412.1.19",
          "form": "저는 선선하게 바람부는 날을 좋아해요",
          "original_form": "저는 선선하게 바람부는 날을 좋아해요",
          "speaker_id": "2",
          "time": "20210903 07:32"
        },
        {
          "id": "MDRW2100029412.1.20",
          "form": "산책하기 좋은 날이기도 하고 책을 읽기도 안성맞춤이죠",
          "original_form": "산책하기 좋은 날이기도 하고 책을 읽기도 안성맞춤이죠",
          "speaker_id": "2",
          "time": "20210903 07:33"
        },
        {
          "id": "MDRW2100029412.1.21",
          "form": "가을을 좋아하시겠어요?",
          "original_form": "가을을 좋아하시겠어요?",
          "speaker_id": "1",
          "time": "20210903 07:33"
        }
      ]
