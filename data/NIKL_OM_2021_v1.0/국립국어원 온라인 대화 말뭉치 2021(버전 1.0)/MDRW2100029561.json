      "utterance": [
        {
          "id": "MDRW2100029561.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210903 10:20"
        },
        {
          "id": "MDRW2100029561.1.2",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "2",
          "time": "20210903 10:23"
        },
        {
          "id": "MDRW2100029561.1.3",
          "form": "뭐 스포츠 즐겨 보시는 것 있나요",
          "original_form": "뭐 스포츠 즐겨 보시는 것 있나요",
          "speaker_id": "2",
          "time": "20210903 10:23"
        },
        {
          "id": "MDRW2100029561.1.4",
          "form": "이번에 올림픽 재밌게 봤어요",
          "original_form": "이번에 올림픽 재밌게 봤어요",
          "speaker_id": "1",
          "time": "20210903 10:25"
        },
        {
          "id": "MDRW2100029561.1.5",
          "form": "트램폴린 너무 신기했어요",
          "original_form": "트램폴린 너무 신기했어요",
          "speaker_id": "1",
          "time": "20210903 10:25"
        },
        {
          "id": "MDRW2100029561.1.6",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210903 12:43"
        },
        {
          "id": "MDRW2100029561.1.7",
          "form": "확실히 올림픽 종목으로는 상상도 못하긴",
          "original_form": "확실히 올림픽 종목으로는 상상도 못하긴",
          "speaker_id": "2",
          "time": "20210903 12:43"
        },
        {
          "id": "MDRW2100029561.1.8",
          "form": "했었죠",
          "original_form": "했었죠",
          "speaker_id": "2",
          "time": "20210903 12:44"
        },
        {
          "id": "MDRW2100029561.1.9",
          "form": "했어요",
          "original_form": "했어요",
          "speaker_id": "2",
          "time": "20210903 14:18"
        },
        {
          "id": "MDRW2100029561.1.10",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "1",
          "time": "20210903 14:56"
        },
        {
          "id": "MDRW2100029561.1.11",
          "form": "신기한 종목",
          "original_form": "신기한 종목",
          "speaker_id": "1",
          "time": "20210903 14:57"
        },
        {
          "id": "MDRW2100029561.1.12",
          "form": "넘 재밌더라구요",
          "original_form": "넘 재밌더라구요",
          "speaker_id": "1",
          "time": "20210903 14:57"
        },
        {
          "id": "MDRW2100029561.1.13",
          "form": "클라이밍이랑 근대5종도 잼있었어요",
          "original_form": "클라이밍이랑 근대5종도 잼있었어요",
          "speaker_id": "1",
          "time": "20210903 14:57"
        },
        {
          "id": "MDRW2100029561.1.14",
          "form": "그러고보니",
          "original_form": "그러고보니",
          "speaker_id": "2",
          "time": "20210903 15:25"
        },
        {
          "id": "MDRW2100029561.1.15",
          "form": "근대5종에서",
          "original_form": "근대5종에서",
          "speaker_id": "2",
          "time": "20210903 15:25"
        },
        {
          "id": "MDRW2100029561.1.16",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "1",
          "time": "20210903 15:25"
        },
        {
          "id": "MDRW2100029561.1.17",
          "form": "말 관련 뭐 사건? 있었던가요",
          "original_form": "말 관련 뭐 사건? 있었던가요",
          "speaker_id": "2",
          "time": "20210903 15:25"
        },
        {
          "id": "MDRW2100029561.1.18",
          "form": "맞아요 말이",
          "original_form": "맞아요 말이",
          "speaker_id": "1",
          "time": "20210903 15:26"
        },
        {
          "id": "MDRW2100029561.1.19",
          "form": "극도의 스트레스땜에",
          "original_form": "극도의 스트레스땜에",
          "speaker_id": "1",
          "time": "20210903 15:26"
        },
        {
          "id": "MDRW2100029561.1.20",
          "form": "기수 떨어트리려고하고",
          "original_form": "기수 떨어트리려고하고",
          "speaker_id": "1",
          "time": "20210903 15:26"
        },
        {
          "id": "MDRW2100029561.1.21",
          "form": "허들도 넘어트리고",
          "original_form": "허들도 넘어트리고",
          "speaker_id": "1",
          "time": "20210903 15:26"
        },
        {
          "id": "MDRW2100029561.1.22",
          "form": "그 처음에는",
          "original_form": "그 처음에는",
          "speaker_id": "2",
          "time": "20210903 15:26"
        },
        {
          "id": "MDRW2100029561.1.23",
          "form": "이상한 방향으로 걷고",
          "original_form": "이상한 방향으로 걷고",
          "speaker_id": "1",
          "time": "20210903 15:26"
        },
        {
          "id": "MDRW2100029561.1.24",
          "form": "저래도 되나",
          "original_form": "저래도 되나",
          "speaker_id": "2",
          "time": "20210903 15:26"
        },
        {
          "id": "MDRW2100029561.1.25",
          "form": "이랬는데",
          "original_form": "이랬는데",
          "speaker_id": "2",
          "time": "20210903 15:26"
        },
        {
          "id": "MDRW2100029561.1.26",
          "form": "얘기 들어보니까",
          "original_form": "얘기 들어보니까",
          "speaker_id": "2",
          "time": "20210903 15:26"
        },
        {
          "id": "MDRW2100029561.1.27",
          "form": "전반적으로 이해는 되더라고요",
          "original_form": "전반적으로 이해는 되더라고요",
          "speaker_id": "2",
          "time": "20210903 15:27"
        },
        {
          "id": "MDRW2100029561.1.28",
          "form": "종목적 특징이나",
          "original_form": "종목적 특징이나",
          "speaker_id": "2",
          "time": "20210903 15:27"
        },
        {
          "id": "MDRW2100029561.1.29",
          "form": "말 스트레스나",
          "original_form": "말 스트레스나",
          "speaker_id": "2",
          "time": "20210903 15:27"
        },
        {
          "id": "MDRW2100029561.1.30",
          "form": "말 불쌍했어요",
          "original_form": "말 불쌍했어요",
          "speaker_id": "1",
          "time": "20210903 15:27"
        },
        {
          "id": "MDRW2100029561.1.31",
          "form": "그 표정이 정말ㅜㅜ",
          "original_form": "그 표정이 정말ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 15:27"
        },
        {
          "id": "MDRW2100029561.1.32",
          "form": "코치가 막 때렸었다죠",
          "original_form": "코치가 막 때렸었다죠",
          "speaker_id": "2",
          "time": "20210903 15:27"
        },
        {
          "id": "MDRW2100029561.1.33",
          "form": "그랬다더라구요",
          "original_form": "그랬다더라구요",
          "speaker_id": "1",
          "time": "20210903 15:27"
        }
      ]
    }
