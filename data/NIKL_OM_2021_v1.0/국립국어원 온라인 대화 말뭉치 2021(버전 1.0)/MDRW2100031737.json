      "utterance": [
        {
          "id": "MDRW2100031737.1.1",
          "form": "아이허브는 인터넷 사이트에요!",
          "original_form": "아이허브는 인터넷 사이트에요!",
          "speaker_id": "1",
          "time": "20210904 23:56"
        },
        {
          "id": "MDRW2100031737.1.2",
          "form": "좀 저렴한가요 ?",
          "original_form": "좀 저렴한가요 ?",
          "speaker_id": "2",
          "time": "20210904 23:56"
        },
        {
          "id": "MDRW2100031737.1.3",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210904 23:56"
        },
        {
          "id": "MDRW2100031737.1.4",
          "form": "안써보셨으면 첫구매 할인 그런게있어서",
          "original_form": "안써보셨으면 첫구매 할인 그런게있어서",
          "speaker_id": "1",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100031737.1.5",
          "form": "더할인많이받으실수있을거에요",
          "original_form": "더할인많이받으실수있을거에요",
          "speaker_id": "1",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100031737.1.6",
          "form": "오 좋네요",
          "original_form": "오 좋네요",
          "speaker_id": "2",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100031737.1.7",
          "form": "해볼게요 ㅎㅎ",
          "original_form": "해볼게요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100031737.1.8",
          "form": "도움이되면 좋겠네요!ㅎㅎ",
          "original_form": "도움이되면 좋겠네요!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100031737.1.9",
          "form": "고마워요 ㅎ",
          "original_form": "고마워요 ㅎ",
          "speaker_id": "2",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100031737.1.10",
          "form": "요즘 구매희망 물건있으세요?",
          "original_form": "요즘 구매희망 물건있으세요?",
          "speaker_id": "1",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100031737.1.11",
          "form": "집이요 ㅎ",
          "original_form": "집이요 ㅎ",
          "speaker_id": "2",
          "time": "20210904 23:57"
        },
        {
          "id": "MDRW2100031737.1.12",
          "form": "아 집 꿈의,,,! 꿈의집!!",
          "original_form": "아 집 꿈의,,,! 꿈의집!!",
          "speaker_id": "1",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100031737.1.13",
          "form": "집값이 많이 비싸죠",
          "original_form": "집값이 많이 비싸죠",
          "speaker_id": "2",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100031737.1.14",
          "form": "네 살면서 자가를 가질수있을지 ,,,",
          "original_form": "네 살면서 자가를 가질수있을지 ,,,",
          "speaker_id": "1",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100031737.1.15",
          "form": "잘 될 거에요~",
          "original_form": "잘 될 거에요~",
          "speaker_id": "2",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100031737.1.16",
          "form": "맞아요! 잘되야합니다,,!",
          "original_form": "맞아요! 잘되야합니다,,!",
