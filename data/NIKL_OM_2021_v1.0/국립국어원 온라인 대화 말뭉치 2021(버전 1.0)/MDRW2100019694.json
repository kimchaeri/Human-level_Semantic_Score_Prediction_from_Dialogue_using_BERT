      "utterance": [
        {
          "id": "MDRW2100019694.1.1",
          "form": "최근 알러지 검사했는데 고양이 알러지가 있어서 놀랐어요",
          "original_form": "최근 알러지 검사했는데 고양이 알러지가 있어서 놀랐어요",
          "speaker_id": "2",
          "time": "20210827 21:22"
        },
        {
          "id": "MDRW2100019694.1.2",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210827 21:23"
        },
        {
          "id": "MDRW2100019694.1.3",
          "form": "집사님인데 헉",
          "original_form": "집사님인데 헉",
          "speaker_id": "1",
          "time": "20210827 21:23"
        },
        {
          "id": "MDRW2100019694.1.4",
          "form": "괜찮으신가요?",
          "original_form": "괜찮으신가요?",
          "speaker_id": "1",
          "time": "20210827 21:28"
        },
        {
          "id": "MDRW2100019694.1.5",
          "form": "아이공ㅜㅜ",
          "original_form": "아이공ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 21:28"
        },
        {
          "id": "MDRW2100019694.1.6",
          "form": "저는 괜찮은데 의사선생님 반응이 너무 커서 놀랐어요",
          "original_form": "저는 괜찮은데 의사선생님 반응이 너무 커서 놀랐어요",
          "speaker_id": "2",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100019694.1.7",
          "form": "냥이 사랑의힘으로",
          "original_form": "냥이 사랑의힘으로",
          "speaker_id": "1",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100019694.1.8",
          "form": "알러지도 극복하나바여",
          "original_form": "알러지도 극복하나바여",
          "speaker_id": "1",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100019694.1.9",
          "form": "넹! 크게 힘든게 없었거든요ㅋㅋ",
          "original_form": "넹! 크게 힘든게 없었거든요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:35"
        },
        {
          "id": "MDRW2100019694.1.10",
          "form": "있는게 더 놀라웠어요",
          "original_form": "있는게 더 놀라웠어요",
          "speaker_id": "2",
          "time": "20210827 21:35"
        },
        {
          "id": "MDRW2100019694.1.11",
          "form": "냥이는 어떻게 생겻나욤",
          "original_form": "냥이는 어떻게 생겻나욤",
          "speaker_id": "1",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100019694.1.12",
          "form": "아메숏이요!ㅂ",
          "original_form": "아메숏이요!ㅂ",
          "speaker_id": "2",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100019694.1.13",
          "form": "백호에용",
          "original_form": "백호에용",
          "speaker_id": "2",
          "time": "20210827 21:40"
        },
        {
          "id": "MDRW2100019694.1.14",
          "form": "귀엽겟당ㅜㅜ백호",
          "original_form": "귀엽겟당ㅜㅜ백호",
          "speaker_id": "1",
          "time": "20210827 21:41"
        },
        {
          "id": "MDRW2100019694.1.15",
          "form": "고양이들도 털 마니빠지나용?",
          "original_form": "고양이들도 털 마니빠지나용?",
          "speaker_id": "1",
          "time": "20210827 21:41"
        },
        {
          "id": "MDRW2100019694.1.16",
          "form": "넵ㅡㅠ 단모종인데 더 빠지는거 같아요",
          "original_form": "넵ㅡㅠ 단모종인데 더 빠지는거 같아요",
          "speaker_id": "2",
          "time": "20210827 21:50"
        },
        {
          "id": "MDRW2100019694.1.17",
          "form": "오 마니빠지는구나",
          "original_form": "오 마니빠지는구나",
          "speaker_id": "1",
          "time": "20210827 21:51"
        },
        {
          "id": "MDRW2100019694.1.18",
          "form": "상상이안가요ㅋㅋ",
          "original_form": "상상이안가요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:51"
        },
        {
          "id": "MDRW2100019694.1.19",
          "form": "제가 한번도 안키워바서",
          "original_form": "제가 한번도 안키워바서",
          "speaker_id": "1",
          "time": "20210827 21:51"
        },
        {
          "id": "MDRW2100019694.1.20",
          "form": "아항ㅋㅋ 고양이 좋아하세요?",
          "original_form": "아항ㅋㅋ 고양이 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 21:52"
        },
        {
          "id": "MDRW2100019694.1.21",
          "form": "털이 복슬복슬하니",
          "original_form": "털이 복슬복슬하니",
          "speaker_id": "1",
          "time": "20210827 21:53"
        },
        {
          "id": "MDRW2100019694.1.22",
          "form": "귀엽자나요 ㅎㅎ",
          "original_form": "귀엽자나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 21:53"
        },
        {
          "id": "MDRW2100019694.1.23",
          "form": "맞아용",
          "original_form": "맞아용",
          "speaker_id": "2",
          "time": "20210827 21:57"
        }
      ]
