      "utterance": [
        {
          "id": "MDRW2100006911.1.1",
          "form": "운동하세요?",
          "original_form": "운동하세요?",
          "speaker_id": "1",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100006911.1.2",
          "form": "집에서 요가 가끔 해요. 무슨 운동하세요?",
          "original_form": "집에서 요가 가끔 해요. 무슨 운동하세요?",
          "speaker_id": "2",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100006911.1.3",
          "form": "저는 주로 달리기요 ㅋㅋ",
          "original_form": "저는 주로 달리기요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100006911.1.4",
          "form": "요가는 어때요?",
          "original_form": "요가는 어때요?",
          "speaker_id": "1",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100006911.1.5",
          "form": "쉬엄쉬엄 하려고 시작했는데 생각보다 어렵더라고요",
          "original_form": "쉬엄쉬엄 하려고 시작했는데 생각보다 어렵더라고요",
          "speaker_id": "2",
          "time": "20210822 00:00"
        },
        {
          "id": "MDRW2100006911.1.6",
          "form": "달리기 너무 멋있어요!",
          "original_form": "달리기 너무 멋있어요!",
          "speaker_id": "2",
          "time": "20210822 00:00"
        },
        {
          "id": "MDRW2100006911.1.7",
          "form": "야외에서 뛰는 거에요?",
          "original_form": "야외에서 뛰는 거에요?",
          "speaker_id": "2",
          "time": "20210822 00:00"
        },
        {
          "id": "MDRW2100006911.1.8",
          "form": "네네! 요새는 마스크 쓰고 하느라 조금 힘들더라구요 ㅋㅋ",
          "original_form": "네네! 요새는 마스크 쓰고 하느라 조금 힘들더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100006911.1.9",
          "form": "마스크만 써도 힘들던데 뛰기까지",
          "original_form": "마스크만 써도 힘들던데 뛰기까지",
          "speaker_id": "2",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006911.1.10",
          "form": "그래도 밖에서 운동하면 기분은 좋겠어요",
          "original_form": "그래도 밖에서 운동하면 기분은 좋겠어요",
          "speaker_id": "2",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006911.1.11",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006911.1.12",
          "form": "요즘 시원해져서 더 상쾌해요",
          "original_form": "요즘 시원해져서 더 상쾌해요",
          "speaker_id": "1",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006911.1.13",
          "form": "요가는 유튜브 보고 하시는거에요?",
          "original_form": "요가는 유튜브 보고 하시는거에요?",
          "speaker_id": "1",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006911.1.14",
          "form": "상상만 해도 상쾌하네요",
          "original_form": "상상만 해도 상쾌하네요",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006911.1.15",
          "form": "네 유튜브 검색해서 해요",
          "original_form": "네 유튜브 검색해서 해요",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006911.1.16",
          "form": "오늘은 요통 요가를 검색하고 싶은 날이네요",
          "original_form": "오늘은 요통 요가를 검색하고 싶은 날이네요",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006911.1.17",
          "form": "아이구 허리가 안좋으신가봐요 ㅜㅜ",
          "original_form": "아이구 허리가 안좋으신가봐요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006911.1.18",
          "form": "네 심하진 않은데 디스크 사이가 좁아졌대요",
          "original_form": "네 심하진 않은데 디스크 사이가 좁아졌대요",
          "speaker_id": "2",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006911.1.19",
          "form": "아이구 ㅜㅜ 어쩌다가..",
          "original_form": "아이구 ㅜㅜ 어쩌다가..",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006911.1.20",
          "form": "크로스핏하다 다친 거 같아요",
          "original_form": "크로스핏하다 다친 거 같아요",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006911.1.21",
          "form": "우와 크로스핏도 하세요?",
          "original_form": "우와 크로스핏도 하세요?",
          "speaker_id": "1",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006911.1.22",
          "form": "엄청 힘들다던데",
          "original_form": "엄청 힘들다던데",
          "speaker_id": "1",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006911.1.23",
          "form": "지금은 안 해요. 되게 재밌어요!",
          "original_form": "지금은 안 해요. 되게 재밌어요!",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006911.1.24",
          "form": "체력 진짜 좋으신가봐요!",
          "original_form": "체력 진짜 좋으신가봐요!",
          "speaker_id": "1",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006911.1.25",
          "form": "운동 열심히 할 때는 막 복근도 생겼었는데 흑 사라졌네요",
          "original_form": "운동 열심히 할 때는 막 복근도 생겼었는데 흑 사라졌네요",
          "speaker_id": "2",
          "time": "20210822 00:12"
        }
      ]
    }
