      "utterance": [
        {
          "id": "MDRW2100029605.1.1",
          "form": "마인크래프트",
          "original_form": "마인크래프트",
          "speaker_id": "2",
          "time": "20210903 10:51"
        },
        {
          "id": "MDRW2100029605.1.2",
          "form": "해보셨나요",
          "original_form": "해보셨나요",
          "speaker_id": "2",
          "time": "20210903 10:51"
        },
        {
          "id": "MDRW2100029605.1.3",
          "form": "롤플레잉은 끝을봐야해서",
          "original_form": "롤플레잉은 끝을봐야해서",
          "speaker_id": "1",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100029605.1.4",
          "form": "안해봤어요",
          "original_form": "안해봤어요",
          "speaker_id": "1",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100029605.1.5",
          "form": "이건 끝이없어요",
          "original_form": "이건 끝이없어요",
          "speaker_id": "2",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100029605.1.6",
          "form": "재밌다면서요",
          "original_form": "재밌다면서요",
          "speaker_id": "1",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100029605.1.7",
          "form": "무한이에요",
          "original_form": "무한이에요",
          "speaker_id": "2",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100029605.1.8",
          "form": "완전이요",
          "original_form": "완전이요",
          "speaker_id": "2",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100029605.1.9",
          "form": "머리쓰는거 절레절레",
          "original_form": "머리쓰는거 절레절레",
          "speaker_id": "1",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100029605.1.10",
          "form": "집지을수도 있고",
          "original_form": "집지을수도 있고",
          "speaker_id": "2",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100029605.1.11",
          "form": "좀비가 나와요",
          "original_form": "좀비가 나와요",
          "speaker_id": "2",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100029605.1.12",
          "form": "무섭네요",
          "original_form": "무섭네요",
          "speaker_id": "1",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100029605.1.13",
          "form": "괴물들 무찌를 수도 있고",
          "original_form": "괴물들 무찌를 수도 있고",
          "speaker_id": "2",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100029605.1.14",
          "form": "좀비랑 싸워야하나요",
          "original_form": "좀비랑 싸워야하나요",
          "speaker_id": "1",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100029605.1.15",
          "form": "아기 좀비가 빠르고 세요",
          "original_form": "아기 좀비가 빠르고 세요",
          "speaker_id": "2",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100029605.1.16",
          "form": "나도 좀비되나요",
          "original_form": "나도 좀비되나요",
          "speaker_id": "1",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100029605.1.17",
          "form": "ㄷㄷ",
          "original_form": "ㄷㄷ",
          "speaker_id": "1",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100029605.1.18",
          "form": "밤에 빨리자면 괴물안나와요",
          "original_form": "밤에 빨리자면 괴물안나와요",
          "speaker_id": "2",
          "time": "20210903 10:53"
        },
        {
          "id": "MDRW2100029605.1.19",
          "form": "좀비때문에 못하것습니다",
          "original_form": "좀비때문에 못하것습니다",
          "speaker_id": "1",
          "time": "20210903 10:53"
        },
        {
          "id": "MDRW2100029605.1.20",
          "form": "빨리안자면 나타나요",
          "original_form": "빨리안자면 나타나요",
          "speaker_id": "2",
          "time": "20210903 10:53"
        },
        {
          "id": "MDRW2100029605.1.21",
          "form": "죽고부활회이",
          "original_form": "죽고부활회이",
          "speaker_id": "2",
          "time": "20210903 10:53"
        },
        {
          "id": "MDRW2100029605.1.22",
          "form": "아 대낮부터 자야겠어요",
          "original_form": "아 대낮부터 자야겠어요",
          "speaker_id": "1",
          "time": "20210903 10:53"
        },
        {
          "id": "MDRW2100029605.1.23",
          "form": "대낮엔 못자요",
          "original_form": "대낮엔 못자요",
          "speaker_id": "2",
          "time": "20210903 10:53"
        }
      ]
