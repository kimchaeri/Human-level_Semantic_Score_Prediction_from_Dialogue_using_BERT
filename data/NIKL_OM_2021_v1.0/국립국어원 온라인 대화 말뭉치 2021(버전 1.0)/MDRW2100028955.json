        {
          "id": "MDRW2100028955.1.1",
          "form": "운동하자",
          "original_form": "운동하자",
          "speaker_id": "1",
          "time": "20210902 23:03"
        },
        {
          "id": "MDRW2100028955.1.2",
          "form": "싫어요",
          "original_form": "싫어요",
          "speaker_id": "2",
          "time": "20210902 23:03"
        },
        {
          "id": "MDRW2100028955.1.3",
          "form": "지금부터 연말까지 운동 하연",
          "original_form": "지금부터 연말까지 운동 하연",
          "speaker_id": "1",
          "time": "20210902 23:03"
        },
        {
          "id": "MDRW2100028955.1.4",
          "form": "하면?",
          "original_form": "하면?",
          "speaker_id": "2",
          "time": "20210902 23:03"
        },
        {
          "id": "MDRW2100028955.1.5",
          "form": "내년이 편햘거같아요",
          "original_form": "내년이 편햘거같아요",
          "speaker_id": "1",
          "time": "20210902 23:04"
        },
        {
          "id": "MDRW2100028955.1.6",
          "form": "엉청 미래지향적이시네요",
          "original_form": "엉청 미래지향적이시네요",
          "speaker_id": "2",
          "time": "20210902 23:04"
        },
        {
          "id": "MDRW2100028955.1.7",
          "form": "ㅋㅋ  미래지향ㅋㅋ",
          "original_form": "ㅋㅋ  미래지향ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:04"
        },
        {
          "id": "MDRW2100028955.1.8",
          "form": "올해 마지막목푠가요",
          "original_form": "올해 마지막목푠가요",
          "speaker_id": "2",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100028955.1.9",
          "form": "그런셈이에요",
          "original_form": "그런셈이에요",
          "speaker_id": "1",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100028955.1.10",
          "form": "사개으넌ㅅ도만 운동",
          "original_form": "사개으넌ㅅ도만 운동",
          "speaker_id": "2",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100028955.1.11",
          "form": "간단하고 멋있조?",
          "original_form": "간단하고 멋있조?",
          "speaker_id": "1",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100028955.1.12",
          "form": "간단한게 최고죠ㅋㅋ",
          "original_form": "간단한게 최고죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100028955.1.13",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100028955.1.14",
          "form": "바디프로빌 찍을건가요",
          "original_form": "바디프로빌 찍을건가요",
          "speaker_id": "2",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100028955.1.15",
          "form": "아뇨ㅋㅋ 운동이목적이라",
          "original_form": "아뇨ㅋㅋ 운동이목적이라",
          "speaker_id": "1",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100028955.1.16",
          "form": "그거하면 일생불가래요",
          "original_form": "그거하면 일생불가래요",
          "speaker_id": "2",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100028955.1.17",
