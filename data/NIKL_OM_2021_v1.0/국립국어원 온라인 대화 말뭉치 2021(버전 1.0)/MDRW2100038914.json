      "utterance": [
        {
          "id": "MDRW2100038914.1.1",
          "form": "여행 좋아하세오?",
          "original_form": "여행 좋아하세오?",
          "speaker_id": "2",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100038914.1.2",
          "form": "여름휴가 어디로 다녀오셨나요?",
          "original_form": "여름휴가 어디로 다녀오셨나요?",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100038914.1.3",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100038914.1.4",
          "form": "ㅇㅅㅇ*",
          "original_form": "ㅇㅅㅇ*",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100038914.1.5",
          "form": "저는 그 강촌 스타밸리",
          "original_form": "저는 그 강촌 스타밸리",
          "speaker_id": "2",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100038914.1.6",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100038914.1.7",
          "form": "고기를 구우셨나요?",
          "original_form": "고기를 구우셨나요?",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100038914.1.8",
          "form": "바이럴 아닙니다",
          "original_form": "바이럴 아닙니다",
          "speaker_id": "2",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100038914.1.9",
          "form": "바이럴 신고가 어딨더라ㅏㅏㅏ",
          "original_form": "바이럴 신고가 어딨더라ㅏㅏㅏ",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100038914.1.10",
          "form": "네 고기 구워서 그 동네 냥이들 다줌",
          "original_form": "네 고기 구워서 그 동네 냥이들 다줌",
          "speaker_id": "2",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100038914.1.11",
          "form": "아 선생님....",
          "original_form": "아 선생님....",
          "speaker_id": "2",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100038914.1.12",
          "form": "앗 여기 주제 봉사활동 아닌데요...",
          "original_form": "앗 여기 주제 봉사활동 아닌데요...",
          "speaker_id": "1",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100038914.1.13",
          "form": "앗....",
          "original_form": "앗....",
          "speaker_id": "2",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100038914.1.14",
          "form": "이분 까다롭네",
          "original_form": "이분 까다롭네",
          "speaker_id": "2",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100038914.1.15",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100038914.1.16",
          "form": "님은 최근 어디 갔나오",
          "original_form": "님은 최근 어디 갔나오",
          "speaker_id": "2",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100038914.1.17",
          "form": "혹시 여수는 아니죠?",
          "original_form": "혹시 여수는 아니죠?",
          "speaker_id": "2",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100038914.1.18",
          "form": "저는 순천만 다녀왔습니다",
          "original_form": "저는 순천만 다녀왔습니다",
          "speaker_id": "1",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100038914.1.19",
          "form": "국가정원 꼭 가세요",
          "original_form": "국가정원 꼭 가세요",
          "speaker_id": "1",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100038914.1.20",
          "form": "어",
          "original_form": "어",
          "speaker_id": "1",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100038914.1.21",
          "form": "독심술 하세요??",
          "original_form": "독심술 하세요??",
          "speaker_id": "1",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100038914.1.22",
          "form": "와 너무 무섭다 ㄷㄷㄷ",
          "original_form": "와 너무 무섭다 ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100038914.1.23",
          "form": "앗 거기 좋다고들었습니다",
          "original_form": "앗 거기 좋다고들었습니다",
          "speaker_id": "2",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100038914.1.24",
          "form": "아니 관상이 여수 갈거 같은 관상이여",
          "original_form": "아니 관상이 여수 갈거 같은 관상이여",
          "speaker_id": "2",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100038914.1.25",
          "form": "앗 저희 주제를 직업으로 바꿔야....",
          "original_form": "앗 저희 주제를 직업으로 바꿔야....",
          "speaker_id": "1",
          "time": "20210902 22:55"
        },
        {
          "id": "MDRW2100038914.1.26",
          "form": "혹시 직업이....",
          "original_form": "혹시 직업이....",
          "speaker_id": "1",
          "time": "20210902 22:55"
        },
        {
          "id": "MDRW2100038914.1.27",
          "form": "탐정이신가요...?",
          "original_form": "탐정이신가요...?",
          "speaker_id": "1",
          "time": "20210902 22:55"
        },
        {
          "id": "MDRW2100038914.1.28",
          "form": "족집게시네요 ㄷㄷㄷ",
          "original_form": "족집게시네요 ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210902 22:55"
        },
        {
          "id": "MDRW2100038914.1.29",
          "form": "여수가 요새 핫하다고 하던데",
          "original_form": "여수가 요새 핫하다고 하던데",
          "speaker_id": "1",
          "time": "20210902 22:55"
        },
        {
          "id": "MDRW2100038914.1.30",
          "form": "남치니가 있으면 꼭 데이트하러 가세요",
          "original_form": "남치니가 있으면 꼭 데이트하러 가세요",
          "speaker_id": "1",
          "time": "20210902 22:56"
        },
        {
          "id": "MDRW2100038914.1.31",
          "form": "해변가 칵테일바가 핫하다고 합니다ㅏㅏ",
          "original_form": "해변가 칵테일바가 핫하다고 합니다ㅏㅏ",
          "speaker_id": "1",
          "time": "20210902 22:56"
        },
        {
          "id": "MDRW2100038914.1.32",
          "form": "어휴 거기까지 운전하다가 황천ㄱ...",
          "original_form": "어휴 거기까지 운전하다가 황천ㄱ...",
          "speaker_id": "2",
          "time": "20210902 22:56"
        },
        {
          "id": "MDRW2100038914.1.33",
          "form": "ㅇ ㅏ 아닙니다",
          "original_form": "ㅇ ㅏ 아닙니다",
          "speaker_id": "2",
          "time": "20210902 22:56"
        },
        {
          "id": "MDRW2100038914.1.34",
          "form": "오 해변가 칵테일 좋네요",
          "original_form": "오 해변가 칵테일 좋네요",
          "speaker_id": "2",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100038914.1.35",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100038914.1.36",
          "form": "바닷가 여행지에서 해변을 보며 칵테일을 마시면 그렇게 좋다네요 ^-^",
          "original_form": "바닷가 여행지에서 해변을 보며 칵테일을 마시면 그렇게 좋다네요 ^-^",
          "speaker_id": "1",
          "time": "20210902 22:57"
        }
      ]
    }
