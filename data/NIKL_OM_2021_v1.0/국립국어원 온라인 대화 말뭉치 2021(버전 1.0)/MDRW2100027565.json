      "utterance": [
        {
          "id": "MDRW2100027565.1.1",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "1",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027565.1.2",
          "form": "루꼴라 좋아하세요?",
          "original_form": "루꼴라 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027565.1.3",
          "form": "안 먹어봤어요ㅋㅋㅋㅋ",
          "original_form": "안 먹어봤어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027565.1.4",
          "form": "그래요?ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그래요?ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027565.1.5",
          "form": "루꼴라 피자 다음에 드셔보세요",
          "original_form": "루꼴라 피자 다음에 드셔보세요",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027565.1.6",
          "form": "담백하고 괜찮아요",
          "original_form": "담백하고 괜찮아요",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027565.1.7",
          "form": "무슨 음식 좋아하세요?",
          "original_form": "무슨 음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027565.1.8",
          "form": "저는 가리는 거 별로 없어요!",
          "original_form": "저는 가리는 거 별로 없어요!",
          "speaker_id": "2",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027565.1.9",
          "form": "최근에 못 먹을 것 같았던 닭발도 먹어봤어욬ㅋㅋㅋㅋ",
          "original_form": "최근에 못 먹을 것 같았던 닭발도 먹어봤어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027565.1.10",
          "form": "와 닭발",
          "original_form": "와 닭발",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027565.1.11",
          "form": "저도 먹을 용기 없다가 친구들덕분애 먹어봤는데",
          "original_form": "저도 먹을 용기 없다가 친구들덕분애 먹어봤는데",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027565.1.12",
          "form": "꼬들꼬들 생각보다 괜찮더라고요",
          "original_form": "꼬들꼬들 생각보다 괜찮더라고요",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027565.1.13",
          "form": "매워서 문제였지만..",
          "original_form": "매워서 문제였지만..",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027565.1.14",
          "form": "매운 음식도 잘 드시나봐요?",
          "original_form": "매운 음식도 잘 드시나봐요?",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027565.1.15",
          "form": "보통 정도로 먹어요 ㅎㅎ",
          "original_form": "보통 정도로 먹어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027565.1.16",
          "form": "그렇구나 전 초딩보다 못한 맵찔이라",
          "original_form": "그렇구나 전 초딩보다 못한 맵찔이라",
          "speaker_id": "1",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027565.1.17",
          "form": "닭발 먹으면서 울면서 먹었던 기억이 나네요",
          "original_form": "닭발 먹으면서 울면서 먹었던 기억이 나네요",
          "speaker_id": "1",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027565.1.18",
          "form": "카페는 자주 가세요?",
          "original_form": "카페는 자주 가세요?",
          "speaker_id": "1",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100027565.1.19",
          "form": "나가면 선택지에 꼭 있어요 ㅎㅎ",
          "original_form": "나가면 선택지에 꼭 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:44"
        },
        {
          "id": "MDRW2100027565.1.20",
          "form": "그쵸 일단 외출하면 한번은 가게 되는 것 같아요",
          "original_form": "그쵸 일단 외출하면 한번은 가게 되는 것 같아요",
          "speaker_id": "1",
          "time": "20210902 13:45"
        },
        {
          "id": "MDRW2100027565.1.21",
          "form": "카페에서 주로 어떤 메뉴 드세요?",
          "original_form": "카페에서 주로 어떤 메뉴 드세요?",
          "speaker_id": "1",
          "time": "20210902 13:45"
        },
        {
          "id": "MDRW2100027565.1.22",
          "form": "요거트 종류나 에이드 종류로 마셔요",
          "original_form": "요거트 종류나 에이드 종류로 마셔요",
          "speaker_id": "2",
          "time": "20210902 13:46"
        },
        {
          "id": "MDRW2100027565.1.23",
          "form": "아하 커피는 잘 안 드시는가보구나",
          "original_form": "아하 커피는 잘 안 드시는가보구나",
          "speaker_id": "1",
          "time": "20210902 13:46"
        },
        {
          "id": "MDRW2100027565.1.24",
          "form": "네네 카페인이 좀 잘 받아서 ㅎㅎ",
          "original_form": "네네 카페인이 좀 잘 받아서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:47"
        },
        {
          "id": "MDRW2100027565.1.25",
          "form": "전 카페인을 너무 안 받아서 문젠데ㅠㅠㅎㅎㅎ",
          "original_form": "전 카페인을 너무 안 받아서 문젠데ㅠㅠㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 13:48"
        },
        {
          "id": "MDRW2100027565.1.26",
          "form": "좋아하는 과일 있으세요?",
          "original_form": "좋아하는 과일 있으세요?",
          "speaker_id": "1",
          "time": "20210902 13:48"
        },
        {
          "id": "MDRW2100027565.1.27",
          "form": "전 에이드 먹게되면 평소 안 먹는 과일 위주로 먹게되던데",
          "original_form": "전 에이드 먹게되면 평소 안 먹는 과일 위주로 먹게되던데",
          "speaker_id": "1",
          "time": "20210902 13:48"
        },
        {
          "id": "MDRW2100027565.1.28",
          "form": "배 빼고는 거의 다 좋아해요",
          "original_form": "배 빼고는 거의 다 좋아해요",
          "speaker_id": "2",
          "time": "20210902 13:49"
        },
        {
          "id": "MDRW2100027565.1.29",
          "form": "무슨 에이드 드세요?",
          "original_form": "무슨 에이드 드세요?",
          "speaker_id": "2",
          "time": "20210902 13:49"
        },
        {
          "id": "MDRW2100027565.1.30",
          "form": "저 자몽에이드 빼고 거의 다 마시는데",
          "original_form": "저 자몽에이드 빼고 거의 다 마시는데",
          "speaker_id": "1",
          "time": "20210902 13:49"
        },
        {
          "id": "MDRW2100027565.1.31",
          "form": "앗 마지막말 잘 못 봤어요",
          "original_form": "앗 마지막말 잘 못 봤어요",
          "speaker_id": "2",
          "time": "20210902 13:50"
        }
      ]
    }
