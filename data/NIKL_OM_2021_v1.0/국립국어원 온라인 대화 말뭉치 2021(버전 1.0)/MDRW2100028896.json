      "utterance": [
        {
          "id": "MDRW2100028896.1.1",
          "form": "일해요?",
          "original_form": "일해요?",
          "speaker_id": "1",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100028896.1.2",
          "form": "네 직장인이긴 해요",
          "original_form": "네 직장인이긴 해요",
          "speaker_id": "2",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100028896.1.3",
          "form": "지금은 퇴근했지만요",
          "original_form": "지금은 퇴근했지만요",
          "speaker_id": "2",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100028896.1.4",
          "form": "직장인이신가요?",
          "original_form": "직장인이신가요?",
          "speaker_id": "2",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100028896.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100028896.1.6",
          "form": "출퇴근은 뭘로 하세요??",
          "original_form": "출퇴근은 뭘로 하세요??",
          "speaker_id": "2",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100028896.1.7",
          "form": "자차요",
          "original_form": "자차요",
          "speaker_id": "1",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100028896.1.8",
          "form": "오 많이 안밀리나요??",
          "original_form": "오 많이 안밀리나요??",
          "speaker_id": "2",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100028896.1.9",
          "form": "그래더 자차가 편해서",
          "original_form": "그래더 자차가 편해서",
          "speaker_id": "1",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100028896.1.10",
          "form": "그건 그래요 ㅋㅋㅋㅋ",
          "original_form": "그건 그래요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:03"
        },
        {
          "id": "MDRW2100028896.1.11",
          "form": "대중교통은 쪽잠을 잘 수 있지만",
          "original_form": "대중교통은 쪽잠을 잘 수 있지만",
          "speaker_id": "2",
          "time": "20210902 23:03"
        },
        {
          "id": "MDRW2100028896.1.12",
          "form": "대신 부대끼는게 넘 싫더라구요",
          "original_form": "대신 부대끼는게 넘 싫더라구요",
          "speaker_id": "2",
          "time": "20210902 23:03"
        },
        {
          "id": "MDRW2100028896.1.13",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028896.1.14",
          "form": "불편하시겠요",
          "original_form": "불편하시겠요",
          "speaker_id": "1",
          "time": "20210903 07:49"
        },
        {
          "id": "MDRW2100028896.1.15",
          "form": "맞아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:03"
        },
        {
          "id": "MDRW2100028896.1.16",
          "form": "요새는 자차로 통근하긴하는데 출근길에 사고 잦은 곳이 있어서 계속 지각하네요ㅠㅠㅠ",
          "original_form": "요새는 자차로 통근하긴하는데 출근길에 사고 잦은 곳이 있어서 계속 지각하네요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 09:04"
        },
        {
          "id": "MDRW2100028896.1.17",
          "form": "일찍 나가셔야 겠네여",
          "original_form": "일찍 나가셔야 겠네여",
          "speaker_id": "1",
          "time": "20210903 09:34"
        },
        {
          "id": "MDRW2100028896.1.18",
          "form": "네.... 일부러 일찍 나가는데도 병목구간에 항상 사고가 나서 막혀있더라구요",
          "original_form": "네.... 일부러 일찍 나가는데도 병목구간에 항상 사고가 나서 막혀있더라구요",
          "speaker_id": "2",
          "time": "20210903 09:35"
        },
        {
          "id": "MDRW2100028896.1.19",
          "form": "다른 출근길을 뚫어보려구요 ㅋㅋㅋ",
          "original_form": "다른 출근길을 뚫어보려구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:35"
        },
        {
          "id": "MDRW2100028896.1.20",
          "form": "우회로뇨?",
          "original_form": "우회로뇨?",
          "speaker_id": "1",
          "time": "20210903 09:39"
        }
      ]
