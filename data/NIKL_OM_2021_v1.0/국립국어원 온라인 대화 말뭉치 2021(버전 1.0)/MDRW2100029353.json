      "utterance": [
        {
          "id": "MDRW2100029353.1.1",
          "form": "바캉스나 뱉어내 같은 곡들도 좋았던 것 같아",
          "original_form": "바캉스나 뱉어내 같은 곡들도 좋았던 것 같아",
          "speaker_id": "2",
          "time": "20210903 02:32"
        },
        {
          "id": "MDRW2100029353.1.2",
          "form": "그냥 그 앨범의 모든 곡을 다 좋아했어",
          "original_form": "그냥 그 앨범의 모든 곡을 다 좋아했어",
          "speaker_id": "1",
          "time": "20210903 02:33"
        },
        {
          "id": "MDRW2100029353.1.3",
          "form": "맨날 멜론에서 반복재생으로 주구장창 들었어...",
          "original_form": "맨날 멜론에서 반복재생으로 주구장창 들었어...",
          "speaker_id": "1",
          "time": "20210903 02:33"
        },
        {
          "id": "MDRW2100029353.1.4",
          "form": "그래도 한곡만 뽑자면 무슨 곡이 제일 좋았어?",
          "original_form": "그래도 한곡만 뽑자면 무슨 곡이 제일 좋았어?",
          "speaker_id": "2",
          "time": "20210903 02:34"
        },
        {
          "id": "MDRW2100029353.1.5",
          "form": "종이심장... 이 노래 진짜 아직도 듣는다",
          "original_form": "종이심장... 이 노래 진짜 아직도 듣는다",
          "speaker_id": "1",
          "time": "20210903 02:34"
        },
        {
          "id": "MDRW2100029353.1.6",
          "form": "크리스탈이 진짜 이 곡에 찰떡이었어",
          "original_form": "크리스탈이 진짜 이 곡에 찰떡이었어",
          "speaker_id": "1",
          "time": "20210903 02:34"
        },
        {
          "id": "MDRW2100029353.1.7",
          "form": "넌 원래 크리스탈 제일 좋아했잖아",
          "original_form": "넌 원래 크리스탈 제일 좋아했잖아",
          "speaker_id": "2",
          "time": "20210903 02:35"
        },
        {
          "id": "MDRW2100029353.1.8",
          "form": "ㅋㅋㅋ근데 그거랑은 또 별개로.. 그 곡 분위기랑 크리스탈 보컬이 너무 잘 어울렸어",
          "original_form": "ㅋㅋㅋ근데 그거랑은 또 별개로.. 그 곡 분위기랑 크리스탈 보컬이 너무 잘 어울렸어",
          "speaker_id": "1",
          "time": "20210903 02:35"
        },
        {
          "id": "MDRW2100029353.1.9",
          "form": "그래 그렇다 치자ㅎㅎ",
          "original_form": "그래 그렇다 치자ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 02:36"
        },
        {
          "id": "MDRW2100029353.1.10",
          "form": "그 앨범에 붐뱅붐인가 그 노래도 좋은데",
          "original_form": "그 앨범에 붐뱅붐인가 그 노래도 좋은데",
          "speaker_id": "2",
          "time": "20210903 02:36"
        },
        {
          "id": "MDRW2100029353.1.11",
          "form": "아 그 노래도 기억 나ㅋㅋㅋ",
          "original_form": "아 그 노래도 기억 나ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 02:36"
        },
        {
          "id": "MDRW2100029353.1.12",
          "form": "그 노래 샤이니 노래중에",
          "original_form": "그 노래 샤이니 노래중에",
          "speaker_id": "1",
          "time": "20210903 02:36"
        },
        {
          "id": "MDRW2100029353.1.13",
          "form": "펀치 드렁크 러브 < 이 곡이랑 연결된다고 유명했는대",
          "original_form": "펀치 드렁크 러브 < 이 곡이랑 연결된다고 유명했는대",
          "speaker_id": "1",
          "time": "20210903 02:37"
        },
        {
          "id": "MDRW2100029353.1.14",
          "form": "진짜인지는 모르겠다",
          "original_form": "진짜인지는 모르겠다",
          "speaker_id": "1",
          "time": "20210903 02:37"
        },
        {
          "id": "MDRW2100029353.1.15",
          "form": "아마 진짜 맞을걸",
          "original_form": "아마 진짜 맞을걸",
          "speaker_id": "1",
          "time": "20210903 02:37"
        },
        {
          "id": "MDRW2100029353.1.16",
          "form": "오 신기하네 다음에 같이 들어봐야겠다",
          "original_form": "오 신기하네 다음에 같이 들어봐야겠다",
          "speaker_id": "2",
          "time": "20210903 02:37"
        },
        {
          "id": "MDRW2100029353.1.17",
          "form": "여름이라 핫썸머도 듣고 싶네",
          "original_form": "여름이라 핫썸머도 듣고 싶네",
          "speaker_id": "2",
          "time": "20210903 02:38"
        },
        {
          "id": "MDRW2100029353.1.18",
          "form": "와 핫썸머...",
          "original_form": "와 핫썸머...",
          "speaker_id": "1",
          "time": "20210903 02:39"
        },
        {
          "id": "MDRW2100029353.1.19",
          "form": "여름의 상징 같은 곡이야",
          "original_form": "여름의 상징 같은 곡이야",
          "speaker_id": "1",
          "time": "20210903 02:39"
        },
        {
          "id": "MDRW2100029353.1.20",
          "form": "땀 흘리는 외국인은 길을 알려주자~",
          "original_form": "땀 흘리는 외국인은 길을 알려주자~",
          "speaker_id": "1",
          "time": "20210903 02:39"
        },
        {
          "id": "MDRW2100029353.1.21",
          "form": "이 가사 진짜 웃겼는데ㅋㅋㅋ",
          "original_form": "이 가사 진짜 웃겼는데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 02:39"
        },
        {
          "id": "MDRW2100029353.1.22",
          "form": "맞어 그 노래 가사가 엄청 특이했던 것 같아",
          "original_form": "맞어 그 노래 가사가 엄청 특이했던 것 같아",
          "speaker_id": "2",
          "time": "20210903 02:40"
        },
        {
          "id": "MDRW2100029353.1.23",
          "form": "에프엑스 노래 가사가 다 특이했지ㅋㅋㅋㅋ",
          "original_form": "에프엑스 노래 가사가 다 특이했지ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 02:40"
        },
        {
          "id": "MDRW2100029353.1.24",
          "form": "처음에 누에삐오 발매됐을 때도 다들 가사 뭔 소린지 모르겠다고 난리였어ㅋㅋㅋ",
          "original_form": "처음에 누에삐오 발매됐을 때도 다들 가사 뭔 소린지 모르겠다고 난리였어ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 02:41"
        },
        {
          "id": "MDRW2100029353.1.25",
          "form": "근데 그게 에프엑스 노래의 매력이었던 것 같네",
          "original_form": "근데 그게 에프엑스 노래의 매력이었던 것 같네",
          "speaker_id": "2",
          "time": "20210903 02:41"
        },
        {
          "id": "MDRW2100029353.1.26",
          "form": "이 새벽에 갑자기 2008-2015년 사이 음악에 빠졌네...ㅋㅋㅋㅋ",
          "original_form": "이 새벽에 갑자기 2008-2015년 사이 음악에 빠졌네...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 02:43"
        },
        {
          "id": "MDRW2100029353.1.27",
          "form": "이때 내가 샤이니 좋아하기 시작해서 sm엔터 음악을 진짜 많이 들었어",
          "original_form": "이때 내가 샤이니 좋아하기 시작해서 sm엔터 음악을 진짜 많이 들었어",
          "speaker_id": "1",
          "time": "20210903 02:43"
        }
      ]
