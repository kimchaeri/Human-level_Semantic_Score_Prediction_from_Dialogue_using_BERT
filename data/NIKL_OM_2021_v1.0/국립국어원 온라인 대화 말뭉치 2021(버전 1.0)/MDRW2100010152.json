      "utterance": [
        {
          "id": "MDRW2100010152.1.1",
          "form": "아 그지",
          "original_form": "아 그지",
          "speaker_id": "2",
          "time": "20210823 14:52"
        },
        {
          "id": "MDRW2100010152.1.2",
          "form": "뭔가 새러운 걸 보게 되잖아",
          "original_form": "뭔가 새러운 걸 보게 되잖아",
          "speaker_id": "2",
          "time": "20210823 14:53"
        },
        {
          "id": "MDRW2100010152.1.3",
          "form": "예전에는판타지안좋아했는데",
          "original_form": "예전에는판타지안좋아했는데",
          "speaker_id": "1",
          "time": "20210823 14:53"
        },
        {
          "id": "MDRW2100010152.1.4",
          "form": "요즘들어재밌음",
          "original_form": "요즘들어재밌음",
          "speaker_id": "1",
          "time": "20210823 14:53"
        },
        {
          "id": "MDRW2100010152.1.5",
          "form": "아 그래??",
          "original_form": "아 그래??",
          "speaker_id": "2",
          "time": "20210823 14:53"
        },
        {
          "id": "MDRW2100010152.1.6",
          "form": "뭐 봐?",
          "original_form": "뭐 봐?",
          "speaker_id": "2",
          "time": "20210823 14:53"
        },
        {
          "id": "MDRW2100010152.1.7",
          "form": "책?",
          "original_form": "책?",
          "speaker_id": "1",
          "time": "20210823 14:54"
        },
        {
          "id": "MDRW2100010152.1.8",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210823 14:54"
        },
        {
          "id": "MDRW2100010152.1.9",
          "form": "요즘엄청다양하게보는듯",
          "original_form": "요즘엄청다양하게보는듯",
          "speaker_id": "1",
          "time": "20210823 14:54"
        },
        {
          "id": "MDRW2100010152.1.10",
          "form": "판타지책제목기억이안남",
          "original_form": "판타지책제목기억이안남",
          "speaker_id": "1",
          "time": "20210823 14:54"
        },
        {
          "id": "MDRW2100010152.1.11",
          "form": "아 그럴 수 있지",
          "original_form": "아 그럴 수 있지",
          "speaker_id": "2",
          "time": "20210823 14:55"
        },
        {
          "id": "MDRW2100010152.1.12",
          "form": "또 다른 책 추천 좀",
          "original_form": "또 다른 책 추천 좀",
          "speaker_id": "1",
          "time": "20210823 14:55"
        },
        {
          "id": "MDRW2100010152.1.13",
          "form": "해줘",
          "original_form": "해줘",
          "speaker_id": "1",
          "time": "20210823 14:55"
        },
        {
          "id": "MDRW2100010152.1.14",
          "form": "읽을만한거",
          "original_form": "읽을만한거",
          "speaker_id": "1",
          "time": "20210823 14:55"
        },
        {
          "id": "MDRW2100010152.1.15",
          "form": "정세랑 작가 책은 거의 다 재밌음",
          "original_form": "정세랑 작가 책은 거의 다 재밌음",
          "speaker_id": "2",
          "time": "20210823 14:55"
        },
        {
          "id": "MDRW2100010152.1.16",
          "form": "판타지만쓰시는게아닌가보네",
          "original_form": "판타지만쓰시는게아닌가보네",
          "speaker_id": "1",
          "time": "20210823 14:56"
        },
        {
          "id": "MDRW2100010152.1.17",
          "form": "응응 약간 판타지 집중이라기 보단",
          "original_form": "응응 약간 판타지 집중이라기 보단",
          "speaker_id": "2",
          "time": "20210823 14:56"
        },
        {
          "id": "MDRW2100010152.1.18",
          "form": "판타지는 그냥 이용하고 실제 보여주는 거는 사람들 이야기를 보는 편이야",
          "original_form": "판타지는 그냥 이용하고 실제 보여주는 거는 사람들 이야기를 보는 편이야",
          "speaker_id": "2",
          "time": "20210823 14:57"
        },
        {
          "id": "MDRW2100010152.1.19",
          "form": "아 맞아 보건교사",
          "original_form": "아 맞아 보건교사",
          "speaker_id": "1",
          "time": "20210823 14:57"
        },
        {
          "id": "MDRW2100010152.1.20",
          "form": "그것도",
          "original_form": "그것도",
          "speaker_id": "1",
          "time": "20210823 14:57"
        },
        {
          "id": "MDRW2100010152.1.21",
          "form": "그런느낌이었던거",
          "original_form": "그런느낌이었던거",
          "speaker_id": "1",
          "time": "20210823 14:57"
        },
        {
          "id": "MDRW2100010152.1.22",
          "form": "같아",
          "original_form": "같아",
          "speaker_id": "1",
          "time": "20210823 14:57"
        },
        {
          "id": "MDRW2100010152.1.23",
          "form": "제대로보진않았지만",
          "original_form": "제대로보진않았지만",
          "speaker_id": "1",
          "time": "20210823 14:57"
        },
        {
          "id": "MDRW2100010152.1.24",
          "form": "응응 그런 식이야 재밌음",
          "original_form": "응응 그런 식이야 재밌음",
          "speaker_id": "2",
          "time": "20210823 14:58"
        },
        {
          "id": "MDRW2100010152.1.25",
          "form": "아 추천작은 피프티 피플 안은영 목소리를 드릴게요",
          "original_form": "아 추천작은 피프티 피플 안은영 목소리를 드릴게요",
          "speaker_id": "2",
          "time": "20210823 14:58"
        }
      ]
