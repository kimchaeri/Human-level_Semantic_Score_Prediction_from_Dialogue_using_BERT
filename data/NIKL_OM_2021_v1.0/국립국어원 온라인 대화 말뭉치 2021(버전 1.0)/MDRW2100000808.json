      "utterance": [
        {
          "id": "MDRW2100000808.1.1",
          "form": "구름이 잔뜩이다",
          "original_form": "구름이 잔뜩이다",
          "speaker_id": "1",
          "time": "20210819 19:38"
        },
        {
          "id": "MDRW2100000808.1.2",
          "form": "비는 안오려나",
          "original_form": "비는 안오려나",
          "speaker_id": "2",
          "time": "20210819 19:38"
        },
        {
          "id": "MDRW2100000808.1.3",
          "form": "우산있어?",
          "original_form": "우산있어?",
          "speaker_id": "1",
          "time": "20210819 19:38"
        },
        {
          "id": "MDRW2100000808.1.4",
          "form": "양산 조그만한것",
          "original_form": "양산 조그만한것",
          "speaker_id": "2",
          "time": "20210819 19:38"
        },
        {
          "id": "MDRW2100000808.1.5",
          "form": "나도 우양산 있어",
          "original_form": "나도 우양산 있어",
          "speaker_id": "1",
          "time": "20210819 19:38"
        },
        {
          "id": "MDRW2100000808.1.6",
          "form": "요즘 날씨 변덕스러워",
          "original_form": "요즘 날씨 변덕스러워",
          "speaker_id": "2",
          "time": "20210819 19:39"
        },
        {
          "id": "MDRW2100000808.1.7",
          "form": "그래도 비온 뒤로 약간 시원해졌어",
          "original_form": "그래도 비온 뒤로 약간 시원해졌어",
          "speaker_id": "1",
          "time": "20210819 19:39"
        },
        {
          "id": "MDRW2100000808.1.8",
          "form": "차라리 비올때 확내리는게나음",
          "original_form": "차라리 비올때 확내리는게나음",
          "speaker_id": "2",
          "time": "20210819 19:39"
        },
        {
          "id": "MDRW2100000808.1.9",
          "form": "근데 그건 집에 있을때",
          "original_form": "근데 그건 집에 있을때",
          "speaker_id": "1",
          "time": "20210819 19:40"
        },
        {
          "id": "MDRW2100000808.1.10",
          "form": "맞아 비올땐 집안이 최고임",
          "original_form": "맞아 비올땐 집안이 최고임",
          "speaker_id": "2",
          "time": "20210819 19:40"
        },
        {
          "id": "MDRW2100000808.1.11",
          "form": "근데 눅눅한건 싫어",
          "original_form": "근데 눅눅한건 싫어",
          "speaker_id": "1",
          "time": "20210819 19:40"
        },
        {
          "id": "MDRW2100000808.1.12",
          "form": "맞아 비오면 질퍽함",
          "original_form": "맞아 비오면 질퍽함",
          "speaker_id": "2",
          "time": "20210819 19:40"
