      "utterance": [
        {
          "id": "MDRW2100043348.1.1",
          "form": "저녁 드셨나용",
          "original_form": "저녁 드셨나용",
          "speaker_id": "1",
          "time": "20210907 18:03"
        },
        {
          "id": "MDRW2100043348.1.2",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "2",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100043348.1.3",
          "form": "그릭요거트에 남은 포카칩이랑",
          "original_form": "그릭요거트에 남은 포카칩이랑",
          "speaker_id": "2",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100043348.1.4",
          "form": "크레이프케익까지 야무지게 먹었답니다",
          "original_form": "크레이프케익까지 야무지게 먹었답니다",
          "speaker_id": "2",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100043348.1.5",
          "form": "오 그릭요거트에 포카칩 같이 먹으면 맛있나요??",
          "original_form": "오 그릭요거트에 포카칩 같이 먹으면 맛있나요??",
          "speaker_id": "1",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100043348.1.6",
          "form": "ㅋㅋㅋㅋㅋ따로먹었답니다",
          "original_form": "ㅋㅋㅋㅋㅋ따로먹었답니다",
          "speaker_id": "2",
          "time": "20210907 18:06"
        },
        {
          "id": "MDRW2100043348.1.7",
          "form": "포카칩-그릭요거트-케익 순으로 먹었어요",
          "original_form": "포카칩-그릭요거트-케익 순으로 먹었어요",
          "speaker_id": "2",
          "time": "20210907 18:06"
        },
        {
          "id": "MDRW2100043348.1.8",
          "form": "아하ㅋㅋ 도넛에 포카칩 먹는 조합을 유튜브에서 봐서 그릭요거트랑도 조합이 잘맞나했답니다 ㅎㅎ",
          "original_form": "아하ㅋㅋ 도넛에 포카칩 먹는 조합을 유튜브에서 봐서 그릭요거트랑도 조합이 잘맞나했답니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 18:28"
        },
        {
          "id": "MDRW2100043348.1.9",
          "form": "군것질류 좋아하시나봐요! 저도 오늘 엄청 먹었어요",
          "original_form": "군것질류 좋아하시나봐요! 저도 오늘 엄청 먹었어요",
          "speaker_id": "1",
          "time": "20210907 18:28"
        },
        {
          "id": "MDRW2100043348.1.10",
          "form": "요즘 군것질류가 엄청땡겨요",
          "original_form": "요즘 군것질류가 엄청땡겨요",
          "speaker_id": "2",
          "time": "20210907 18:29"
        },
        {
          "id": "MDRW2100043348.1.11",
          "form": "확실히 군것질을 하기 시작하니까",
          "original_form": "확실히 군것질을 하기 시작하니까",
          "speaker_id": "2",
          "time": "20210907 18:29"
        },
        {
          "id": "MDRW2100043348.1.12",
          "form": "허리둘레가 늘더라고요(?)",
          "original_form": "허리둘레가 늘더라고요(?)",
          "speaker_id": "2",
          "time": "20210907 18:29"
        },
        {
          "id": "MDRW2100043348.1.13",
          "form": "맞아요 진짜 배에만 살이 붙는지 ..",
          "original_form": "맞아요 진짜 배에만 살이 붙는지 ..",
          "speaker_id": "1",
          "time": "20210907 18:33"
        },
        {
          "id": "MDRW2100043348.1.14",
          "form": "군것질류 며칠 먹으면 배가 훅 나오더라구요ㅋㅋ",
          "original_form": "군것질류 며칠 먹으면 배가 훅 나오더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:33"
        },
        {
          "id": "MDRW2100043348.1.15",
          "form": "저는 보통 하체에 살이 붙는편이었는데",
          "original_form": "저는 보통 하체에 살이 붙는편이었는데",
          "speaker_id": "2",
          "time": "20210907 18:38"
        },
        {
          "id": "MDRW2100043348.1.16",
          "form": "달달한 간식  먹기 시작한뒤로",
          "original_form": "달달한 간식  먹기 시작한뒤로",
          "speaker_id": "2",
          "time": "20210907 18:38"
        },
        {
          "id": "MDRW2100043348.1.17",
          "form": "배에 살이 붙기 시작해서",
          "original_form": "배에 살이 붙기 시작해서",
          "speaker_id": "2",
          "time": "20210907 18:39"
        },
        {
          "id": "MDRW2100043348.1.18",
          "form": "옷 사이즈도 하나 늘었답니다",
          "original_form": "옷 사이즈도 하나 늘었답니다",
          "speaker_id": "2",
          "time": "20210907 18:39"
        },
        {
          "id": "MDRW2100043348.1.19",
          "form": "히히",
          "original_form": "히히",
          "speaker_id": "2",
          "time": "20210907 18:39"
        },
        {
          "id": "MDRW2100043348.1.20",
          "form": "와 저랑 똑같으세요 ㅋㅋ저도 다리에만 살찌는 체형이었는데 군것질 계속먹으니까 이제 균형적인 통통이 되었네요",
          "original_form": "와 저랑 똑같으세요 ㅋㅋ저도 다리에만 살찌는 체형이었는데 군것질 계속먹으니까 이제 균형적인 통통이 되었네요",
          "speaker_id": "1",
          "time": "20210907 18:44"
        },
        {
          "id": "MDRW2100043348.1.21",
          "form": "균형적 통통!!",
          "original_form": "균형적 통통!!",
          "speaker_id": "2",
          "time": "20210907 18:46"
        },
        {
          "id": "MDRW2100043348.1.22",
          "form": "좋은거겠죠?ㅋㅋㅋ",
          "original_form": "좋은거겠죠?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:46"
        },
        {
          "id": "MDRW2100043348.1.23",
          "form": "그렇겠죠....? 핫 ㅎㅎ",
          "original_form": "그렇겠죠....? 핫 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 18:47"
        },
        {
          "id": "MDRW2100043348.1.24",
          "form": "과자는 감자칩 좋아하시나요?",
          "original_form": "과자는 감자칩 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 18:47"
        },
        {
          "id": "MDRW2100043348.1.25",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210907 18:48"
        },
        {
          "id": "MDRW2100043348.1.26",
          "form": "감자칩은 역시",
          "original_form": "감자칩은 역시",
          "speaker_id": "2",
          "time": "20210907 18:48"
        },
        {
          "id": "MDRW2100043348.1.27",
          "form": "포카칩 어니언맛",
          "original_form": "포카칩 어니언맛",
          "speaker_id": "2",
          "time": "20210907 18:48"
        },
        {
          "id": "MDRW2100043348.1.28",
          "form": "포카칩은 어니언맛이 진리죠",
          "original_form": "포카칩은 어니언맛이 진리죠",
          "speaker_id": "1",
