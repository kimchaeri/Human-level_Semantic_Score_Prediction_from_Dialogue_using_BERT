      "utterance": [
        {
          "id": "MDRW2100019739.1.1",
          "form": "펜트하우스 보세요?",
          "original_form": "펜트하우스 보세요?",
          "speaker_id": "2",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100019739.1.2",
          "form": "예전에 시즌1봤어요",
          "original_form": "예전에 시즌1봤어요",
          "speaker_id": "1",
          "time": "20210827 21:41"
        },
        {
          "id": "MDRW2100019739.1.3",
          "form": "오홍 요즘 보시는거 있으세요?",
          "original_form": "오홍 요즘 보시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210827 21:50"
        },
        {
          "id": "MDRW2100019739.1.4",
          "form": "금쪽이여ㅎ",
          "original_form": "금쪽이여ㅎ",
          "speaker_id": "1",
          "time": "20210827 21:53"
        },
        {
          "id": "MDRW2100019739.1.5",
          "form": "예능이죠??",
          "original_form": "예능이죠??",
          "speaker_id": "2",
          "time": "20210827 21:58"
        },
        {
          "id": "MDRW2100019739.1.6",
          "form": "육아프로여",
          "original_form": "육아프로여",
          "speaker_id": "1",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100019739.1.7",
          "form": "아 저도 가끔 봐요",
          "original_form": "아 저도 가끔 봐요",
          "speaker_id": "2",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100019739.1.8",
          "form": "짠하고 속상하더라구요",
          "original_form": "짠하고 속상하더라구요",
          "speaker_id": "2",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100019739.1.9",
          "form": "엄청 울게되는",
          "original_form": "엄청 울게되는",
          "speaker_id": "1",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100019739.1.10",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100019739.1.11",
          "form": "둘 다 이해되고 안타깝고 그래요",
          "original_form": "둘 다 이해되고 안타깝고 그래요",
          "speaker_id": "2",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100019739.1.12",
          "form": "다 제일같더라고요",
          "original_form": "다 제일같더라고요",
          "speaker_id": "1",
          "time": "20210827 22:04"
        },
        {
          "id": "MDRW2100019739.1.13",
          "form": "공감가시는게 많으셨나봐요",
          "original_form": "공감가시는게 많으셨나봐요",
          "speaker_id": "2",
          "time": "20210827 22:04"
        },
        {
          "id": "MDRW2100019739.1.14",
          "form": "네 아이키우기가 다 힘들구나..",
          "original_form": "네 아이키우기가 다 힘들구나..",
