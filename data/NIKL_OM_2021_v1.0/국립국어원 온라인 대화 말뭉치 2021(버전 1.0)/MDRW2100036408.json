        {
          "id": "MDRW2100036408.1.2",
          "form": "아녕하세요",
          "original_form": "아녕하세요",
          "speaker_id": "1",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100036408.1.3",
          "form": "별명이 왜 슈우우 인가요",
          "original_form": "별명이 왜 슈우우 인가요",
          "speaker_id": "1",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100036408.1.4",
          "form": "그러개요",
          "original_form": "그러개요",
          "speaker_id": "2",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100036408.1.5",
          "form": "술 매니아라서 일본말로 슈 인가요?",
          "original_form": "술 매니아라서 일본말로 슈 인가요?",
          "speaker_id": "1",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100036408.1.6",
          "form": "헐... 맞아요!",
          "original_form": "헐... 맞아요!",
          "speaker_id": "2",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100036408.1.7",
          "form": "아하.. 무슨 술이 젤좋나여",
          "original_form": "아하.. 무슨 술이 젤좋나여",
          "speaker_id": "1",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100036408.1.8",
          "form": "양주요",
          "original_form": "양주요",
          "speaker_id": "2",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100036408.1.9",
          "form": "아........ 그러시군요 위스키중에 뭐가좋죠??",
          "original_form": "아........ 그러시군요 위스키중에 뭐가좋죠??",
          "speaker_id": "1",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100036408.1.10",
          "form": "그런건 없고 칵테일 좋아래요",
          "original_form": "그런건 없고 칵테일 좋아래요",
          "speaker_id": "2",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100036408.1.11",
          "form": "오... 무슨칵테일이요",
          "original_form": "오... 무슨칵테일이요",
          "speaker_id": "1",
          "time": "20210830 22:37"
        },
        {
          "id": "MDRW2100036408.1.12",
          "form": "???????",
          "original_form": "???????",
          "speaker_id": "1",
          "time": "20210830 22:42"
        },
        {
          "id": "MDRW2100036408.1.13",
          "form": "준벅?",
          "original_form": "준벅?",
          "speaker_id": "2",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100036408.1.14",
          "form": "이름 기억안나내요",
          "original_form": "이름 기억안나내요",
          "speaker_id": "2",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100036408.1.15",
          "form": "오 이름은 들어봣어여",
          "original_form": "오 이름은 들어봣어여",
          "speaker_id": "1",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100036408.1.16",
          "form": "혹시 멸치다시다 구입하는것에대해 어떻게생각하세여?",
          "original_form": "혹시 멸치다시다 구입하는것에대해 어떻게생각하세여?",
          "speaker_id": "1",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100036408.1.17",
          "form": "전 혼다시로 충분해요",
          "original_form": "전 혼다시로 충분해요",
          "speaker_id": "2",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100036408.1.18",
          "form": "멸치다시다로 집앞 떡볶이 완성인데",
          "original_form": "멸치다시다로 집앞 떡볶이 완성인데",
