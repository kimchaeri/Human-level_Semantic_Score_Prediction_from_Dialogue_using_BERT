      "utterance": [
        {
          "id": "MDRW2100045206.1.1",
          "form": "또 어떤거 재밌게보셨어요? 넷플에서",
          "original_form": "또 어떤거 재밌게보셨어요? 넷플에서",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045206.1.2",
          "form": "저는 코미디 드라마 좋아해서",
          "original_form": "저는 코미디 드라마 좋아해서",
          "speaker_id": "2",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045206.1.3",
          "form": "숫플레이스 재미있었어요",
          "original_form": "숫플레이스 재미있었어요",
          "speaker_id": "2",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045206.1.4",
          "form": "굿플레이스!",
          "original_form": "굿플레이스!",
          "speaker_id": "2",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045206.1.5",
          "form": "아 굿플레이스 재밌다고 들었어요!",
          "original_form": "아 굿플레이스 재밌다고 들었어요!",
          "speaker_id": "1",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045206.1.6",
          "form": "거기 막 악마나오고 그런거죠?",
          "original_form": "거기 막 악마나오고 그런거죠?",
          "speaker_id": "1",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045206.1.7",
          "form": "네네 ㅋㅋㅋ",
          "original_form": "네네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045206.1.8",
          "form": "대충 생전에 착한일을 많이하면",
          "original_form": "대충 생전에 착한일을 많이하면",
          "speaker_id": "2",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045206.1.9",
          "form": "ㅋㅋㅋㅋㅋ역쉬 그거맞았군여",
          "original_form": "ㅋㅋㅋㅋㅋ역쉬 그거맞았군여",
          "speaker_id": "1",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045206.1.10",
          "form": "+점수로 천국에가는 내용이에요",
          "original_form": "+점수로 천국에가는 내용이에요",
          "speaker_id": "2",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045206.1.11",
          "form": "엄청높은 점수만 천국이고 그냥저냥 산사람은 지옥에 보내는 ㅜ",
          "original_form": "엄청높은 점수만 천국이고 그냥저냥 산사람은 지옥에 보내는 ㅜ",
          "speaker_id": "2",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045206.1.12",
          "form": "아하 ㅋㅋㅋㅋㅋ앗 중간은없나여",
          "original_form": "아하 ㅋㅋㅋㅋㅋ앗 중간은없나여",
          "speaker_id": "1",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045206.1.13",
          "form": "한90퍼는 다지옥보내나여 ㅋㅋㅋㅋ",
          "original_form": "한90퍼는 다지옥보내나여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045206.1.14",
          "form": "주인공은 쓰레기인데 뭔가잘못되서 천국에 떨어지는 내용이에요 ㅋㅋ",
          "original_form": "주인공은 쓰레기인데 뭔가잘못되서 천국에 떨어지는 내용이에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045206.1.15",
          "form": "보다보면 나오는데 거의 99.9퍼...",
          "original_form": "보다보면 나오는데 거의 99.9퍼...",
          "speaker_id": "2",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045206.1.16",
          "form": "막 노예제도 끝낸 링컨 이정도 되야 천국가는거죠",
          "original_form": "막 노예제도 끝낸 링컨 이정도 되야 천국가는거죠",
          "speaker_id": "2",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045206.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아이고 ㅋㅋㅋㅋㅋㅋ엄청 바늘구멍이네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아이고 ㅋㅋㅋㅋㅋㅋ엄청 바늘구멍이네요",
          "speaker_id": "1",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045206.1.18",
          "form": "그러니까요 전 굿플레이스가기 포기했어요 보면서 ㅋㅋ",
          "original_form": "그러니까요 전 굿플레이스가기 포기했어요 보면서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045206.1.19",
          "form": "ㅋㅋㅋㅋㅋ저도안될거같아여 성자들만가네요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ저도안될거같아여 성자들만가네요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100045206.1.20",
          "form": "근대 주인공은 싸구려 가짜약 노인들한테 사기쳐서팔고사는 술주정뱅이인데",
          "original_form": "근대 주인공은 싸구려 가짜약 노인들한테 사기쳐서팔고사는 술주정뱅이인데",
          "speaker_id": "2",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045206.1.21",
          "form": "앜 ㅋㅋㅋㅋㅋㅋㅋㅋ완전 지옥행인데",
          "original_form": "앜 ㅋㅋㅋㅋㅋㅋㅋㅋ완전 지옥행인데",
          "speaker_id": "1",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045206.1.22",
          "form": "난민구호활동하고 사형수 변호봉사하는 동명이인이랑 바뀌어서",
          "original_form": "난민구호활동하고 사형수 변호봉사하는 동명이인이랑 바뀌어서",
          "speaker_id": "2",
          "time": "20210908 01:24"
        }
      ]
    }
  ]
