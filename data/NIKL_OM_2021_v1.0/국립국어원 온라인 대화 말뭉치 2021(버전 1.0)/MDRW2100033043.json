      "utterance": [
        {
          "id": "MDRW2100033043.1.1",
          "form": "아 그니까요",
          "original_form": "아 그니까요",
          "speaker_id": "1",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100033043.1.2",
          "form": "자기는 자기를 여자라고 생각하는데",
          "original_form": "자기는 자기를 여자라고 생각하는데",
          "speaker_id": "1",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100033043.1.3",
          "form": "왜 자기를 무시하냐",
          "original_form": "왜 자기를 무시하냐",
          "speaker_id": "1",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100033043.1.4",
          "form": "그러면서 항의한다잖아요",
          "original_form": "그러면서 항의한다잖아요",
          "speaker_id": "1",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100033043.1.5",
          "form": "그래서 차별금지법도 잘 알아야하는게",
          "original_form": "그래서 차별금지법도 잘 알아야하는게",
          "speaker_id": "1",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100033043.1.6",
          "form": "아니 그렇게따지면",
          "original_form": "아니 그렇게따지면",
          "speaker_id": "2",
          "time": "20210906 14:39"
        },
        {
          "id": "MDRW2100033043.1.7",
          "form": "표면만 보면 좋아보이는데",
          "original_form": "표면만 보면 좋아보이는데",
          "speaker_id": "1",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100033043.1.8",
          "form": "나도 나 10대라 우기면",
          "original_form": "나도 나 10대라 우기면",
          "speaker_id": "2",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100033043.1.9",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100033043.1.10",
          "form": "10대해주냐구요",
          "original_form": "10대해주냐구요",
          "speaker_id": "2",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100033043.1.11",
          "form": "아주 맞는 말씀이세요",
          "original_form": "아주 맞는 말씀이세요",
          "speaker_id": "1",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100033043.1.12",
          "form": "그래서 다들 하는 말이 ㅋㅋㅋㅋㅋ",
          "original_form": "그래서 다들 하는 말이 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100033043.1.13",
          "form": "그럼 나는 트랜스 미국인이다!!!",
          "original_form": "그럼 나는 트랜스 미국인이다!!!",
          "speaker_id": "1",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100033043.1.14",
          "form": "나는 내가 백인 남자라고 생각한다. 어서 인정해줘!",
          "original_form": "나는 내가 백인 남자라고 생각한다. 어서 인정해줘!",
          "speaker_id": "1",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100033043.1.15",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100033043.1.16",
          "form": "이런거 관련한 책들도 많거든요,,,",
          "original_form": "이런거 관련한 책들도 많거든요,,,",
          "speaker_id": "1",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100033043.1.17",
          "form": "(주제가 도서네요...)",
          "original_form": "(주제가 도서네요...)",
          "speaker_id": "1",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100033043.1.18",
          "form": "책ㅋㅋㅋㅋㅋ",
          "original_form": "책ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100033043.1.19",
          "form": "네...",
          "original_form": "네...",
          "speaker_id": "1",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100033043.1.20",
          "form": "실제로 열다북스 라는 출판사에서",
          "original_form": "실제로 열다북스 라는 출판사에서",
          "speaker_id": "1",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100033043.1.21",
          "form": "이런",
          "original_form": "이런",
          "speaker_id": "1",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100033043.1.22",
          "form": "넵넵",
          "original_form": "넵넵",
          "speaker_id": "2",
          "time": "20210906 14:41"
        },
        {
          "id": "MDRW2100033043.1.23",
          "form": "내용의 책을 많이 내는데",
          "original_form": "내용의 책을 많이 내는데",
          "speaker_id": "1",
          "time": "20210906 14:41"
        },
        {
          "id": "MDRW2100033043.1.24",
          "form": "근데 저 출판사도 말이 많아서",
          "original_form": "근데 저 출판사도 말이 많아서",
          "speaker_id": "1",
          "time": "20210906 14:41"
        },
        {
          "id": "MDRW2100033043.1.25",
          "form": "저도 사서 보진 않고",
          "original_form": "저도 사서 보진 않고",
