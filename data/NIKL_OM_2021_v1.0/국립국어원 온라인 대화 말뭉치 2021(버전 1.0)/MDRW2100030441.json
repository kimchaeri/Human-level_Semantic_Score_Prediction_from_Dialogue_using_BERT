        {
          "id": "MDRW2100030441.1.1",
          "form": "여행 안가시나요",
          "original_form": "여행 안가시나요",
          "speaker_id": "1",
          "time": "20210904 01:03"
        },
        {
          "id": "MDRW2100030441.1.2",
          "form": "저 나름 자주 갑니다 ㅎㅎ",
          "original_form": "저 나름 자주 갑니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 01:05"
        },
        {
          "id": "MDRW2100030441.1.3",
          "form": "오 정말요 최근 어디?",
          "original_form": "오 정말요 최근 어디?",
          "speaker_id": "1",
          "time": "20210904 01:07"
        },
        {
          "id": "MDRW2100030441.1.4",
          "form": "최근에는 송도 오크우드요 ㅎㅎ",
          "original_form": "최근에는 송도 오크우드요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 01:11"
        },
        {
          "id": "MDRW2100030441.1.5",
          "form": "리조트에요?",
          "original_form": "리조트에요?",
          "speaker_id": "1",
          "time": "20210904 01:13"
        },
        {
          "id": "MDRW2100030441.1.6",
          "form": "아뇨 호텔인댕",
          "original_form": "아뇨 호텔인댕",
          "speaker_id": "2",
          "time": "20210904 01:17"
        },
        {
          "id": "MDRW2100030441.1.7",
          "form": "호캉스하셧어요?",
          "original_form": "호캉스하셧어요?",
          "speaker_id": "1",
          "time": "20210904 01:18"
        },
        {
          "id": "MDRW2100030441.1.8",
          "form": "네 즐기다왔죠",
          "original_form": "네 즐기다왔죠",
          "speaker_id": "2",
          "time": "20210904 01:24"
        },
        {
          "id": "MDRW2100030441.1.9",
          "form": "38층!!",
          "original_form": "38층!!",
          "speaker_id": "2",
          "time": "20210904 01:24"
        },
        {
          "id": "MDRW2100030441.1.10",
          "form": "우와 .. 멋잇엇겟다",
          "original_form": "우와 .. 멋잇엇겟다",
          "speaker_id": "1",
          "time": "20210904 01:28"
        },
        {
          "id": "MDRW2100030441.1.11",
          "form": "조식 즐기셨나요",
          "original_form": "조식 즐기셨나요",
          "speaker_id": "1",
          "time": "20210904 01:28"
        },
        {
          "id": "MDRW2100030441.1.12",
          "form": "ㅋㅋㅋ 코시국이라 안했는데 할껄 그랬나봐요",
          "original_form": "ㅋㅋㅋ 코시국이라 안했는데 할껄 그랬나봐요",
          "speaker_id": "2",
          "time": "20210904 05:17"
        },
        {
          "id": "MDRW2100030441.1.13",
          "form": "ㅋㅋㅋㅋㅋ 으아 아깝당 ,, 거기만 잇으셧나요",
          "original_form": "ㅋㅋㅋㅋㅋ 으아 아깝당 ,, 거기만 잇으셧나요",
          "speaker_id": "1",
          "time": "20210904 09:39"
        },
        {
          "id": "MDRW2100030441.1.14",
          "form": "네 2박 있다가 복귀 ㅋㅋ",
          "original_form": "네 2박 있다가 복귀 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 09:41"
        },
        {
          "id": "MDRW2100030441.1.15",
          "form": "힐링하고오셨군요ㅏ",
          "original_form": "힐링하고오셨군요ㅏ",
          "speaker_id": "1",
          "time": "20210904 09:42"
        },
        {
          "id": "MDRW2100030441.1.16",
          "form": "또가고싶으신덴 어딘가요",
          "original_form": "또가고싶으신덴 어딘가요",
          "speaker_id": "1",
          "time": "20210904 09:42"
        },
        {
          "id": "MDRW2100030441.1.17",
          "form": "와근데 오크우드 엄청비싸네요 칠십마넌 ?_?_?",
          "original_form": "와근데 오크우드 엄청비싸네요 칠십마넌 ?_?_?",
