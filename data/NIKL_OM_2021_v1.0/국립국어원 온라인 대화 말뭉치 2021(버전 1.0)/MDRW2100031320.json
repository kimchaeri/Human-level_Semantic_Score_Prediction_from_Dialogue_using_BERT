      "utterance": [
        {
          "id": "MDRW2100031320.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210904 20:25"
        },
        {
          "id": "MDRW2100031320.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210904 20:27"
        },
        {
          "id": "MDRW2100031320.1.3",
          "form": "혹시 최근에 읽으신 책 있나용?",
          "original_form": "혹시 최근에 읽으신 책 있나용?",
          "speaker_id": "1",
          "time": "20210904 20:28"
        },
        {
          "id": "MDRW2100031320.1.4",
          "form": "짜라투스트라는 이렇게 말했다 라는 책이요...",
          "original_form": "짜라투스트라는 이렇게 말했다 라는 책이요...",
          "speaker_id": "2",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100031320.1.5",
          "form": "넘 두꺼워서 하루에 막1페이지도 못읽고있어요...",
          "original_form": "넘 두꺼워서 하루에 막1페이지도 못읽고있어요...",
          "speaker_id": "2",
          "time": "20210904 20:33"
        },
        {
          "id": "MDRW2100031320.1.6",
          "form": "헐..멋있어요 그거 저 읽어봐야지 라는 생각만 하고 있는 책인데..",
          "original_form": "헐..멋있어요 그거 저 읽어봐야지 라는 생각만 하고 있는 책인데..",
          "speaker_id": "1",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100031320.1.7",
          "form": "전 책 편식이 심해가지구 너무 멋있네여ㅠㅠ",
          "original_form": "전 책 편식이 심해가지구 너무 멋있네여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 20:34"
        },
        {
          "id": "MDRW2100031320.1.8",
          "form": "허걱",
          "original_form": "허걱",
          "speaker_id": "2",
          "time": "20210904 20:36"
        },
        {
          "id": "MDRW2100031320.1.9",
          "form": "저도 책 편식이 엄청심해요",
          "original_form": "저도 책 편식이 엄청심해요",
          "speaker_id": "2",
          "time": "20210904 20:36"
        },
        {
          "id": "MDRW2100031320.1.10",
          "form": "그래서 항상 철학책이나",
          "original_form": "그래서 항상 철학책이나",
          "speaker_id": "2",
          "time": "20210904 20:36"
        },
        {
          "id": "MDRW2100031320.1.11",
          "form": "전문서적이런거만보게되요ㅠ",
          "original_form": "전문서적이런거만보게되요ㅠ",
          "speaker_id": "2",
          "time": "20210904 20:36"
        },
        {
          "id": "MDRW2100031320.1.12",
          "form": "소설책도일겅야하는데",
          "original_form": "소설책도일겅야하는데",
          "speaker_id": "2",
          "time": "20210904 20:36"
        },
        {
          "id": "MDRW2100031320.1.13",
          "form": "헉 저랑 완전 반대시네요!! 전 소설책만 봐가지고요ㅠㅋㅋ",
          "original_form": "헉 저랑 완전 반대시네요!! 전 소설책만 봐가지고요ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:37"
        },
        {
          "id": "MDRW2100031320.1.14",
          "form": "전문서적은 최근에 방구석 미술관 재밌게 봤어요!",
          "original_form": "전문서적은 최근에 방구석 미술관 재밌게 봤어요!",
          "speaker_id": "1",
          "time": "20210904 20:38"
        },
        {
          "id": "MDRW2100031320.1.15",
          "form": "오오 들어본 책이에요",
          "original_form": "오오 들어본 책이에요",
          "speaker_id": "2",
          "time": "20210904 21:17"
        },
        {
          "id": "MDRW2100031320.1.16",
          "form": "소설은 어떤 장르좋아하시나요",
          "original_form": "소설은 어떤 장르좋아하시나요",
          "speaker_id": "2",
          "time": "20210904 21:17"
        },
        {
          "id": "MDRW2100031320.1.17",
          "form": "<칵테일 러브 좀비>",
          "original_form": "<칵테일 러브 좀비>",
          "speaker_id": "2",
          "time": "20210904 21:18"
        },
        {
          "id": "MDRW2100031320.1.18",
          "form": "라는 책을 최근읽었었거든요",
          "original_form": "라는 책을 최근읽었었거든요",
          "speaker_id": "2",
          "time": "20210904 21:18"
        },
        {
          "id": "MDRW2100031320.1.19",
          "form": "단편집? 이런느낌인데",
          "original_form": "단편집? 이런느낌인데",
          "speaker_id": "2",
          "time": "20210904 21:18"
        },
        {
          "id": "MDRW2100031320.1.20",
          "form": "재밋으니까 나중에 시간되시면 꼭한번 읽어보세용",
          "original_form": "재밋으니까 나중에 시간되시면 꼭한번 읽어보세용",
          "speaker_id": "2",
          "time": "20210904 21:18"
        },
        {
          "id": "MDRW2100031320.1.21",
          "form": "헐 쳐보니까 재밌을거같아요!!! 약간 sf느낌나는거 좋아하는데 제 취향일거같네여 ㅠㅠㅠ 감사합니당!",
          "original_form": "헐 쳐보니까 재밌을거같아요!!! 약간 sf느낌나는거 좋아하는데 제 취향일거같네여 ㅠㅠㅠ 감사합니당!",
          "speaker_id": "1",
          "time": "20210904 21:27"
        },
        {
          "id": "MDRW2100031320.1.22",
          "form": "저 약간 sf좋아해서.. 우리가 빛의 속도로 갈 수 없다면 이랑 목소리를 드릴게요 재밌게 봤어요!",
          "original_form": "저 약간 sf좋아해서.. 우리가 빛의 속도로 갈 수 없다면 이랑 목소리를 드릴게요 재밌게 봤어요!",
          "speaker_id": "1",
          "time": "20210904 21:27"
        },
        {
          "id": "MDRW2100031320.1.23",
          "form": "허거걱",
          "original_form": "허거걱",
          "speaker_id": "2",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100031320.1.24",
          "form": "나중에한번 찾아읽어볼게용",
          "original_form": "나중에한번 찾아읽어볼게용",
          "speaker_id": "2",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100031320.1.25",
          "form": "넹ㅎㅎ 후루룩 읽혀서 추천드려여!",
          "original_form": "넹ㅎㅎ 후루룩 읽혀서 추천드려여!",
          "speaker_id": "1",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100031320.1.26",
          "form": "베르나르베르베르 소설도 좋아하시겠어요!",
          "original_form": "베르나르베르베르 소설도 좋아하시겠어요!",
          "speaker_id": "2",
          "time": "20210905 02:13"
        },
        {
          "id": "MDRW2100031320.1.27",
          "form": "파피용을 최근에 읽었었는데",
          "original_form": "파피용을 최근에 읽었었는데",
          "speaker_id": "2",
          "time": "20210905 02:14"
        },
        {
          "id": "MDRW2100031320.1.28",
          "form": "너무 좋더라구요... 막 읽으면서 뭔가 저희 인류의 처음과 마지막을 닮은듯한...",
          "original_form": "너무 좋더라구요... 막 읽으면서 뭔가 저희 인류의 처음과 마지막을 닮은듯한...",
          "speaker_id": "2",
          "time": "20210905 02:14"
        },
        {
          "id": "MDRW2100031320.1.29",
          "form": "정말 천재인거같아요ㅠ",
          "original_form": "정말 천재인거같아요ㅠ",
          "speaker_id": "2",
          "time": "20210905 02:14"
        },
        {
          "id": "MDRW2100031320.1.30",
          "form": "헉!! 저 나무 재밌게 봤었는데!!",
          "original_form": "헉!! 저 나무 재밌게 봤었는데!!",
          "speaker_id": "1",
          "time": "20210905 14:27"
        },
        {
          "id": "MDRW2100031320.1.31",
          "form": "파피용도 보고싶었던거예요!! 또 읽고싶은책이 많아지네요ㅋㅋㅋㅋ",
          "original_form": "파피용도 보고싶었던거예요!! 또 읽고싶은책이 많아지네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:27"
        },
        {
          "id": "MDRW2100031320.1.32",
          "form": "이렇게 책 얘기하니까 갑자기 막 읽고싶어져요ㅋㅋㅋ사실 요즘들어 책을 멀리했었는데,,",
          "original_form": "이렇게 책 얘기하니까 갑자기 막 읽고싶어져요ㅋㅋㅋ사실 요즘들어 책을 멀리했었는데,,",
          "speaker_id": "1",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100031320.1.33",
          "form": "허걱",
          "original_form": "허걱",
          "speaker_id": "2",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100031320.1.34",
          "form": "책은 언제나 주변에있으니까",
          "original_form": "책은 언제나 주변에있으니까",
          "speaker_id": "2",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100031320.1.35",
          "form": "멀리해도 손만뻗으면",
          "original_form": "멀리해도 손만뻗으면",
          "speaker_id": "2",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100031320.1.36",
          "form": "잡히는게책이라",
          "original_form": "잡히는게책이라",
          "speaker_id": "2",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100031320.1.37",
          "form": "읽고싶을때읽을수있다는게",
          "original_form": "읽고싶을때읽을수있다는게",
          "speaker_id": "2",
          "time": "20210905 14:32"
        },
        {
          "id": "MDRW2100031320.1.38",
          "form": "너무좋은거같네용",
          "original_form": "너무좋은거같네용",
          "speaker_id": "2",
          "time": "20210905 14:32"
        }
      ]
