      "utterance": [
        {
          "id": "MDRW2100024781.1.1",
          "form": "혹시 직장인이세요?",
          "original_form": "혹시 직장인이세요?",
          "speaker_id": "1",
          "time": "20210831 18:27"
        },
        {
          "id": "MDRW2100024781.1.2",
          "form": "직장인이세요?",
          "original_form": "직장인이세요?",
          "speaker_id": "2",
          "time": "20210831 18:27"
        },
        {
          "id": "MDRW2100024781.1.3",
          "form": "전 직장인이긴 하네요",
          "original_form": "전 직장인이긴 하네요",
          "speaker_id": "2",
          "time": "20210831 18:32"
        },
        {
          "id": "MDRW2100024781.1.4",
          "form": "연구직입니다",
          "original_form": "연구직입니다",
          "speaker_id": "2",
          "time": "20210831 18:33"
        },
        {
          "id": "MDRW2100024781.1.5",
          "form": "우와 멋진 직업이시네요~^^",
          "original_form": "우와 멋진 직업이시네요~^^",
          "speaker_id": "1",
          "time": "20210831 18:36"
        },
        {
          "id": "MDRW2100024781.1.6",
          "form": "주로 어떤 분야를 연구하시나요?",
          "original_form": "주로 어떤 분야를 연구하시나요?",
          "speaker_id": "1",
          "time": "20210831 18:37"
        },
        {
          "id": "MDRW2100024781.1.7",
          "form": "화학인데요",
          "original_form": "화학인데요",
          "speaker_id": "2",
          "time": "20210831 18:37"
        },
        {
          "id": "MDRW2100024781.1.8",
          "form": "유사단백질을 연구한다고 하면 되겠습니다ㅎㅎ",
          "original_form": "유사단백질을 연구한다고 하면 되겠습니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 18:38"
        },
        {
          "id": "MDRW2100024781.1.9",
          "form": "연구결과에 따라 제품 생산으로 연결되기까지 하는건가요?",
          "original_form": "연구결과에 따라 제품 생산으로 연결되기까지 하는건가요?",
          "speaker_id": "1",
          "time": "20210831 18:40"
        },
        {
          "id": "MDRW2100024781.1.10",
          "form": "그랬으면 좋겠지만",
          "original_form": "그랬으면 좋겠지만",
          "speaker_id": "2",
          "time": "20210831 18:41"
        },
        {
          "id": "MDRW2100024781.1.11",
          "form": "전 기업이 아니라 대학 연구실 소속이라서요",
          "original_form": "전 기업이 아니라 대학 연구실 소속이라서요",
          "speaker_id": "2",
          "time": "20210831 18:42"
        },
        {
          "id": "MDRW2100024781.1.12",
          "form": "논문정도만 생산되겠네요",
          "original_form": "논문정도만 생산되겠네요",
          "speaker_id": "2",
          "time": "20210831 18:42"
        },
        {
          "id": "MDRW2100024781.1.13",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "1",
          "time": "20210831 18:43"
        },
        {
          "id": "MDRW2100024781.1.14",
          "form": "업무량은 많으신가요?",
          "original_form": "업무량은 많으신가요?",
          "speaker_id": "1",
          "time": "20210831 18:43"
        },
        {
          "id": "MDRW2100024781.1.15",
          "form": "업무량은 음 많다고하긴 그런데",
          "original_form": "업무량은 음 많다고하긴 그런데",
          "speaker_id": "2",
          "time": "20210831 18:44"
        },
        {
          "id": "MDRW2100024781.1.16",
          "form": "출근 10시 퇴근 9시긴 해요",
          "original_form": "출근 10시 퇴근 9시긴 해요",
          "speaker_id": "2",
          "time": "20210831 18:44"
        },
        {
          "id": "MDRW2100024781.1.17",
          "form": "아......대신 그만큼 보람도 있고 통장도 넉넉해지나요?",
          "original_form": "아......대신 그만큼 보람도 있고 통장도 넉넉해지나요?",
          "speaker_id": "1",
          "time": "20210831 18:45"
        },
        {
          "id": "MDRW2100024781.1.18",
          "form": "통장은 안넉넉해지고요",
          "original_form": "통장은 안넉넉해지고요",
          "speaker_id": "2",
          "time": "20210831 18:47"
        },
        {
          "id": "MDRW2100024781.1.19",
          "form": "보람은 개인문제고",
          "original_form": "보람은 개인문제고",
          "speaker_id": "2",
          "time": "20210831 18:47"
        },
        {
          "id": "MDRW2100024781.1.20",
          "form": "약간 미래를 위한 초석? 같은거죠",
          "original_form": "약간 미래를 위한 초석? 같은거죠",
          "speaker_id": "2",
          "time": "20210831 18:47"
        },
        {
          "id": "MDRW2100024781.1.21",
          "form": "아....스트레스는 많이 받으시나요?",
          "original_form": "아....스트레스는 많이 받으시나요?",
          "speaker_id": "1",
          "time": "20210831 18:50"
        },
        {
          "id": "MDRW2100024781.1.22",
          "form": "어... 실험 잘 안될때 많이 받았는데",
          "original_form": "어... 실험 잘 안될때 많이 받았는데",
          "speaker_id": "2",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100024781.1.23",
          "form": "이젠 걍   그러려니 해요^^",
          "original_form": "이젠 걍   그러려니 해요^^",
          "speaker_id": "2",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100024781.1.24",
          "form": "멘탈관리가 중요하겠네요",
          "original_form": "멘탈관리가 중요하겠네요",
          "speaker_id": "1",
          "time": "20210831 18:54"
        },
        {
          "id": "MDRW2100024781.1.25",
          "form": "그렇죠.",
          "original_form": "그렇죠.",
          "speaker_id": "2",
          "time": "20210831 18:55"
        },
        {
          "id": "MDRW2100024781.1.26",
          "form": "나중에 취직은 잘될까 하는 고민도 있고요",
          "original_form": "나중에 취직은 잘될까 하는 고민도 있고요",
          "speaker_id": "2",
          "time": "20210831 18:55"
        },
        {
          "id": "MDRW2100024781.1.27",
          "form": "요새는 워낙 취직이 힘들어서ㅜㅜ",
          "original_form": "요새는 워낙 취직이 힘들어서ㅜㅜ",
