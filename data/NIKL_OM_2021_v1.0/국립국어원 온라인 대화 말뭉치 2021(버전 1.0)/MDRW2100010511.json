      "utterance": [
        {
          "id": "MDRW2100010511.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210823 19:02"
        },
        {
          "id": "MDRW2100010511.1.2",
          "form": "게임 하시는 거 있나요?",
          "original_form": "게임 하시는 거 있나요?",
          "speaker_id": "1",
          "time": "20210823 19:02"
        },
        {
          "id": "MDRW2100010511.1.3",
          "form": "얼마전에 어몽어스 좀 하다가 요즘엔 안 하고 있어요",
          "original_form": "얼마전에 어몽어스 좀 하다가 요즘엔 안 하고 있어요",
          "speaker_id": "2",
          "time": "20210823 21:09"
        },
        {
          "id": "MDRW2100010511.1.4",
          "form": "재밌는 게임 있나요??",
          "original_form": "재밌는 게임 있나요??",
          "speaker_id": "2",
          "time": "20210823 21:09"
        },
        {
          "id": "MDRW2100010511.1.5",
          "form": "오 어몽어스 어떤가요???!",
          "original_form": "오 어몽어스 어떤가요???!",
          "speaker_id": "1",
          "time": "20210823 21:27"
        },
        {
          "id": "MDRW2100010511.1.6",
          "form": "재밌어요! 익명이라서 가끔 트롤들이 있긴하지만 ㅠㅠ",
          "original_form": "재밌어요! 익명이라서 가끔 트롤들이 있긴하지만 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 21:28"
        },
        {
          "id": "MDRW2100010511.1.7",
          "form": "친구들이랑 해도 재밌어요 ㅎㅎ",
          "original_form": "친구들이랑 해도 재밌어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 21:29"
        },
        {
          "id": "MDRW2100010511.1.8",
          "form": "재밌을 거 같아요",
          "original_form": "재밌을 거 같아요",
          "speaker_id": "1",
          "time": "20210823 21:39"
        },
        {
          "id": "MDRW2100010511.1.9",
          "form": "한창 핫했을 때 궁금했는데",
          "original_form": "한창 핫했을 때 궁금했는데",
          "speaker_id": "1",
          "time": "20210823 21:39"
        },
        {
          "id": "MDRW2100010511.1.10",
          "form": "못했거든요",
          "original_form": "못했거든요",
          "speaker_id": "1",
          "time": "20210823 21:39"
        },
        {
          "id": "MDRW2100010511.1.11",
          "form": "업데이트 되고 맵도 새로워지고 한번에 참여할 수 있는 인원이 많아지면서 입문 난이도가 좀 높아지긴 했어요 ㅠㅠ",
          "original_form": "업데이트 되고 맵도 새로워지고 한번에 참여할 수 있는 인원이 많아지면서 입문 난이도가 좀 높아지긴 했어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 21:42"
        },
        {
          "id": "MDRW2100010511.1.12",
          "form": "혹시 하시게 되면 유튭 같은데서 게임 영상 보시구 시작하시는 걸 추천드려요 ㅎㅎ",
          "original_form": "혹시 하시게 되면 유튭 같은데서 게임 영상 보시구 시작하시는 걸 추천드려요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 21:43"
        },
        {
          "id": "MDRW2100010511.1.13",
          "form": "오 감사합니다 컴퓨터 게임은",
          "original_form": "오 감사합니다 컴퓨터 게임은",
          "speaker_id": "1",
          "time": "20210823 21:45"
        },
        {
          "id": "MDRW2100010511.1.14",
          "form": "뭐 하셨어요??",
          "original_form": "뭐 하셨어요??",
          "speaker_id": "1",
          "time": "20210823 21:45"
        },
        {
          "id": "MDRW2100010511.1.15",
          "form": "예전에 옵치 조금하고 카트도 좀 했어요 ㅎㅎ",
          "original_form": "예전에 옵치 조금하고 카트도 좀 했어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 21:48"
        },
        {
          "id": "MDRW2100010511.1.16",
          "form": "최근 카트에 롯데월드 맵이 나왔다고 해서 해봤어요 ㅎㅎ",
          "original_form": "최근 카트에 롯데월드 맵이 나왔다고 해서 해봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 21:48"
        },
        {
          "id": "MDRW2100010511.1.17",
          "form": "롯데월드 맵 재밌을 거 같아요ㅋㅋㅋㅋㅋ",
          "original_form": "롯데월드 맵 재밌을 거 같아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:09"
        },
        {
          "id": "MDRW2100010511.1.18",
          "form": "정말 롯데월드랑 비슷한 그런 느낌이에요?",
          "original_form": "정말 롯데월드랑 비슷한 그런 느낌이에요?",
          "speaker_id": "1",
          "time": "20210823 22:09"
        },
        {
          "id": "MDRW2100010511.1.19",
          "form": "완전 똑같은 구조는 아닌데 아 이거! 하고 알아볼 수 있는 것들이 있어요 ㅋㅋㅋㅋㅋ",
          "original_form": "완전 똑같은 구조는 아닌데 아 이거! 하고 알아볼 수 있는 것들이 있어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:03"
        },
        {
          "id": "MDRW2100010511.1.20",
          "form": "서울맵 하면 청계천도 구경할 수 있답니당",
          "original_form": "서울맵 하면 청계천도 구경할 수 있답니당",
          "speaker_id": "2",
          "time": "20210823 23:03"
        },
        {
          "id": "MDRW2100010511.1.21",
          "form": "우와ㅋㅋㅋㅋㅋㄴㄴ 정말 좋네요",
          "original_form": "우와ㅋㅋㅋㅋㅋㄴㄴ 정말 좋네요",
          "speaker_id": "1",
          "time": "20210823 23:19"
        },
        {
          "id": "MDRW2100010511.1.22",
          "form": "이 시국에 너무 답답하시면 드라이브... 한번 다녀오세요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이 시국에 너무 답답하시면 드라이브... 한번 다녀오세요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:27"
        },
        {
          "id": "MDRW2100010511.1.23",
          "form": "그래야겠네요 허허",
          "original_form": "그래야겠네요 허허",
          "speaker_id": "1",
          "time": "20210823 23:40"
        }
      ]
    }
