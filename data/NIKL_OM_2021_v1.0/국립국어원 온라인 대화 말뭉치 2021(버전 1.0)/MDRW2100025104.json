          "speaker_id": "2",
          "time": "20210831 23:10"
        },
        {
          "id": "MDRW2100025104.1.3",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100025104.1.4",
          "form": "오 며칠돼셨어요",
          "original_form": "오 며칠돼셨어요",
          "speaker_id": "2",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100025104.1.5",
          "form": "되셨어요",
          "original_form": "되셨어요",
          "speaker_id": "2",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100025104.1.6",
          "form": "2년이여",
          "original_form": "2년이여",
          "speaker_id": "1",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100025104.1.7",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "1",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100025104.1.8",
          "form": "전 5년이요",
          "original_form": "전 5년이요",
          "speaker_id": "2",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100025104.1.9",
          "form": "중학생때 부터 사귀었어요",
          "original_form": "중학생때 부터 사귀었어요",
          "speaker_id": "2",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100025104.1.10",
          "form": "님은 언제부터 사귀셨어요 ?",
          "original_form": "님은 언제부터 사귀셨어요 ?",
          "speaker_id": "2",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100025104.1.11",
          "form": "중2때부터요?",
          "original_form": "중2때부터요?",
          "speaker_id": "1",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100025104.1.12",
          "form": "전 대학교 다니면서 만낫어요",
          "original_form": "전 대학교 다니면서 만낫어요",
          "speaker_id": "1",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100025104.1.13",
          "form": "애인분의 어디가 좋으신가요?",
          "original_form": "애인분의 어디가 좋으신가요?",
          "speaker_id": "1",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100025104.1.14",
          "form": "콕 집기 얼럽네요",
          "original_form": "콕 집기 얼럽네요",
          "speaker_id": "2",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100025104.1.15",
          "form": "목소리요",
          "original_form": "목소리요",
          "speaker_id": "2",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100025104.1.16",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "2",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100025104.1.17",
          "form": "목소리ㅋ",
          "original_form": "목소리ㅋ",
          "speaker_id": "1",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100025104.1.18",
          "form": "저는 모든거요",
          "original_form": "저는 모든거요",
          "speaker_id": "1",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100025104.1.19",
          "form": "콩깍지가 제대로 씌셨네요",
          "original_form": "콩깍지가 제대로 씌셨네요",
          "speaker_id": "2",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100025104.1.20",
          "form": "대학교에서 만나셨으면 cc인가요 ?",
          "original_form": "대학교에서 만나셨으면 cc인가요 ?",
          "speaker_id": "2",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100025104.1.21",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210831 23:17"
        },
        {
          "id": "MDRW2100025104.1.22",
          "form": "오 안깨지고 잘 사귀셨나요 ?",
          "original_form": "오 안깨지고 잘 사귀셨나요 ?",
          "speaker_id": "2",
          "time": "20210831 23:18"
        },
        {
          "id": "MDRW2100025104.1.23",
          "form": "cc힘드실텐데",
          "original_form": "cc힘드실텐데",
          "speaker_id": "2",
          "time": "20210831 23:18"
        },
        {
          "id": "MDRW2100025104.1.24",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210831 23:20"
        },
        {
          "id": "MDRW2100025104.1.25",
          "form": "지금도 사귀고 잇죠 하하",
          "original_form": "지금도 사귀고 잇죠 하하",
          "speaker_id": "1",
          "time": "20210831 23:20"
        },
        {
          "id": "MDRW2100025104.1.26",
          "form": "올",
          "original_form": "올",
          "speaker_id": "2",
          "time": "20210831 23:20"
        },
        {
          "id": "MDRW2100025104.1.27",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "2",
          "time": "20210831 23:20"
        },
        {
          "id": "MDRW2100025104.1.28",
          "form": "결혼 생각도 있으세요?",
          "original_form": "결혼 생각도 있으세요?",
