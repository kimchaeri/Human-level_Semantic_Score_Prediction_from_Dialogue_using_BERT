      "utterance": [
        {
          "id": "MDRW2100036191.1.1",
          "form": "하 어려운데여..ㅠ",
          "original_form": "하 어려운데여..ㅠ",
          "speaker_id": "2",
          "time": "20210830 18:38"
        },
        {
          "id": "MDRW2100036191.1.2",
          "form": "매운맛 하겠습니다",
          "original_form": "매운맛 하겠습니다",
          "speaker_id": "2",
          "time": "20210830 18:38"
        },
        {
          "id": "MDRW2100036191.1.3",
          "form": "그것이 한국인이니까",
          "original_form": "그것이 한국인이니까",
          "speaker_id": "2",
          "time": "20210830 18:38"
        },
        {
          "id": "MDRW2100036191.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 18:38"
        },
        {
          "id": "MDRW2100036191.1.5",
          "form": "저도 매운맛이요!",
          "original_form": "저도 매운맛이요!",
          "speaker_id": "1",
          "time": "20210830 18:39"
        },
        {
          "id": "MDRW2100036191.1.6",
          "form": "하지만 마음속 한구석엔 순한맛이 있습니다",
          "original_form": "하지만 마음속 한구석엔 순한맛이 있습니다",
          "speaker_id": "1",
          "time": "20210830 18:39"
        },
        {
          "id": "MDRW2100036191.1.7",
          "form": "그죠 마음 한쪽은 순정이죠",
          "original_form": "그죠 마음 한쪽은 순정이죠",
          "speaker_id": "2",
          "time": "20210830 18:40"
        },
        {
          "id": "MDRW2100036191.1.8",
          "form": "계란 넣는것도 좋아하시나요",
          "original_form": "계란 넣는것도 좋아하시나요",
          "speaker_id": "2",
          "time": "20210830 18:40"
        },
        {
          "id": "MDRW2100036191.1.9",
          "form": "순한맛에 연두 청양(빨간연두)랑 고춧가루 넣어먹어도 맛있더라고요",
          "original_form": "순한맛에 연두 청양(빨간연두)랑 고춧가루 넣어먹어도 맛있더라고요",
          "speaker_id": "1",
          "time": "20210830 18:40"
        },
        {
          "id": "MDRW2100036191.1.10",
          "form": "최애라면이 진라면이신가요!",
          "original_form": "최애라면이 진라면이신가요!",
          "speaker_id": "1",
          "time": "20210830 18:40"
        },
        {
          "id": "MDRW2100036191.1.11",
          "form": "오모 네 진라면 그담 육개장 작은컵 입니다ㅋㅋ",
          "original_form": "오모 네 진라면 그담 육개장 작은컵 입니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:41"
        },
        {
          "id": "MDRW2100036191.1.12",
          "form": "오모하시니까 오모가리 김치찌개도 땡기네요",
          "original_form": "오모하시니까 오모가리 김치찌개도 땡기네요",
          "speaker_id": "1",
          "time": "20210830 18:41"
        },
        {
          "id": "MDRW2100036191.1.13",
          "form": "오 고전적인 라인업이군요!",
          "original_form": "오 고전적인 라인업이군요!",
          "speaker_id": "1",
          "time": "20210830 18:41"
        },
        {
          "id": "MDRW2100036191.1.14",
          "form": "그거 맛있죠",
          "original_form": "그거 맛있죠",
          "speaker_id": "2",
          "time": "20210830 18:41"
        },
        {
          "id": "MDRW2100036191.1.15",
          "form": "전 신라면파였다가 한 5년전부터 진라면 매운맛 그다음은 너구리 매운맛입니다",
          "original_form": "전 신라면파였다가 한 5년전부터 진라면 매운맛 그다음은 너구리 매운맛입니다",
          "speaker_id": "1",
          "time": "20210830 18:41"
        },
        {
          "id": "MDRW2100036191.1.16",
          "form": "오동통한 면발.. 좋아요",
          "original_form": "오동통한 면발.. 좋아요",
          "speaker_id": "1",
          "time": "20210830 18:41"
        },
        {
          "id": "MDRW2100036191.1.17",
          "form": "김치 패이스트 넣을때 조심히 넣어야만.. 손에 꼭 묻어요ㅋㅋ",
          "original_form": "김치 패이스트 넣을때 조심히 넣어야만.. 손에 꼭 묻어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:42"
        },
        {
          "id": "MDRW2100036191.1.18",
          "form": "ㅋㅋㅋㅋㅋ맞아요 그 묻은거 자극적이고 맛있지 않나요",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 그 묻은거 자극적이고 맛있지 않나요",
          "speaker_id": "1",
          "time": "20210830 18:42"
        },
        {
          "id": "MDRW2100036191.1.19",
          "form": "농축된 엠에스지",
          "original_form": "농축된 엠에스지",
          "speaker_id": "1",
          "time": "20210830 18:42"
        },
        {
          "id": "MDRW2100036191.1.20",
          "form": "너구리도 가끔 먹으면 넘 맛있어요 쫄깃쫄깃",
          "original_form": "너구리도 가끔 먹으면 넘 맛있어요 쫄깃쫄깃",
          "speaker_id": "2",
          "time": "20210830 18:42"
        },
        {
          "id": "MDRW2100036191.1.21",
          "form": "ㅋㅋ 네 그거 넣어야 기름진 김치찌개가 완성돼요",
          "original_form": "ㅋㅋ 네 그거 넣어야 기름진 김치찌개가 완성돼요",
          "speaker_id": "2",
          "time": "20210830 18:43"
        },
        {
          "id": "MDRW2100036191.1.22",
          "form": "기름진 김치찌개 넘좋아요",
          "original_form": "기름진 김치찌개 넘좋아요",
          "speaker_id": "1",
          "time": "20210830 18:44"
        },
        {
          "id": "MDRW2100036191.1.23",
          "form": "돼지고기 김치찌개 끓일때 라드 넣으면 맛있대요 돼지기름씌",
          "original_form": "돼지고기 김치찌개 끓일때 라드 넣으면 맛있대요 돼지기름씌",
          "speaker_id": "1",
          "time": "20210830 18:45"
        },
        {
          "id": "MDRW2100036191.1.24",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100036191.1.25",
          "form": "혈관이 비명지르는 존맛레시피..",
          "original_form": "혈관이 비명지르는 존맛레시피..",
          "speaker_id": "2",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100036191.1.26",
          "form": "그 양푼이에 나오는 김치찌개느낌이네요ㅋㅋ!",
          "original_form": "그 양푼이에 나오는 김치찌개느낌이네요ㅋㅋ!",
          "speaker_id": "2",
          "time": "20210830 18:47"
        },
        {
          "id": "MDRW2100036191.1.27",
          "form": "그츄 한스푼 넣기",
          "original_form": "그츄 한스푼 넣기",
          "speaker_id": "1",
          "time": "20210830 18:47"
        }
      ]
    }
