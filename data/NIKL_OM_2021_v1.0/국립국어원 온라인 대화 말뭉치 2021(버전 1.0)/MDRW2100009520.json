      "utterance": [
        {
          "id": "MDRW2100009520.1.1",
          "form": "와 최악이다ㅜ",
          "original_form": "와 최악이다ㅜ",
          "speaker_id": "2",
          "time": "20210823 00:10"
        },
        {
          "id": "MDRW2100009520.1.2",
