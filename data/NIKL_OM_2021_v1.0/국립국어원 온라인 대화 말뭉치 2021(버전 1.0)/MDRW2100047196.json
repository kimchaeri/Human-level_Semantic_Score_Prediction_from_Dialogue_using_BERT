      "utterance": [
        {
          "id": "MDRW2100047196.1.1",
          "form": "마우스와 키보드까지 갖추면 좋지만",
          "original_form": "마우스와 키보드까지 갖추면 좋지만",
          "speaker_id": "1",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100047196.1.2",
          "form": "하나씩 사모아야죵  뭐",
          "original_form": "하나씩 사모아야죵  뭐",
          "speaker_id": "1",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100047196.1.3",
          "form": "그춐ㅋㅋㅋㅋㅋ",
          "original_form": "그춐ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100047196.1.4",
          "form": "그래서 당근으로 싸게 구하는거죠",
          "original_form": "그래서 당근으로 싸게 구하는거죠",
          "speaker_id": "1",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100047196.1.5",
          "form": "당근거래 근데 뭔가",
          "original_form": "당근거래 근데 뭔가",
          "speaker_id": "2",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100047196.1.6",
          "form": "쑥스럽고 무섭고(???)",
          "original_form": "쑥스럽고 무섭고(???)",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100047196.1.7",
          "form": "아..아니에요",
          "original_form": "아..아니에요",
          "speaker_id": "1",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100047196.1.8",
          "form": "그런게 있어섴ㅋㅋㅋㅋㅋ큐ㅠ",
          "original_form": "그런게 있어섴ㅋㅋㅋㅋㅋ큐ㅠ",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100047196.1.9",
          "form": "목적이 뚜렷하기때문에..",
          "original_form": "목적이 뚜렷하기때문에..",
          "speaker_id": "1",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100047196.1.10",
          "form": "딱히 그렇진않아요 자기 볼일보고 갈길가면돼요ㅋㅋ",
          "original_form": "딱히 그렇진않아요 자기 볼일보고 갈길가면돼요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100047196.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ몇 번 하다보면 익숙해지겠죠 ㅠㅠ??",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ몇 번 하다보면 익숙해지겠죠 ㅠㅠ??",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100047196.1.12",
          "form": "봐서 살만한거 생기면 시도해봐야겠네요",
          "original_form": "봐서 살만한거 생기면 시도해봐야겠네요",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100047196.1.13",
          "form": "버티컬마우스가 삼마넌인데 삼처넌에 구했답니다",
          "original_form": "버티컬마우스가 삼마넌인데 삼처넌에 구했답니다",
          "speaker_id": "1",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100047196.1.14",
          "form": "와 진짜 싸게 사셨네요",
          "original_form": "와 진짜 싸게 사셨네요",
          "speaker_id": "2",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100047196.1.15",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100047196.1.16",
          "form": "보통 오처넌정도에 올라와요 칠처넌",
          "original_form": "보통 오처넌정도에 올라와요 칠처넌",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100047196.1.17",
          "form": "그것도 싸네요",
          "original_form": "그것도 싸네요",
          "speaker_id": "2",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100047196.1.18",
          "form": "다들 자기 사정에 처분하는거라 싼편이에요",
          "original_form": "다들 자기 사정에 처분하는거라 싼편이에요",
          "speaker_id": "1",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100047196.1.19",
          "form": "한 번 진짜 찾아봐야겠어요",
          "original_form": "한 번 진짜 찾아봐야겠어요",
          "speaker_id": "2",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100047196.1.20",
          "form": "귀찮음을...무릅쓰고...",
          "original_form": "귀찮음을...무릅쓰고...",
          "speaker_id": "2",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100047196.1.21",
          "form": "키보드도 아마 잘구하실수있을거에요",
          "original_form": "키보드도 아마 잘구하실수있을거에요",
          "speaker_id": "1",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100047196.1.22",
          "form": "꿀팁 감사합니다 ㅋㅋㅋㅋㅋ!!",
          "original_form": "꿀팁 감사합니다 ㅋㅋㅋㅋㅋ!!",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100047196.1.23",
          "form": "4단계라서 그런걸까",
          "original_form": "4단계라서 그런걸까",
          "speaker_id": "1",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100047196.1.24",
          "form": "서점과 도서관이 참 아쉽네요",
          "original_form": "서점과 도서관이 참 아쉽네요",
