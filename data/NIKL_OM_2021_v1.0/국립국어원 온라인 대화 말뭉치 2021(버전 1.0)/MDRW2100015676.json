      "utterance": [
        {
          "id": "MDRW2100015676.1.1",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "1",
          "time": "20210826 00:12"
        },
        {
          "id": "MDRW2100015676.1.2",
          "form": "아니요 학생이신가요?",
          "original_form": "아니요 학생이신가요?",
          "speaker_id": "2",
          "time": "20210826 07:52"
        },
        {
          "id": "MDRW2100015676.1.3",
          "form": "네! 전 학생이에요",
          "original_form": "네! 전 학생이에요",
          "speaker_id": "1",
          "time": "20210826 11:57"
        },
        {
          "id": "MDRW2100015676.1.4",
          "form": "직장인이신가요?",
          "original_form": "직장인이신가요?",
          "speaker_id": "1",
          "time": "20210826 11:57"
        },
        {
          "id": "MDRW2100015676.1.5",
          "form": "이직준비중이예요ㅠ",
          "original_form": "이직준비중이예요ㅠ",
          "speaker_id": "2",
          "time": "20210826 11:58"
        },
        {
          "id": "MDRW2100015676.1.6",
          "form": "코로나로 상황이 안좋아서요",
          "original_form": "코로나로 상황이 안좋아서요",
          "speaker_id": "2",
          "time": "20210826 11:58"
        },
        {
          "id": "MDRW2100015676.1.7",
          "form": "졸업반이세요?",
          "original_form": "졸업반이세요?",
          "speaker_id": "2",
          "time": "20210826 11:59"
        },
        {
          "id": "MDRW2100015676.1.8",
          "form": "아뇨ㅎㅎ",
          "original_form": "아뇨ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 12:02"
        },
        {
          "id": "MDRW2100015676.1.9",
          "form": "그러시구나 그럼 아직 구직활동은 먼이야기시겠다",
          "original_form": "그러시구나 그럼 아직 구직활동은 먼이야기시겠다",
          "speaker_id": "2",
          "time": "20210826 12:02"
        },
        {
          "id": "MDRW2100015676.1.10",
          "form": "앗 멀지만 멀지도 않은ㅋㅋㅋㅋㅋ...이야기쥬",
          "original_form": "앗 멀지만 멀지도 않은ㅋㅋㅋㅋㅋ...이야기쥬",
          "speaker_id": "1",
          "time": "20210826 12:03"
        },
        {
          "id": "MDRW2100015676.1.11",
          "form": "학교 선배림으로써 학교 생활 꿀팁 말해주실 수 있나오?",
          "original_form": "학교 선배림으로써 학교 생활 꿀팁 말해주실 수 있나오?",
          "speaker_id": "1",
          "time": "20210826 12:03"
        },
        {
          "id": "MDRW2100015676.1.12",
          "form": "음 ...각자 학교생활이 달라서 모르겠지만요",
          "original_form": "음 ...각자 학교생활이 달라서 모르겠지만요",
          "speaker_id": "2",
          "time": "20210826 12:03"
        },
        {
          "id": "MDRW2100015676.1.13",
          "form": "진부한 이야기로는 학점챙기기 스펙쌓기 챙겨야하는게 있구요",
          "original_form": "진부한 이야기로는 학점챙기기 스펙쌓기 챙겨야하는게 있구요",
          "speaker_id": "2",
          "time": "20210826 12:04"
        },
        {
          "id": "MDRW2100015676.1.14",
          "form": "졸업반이되면 거의 혼강하는경우가 많아서 친구관계에 너무 스트레스받지마시란 소리 해드리고싶어요",
          "original_form": "졸업반이되면 거의 혼강하는경우가 많아서 친구관계에 너무 스트레스받지마시란 소리 해드리고싶어요",
          "speaker_id": "2",
          "time": "20210826 12:04"
        },
        {
          "id": "MDRW2100015676.1.15",
          "form": "흗흑 이제 거의다 혼강이라 그건 크게 걱정 안해도 될 것같아요",
          "original_form": "흗흑 이제 거의다 혼강이라 그건 크게 걱정 안해도 될 것같아요",
          "speaker_id": "1",
          "time": "20210826 12:05"
        },
        {
          "id": "MDRW2100015676.1.16",
          "form": "그렇군요ㅠㅠ 첫연봉신경쓰셔야한다는 이야기도 드려야겠네요ㅜ",
          "original_form": "그렇군요ㅠㅠ 첫연봉신경쓰셔야한다는 이야기도 드려야겠네요ㅜ",
          "speaker_id": "2",
          "time": "20210826 12:06"
        },
        {
          "id": "MDRW2100015676.1.17",
          "form": "헉 왜요??",
          "original_form": "헉 왜요??",
          "speaker_id": "1",
          "time": "20210826 12:07"
        },
        {
          "id": "MDRW2100015676.1.18",
          "form": "연봉 인상률도 첫연봉이 다르면 액수가 다르구요",
          "original_form": "연봉 인상률도 첫연봉이 다르면 액수가 다르구요",
          "speaker_id": "2",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100015676.1.19",
          "form": "첫연봉을 얼마받았는지를 보고 그것을 기준으로 다음연봉이 정해지기 쉬워서요 이직을하더라도요",
          "original_form": "첫연봉을 얼마받았는지를 보고 그것을 기준으로 다음연봉이 정해지기 쉬워서요 이직을하더라도요",
          "speaker_id": "2",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100015676.1.20",
          "form": "아하...이건 처음 듣는 얘기에요 유익하네용",
          "original_form": "아하...이건 처음 듣는 얘기에요 유익하네용",
          "speaker_id": "1",
          "time": "20210826 12:11"
        }
      ]
    }
