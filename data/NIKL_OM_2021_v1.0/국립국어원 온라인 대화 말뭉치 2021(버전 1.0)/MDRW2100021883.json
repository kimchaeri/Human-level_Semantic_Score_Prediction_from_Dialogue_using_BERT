      "utterance": [
        {
          "id": "MDRW2100021883.1.1",
          "form": "아니 얘네 진짜",
          "original_form": "아니 얘네 진짜",
          "speaker_id": "2",
          "time": "20210828 23:28"
        },
        {
          "id": "MDRW2100021883.1.2",
          "form": "이 겜이 이렇게 설정을 꼼꼼히 짰다고....?",
          "original_form": "이 겜이 이렇게 설정을 꼼꼼히 짰다고....?",
          "speaker_id": "1",
          "time": "20210828 23:28"
        },
        {
          "id": "MDRW2100021883.1.3",
          "form": "대사만 안치면",
          "original_form": "대사만 안치면",
          "speaker_id": "2",
          "time": "20210828 23:28"
        },
        {
          "id": "MDRW2100021883.1.4",
          "form": "하는 상태가 되어버림",
          "original_form": "하는 상태가 되어버림",
          "speaker_id": "1",
          "time": "20210828 23:28"
        },
        {
          "id": "MDRW2100021883.1.5",
          "form": "참 좋을텐데...",
          "original_form": "참 좋을텐데...",
          "speaker_id": "2",
          "time": "20210828 23:29"
        },
        {
          "id": "MDRW2100021883.1.6",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210828 23:29"
        },
        {
          "id": "MDRW2100021883.1.7",
          "form": "그리고 이번 버전은",
          "original_form": "그리고 이번 버전은",
          "speaker_id": "1",
          "time": "20210828 23:29"
        },
        {
          "id": "MDRW2100021883.1.8",
          "form": "번역이 특히 더 구림",
          "original_form": "번역이 특히 더 구림",
          "speaker_id": "1",
          "time": "20210828 23:29"
        },
        {
          "id": "MDRW2100021883.1.9",
          "form": "번역가 누구냐 대채",
          "original_form": "번역가 누구냐 대채",
          "speaker_id": "1",
          "time": "20210828 23:29"
        },
        {
          "id": "MDRW2100021883.1.10",
          "form": "ㅇㅇ막",
          "original_form": "ㅇㅇ막",
          "speaker_id": "2",
          "time": "20210828 23:29"
        },
        {
          "id": "MDRW2100021883.1.11",
          "form": "갑자기 존대하고",
          "original_form": "갑자기 존대하고",
          "speaker_id": "2",
          "time": "20210828 23:29"
        },
        {
          "id": "MDRW2100021883.1.12",
          "form": "갑자기 반말하고",
          "original_form": "갑자기 반말하고",
          "speaker_id": "2",
          "time": "20210828 23:29"
        },
        {
          "id": "MDRW2100021883.1.13",
          "form": "난ㅇ리가 아니더라",
          "original_form": "난ㅇ리가 아니더라",
          "speaker_id": "2",
          "time": "20210828 23:29"
        },
        {
          "id": "MDRW2100021883.1.14",
          "form": "그치 ㅋㅋㅋㅋㅋ",
          "original_form": "그치 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:30"
        },
        {
          "id": "MDRW2100021883.1.15",
          "form": "그리고",
          "original_form": "그리고",
          "speaker_id": "1",
          "time": "20210828 23:30"
        },
        {
          "id": "MDRW2100021883.1.16",
          "form": "얘네가 떡밥을 자잘하게 다 뿌려놓는데",
          "original_form": "얘네가 떡밥을 자잘하게 다 뿌려놓는데",
          "speaker_id": "1",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100021883.1.17",
          "form": "번역 이상하게 됨",
          "original_form": "번역 이상하게 됨",
          "speaker_id": "1",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100021883.1.18",
          "form": "예를 들면",
          "original_form": "예를 들면",
          "speaker_id": "1",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100021883.1.19",
          "form": "ㅇㅇ....원어 해석해둔",
          "original_form": "ㅇㅇ....원어 해석해둔",
          "speaker_id": "2",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100021883.1.20",
          "form": "커뮤니티를 찾아봐야되잖아",
          "original_form": "커뮤니티를 찾아봐야되잖아",
          "speaker_id": "2",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100021883.1.21",
          "form": "게임에서 몬드지역 별이 떨어지는 절벽에 가면",
          "original_form": "게임에서 몬드지역 별이 떨어지는 절벽에 가면",
          "speaker_id": "1",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100021883.1.22",
          "form": "봉화탑 근처에 수녀복장을 한 사람이 서성이는거",
          "original_form": "봉화탑 근처에 수녀복장을 한 사람이 서성이는거",
          "speaker_id": "1",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100021883.1.23",
          "form": "본적 있지?",
          "original_form": "본적 있지?",
          "speaker_id": "1",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100021883.1.24",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210828 23:32"
        },
        {
          "id": "MDRW2100021883.1.25",
          "form": "한국 서버에는 폭풍을 수호하는 미녀 ㅋ로 번역되어있는데",
          "original_form": "한국 서버에는 폭풍을 수호하는 미녀 ㅋ로 번역되어있는데",
          "speaker_id": "1",
          "time": "20210828 23:32"
        },
        {
          "id": "MDRW2100021883.1.26",
          "form": "ㅎ",
          "original_form": "ㅎ",
          "speaker_id": "2",
          "time": "20210828 23:32"
        },
        {
          "id": "MDRW2100021883.1.27",
          "form": "근데 성유물 스토리",
          "original_form": "근데 성유물 스토리",
          "speaker_id": "2",
          "time": "20210828 23:34"
        },
        {
          "id": "MDRW2100021883.1.28",
          "form": "하나씩 훑어보고있는데",
          "original_form": "하나씩 훑어보고있는데",
          "speaker_id": "2",
          "time": "20210828 23:34"
        },
        {
          "id": "MDRW2100021883.1.29",
          "form": "전반적으로 옛날얘기긴 하네",
          "original_form": "전반적으로 옛날얘기긴 하네",
          "speaker_id": "2",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100021883.1.30",
          "form": "걍 심연사도랑 시뇨라가",
          "original_form": "걍 심연사도랑 시뇨라가",
          "speaker_id": "2",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100021883.1.31",
          "form": "겁나 옛날사람이라 있나봐",
          "original_form": "겁나 옛날사람이라 있나봐",
          "speaker_id": "2",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100021883.1.32",
          "form": "찐 오역이고 원제는 폭풍으로부터 수호하는 소녀임",
          "original_form": "찐 오역이고 원제는 폭풍으로부터 수호하는 소녀임",
          "speaker_id": "1",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100021883.1.33",
          "form": "ㅇㅇ 그렇긴 해",
          "original_form": "ㅇㅇ 그렇긴 해",
          "speaker_id": "1",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100021883.1.34",
          "form": "ㅎ",
          "original_form": "ㅎ",
          "speaker_id": "2",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100021883.1.35",
          "form": "그리고 그 npc얘기 들어보면",
          "original_form": "그리고 그 npc얘기 들어보면",
          "speaker_id": "1",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100021883.1.36",
          "form": "몬드에 폭풍이 불어오는지",
          "original_form": "몬드에 폭풍이 불어오는지",
          "speaker_id": "1",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100021883.1.37",
          "form": "대대로 감시하고 있다고 하는데",
          "original_form": "대대로 감시하고 있다고 하는데",
          "speaker_id": "1",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100021883.1.38",
          "form": "소용돌이 마신이",
          "original_form": "소용돌이 마신이",
          "speaker_id": "2",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100021883.1.39",
          "form": "그게 벤티가 쓸어버린 폭풍의 마신이라고 하기엔",
          "original_form": "그게 벤티가 쓸어버린 폭풍의 마신이라고 하기엔",
          "speaker_id": "1",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100021883.1.40",
          "form": "깨어나는지 보는건가?",
          "original_form": "깨어나는지 보는건가?",
          "speaker_id": "2",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100021883.1.41",
          "form": "너무 먼 옛날임",
          "original_form": "너무 먼 옛날임",
          "speaker_id": "1",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100021883.1.42",
          "form": "그 신은 3천년 전이라",
          "original_form": "그 신은 3천년 전이라",
          "speaker_id": "1",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100021883.1.43",
          "form": "몬드가 세워지기 전에 이미 난리가 끝났을거란말야?",
          "original_form": "몬드가 세워지기 전에 이미 난리가 끝났을거란말야?",
          "speaker_id": "1",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100021883.1.44",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100021883.1.45",
          "form": "하긴 몬드 귀족 역사같은거 보면",
          "original_form": "하긴 몬드 귀족 역사같은거 보면",
          "speaker_id": "2",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100021883.1.46",
          "form": "그래서 그 폭풍은",
          "original_form": "그래서 그 폭풍은",
          "speaker_id": "1",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100021883.1.47",
          "form": "생각보다 최근이더라",
          "original_form": "생각보다 최근이더라",
          "speaker_id": "2",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100021883.1.48",
          "form": "데카라비안이 아니라 켄리아의 멸망이라고",
          "original_form": "데카라비안이 아니라 켄리아의 멸망이라고",
          "speaker_id": "1",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100021883.1.49",
          "form": "많이 추론함",
          "original_form": "많이 추론함",
          "speaker_id": "1",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100021883.1.50",
          "form": "켄리아가 멸망하면서",
          "original_form": "켄리아가 멸망하면서",
          "speaker_id": "1",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100021883.1.51",
          "form": "아 설마",
          "original_form": "아 설마",
          "speaker_id": "2",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100021883.1.52",
          "form": "7신이 모여서",
          "original_form": "7신이 모여서",
          "speaker_id": "2",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100021883.1.53",
          "form": "그 안에 있던 마수들이 대륙으로 퍼져나갔다는 기록이 있어서",
          "original_form": "그 안에 있던 마수들이 대륙으로 퍼져나갔다는 기록이 있어서",
          "speaker_id": "1",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100021883.1.54",
          "form": "나라 하나 조지는거?",
          "original_form": "나라 하나 조지는거?",
          "speaker_id": "2",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100021883.1.55",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100021883.1.56",
          "form": "그걸 경계하는건가?",
          "original_form": "그걸 경계하는건가?",
          "speaker_id": "2",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100021883.1.57",
          "form": "그 당시에 마수들이 몰려온걸 알리기 위해서",
          "original_form": "그 당시에 마수들이 몰려온걸 알리기 위해서",
          "speaker_id": "1",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100021883.1.58",
          "form": "저 npc의 조상이 봉화를 올렷고",
          "original_form": "저 npc의 조상이 봉화를 올렷고",
          "speaker_id": "1",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100021883.1.59",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100021883.1.60",
          "form": "이 시대 사람들은 켄리아의 멸망은 모르니까",
          "original_form": "이 시대 사람들은 켄리아의 멸망은 모르니까",
          "speaker_id": "1",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100021883.1.61",
          "form": "근데 난 그 봉화 위에 기어올라가서",
          "original_form": "근데 난 그 봉화 위에 기어올라가서",
          "speaker_id": "2",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100021883.1.62",
          "form": "그냥 그 현상만 기억한 채로",
          "original_form": "그냥 그 현상만 기억한 채로",
          "speaker_id": "1",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100021883.1.63",
          "form": "보물상자를 훔쳐왓지...",
          "original_form": "보물상자를 훔쳐왓지...",
          "speaker_id": "2",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100021883.1.64",
          "form": "대대로 물려져내려온거 아니냐고 하던...",
          "original_form": "대대로 물려져내려온거 아니냐고 하던...",
          "speaker_id": "1",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100021883.1.65",
          "form": "아 나 성유물 스토리 보다가",
          "original_form": "아 나 성유물 스토리 보다가",
          "speaker_id": "2",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100021883.1.66",
