      "utterance": [
        {
          "id": "MDRW2100023193.1.1",
          "form": "개강이 슬프고 나쁜일이긴 하지",
          "original_form": "개강이 슬프고 나쁜일이긴 하지",
          "speaker_id": "1",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100023193.1.2",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023193.1.3",
          "form": "정말 싫어..",
          "original_form": "정말 싫어..",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023193.1.4",
          "form": "어우 학교 가기 싫어",
          "original_form": "어우 학교 가기 싫어",
          "speaker_id": "1",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023193.1.5",
          "form": "근데 9월이 오는건 좋아",
          "original_form": "근데 9월이 오는건 좋아",
          "speaker_id": "1",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023193.1.6",
          "form": "동아리 사람들도 곧 만날 수 있고",
          "original_form": "동아리 사람들도 곧 만날 수 있고",
          "speaker_id": "1",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023193.1.7",
          "form": "아 맞아",
          "original_form": "아 맞아",
          "speaker_id": "2",
          "time": "20210830 00:07"
        },
        {
          "id": "MDRW2100023193.1.8",
          "form": "물론 방학때도 줄기차게 만났지만",
          "original_form": "물론 방학때도 줄기차게 만났지만",
          "speaker_id": "2",
          "time": "20210830 00:07"
        },
        {
          "id": "MDRW2100023193.1.9",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:07"
        },
        {
          "id": "MDRW2100023193.1.10",
          "form": "그래도 맨날 보는게 좋지",
          "original_form": "그래도 맨날 보는게 좋지",
          "speaker_id": "1",
          "time": "20210830 00:08"
        },
        {
          "id": "MDRW2100023193.1.11",
          "form": "동아리 사람들 보면 볼 수록 재밌자너",
          "original_form": "동아리 사람들 보면 볼 수록 재밌자너",
          "speaker_id": "1",
          "time": "20210830 00:09"
        },
        {
          "id": "MDRW2100023193.1.12",
          "form": "아 그치 우리 고딩때도 우리끼리 재밌었는데",
          "original_form": "아 그치 우리 고딩때도 우리끼리 재밌었는데",
          "speaker_id": "2",
          "time": "20210830 02:13"
        },
        {
          "id": "MDRW2100023193.1.13",
          "form": "대학 동아리는 선배들 덕분에 더 재밌어",
          "original_form": "대학 동아리는 선배들 덕분에 더 재밌어",
          "speaker_id": "2",
          "time": "20210830 02:13"
        },
        {
          "id": "MDRW2100023193.1.14",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 02:13"
        },
        {
          "id": "MDRW2100023193.1.15",
          "form": "맞아 대학 와서 유일하게 좋은점은 동아리 사람들을 만날 수 있다는거... 나머진 최악이야",
          "original_form": "맞아 대학 와서 유일하게 좋은점은 동아리 사람들을 만날 수 있다는거... 나머진 최악이야",
          "speaker_id": "1",
          "time": "20210830 14:33"
        },
        {
          "id": "MDRW2100023193.1.16",
          "form": "그치 나머지는 최악 교수님 저한테 왜이러세요...",
          "original_form": "그치 나머지는 최악 교수님 저한테 왜이러세요...",
          "speaker_id": "2",
          "time": "20210830 15:54"
        },
        {
          "id": "MDRW2100023193.1.17",
          "form": "나 최악인 일 추가...",
          "original_form": "나 최악인 일 추가...",
          "speaker_id": "1",
          "time": "20210830 16:35"
        },
        {
          "id": "MDRW2100023193.1.18",
          "form": "서포터즈 활동하는데 새로 뽑힌 사람이 전에 프로그램에서 만난 같은 중학교 나왔던 사람이야...",
          "original_form": "서포터즈 활동하는데 새로 뽑힌 사람이 전에 프로그램에서 만난 같은 중학교 나왔던 사람이야...",
          "speaker_id": "1",
          "time": "20210830 16:37"
        },
        {
          "id": "MDRW2100023193.1.19",
          "form": "???전어ㅣ 말한 그 사람?",
          "original_form": "???전어ㅣ 말한 그 사람?",
          "speaker_id": "2",
          "time": "20210830 16:39"
        },
        {
          "id": "MDRW2100023193.1.20",
          "form": "진짜 최악이네...",
          "original_form": "진짜 최악이네...",
          "speaker_id": "2",
          "time": "20210830 16:39"
        },
        {
          "id": "MDRW2100023193.1.21",
          "form": "웅...",
          "original_form": "웅...",
          "speaker_id": "1",
          "time": "20210830 16:39"
        },
        {
          "id": "MDRW2100023193.1.22",
          "form": "나 머리 잘라서 그쪽도 몰라봤는데",
          "original_form": "나 머리 잘라서 그쪽도 몰라봤는데",
          "speaker_id": "1",
          "time": "20210830 16:39"
        },
        {
          "id": "MDRW2100023193.1.23",
          "form": "선생님이 밀씀해주심...",
          "original_form": "선생님이 밀씀해주심...",
          "speaker_id": "1",
          "time": "20210830 16:40"
        },
        {
          "id": "MDRW2100023193.1.24",
          "form": "어쩌다 보니 영상도 같이 찍게 되었어...",
          "original_form": "어쩌다 보니 영상도 같이 찍게 되었어...",
          "speaker_id": "1",
          "time": "20210830 16:40"
        }
      ]
    }
