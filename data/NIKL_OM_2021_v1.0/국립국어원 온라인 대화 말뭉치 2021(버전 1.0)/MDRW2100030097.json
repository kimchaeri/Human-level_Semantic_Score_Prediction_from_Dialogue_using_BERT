        {
          "id": "MDRW2100030097.1.1",
          "form": "네 여행 좋아해요 ㅋㅋㅋ",
          "original_form": "네 여행 좋아해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100030097.1.2",
          "form": "코시국 전에 자주 다니셨나요?",
          "original_form": "코시국 전에 자주 다니셨나요?",
          "speaker_id": "2",
          "time": "20210903 21:31"
        },
        {
          "id": "MDRW2100030097.1.3",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100030097.1.4",
          "form": "주로 어디 다녀오셨어요??",
          "original_form": "주로 어디 다녀오셨어요??",
          "speaker_id": "2",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100030097.1.5",
          "form": "바다요",
          "original_form": "바다요",
          "speaker_id": "1",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100030097.1.6",
          "form": "편안한느낌요",
          "original_form": "편안한느낌요",
          "speaker_id": "1",
          "time": "20210904 08:36"
        },
        {
          "id": "MDRW2100030097.1.7",
          "form": "좋죠",
          "original_form": "좋죠",
          "speaker_id": "1",
          "time": "20210904 21:10"
        },
        {
          "id": "MDRW2100030097.1.8",
          "form": "수영도 하셨어요?",
          "original_form": "수영도 하셨어요?",
          "speaker_id": "2",
          "time": "20210904 21:18"
        },
        {
          "id": "MDRW2100030097.1.9",
          "form": "수영은 못해요",
          "original_form": "수영은 못해요",
          "speaker_id": "1",
          "time": "20210904 21:19"
        },
        {
          "id": "MDRW2100030097.1.10",
          "form": "그럼 바닷물에 발만 담그시는 정도인가요?",
          "original_form": "그럼 바닷물에 발만 담그시는 정도인가요?",
          "speaker_id": "2",
          "time": "20210904 21:28"
        },
        {
          "id": "MDRW2100030097.1.11",
          "form": "네 그정도",
          "original_form": "네 그정도",
          "speaker_id": "1",
          "time": "20210904 22:10"
        },
        {
          "id": "MDRW2100030097.1.12",
          "form": "갯벌에서 고둥 캐기는요?",
          "original_form": "갯벌에서 고둥 캐기는요?",
          "speaker_id": "2",
          "time": "20210904 22:57"
        },
        {
          "id": "MDRW2100030097.1.13",
          "form": "조개요",
          "original_form": "조개요",
          "speaker_id": "1",
          "time": "20210905 08:55"
        },
        {
          "id": "MDRW2100030097.1.14",
          "form": "조개 캐기도 좋아하세요?",
          "original_form": "조개 캐기도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 09:39"
        },
        {
          "id": "MDRW2100030097.1.15",
          "form": "애들때문에",
          "original_form": "애들때문에",
          "speaker_id": "1",
          "time": "20210905 09:41"
        },
        {
          "id": "MDRW2100030097.1.16",
          "form": "아 어릴 땐 채집하는 거 좋죠 ㅋㅋㅋ",
