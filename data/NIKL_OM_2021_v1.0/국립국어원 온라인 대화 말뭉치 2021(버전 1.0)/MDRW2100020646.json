      "utterance": [
        {
          "id": "MDRW2100020646.1.1",
          "form": "와 역시",
          "original_form": "와 역시",
          "speaker_id": "2",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100020646.1.2",
          "form": "사실 못만들어 먹잖아요 휘핑도 없고ㅋㅋㅋ",
          "original_form": "사실 못만들어 먹잖아요 휘핑도 없고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100020646.1.3",
          "form": "요즘 컵커피 잘 나오더라구요",
          "original_form": "요즘 컵커피 잘 나오더라구요",
          "speaker_id": "1",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100020646.1.4",
          "form": "편의점 근처에 살아야해요",
          "original_form": "편의점 근처에 살아야해요",
          "speaker_id": "2",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020646.1.5",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020646.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020646.1.7",
          "form": "인간은 편세권에 위치해있어야",
          "original_form": "인간은 편세권에 위치해있어야",
          "speaker_id": "1",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020646.1.8",
          "form": "멀면 너무 힘들어요",
          "original_form": "멀면 너무 힘들어요",
          "speaker_id": "1",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020646.1.9",
          "form": "진짜 편의점 앞 원룸 살 때가 제일 편했어요",
          "original_form": "진짜 편의점 앞 원룸 살 때가 제일 편했어요",
          "speaker_id": "2",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020646.1.10",
          "form": "편의점 가면 주로 뭐먹어요",
          "original_form": "편의점 가면 주로 뭐먹어요",
          "speaker_id": "1",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020646.1.11",
          "form": "저 때는 한창",
          "original_form": "저 때는 한창",
          "speaker_id": "2",
          "time": "20210828 10:46"
        },
        {
          "id": "MDRW2100020646.1.12",
          "form": "위대한 시리즈를",
          "original_form": "위대한 시리즈를",
          "speaker_id": "2",
          "time": "20210828 10:46"
        },
        {
          "id": "MDRW2100020646.1.13",
          "form": "먹었답니다",
          "original_form": "먹었답니다",
          "speaker_id": "2",
          "time": "20210828 10:46"
        },
        {
          "id": "MDRW2100020646.1.14",
          "form": "와 위대한 떡볶이",
          "original_form": "와 위대한 떡볶이",
          "speaker_id": "1",
          "time": "20210828 10:46"
        },
        {
          "id": "MDRW2100020646.1.15",
          "form": "이런거 드신건가",
          "original_form": "이런거 드신건가",
          "speaker_id": "1",
          "time": "20210828 10:46"
        },
        {
          "id": "MDRW2100020646.1.16",
          "form": "한창 그 시리즈가 나올 때",
          "original_form": "한창 그 시리즈가 나올 때",
          "speaker_id": "2",
          "time": "20210828 10:46"
        },
        {
          "id": "MDRW2100020646.1.17",
          "form": "자취를 했거든요",
          "original_form": "자취를 했거든요",
          "speaker_id": "2",
          "time": "20210828 10:46"
        },
        {
          "id": "MDRW2100020646.1.18",
          "form": "위대한 햄버거 이런,,",
          "original_form": "위대한 햄버거 이런,,",
          "speaker_id": "2",
          "time": "20210828 10:46"
        },
        {
          "id": "MDRW2100020646.1.19",
          "form": "떡볶이도 먹었어요",
          "original_form": "떡볶이도 먹었어요",
          "speaker_id": "2",
          "time": "20210828 10:46"
        },
        {
          "id": "MDRW2100020646.1.20",
          "form": "가성비 줄줄",
          "original_form": "가성비 줄줄",
          "speaker_id": "1",
          "time": "20210828 10:46"
        },
        {
          "id": "MDRW2100020646.1.21",
          "form": "버거 어땠어요",
          "original_form": "버거 어땠어요",
          "speaker_id": "1",
          "time": "20210828 10:46"
        },
        {
          "id": "MDRW2100020646.1.22",
          "form": "그거 아니면 샌드위치나... 만두나...",
          "original_form": "그거 아니면 샌드위치나... 만두나...",
          "speaker_id": "2",
          "time": "20210828 10:46"
        },
        {
          "id": "MDRW2100020646.1.23",
          "form": "약간 매점빵 같은 맛인가",
          "original_form": "약간 매점빵 같은 맛인가",
          "speaker_id": "1",
          "time": "20210828 10:47"
        },
        {
          "id": "MDRW2100020646.1.24",
          "form": "편의점을 생각하면 나쁘지 않았답니다",
          "original_form": "편의점을 생각하면 나쁘지 않았답니다",
          "speaker_id": "2",
          "time": "20210828 10:47"
        },
        {
          "id": "MDRW2100020646.1.25",
          "form": "예 딱",
          "original_form": "예 딱",
          "speaker_id": "2",
