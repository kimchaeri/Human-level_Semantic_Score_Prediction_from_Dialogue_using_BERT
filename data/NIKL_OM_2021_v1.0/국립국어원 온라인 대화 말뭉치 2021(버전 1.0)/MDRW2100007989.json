      "utterance": [
        {
          "id": "MDRW2100007989.1.1",
          "form": "최근 코로나라서 소개팅도 가뭄이라고 하더라고요",
          "original_form": "최근 코로나라서 소개팅도 가뭄이라고 하더라고요",
          "speaker_id": "2",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007989.1.2",
          "form": "아 그럴것 같긴하네요",
          "original_form": "아 그럴것 같긴하네요",
          "speaker_id": "1",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100007989.1.3",
          "form": "아무래도  사람 만나는데 제약도 많고",
          "original_form": "아무래도  사람 만나는데 제약도 많고",
          "speaker_id": "1",
          "time": "20210822 14:55"
        },
        {
          "id": "MDRW2100007989.1.4",
          "form": "서로 조심해야 할테니까요",
          "original_form": "서로 조심해야 할테니까요",
          "speaker_id": "1",
          "time": "20210822 14:55"
        },
        {
          "id": "MDRW2100007989.1.5",
          "form": "최근에 데이트한적 있으세요?",
          "original_form": "최근에 데이트한적 있으세요?",
          "speaker_id": "1",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100007989.1.6",
          "form": "음 아니요",
          "original_form": "음 아니요",
          "speaker_id": "2",
          "time": "20210822 14:57"
        },
        {
          "id": "MDRW2100007989.1.7",
          "form": "앱 깔아보는것? 정도의 열의만 보였네요 ㅋㅋ",
          "original_form": "앱 깔아보는것? 정도의 열의만 보였네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:57"
        },
        {
          "id": "MDRW2100007989.1.8",
          "form": "제 친구도 최근에 데이팅앱으로 여자분 한번 만난적이 있었는데",
          "original_form": "제 친구도 최근에 데이팅앱으로 여자분 한번 만난적이 있었는데",
          "speaker_id": "1",
          "time": "20210822 14:58"
        },
        {
          "id": "MDRW2100007989.1.9",
          "form": "누군가 만나고 싶다면 데이팅앱도 좋은것 같아요",
          "original_form": "누군가 만나고 싶다면 데이팅앱도 좋은것 같아요",
          "speaker_id": "1",
          "time": "20210822 14:59"
        },
        {
          "id": "MDRW2100007989.1.10",
          "form": "저는 결혼정보회사 가입한적 있었는데",
          "original_form": "저는 결혼정보회사 가입한적 있었는데",
          "speaker_id": "1",
          "time": "20210822 14:59"
        },
        {
          "id": "MDRW2100007989.1.11",
          "form": "그것도 나쁘지 않더라고요",
          "original_form": "그것도 나쁘지 않더라고요",
          "speaker_id": "1",
          "time": "20210822 14:59"
        },
        {
          "id": "MDRW2100007989.1.12",
          "form": "사람을 만날 수있는 폭을 넓힐 수 있어서 좋았어요",
          "original_form": "사람을 만날 수있는 폭을 넓힐 수 있어서 좋았어요",
          "speaker_id": "1",
          "time": "20210822 15:00"
        },
        {
          "id": "MDRW2100007989.1.13",
          "form": "음... 선호하는 데이트 있나요?",
          "original_form": "음... 선호하는 데이트 있나요?",
          "speaker_id": "1",
          "time": "20210822 15:00"
        },
        {
          "id": "MDRW2100007989.1.14",
          "form": "영화 밥 커피같은",
          "original_form": "영화 밥 커피같은",
          "speaker_id": "1",
          "time": "20210822 15:01"
        },
        {
          "id": "MDRW2100007989.1.15",
          "form": "대화가 잘 통하는 사람이면 좋을것같아요",
          "original_form": "대화가 잘 통하는 사람이면 좋을것같아요",
          "speaker_id": "2",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100007989.1.16",
          "form": "잘들어주고 본인이야기도 잘공유하는?",
          "original_form": "잘들어주고 본인이야기도 잘공유하는?",
          "speaker_id": "2",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100007989.1.17",
          "form": "아 그쵸 아무래도 이야기가 잘 통해야 좋죠",
          "original_form": "아 그쵸 아무래도 이야기가 잘 통해야 좋죠",
          "speaker_id": "1",
          "time": "20210822 16:02"
        },
        {
          "id": "MDRW2100007989.1.18",
          "form": "그러고보면 유독 대화할때 케미 좋은 사람들이랑 안맞는 사람들이 있는것 같아요",
          "original_form": "그러고보면 유독 대화할때 케미 좋은 사람들이랑 안맞는 사람들이 있는것 같아요",
          "speaker_id": "1",
          "time": "20210822 16:03"
        },
        {
          "id": "MDRW2100007989.1.19",
          "form": "그렇게 생각하지 않으세요",
          "original_form": "그렇게 생각하지 않으세요",
          "speaker_id": "1",
          "time": "20210822 16:03"
        },
        {
          "id": "MDRW2100007989.1.20",
          "form": "그런것같아요",
          "original_form": "그런것같아요",
          "speaker_id": "2",
          "time": "20210822 16:09"
        },
        {
          "id": "MDRW2100007989.1.21",
          "form": "시간가는줄모르고 대화할수있는 그런상대가 진짜좋을듯요",
          "original_form": "시간가는줄모르고 대화할수있는 그런상대가 진짜좋을듯요",
          "speaker_id": "2",
          "time": "20210822 16:10"
        },
        {
          "id": "MDRW2100007989.1.22",
          "form": "네 저도 그렇게 생각해요",
          "original_form": "네 저도 그렇게 생각해요",
          "speaker_id": "1",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100007989.1.23",
          "form": "결혼은 어떻게 생각하세요",
          "original_form": "결혼은 어떻게 생각하세요",
          "speaker_id": "1",
          "time": "20210822 16:12"
        },
        {
          "id": "MDRW2100007989.1.24",
          "form": "꼭 필요하다고 보시나요?",
          "original_form": "꼭 필요하다고 보시나요?",
          "speaker_id": "1",
          "time": "20210822 16:12"
        },
        {
          "id": "MDRW2100007989.1.25",
          "form": "결혼할만큼 좋은사람이 생긴다면 자연스럽게 하게되지않을까요오 ㅋㅋ",
          "original_form": "결혼할만큼 좋은사람이 생긴다면 자연스럽게 하게되지않을까요오 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:22"
        },
        {
          "id": "MDRW2100007989.1.26",
          "form": "요새 딩크족 많잖아요",
          "original_form": "요새 딩크족 많잖아요",
          "speaker_id": "1",
          "time": "20210822 16:23"
        },
        {
          "id": "MDRW2100007989.1.27",
          "form": "그런데 아이를 갖을 계획이 아니면 굳이 결혼을 해야할 필요가 있나 싶어요",
          "original_form": "그런데 아이를 갖을 계획이 아니면 굳이 결혼을 해야할 필요가 있나 싶어요",
          "speaker_id": "1",
          "time": "20210822 16:23"
        },
        {
          "id": "MDRW2100007989.1.28",
          "form": "그렇게 생각하면 좀슬퍼요",
          "original_form": "그렇게 생각하면 좀슬퍼요",
          "speaker_id": "2",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100007989.1.29",
          "form": "아플때 보호자가 되주거나",
          "original_form": "아플때 보호자가 되주거나",
          "speaker_id": "2",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100007989.1.30",
          "form": "큰일 생겼을때 상속문제라던가",
          "original_form": "큰일 생겼을때 상속문제라던가",
          "speaker_id": "2",
          "time": "20210822 16:31"
        },
        {
          "id": "MDRW2100007989.1.31",
          "form": "모두 가족일때만 할수있는거니까요",
          "original_form": "모두 가족일때만 할수있는거니까요",
          "speaker_id": "2",
          "time": "20210822 16:31"
        },
        {
          "id": "MDRW2100007989.1.32",
          "form": "그런가요",
          "original_form": "그런가요",
          "speaker_id": "1",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100007989.1.33",
          "form": "그렇게 생각할 수도 있긴하네요",
          "original_form": "그렇게 생각할 수도 있긴하네요",
          "speaker_id": "1",
          "time": "20210822 17:27"
        }
      ]
