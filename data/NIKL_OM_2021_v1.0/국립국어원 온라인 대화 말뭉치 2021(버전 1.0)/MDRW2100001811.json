      "utterance": [
        {
          "id": "MDRW2100001811.1.1",
          "form": "알바해보신 적 있으세요?",
          "original_form": "알바해보신 적 있으세요?",
          "speaker_id": "1",
          "time": "20210819 21:21"
        },
        {
          "id": "MDRW2100001811.1.2",
          "form": "네 전 하고 있어요",
          "original_form": "네 전 하고 있어요",
          "speaker_id": "2",
          "time": "20210819 21:21"
        },
        {
          "id": "MDRW2100001811.1.3",
          "form": "마트 알바",
          "original_form": "마트 알바",
          "speaker_id": "2",
          "time": "20210819 21:21"
        },
        {
          "id": "MDRW2100001811.1.4",
          "form": "상하차도 몇번 해봤구",
          "original_form": "상하차도 몇번 해봤구",
          "speaker_id": "2",
          "time": "20210819 21:21"
        },
        {
          "id": "MDRW2100001811.1.5",
          "form": "헉 상하차",
          "original_form": "헉 상하차",
          "speaker_id": "1",
          "time": "20210819 21:22"
        },
        {
          "id": "MDRW2100001811.1.6",
          "form": "엄청 힘들지 않아요?",
          "original_form": "엄청 힘들지 않아요?",
          "speaker_id": "1",
          "time": "20210819 21:22"
        },
        {
          "id": "MDRW2100001811.1.7",
          "form": "죽을맛이죠..",
          "original_form": "죽을맛이죠..",
          "speaker_id": "2",
          "time": "20210819 21:22"
        },
        {
          "id": "MDRW2100001811.1.8",
          "form": "친구랑 가면 그래도",
          "original_form": "친구랑 가면 그래도",
          "speaker_id": "2",
          "time": "20210819 21:22"
        },
        {
          "id": "MDRW2100001811.1.9",
          "form": "죽어도 재밌어요 ㅋㅋㅋㅋㅎㅋ",
          "original_form": "죽어도 재밌어요 ㅋㅋㅋㅋㅎㅋ",
          "speaker_id": "2",
          "time": "20210819 21:22"
        },
        {
          "id": "MDRW2100001811.1.10",
          "form": "그쪽은요? 알바해 보셨어요?",
          "original_form": "그쪽은요? 알바해 보셨어요?",
          "speaker_id": "2",
          "time": "20210819 21:22"
        },
        {
          "id": "MDRW2100001811.1.11",
          "form": "전 편의점 잠깐 했었구",
          "original_form": "전 편의점 잠깐 했었구",
          "speaker_id": "1",
          "time": "20210819 21:23"
        },
        {
          "id": "MDRW2100001811.1.12",
          "form": "지금은 안해요ㅋㅋㅋㅋ",
          "original_form": "지금은 안해요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:24"
        },
        {
          "id": "MDRW2100001811.1.13",
          "form": "우옹",
          "original_form": "우옹",
          "speaker_id": "2",
          "time": "20210819 21:24"
        },
        {
          "id": "MDRW2100001811.1.14",
          "form": "곧 서울 올라가야해서",
          "original_form": "곧 서울 올라가야해서",
          "speaker_id": "1",
          "time": "20210819 21:24"
        },
        {
          "id": "MDRW2100001811.1.15",
          "form": "그럼 자취하실때는",
          "original_form": "그럼 자취하실때는",
          "speaker_id": "2",
          "time": "20210819 21:24"
        },
        {
          "id": "MDRW2100001811.1.16",
          "form": "거기서 구할랴구요",
          "original_form": "거기서 구할랴구요",
          "speaker_id": "1",
          "time": "20210819 21:24"
        },
        {
          "id": "MDRW2100001811.1.17",
          "form": "아아 하시구나!!",
          "original_form": "아아 하시구나!!",
          "speaker_id": "2",
          "time": "20210819 21:24"
        },
        {
          "id": "MDRW2100001811.1.18",
          "form": "아무래도 자취하면 돈이 많이 드니까ㅠㅠ",
          "original_form": "아무래도 자취하면 돈이 많이 드니까ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 21:24"
        },
        {
          "id": "MDRW2100001811.1.19",
          "form": "자취하면서는.. 해야죠..",
          "original_form": "자취하면서는.. 해야죠..",
          "speaker_id": "2",
          "time": "20210819 21:24"
        },
        {
          "id": "MDRW2100001811.1.20",
          "form": "과외같은거 알아볼려구요",
          "original_form": "과외같은거 알아볼려구요",
          "speaker_id": "1",
          "time": "20210819 21:24"
        },
        {
          "id": "MDRW2100001811.1.21",
          "form": "옹오!!!",
          "original_form": "옹오!!!",
          "speaker_id": "2",
          "time": "20210819 21:24"
        },
        {
          "id": "MDRW2100001811.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210819 21:24"
        },
        {
          "id": "MDRW2100001811.1.23",
          "form": "과외가 꿀이에요",
          "original_form": "과외가 꿀이에요",
          "speaker_id": "2",
          "time": "20210819 21:24"
        },
        {
          "id": "MDRW2100001811.1.24",
          "form": "친구들 보면 진짜",
          "original_form": "친구들 보면 진짜",
          "speaker_id": "2",
          "time": "20210819 21:25"
        },
        {
          "id": "MDRW2100001811.1.25",
          "form": "너무 부러워요 ㅋㅋㅋ",
          "original_form": "너무 부러워요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:25"
        },
        {
          "id": "MDRW2100001811.1.26",
          "form": "일단 과외는 중학교까지!",
          "original_form": "일단 과외는 중학교까지!",
          "speaker_id": "2",
          "time": "20210819 21:25"
        },
        {
          "id": "MDRW2100001811.1.27",
          "form": "진짜 제 친구들도 많이 하는데",
          "original_form": "진짜 제 친구들도 많이 하는데",
          "speaker_id": "1",
          "time": "20210819 21:25"
        },
        {
          "id": "MDRW2100001811.1.28",
          "form": "고등학생은.. 빡세죠.. ㅋㅋㅋ",
          "original_form": "고등학생은.. 빡세죠.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:25"
        },
        {
          "id": "MDRW2100001811.1.29",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210819 21:25"
        },
        {
          "id": "MDRW2100001811.1.30",
          "form": "고등학교 이상으로 가면 힘들어하더라구요ㅋㅋㅋ",
          "original_form": "고등학교 이상으로 가면 힘들어하더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:25"
        },
        {
          "id": "MDRW2100001811.1.31",
          "form": "질문받기가 두려워지는...ㅋㅋㅋㅋ",
          "original_form": "질문받기가 두려워지는...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:26"
        },
        {
          "id": "MDRW2100001811.1.32",
          "form": "그쵸 ㅋㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:26"
        },
        {
          "id": "MDRW2100001811.1.33",
          "form": "참 2학기엔 교내근로 신청했는데",
          "original_form": "참 2학기엔 교내근로 신청했는데",
          "speaker_id": "1",
          "time": "20210819 21:27"
        },
        {
          "id": "MDRW2100001811.1.34",
          "form": "제발 됐으면 좋겠네용ㅋㅋㅋㅋ",
          "original_form": "제발 됐으면 좋겠네용ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:27"
        },
        {
          "id": "MDRW2100001811.1.35",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210819 21:28"
        },
        {
          "id": "MDRW2100001811.1.36",
          "form": "그게 진짜 꿀이래요",
          "original_form": "그게 진짜 꿀이래요",
          "speaker_id": "2",
          "time": "20210819 21:28"
        },
        {
          "id": "MDRW2100001811.1.37",
          "form": "교내 근로가",
          "original_form": "교내 근로가",
          "speaker_id": "2",
          "time": "20210819 21:28"
        },
        {
          "id": "MDRW2100001811.1.38",
          "form": "꼭 붙으시길..!!",
          "original_form": "꼭 붙으시길..!!",
          "speaker_id": "2",
          "time": "20210819 21:28"
        },
        {
          "id": "MDRW2100001811.1.39",
          "form": "ㅎㅎ감사합니당",
          "original_form": "ㅎㅎ감사합니당",
          "speaker_id": "1",
          "time": "20210819 21:28"
        }
      ]
