      "utterance": [
        {
          "id": "MDRW2100014722.1.1",
          "form": "일과 직업이라니",
          "original_form": "일과 직업이라니",
          "speaker_id": "2",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014722.1.2",
          "form": "알바 딱 31일에 끝나니",
          "original_form": "알바 딱 31일에 끝나니",
          "speaker_id": "1",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014722.1.3",
          "form": "응 그렇대..",
          "original_form": "응 그렇대..",
          "speaker_id": "2",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014722.1.4",
          "form": "하",
          "original_form": "하",
          "speaker_id": "2",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014722.1.5",
          "form": "ㅠㅠ 에반데",
          "original_form": "ㅠㅠ 에반데",
          "speaker_id": "1",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014722.1.6",
          "form": "다음 알바 구함?",
          "original_form": "다음 알바 구함?",
          "speaker_id": "1",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014722.1.7",
          "form": "아뇨...",
          "original_form": "아뇨...",
          "speaker_id": "2",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014722.1.8",
          "form": "일단 백신 맞기 전까진 좀 쉴 생각..",
          "original_form": "일단 백신 맞기 전까진 좀 쉴 생각..",
          "speaker_id": "2",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014722.1.9",
          "form": "아하.. 그건 그렇네",
          "original_form": "아하.. 그건 그렇네",
          "speaker_id": "1",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014722.1.10",
          "form": "정말 돈벌기 싫ㄷ다",
          "original_form": "정말 돈벌기 싫ㄷ다",
          "speaker_id": "2",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014722.1.11",
          "form": "일단 이력서에는 버거왕 6개월 햇다고 적어놔야지..",
          "original_form": "일단 이력서에는 버거왕 6개월 햇다고 적어놔야지..",
          "speaker_id": "2",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014722.1.12",
          "form": "나도.. 난 정말 돈 벌기 싫어",
          "original_form": "나도.. 난 정말 돈 벌기 싫어",
          "speaker_id": "1",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014722.1.13",
          "form": "일은 취미로 하고 싶어",
          "original_form": "일은 취미로 하고 싶어",
          "speaker_id": "1",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014722.1.14",
          "form": "....",
          "original_form": "....",
          "speaker_id": "2",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014722.1.15",
          "form": "가만히 있어도 돈이 들어왔으면 좋겠어",
          "original_form": "가만히 있어도 돈이 들어왔으면 좋겠어",
          "speaker_id": "1",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014722.1.16",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014722.1.17",
          "form": "그러기 위해서는",
          "original_form": "그러기 위해서는",
          "speaker_id": "1",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014722.1.18",
          "form": "내가 카카오톡을 먼저 개발했어야..",
          "original_form": "내가 카카오톡을 먼저 개발했어야..",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014722.1.19",
          "form": "일단",
          "original_form": "일단",
          "speaker_id": "1",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014722.1.20",
          "form": "돈을 모아야해",
          "original_form": "돈을 모아야해",
          "speaker_id": "1",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014722.1.21",
          "form": "...",
          "original_form": "...",
          "speaker_id": "1",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014722.1.22",
          "form": "결국돈이",
          "original_form": "결국돈이",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014722.1.23",
          "form": "...",
          "original_form": "...",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014722.1.24",
          "form": "그리고 주식과 부동산에 투자를 하는거지",
          "original_form": "그리고 주식과 부동산에 투자를 하는거지",
          "speaker_id": "1",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014722.1.25",
          "form": "돈을 모으려면 돈을 모아야하네",
          "original_form": "돈을 모으려면 돈을 모아야하네",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014722.1.26",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014722.1.27",
          "form": "....",
          "original_form": "....",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014722.1.28",
          "form": "주식은..",
          "original_form": "주식은..",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014722.1.29",
          "form": "내가 100만원 쉽게 버는 법 알려줄까",
          "original_form": "내가 100만원 쉽게 버는 법 알려줄까",
          "speaker_id": "1",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014722.1.30",
          "form": "남는돈으로 해야하는데",
          "original_form": "남는돈으로 해야하는데",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014722.1.31",
          "form": "난 남는돈아없ㅇㅇ는데",
          "original_form": "난 남는돈아없ㅇㅇ는데",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014722.1.32",
          "form": "100만원..",
          "original_form": "100만원..",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014722.1.33",
          "form": "어떻게..",
          "original_form": "어떻게..",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014722.1.34",
          "form": "준비물 : 1000만원",
          "original_form": "준비물 : 1000만원",
          "speaker_id": "1",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014722.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014722.1.36",
          "form": "..",
          "original_form": "..",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014722.1.37",
          "form": "무슨 방법인지알거같다",
          "original_form": "무슨 방법인지알거같다",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014722.1.38",
          "form": "100만원 껌이지",
          "original_form": "100만원 껌이지",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014722.1.39",
          "form": "맞아 결국 돈이 돈을 낳지",
          "original_form": "맞아 결국 돈이 돈을 낳지",
