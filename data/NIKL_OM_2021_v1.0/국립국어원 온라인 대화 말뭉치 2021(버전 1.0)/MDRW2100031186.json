      "utterance": [
        {
          "id": "MDRW2100031186.1.1",
          "form": "name2님은 친구분과 가셨나요?",
          "original_form": "&name2&님은 친구분과 가셨나요?",
          "speaker_id": "1",
          "time": "20210904 18:00"
        },
        {
          "id": "MDRW2100031186.1.2",
          "form": "아니요 저도 혼자 갔어요 ㅋㅋ",
          "original_form": "아니요 저도 혼자 갔어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:01"
        },
        {
          "id": "MDRW2100031186.1.3",
          "form": "아 근데 에어비앤비를 그닥 안좋아하시는?",
          "original_form": "아 근데 에어비앤비를 그닥 안좋아하시는?",
          "speaker_id": "1",
          "time": "20210904 23:54"
        },
        {
          "id": "MDRW2100031186.1.4",
          "form": "아 아뇨 호스텔이 가격이 더 싸서 그냥 그렇게 갔던거 같아요 ㅋㅋ",
          "original_form": "아 아뇨 호스텔이 가격이 더 싸서 그냥 그렇게 갔던거 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:58"
        },
        {
          "id": "MDRW2100031186.1.5",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210905 00:58"
        },
        {
          "id": "MDRW2100031186.1.6",
          "form": "에어비앤비가 복불복이라 위험한건 있죠 ㅋㅋㅋ",
          "original_form": "에어비앤비가 복불복이라 위험한건 있죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:58"
        },
        {
          "id": "MDRW2100031186.1.7",
          "form": "사진이랑 다른적도 많아서",
          "original_form": "사진이랑 다른적도 많아서",
          "speaker_id": "1",
          "time": "20210905 00:58"
        },
        {
          "id": "MDRW2100031186.1.8",
          "form": "네 저는 그냥 머리아플거 같아서 호스텔이랑 민박으로만 갔었어요 ㅎㅎ",
          "original_form": "네 저는 그냥 머리아플거 같아서 호스텔이랑 민박으로만 갔었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 13:22"
        },
        {
          "id": "MDRW2100031186.1.9",
          "form": "유일하게 부유하게 갔었던곳이 마카오 ㅋㅋ 호텔에서만자고",
          "original_form": "유일하게 부유하게 갔었던곳이 마카오 ㅋㅋ 호텔에서만자고",
          "speaker_id": "1",
          "time": "20210905 13:32"
        },
        {
          "id": "MDRW2100031186.1.10",
          "form": "호텔이란 단어때문인가 자꾸 나쁜말이라고 못쓰네요 ㅋㅋㅋ",
          "original_form": "호텔이란 단어때문인가 자꾸 나쁜말이라고 못쓰네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:35"
        },
        {
          "id": "MDRW2100031186.1.11",
          "form": "ㅋㅋㅋㅋㅋㄱ 호텔이왜요 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㄱ 호텔이왜요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100031186.1.12",
          "form": "불건전한 심심이",
          "original_form": "불건전한 심심이",
          "speaker_id": "1",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100031186.1.13",
          "form": "몰라요 ㅋㅋㅋ 지금 3번 바꿔 말했는데 다 뺀치 먹었어요 ㅋㅋ",
          "original_form": "몰라요 ㅋㅋㅋ 지금 3번 바꿔 말했는데 다 뺀치 먹었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100031186.1.14",
