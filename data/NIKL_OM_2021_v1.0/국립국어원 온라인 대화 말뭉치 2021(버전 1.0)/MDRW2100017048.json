      "utterance": [
        {
          "id": "MDRW2100017048.1.1",
          "form": "나 그냥 name3 집에서 사는 기분이야",
          "original_form": "나 그냥 &name3& 집에서 사는 기분이야",
          "speaker_id": "2",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100017048.1.2",
          "form": "그러긴해",
          "original_form": "그러긴해",
          "speaker_id": "1",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100017048.1.3",
          "form": "우리집 가는 길 보다 익숙해",
          "original_form": "우리집 가는 길 보다 익숙해",
          "speaker_id": "2",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100017048.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100017048.1.5",
          "form": "넌 name3 집에서 사는 느낌이고",
          "original_form": "넌 &name3& 집에서 사는 느낌이고",
          "speaker_id": "1",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100017048.1.6",
          "form": "name4이는 집이 없는 느낌이야",
          "original_form": "&name4&이는 집이 없는 느낌이야",
          "speaker_id": "1",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100017048.1.7",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100017048.1.8",
          "form": "왜 매번 다른 집에 있지 얘는?",
          "original_form": "왜 매번 다른 집에 있지 얘는?",
          "speaker_id": "1",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100017048.1.9",
          "form": "이name4 그냥 떠돌이아냐?",
          "original_form": "이&name4& 그냥 떠돌이아냐?",
          "speaker_id": "2",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100017048.1.10",
          "form": "걘 그냥 홍길동임",
          "original_form": "걘 그냥 홍길동임",
          "speaker_id": "2",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100017048.1.11",
          "form": "몰라... 저번에는 우리 집에서도 잤어...",
          "original_form": "몰라... 저번에는 우리 집에서도 잤어...",
          "speaker_id": "1",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100017048.1.12",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100017048.1.13",
          "form": "전에 동방에서도 자던데",
          "original_form": "전에 동방에서도 자던데",
          "speaker_id": "2",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100017048.1.14",
          "form": "그냥 집 빼고 그돈으로 밥먹는게 나을듯",
          "original_form": "그냥 집 빼고 그돈으로 밥먹는게 나을듯",
          "speaker_id": "2",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100017048.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100017048.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ 내가 볼 때는 그거 이name5한테 시위하는 거야",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 내가 볼 때는 그거 이&name5&한테 시위하는 거야",
          "speaker_id": "1",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100017048.1.17",
          "form": "왜??",
          "original_form": "왜??",
          "speaker_id": "2",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100017048.1.18",
          "form": "우리 집에서 잤을때도 아침에 누가 띵동하길래 열었더니 name5이였어",
          "original_form": "우리 집에서 잤을때도 아침에 누가 띵동하길래 열었더니 &name5&이였어",
          "speaker_id": "1",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100017048.1.19",
          "form": "둘이 뭔일 있나",
          "original_form": "둘이 뭔일 있나",
          "speaker_id": "2",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100017048.1.20",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100017048.1.21",
          "form": "한숨쉬고 이name4 업고 가더라",
          "original_form": "한숨쉬고 이&name4& 업고 가더라",
          "speaker_id": "1",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100017048.1.22",
          "form": "뭔데 딴집 간 강아지 찾으러간 주인같냐",
          "original_form": "뭔데 딴집 간 강아지 찾으러간 주인같냐",
          "speaker_id": "2",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100017048.1.23",
          "form": "어...틀린소리는 아니지",
          "original_form": "어...틀린소리는 아니지",
