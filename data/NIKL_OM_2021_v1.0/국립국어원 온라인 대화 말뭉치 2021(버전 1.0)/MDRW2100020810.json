      "utterance": [
        {
          "id": "MDRW2100020810.1.1",
          "form": "따로따로 놓고보면",
          "original_form": "따로따로 놓고보면",
          "speaker_id": "2",
          "time": "20210828 12:56"
        },
        {
          "id": "MDRW2100020810.1.2",
          "form": "다 어딘가 모자라고 구리지만",
          "original_form": "다 어딘가 모자라고 구리지만",
          "speaker_id": "2",
          "time": "20210828 12:56"
        },
        {
          "id": "MDRW2100020810.1.3",
          "form": "세개를 한번에 다쓴다면?",
          "original_form": "세개를 한번에 다쓴다면?",
          "speaker_id": "2",
          "time": "20210828 12:56"
        },
        {
          "id": "MDRW2100020810.1.4",
          "form": "마자",
          "original_form": "마자",
          "speaker_id": "1",
          "time": "20210828 12:56"
        },
        {
          "id": "MDRW2100020810.1.5",
          "form": "그렇게 상호보완하는거지",
          "original_form": "그렇게 상호보완하는거지",
          "speaker_id": "1",
          "time": "20210828 12:56"
        },
        {
          "id": "MDRW2100020810.1.6",
          "form": "게임 밸런스 처음 잡아본 것처럼 왜그러는거임",
          "original_form": "게임 밸런스 처음 잡아본 것처럼 왜그러는거임",
          "speaker_id": "1",
          "time": "20210828 12:56"
        },
        {
          "id": "MDRW2100020810.1.7",
          "form": "ㅇㅇ그리고",
          "original_form": "ㅇㅇ그리고",
          "speaker_id": "2",
          "time": "20210828 12:56"
        },
        {
          "id": "MDRW2100020810.1.8",
          "form": "언젠간 만렙 확장 하고 다음맵 전체 난이도 올려야할텐데",
          "original_form": "언젠간 만렙 확장 하고 다음맵 전체 난이도 올려야할텐데",
          "speaker_id": "2",
          "time": "20210828 12:57"
        },
        {
          "id": "MDRW2100020810.1.9",
          "form": "언제까지 주인공은 다음원소얻으면 초기화야",
          "original_form": "언제까지 주인공은 다음원소얻으면 초기화야",
          "speaker_id": "2",
          "time": "20210828 12:57"
        },
        {
          "id": "MDRW2100020810.1.10",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210828 12:58"
        },
        {
          "id": "MDRW2100020810.1.11",
          "form": "이미 지금도",
          "original_form": "이미 지금도",
          "speaker_id": "1",
          "time": "20210828 12:58"
        },
        {
          "id": "MDRW2100020810.1.12",
          "form": "몹 스펙 인플레이션 일어나셔",
          "original_form": "몹 스펙 인플레이션 일어나셔",
          "speaker_id": "1",
          "time": "20210828 12:58"
        },
        {
          "id": "MDRW2100020810.1.13",
          "form": "몬드 보스몹보다 이나즈마 잡몹 체력이 더 크잖아",
          "original_form": "몬드 보스몹보다 이나즈마 잡몹 체력이 더 크잖아",
          "speaker_id": "1",
          "time": "20210828 12:58"
        },
        {
          "id": "MDRW2100020810.1.14",
          "form": "이게 말이 되냐구;;",
          "original_form": "이게 말이 되냐구;;",
          "speaker_id": "1",
          "time": "20210828 12:58"
        },
        {
          "id": "MDRW2100020810.1.15",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210828 12:58"
        },
        {
          "id": "MDRW2100020810.1.16",
          "form": "그거 안되게 하려고",
          "original_form": "그거 안되게 하려고",
          "speaker_id": "2",
          "time": "20210828 12:59"
        },
        {
          "id": "MDRW2100020810.1.17",
          "form": "월드 렙 시스템",
          "original_form": "월드 렙 시스템",
          "speaker_id": "2",
          "time": "20210828 12:59"
        },
        {
          "id": "MDRW2100020810.1.18",
          "form": "넣은거 아니야",
          "original_form": "넣은거 아니야",
          "speaker_id": "2",
          "time": "20210828 12:59"
        },
        {
          "id": "MDRW2100020810.1.19",
          "form": "그런줄 알앗는데",
          "original_form": "그런줄 알앗는데",
          "speaker_id": "1",
          "time": "20210828 12:59"
        },
        {
          "id": "MDRW2100020810.1.20",
          "form": "반쯤 망한듯",
          "original_form": "반쯤 망한듯",
          "speaker_id": "1",
          "time": "20210828 12:59"
        },
        {
          "id": "MDRW2100020810.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:59"
        },
        {
          "id": "MDRW2100020810.1.22",
          "form": "ㅎ",
          "original_form": "ㅎ",
          "speaker_id": "2",
          "time": "20210828 12:59"
        },
        {
          "id": "MDRW2100020810.1.23",
          "form": "아니 이나즈마 열때",
          "original_form": "아니 이나즈마 열때",
          "speaker_id": "2",
          "time": "20210828 13:00"
        },
        {
          "id": "MDRW2100020810.1.24",
          "form": "만렙 확장을 했어야지",
          "original_form": "만렙 확장을 했어야지",
          "speaker_id": "2",
          "time": "20210828 13:00"
        },
        {
          "id": "MDRW2100020810.1.25",
          "form": "100까지랃ᆢ",
          "original_form": "100까지랃ᆢ",
          "speaker_id": "2",
          "time": "20210828 13:00"
        },
        {
          "id": "MDRW2100020810.1.26",
          "form": "그리고 맵 난이도 자체가 올라가는건",
          "original_form": "그리고 맵 난이도 자체가 올라가는건",
          "speaker_id": "2",
          "time": "20210828 13:00"
        },
        {
          "id": "MDRW2100020810.1.27",
          "form": "전투에 큰 미련 없는 유저들한테는",
          "original_form": "전투에 큰 미련 없는 유저들한테는",
          "speaker_id": "2",
          "time": "20210828 13:00"
        },
        {
          "id": "MDRW2100020810.1.28",
          "form": "마이너스라고",
          "original_form": "마이너스라고",
          "speaker_id": "2",
          "time": "20210828 13:00"
        },
        {
          "id": "MDRW2100020810.1.29",
          "form": "이런 겜 특성상 뒤에 나오는 캐일수록 딜 세질듯",
          "original_form": "이런 겜 특성상 뒤에 나오는 캐일수록 딜 세질듯",
          "speaker_id": "1",
          "time": "20210828 13:00"
        },
        {
          "id": "MDRW2100020810.1.30",
          "form": "난 내가 90렙 찍으면 다 쓸어버리길 원해",
          "original_form": "난 내가 90렙 찍으면 다 쓸어버리길 원해",
          "speaker_id": "2",
          "time": "20210828 13:01"
        },
        {
          "id": "MDRW2100020810.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:01"
        },
        {
          "id": "MDRW2100020810.1.32",
          "form": "몬드에선 가능인데",
          "original_form": "몬드에선 가능인데",
          "speaker_id": "2",
          "time": "20210828 13:01"
        },
        {
          "id": "MDRW2100020810.1.33",
          "form": "이나즈마는 월드레벨 낮추지 않는 이상",
          "original_form": "이나즈마는 월드레벨 낮추지 않는 이상",
          "speaker_id": "2",
          "time": "20210828 13:01"
        },
        {
          "id": "MDRW2100020810.1.34",
          "form": "무리잖아",
          "original_form": "무리잖아",
          "speaker_id": "2",
          "time": "20210828 13:01"
        },
        {
          "id": "MDRW2100020810.1.35",
          "form": "글킨 하지",
          "original_form": "글킨 하지",
          "speaker_id": "1",
          "time": "20210828 13:02"
        },
        {
          "id": "MDRW2100020810.1.36",
          "form": "앞으로",
          "original_form": "앞으로",
          "speaker_id": "1",
          "time": "20210828 13:02"
        },
        {
          "id": "MDRW2100020810.1.37",
          "form": "폰타인이나 나타 같은",
          "original_form": "폰타인이나 나타 같은",
          "speaker_id": "1",
          "time": "20210828 13:02"
        },
        {
          "id": "MDRW2100020810.1.38",
          "form": "신규 도시 나오면",
          "original_form": "신규 도시 나오면",
          "speaker_id": "1",
          "time": "20210828 13:02"
        },
        {
          "id": "MDRW2100020810.1.39",
          "form": "ㅇㅇ아직도 4개나 더 남았으면서",
          "original_form": "ㅇㅇ아직도 4개나 더 남았으면서",
          "speaker_id": "2",
          "time": "20210828 13:02"
        },
        {
          "id": "MDRW2100020810.1.40",
          "form": "더 난이도 올라갈듯",
          "original_form": "더 난이도 올라갈듯",
          "speaker_id": "1",
          "time": "20210828 13:02"
        },
        {
          "id": "MDRW2100020810.1.41",
          "form": "신규도시 기대되면서 뭔가 기대 안돼",
          "original_form": "신규도시 기대되면서 뭔가 기대 안돼",
