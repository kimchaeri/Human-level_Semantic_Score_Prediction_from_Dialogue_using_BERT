      "utterance": [
        {
          "id": "MDRW2100003084.1.1",
          "form": "방학인데도 과제있어",
          "original_form": "방학인데도 과제있어",
          "speaker_id": "1",
          "time": "20210820 18:28"
        },
        {
          "id": "MDRW2100003084.1.2",
          "form": "오바야",
          "original_form": "오바야",
          "speaker_id": "1",
          "time": "20210820 18:28"
        },
        {
          "id": "MDRW2100003084.1.3",
          "form": "원래 방학은 과제하는 맛 아닌가여",
          "original_form": "원래 방학은 과제하는 맛 아닌가여",
          "speaker_id": "2",
          "time": "20210820 18:28"
        },
        {
          "id": "MDRW2100003084.1.4",
          "form": "과제 대신 해주신다니 감사합니다ㅎㅎ",
          "original_form": "과제 대신 해주신다니 감사합니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 18:30"
        },
        {
          "id": "MDRW2100003084.1.5",
          "form": "음.... 힘내세요 학생",
          "original_form": "음.... 힘내세요 학생",
          "speaker_id": "2",
          "time": "20210820 18:31"
        },
        {
          "id": "MDRW2100003084.1.6",
          "form": "안 돼요ㅜㅜ",
          "original_form": "안 돼요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 18:31"
        },
        {
          "id": "MDRW2100003084.1.7",
          "form": "과제가 많나요?",
          "original_form": "과제가 많나요?",
          "speaker_id": "2",
          "time": "20210820 18:36"
        },
        {
          "id": "MDRW2100003084.1.8",
          "form": "네 코로나때문에 실습을 못 나가서 과제가 많아요",
          "original_form": "네 코로나때문에 실습을 못 나가서 과제가 많아요",
          "speaker_id": "1",
          "time": "20210820 18:40"
        },
        {
          "id": "MDRW2100003084.1.9",
          "form": "레포트 하루에 10장씩 쓰고 있어요ㅠㅠㅜㅜㅜ",
          "original_form": "레포트 하루에 10장씩 쓰고 있어요ㅠㅠㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 18:40"
        },
        {
          "id": "MDRW2100003084.1.10",
          "form": "와 고생이 많네요 진짜ㅜㅜ",
          "original_form": "와 고생이 많네요 진짜ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 18:45"
        },
        {
          "id": "MDRW2100003084.1.11",
          "form": "뭐 전공하시는거에요?",
          "original_form": "뭐 전공하시는거에요?",
          "speaker_id": "2",
          "time": "20210820 18:45"
        },
        {
          "id": "MDRW2100003084.1.12",
          "form": "물리치료 전공하고 있어요",
          "original_form": "물리치료 전공하고 있어요",
          "speaker_id": "1",
          "time": "20210820 18:49"
        },
        {
          "id": "MDRW2100003084.1.13",
          "form": "4학년인데 실습은 아예 못 나가서 너무 걱정이에요",
          "original_form": "4학년인데 실습은 아예 못 나가서 너무 걱정이에요",
          "speaker_id": "1",
          "time": "20210820 18:49"
        },
        {
          "id": "MDRW2100003084.1.14",
          "form": "곧 졸업이신데 ㅜㅜ\r\n코로나가 진짜 문제네여",
          "original_form": "곧 졸업이신데 ㅜㅜ\r\n코로나가 진짜 문제네여",
          "speaker_id": "2",
          "time": "20210820 18:49"
        },
        {
          "id": "MDRW2100003084.1.15",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "1",
          "time": "20210820 18:54"
        },
        {
          "id": "MDRW2100003084.1.16",
          "form": "작년에 입학한 21학번 애들은 학교 제대로 다녀보고 졸업할 수 있을지 걱정이에요",
          "original_form": "작년에 입학한 21학번 애들은 학교 제대로 다녀보고 졸업할 수 있을지 걱정이에요",
          "speaker_id": "1",
          "time": "20210820 18:55"
        },
        {
          "id": "MDRW2100003084.1.17",
          "form": "그러게요ㅠ 코로나 하나로 인해\r\n참 많은게 불편해졌네여ㅠㅠ",
          "original_form": "그러게요ㅠ 코로나 하나로 인해\r\n참 많은게 불편해졌네여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 19:05"
        },
        {
          "id": "MDRW2100003084.1.18",
          "form": "휴학하고싶어요",
          "original_form": "휴학하고싶어요",
          "speaker_id": "1",
          "time": "20210820 19:16"
        }
      ]
