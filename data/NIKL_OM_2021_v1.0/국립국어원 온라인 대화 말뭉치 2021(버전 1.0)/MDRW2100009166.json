      "utterance": [
        {
          "id": "MDRW2100009166.1.1",
          "form": "요샌 할수있는 시간만큼 하고",
          "original_form": "요샌 할수있는 시간만큼 하고",
          "speaker_id": "2",
          "time": "20210822 21:54"
        },
        {
          "id": "MDRW2100009166.1.2",
          "form": "하루종일 하는건 아니라 좋아요",
          "original_form": "하루종일 하는건 아니라 좋아요",
          "speaker_id": "2",
          "time": "20210822 21:55"
        },
        {
          "id": "MDRW2100009166.1.3",
          "form": "전 요새 하고 싶어도  못하고 있어요",
          "original_form": "전 요새 하고 싶어도  못하고 있어요",
          "speaker_id": "2",
          "time": "20210822 21:55"
        },
        {
          "id": "MDRW2100009166.1.4",
          "form": "안불러주네요",
          "original_form": "안불러주네요",
          "speaker_id": "2",
          "time": "20210822 21:55"
        },
        {
          "id": "MDRW2100009166.1.5",
          "form": "학교만 신청하니",
          "original_form": "학교만 신청하니",
          "speaker_id": "2",
          "time": "20210822 21:56"
        },
        {
          "id": "MDRW2100009166.1.6",
          "form": "그냥 쉬고 있어요",
          "original_form": "그냥 쉬고 있어요",
          "speaker_id": "2",
          "time": "20210822 21:57"
        },
        {
          "id": "MDRW2100009166.1.7",
          "form": "전 name1님 처럼 진정한 봉사자는 아닙니다 ㅡㅡ",
          "original_form": "전 &name1&님 처럼 진정한 봉사자는 아닙니다 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210822 21:57"
        },
        {
          "id": "MDRW2100009166.1.8",
          "form": "진정한 봉사자는 대체ㅋㅋㅋㅋ 자기 여유되는만큼 마음내서 하는게 가장 좋은 봉사라고생각해요",
          "original_form": "진정한 봉사자는 대체ㅋㅋㅋㅋ 자기 여유되는만큼 마음내서 하는게 가장 좋은 봉사라고생각해요",
          "speaker_id": "1",
          "time": "20210822 22:11"
        },
        {
          "id": "MDRW2100009166.1.9",
          "form": "우리나라에서는 NGO를 그렇게 좋게 안봐주는거 같아요",
          "original_form": "우리나라에서는 NGO를 그렇게 좋게 안봐주는거 같아요",
          "speaker_id": "1",
          "time": "20210822 22:11"
        },
        {
          "id": "MDRW2100009166.1.10",
          "form": "최근에 뉴스나왔던 돌아가신 서울대기숙사청소부분도",
          "original_form": "최근에 뉴스나왔던 돌아가신 서울대기숙사청소부분도",
          "speaker_id": "1",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009166.1.11",
          "form": "15년이나 제3세계서 NGO활동하시던분이잖아요",
          "original_form": "15년이나 제3세계서 NGO활동하시던분이잖아요",
          "speaker_id": "1",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009166.1.12",
          "form": "그래서 우리나라서는 가능한만큼 마음내서 여유롭게하는게 더 길게갈수있는거같아요",
          "original_form": "그래서 우리나라서는 가능한만큼 마음내서 여유롭게하는게 더 길게갈수있는거같아요",
          "speaker_id": "1",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100009166.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 22:15"
        },
        {
          "id": "MDRW2100009166.1.14",
          "form": "절대 돌봐주지도 인정해주지도 않죠",
          "original_form": "절대 돌봐주지도 인정해주지도 않죠",
          "speaker_id": "2",
          "time": "20210822 22:15"
        },
        {
          "id": "MDRW2100009166.1.15",
          "form": "나라는 선진국이 됐는데 건강하고 돈을벌고 선거권이 있는 국민만 정상이고 나머지 장애인 다문화, 어린이  노약자 등등에게는 박한거같아요",
          "original_form": "나라는 선진국이 됐는데 건강하고 돈을벌고 선거권이 있는 국민만 정상이고 나머지 장애인 다문화, 어린이  노약자 등등에게는 박한거같아요",
          "speaker_id": "1",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100009166.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009166.1.17",
          "form": "장래인이나 노인분 복지가 엉망이죠",
          "original_form": "장래인이나 노인분 복지가 엉망이죠",
          "speaker_id": "2",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009166.1.18",
          "form": "어린이들은 진짜 불쌍해요",
          "original_form": "어린이들은 진짜 불쌍해요",
          "speaker_id": "2",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009166.1.19",
          "form": "저옆아파트에사",
          "original_form": "저옆아파트에사",
