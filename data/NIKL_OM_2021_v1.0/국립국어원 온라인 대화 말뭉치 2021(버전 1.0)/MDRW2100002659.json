      "utterance": [
        {
          "id": "MDRW2100002659.1.1",
          "form": "요즘 읽을만한 책같은거 있어?",
          "original_form": "요즘 읽을만한 책같은거 있어?",
          "speaker_id": "1",
          "time": "20210820 14:26"
        },
        {
          "id": "MDRW2100002659.1.2",
          "form": "글쎄",
          "original_form": "글쎄",
          "speaker_id": "2",
          "time": "20210820 14:29"
        },
        {
          "id": "MDRW2100002659.1.3",
          "form": "집에만 있어서 책을 잘..",
          "original_form": "집에만 있어서 책을 잘..",
          "speaker_id": "2",
          "time": "20210820 14:29"
        },
        {
          "id": "MDRW2100002659.1.4",
          "form": "나도 졸업하고 나서 책을 잘 안읽었는데",
          "original_form": "나도 졸업하고 나서 책을 잘 안읽었는데",
          "speaker_id": "1",
          "time": "20210820 14:30"
        },
        {
          "id": "MDRW2100002659.1.5",
          "form": "최근에 집에 놔뒀던 책 다시 읽어보고 있어",
          "original_form": "최근에 집에 놔뒀던 책 다시 읽어보고 있어",
          "speaker_id": "1",
          "time": "20210820 14:30"
        },
        {
          "id": "MDRW2100002659.1.6",
          "form": "오 어떤 책 읽는데?",
          "original_form": "오 어떤 책 읽는데?",
          "speaker_id": "2",
          "time": "20210820 14:30"
        },
        {
          "id": "MDRW2100002659.1.7",
          "form": "메이즈러너라고 알아?",
          "original_form": "메이즈러너라고 알아?",
          "speaker_id": "1",
          "time": "20210820 14:30"
        },
        {
          "id": "MDRW2100002659.1.8",
          "form": "아 알아",
          "original_form": "아 알아",
          "speaker_id": "2",
          "time": "20210820 14:31"
        },
        {
          "id": "MDRW2100002659.1.9",
          "form": "나 고등학교때 읽었어",
          "original_form": "나 고등학교때 읽었어",
          "speaker_id": "2",
          "time": "20210820 14:31"
        },
        {
          "id": "MDRW2100002659.1.10",
          "form": "그거 우리집에도 있어서 다시 읽어보고 있어",
          "original_form": "그거 우리집에도 있어서 다시 읽어보고 있어",
          "speaker_id": "1",
          "time": "20210820 14:32"
        },
        {
          "id": "MDRW2100002659.1.11",
          "form": "혹시 고등학교 때 읽었던 책 이름들 기억해?",
          "original_form": "혹시 고등학교 때 읽었던 책 이름들 기억해?",
          "speaker_id": "1",
          "time": "20210820 14:33"
        },
        {
          "id": "MDRW2100002659.1.12",
          "form": "기억나는 것도 있고 까먹은 것도 있고",
          "original_form": "기억나는 것도 있고 까먹은 것도 있고",
          "speaker_id": "2",
          "time": "20210820 14:34"
        },
        {
          "id": "MDRW2100002659.1.13",
          "form": "뭐뭐 기억해?",
          "original_form": "뭐뭐 기억해?",
          "speaker_id": "1",
          "time": "20210820 14:34"
        },
        {
          "id": "MDRW2100002659.1.14",
          "form": "룬의 아이들?",
          "original_form": "룬의 아이들?",
          "speaker_id": "2",
          "time": "20210820 14:35"
        },
        {
          "id": "MDRW2100002659.1.15",
          "form": "나 셜록홈즈도 읽었고",
          "original_form": "나 셜록홈즈도 읽었고",
          "speaker_id": "2",
          "time": "20210820 14:35"
        },
        {
          "id": "MDRW2100002659.1.16",
          "form": "와 방금 너가 룬의 아이들이라고 해서 나도 어렴풋이 기억났어",
          "original_form": "와 방금 너가 룬의 아이들이라고 해서 나도 어렴풋이 기억났어",
          "speaker_id": "1",
          "time": "20210820 14:35"
        },
        {
          "id": "MDRW2100002659.1.17",
          "form": "말안했으면 기억도 못했을것 같은데",
          "original_form": "말안했으면 기억도 못했을것 같은데",
          "speaker_id": "1",
          "time": "20210820 14:36"
        },
        {
          "id": "MDRW2100002659.1.18",
          "form": "기억력 되게 좋네",
          "original_form": "기억력 되게 좋네",
          "speaker_id": "1",
          "time": "20210820 14:36"
        },
        {
          "id": "MDRW2100002659.1.19",
          "form": "이런 장편 읽은적 없어서 기억하고 있어",
          "original_form": "이런 장편 읽은적 없어서 기억하고 있어",
          "speaker_id": "2",
          "time": "20210820 14:36"
        },
        {
          "id": "MDRW2100002659.1.20",
          "form": "난 눈물을 마시는새 기억하는데",
          "original_form": "난 눈물을 마시는새 기억하는데",
          "speaker_id": "1",
          "time": "20210820 14:37"
        },
        {
          "id": "MDRW2100002659.1.21",
          "form": "내용은 하나도 기억안나",
          "original_form": "내용은 하나도 기억안나",
          "speaker_id": "1",
          "time": "20210820 14:37"
        },
        {
          "id": "MDRW2100002659.1.22",
          "form": "아 그 책도 있었지",
          "original_form": "아 그 책도 있었지",
          "speaker_id": "2",
          "time": "20210820 14:37"
        },
        {
          "id": "MDRW2100002659.1.23",
          "form": "나도 읽었는데 내용 다 까먹은 듯",
          "original_form": "나도 읽었는데 내용 다 까먹은 듯",
          "speaker_id": "2",
          "time": "20210820 14:38"
        }
      ]
