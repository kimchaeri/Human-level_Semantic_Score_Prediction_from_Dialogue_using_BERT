      "utterance": [
        {
          "id": "MDRW2100040306.1.1",
          "form": "졸업하고 가봤는데",
          "original_form": "졸업하고 가봤는데",
          "speaker_id": "2",
          "time": "20210904 15:54"
        },
        {
          "id": "MDRW2100040306.1.2",
          "form": "없었어요 할머니 돌아가셨나 봐요",
          "original_form": "없었어요 할머니 돌아가셨나 봐요",
          "speaker_id": "2",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100040306.1.3",
          "form": "갑자기 급 슬퍼지네요",
          "original_form": "갑자기 급 슬퍼지네요",
          "speaker_id": "1",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100040306.1.4",
          "form": "연세가 있으셨거든요",
          "original_form": "연세가 있으셨거든요",
          "speaker_id": "2",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100040306.1.5",
          "form": "우리 다음에 신평에 영진 국밥 먹으러 가요",
          "original_form": "우리 다음에 신평에 영진 국밥 먹으러 가요",
          "speaker_id": "1",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100040306.1.6",
          "form": "일번으로 먹어야 되는 게 뭐죠",
          "original_form": "일번으로 먹어야 되는 게 뭐죠",
          "speaker_id": "2",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100040306.1.7",
          "form": "수육국밥 시켜 먹으면 볶음김치 주는데 진짜 맛있어요",
          "original_form": "수육국밥 시켜 먹으면 볶음김치 주는데 진짜 맛있어요",
          "speaker_id": "1",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100040306.1.8",
          "form": "그건 정할 수가 없네요",
          "original_form": "그건 정할 수가 없네요",
          "speaker_id": "1",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100040306.1.9",
          "form": "국밥 어우 맛있겠다",
          "original_form": "국밥 어우 맛있겠다",
          "speaker_id": "2",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100040306.1.10",
          "form": "페리카나 금옥 쌀로 튀긴 치킨 그것도 먹으러 가야 하는데",
          "original_form": "페리카나 금옥 쌀로 튀긴 치킨 그것도 먹으러 가야 하는데",
          "speaker_id": "1",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100040306.1.11",
          "form": "딴 대신 동에 첫 번째로 먹어야 되는 게 뭘까요",
          "original_form": "딴 대신 동에 첫 번째로 먹어야 되는 게 뭘까요",
          "speaker_id": "2",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100040306.1.12",
          "form": "제 생각에는 이거 하다 보니 백 번 넘어야 될 거 같은데요",
          "original_form": "제 생각에는 이거 하다 보니 백 번 넘어야 될 거 같은데요",
          "speaker_id": "2",
          "time": "20210904 15:56"
        },
        {
          "id": "MDRW2100040306.1.13",
          "form": "굉장히 고민이 되네요",
          "original_form": "굉장히 고민이 되네요",
          "speaker_id": "1",
          "time": "20210904 15:56"
        },
        {
          "id": "MDRW2100040306.1.14",
          "form": "세상에는 맛있는 음식이 너무 많네요",
          "original_form": "세상에는 맛있는 음식이 너무 많네요",
          "speaker_id": "2",
          "time": "20210904 15:56"
        },
        {
          "id": "MDRW2100040306.1.15",
          "form": "맛집도 많이 있고요",
          "original_form": "맛집도 많이 있고요",
          "speaker_id": "1",
          "time": "20210904 15:56"
        },
        {
          "id": "MDRW2100040306.1.16",
          "form": "일번이 뭘까요",
          "original_form": "일번이 뭘까요",
          "speaker_id": "2",
          "time": "20210904 15:56"
        },
        {
          "id": "MDRW2100040306.1.17",
          "form": "첫 번째로 갈 장소를 정합시다",
          "original_form": "첫 번째로 갈 장소를 정합시다",
          "speaker_id": "2",
          "time": "20210904 15:56"
        }
      ]
