      "utterance": [
        {
          "id": "MDRW2100042259.1.1",
          "form": "최근에 구매 한게 어떤거에요?",
          "original_form": "최근에 구매 한게 어떤거에요?",
          "speaker_id": "2",
          "time": "20210906 21:10"
        },
        {
          "id": "MDRW2100042259.1.2",
          "form": "기저귀요ㅠㅋ",
          "original_form": "기저귀요ㅠㅋ",
          "speaker_id": "1",
          "time": "20210906 21:11"
        },
        {
          "id": "MDRW2100042259.1.3",
          "form": "아 육아동지시군요  ㅎㅎ\n저희아이는 좀 크지만요",
          "original_form": "아 육아동지시군요  ㅎㅎ\n저희아이는 좀 크지만요",
          "speaker_id": "2",
          "time": "20210906 21:12"
        },
        {
          "id": "MDRW2100042259.1.4",
          "form": "몇살이에요?ㅎ",
          "original_form": "몇살이에요?ㅎ",
          "speaker_id": "1",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042259.1.5",
          "form": "5살요 ㅎㅎ",
          "original_form": "5살요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042259.1.6",
          "form": "아 ㅎㅎ 한창 이쁠때네욤",
          "original_form": "아 ㅎㅎ 한창 이쁠때네욤",
          "speaker_id": "1",
          "time": "20210906 21:14"
        },
        {
          "id": "MDRW2100042259.1.7",
          "form": "ㅎㅎ 말썽쟁이이기도 하구요 ㅋ",
          "original_form": "ㅎㅎ 말썽쟁이이기도 하구요 ㅋ",
          "speaker_id": "2",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100042259.1.8",
          "form": "한창그럴때죠 ㅎㅎ  최근에사신거 있으세요?",
          "original_form": "한창그럴때죠 ㅎㅎ  최근에사신거 있으세요?",
          "speaker_id": "1",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100042259.1.9",
          "form": "저는 스마트벨트??  삿어요",
          "original_form": "저는 스마트벨트??  삿어요",
          "speaker_id": "2",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100042259.1.10",
          "form": "화장실 가려면 벨트 푸는게 귀찮? 잖아요 ㅎㅎ",
          "original_form": "화장실 가려면 벨트 푸는게 귀찮? 잖아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100042259.1.11",
          "form": "근데 뒤쪽만 잡아주는 벨트가 잇더라구요 ㅎ",
          "original_form": "근데 뒤쪽만 잡아주는 벨트가 잇더라구요 ㅎ",
          "speaker_id": "2",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100042259.1.12",
          "form": "그런게있나요 와첨알았네여?",
          "original_form": "그런게있나요 와첨알았네여?",
          "speaker_id": "1",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100042259.1.13",
          "form": "아이꺼 먼저사서  잘쓰고 잇는데",
          "original_form": "아이꺼 먼저사서  잘쓰고 잇는데",
          "speaker_id": "2",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100042259.1.14",
          "form": "남편도 사달라해서 ㅎㅎ",
          "original_form": "남편도 사달라해서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100042259.1.15",
          "form": "와 남자들도 있음 편하겠네요",
          "original_form": "와 남자들도 있음 편하겠네요",
          "speaker_id": "1",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100042259.1.16",
          "form": "어른 사이즈도 있더라구요 ㅋ",
          "original_form": "어른 사이즈도 있더라구요 ㅋ",
          "speaker_id": "2",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100042259.1.17",
          "form": "가격은 얼마정도?해요?",
          "original_form": "가격은 얼마정도?해요?",
          "speaker_id": "1",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100042259.1.18",
          "form": "만 얼마요",
          "original_form": "만 얼마요",
          "speaker_id": "2",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042259.1.19",
          "form": "오 가격도 괜찮네요?",
          "original_form": "오 가격도 괜찮네요?",
          "speaker_id": "1",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042259.1.20",
          "form": "배송비가 붙어서 ㅜㅁ ㅜ",
          "original_form": "배송비가 붙어서 ㅜㅁ ㅜ",
          "speaker_id": "2",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042259.1.21",
          "form": "남편 좋다하면 여러개 더사려구요",
          "original_form": "남편 좋다하면 여러개 더사려구요",
