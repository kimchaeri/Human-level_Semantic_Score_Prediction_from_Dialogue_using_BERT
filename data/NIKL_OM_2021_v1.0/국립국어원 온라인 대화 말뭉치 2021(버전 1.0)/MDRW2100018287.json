      "utterance": [
        {
          "id": "MDRW2100018287.1.1",
          "form": "둘다좋아하긴하는데 요즘은 짜장면!",
          "original_form": "둘다좋아하긴하는데 요즘은 짜장면!",
          "speaker_id": "2",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100018287.1.2",
          "form": "짜장면맛집이 잘없어요",
          "original_form": "짜장면맛집이 잘없어요",
          "speaker_id": "2",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018287.1.3",
          "form": "헐.......진짜 와",
          "original_form": "헐.......진짜 와",
          "speaker_id": "1",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018287.1.4",
          "form": "충격의연속",
          "original_form": "충격의연속",
          "speaker_id": "1",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018287.1.5",
          "form": "버거킹vs맥도날드는 어떠세요?",
          "original_form": "버거킹vs맥도날드는 어떠세요?",
          "speaker_id": "1",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018287.1.6",
          "form": "맥도날드요 ㅋㅋㅋㅋㅋ",
          "original_form": "맥도날드요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018287.1.7",
          "form": "오오오오",
          "original_form": "오오오오",
          "speaker_id": "1",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018287.1.8",
          "form": "아주그레이트",
          "original_form": "아주그레이트",
          "speaker_id": "1",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018287.1.9",
          "form": "만나면 딴데말고 맥날만가야겠어요ㅋㅋㅋㅋ",
          "original_form": "만나면 딴데말고 맥날만가야겠어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018287.1.10",
          "form": "버거킹이 훨씬가까운데 맥도날드가 더좋아요 ㅋㅋㅋ",
          "original_form": "버거킹이 훨씬가까운데 맥도날드가 더좋아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018287.1.11",
          "form": "수제버거좋아하세요?",
          "original_form": "수제버거좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018287.1.12",
          "form": "ㅋㅋㅋㅋ 아뇨저는 사실",
          "original_form": "ㅋㅋㅋㅋ 아뇨저는 사실",
          "speaker_id": "1",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018287.1.13",
          "form": "햄버거를그닥",
          "original_form": "햄버거를그닥",
          "speaker_id": "1",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018287.1.14",
          "form": "즐기지않는답니다",
          "original_form": "즐기지않는답니다",
          "speaker_id": "1",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018287.1.15",
          "form": "저는 뭐 일년에 한두번먹는데  요즘 수제버거집이 많더라구요",
          "original_form": "저는 뭐 일년에 한두번먹는데  요즘 수제버거집이 많더라구요",
          "speaker_id": "2",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018287.1.16",
          "form": "얼마전 먹은 수제버거집이 빵이 브리오슈더라구요? 요즘 대부분 이거쓰는거 같은데 맛있는지모르겠어요 ㅋㅋ",
          "original_form": "얼마전 먹은 수제버거집이 빵이 브리오슈더라구요? 요즘 대부분 이거쓰는거 같은데 맛있는지모르겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018287.1.17",
          "form": "브리오슈가뭐죠?!!!!!!!",
          "original_form": "브리오슈가뭐죠?!!!!!!!",
          "speaker_id": "1",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100018287.1.18",
          "form": "제가 버거쪽은",
          "original_form": "제가 버거쪽은",
          "speaker_id": "1",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100018287.1.19",
          "form": "문외한이랍니다..",
          "original_form": "문외한이랍니다..",
          "speaker_id": "1",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100018287.1.20",
          "form": "빵종류인데 좀 부드럽고 버터맛 많이나는? 제입맛에는 너무 달달한 느낌이더라구요",
          "original_form": "빵종류인데 좀 부드럽고 버터맛 많이나는? 제입맛에는 너무 달달한 느낌이더라구요",
          "speaker_id": "2",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100018287.1.21",
          "form": "와우 저는 버터좋아하는데",
          "original_form": "와우 저는 버터좋아하는데",
          "speaker_id": "1",
          "time": "20210826 23:42"
        },
        {
          "id": "MDRW2100018287.1.22",
          "form": "잘맞으려나요?!",
          "original_form": "잘맞으려나요?!",
          "speaker_id": "1",
          "time": "20210826 23:42"
        },
        {
          "id": "MDRW2100018287.1.23",
          "form": "좋아하실거같아요! ㅋㅋ",
          "original_form": "좋아하실거같아요! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:43"
        },
        {
          "id": "MDRW2100018287.1.24",
          "form": "앙버터 드셔보셨어요?",
          "original_form": "앙버터 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018287.1.25",
          "form": "ㅋㅋㅋㅋ좋네요",
          "original_form": "ㅋㅋㅋㅋ좋네요",
          "speaker_id": "1",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018287.1.26",
          "form": "앙버터 먹어봤어요!",
          "original_form": "앙버터 먹어봤어요!",
          "speaker_id": "1",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018287.1.27",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "1",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018287.1.28",
          "form": "제가먹은곳이 맛집이아니었나봐요",
          "original_form": "제가먹은곳이 맛집이아니었나봐요",
          "speaker_id": "1",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018287.1.29",
          "form": "별로셨나요?!",
          "original_form": "별로셨나요?!",
          "speaker_id": "2",
          "time": "20210826 23:44"
        }
      ]
    }
  ]
