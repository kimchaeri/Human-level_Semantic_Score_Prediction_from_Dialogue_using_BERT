      "utterance": [
        {
          "id": "MDRW2100014249.1.1",
          "form": "드라마 보시나요?",
          "original_form": "드라마 보시나요?",
          "speaker_id": "1",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014249.1.2",
          "form": "드라마 보시나요?",
          "original_form": "드라마 보시나요?",
          "speaker_id": "1",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014249.1.3",
          "form": "네 자주보진 않지만 가끔 보고 있어요",
          "original_form": "네 자주보진 않지만 가끔 보고 있어요",
          "speaker_id": "2",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100014249.1.4",
          "form": "드라마 좋아하세요?",
          "original_form": "드라마 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100014249.1.5",
          "form": "어떤거 보세요?",
          "original_form": "어떤거 보세요?",
          "speaker_id": "1",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100014249.1.6",
          "form": "요새 슬의생2  재밌더라고요ㅎㅎ",
          "original_form": "요새 슬의생2  재밌더라고요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014249.1.7",
          "form": "요새 보는거는 슬기로운 의사생활이요 ㅋㅋㅋ",
          "original_form": "요새 보는거는 슬기로운 의사생활이요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014249.1.8",
          "form": "슬의생 재밌죠ㅋㅋㅋㅋ",
          "original_form": "슬의생 재밌죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014249.1.9",
          "form": "ㅋㅋㅋ네 희노애락이 다 있는 드라마 같아요ㅋㅋ",
          "original_form": "ㅋㅋㅋ네 희노애락이 다 있는 드라마 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014249.1.10",
          "form": "어떤 주인공을 제일 좋아하세요?",
          "original_form": "어떤 주인공을 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100014249.1.11",
          "form": "보기 시작한거는 조정석때문에 보기 시작했는데",
          "original_form": "보기 시작한거는 조정석때문에 보기 시작했는데",
          "speaker_id": "2",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014249.1.12",
          "form": "지금은 석형이요ㅠㅠㅠ",
          "original_form": "지금은 석형이요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 18:04"
        },
        {
          "id": "MDRW2100014249.1.13",
          "form": "뭔가 응원하게 되는 캐릭터같아요",
          "original_form": "뭔가 응원하게 되는 캐릭터같아요",
          "speaker_id": "2",
          "time": "20210825 18:04"
        },
        {
          "id": "MDRW2100014249.1.14",
          "form": "저도 조정석 땜에 보다가 석형이 응원해요ㅋㅋㅋ",
          "original_form": "저도 조정석 땜에 보다가 석형이 응원해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:06"
        },
        {
          "id": "MDRW2100014249.1.15",
          "form": "뭔가 가여운 캐릭터 같아서ㅎㅎ",
          "original_form": "뭔가 가여운 캐릭터 같아서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:06"
        },
        {
          "id": "MDRW2100014249.1.16",
          "form": "맞아요 가여운 면도 있고 완벽한 캐릭이 아니라서 더 좋아요ㅋㅋㅋ",
          "original_form": "맞아요 가여운 면도 있고 완벽한 캐릭이 아니라서 더 좋아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:11"
        },
        {
          "id": "MDRW2100014249.1.17",
          "form": "ㅋㅋ맞아요 넘사벽 캐릭터가 아니에요ㅋㅋ",
          "original_form": "ㅋㅋ맞아요 넘사벽 캐릭터가 아니에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014249.1.18",
          "form": "그리고 그 펠로우 샘이랑 잘 됬으면 좋겠어요ㅋㅋㄱ",
          "original_form": "그리고 그 펠로우 샘이랑 잘 됬으면 좋겠어요ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014249.1.19",
          "form": "아 펠로우쌤 ㅋㅋㅋ",
          "original_form": "아 펠로우쌤 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100014249.1.20",
          "form": "그 쌤 넘 귀여워요ㅋㅋㅋ",
          "original_form": "그 쌤 넘 귀여워요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100014249.1.21",
          "form": "근데 뭔가 민하랑 될거같아서요...",
          "original_form": "근데 뭔가 민하랑 될거같아서요...",
          "speaker_id": "2",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100014249.1.22",
          "form": "좀 뻔한 전개일까요?ㅋㅋㅋㅋ",
          "original_form": "좀 뻔한 전개일까요?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014249.1.23",
          "form": "하지만 당연하죠ㅋㅋㅋ",
          "original_form": "하지만 당연하죠ㅋㅋㅋ",
