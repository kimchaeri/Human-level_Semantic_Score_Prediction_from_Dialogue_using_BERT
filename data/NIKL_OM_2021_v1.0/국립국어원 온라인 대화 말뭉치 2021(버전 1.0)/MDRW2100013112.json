      "utterance": [
        {
          "id": "MDRW2100013112.1.1",
          "form": "우리name3 ...그러고보니",
          "original_form": "우리&name3& ...그러고보니",
          "speaker_id": "2",
          "time": "20210825 13:16"
        },
        {
          "id": "MDRW2100013112.1.2",
          "form": "내일 목욕시켜야하는데",
          "original_form": "내일 목욕시켜야하는데",
          "speaker_id": "2",
          "time": "20210825 13:16"
        },
        {
          "id": "MDRW2100013112.1.3",
          "form": "name3가 생후 몇 개월이야ㅉ",
          "original_form": "&name3&가 생후 몇 개월이야ㅉ",
          "speaker_id": "1",
          "time": "20210825 13:17"
        },
        {
          "id": "MDRW2100013112.1.4",
          "form": "지금 3개월되갈거얌 ㅋㅋ",
          "original_form": "지금 3개월되갈거얌 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100013112.1.5",
          "form": "아고 진짜 애기애기하네ㅋㅋ",
          "original_form": "아고 진짜 애기애기하네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013112.1.6",
          "form": "이도 막 앙앙앙 하나?ㅋㅋ",
          "original_form": "이도 막 앙앙앙 하나?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013112.1.7",
          "form": "웅 ㅋㅋㅋ",
          "original_form": "웅 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100013112.1.8",
          "form": "근데아파 ...",
          "original_form": "근데아파 ...",
          "speaker_id": "2",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100013112.1.9",
          "form": "잉? 아파?",
          "original_form": "잉? 아파?",
          "speaker_id": "1",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100013112.1.10",
          "form": "깨무는데",
          "original_form": "깨무는데",
          "speaker_id": "2",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100013112.1.11",
          "form": "좀아프드라구 ㅋㅋㅋ",
          "original_form": "좀아프드라구 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100013112.1.12",
          "form": "무는힘이 장난이아닌가배",
          "original_form": "무는힘이 장난이아닌가배",
          "speaker_id": "2",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100013112.1.13",
          "form": "그런가보당ㅋㅋ",
          "original_form": "그런가보당ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013112.1.14",
          "form": "아직 애기라\r\n엄청 귀엽게 앙앙 무는 정도일줄\r\n알았는데ㅋㅋ",
          "original_form": "아직 애기라\r\n엄청 귀엽게 앙앙 무는 정도일줄\r\n알았는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013112.1.15",
          "form": "힘도",
          "original_form": "힘도",
          "speaker_id": "2",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013112.1.16",
          "form": "넘쳐",
          "original_form": "넘쳐",
          "speaker_id": "2",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013112.1.17",
          "form": "계속놀아줘동",
          "original_form": "계속놀아줘동",
          "speaker_id": "2",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100013112.1.18",
          "form": "안지쳐...",
          "original_form": "안지쳐...",
          "speaker_id": "2",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100013112.1.19",
          "form": "ㅋㅋ뭐하고 놀아죠?",
          "original_form": "ㅋㅋ뭐하고 놀아죠?",
          "speaker_id": "1",
          "time": "20210825 13:45"
        },
        {
          "id": "MDRW2100013112.1.20",
          "form": "그냥 인형 던져주고",
          "original_form": "그냥 인형 던져주고",
          "speaker_id": "2",
          "time": "20210825 13:49"
        },
        {
          "id": "MDRW2100013112.1.21",
          "form": "장난감가지고 막뛰고 ㅋㅋㅋ",
          "original_form": "장난감가지고 막뛰고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:49"
        },
        {
          "id": "MDRW2100013112.1.22",
          "form": "ㅋㅋ주인 잘 만났네",
          "original_form": "ㅋㅋ주인 잘 만났네",
          "speaker_id": "1",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013112.1.23",
          "form": "산책도 나가나?ㅋㅋ",
          "original_form": "산책도 나가나?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013112.1.24",
          "form": "아직 1차예방 접종만 맞았는데",
          "original_form": "아직 1차예방 접종만 맞았는데",
          "speaker_id": "2",
          "time": "20210825 14:13"
        },
        {
          "id": "MDRW2100013112.1.25",
          "form": "오늘 잠깐 바람만 쐬러나갈까",
          "original_form": "오늘 잠깐 바람만 쐬러나갈까",
          "speaker_id": "2",
          "time": "20210825 14:13"
        },
        {
          "id": "MDRW2100013112.1.26",
          "form": "생각중 ㅋㅋ",
          "original_form": "생각중 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:13"
        },
        {
          "id": "MDRW2100013112.1.27",
          "form": "아... 아직 접종이 남았구나ㅠ",
          "original_form": "아... 아직 접종이 남았구나ㅠ",
          "speaker_id": "1",
          "time": "20210825 14:14"
        },
        {
          "id": "MDRW2100013112.1.28",
          "form": "웅..코로나약도 조끔 섞어서",
          "original_form": "웅..코로나약도 조끔 섞어서",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013112.1.29",
          "form": "아ㅜㅜ 몽몽이들도 맞는구나ㅠㅠ",
          "original_form": "아ㅜㅜ 몽몽이들도 맞는구나ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013112.1.30",
          "form": "그럼 타이레놀처럼\r\n접종하고 먹는 약도 있나?",
          "original_form": "그럼 타이레놀처럼\r\n접종하고 먹는 약도 있나?",
          "speaker_id": "1",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013112.1.31",
          "form": "그런건 없눈거같앙 ㅋㅋㅋ",
          "original_form": "그런건 없눈거같앙 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013112.1.32",
          "form": "그냥 2차 까지만 맞고 마는거같던뎅",
          "original_form": "그냥 2차 까지만 맞고 마는거같던뎅",
          "speaker_id": "2",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013112.1.33",
          "form": "근데 멀쩡해 ㅋㅋㅋ",
          "original_form": "근데 멀쩡해 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:05"
        }
      ]
    }
