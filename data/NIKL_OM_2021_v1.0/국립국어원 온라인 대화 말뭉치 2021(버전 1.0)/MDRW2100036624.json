      "utterance": [
        {
          "id": "MDRW2100036624.1.1",
          "form": "하나로마트근처라서 자주가요",
          "original_form": "하나로마트근처라서 자주가요",
          "speaker_id": "2",
          "time": "20210831 02:33"
        },
        {
          "id": "MDRW2100036624.1.2",
          "form": "득템하셨으면 좋겠어요!ㅎㅎ",
          "original_form": "득템하셨으면 좋겠어요!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 02:34"
        },
        {
          "id": "MDRW2100036624.1.3",
          "form": "내일부터",
          "original_form": "내일부터",
          "speaker_id": "1",
          "time": "20210831 02:34"
        },
        {
          "id": "MDRW2100036624.1.4",
          "form": "탑마트 이마트도 가까워서 일주일에한번씩?",
          "original_form": "탑마트 이마트도 가까워서 일주일에한번씩?",
          "speaker_id": "2",
          "time": "20210831 02:34"
        },
        {
          "id": "MDRW2100036624.1.5",
          "form": "하나로마트는 거의이틀에한번",
          "original_form": "하나로마트는 거의이틀에한번",
          "speaker_id": "2",
          "time": "20210831 02:34"
        },
        {
          "id": "MDRW2100036624.1.6",
          "form": "지마켓 옥션 빅스마일데이래요!",
          "original_form": "지마켓 옥션 빅스마일데이래요!",
          "speaker_id": "1",
          "time": "20210831 02:34"
        },
        {
          "id": "MDRW2100036624.1.7",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210831 02:34"
        },
        {
          "id": "MDRW2100036624.1.8",
          "form": "살거 정해놓으셨나요?",
          "original_form": "살거 정해놓으셨나요?",
          "speaker_id": "1",
          "time": "20210831 02:34"
        },
        {
          "id": "MDRW2100036624.1.9",
          "form": "근데 빅스마일데이때만되면 살게없어져요",
          "original_form": "근데 빅스마일데이때만되면 살게없어져요",
          "speaker_id": "2",
          "time": "20210831 02:34"
        },
        {
          "id": "MDRW2100036624.1.10",
          "form": "앗ㅋㅋㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 02:34"
        },
        {
          "id": "MDRW2100036624.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ슬프게도",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ슬프게도",
          "speaker_id": "2",
          "time": "20210831 02:34"
        },
        {
          "id": "MDRW2100036624.1.12",
          "form": "저는 두루마리휴지랑",
          "original_form": "저는 두루마리휴지랑",
          "speaker_id": "1",
          "time": "20210831 02:34"
        },
        {
          "id": "MDRW2100036624.1.13",
          "form": "세제랑",
          "original_form": "세제랑",
          "speaker_id": "1",
          "time": "20210831 02:34"
        },
        {
          "id": "MDRW2100036624.1.14",
          "form": "칫솔 사려구요!",
          "original_form": "칫솔 사려구요!",
          "speaker_id": "1",
          "time": "20210831 02:34"
        },
        {
          "id": "MDRW2100036624.1.15",
          "form": "왜 사고싶은게 세일을비껴가는지",
          "original_form": "왜 사고싶은게 세일을비껴가는지",
          "speaker_id": "2",
          "time": "20210831 02:35"
        },
        {
          "id": "MDRW2100036624.1.16",
          "form": "아하ㅠㅠ 타이밍이 안맞을때가 있죠...",
          "original_form": "아하ㅠㅠ 타이밍이 안맞을때가 있죠...",
          "speaker_id": "1",
          "time": "20210831 02:35"
        },
        {
          "id": "MDRW2100036624.1.17",
          "form": "생필품 재워두실려나보네요",
          "original_form": "생필품 재워두실려나보네요",
          "speaker_id": "2",
          "time": "20210831 02:35"
        },
        {
          "id": "MDRW2100036624.1.18",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210831 02:35"
        },
        {
          "id": "MDRW2100036624.1.19",
          "form": "빅스마일데이때마다",
          "original_form": "빅스마일데이때마다",
          "speaker_id": "1",
          "time": "20210831 02:35"
        },
        {
          "id": "MDRW2100036624.1.20",
          "form": "생필품 가득 주문하고",
          "original_form": "생필품 가득 주문하고",
          "speaker_id": "1",
          "time": "20210831 02:35"
        },
        {
          "id": "MDRW2100036624.1.21",
          "form": "에휴 휴지도 꽉꽉있고",
          "original_form": "에휴 휴지도 꽉꽉있고",
          "speaker_id": "2",
          "time": "20210831 02:35"
        },
        {
          "id": "MDRW2100036624.1.22",
          "form": "다음 빅스마일데이 세일행사 할때까지 버텨요ㅋㅋㅋ",
          "original_form": "다음 빅스마일데이 세일행사 할때까지 버텨요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 02:35"
        },
        {
          "id": "MDRW2100036624.1.23",
          "form": "평소에 장을 꼼꼼히 보시나봐요!",
          "original_form": "평소에 장을 꼼꼼히 보시나봐요!",
          "speaker_id": "1",
          "time": "20210831 02:36"
        }
      ]
    }
