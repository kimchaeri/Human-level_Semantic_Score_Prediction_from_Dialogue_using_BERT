      "utterance": [
        {
          "id": "MDRW2100029321.1.1",
          "form": "name2은 감성적이어서",
          "original_form": "&name2&은 감성적이어서",
          "speaker_id": "1",
          "time": "20210903 01:30"
        },
        {
          "id": "MDRW2100029321.1.2",
          "form": "왠지 멋진 연애를 하셧을것같네요",
          "original_form": "왠지 멋진 연애를 하셧을것같네요",
          "speaker_id": "1",
          "time": "20210903 01:30"
        },
        {
          "id": "MDRW2100029321.1.3",
          "form": "말씀만으로도 감사합니다",
          "original_form": "말씀만으로도 감사합니다",
          "speaker_id": "2",
          "time": "20210903 01:30"
        },
        {
          "id": "MDRW2100029321.1.4",
          "form": "이상형이 어케되시나요",
          "original_form": "이상형이 어케되시나요",
          "speaker_id": "1",
          "time": "20210903 01:31"
        },
        {
          "id": "MDRW2100029321.1.5",
          "form": "민망하게도 여태 혼자네요ㅠ",
          "original_form": "민망하게도 여태 혼자네요ㅠ",
          "speaker_id": "2",
          "time": "20210903 01:31"
        },
        {
          "id": "MDRW2100029321.1.6",
          "form": "생각이 닮은, 대화가 통하는 사람이 이상형이랍니다",
          "original_form": "생각이 닮은, 대화가 통하는 사람이 이상형이랍니다",
          "speaker_id": "2",
          "time": "20210903 01:31"
        },
        {
          "id": "MDRW2100029321.1.7",
          "form": "아이고 왜그러시지 맘에드는 사람 없엇나요",
          "original_form": "아이고 왜그러시지 맘에드는 사람 없엇나요",
          "speaker_id": "1",
          "time": "20210903 01:32"
        },
        {
          "id": "MDRW2100029321.1.8",
          "form": "없었다면 거짓이겠죠",
          "original_form": "없었다면 거짓이겠죠",
          "speaker_id": "2",
          "time": "20210903 01:35"
        },
        {
          "id": "MDRW2100029321.1.9",
          "form": "아이고 근데 잘 안됐나요",
          "original_form": "아이고 근데 잘 안됐나요",
          "speaker_id": "1",
          "time": "20210903 01:35"
        },
        {
          "id": "MDRW2100029321.1.10",
          "form": "마음이 부족해서 좋아한단 고백한번 못해본 인연들이죠",
          "original_form": "마음이 부족해서 좋아한단 고백한번 못해본 인연들이죠",
          "speaker_id": "2",
          "time": "20210903 01:35"
        },
        {
          "id": "MDRW2100029321.1.11",
          "form": "아이고...마음이부족 ?",
          "original_form": "아이고...마음이부족 ?",
          "speaker_id": "1",
          "time": "20210903 01:35"
        },
        {
          "id": "MDRW2100029321.1.12",
          "form": "덜조아햇다?",
          "original_form": "덜조아햇다?",
          "speaker_id": "1",
          "time": "20210903 01:35"
        },
        {
          "id": "MDRW2100029321.1.13",
          "form": "아녜요 스스로 그사람에게 너무 부족한것 같았어요",
          "original_form": "아녜요 스스로 그사람에게 너무 부족한것 같았어요",
          "speaker_id": "2",
          "time": "20210903 01:38"
        },
        {
          "id": "MDRW2100029321.1.14",
          "form": "지금도 달라진건 없지만..",
          "original_form": "지금도 달라진건 없지만..",
          "speaker_id": "2",
          "time": "20210903 01:38"
        },
        {
          "id": "MDRW2100029321.1.15",
          "form": "왜부족해요 뭐가",
          "original_form": "왜부족해요 뭐가",
          "speaker_id": "1",
          "time": "20210903 01:38"
        },
        {
          "id": "MDRW2100029321.1.16",
          "form": "조아하면 표현해야죠",
          "original_form": "조아하면 표현해야죠",
          "speaker_id": "1",
          "time": "20210903 01:39"
        },
        {
          "id": "MDRW2100029321.1.17",
          "form": "맞아요 그런데 앞으로도 표현못할거 같아요",
          "original_form": "맞아요 그런데 앞으로도 표현못할거 같아요",
          "speaker_id": "2",
          "time": "20210903 01:39"
        },
        {
          "id": "MDRW2100029321.1.18",
          "form": "왜요!!! ????",
          "original_form": "왜요!!! ????",
          "speaker_id": "1",
          "time": "20210903 01:40"
        },
        {
          "id": "MDRW2100029321.1.19",
          "form": "혹시 로맨스드라마 좋아하시나요?",
          "original_form": "혹시 로맨스드라마 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210903 01:41"
        },
        {
          "id": "MDRW2100029321.1.20",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210903 01:41"
        },
        {
          "id": "MDRW2100029321.1.21",
          "form": "왜영?",
          "original_form": "왜영?",
          "speaker_id": "1",
          "time": "20210903 01:41"
        },
        {
