      "utterance": [
        {
          "id": "MDRW2100030813.1.1",
          "form": "엄마는 에릭 노래부르는게 좋아",
          "original_form": "엄마는 에릭 노래부르는게 좋아",
          "speaker_id": "2",
          "time": "20210904 12:51"
        },
        {
          "id": "MDRW2100030813.1.2",
          "form": "오페라의 유령 주제가 부분이요? ㅋㅋㅋ",
          "original_form": "오페라의 유령 주제가 부분이요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:51"
        },
        {
          "id": "MDRW2100030813.1.3",
          "form": "저도 좋아해요",
          "original_form": "저도 좋아해요",
          "speaker_id": "1",
          "time": "20210904 12:51"
        },
        {
          "id": "MDRW2100030813.1.4",
          "form": "씽포미 인가 하는 부분",
          "original_form": "씽포미 인가 하는 부분",
          "speaker_id": "2",
          "time": "20210904 12:52"
        },
        {
          "id": "MDRW2100030813.1.5",
          "form": "재밌어",
          "original_form": "재밌어",
          "speaker_id": "2",
          "time": "20210904 12:52"
        },
        {
          "id": "MDRW2100030813.1.6",
          "form": "계속 소리 커지고 그래서 크리스틴 배우 멋있어요",
          "original_form": "계속 소리 커지고 그래서 크리스틴 배우 멋있어요",
          "speaker_id": "1",
          "time": "20210904 12:52"
        },
        {
          "id": "MDRW2100030813.1.7",
          "form": "그정도는 해야 뮤지컬 배우 되나보다",
          "original_form": "그정도는 해야 뮤지컬 배우 되나보다",
          "speaker_id": "2",
          "time": "20210904 12:52"
        },
        {
          "id": "MDRW2100030813.1.8",
          "form": "오페라인가??",
          "original_form": "오페라인가??",
          "speaker_id": "2",
          "time": "20210904 12:52"
        },
        {
          "id": "MDRW2100030813.1.9",
          "form": "뮤지컬일걸요??",
          "original_form": "뮤지컬일걸요??",
          "speaker_id": "1",
          "time": "20210904 12:52"
        },
        {
          "id": "MDRW2100030813.1.10",
          "form": "근데 그거 부르는사람 얼마 안된다고 들은거같기도",
          "original_form": "근데 그거 부르는사람 얼마 안된다고 들은거같기도",
          "speaker_id": "1",
          "time": "20210904 12:52"
        },
        {
          "id": "MDRW2100030813.1.11",
          "form": "잘하는 사람만 되는거구나",
          "original_form": "잘하는 사람만 되는거구나",
          "speaker_id": "2",
          "time": "20210904 12:52"
        },
        {
          "id": "MDRW2100030813.1.12",
          "form": "약간 밤의 여왕 아리아 같은 ㅋㅋㅋ",
          "original_form": "약간 밤의 여왕 아리아 같은 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:52"
        },
        {
          "id": "MDRW2100030813.1.13",
          "form": "그 소리지르는거지",
          "original_form": "그 소리지르는거지",
          "speaker_id": "2",
          "time": "20210904 12:53"
        },
        {
          "id": "MDRW2100030813.1.14",
          "form": "아아아아아 하고?",
          "original_form": "아아아아아 하고?",
          "speaker_id": "2",
          "time": "20210904 12:53"
        },
        {
          "id": "MDRW2100030813.1.15",
          "form": "마술피리에 나오는",
          "original_form": "마술피리에 나오는",
          "speaker_id": "2",
          "time": "20210904 12:53"
        },
        {
          "id": "MDRW2100030813.1.16",
          "form": "맞아요 ㅋㅋㅋㅋ 집에서 자주 불렀는데 못불러거",
          "original_form": "맞아요 ㅋㅋㅋㅋ 집에서 자주 불렀는데 못불러거",
          "speaker_id": "1",
          "time": "20210904 12:53"
        },
        {
          "id": "MDRW2100030813.1.17",
          "form": "오빠가 맨날 ㅎㅎ 그만하라고...",
          "original_form": "오빠가 맨날 ㅎㅎ 그만하라고...",
          "speaker_id": "1",
          "time": "20210904 12:53"
        },
        {
          "id": "MDRW2100030813.1.18",
          "form": "잘부르던데 뭘",
          "original_form": "잘부르던데 뭘",
          "speaker_id": "2",
          "time": "20210904 12:53"
        },
        {
          "id": "MDRW2100030813.1.19",
          "form": "걔가 나빴지",
          "original_form": "걔가 나빴지",
          "speaker_id": "2",
          "time": "20210904 12:53"
        },
        {
          "id": "MDRW2100030813.1.20",
          "form": "저 그래도 음치는 아닌데 ㅠ",
          "original_form": "저 그래도 음치는 아닌데 ㅠ",
          "speaker_id": "1",
          "time": "20210904 12:53"
        },
        {
          "id": "MDRW2100030813.1.21",
          "form": "우리딸이 우리 가족중에서 제일 잘불러",
          "original_form": "우리딸이 우리 가족중에서 제일 잘불러",
          "speaker_id": "2",
          "time": "20210904 12:54"
        }
