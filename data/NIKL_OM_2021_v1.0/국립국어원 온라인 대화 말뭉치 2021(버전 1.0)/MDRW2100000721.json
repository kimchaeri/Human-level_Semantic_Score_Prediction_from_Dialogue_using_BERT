        },
        {
          "id": "MDRW2100000721.1.2",
          "form": "나도 모름 까페에서 싸대",
          "original_form": "나도 모름 까페에서 싸대",
          "speaker_id": "2",
          "time": "20210819 18:47"
        },
        {
          "id": "MDRW2100000721.1.3",
          "form": "얼마인데 가격이?",
          "original_form": "얼마인데 가격이?",
          "speaker_id": "1",
          "time": "20210819 18:47"
        },
        {
          "id": "MDRW2100000721.1.4",
          "form": "십만원 정도인듯",
          "original_form": "십만원 정도인듯",
          "speaker_id": "2",
          "time": "20210819 18:47"
        },
        {
          "id": "MDRW2100000721.1.5",
          "form": "비싼데",
          "original_form": "비싼데",
          "speaker_id": "1",
          "time": "20210819 18:47"
        },
        {
          "id": "MDRW2100000721.1.6",
          "form": "비싼데",
          "original_form": "비싼데",
          "speaker_id": "1",
          "time": "20210819 18:47"
        },
        {
          "id": "MDRW2100000721.1.7",
          "form": "겨울에 나오면 더 비싸니까 ㅋ",
          "original_form": "겨울에 나오면 더 비싸니까 ㅋ",
          "speaker_id": "2",
          "time": "20210819 18:48"
        },
        {
          "id": "MDRW2100000721.1.8",
          "form": "보통 이삼십만원인가",
          "original_form": "보통 이삼십만원인가",
          "speaker_id": "1",
          "time": "20210819 18:48"
        },
        {
          "id": "MDRW2100000721.1.9",
          "form": "백화점은 오십씩하지",
          "original_form": "백화점은 오십씩하지",
          "speaker_id": "2",
          "time": "20210819 18:48"
        },
        {
          "id": "MDRW2100000721.1.10",
          "form": "요즘 옷들이 넘 비싸",
          "original_form": "요즘 옷들이 넘 비싸",
          "speaker_id": "1",
          "time": "20210819 18:48"
        },
        {
          "id": "MDRW2100000721.1.11",
          "form": "ㅇㅇ 지하에서만 사야하나봐",
          "original_form": "ㅇㅇ 지하에서만 사야하나봐",
          "speaker_id": "2",
          "time": "20210819 18:49"
        },
        {
          "id": "MDRW2100000721.1.12",
          "form": "지하도 요즘 코로나 때문에 못가",
          "original_form": "지하도 요즘 코로나 때문에 못가",
          "speaker_id": "1",
          "time": "20210819 18:49"
        },
        {
          "id": "MDRW2100000721.1.13",
          "form": "인터넷 최저가를 노려야겠구만",
          "original_form": "인터넷 최저가를 노려야겠구만",
