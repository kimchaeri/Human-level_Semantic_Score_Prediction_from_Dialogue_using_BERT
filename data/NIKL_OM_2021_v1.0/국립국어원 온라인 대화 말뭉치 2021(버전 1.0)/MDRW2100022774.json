      "utterance": [
        {
          "id": "MDRW2100022774.1.1",
          "form": "가짜제????",
          "original_form": "가짜제????",
          "speaker_id": "2",
          "time": "20210829 18:01"
        },
        {
          "id": "MDRW2100022774.1.2",
          "form": "여기서 만들어낸 시나리오지?",
          "original_form": "여기서 만들어낸 시나리오지?",
          "speaker_id": "1",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100022774.1.3",
          "form": "근데 분위기가 너무 무서워",
          "original_form": "근데 분위기가 너무 무서워",
          "speaker_id": "1",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100022774.1.4",
          "form": "지금 김종국 탈락했는데",
          "original_form": "지금 김종국 탈락했는데",
          "speaker_id": "1",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100022774.1.5",
          "form": "아 목소리 너무 무섭당",
          "original_form": "아 목소리 너무 무섭당",
          "speaker_id": "1",
          "time": "20210829 18:03"
        },
        {
          "id": "MDRW2100022774.1.6",
          "form": "유튜브로봐야겠다",
          "original_form": "유튜브로봐야겠다",
          "speaker_id": "2",
          "time": "20210830 11:43"
        },
        {
          "id": "MDRW2100022774.1.7",
          "form": "런닝맨 이번 회차 추천할게",
          "original_form": "런닝맨 이번 회차 추천할게",
          "speaker_id": "1",
          "time": "20210830 12:57"
        },
        {
          "id": "MDRW2100022774.1.8",
          "form": "아 어제 대탈출도 진짜 재밌었어",
          "original_form": "아 어제 대탈출도 진짜 재밌었어",
          "speaker_id": "1",
          "time": "20210830 12:58"
        },
        {
          "id": "MDRW2100022774.1.9",
          "form": "물끄고 봐서 좀 무섭긴 했지만",
          "original_form": "물끄고 봐서 좀 무섭긴 했지만",
          "speaker_id": "1",
          "time": "20210830 12:58"
        },
        {
          "id": "MDRW2100022774.1.10",
          "form": "불끄고봐서",
          "original_form": "불끄고봐서",
          "speaker_id": "1",
          "time": "20210830 12:58"
        },
        {
          "id": "MDRW2100022774.1.11",
          "form": "정말보고싶다ㅜㅜ",
          "original_form": "정말보고싶다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 16:48"
        },
        {
          "id": "MDRW2100022774.1.12",
          "form": "하지만시간이없는걸,,",
          "original_form": "하지만시간이없는걸,,",
          "speaker_id": "2",
          "time": "20210901 16:48"
        },
        {
          "id": "MDRW2100022774.1.13",
          "form": "쉬는 날 있으면 보자!",
          "original_form": "쉬는 날 있으면 보자!",
          "speaker_id": "1",
          "time": "20210901 16:49"
        },
        {
          "id": "MDRW2100022774.1.14",
          "form": "나도 이번 주가 마지막으로 쉬는 날일 거야",
          "original_form": "나도 이번 주가 마지막으로 쉬는 날일 거야",
          "speaker_id": "1",
          "time": "20210901 16:49"
        },
        {
          "id": "MDRW2100022774.1.15",
          "form": "보고싶었던 드라마 다 몰아봐야지",
          "original_form": "보고싶었던 드라마 다 몰아봐야지",
          "speaker_id": "1",
          "time": "20210901 16:49"
        },
        {
          "id": "MDRW2100022774.1.16",
          "form": "기묘한 이야기 재밌다 추천!",
          "original_form": "기묘한 이야기 재밌다 추천!",
          "speaker_id": "1",
          "time": "20210901 16:49"
        },
        {
          "id": "MDRW2100022774.1.17",
          "form": "나 주말에 볼거야, ,",
          "original_form": "나 주말에 볼거야, ,",
          "speaker_id": "2",
          "time": "20210901 17:44"
        },
        {
          "id": "MDRW2100022774.1.18",
          "form": "아마두",
          "original_form": "아마두",
          "speaker_id": "2",
          "time": "20210901 17:44"
        },
        {
          "id": "MDRW2100022774.1.19",
          "form": "좋아하는 연예인 있니?",
          "original_form": "좋아하는 연예인 있니?",
          "speaker_id": "2",
          "time": "20210901 17:44"
        },
        {
          "id": "MDRW2100022774.1.20",
          "form": "난 아이유를 좋아해!",
          "original_form": "난 아이유를 좋아해!",
          "speaker_id": "1",
          "time": "20210901 18:07"
        },
        {
          "id": "MDRW2100022774.1.21",
          "form": "좋아해",
          "original_form": "좋아해",
          "speaker_id": "2",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100022774.1.22",
          "form": "아이ㅠ",
          "original_form": "아이ㅠ",
          "speaker_id": "2",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100022774.1.23",
          "form": "아이유",
          "original_form": "아이유",
          "speaker_id": "2",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100022774.1.24",
          "form": "아이유는 안 좋아할 수가 없지!",
          "original_form": "아이유는 안 좋아할 수가 없지!",
          "speaker_id": "1",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100022774.1.25",
          "form": "아이유 노래 중에 뭘 제일 좋아하니?",
          "original_form": "아이유 노래 중에 뭘 제일 좋아하니?",
          "speaker_id": "1",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100022774.1.26",
          "form": "나는 무릎을 좋아해",
          "original_form": "나는 무릎을 좋아해",
          "speaker_id": "2",
          "time": "20210901 21:05"
        },
        {
          "id": "MDRW2100022774.1.27",
          "form": "곡 만든 이유와 가사가 내 생각을 꼭 적은 거 같아",
          "original_form": "곡 만든 이유와 가사가 내 생각을 꼭 적은 거 같아",
          "speaker_id": "2",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100022774.1.28",
          "form": "나도 그 곡 좋아해",
          "original_form": "나도 그 곡 좋아해",
          "speaker_id": "1",
          "time": "20210901 21:38"
        },
        {
          "id": "MDRW2100022774.1.29",
          "form": "나는 요즘 Love poem 이랑 아이와 나의 바다 자주 들어",
          "original_form": "나는 요즘 Love poem 이랑 아이와 나의 바다 자주 들어",
          "speaker_id": "1",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100022774.1.30",
          "form": "솔직히 아이유 노래는 다 좋아",
          "original_form": "솔직히 아이유 노래는 다 좋아",
          "speaker_id": "2",
          "time": "20210901 21:39"
        },
        {
          "id": "MDRW2100022774.1.31",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100022774.1.32",
          "form": "아나바다도 탑에 드는 곡이양",
          "original_form": "아나바다도 탑에 드는 곡이양",
          "speaker_id": "2",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100022774.1.33",
          "form": "아이와 나의 바다 완전 명곡이지",
          "original_form": "아이와 나의 바다 완전 명곡이지",
          "speaker_id": "1",
          "time": "20210901 21:40"
        }
      ]
    }
