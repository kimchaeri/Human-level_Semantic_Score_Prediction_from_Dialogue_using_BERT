      "utterance": [
        {
          "id": "MDRW2100018501.1.1",
          "form": "카페가면 어떤 음료로 드세요?",
          "original_form": "카페가면 어떤 음료로 드세요?",
          "speaker_id": "2",
          "time": "20210827 06:47"
        },
        {
          "id": "MDRW2100018501.1.2",
          "form": "앗ㅋㅋㅋㅋㅋ저는 젤좋아하는건 치킨..",
          "original_form": "앗ㅋㅋㅋㅋㅋ저는 젤좋아하는건 치킨..",
          "speaker_id": "2",
          "time": "20210827 06:47"
        },
        {
          "id": "MDRW2100018501.1.3",
          "form": "저는 커피를 잘 못마셔서",
          "original_form": "저는 커피를 잘 못마셔서",
          "speaker_id": "1",
          "time": "20210827 06:47"
        },
        {
          "id": "MDRW2100018501.1.4",
          "form": "프라푸치노류 좋아해요!",
          "original_form": "프라푸치노류 좋아해요!",
          "speaker_id": "1",
          "time": "20210827 06:47"
        },
        {
          "id": "MDRW2100018501.1.5",
          "form": "오 최애 프라푸치노 추천해주세용!",
          "original_form": "오 최애 프라푸치노 추천해주세용!",
          "speaker_id": "2",
          "time": "20210827 06:47"
        },
        {
          "id": "MDRW2100018501.1.6",
          "form": "그 스타벅스 자바칩 9번 넣은",
          "original_form": "그 스타벅스 자바칩 9번 넣은",
          "speaker_id": "1",
          "time": "20210827 06:48"
        },
        {
          "id": "MDRW2100018501.1.7",
          "form": "자바칩 프라푸치노 맛있어용",
          "original_form": "자바칩 프라푸치노 맛있어용",
          "speaker_id": "1",
          "time": "20210827 06:48"
        },
        {
          "id": "MDRW2100018501.1.8",
          "form": "반은 갈고 반은 위에 통으로 뿌려서 먹으면 꿀맛이에요",
          "original_form": "반은 갈고 반은 위에 통으로 뿌려서 먹으면 꿀맛이에요",
          "speaker_id": "1",
          "time": "20210827 06:48"
        },
        {
          "id": "MDRW2100018501.1.9",
          "form": "자바칩 프라푸치노 먹어봤는데 9번으로 추가해서 먹어볼께요!",
          "original_form": "자바칩 프라푸치노 먹어봤는데 9번으로 추가해서 먹어볼께요!",
          "speaker_id": "2",
          "time": "20210827 06:49"
        },
        {
          "id": "MDRW2100018501.1.10",
          "form": "네넹 추천합니다",
          "original_form": "네넹 추천합니다",
          "speaker_id": "1",
          "time": "20210827 06:49"
        },
        {
          "id": "MDRW2100018501.1.11",
          "form": "휩은 에소휩으로 하시나요?",
          "original_form": "휩은 에소휩으로 하시나요?",
          "speaker_id": "2",
          "time": "20210827 06:49"
        },
        {
          "id": "MDRW2100018501.1.12",
          "form": "한번을 추가하든 9번을 추가하든 가격은 똑같으니까용!",
          "original_form": "한번을 추가하든 9번을 추가하든 가격은 똑같으니까용!",
          "speaker_id": "1",
          "time": "20210827 06:49"
        },
        {
          "id": "MDRW2100018501.1.13",
          "form": "휩은 빼고 먹어욬ㅋㅋ",
          "original_form": "휩은 빼고 먹어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 06:49"
        },
        {
          "id": "MDRW2100018501.1.14",
          "form": "나름 다이어트 한다고 또...",
          "original_form": "나름 다이어트 한다고 또...",
          "speaker_id": "1",
          "time": "20210827 06:50"
        },
        {
          "id": "MDRW2100018501.1.15",
          "form": "현명한 소비자",
          "original_form": "현명한 소비자",
          "speaker_id": "2",
          "time": "20210827 06:50"
        },
        {
          "id": "MDRW2100018501.1.16",
          "form": "말도안되는...",
          "original_form": "말도안되는...",
          "speaker_id": "1",
          "time": "20210827 06:50"
        },
        {
          "id": "MDRW2100018501.1.17",
          "form": "ㅋㅋㅋ공감해여!",
          "original_form": "ㅋㅋㅋ공감해여!",
          "speaker_id": "2",
          "time": "20210827 06:50"
        },
        {
          "id": "MDRW2100018501.1.18",
          "form": "스타벅스 종이빨대",
          "original_form": "스타벅스 종이빨대",
          "speaker_id": "1",
          "time": "20210827 06:50"
        },
        {
          "id": "MDRW2100018501.1.19",
          "form": "너무 싫어요ㅠㅠ",
          "original_form": "너무 싫어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 06:50"
        },
        {
          "id": "MDRW2100018501.1.20",
          "form": "자바칩 9번넣은대신 휩을 뺄수 있는거죠!ㅎ.ㅎ",
          "original_form": "자바칩 9번넣은대신 휩을 뺄수 있는거죠!ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210827 06:50"
        },
        {
          "id": "MDRW2100018501.1.21",
          "form": "자바칩먹을때 넘 물렁물렁해져서",
          "original_form": "자바칩먹을때 넘 물렁물렁해져서",
          "speaker_id": "2",
          "time": "20210827 06:50"
        },
        {
          "id": "MDRW2100018501.1.22",
          "form": "마자요ㅋㅋㅋ 휩은 빼고! 양심챙기고!",
          "original_form": "마자요ㅋㅋㅋ 휩은 빼고! 양심챙기고!",
          "speaker_id": "1",
          "time": "20210827 06:50"
        },
        {
          "id": "MDRW2100018501.1.23",
          "form": "중간에 빨대 바꿔야해요ㅠㅠ",
          "original_form": "중간에 빨대 바꿔야해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 06:50"
        },
        {
          "id": "MDRW2100018501.1.24",
          "form": "맞아요ㅠㅠ 그리고 골판지향이랑 맛이 나요..",
          "original_form": "맞아요ㅠㅠ 그리고 골판지향이랑 맛이 나요..",
          "speaker_id": "1",
          "time": "20210827 06:50"
        },
        {
          "id": "MDRW2100018501.1.25",
          "form": "흑 선생님 제가 분리수거를 하러 나가야되가지구ㅠㅠ 넘 즐거웠어용",
          "original_form": "흑 선생님 제가 분리수거를 하러 나가야되가지구ㅠㅠ 넘 즐거웠어용",
          "speaker_id": "1",
          "time": "20210827 06:51"
        }
      ]
    }
  ]
