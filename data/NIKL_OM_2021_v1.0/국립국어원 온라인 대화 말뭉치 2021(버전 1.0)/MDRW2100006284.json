      "utterance": [
        {
          "id": "MDRW2100006284.1.1",
          "form": "오늘 날씨는 어떤 가요?",
          "original_form": "오늘 날씨는 어떤 가요?",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100006284.1.2",
          "form": "그곳은?",
          "original_form": "그곳은?",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100006284.1.3",
          "form": "비오다춥네요",
          "original_form": "비오다춥네요",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100006284.1.4",
          "form": "정말요?",
          "original_form": "정말요?",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100006284.1.5",
          "form": "거기는요?",
          "original_form": "거기는요?",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100006284.1.6",
          "form": "저희도 그랬네요ㅜㅜ",
          "original_form": "저희도 그랬네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100006284.1.7",
          "form": "이제곧겨울올듯",
          "original_form": "이제곧겨울올듯",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100006284.1.8",
          "form": "인정해요",
          "original_form": "인정해요",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100006284.1.9",
          "form": "가을좋은데",
          "original_form": "가을좋은데",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100006284.1.10",
          "form": "겨울 전 좋아해요",
          "original_form": "겨울 전 좋아해요",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100006284.1.11",
          "form": "아..그렇군요",
          "original_form": "아..그렇군요",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100006284.1.12",
          "form": "전 가을요",
          "original_form": "전 가을요",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100006284.1.13",
          "form": "이불 폭 덮고 있으면",
          "original_form": "이불 폭 덮고 있으면",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100006284.1.14",
          "form": "아주 좋죠",
          "original_form": "아주 좋죠",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100006284.1.15",
          "form": "화창한날",
          "original_form": "화창한날",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100006284.1.16",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100006284.1.17",
          "form": "화창한날이요?",
          "original_form": "화창한날이요?",
          "speaker_id": "1",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100006284.1.18",
          "form": "좋아해요..옷입기도 좋고",
          "original_form": "좋아해요..옷입기도 좋고",
