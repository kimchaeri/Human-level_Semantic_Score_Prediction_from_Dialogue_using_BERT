      "utterance": [
        {
          "id": "MDRW2100019982.1.1",
          "form": "상미종식빵이라고 있는데 기회되면 드셔보세요",
          "original_form": "상미종식빵이라고 있는데 기회되면 드셔보세요",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019982.1.2",
          "form": "상미종!",
          "original_form": "상미종!",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019982.1.3",
          "form": "아시나요",
          "original_form": "아시나요",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019982.1.4",
          "form": "아까 파바에서 사왓는데!",
          "original_form": "아까 파바에서 사왓는데!",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019982.1.5",
          "form": "아직 먹어보진않앗어요",
          "original_form": "아직 먹어보진않앗어요",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019982.1.6",
          "form": "헉 아시는구나",
          "original_form": "헉 아시는구나",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019982.1.7",
          "form": "그거 진짜 맛있더라구요",
          "original_form": "그거 진짜 맛있더라구요",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019982.1.8",
          "form": "혹시 오늘 요기요 파바를 참여하셨나요",
          "original_form": "혹시 오늘 요기요 파바를 참여하셨나요",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019982.1.9",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019982.1.10",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019982.1.11",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019982.1.12",
          "form": "혹시 name1님 샐러드랑 뀐아망도....??",
          "original_form": "혹시 &name1&님 샐러드랑 뀐아망도....??",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019982.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019982.1.14",
          "form": "ㅋㅋㅋㅋㅋ대박 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ대박 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019982.1.15",
          "form": "대박 빵 또 뭐사셨어요",
          "original_form": "대박 빵 또 뭐사셨어요",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019982.1.16",
          "form": "저 한 번 주문취소당햇잖아요",
          "original_form": "저 한 번 주문취소당햇잖아요",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019982.1.17",
          "form": "헐왜요",
          "original_form": "헐왜요",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019982.1.18",
          "form": "버터꿀식빵?",
          "original_form": "버터꿀식빵?",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019982.1.19",
          "form": "그거 품절이라서",
          "original_form": "그거 품절이라서",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019982.1.20",
          "form": "그것도 맛있는데",
          "original_form": "그것도 맛있는데",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019982.1.21",
          "form": "취소한다고 해서 상미종으로 다시 골랏어요",
          "original_form": "취소한다고 해서 상미종으로 다시 골랏어요",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019982.1.22",
          "form": "와 식빵을 잘 아시네요",
          "original_form": "와 식빵을 잘 아시네요",
          "speaker_id": "1",
          "time": "20210827 23:08"
        }
