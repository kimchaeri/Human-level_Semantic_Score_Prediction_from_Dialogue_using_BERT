      "utterance": [
        {
          "id": "MDRW2100030161.1.1",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100030161.1.2",
          "form": "주말이라 일찍 갔다와야돼",
          "original_form": "주말이라 일찍 갔다와야돼",
          "speaker_id": "2",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100030161.1.3",
          "form": "전복 살거야?",
          "original_form": "전복 살거야?",
          "speaker_id": "1",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100030161.1.4",
          "form": "사야지 니 미역국 끓여주려면",
          "original_form": "사야지 니 미역국 끓여주려면",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100030161.1.5",
          "form": "성은이 망극하옵니다",
          "original_form": "성은이 망극하옵니다",
          "speaker_id": "1",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100030161.1.6",
          "form": "엄마 그리고 나 아몬드우유사도 돼?",
          "original_form": "엄마 그리고 나 아몬드우유사도 돼?",
          "speaker_id": "1",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100030161.1.7",
          "form": "샐러드 야채랑!",
          "original_form": "샐러드 야채랑!",
          "speaker_id": "1",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100030161.1.8",
          "form": "샐러드 먹게?",
          "original_form": "샐러드 먹게?",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100030161.1.9",
          "form": "냉동실에 용가리랑 해서 먹게!",
          "original_form": "냉동실에 용가리랑 해서 먹게!",
          "speaker_id": "1",
          "time": "20210903 22:43"
        },
        {
          "id": "MDRW2100030161.1.10",
          "form": "맛있냐?",
          "original_form": "맛있냐?",
          "speaker_id": "2",
          "time": "20210903 22:43"
        },
        {
          "id": "MDRW2100030161.1.11",
          "form": "그렇게 하면?",
          "original_form": "그렇게 하면?",
          "speaker_id": "2",
          "time": "20210903 22:43"
        },
        {
          "id": "MDRW2100030161.1.12",
          "form": "야채만 먹는 것보다 훨씬 맛있지",
          "original_form": "야채만 먹는 것보다 훨씬 맛있지",
          "speaker_id": "1",
          "time": "20210903 22:43"
        },
        {
          "id": "MDRW2100030161.1.13",
          "form": "어차피 냉동실도 비워야하는데",
          "original_form": "어차피 냉동실도 비워야하는데",
          "speaker_id": "1",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100030161.1.14",
          "form": "그래 그럼",
          "original_form": "그래 그럼",
          "speaker_id": "2",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100030161.1.15",
          "form": "안 잊어먹게 메모해놔",
          "original_form": "안 잊어먹게 메모해놔",
          "speaker_id": "2",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100030161.1.16",
          "form": "ㅇㅇ아몬드 우유도 넣는다?",
          "original_form": "ㅇㅇ아몬드 우유도 넣는다?",
          "speaker_id": "1",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100030161.1.17",
          "form": "야 그거 아무도 안먹는데 너 먹을 수 있어?",
          "original_form": "야 그거 아무도 안먹는데 너 먹을 수 있어?",
          "speaker_id": "2",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100030161.1.18",
          "form": "다",
          "original_form": "다",
          "speaker_id": "2",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100030161.1.19",
          "form": "양이 많진 않지만 꾸준히 먹어서 다 먹어치울거니까 걱정마슈",
          "original_form": "양이 많진 않지만 꾸준히 먹어서 다 먹어치울거니까 걱정마슈",
          "speaker_id": "1",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100030161.1.20",
          "form": "아이고 먹고 살겠다 살겠어",
          "original_form": "아이고 먹고 살겠다 살겠어",
          "speaker_id": "2",
          "time": "20210903 22:45"
        }
      ]
