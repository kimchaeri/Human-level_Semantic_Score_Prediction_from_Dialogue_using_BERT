      "utterance": [
        {
          "id": "MDRW2100008296.1.1",
          "form": "봉사 하고 계세요?",
          "original_form": "봉사 하고 계세요?",
          "speaker_id": "1",
          "time": "20210823 13:02"
        },
        {
          "id": "MDRW2100008296.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210823 13:22"
        },
        {
          "id": "MDRW2100008296.1.3",
          "form": "봉사 하는거 있으세요?",
          "original_form": "봉사 하는거 있으세요?",
          "speaker_id": "2",
          "time": "20210823 13:22"
        },
        {
          "id": "MDRW2100008296.1.4",
          "form": "아니요 저도 없어요 ㅎ..",
          "original_form": "아니요 저도 없어요 ㅎ..",
          "speaker_id": "1",
          "time": "20210823 13:48"
        },
        {
          "id": "MDRW2100008296.1.5",
          "form": "마음은 해야지 하는데",
          "original_form": "마음은 해야지 하는데",
          "speaker_id": "1",
          "time": "20210823 13:48"
        },
        {
          "id": "MDRW2100008296.1.6",
          "form": "막상 시간이 없더라구요",
          "original_form": "막상 시간이 없더라구요",
          "speaker_id": "1",
          "time": "20210823 13:49"
        },
        {
          "id": "MDRW2100008296.1.7",
          "form": "맞아요 무언가를 시간 돼서 한다는 건 참 어려운 일 같애요",
          "original_form": "맞아요 무언가를 시간 돼서 한다는 건 참 어려운 일 같애요",
          "speaker_id": "2",
          "time": "20210823 14:14"
        },
        {
          "id": "MDRW2100008296.1.8",
          "form": "혹시 기회가 된다면 봉사할 생각은 있으신가요?",
          "original_form": "혹시 기회가 된다면 봉사할 생각은 있으신가요?",
          "speaker_id": "2",
          "time": "20210823 14:16"
        },
        {
          "id": "MDRW2100008296.1.9",
          "form": "네 한번쯤은 유기보호소에서",
          "original_form": "네 한번쯤은 유기보호소에서",
          "speaker_id": "1",
          "time": "20210823 14:30"
        },
        {
          "id": "MDRW2100008296.1.10",
          "form": "봉사 해보고싶어요",
          "original_form": "봉사 해보고싶어요",
          "speaker_id": "1",
          "time": "20210823 14:30"
        },
        {
          "id": "MDRW2100008296.1.11",
          "form": "유기견 센터 말씀하시는 거죠",
          "original_form": "유기견 센터 말씀하시는 거죠",
          "speaker_id": "2",
          "time": "20210823 14:31"
        },
        {
          "id": "MDRW2100008296.1.12",
          "form": "특별히 유기견 센터에서 봉사하고 싶은 이유가 있나요",
          "original_form": "특별히 유기견 센터에서 봉사하고 싶은 이유가 있나요",
          "speaker_id": "2",
          "time": "20210823 14:32"
        },
        {
          "id": "MDRW2100008296.1.13",
          "form": "약간 유기견센터하면 마음이 아파서",
          "original_form": "약간 유기견센터하면 마음이 아파서",
          "speaker_id": "2",
          "time": "20210823 15:13"
        },
        {
          "id": "MDRW2100008296.1.14",
          "form": "비겁한 변명인거 아는데 도움이 필요한건 아는데 선뜻 나서기가 겁나더라구요",
          "original_form": "비겁한 변명인거 아는데 도움이 필요한건 아는데 선뜻 나서기가 겁나더라구요",
          "speaker_id": "2",
          "time": "20210823 15:14"
        },
        {
          "id": "MDRW2100008296.1.15",
          "form": "그래서 직접 가지는 못하고 가끔 해피빈 같은데 보고 기부는 하는편인거 같아요",
          "original_form": "그래서 직접 가지는 못하고 가끔 해피빈 같은데 보고 기부는 하는편인거 같아요",
          "speaker_id": "2",
          "time": "20210823 15:14"
        },
        {
          "id": "MDRW2100008296.1.16",
          "form": "오 기부 하는 거 부터가",
          "original_form": "오 기부 하는 거 부터가",
          "speaker_id": "1",
          "time": "20210823 15:23"
        },
        {
          "id": "MDRW2100008296.1.17",
          "form": "멋있는거죠!!",
          "original_form": "멋있는거죠!!",
          "speaker_id": "1",
          "time": "20210823 15:23"
        },
        {
          "id": "MDRW2100008296.1.18",
          "form": "뭔가 작은 도움이라도 된다는게",
          "original_form": "뭔가 작은 도움이라도 된다는게",
          "speaker_id": "2",
          "time": "20210823 15:23"
        },
        {
          "id": "MDRW2100008296.1.19",
          "form": "좋은거 같아요",
          "original_form": "좋은거 같아요",
          "speaker_id": "2",
          "time": "20210823 15:23"
        },
        {
          "id": "MDRW2100008296.1.20",
          "form": "시간 나실 때 해피빈 같은 곳 둘러보세요",
          "original_form": "시간 나실 때 해피빈 같은 곳 둘러보세요",
          "speaker_id": "2",
          "time": "20210823 15:23"
        },
        {
          "id": "MDRW2100008296.1.21",
          "form": "ㅎㅎ ㅋ 그래야겠어요",
          "original_form": "ㅎㅎ ㅋ 그래야겠어요",
          "speaker_id": "1",
          "time": "20210823 15:31"
        },
        {
          "id": "MDRW2100008296.1.22",
          "form": "나름 뿌듯할 거 같아요",
          "original_form": "나름 뿌듯할 거 같아요",
          "speaker_id": "1",
          "time": "20210823 15:32"
        },
        {
          "id": "MDRW2100008296.1.23",
          "form": "맞아요 거창한 금액이 아니어도 도움을 줄 수 있는거니까요",
          "original_form": "맞아요 거창한 금액이 아니어도 도움을 줄 수 있는거니까요",
          "speaker_id": "2",
          "time": "20210823 15:34"
        },
        {
          "id": "MDRW2100008296.1.24",
          "form": "그 행위 만으로 의미가 있는거죠",
          "original_form": "그 행위 만으로 의미가 있는거죠",
          "speaker_id": "2",
          "time": "20210823 15:34"
        },
        {
          "id": "MDRW2100008296.1.25",
          "form": "그렇죠 기부를 한다는 자체가",
          "original_form": "그렇죠 기부를 한다는 자체가",
          "speaker_id": "1",
          "time": "20210823 15:36"
        },
        {
          "id": "MDRW2100008296.1.26",
          "form": "일단 보람 있으니까요!",
          "original_form": "일단 보람 있으니까요!",
          "speaker_id": "1",
          "time": "20210823 15:36"
        }
      ]
    }
  ]
