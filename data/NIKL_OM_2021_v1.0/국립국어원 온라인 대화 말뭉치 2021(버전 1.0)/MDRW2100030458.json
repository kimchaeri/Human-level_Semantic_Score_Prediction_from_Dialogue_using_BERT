      "utterance": [
        {
          "id": "MDRW2100030458.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210904 01:24"
        },
        {
          "id": "MDRW2100030458.1.2",
          "form": "안녕하세요 가족이랑 같이 사시나요?",
          "original_form": "안녕하세요 가족이랑 같이 사시나요?",
          "speaker_id": "2",
          "time": "20210904 01:24"
        },
        {
          "id": "MDRW2100030458.1.3",
          "form": "네. name2님은요?",
          "original_form": "네. &name2&님은요?",
          "speaker_id": "1",
          "time": "20210904 01:24"
        },
        {
          "id": "MDRW2100030458.1.4",
          "form": "저는 부모님이랑 같이 살아요",
          "original_form": "저는 부모님이랑 같이 살아요",
          "speaker_id": "2",
          "time": "20210904 01:25"
        },
        {
          "id": "MDRW2100030458.1.5",
          "form": "저도 그래요",
          "original_form": "저도 그래요",
          "speaker_id": "1",
          "time": "20210904 01:25"
        },
        {
          "id": "MDRW2100030458.1.6",
          "form": "형제자매가 있으신가요?",
          "original_form": "형제자매가 있으신가요?",
          "speaker_id": "1",
          "time": "20210904 01:25"
        },
        {
          "id": "MDRW2100030458.1.7",
          "form": "동생이 한 명 있어요",
          "original_form": "동생이 한 명 있어요",
          "speaker_id": "2",
          "time": "20210904 01:26"
        },
        {
          "id": "MDRW2100030458.1.8",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "1",
          "time": "20210904 01:26"
        },
        {
          "id": "MDRW2100030458.1.9",
          "form": "저는 언니가 한명있어요",
          "original_form": "저는 언니가 한명있어요",
          "speaker_id": "1",
          "time": "20210904 01:26"
        },
        {
          "id": "MDRW2100030458.1.10",
          "form": "사이가 좋나요?",
          "original_form": "사이가 좋나요?",
          "speaker_id": "2",
          "time": "20210904 01:26"
        },
        {
          "id": "MDRW2100030458.1.11",
          "form": "나이차이가 많이 나서 그런지 사이는 좋은 것 같아요",
          "original_form": "나이차이가 많이 나서 그런지 사이는 좋은 것 같아요",
          "speaker_id": "1",
          "time": "20210904 01:27"
        },
        {
          "id": "MDRW2100030458.1.12",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210904 01:27"
        },
        {
          "id": "MDRW2100030458.1.13",
          "form": "저는 10살 차이인데 서로 가치관이 달라서 잘 부딪혀요",
          "original_form": "저는 10살 차이인데 서로 가치관이 달라서 잘 부딪혀요",
          "speaker_id": "2",
          "time": "20210904 01:27"
        },
        {
          "id": "MDRW2100030458.1.14",
          "form": "헉 10살차이면 초등학생이나 중학생 아닌가요?",
          "original_form": "헉 10살차이면 초등학생이나 중학생 아닌가요?",
          "speaker_id": "1",
          "time": "20210904 01:28"
        },
        {
          "id": "MDRW2100030458.1.15",
          "form": "그정도면 보통 언니 말을 많이 따르던데",
          "original_form": "그정도면 보통 언니 말을 많이 따르던데",
          "speaker_id": "1",
          "time": "20210904 01:28"
        },
        {
          "id": "MDRW2100030458.1.16",
          "form": "네 곧 중학교 들어가죠",
          "original_form": "네 곧 중학교 들어가죠",
          "speaker_id": "2",
          "time": "20210904 01:28"
        },
        {
          "id": "MDRW2100030458.1.17",
          "form": "지금 애들 보면 딱히 그런거 같지는 않아요. 제 주변도 비슷한 고민을 안고 있고요",
          "original_form": "지금 애들 보면 딱히 그런거 같지는 않아요. 제 주변도 비슷한 고민을 안고 있고요",
          "speaker_id": "2",
          "time": "20210904 01:28"
        },
        {
          "id": "MDRW2100030458.1.18",
          "form": "아하ㅋㅋㅋ...",
          "original_form": "아하ㅋㅋㅋ...",
          "speaker_id": "1",
          "time": "20210904 01:29"
        },
        {
          "id": "MDRW2100030458.1.19",
          "form": "동생이 자기주장이 강한가봐요",
          "original_form": "동생이 자기주장이 강한가봐요",
          "speaker_id": "1",
          "time": "20210904 01:29"
        },
        {
          "id": "MDRW2100030458.1.20",
          "form": "동생입장에서는 은근 언니가 있으니까 많이 든든하던데",
          "original_form": "동생입장에서는 은근 언니가 있으니까 많이 든든하던데",
          "speaker_id": "1",
          "time": "20210904 01:30"
        },
        {
          "id": "MDRW2100030458.1.21",
          "form": "그럴 나이죠. 이해는 하지만 저는 안 그래봐서 잘 모르겠네요",
          "original_form": "그럴 나이죠. 이해는 하지만 저는 안 그래봐서 잘 모르겠네요",
          "speaker_id": "2",
          "time": "20210904 01:30"
        },
        {
          "id": "MDRW2100030458.1.22",
          "form": "내심 많이 의지하고있을 수도 있을것같아요",
          "original_form": "내심 많이 의지하고있을 수도 있을것같아요",
          "speaker_id": "1",
          "time": "20210904 01:30"
        },
        {
          "id": "MDRW2100030458.1.23",
          "form": "그렇게 느끼는건 맞는것 같기도 해요. 무슨 문제가 생기면 저한테 달려오거든요 ㅎㅎ",
          "original_form": "그렇게 느끼는건 맞는것 같기도 해요. 무슨 문제가 생기면 저한테 달려오거든요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 01:30"
        },
        {
          "id": "MDRW2100030458.1.24",
          "form": "우와ㅋㅋㅋㅋㅋ",
          "original_form": "우와ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:31"
        },
        {
          "id": "MDRW2100030458.1.25",
          "form": "엄청 믿음직한 언니인가봐요",
          "original_form": "엄청 믿음직한 언니인가봐요",
          "speaker_id": "1",
          "time": "20210904 01:31"
        }
      ]
