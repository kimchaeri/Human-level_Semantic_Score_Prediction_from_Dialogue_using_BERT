      "utterance": [
        {
          "id": "MDRW2100009757.1.1",
          "form": "혹시 현재 대학 재학중이신가요?",
          "original_form": "혹시 현재 대학 재학중이신가요?",
          "speaker_id": "1",
          "time": "20210823 09:31"
        },
        {
          "id": "MDRW2100009757.1.2",
          "form": "안녕하세요  대학 졸업한지 얼마 안됐고 9월에 대학원 입학예정입니다",
          "original_form": "안녕하세요  대학 졸업한지 얼마 안됐고 9월에 대학원 입학예정입니다",
          "speaker_id": "2",
          "time": "20210823 10:28"
        },
        {
          "id": "MDRW2100009757.1.3",
          "form": "대학 재학중이신가요",
          "original_form": "대학 재학중이신가요",
          "speaker_id": "2",
          "time": "20210823 10:28"
        },
        {
          "id": "MDRW2100009757.1.4",
          "form": "올해 입학한 따끈따끈한 새내기입니다",
          "original_form": "올해 입학한 따끈따끈한 새내기입니다",
          "speaker_id": "1",
          "time": "20210823 10:30"
        },
        {
          "id": "MDRW2100009757.1.5",
          "form": "코시국 새내기시군요...",
          "original_form": "코시국 새내기시군요...",
          "speaker_id": "2",
          "time": "20210823 10:33"
        },
        {
          "id": "MDRW2100009757.1.6",
          "form": "등교하신 적 많으세요?",
          "original_form": "등교하신 적 많으세요?",
          "speaker_id": "2",
          "time": "20210823 10:33"
        },
        {
          "id": "MDRW2100009757.1.7",
          "form": "대면 비대면 반반이었어요",
          "original_form": "대면 비대면 반반이었어요",
          "speaker_id": "1",
          "time": "20210823 10:48"
        },
        {
          "id": "MDRW2100009757.1.8",
          "form": "아 그럼 그래도 학교 좀 아시겠네요",
          "original_form": "아 그럼 그래도 학교 좀 아시겠네요",
          "speaker_id": "2",
          "time": "20210823 10:49"
        },
        {
          "id": "MDRW2100009757.1.9",
          "form": "학교 거의 안가본 친구도 많더라구요",
          "original_form": "학교 거의 안가본 친구도 많더라구요",
          "speaker_id": "2",
          "time": "20210823 10:49"
        },
        {
          "id": "MDRW2100009757.1.10",
          "form": "시험 제외 아예 비대면인 학교 다닌 친구는 등록금 아깝다고 하더라구요",
          "original_form": "시험 제외 아예 비대면인 학교 다닌 친구는 등록금 아깝다고 하더라구요",
          "speaker_id": "1",
          "time": "20210823 11:10"
        },
        {
          "id": "MDRW2100009757.1.11",
          "form": "사이버대학교인데 등록금 내기 진짜 아깝겠어요",
          "original_form": "사이버대학교인데 등록금 내기 진짜 아깝겠어요",
          "speaker_id": "2",
          "time": "20210823 11:31"
        },
        {
          "id": "MDRW2100009757.1.12",
          "form": "학교 인프라도 활용 못하구",
          "original_form": "학교 인프라도 활용 못하구",
          "speaker_id": "2",
          "time": "20210823 11:32"
        },
        {
          "id": "MDRW2100009757.1.13",
          "form": "인프라도 활용 못하고 동아리 활동도 못해봤어요",
          "original_form": "인프라도 활용 못하고 동아리 활동도 못해봤어요",
          "speaker_id": "1",
          "time": "20210823 11:40"
        },
        {
          "id": "MDRW2100009757.1.14",
          "form": "아 동아리 ㅜㅜ",
          "original_form": "아 동아리 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 11:40"
        },
        {
          "id": "MDRW2100009757.1.15",
          "form": "저도 연극부였는데 요즘 활동 아예 못한다더라구요",
          "original_form": "저도 연극부였는데 요즘 활동 아예 못한다더라구요",
          "speaker_id": "2",
          "time": "20210823 11:40"
        },
        {
          "id": "MDRW2100009757.1.16",
          "form": "연극하셨다니 멋있어요 저는 탁구부 정도 하려고 했는데",
          "original_form": "연극하셨다니 멋있어요 저는 탁구부 정도 하려고 했는데",
          "speaker_id": "1",
          "time": "20210823 11:50"
        },
        {
          "id": "MDRW2100009757.1.17",
          "form": "탁구부도 못하겠네요 요새",
          "original_form": "탁구부도 못하겠네요 요새",
          "speaker_id": "2",
          "time": "20210823 11:51"
        },
        {
          "id": "MDRW2100009757.1.18",
          "form": "뭐 학술분과 같은거나 제대로 활동하려나요?",
          "original_form": "뭐 학술분과 같은거나 제대로 활동하려나요?",
          "speaker_id": "2",
          "time": "20210823 11:51"
        },
        {
          "id": "MDRW2100009757.1.19",
          "form": "그냥 동아리 전체가 멈춘거 같더라구요",
          "original_form": "그냥 동아리 전체가 멈춘거 같더라구요",
          "speaker_id": "1",
          "time": "20210823 11:51"
        },
        {
          "id": "MDRW2100009757.1.20",
          "form": "기대 많이하셨을텐ㄴ데 아쉽겠어요 ㅜ",
          "original_form": "기대 많이하셨을텐ㄴ데 아쉽겠어요 ㅜ",
          "speaker_id": "2",
          "time": "20210823 11:55"
        }
      ]
