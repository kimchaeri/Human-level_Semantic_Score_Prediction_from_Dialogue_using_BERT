      "utterance": [
        {
          "id": "MDRW2100023754.1.1",
          "form": "우리 오늘 와플 먹기로 했잖아",
          "original_form": "우리 오늘 와플 먹기로 했잖아",
          "speaker_id": "2",
          "time": "20210830 17:32"
        },
        {
          "id": "MDRW2100023754.1.2",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210830 17:32"
        },
        {
          "id": "MDRW2100023754.1.3",
          "form": "또 까먹었네",
          "original_form": "또 까먹었네",
          "speaker_id": "1",
          "time": "20210830 17:32"
        },
        {
          "id": "MDRW2100023754.1.4",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 17:32"
        },
        {
          "id": "MDRW2100023754.1.5",
          "form": "어떤 와플 먹고 싶어?",
          "original_form": "어떤 와플 먹고 싶어?",
          "speaker_id": "2",
          "time": "20210830 17:32"
        },
        {
          "id": "MDRW2100023754.1.6",
          "form": "난 약간 달콤한 와플 먹고 싶어",
          "original_form": "난 약간 달콤한 와플 먹고 싶어",
          "speaker_id": "1",
          "time": "20210830 17:32"
        },
        {
          "id": "MDRW2100023754.1.7",
          "form": "와플은 다 달콤해",
          "original_form": "와플은 다 달콤해",
          "speaker_id": "2",
          "time": "20210830 17:32"
        },
        {
          "id": "MDRW2100023754.1.8",
          "form": "근데 많이 단 와플도 있고",
          "original_form": "근데 많이 단 와플도 있고",
          "speaker_id": "1",
          "time": "20210830 17:33"
        },
        {
          "id": "MDRW2100023754.1.9",
          "form": "덜 단 와플도 있잖아",
          "original_form": "덜 단 와플도 있잖아",
          "speaker_id": "1",
          "time": "20210830 17:33"
        },
        {
          "id": "MDRW2100023754.1.10",
          "form": "그게 이제 토핑에 따라 많이 달라지지",
          "original_form": "그게 이제 토핑에 따라 많이 달라지지",
          "speaker_id": "2",
          "time": "20210830 17:33"
        },
        {
          "id": "MDRW2100023754.1.11",
          "form": "와플에 토핑이 많이 들어가나..?",
          "original_form": "와플에 토핑이 많이 들어가나..?",
          "speaker_id": "1",
          "time": "20210830 17:33"
        },
        {
          "id": "MDRW2100023754.1.12",
          "form": "그럼 빵만 먹는 사람이 어딨어",
          "original_form": "그럼 빵만 먹는 사람이 어딨어",
          "speaker_id": "2",
          "time": "20210830 17:34"
        },
        {
          "id": "MDRW2100023754.1.13",
          "form": "그건 그렇지;;",
          "original_form": "그건 그렇지;;",
          "speaker_id": "1",
          "time": "20210830 17:34"
        },
        {
          "id": "MDRW2100023754.1.14",
          "form": "그건 그렇지;;",
          "original_form": "그건 그렇지;;",
          "speaker_id": "1",
          "time": "20210830 17:35"
        },
        {
          "id": "MDRW2100023754.1.15",
          "form": "나는 젤라또 와플 먹고 싶어",
          "original_form": "나는 젤라또 와플 먹고 싶어",
          "speaker_id": "2",
          "time": "20210830 17:35"
        },
        {
          "id": "MDRW2100023754.1.16",
          "form": "젤라토가 머야?",
          "original_form": "젤라토가 머야?",
          "speaker_id": "1",
          "time": "20210830 17:36"
        },
        {
          "id": "MDRW2100023754.1.17",
          "form": "쫀득한 아이스크림",
          "original_form": "쫀득한 아이스크림",
          "speaker_id": "2",
          "time": "20210830 17:36"
        },
        {
          "id": "MDRW2100023754.1.18",
          "form": "너무 달지 않나?",
          "original_form": "너무 달지 않나?",
          "speaker_id": "1",
          "time": "20210830 17:36"
        },
        {
          "id": "MDRW2100023754.1.19",
          "form": "막 너무 달아서 못 먹겠고 이런 건 아닌데",
