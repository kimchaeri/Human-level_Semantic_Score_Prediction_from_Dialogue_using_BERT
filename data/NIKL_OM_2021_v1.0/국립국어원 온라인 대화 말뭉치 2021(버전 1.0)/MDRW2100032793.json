      "utterance": [
        {
          "id": "MDRW2100032793.1.1",
          "form": "진짜 저 노래도 다 알고.. 다 좋아하고..말해뭐해요 아이유잖아툐,,",
          "original_form": "진짜 저 노래도 다 알고.. 다 좋아하고..말해뭐해요 아이유잖아툐,,",
          "speaker_id": "2",
          "time": "20210905 23:43"
        },
        {
          "id": "MDRW2100032793.1.2",
          "form": "ㅠ ㅠㅠㅠㅜㅜㅜ",
          "original_form": "ㅠ ㅠㅠㅠㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 23:43"
        },
        {
          "id": "MDRW2100032793.1.3",
          "form": "긍까요!!!!! 대중들도 많이 좋아하니까!!!!",
          "original_form": "긍까요!!!!! 대중들도 많이 좋아하니까!!!!",
          "speaker_id": "2",
          "time": "20210905 23:43"
        },
        {
          "id": "MDRW2100032793.1.4",
          "form": "진짜 3단고음 대박..",
          "original_form": "진짜 3단고음 대박..",
          "speaker_id": "2",
          "time": "20210905 23:43"
        },
        {
          "id": "MDRW2100032793.1.5",
          "form": "제가 막 엄청 좋아해도 ㅇㅋ고 엄청 얌전해도 ㅇㅋㅇㅋ인 느낌",
          "original_form": "제가 막 엄청 좋아해도 ㅇㅋ고 엄청 얌전해도 ㅇㅋㅇㅋ인 느낌",
          "speaker_id": "1",
          "time": "20210905 23:44"
        },
        {
          "id": "MDRW2100032793.1.6",
          "form": "귀에서 성수흐르시겠어요,,",
          "original_form": "귀에서 성수흐르시겠어요,,",
          "speaker_id": "2",
          "time": "20210905 23:44"
        },
        {
          "id": "MDRW2100032793.1.7",
          "form": "아이유 콘은 좀 하이브리드로 태도를 변경 가능해요",
          "original_form": "아이유 콘은 좀 하이브리드로 태도를 변경 가능해요",
          "speaker_id": "1",
          "time": "20210905 23:44"
        },
        {
          "id": "MDRW2100032793.1.8",
          "form": "아 뭔느낌인지 알겠아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 뭔느낌인지 알겠아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:44"
        },
        {
          "id": "MDRW2100032793.1.9",
          "form": "그래서 아 아이유 뽕차오른다 하면 우와아아아각 하고",
          "original_form": "그래서 아 아이유 뽕차오른다 하면 우와아아아각 하고",
          "speaker_id": "1",
          "time": "20210905 23:44"
        },
        {
          "id": "MDRW2100032793.1.10",
          "form": "완전 덕후들만 모인곳에 나는 덜 덕훈데..가면.. 진짜 나혼자만 갑분싸되거든요,,",
          "original_form": "완전 덕후들만 모인곳에 나는 덜 덕훈데..가면.. 진짜 나혼자만 갑분싸되거든요,,",
          "speaker_id": "2",
          "time": "20210905 23:44"
        },
        {
          "id": "MDRW2100032793.1.11",
          "form": "하..",
          "original_form": "하..",
          "speaker_id": "2",
          "time": "20210905 23:44"
        },
        {
          "id": "MDRW2100032793.1.12",
          "form": "진짜 대박일거같아요...",
          "original_form": "진짜 대박일거같아요...",
          "speaker_id": "2",
          "time": "20210905 23:44"
        },
        {
          "id": "MDRW2100032793.1.13",
          "form": "라이브로 듣고 싶은 곡 너무많음데ㅜㅠ",
          "original_form": "라이브로 듣고 싶은 곡 너무많음데ㅜㅠ",
          "speaker_id": "2",
          "time": "20210905 23:44"
        },
        {
          "id": "MDRW2100032793.1.14",
          "form": "아 좀 힘든데? 하면",
          "original_form": "아 좀 힘든데? 하면",
          "speaker_id": "1",
          "time": "20210905 23:45"
        },
        {
          "id": "MDRW2100032793.1.15",
          "form": "갱장한 리스너에 빙의해서",
          "original_form": "갱장한 리스너에 빙의해서",
          "speaker_id": "1",
          "time": "20210905 23:45"
        },
        {
          "id": "MDRW2100032793.1.16",
          "form": "눈감고 듣는척 쉬면 조아요",
          "original_form": "눈감고 듣는척 쉬면 조아요",
          "speaker_id": "1",
          "time": "20210905 23:45"
        },
        {
          "id": "MDRW2100032793.1.17",
          "form": "아 약간 나는 노래를 음미하면서 듣는다",
          "original_form": "아 약간 나는 노래를 음미하면서 듣는다",
          "speaker_id": "2",
          "time": "20210905 23:45"
        },
        {
          "id": "MDRW2100032793.1.18",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210905 23:45"
        },
        {
          "id": "MDRW2100032793.1.19",
          "form": "진짜 개좋을거같아요,,,",
          "original_form": "진짜 개좋을거같아요,,,",
          "speaker_id": "2",
          "time": "20210905 23:45"
        },
        {
          "id": "MDRW2100032793.1.20",
          "form": "노래도 솔까 다 아는 곡이라",
          "original_form": "노래도 솔까 다 아는 곡이라",
          "speaker_id": "1",
          "time": "20210905 23:45"
        },
        {
          "id": "MDRW2100032793.1.21",
          "form": "쳐지지가 아나요",
          "original_form": "쳐지지가 아나요",
          "speaker_id": "1",
          "time": "20210905 23:45"
        },
        {
          "id": "MDRW2100032793.1.22",
          "form": "맞아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:45"
        },
        {
          "id": "MDRW2100032793.1.23",
          "form": "ㅋㅋㅋㅋㅋ진짜 다 알아서 와.. 나 사실 유애나인가?",
          "original_form": "ㅋㅋㅋㅋㅋ진짜 다 알아서 와.. 나 사실 유애나인가?",
          "speaker_id": "1",
          "time": "20210905 23:46"
        },
        {
          "id": "MDRW2100032793.1.24",
          "form": "(아님)",
          "original_form": "(아님)",
          "speaker_id": "1",
          "time": "20210905 23:46"
        },
        {
          "id": "MDRW2100032793.1.25",
          "form": "아 개웃겨욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 개웃겨욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:46"
        },
        {
          "id": "MDRW2100032793.1.26",
          "form": "이런 착각으로 잘 즐길 수 있어요",
          "original_form": "이런 착각으로 잘 즐길 수 있어요",
          "speaker_id": "1",
          "time": "20210905 23:46"
        },
        {
          "id": "MDRW2100032793.1.27",
          "form": "거의 마음으론",
          "original_form": "거의 마음으론",
          "speaker_id": "2",
          "time": "20210905 23:46"
        },
        {
          "id": "MDRW2100032793.1.28",
          "form": "유애나하고 친구먹었다",
          "original_form": "유애나하고 친구먹었다",
          "speaker_id": "2",
          "time": "20210905 23:46"
        },
        {
          "id": "MDRW2100032793.1.29",
          "form": "그래서 진짜 추천입니당",
          "original_form": "그래서 진짜 추천입니당",
          "speaker_id": "1",
          "time": "20210905 23:46"
        },
        {
          "id": "MDRW2100032793.1.30",
          "form": "앵콜 앵앵콜 그냥 서비스 막 줘요",
          "original_form": "앵콜 앵앵콜 그냥 서비스 막 줘요",
          "speaker_id": "1",
          "time": "20210905 23:46"
        },
        {
          "id": "MDRW2100032793.1.31",
          "form": "그래서 저 도중에 나갔어요",
          "original_form": "그래서 저 도중에 나갔어요",
