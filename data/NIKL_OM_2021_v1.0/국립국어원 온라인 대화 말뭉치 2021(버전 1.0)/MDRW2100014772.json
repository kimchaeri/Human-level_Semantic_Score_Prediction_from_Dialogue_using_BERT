        },
        {
          "id": "MDRW2100014772.1.2",
          "form": "선선하지 않았어?",
          "original_form": "선선하지 않았어?",
          "speaker_id": "1",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014772.1.3",
          "form": "맞아 요즘",
          "original_form": "맞아 요즘",
          "speaker_id": "2",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014772.1.4",
          "form": "날씨 완전 시원함",
          "original_form": "날씨 완전 시원함",
          "speaker_id": "2",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014772.1.5",
          "form": "잘때 선풍기도 안틀어",
          "original_form": "잘때 선풍기도 안틀어",
          "speaker_id": "2",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014772.1.6",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014772.1.7",
          "form": "며칠 비오더니",
          "original_form": "며칠 비오더니",
          "speaker_id": "1",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014772.1.8",
          "form": "시원해짐",
          "original_form": "시원해짐",
          "speaker_id": "1",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014772.1.9",
          "form": "이제 가을이 오는듯??",
          "original_form": "이제 가을이 오는듯??",
          "speaker_id": "1",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014772.1.10",
          "form": "ㅇㅈ",
          "original_form": "ㅇㅈ",
          "speaker_id": "2",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014772.1.11",
          "form": "아까 밖에 나갔을때",
          "original_form": "아까 밖에 나갔을때",
          "speaker_id": "2",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014772.1.12",
          "form": "진짜 가을인줄알았어",
          "original_form": "진짜 가을인줄알았어",
          "speaker_id": "2",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014772.1.13",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014772.1.14",
          "form": "맨날",
          "original_form": "맨날",
          "speaker_id": "1",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014772.1.15",
          "form": "팔 걷고 다녔는데",
          "original_form": "팔 걷고 다녔는데",
          "speaker_id": "1",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014772.1.16",
          "form": "그저께부턴가",
          "original_form": "그저께부턴가",
          "speaker_id": "1",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014772.1.17",
          "form": "아 요즘 반팔입으면 좀 춥더라",
          "original_form": "아 요즘 반팔입으면 좀 춥더라",
          "speaker_id": "2",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014772.1.18",
          "form": "저번에 전시회 보러갔을때",
          "original_form": "저번에 전시회 보러갔을때",
          "speaker_id": "2",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014772.1.19",
          "form": "팔 내리고 다녀",
          "original_form": "팔 내리고 다녀",
          "speaker_id": "1",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014772.1.20",
          "form": "끝날때쯤은 거의",
          "original_form": "끝날때쯤은 거의",
          "speaker_id": "2",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014772.1.21",
          "form": "덜덜 떨면서봄",
          "original_form": "덜덜 떨면서봄",
          "speaker_id": "2",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014772.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 넌 아예 긴팔입고다니냐",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 넌 아예 긴팔입고다니냐",
          "speaker_id": "2",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014772.1.23",
          "form": "요즘 에어컨틀면 좀 추울정도야",
          "original_form": "요즘 에어컨틀면 좀 추울정도야",
          "speaker_id": "2",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014772.1.24",
          "form": "더위를 안타기때문에",
          "original_form": "더위를 안타기때문에",
          "speaker_id": "1",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014772.1.25",
          "form": "바로 긴팔행",
          "original_form": "바로 긴팔행",
          "speaker_id": "1",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014772.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014772.1.27",
          "form": "혹시 여름에 태어나섰어요",
          "original_form": "혹시 여름에 태어나섰어요",
          "speaker_id": "2",
          "time": "20210825 20:42"
