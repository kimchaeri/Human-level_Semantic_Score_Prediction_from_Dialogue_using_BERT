      "utterance": [
        {
          "id": "MDRW2100041464.1.1",
          "form": "그거 아직도 하는 사람 많아",
          "original_form": "그거 아직도 하는 사람 많아",
          "speaker_id": "2",
          "time": "20210906 00:43"
        },
        {
          "id": "MDRW2100041464.1.2",
          "form": "ㅋㅋㅋ인기는 많지",
          "original_form": "ㅋㅋㅋ인기는 많지",
          "speaker_id": "1",
          "time": "20210906 00:44"
        },
        {
          "id": "MDRW2100041464.1.3",
          "form": "내 친구도 아직까지 해",
          "original_form": "내 친구도 아직까지 해",
          "speaker_id": "1",
          "time": "20210906 00:44"
        },
        {
          "id": "MDRW2100041464.1.4",
          "form": "걔도 엄청 고렙일걸...",
          "original_form": "걔도 엄청 고렙일걸...",
          "speaker_id": "1",
          "time": "20210906 00:44"
        },
        {
          "id": "MDRW2100041464.1.5",
          "form": "지금까지 하고 있으면 당연히 그러겠지",
          "original_form": "지금까지 하고 있으면 당연히 그러겠지",
          "speaker_id": "2",
          "time": "20210906 00:44"
        },
        {
          "id": "MDRW2100041464.1.6",
          "form": "메이플도 엄청 오래 게임 운영한다",
          "original_form": "메이플도 엄청 오래 게임 운영한다",
          "speaker_id": "2",
          "time": "20210906 00:45"
        },
        {
          "id": "MDRW2100041464.1.7",
          "form": "아직 하는 사람이 많으니까 게임을 종료시켜버릴 수가 없겠지",
          "original_form": "아직 하는 사람이 많으니까 게임을 종료시켜버릴 수가 없겠지",
          "speaker_id": "1",
          "time": "20210906 00:45"
        },
        {
          "id": "MDRW2100041464.1.8",
          "form": "그거 현질 많이 한 사람들이 한둘이 아니라ㅋㅋㅋ",
          "original_form": "그거 현질 많이 한 사람들이 한둘이 아니라ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 00:45"
        },
        {
          "id": "MDRW2100041464.1.9",
          "form": "그렇네. 그거 캐시템이 많았던 것 같아",
          "original_form": "그렇네. 그거 캐시템이 많았던 것 같아",
          "speaker_id": "2",
          "time": "20210906 00:45"
        },
        {
          "id": "MDRW2100041464.1.10",
          "form": "캐릭터 옷이며 얼굴도 다 캐시로 바꿀 수 있고.",
          "original_form": "캐릭터 옷이며 얼굴도 다 캐시로 바꿀 수 있고.",
          "speaker_id": "2",
          "time": "20210906 00:46"
        },
        {
          "id": "MDRW2100041464.1.11",
          "form": "맞어.. 나도 초딩 때 메이플 캐릭 헤어스타일 바꾼다고 현질 좀 함",
          "original_form": "맞어.. 나도 초딩 때 메이플 캐릭 헤어스타일 바꾼다고 현질 좀 함",
          "speaker_id": "1",
          "time": "20210906 00:46"
        },
        {
          "id": "MDRW2100041464.1.12",
          "form": "메이플이 사냥하는 게임이었나",
          "original_form": "메이플이 사냥하는 게임이었나",
          "speaker_id": "2",
          "time": "20210906 00:47"
        },
        {
          "id": "MDRW2100041464.1.13",
          "form": "어 몬스터 잡는 게임",
          "original_form": "어 몬스터 잡는 게임",
          "speaker_id": "1",
          "time": "20210906 00:47"
        },
        {
          "id": "MDRW2100041464.1.14",
          "form": "일정 레벨되면 전직해서 직업 가지고..",
          "original_form": "일정 레벨되면 전직해서 직업 가지고..",
          "speaker_id": "1",
          "time": "20210906 00:47"
        },
        {
          "id": "MDRW2100041464.1.15",
          "form": "마법사/전사/궁수/도적 뭐 이렇게ㅋㅋㅋㅋ",
          "original_form": "마법사/전사/궁수/도적 뭐 이렇게ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 00:47"
        },
        {
          "id": "MDRW2100041464.1.16",
          "form": "웃기네. .그 게임이 아직까지 그렇게 잘 기억나?",
          "original_form": "웃기네. .그 게임이 아직까지 그렇게 잘 기억나?",
          "speaker_id": "2",
          "time": "20210906 00:48"
        },
        {
          "id": "MDRW2100041464.1.17",
          "form": "그럼 메이플은 진짜 역대급인기였는데...",
          "original_form": "그럼 메이플은 진짜 역대급인기였는데...",
          "speaker_id": "1",
          "time": "20210906 00:48"
        },
        {
          "id": "MDRW2100041464.1.18",
          "form": "괜히 아직까지 하는 사람이 있는 게 아냐",
          "original_form": "괜히 아직까지 하는 사람이 있는 게 아냐",
          "speaker_id": "1",
          "time": "20210906 00:48"
        },
        {
          "id": "MDRW2100041464.1.19",
          "form": "그렇게 재밌는 게임을 왜 그만뒀어",
          "original_form": "그렇게 재밌는 게임을 왜 그만뒀어",
          "speaker_id": "2",
          "time": "20210906 00:49"
        },
        {
          "id": "MDRW2100041464.1.20",
          "form": "레벨이 올라가면 올라갈수록 렙업도 어렵고",
          "original_form": "레벨이 올라가면 올라갈수록 렙업도 어렵고",
          "speaker_id": "1",
          "time": "20210906 00:49"
        },
        {
          "id": "MDRW2100041464.1.21",
          "form": "퀘스트도 어려워짐...",
          "original_form": "퀘스트도 어려워짐...",
          "speaker_id": "1",
          "time": "20210906 00:49"
        },
        {
          "id": "MDRW2100041464.1.22",
          "form": "하다가 짜중나서 그만뒀어",
          "original_form": "하다가 짜중나서 그만뒀어",
          "speaker_id": "1",
          "time": "20210906 00:49"
        },
        {
          "id": "MDRW2100041464.1.23",
          "form": "게임할 때 즐거워야지 짜증나면 쓰나",
          "original_form": "게임할 때 즐거워야지 짜증나면 쓰나",
          "speaker_id": "2",
          "time": "20210906 00:50"
        },
        {
          "id": "MDRW2100041464.1.24",
          "form": "요즘엔 무슨 게임이 유행이야?",
          "original_form": "요즘엔 무슨 게임이 유행이야?",
          "speaker_id": "2",
          "time": "20210906 00:50"
        },
        {
          "id": "MDRW2100041464.1.25",
          "form": "나도 컴퓨터 게임은 안 한지 오래돼서 몰라",
          "original_form": "나도 컴퓨터 게임은 안 한지 오래돼서 몰라",
          "speaker_id": "1",
          "time": "20210906 00:51"
        },
        {
          "id": "MDRW2100041464.1.26",
          "form": "오버워치나 배그가 아직까지 인기 많을걸",
          "original_form": "오버워치나 배그가 아직까지 인기 많을걸",
          "speaker_id": "1",
          "time": "20210906 00:51"
        }
      ]
    }
