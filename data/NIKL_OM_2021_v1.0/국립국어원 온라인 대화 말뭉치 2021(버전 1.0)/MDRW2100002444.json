      "utterance": [
        {
          "id": "MDRW2100002444.1.1",
          "form": "순천 여행 가보고싶은데",
          "original_form": "순천 여행 가보고싶은데",
          "speaker_id": "1",
          "time": "20210820 12:18"
        },
        {
          "id": "MDRW2100002444.1.2",
          "form": "순천 가보셨어요???",
          "original_form": "순천 가보셨어요???",
          "speaker_id": "1",
          "time": "20210820 12:18"
        },
        {
          "id": "MDRW2100002444.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100002444.1.4",
          "form": "순천 1번가봤어요",
          "original_form": "순천 1번가봤어요",
          "speaker_id": "2",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100002444.1.5",
          "form": "순천만이요",
          "original_form": "순천만이요",
          "speaker_id": "2",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100002444.1.6",
          "form": "헉 어떠셨어요????",
          "original_form": "헉 어떠셨어요????",
          "speaker_id": "1",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100002444.1.7",
          "form": "정원박람회장 들어갔는데 되게 이뻤어요",
          "original_form": "정원박람회장 들어갔는데 되게 이뻤어요",
          "speaker_id": "2",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100002444.1.8",
          "form": "저는 뚜벅이인데",
          "original_form": "저는 뚜벅이인데",
          "speaker_id": "1",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100002444.1.9",
          "form": "뚜벅이가 가기에도",
          "original_form": "뚜벅이가 가기에도",
          "speaker_id": "1",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100002444.1.10",
          "form": "좋나요???",
          "original_form": "좋나요???",
          "speaker_id": "1",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100002444.1.11",
          "form": "부지도 넓고 꽃구경도좋고",
          "original_form": "부지도 넓고 꽃구경도좋고",
          "speaker_id": "2",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100002444.1.12",
          "form": "ㅜㅠㅠ꽃구경이 진짜",
          "original_form": "ㅜㅠㅠ꽃구경이 진짜",
          "speaker_id": "1",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100002444.1.13",
          "form": "이쁠거같아요",
          "original_form": "이쁠거같아요",
          "speaker_id": "1",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100002444.1.14",
          "form": "제가 교통편은 잘 몰라서 그 안에는 걸어서만 이동가능해요",
          "original_form": "제가 교통편은 잘 몰라서 그 안에는 걸어서만 이동가능해요",
          "speaker_id": "2",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100002444.1.15",
          "form": "진짜 힐링그자체",
          "original_form": "진짜 힐링그자체",
          "speaker_id": "2",
          "time": "20210820 12:20"
        },
        {
          "id": "MDRW2100002444.1.16",
          "form": "국내여행지  어디 가보셨나요?",
          "original_form": "국내여행지  어디 가보셨나요?",
          "speaker_id": "2",
          "time": "20210820 12:20"
        },
        {
          "id": "MDRW2100002444.1.17",
          "form": "저는 수도권이나 강원도요!!!!",
          "original_form": "저는 수도권이나 강원도요!!!!",
          "speaker_id": "1",
          "time": "20210820 12:24"
        },
        {
          "id": "MDRW2100002444.1.18",
          "form": "좋았던 곳은 어디였나요",
          "original_form": "좋았던 곳은 어디였나요",
          "speaker_id": "2",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002444.1.19",
          "form": "강릉이요!!!!",
          "original_form": "강릉이요!!!!",
          "speaker_id": "1",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002444.1.20",
          "form": "강릉 맛집많다던데",
          "original_form": "강릉 맛집많다던데",
          "speaker_id": "2",
          "time": "20210820 12:27"
        },
        {
          "id": "MDRW2100002444.1.21",
          "form": "어때요 강릉 가면 힐링되나요",
          "original_form": "어때요 강릉 가면 힐링되나요",
          "speaker_id": "2",
          "time": "20210820 12:27"
        },
        {
          "id": "MDRW2100002444.1.22",
          "form": "강릉 바다가 진짜 이뻐요",
          "original_form": "강릉 바다가 진짜 이뻐요",
          "speaker_id": "1",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002444.1.23",
          "form": "진짜 동해가...너무 맑아서ㅜㅜ",
          "original_form": "진짜 동해가...너무 맑아서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 12:34"
        }
