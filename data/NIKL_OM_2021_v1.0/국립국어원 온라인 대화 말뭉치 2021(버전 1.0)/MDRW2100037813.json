      "utterance": [
        {
          "id": "MDRW2100037813.1.1",
          "form": "고추바사삭 시켜먹을려구요!",
          "original_form": "고추바사삭 시켜먹을려구요!",
          "speaker_id": "2",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100037813.1.2",
          "form": "저녁뭐드시나요?",
          "original_form": "저녁뭐드시나요?",
          "speaker_id": "2",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100037813.1.3",
          "form": "어제 저녁은 양념게장이랑 밥먹구",
          "original_form": "어제 저녁은 양념게장이랑 밥먹구",
          "speaker_id": "1",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100037813.1.4",
          "form": "오늘은 집 가봐여 알아요ㅋㅋ",
          "original_form": "오늘은 집 가봐여 알아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100037813.1.5",
          "form": "양념게장 맛있겠어요ㅠㅠ",
          "original_form": "양념게장 맛있겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 18:56"
        },
        {
          "id": "MDRW2100037813.1.6",
          "form": "부모님이 준비해주시나요?",
          "original_form": "부모님이 준비해주시나요?",
          "speaker_id": "2",
          "time": "20210902 18:56"
        },
        {
          "id": "MDRW2100037813.1.7",
          "form": "엄마가 퇴근시간 맞춰서요",
          "original_form": "엄마가 퇴근시간 맞춰서요",
          "speaker_id": "1",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100037813.1.8",
          "form": "좋으시겠어요",
          "original_form": "좋으시겠어요",
          "speaker_id": "2",
          "time": "20210902 19:02"
        },
        {
          "id": "MDRW2100037813.1.9",
          "form": "ㅎㅎ배고파용 빨리 가야겠어요",
          "original_form": "ㅎㅎ배고파용 빨리 가야겠어요",
          "speaker_id": "1",
          "time": "20210902 19:03"
        },
        {
          "id": "MDRW2100037813.1.10",
          "form": "오늘 저녁 직접 해 드세요?",
          "original_form": "오늘 저녁 직접 해 드세요?",
          "speaker_id": "1",
          "time": "20210902 19:03"
        },
        {
          "id": "MDRW2100037813.1.11",
          "form": "네 직접해먹을려구요",
          "original_form": "네 직접해먹을려구요",
          "speaker_id": "2",
          "time": "20210902 19:04"
        },
        {
          "id": "MDRW2100037813.1.12",
          "form": "저녁 오늘 뭐드세요?",
          "original_form": "저녁 오늘 뭐드세요?",
          "speaker_id": "1",
          "time": "20210902 19:04"
        },
        {
          "id": "MDRW2100037813.1.13",
          "form": "정육점에서 돼지앞다리살 사왔어요",
          "original_form": "정육점에서 돼지앞다리살 사왔어요",
          "speaker_id": "2",
          "time": "20210902 19:04"
        },
        {
          "id": "MDRW2100037813.1.14",
          "form": "구워먹을려구요",
          "original_form": "구워먹을려구요",
          "speaker_id": "2",
          "time": "20210902 19:04"
        },
        {
          "id": "MDRW2100037813.1.15",
          "form": "오 맛있겠다",
          "original_form": "오 맛있겠다",
          "speaker_id": "1",
          "time": "20210902 19:04"
        },
        {
          "id": "MDRW2100037813.1.16",
          "form": "고기고기",
          "original_form": "고기고기",
          "speaker_id": "1",
          "time": "20210902 19:04"
        },
        {
          "id": "MDRW2100037813.1.17",
          "form": "파채랑 양파절임도 같이 먹을거예요",
          "original_form": "파채랑 양파절임도 같이 먹을거예요",
          "speaker_id": "2",
          "time": "20210902 19:05"
        },
        {
          "id": "MDRW2100037813.1.18",
          "form": "고기 쌈장에 찍어드세요?",
          "original_form": "고기 쌈장에 찍어드세요?",
          "speaker_id": "1",
          "time": "20210902 19:07"
        },
        {
          "id": "MDRW2100037813.1.19",
          "form": "네! 쌈장하고 참소스 필수예요",
          "original_form": "네! 쌈장하고 참소스 필수예요",
