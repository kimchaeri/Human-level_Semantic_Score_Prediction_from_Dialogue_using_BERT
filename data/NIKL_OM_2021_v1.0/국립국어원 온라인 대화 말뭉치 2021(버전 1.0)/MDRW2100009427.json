      "utterance": [
        {
          "id": "MDRW2100009427.1.1",
          "form": "저 요즘 무법변호사 보고있어요",
          "original_form": "저 요즘 무법변호사 보고있어요",
          "speaker_id": "1",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100009427.1.2",
          "form": "보셨어요?",
          "original_form": "보셨어요?",
          "speaker_id": "1",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100009427.1.3",
          "form": "심야괴담회 보세요?",
          "original_form": "심야괴담회 보세요?",
          "speaker_id": "1",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100009427.1.4",
          "form": "아 지금 하는군요",
          "original_form": "아 지금 하는군요",
          "speaker_id": "2",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100009427.1.5",
          "form": "네 저도 모르다가",
          "original_form": "네 저도 모르다가",
          "speaker_id": "1",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100009427.1.6",
          "form": "아빠가 알려줘서 보고있어요 ㅋㅋ",
          "original_form": "아빠가 알려줘서 보고있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100009427.1.7",
          "form": "저도 틀어야겠어요",
          "original_form": "저도 틀어야겠어요",
          "speaker_id": "2",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100009427.1.8",
          "form": "네 이제 두번째꺼 같아요",
          "original_form": "네 이제 두번째꺼 같아요",
          "speaker_id": "1",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100009427.1.9",
          "form": "오늘 누구 나왔어요?",
          "original_form": "오늘 누구 나왔어요?",
          "speaker_id": "2",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100009427.1.10",
          "form": "오늘 이진혁이요",
          "original_form": "오늘 이진혁이요",
          "speaker_id": "1",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100009427.1.11",
          "form": "배우??",
          "original_form": "배우??",
          "speaker_id": "2",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100009427.1.12",
          "form": "아니요 아이돌이요",
          "original_form": "아니요 아이돌이요",
          "speaker_id": "1",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100009427.1.13",
          "form": "프듀 나온 ㅋㅋ",
          "original_form": "프듀 나온 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100009427.1.14",
          "form": "아 그렇군요 ㅎㅎ",
          "original_form": "아 그렇군요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100009427.1.15",
          "form": "저도 틀었어요",
          "original_form": "저도 틀었어요",
          "speaker_id": "2",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100009427.1.16",
          "form": "오 네 첫번째꺼 재밌었어요",
          "original_form": "오 네 첫번째꺼 재밌었어요",
          "speaker_id": "1",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100009427.1.17",
          "form": "저도 유튜브로 다시 봐야겠네요",
          "original_form": "저도 유튜브로 다시 봐야겠네요",
          "speaker_id": "2",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100009427.1.18",
          "form": "네 나름 살벌한 재연 ㅎㅎ",
          "original_form": "네 나름 살벌한 재연 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:58"
        },
        {
