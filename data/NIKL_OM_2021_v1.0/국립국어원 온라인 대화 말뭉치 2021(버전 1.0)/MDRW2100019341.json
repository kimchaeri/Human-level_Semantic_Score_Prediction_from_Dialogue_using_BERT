        {
          "id": "MDRW2100019341.1.1",
          "form": "국내 여행 좋아하세요?",
          "original_form": "국내 여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100019341.1.2",
          "form": "네 ~ 국내여행 좋아해요",
          "original_form": "네 ~ 국내여행 좋아해요",
          "speaker_id": "1",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100019341.1.3",
          "form": "어디 가보셨나요",
          "original_form": "어디 가보셨나요",
          "speaker_id": "2",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100019341.1.4",
          "form": "최근에는 경주요!",
          "original_form": "최근에는 경주요!",
          "speaker_id": "1",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100019341.1.5",
          "form": "ㅇ오저도 경주 가봣는데 ㅋㅋㅋㅋ",
          "original_form": "ㅇ오저도 경주 가봣는데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100019341.1.6",
          "form": "수련회때 많이들 가죠 ㅋㅋㅋㅋㅋ",
          "original_form": "수련회때 많이들 가죠 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100019341.1.7",
          "form": "네 그때간게ㅜ마지막이요 ㅋㅋㅋㅋ",
          "original_form": "네 그때간게ㅜ마지막이요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100019341.1.8",
          "form": "경주 다시 가보는게 괜찮더라고요",
          "original_form": "경주 다시 가보는게 괜찮더라고요",
          "speaker_id": "1",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100019341.1.9",
          "form": "오 어떤면에서요?",
          "original_form": "오 어떤면에서요?",
          "speaker_id": "2",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100019341.1.10",
          "form": "음 그때는 불국사 이런거 관심이 없었는데",
          "original_form": "음 그때는 불국사 이런거 관심이 없었는데",
          "speaker_id": "1",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100019341.1.11",
          "form": "그럴 나이죠 ㅋㅋㅋㅋ",
          "original_form": "그럴 나이죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100019341.1.12",
          "form": "마자요 ㅋㅋㅋㅋㅋ구때 노관심이엇어요",
          "original_form": "마자요 ㅋㅋㅋㅋㅋ구때 노관심이엇어요",
          "speaker_id": "1",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100019341.1.13",
          "form": "네네ㅜㅋㅋㅋㅋㅋ 근데여???",
          "original_form": "네네ㅜㅋㅋㅋㅋㅋ 근데여???",
          "speaker_id": "2",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100019341.1.14",
          "form": "성인되니까 아무래도 불국사 이런게 관심잇어지더라고요",
          "original_form": "성인되니까 아무래도 불국사 이런게 관심잇어지더라고요",
