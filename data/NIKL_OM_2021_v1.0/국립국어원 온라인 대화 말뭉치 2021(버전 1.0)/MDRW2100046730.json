      "utterance": [
        {
          "id": "MDRW2100046730.1.1",
          "form": "점심시간인가봅니다",
          "original_form": "점심시간인가봅니다",
          "speaker_id": "1",
          "time": "20211105 00:53"
        },
        {
          "id": "MDRW2100046730.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211104 13:02"
        },
        {
          "id": "MDRW2100046730.1.3",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211104 13:04"
        },
        {
          "id": "MDRW2100046730.1.4",
          "form": "커피 좋아하세요?",
          "original_form": "커피 좋아하세요?",
          "speaker_id": "2",
          "time": "20211104 13:05"
        },
        {
          "id": "MDRW2100046730.1.5",
          "form": "커피 좋아합니다 매일 마시죠",
          "original_form": "커피 좋아합니다 매일 마시죠",
          "speaker_id": "1",
          "time": "20211104 13:05"
        },
        {
          "id": "MDRW2100046730.1.6",
          "form": "하루에 몇 잔 정도 마시세요?",
          "original_form": "하루에 몇 잔 정도 마시세요?",
          "speaker_id": "2",
          "time": "20211104 13:05"
        },
        {
          "id": "MDRW2100046730.1.7",
          "form": "2잔 정도 마십니다 투샷으로 마시니 총4샷이나 마시네요",
          "original_form": "2잔 정도 마십니다 투샷으로 마시니 총4샷이나 마시네요",
          "speaker_id": "1",
          "time": "20211104 13:06"
        },
        {
          "id": "MDRW2100046730.1.8",
          "form": "꽤 드시네요!",
          "original_form": "꽤 드시네요!",
          "speaker_id": "2",
          "time": "20211104 13:07"
        },
        {
          "id": "MDRW2100046730.1.9",
          "form": "카페인 잠에는 영향 없으신가요 ㅎㅎ",
          "original_form": "카페인 잠에는 영향 없으신가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 13:07"
        },
        {
          "id": "MDRW2100046730.1.10",
          "form": "그래서 오후부턴 안마십니다\r\n근데 특별히 영향은 없어보입니다.",
          "original_form": "그래서 오후부턴 안마십니다\r\n근데 특별히 영향은 없어보입니다.",
          "speaker_id": "1",
          "time": "20211104 13:07"
        },
        {
          "id": "MDRW2100046730.1.11",
          "form": "저는 점심 먹고 디카페인 아메리카노를 한잔 사왔어요",
          "original_form": "저는 점심 먹고 디카페인 아메리카노를 한잔 사왔어요",
          "speaker_id": "2",
          "time": "20211104 13:21"
        },
        {
          "id": "MDRW2100046730.1.12",
          "form": "디카페인인 뭔가 맛없다는 느낌이 들어요 커피맛은 모르지만 그냥...?",
          "original_form": "디카페인인 뭔가 맛없다는 느낌이 들어요 커피맛은 모르지만 그냥...?",
          "speaker_id": "1",
          "time": "20211104 13:22"
        },
        {
          "id": "MDRW2100046730.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20211104 13:25"
        },
        {
          "id": "MDRW2100046730.1.14",
          "form": "사실 카페인 들어간게 훨씬 맛있긴 해요",
          "original_form": "사실 카페인 들어간게 훨씬 맛있긴 해요",
          "speaker_id": "2",
          "time": "20211104 13:25"
        },
        {
          "id": "MDRW2100046730.1.15",
          "form": "근데 저는 오후에 커피를 마시면 잠을 못자서요!",
          "original_form": "근데 저는 오후에 커피를 마시면 잠을 못자서요!",
          "speaker_id": "2",
          "time": "20211104 13:25"
        },
        {
          "id": "MDRW2100046730.1.16",
          "form": "제 주변에도 있어요 \r\n항상 커피외 다른걸 먹더라구요",
          "original_form": "제 주변에도 있어요 \r\n항상 커피외 다른걸 먹더라구요",
          "speaker_id": "1",
          "time": "20211104 13:26"
        },
        {
          "id": "MDRW2100046730.1.17",
          "form": "맞아요 ㅋㅋ ",
          "original_form": "맞아요 ㅋㅋ ",
          "speaker_id": "2",
          "time": "20211104 13:35"
        },
        {
          "id": "MDRW2100046730.1.18",
          "form": "근데 또 커피 맛은 너무 좋아해서",
          "original_form": "근데 또 커피 맛은 너무 좋아해서",
          "speaker_id": "2",
          "time": "20211104 13:35"
        },
        {
          "id": "MDRW2100046730.1.19",
          "form": "포기를 못하겠어요 ㅋㅋㅋ",
          "original_form": "포기를 못하겠어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 13:35"
        },
        {
          "id": "MDRW2100046730.1.20",
          "form": "디카페인으로라도 마시게 되더라구요",
          "original_form": "디카페인으로라도 마시게 되더라구요",
          "speaker_id": "2",
          "time": "20211104 13:35"
        },
        {
          "id": "MDRW2100046730.1.21",
          "form": "한국의 직장인들에게 커피는 영양제와 비슷무리 하죠",
          "original_form": "한국의 직장인들에게 커피는 영양제와 비슷무리 하죠",
          "speaker_id": "1",
          "time": "20211104 13:37"
        }
      ]
    }
