      "utterance": [
        {
          "id": "MDRW2100039592.1.1",
          "form": "여행 많이 다녀보셨나용?!",
          "original_form": "여행 많이 다녀보셨나용?!",
          "speaker_id": "1",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100039592.1.2",
          "form": "아뇨ㅠ",
          "original_form": "아뇨ㅠ",
          "speaker_id": "2",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100039592.1.3",
          "form": "여행좋아세요?",
          "original_form": "여행좋아세요?",
          "speaker_id": "2",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100039592.1.4",
          "form": "저 완전요!!",
          "original_form": "저 완전요!!",
          "speaker_id": "1",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100039592.1.5",
          "form": "해외는 아니어도",
          "original_form": "해외는 아니어도",
          "speaker_id": "1",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100039592.1.6",
          "form": "국내여행도 좋아해요!!",
          "original_form": "국내여행도 좋아해요!!",
          "speaker_id": "1",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100039592.1.7",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100039592.1.8",
          "form": "가장기억나는곳은",
          "original_form": "가장기억나는곳은",
          "speaker_id": "2",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100039592.1.9",
          "form": "어디세요?",
          "original_form": "어디세요?",
          "speaker_id": "2",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100039592.1.10",
          "form": "저 제주도 우도요!!",
          "original_form": "저 제주도 우도요!!",
          "speaker_id": "1",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100039592.1.11",
          "form": "코로나 전에 다녀왔어요!!",
          "original_form": "코로나 전에 다녀왔어요!!",
          "speaker_id": "1",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100039592.1.12",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100039592.1.13",
          "form": "거기 바다가 진짜진짜 이뻐서",
          "original_form": "거기 바다가 진짜진짜 이뻐서",
          "speaker_id": "1",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100039592.1.14",
          "form": "저제주도안가봤거든요",
          "original_form": "저제주도안가봤거든요",
          "speaker_id": "2",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100039592.1.15",
          "form": "사진 많이 찍었었네요ㅋㅋㅋ",
          "original_form": "사진 많이 찍었었네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100039592.1.16",
          "form": "가서 기념품도 사오셨어요?!",
          "original_form": "가서 기념품도 사오셨어요?!",
          "speaker_id": "2",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039592.1.17",
          "form": "나중에 꼭 한번 가보세요!!",
          "original_form": "나중에 꼭 한번 가보세요!!",
          "speaker_id": "1",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039592.1.18",
          "form": "네!좋아요",
          "original_form": "네!좋아요",
          "speaker_id": "2",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039592.1.19",
          "form": "ㅋㅋㅋㅋ가서 귤 다섯박스 사왔는데",
          "original_form": "ㅋㅋㅋㅋ가서 귤 다섯박스 사왔는데",
          "speaker_id": "1",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039592.1.20",
          "form": "기념품으로 쳐주세요ㅋㅋㅋ",
          "original_form": "기념품으로 쳐주세요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039592.1.21",
          "form": "ㅋㄱㅋㅋㅋ헉 엄청많이사셨네요",
          "original_form": "ㅋㄱㅋㅋㅋ헉 엄청많이사셨네요",
          "speaker_id": "2",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039592.1.22",
          "form": "택배로받으셨나요",
          "original_form": "택배로받으셨나요",
          "speaker_id": "2",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100039592.1.23",
          "form": "네네!!",
          "original_form": "네네!!",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039592.1.24",
          "form": "한달동안 다먹었어요!!ㅋㅋㅋ",
          "original_form": "한달동안 다먹었어요!!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039592.1.25",
          "form": "정말 단가요?!",
          "original_form": "정말 단가요?!",
          "speaker_id": "2",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100039592.1.26",
          "form": "제주귤이 맛있다고 워낙 유명하니까ㅋㅋ",
          "original_form": "제주귤이 맛있다고 워낙 유명하니까ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100039592.1.27",
          "form": "ㅋㅋㅋㅋ다행히 저희 건 달았어요!!",
          "original_form": "ㅋㅋㅋㅋ다행히 저희 건 달았어요!!",
          "speaker_id": "1",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100039592.1.28",
          "form": "궁금하네요",
