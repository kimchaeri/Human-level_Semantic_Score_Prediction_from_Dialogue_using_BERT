      "utterance": [
        {
          "id": "MDRW2100015270.1.1",
          "form": "푸라닭어떤거요?",
          "original_form": "푸라닭어떤거요?",
          "speaker_id": "1",
          "time": "20210825 22:26"
        },
        {
          "id": "MDRW2100015270.1.2",
          "form": "푸라닭어떤메뉴 드셨어요?",
          "original_form": "푸라닭어떤메뉴 드셨어요?",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015270.1.3",
          "form": "아...  필터링이 된거 같은데",
          "original_form": "아...  필터링이 된거 같은데",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015270.1.4",
          "form": "고 추 마요 에요.ㅋㅋㅋㅋㅋ",
          "original_form": "고 추 마요 에요.ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015270.1.5",
          "form": "아 ㅋㅋㅋㅋㅋ고마 맛있죠",
          "original_form": "아 ㅋㅋㅋㅋㅋ고마 맛있죠",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015270.1.6",
          "form": "카페가는거 좋아하세요?",
          "original_form": "카페가는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100015270.1.7",
          "form": "네ㅋㅋㅋ 커피도 좋아하고 카페 가는거도 좋아해요",
          "original_form": "네ㅋㅋㅋ 커피도 좋아하고 카페 가는거도 좋아해요",
          "speaker_id": "2",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015270.1.8",
          "form": "프차카페중에선 어디가는거좋아하시나요",
          "original_form": "프차카페중에선 어디가는거좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100015270.1.9",
          "form": "스벅이요 ㅋㅋㅋ 제일 자주 가는거같아요",
          "original_form": "스벅이요 ㅋㅋㅋ 제일 자주 가는거같아요",
          "speaker_id": "2",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100015270.1.10",
          "form": "name1님은 카페 자주 가세요?",
          "original_form": "&name1&님은 카페 자주 가세요?",
          "speaker_id": "2",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100015270.1.11",
          "form": "저도 집근처에 스벅있어서 자주가요",
          "original_form": "저도 집근처에 스벅있어서 자주가요",
          "speaker_id": "1",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100015270.1.12",
          "form": "스벅가시면 어떤거 자주드세요?",
          "original_form": "스벅가시면 어떤거 자주드세요?",
          "speaker_id": "1",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100015270.1.13",
          "form": "아아요 ㅋㅋㅋ",
          "original_form": "아아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100015270.1.14",
          "form": "제일 무난한거같아요 ㅋㅋ",
          "original_form": "제일 무난한거같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100015270.1.15",
          "form": "ㅋㅋㅋ맞아요 전 달달한것만먹어서 단거별로안좋아하시나봐요",
          "original_form": "ㅋㅋㅋ맞아요 전 달달한것만먹어서 단거별로안좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210826 12:47"
        },
        {
          "id": "MDRW2100015270.1.16",
          "form": "단거는 좋아하는데 음료는 안달은게 좋더라고요",
          "original_form": "단거는 좋아하는데 음료는 안달은게 좋더라고요",
          "speaker_id": "2",
          "time": "20210826 12:55"
        },
        {
          "id": "MDRW2100015270.1.17",
          "form": "대신에 디저트를 달달한거로 ㅎㅎ",
          "original_form": "대신에 디저트를 달달한거로 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 12:55"
        },
        {
          "id": "MDRW2100015270.1.18",
          "form": "전 단은료와 달달디저트를 같이먹어요 ㅋㅋㅋㅋ디저트는 어떤거 자주드세요",
          "original_form": "전 단은료와 달달디저트를 같이먹어요 ㅋㅋㅋㅋ디저트는 어떤거 자주드세요",
          "speaker_id": "1",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100015270.1.19",
          "form": "스벅에서는 생크림 카스테라? 그게 제일 맛있더라고요 ㅋㅋㅋ",
          "original_form": "스벅에서는 생크림 카스테라? 그게 제일 맛있더라고요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100015270.1.20",
          "form": "달달한 음료는 주로 뭐드세요?",
          "original_form": "달달한 음료는 주로 뭐드세요?",
          "speaker_id": "2",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100015270.1.21",
          "form": "맞아요 저도 스벅에선 카스테라가 젤낫더라구여",
