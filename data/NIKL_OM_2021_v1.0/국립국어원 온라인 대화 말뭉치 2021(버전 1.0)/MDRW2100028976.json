      "utterance": [
        {
          "id": "MDRW2100028976.1.1",
          "form": "학교 생활이라...",
          "original_form": "학교 생활이라...",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028976.1.2",
          "form": "학교하면 나 교수님부터 생각나",
          "original_form": "학교하면 나 교수님부터 생각나",
          "speaker_id": "1",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028976.1.3",
          "form": "다들 되게 특색잇으셨는디",
          "original_form": "다들 되게 특색잇으셨는디",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028976.1.4",
          "form": "맞아 다들 캐릭터가 강했어",
          "original_form": "맞아 다들 캐릭터가 강했어",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028976.1.5",
          "form": "지금도 생각하면 웃겨",
          "original_form": "지금도 생각하면 웃겨",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028976.1.6",
          "form": "그치~? 수강신청도 진짜 열심히 했는데",
          "original_form": "그치~? 수강신청도 진짜 열심히 했는데",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100028976.1.7",
          "form": "난 pc방도 가서 했어",
          "original_form": "난 pc방도 가서 했어",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100028976.1.8",
          "form": "1분 1초가 중요했어서",
          "original_form": "1분 1초가 중요했어서",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100028976.1.9",
          "form": "완전 아이돌 콘서트 예매였잖어",
          "original_form": "완전 아이돌 콘서트 예매였잖어",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100028976.1.10",
          "form": "그치...막 서버 터지고 난리였지",
          "original_form": "그치...막 서버 터지고 난리였지",
          "speaker_id": "2",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100028976.1.11",
          "form": "아 맞어 10분동안 먹통이어서 망했다 싶었는데",
          "original_form": "아 맞어 10분동안 먹통이어서 망했다 싶었는데",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100028976.1.12",
          "form": "다들 그랬었지?",
          "original_form": "다들 그랬었지?",
          "speaker_id": "2",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100028976.1.13",
          "form": "다 터졌어 나만 터진거 아니야",
          "original_form": "다 터졌어 나만 터진거 아니야",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100028976.1.14",
          "form": "다시하라고 하면 할 수 있을까",
          "original_form": "다시하라고 하면 할 수 있을까",
          "speaker_id": "2",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100028976.1.15",
          "form": "좋아하는 뮤지컬 예매를 하고말지",
          "original_form": "좋아하는 뮤지컬 예매를 하고말지",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100028976.1.16",
          "form": "돈주고 수강신청하긴 싫어...",
          "original_form": "돈주고 수강신청하긴 싫어...",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100028976.1.17",
          "form": "그래도 배우기 위해서 하는 거니까~!",
          "original_form": "그래도 배우기 위해서 하는 거니까~!",
          "speaker_id": "2",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100028976.1.18",
          "form": "맞어! 할때는 해요! 익숙해지면 자리 잘잡기도 하고~",
          "original_form": "맞어! 할때는 해요! 익숙해지면 자리 잘잡기도 하고~",
          "speaker_id": "1",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100028976.1.19",
          "form": "그치?점점 성장하는 것 같아",
          "original_form": "그치?점점 성장하는 것 같아",
          "speaker_id": "2",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100028976.1.20",
          "form": "수업은 살짝 어렵지만...지금도 어렵다",
          "original_form": "수업은 살짝 어렵지만...지금도 어렵다",
          "speaker_id": "2",
          "time": "20210902 23:30"
        }
      ]
