      "utterance": [
        {
          "id": "MDRW2100035122.1.1",
          "form": "저번에 제주도 갔을때",
          "original_form": "저번에 제주도 갔을때",
          "speaker_id": "1",
          "time": "20210829 03:18"
        },
        {
          "id": "MDRW2100035122.1.2",
          "form": "사우나? 좀 유명한 곳이었는데",
          "original_form": "사우나? 좀 유명한 곳이었는데",
          "speaker_id": "1",
          "time": "20210829 03:18"
        },
        {
          "id": "MDRW2100035122.1.3",
          "form": "오 그런가요!",
          "original_form": "오 그런가요!",
          "speaker_id": "2",
          "time": "20210829 03:18"
        },
        {
          "id": "MDRW2100035122.1.4",
          "form": "현지인이랑",
          "original_form": "현지인이랑",
          "speaker_id": "1",
          "time": "20210829 03:18"
        },
        {
          "id": "MDRW2100035122.1.5",
          "form": "외지인이랑 구분하기 위해서",
          "original_form": "외지인이랑 구분하기 위해서",
          "speaker_id": "1",
          "time": "20210829 03:18"
        },
        {
          "id": "MDRW2100035122.1.6",
          "form": "말을 시키더라구요",
          "original_form": "말을 시키더라구요",
          "speaker_id": "1",
          "time": "20210829 03:18"
        },
        {
          "id": "MDRW2100035122.1.7",
          "form": "그래서 제주도 사투리를 못쓰면",
          "original_form": "그래서 제주도 사투리를 못쓰면",
          "speaker_id": "1",
          "time": "20210829 03:19"
        },
        {
          "id": "MDRW2100035122.1.8",
          "form": "돈을 이천원인가 더 받더라구욬ㅋㅋㅋ",
          "original_form": "돈을 이천원인가 더 받더라구욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:19"
        },
        {
          "id": "MDRW2100035122.1.9",
          "form": "사우나가고싶네요ㅠㅠ",
          "original_form": "사우나가고싶네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 03:19"
        },
        {
          "id": "MDRW2100035122.1.10",
          "form": "어머나 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "어머나 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:19"
        },
        {
          "id": "MDRW2100035122.1.11",
          "form": "그래서 여기는 제주도민끼리 잘 뭉쳐 있구나",
          "original_form": "그래서 여기는 제주도민끼리 잘 뭉쳐 있구나",
          "speaker_id": "1",
          "time": "20210829 03:19"
        },
        {
          "id": "MDRW2100035122.1.12",
          "form": "생각했어용",
          "original_form": "생각했어용",
          "speaker_id": "1",
          "time": "20210829 03:19"
        },
        {
          "id": "MDRW2100035122.1.13",
          "form": "엄마데러가고싶네요",
          "original_form": "엄마데러가고싶네요",
          "speaker_id": "2",
          "time": "20210829 03:19"
        },
        {
          "id": "MDRW2100035122.1.14",
          "form": "앜ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:19"
        },
        {
          "id": "MDRW2100035122.1.15",
          "form": "제주도 오메기떡 넘 맛있어서",
          "original_form": "제주도 오메기떡 넘 맛있어서",
          "speaker_id": "1",
          "time": "20210829 03:19"
        },
        {
          "id": "MDRW2100035122.1.16",
          "form": "20살때서울오셔서 지금까지 사셨는데 ㅋㅋㅋ",
          "original_form": "20살때서울오셔서 지금까지 사셨는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:20"
        },
        {
          "id": "MDRW2100035122.1.17",
          "form": "올라와서도 자주 시켜먹어용",
          "original_form": "올라와서도 자주 시켜먹어용",
          "speaker_id": "1",
          "time": "20210829 03:20"
        },
        {
          "id": "MDRW2100035122.1.18",
          "form": "과연 구분될런지 ㅋㅋㅋ",
          "original_form": "과연 구분될런지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:20"
        },
        {
          "id": "MDRW2100035122.1.19",
          "form": "어머ㅋㅋㅋ 그래도 나고 자라신곳이 제주도시니까ㅋㅋㅋ",
          "original_form": "어머ㅋㅋㅋ 그래도 나고 자라신곳이 제주도시니까ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:20"
        },
        {
          "id": "MDRW2100035122.1.20",
          "form": "저도 몇번먹어봣는데 ㅋㅋ",
          "original_form": "저도 몇번먹어봣는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:20"
        },
        {
          "id": "MDRW2100035122.1.21",
          "form": "맛집은 뭐기억나세요!?",
          "original_form": "맛집은 뭐기억나세요!?",
          "speaker_id": "2",
          "time": "20210829 03:21"
        },
        {
          "id": "MDRW2100035122.1.22",
          "form": "삼촌이",
          "original_form": "삼촌이",
          "speaker_id": "1",
          "time": "20210829 03:21"
        },
        {
          "id": "MDRW2100035122.1.23",
          "form": "갈치국?",
          "original_form": "갈치국?",
          "speaker_id": "1",
          "time": "20210829 03:21"
        },
        {
          "id": "MDRW2100035122.1.24",
          "form": "현지인들만 가는 로컬 맛집이라고 해서",
          "original_form": "현지인들만 가는 로컬 맛집이라고 해서",
          "speaker_id": "1",
          "time": "20210829 03:21"
        },
        {
          "id": "MDRW2100035122.1.25",
          "form": "와 ~",
          "original_form": "와 ~",
          "speaker_id": "2",
          "time": "20210829 03:21"
        },
        {
          "id": "MDRW2100035122.1.26",
          "form": "되게 허름한 간판이 있는 식당에 갔는데",
          "original_form": "되게 허름한 간판이 있는 식당에 갔는데",
          "speaker_id": "1",
          "time": "20210829 03:21"
        },
        {
          "id": "MDRW2100035122.1.27",
          "form": "제대로된곳가셨나보네요",
          "original_form": "제대로된곳가셨나보네요",
          "speaker_id": "2",
          "time": "20210829 03:21"
        },
        {
          "id": "MDRW2100035122.1.28",
          "form": "갈치가 안비리더라구욬ㅋㅋㅋ",
          "original_form": "갈치가 안비리더라구욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:21"
        },
        {
          "id": "MDRW2100035122.1.29",
          "form": "워낙 싱싱해서",
          "original_form": "워낙 싱싱해서",
          "speaker_id": "1",
          "time": "20210829 03:21"
        }
      ]
    }
