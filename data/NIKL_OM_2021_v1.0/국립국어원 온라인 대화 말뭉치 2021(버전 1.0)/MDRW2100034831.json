      "utterance": [
        {
          "id": "MDRW2100034831.1.1",
          "form": "저 심지어 그거 본방으로 봤어요 ㅋㅋ",
          "original_form": "저 심지어 그거 본방으로 봤어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:18"
        },
        {
          "id": "MDRW2100034831.1.2",
          "form": "저두요 ㅣㅋㅋㅋㅋ 그때 고등학생이었는데",
          "original_form": "저두요 ㅣㅋㅋㅋㅋ 그때 고등학생이었는데",
          "speaker_id": "2",
          "time": "20210828 20:18"
        },
        {
          "id": "MDRW2100034831.1.3",
          "form": "야자중간에",
          "original_form": "야자중간에",
          "speaker_id": "2",
          "time": "20210828 20:18"
        },
        {
          "id": "MDRW2100034831.1.4",
          "form": "dmb로 몰래 봤는데",
          "original_form": "dmb로 몰래 봤는데",
          "speaker_id": "2",
          "time": "20210828 20:18"
        },
        {
          "id": "MDRW2100034831.1.5",
          "form": "그때 애들 다 난리났었어요 ㅋㅋ",
          "original_form": "그때 애들 다 난리났었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:18"
        },
        {
          "id": "MDRW2100034831.1.6",
          "form": "결말 이상하다고",
          "original_form": "결말 이상하다고",
          "speaker_id": "2",
          "time": "20210828 20:19"
        },
        {
          "id": "MDRW2100034831.1.7",
          "form": "아 맞아욬ㅋㄱㅋㅋ",
          "original_form": "아 맞아욬ㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:20"
        },
        {
          "id": "MDRW2100034831.1.8",
          "form": "전 충격받았어요 ㅋㅋㅋ",
          "original_form": "전 충격받았어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:20"
        },
        {
          "id": "MDRW2100034831.1.9",
          "form": "갑자기 이렇게 끝난다고? 하면서",
          "original_form": "갑자기 이렇게 끝난다고? 하면서",
          "speaker_id": "1",
          "time": "20210828 20:20"
        },
        {
          "id": "MDRW2100034831.1.10",
          "form": "개인적으로 이 감독 작품은 다 이렇다던데",
          "original_form": "개인적으로 이 감독 작품은 다 이렇다던데",
          "speaker_id": "2",
          "time": "20210828 20:21"
        },
        {
          "id": "MDRW2100034831.1.11",
          "form": "이런 시트콤류 안만들었으면 좋겠어요ㅠ",
          "original_form": "이런 시트콤류 안만들었으면 좋겠어요ㅠ",
          "speaker_id": "2",
          "time": "20210828 20:21"
        },
        {
          "id": "MDRW2100034831.1.12",
          "form": "재밌자고 보는건데 결말이 이러면.....ㅠ",
          "original_form": "재밌자고 보는건데 결말이 이러면.....ㅠ",
          "speaker_id": "2",
          "time": "20210828 20:21"
        },
        {
          "id": "MDRW2100034831.1.13",
          "form": "아 맞아요 거침없이하이킥도 이상했는데 ,,",
          "original_form": "아 맞아요 거침없이하이킥도 이상했는데 ,,",
          "speaker_id": "1",
          "time": "20210828 20:22"
        },
        {
          "id": "MDRW2100034831.1.14",
          "form": "꼭 글은 잘쓰면서",
          "original_form": "꼭 글은 잘쓰면서",
          "speaker_id": "1",
          "time": "20210828 20:22"
        },
        {
          "id": "MDRW2100034831.1.15",
          "form": "결말만 이상하던걸요 ..",
          "original_form": "결말만 이상하던걸요 ..",
          "speaker_id": "1",
          "time": "20210828 20:22"
        },
        {
          "id": "MDRW2100034831.1.16",
          "form": "네네ㅠㅠ 시트콤은 결말이 꽉 닫힌 해피엔딩이 좋아용",
          "original_form": "네네ㅠㅠ 시트콤은 결말이 꽉 닫힌 해피엔딩이 좋아용",
          "speaker_id": "2",
          "time": "20210828 20:22"
        },
        {
          "id": "MDRW2100034831.1.17",
          "form": "시트콤 안한지도 오래된거같아요",
          "original_form": "시트콤 안한지도 오래된거같아요",
          "speaker_id": "1",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100034831.1.18",
          "form": "제 기억상 그 박하선 나온게 마지막같은데",
          "original_form": "제 기억상 그 박하선 나온게 마지막같은데",
          "speaker_id": "1",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100034831.1.19",
          "form": "그러게요ㅠㅠ 논스톱 이런거 재밌게 봤는데ㅋㅋㅋㅋ",
          "original_form": "그러게요ㅠㅠ 논스톱 이런거 재밌게 봤는데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100034831.1.20",
          "form": "아 마자용",
          "original_form": "아 마자용",
          "speaker_id": "2",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100034831.1.21",
          "form": "롤리폴리 추는 박하선 아직도 웃기네요ㅋㅋㅋㅋ",
          "original_form": "롤리폴리 추는 박하선 아직도 웃기네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100034831.1.22",
          "form": "아 ㅋㅋㄱㅋㅋ 맞아요 추억이에요 ㅋㅋ",
          "original_form": "아 ㅋㅋㄱㅋㅋ 맞아요 추억이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:24"
        },
        {
          "id": "MDRW2100034831.1.23",
          "form": "사람들 재미없다해도 전 그 시리즈도 재밌었어요 ㅋㅋㅋ",
          "original_form": "사람들 재미없다해도 전 그 시리즈도 재밌었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:24"
        },
        {
          "id": "MDRW2100034831.1.24",
          "form": "저두요ㅋㅋㅋ 이적 숨은 부인찾기ㅋㅋㅋ",
          "original_form": "저두요ㅋㅋㅋ 이적 숨은 부인찾기ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:25"
        },
        {
          "id": "MDRW2100034831.1.25",
          "form": "드라마는 재밌게보신거 있으세요?",
          "original_form": "드라마는 재밌게보신거 있으세요?",
          "speaker_id": "2",
          "time": "20210828 20:25"
        },
        {
          "id": "MDRW2100034831.1.26",
          "form": "음 사람들이 잘 모르긴 하는데",
          "original_form": "음 사람들이 잘 모르긴 하는데",
          "speaker_id": "1",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100034831.1.27",
          "form": "착한 남자 아세요?",
          "original_form": "착한 남자 아세요?",
          "speaker_id": "1",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100034831.1.28",
          "form": "문채원이랑 송중기 출연한 드라마죵?",
          "original_form": "문채원이랑 송중기 출연한 드라마죵?",
          "speaker_id": "2",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100034831.1.29",
          "form": "네네 맞아요 !",
          "original_form": "네네 맞아요 !",
          "speaker_id": "1",
          "time": "20210828 20:28"
        },
        {
          "id": "MDRW2100034831.1.30",
          "form": "저는 그거 좋아해서",
          "original_form": "저는 그거 좋아해서",
          "speaker_id": "1",
          "time": "20210828 20:29"
        },
        {
          "id": "MDRW2100034831.1.31",
