      "utterance": [
        {
          "id": "MDRW2100013199.1.1",
          "form": "이번에 도쿄올림픽 보셨어요?",
          "original_form": "이번에 도쿄올림픽 보셨어요?",
          "speaker_id": "1",
          "time": "20210825 13:25"
        },
        {
          "id": "MDRW2100013199.1.2",
          "form": "아뇨 뉴스만 가끔 들었어요",
          "original_form": "아뇨 뉴스만 가끔 들었어요",
          "speaker_id": "2",
          "time": "20210825 13:25"
        },
        {
          "id": "MDRW2100013199.1.3",
          "form": "오 그럼 좋아하는 스포츠 있으세요?",
          "original_form": "오 그럼 좋아하는 스포츠 있으세요?",
          "speaker_id": "1",
          "time": "20210825 13:26"
        },
        {
          "id": "MDRW2100013199.1.4",
          "form": "수영좋아해요 name1은요?",
          "original_form": "수영좋아해요 &name1&은요?",
          "speaker_id": "2",
          "time": "20210825 13:26"
        },
        {
          "id": "MDRW2100013199.1.5",
          "form": "저두요!! 원래 문화센터에서 했었는데 요즘은 못하고 잇어요",
          "original_form": "저두요!! 원래 문화센터에서 했었는데 요즘은 못하고 잇어요",
          "speaker_id": "1",
          "time": "20210825 13:26"
        },
        {
          "id": "MDRW2100013199.1.6",
          "form": "반갑네요 ㅎㅎ 수영하면 몸이 매끈해지고",
          "original_form": "반갑네요 ㅎㅎ 수영하면 몸이 매끈해지고",
          "speaker_id": "2",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100013199.1.7",
          "form": "운동도되고 좋더라구요",
          "original_form": "운동도되고 좋더라구요",
          "speaker_id": "2",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100013199.1.8",
          "form": "ㅋㅋㅋㅋ 수영 얼마나 하셨어요?? 전 완전 쪼금햇어용",
          "original_form": "ㅋㅋㅋㅋ 수영 얼마나 하셨어요?? 전 완전 쪼금햇어용",
          "speaker_id": "1",
          "time": "20210825 13:28"
        },
        {
          "id": "MDRW2100013199.1.9",
          "form": "2-3년정도요",
          "original_form": "2-3년정도요",
          "speaker_id": "2",
          "time": "20210825 13:28"
        },
        {
          "id": "MDRW2100013199.1.10",
          "form": "와!! 확실히 근육이 붙나요?",
          "original_form": "와!! 확실히 근육이 붙나요?",
          "speaker_id": "1",
          "time": "20210825 13:29"
        },
        {
          "id": "MDRW2100013199.1.11",
          "form": "네 수영안한지 2년정도됐는데요",
          "original_form": "네 수영안한지 2년정도됐는데요",
          "speaker_id": "2",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100013199.1.12",
          "form": "제가그때 평영엄청 열심히해서",
          "original_form": "제가그때 평영엄청 열심히해서",
          "speaker_id": "2",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100013199.1.13",
          "form": "팔 전완근에 근육생겼는데 잘안없어지고 아직 남아있어요",
          "original_form": "팔 전완근에 근육생겼는데 잘안없어지고 아직 남아있어요",
          "speaker_id": "2",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100013199.1.14",
          "form": "와!!! 진짜 멋잇어요",
          "original_form": "와!!! 진짜 멋잇어요",
          "speaker_id": "1",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100013199.1.15",
          "form": "요즘은 수영 못가시죠 ㅜㅜ",
          "original_form": "요즘은 수영 못가시죠 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100013199.1.16",
          "form": "저희집 근처엔 열긴 여는데요",
          "original_form": "저희집 근처엔 열긴 여는데요",
          "speaker_id": "2",
          "time": "20210825 13:36"
        },
        {
          "id": "MDRW2100013199.1.17",
          "form": "안가게되네요 ㅜㅜ",
          "original_form": "안가게되네요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 13:36"
        },
        {
          "id": "MDRW2100013199.1.18",
          "form": "아 그죠그죠 아무래도 수영은 물을 같이 쓰니까..",
