        },
        {
          "id": "MDRW2100019579.1.2",
          "form": "대학교 수업기억나?",
          "original_form": "대학교 수업기억나?",
          "speaker_id": "1",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019579.1.3",
          "form": "졸업한지 좀 돼서 가물가물한데",
          "original_form": "졸업한지 좀 돼서 가물가물한데",
          "speaker_id": "2",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019579.1.4",
          "form": "재밌었던 수업 같은거",
          "original_form": "재밌었던 수업 같은거",
          "speaker_id": "1",
          "time": "20210827 20:57"
        },
        {
          "id": "MDRW2100019579.1.5",
          "form": "나는 교양 수업이 제일 좋더라 ㅋㅋ",
          "original_form": "나는 교양 수업이 제일 좋더라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:58"
        },
        {
          "id": "MDRW2100019579.1.6",
          "form": "나늨 답사 수업이 기억나",
          "original_form": "나늨 답사 수업이 기억나",
          "speaker_id": "1",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100019579.1.7",
          "form": "오 답사수업 특이하다",
          "original_form": "오 답사수업 특이하다",
          "speaker_id": "2",
          "time": "20210827 21:35"
        },
        {
          "id": "MDRW2100019579.1.8",
          "form": "다같이 답사를가니까 꼭 고등학생 같았어",
          "original_form": "다같이 답사를가니까 꼭 고등학생 같았어",
          "speaker_id": "1",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100019579.1.9",
          "form": "전공ㅈ수업이야?",
          "original_form": "전공ㅈ수업이야?",
          "speaker_id": "2",
          "time": "20210827 21:48"
        },
        {
          "id": "MDRW2100019579.1.10",
          "form": "응응 전공1학점",
          "original_form": "응응 전공1학점",
          "speaker_id": "1",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100019579.1.11",
          "form": "좀 적은데?",
          "original_form": "좀 적은데?",
          "speaker_id": "2",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100019579.1.12",
          "form": "그치 그래도 재밌었어",
          "original_form": "그치 그래도 재밌었어",
          "speaker_id": "1",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100019579.1.13",
          "form": "난 전공수업은 재미엢었어",
          "original_form": "난 전공수업은 재미엢었어",
          "speaker_id": "2",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100019579.1.14",
          "form": "나는 복수전공이었어",
          "original_form": "나는 복수전공이었어",
          "speaker_id": "1",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100019579.1.15",
          "form": "뭐랑 뭐?",
          "original_form": "뭐랑 뭐?",
          "speaker_id": "2",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100019579.1.16",
          "form": "건축이랑 화학을 배웠어",
          "original_form": "건축이랑 화학을 배웠어",
          "speaker_id": "1",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100019579.1.17",
          "form": "방학이 얼마 안 남았어",
          "original_form": "방학이 얼마 안 남았어",
          "speaker_id": "2",
          "time": "20210828 00:52"
        }
