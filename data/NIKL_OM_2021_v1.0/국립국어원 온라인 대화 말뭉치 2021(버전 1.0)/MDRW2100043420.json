      "utterance": [
        {
          "id": "MDRW2100043420.1.1",
          "form": "저 그냥 파스타해먹었습니당ㅋㅋ",
          "original_form": "저 그냥 파스타해먹었습니당ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043420.1.2",
          "form": "뭐드셨나요?",
          "original_form": "뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043420.1.3",
          "form": "술한잔중요 ㅎㅎ고기랑 꽃게해서요",
          "original_form": "술한잔중요 ㅎㅎ고기랑 꽃게해서요",
          "speaker_id": "1",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043420.1.4",
          "form": "우와! 진수성찬이네요",
          "original_form": "우와! 진수성찬이네요",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043420.1.5",
          "form": "맛있게드세욥~",
          "original_form": "맛있게드세욥~",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043420.1.6",
          "form": "파스타도 맛나죠",
          "original_form": "파스타도 맛나죠",
          "speaker_id": "1",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043420.1.7",
          "form": "페페론치노를 쏟아부어서 엄청매웠어요ㅋㅋ",
          "original_form": "페페론치노를 쏟아부어서 엄청매웠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043420.1.8",
          "form": "헉 .. 그럼 엄청 매울텐데요!",
          "original_form": "헉 .. 그럼 엄청 매울텐데요!",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100043420.1.9",
          "form": "배가아프네요ㅜㅜㅋㅋ",
          "original_form": "배가아프네요ㅜㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100043420.1.10",
          "form": "ㅋㅋㅋㅋ그래도 잘 해드시나봐요",
          "original_form": "ㅋㅋㅋㅋ그래도 잘 해드시나봐요",
          "speaker_id": "1",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043420.1.11",
          "form": "되도록이면 해먹으려고 노력중입니다ㅜㅜ",
          "original_form": "되도록이면 해먹으려고 노력중입니다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043420.1.12",
          "form": "저도 하는거 좋아하는데",
          "original_form": "저도 하는거 좋아하는데",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043420.1.13",
          "form": "퇴근하고오면 힘이없어요 ..ㅋㅋ",
          "original_form": "퇴근하고오면 힘이없어요 ..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043420.1.14",
          "form": "그쵸ㅠㅠㅠㅠ",
          "original_form": "그쵸ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043420.1.15",
          "form": "넹 ㅠㅠ",
          "original_form": "넹 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043420.1.16",
          "form": "그래서 저녁은 진짜 쥐어짜야 해먹을수있어요ㅜㅜ",
          "original_form": "그래서 저녁은 진짜 쥐어짜야 해먹을수있어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043420.1.17",
          "form": "주말에는 가능하면",
          "original_form": "주말에는 가능하면",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043420.1.18",
          "form": "맛있는거 해먹으려고 노력해요",
          "original_form": "맛있는거 해먹으려고 노력해요",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043420.1.19",
          "form": "부모님이랑 같이사세요?",
          "original_form": "부모님이랑 같이사세요?",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043420.1.20",
