      "utterance": [
        {
          "id": "MDRW2100012149.1.1",
          "form": "여행좋아하시나요",
          "original_form": "여행좋아하시나요",
          "speaker_id": "1",
          "time": "20210824 19:41"
        },
        {
          "id": "MDRW2100012149.1.2",
          "form": "좋아하는데 코로나라 못 간지 오래 됐어요",
          "original_form": "좋아하는데 코로나라 못 간지 오래 됐어요",
          "speaker_id": "2",
          "time": "20210824 19:44"
        },
        {
          "id": "MDRW2100012149.1.3",
          "form": "저도요ㅜㅜ코로나전엔 그럼 기억남은여행지있으신가요",
          "original_form": "저도요ㅜㅜ코로나전엔 그럼 기억남은여행지있으신가요",
          "speaker_id": "1",
          "time": "20210824 19:45"
        },
        {
          "id": "MDRW2100012149.1.4",
          "form": "수학여행으로 갔던 제주도요",
          "original_form": "수학여행으로 갔던 제주도요",
          "speaker_id": "2",
          "time": "20210824 19:47"
        },
        {
          "id": "MDRW2100012149.1.5",
          "form": "그 뒤로 한번도 안 가서 또 가보고 싶어요",
          "original_form": "그 뒤로 한번도 안 가서 또 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210824 19:47"
        },
        {
          "id": "MDRW2100012149.1.6",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210824 19:47"
        },
        {
          "id": "MDRW2100012149.1.7",
          "form": "저도 제주도 넘기억에 남아여ㅜ",
          "original_form": "저도 제주도 넘기억에 남아여ㅜ",
          "speaker_id": "1",
          "time": "20210824 19:51"
        },
        {
          "id": "MDRW2100012149.1.8",
          "form": "작년부터 가고싶었는데 코로나터져서 계속취소되고ㅜㅜㅜ",
          "original_form": "작년부터 가고싶었는데 코로나터져서 계속취소되고ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 19:52"
        },
        {
          "id": "MDRW2100012149.1.9",
          "form": "코로나끝나면 제주도부터갈랴구요",
          "original_form": "코로나끝나면 제주도부터갈랴구요",
          "speaker_id": "1",
          "time": "20210824 19:52"
        },
        {
          "id": "MDRW2100012149.1.10",
          "form": "그런 사람 많을 것 같아요",
          "original_form": "그런 사람 많을 것 같아요",
          "speaker_id": "2",
          "time": "20210824 20:06"
        },
        {
          "id": "MDRW2100012149.1.11",
          "form": "언제 종식될런지 끝이 안 보이네요",
          "original_form": "언제 종식될런지 끝이 안 보이네요",
          "speaker_id": "2",
          "time": "20210824 20:06"
        },
        {
          "id": "MDRW2100012149.1.12",
          "form": "그쳐ㅜㅜ해외는 가보고싶은곳있으신가요",
          "original_form": "그쳐ㅜㅜ해외는 가보고싶은곳있으신가요",
          "speaker_id": "1",
          "time": "20210824 21:52"
        },
        {
          "id": "MDRW2100012149.1.13",
          "form": "스위스요! 멋진 자연 보러가고 싶어요",
          "original_form": "스위스요! 멋진 자연 보러가고 싶어요",
          "speaker_id": "2",
          "time": "20210824 22:08"
        },
        {
          "id": "MDRW2100012149.1.14",
          "form": "오 저도 스위스의 자연을 맘껏 구경하고싶어요ㅜㅜ",
          "original_form": "오 저도 스위스의 자연을 맘껏 구경하고싶어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 22:15"
        },
        {
          "id": "MDRW2100012149.1.15",
          "form": "해외여행 다녀오신적있으신가요",
          "original_form": "해외여행 다녀오신적있으신가요",
          "speaker_id": "1",
          "time": "20210824 22:15"
        },
        {
          "id": "MDRW2100012149.1.16",
          "form": "아뇨 안 가봤어요 코로나 아니었으면 갔을 수도 있었을텐데 아쉬워요",
          "original_form": "아뇨 안 가봤어요 코로나 아니었으면 갔을 수도 있었을텐데 아쉬워요",
          "speaker_id": "2",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100012149.1.17",
          "form": "name1은 다녀와보셨어요?",
          "original_form": "&name1&은 다녀와보셨어요?",
          "speaker_id": "2",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100012149.1.18",
          "form": "네 제가 여행엄청좋아해서요 코로나전까지 다녔다고 터지고 못가니까ㅜㅜ",
          "original_form": "네 제가 여행엄청좋아해서요 코로나전까지 다녔다고 터지고 못가니까ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100012149.1.19",
          "form": "넘가고싶어요",
          "original_form": "넘가고싶어요",
          "speaker_id": "1",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100012149.1.20",
          "form": "어느 나라 가보셨어요?",
          "original_form": "어느 나라 가보셨어요?",
          "speaker_id": "2",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100012149.1.21",
          "form": "저 대만 베트남 미국 캐나다 남미여",
          "original_form": "저 대만 베트남 미국 캐나다 남미여",
