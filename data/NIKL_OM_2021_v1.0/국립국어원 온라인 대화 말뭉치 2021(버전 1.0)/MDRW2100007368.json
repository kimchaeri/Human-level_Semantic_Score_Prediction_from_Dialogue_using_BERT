      "utterance": [
        {
          "id": "MDRW2100007368.1.1",
          "form": "name2님 h패션몰 마저주문하셨어요?",
          "original_form": "&name2&님 h패션몰 마저주문하셨어요?",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007368.1.2",
          "form": "네 또 제티셔츠 추가요 ㅎ",
          "original_form": "네 또 제티셔츠 추가요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007368.1.3",
          "form": "저 룰렛 포인트도 없이 7퍼 중복쿠폰나왔어요 ㅠㅠ",
          "original_form": "저 룰렛 포인트도 없이 7퍼 중복쿠폰나왔어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007368.1.4",
          "form": "아 12시지났죠",
          "original_form": "아 12시지났죠",
          "speaker_id": "2",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007368.1.5",
          "form": "만근해야할듯요 2만점이 안되네요 주말3천 받아도",
          "original_form": "만근해야할듯요 2만점이 안되네요 주말3천 받아도",
          "speaker_id": "1",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007368.1.6",
          "form": "아고 오늘이 막날이군요 ㅜㅠ",
          "original_form": "아고 오늘이 막날이군요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007368.1.7",
          "form": "네 ㅠㅠ 포인트좀 주지 어쩜 쿠폰만 주는지",
          "original_form": "네 ㅠㅠ 포인트좀 주지 어쩜 쿠폰만 주는지",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007368.1.8",
          "form": "긴팔티 사셨어요?",
          "original_form": "긴팔티 사셨어요?",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007368.1.9",
          "form": "네 긴팔로 샀어요",
          "original_form": "네 긴팔로 샀어요",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007368.1.10",
          "form": "DKNY 꺼샀어요",
          "original_form": "DKNY 꺼샀어요",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007368.1.11",
          "form": "실결제 15000원했지만 입을꺼하자싶어서요 ㅎ",
          "original_form": "실결제 15000원했지만 입을꺼하자싶어서요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007368.1.12",
          "form": "오 dkny  적립금되나요?",
          "original_form": "오 dkny  적립금되나요?",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007368.1.13",
          "form": "네 되던데요",
          "original_form": "네 되던데요",
          "speaker_id": "2",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007368.1.14",
          "form": "죄다 타미만 사니 그것도 괜찮겠어요",
          "original_form": "죄다 타미만 사니 그것도 괜찮겠어요",
          "speaker_id": "1",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007368.1.15",
          "form": "할인율이 작긴해요",
          "original_form": "할인율이 작긴해요",
          "speaker_id": "2",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007368.1.16",
          "form": "게스가 좀 저렴하던데 안더더라구요",
          "original_form": "게스가 좀 저렴하던데 안더더라구요",
          "speaker_id": "1",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007368.1.17",
          "form": "긴팔을 좀 골라놔야겠어요",
          "original_form": "긴팔을 좀 골라놔야겠어요",
          "speaker_id": "1",
          "time": "20210822 01:51"
        },
        {
          "id": "MDRW2100007368.1.18",
          "form": "게스는 안되던가요? 아깝네요",
          "original_form": "게스는 안되던가요? 아깝네요",
          "speaker_id": "2",
          "time": "20210822 01:51"
        },
        {
          "id": "MDRW2100007368.1.19",
          "form": "본인들이 하는게아니고 업체로 들어온건가봐요",
          "original_form": "본인들이 하는게아니고 업체로 들어온건가봐요",
          "speaker_id": "1",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007368.1.20",
          "form": "그런건 안되던데요",
          "original_form": "그런건 안되던데요",
          "speaker_id": "1",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007368.1.21",
          "form": "아...아모레에 업체배송같은거네요",
          "original_form": "아...아모레에 업체배송같은거네요",
          "speaker_id": "2",
          "time": "20210822 01:53"
        },
        {
          "id": "MDRW2100007368.1.22",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210822 01:53"
        },
        {
          "id": "MDRW2100007368.1.23",
          "form": "치사하더라구요",
          "original_form": "치사하더라구요",
