      "utterance": [
        {
          "id": "MDRW2100019259.1.1",
          "form": "무교로서",
          "original_form": "무교로서",
          "speaker_id": "1",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019259.1.2",
          "form": "제일 가깝게 느끼는 종교는",
          "original_form": "제일 가깝게 느끼는 종교는",
          "speaker_id": "1",
          "time": "20210827 16:45"
        },
        {
          "id": "MDRW2100019259.1.3",
          "form": "어디일까",
          "original_form": "어디일까",
          "speaker_id": "1",
          "time": "20210827 16:45"
        },
        {
          "id": "MDRW2100019259.1.4",
          "form": "흠 그나마 불교?",
          "original_form": "흠 그나마 불교?",
          "speaker_id": "2",
          "time": "20210827 16:45"
        },
        {
          "id": "MDRW2100019259.1.5",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100019259.1.6",
          "form": "부처가 조물주나 유일신처럼 모셔지지 않고",
          "original_form": "부처가 조물주나 유일신처럼 모셔지지 않고",
          "speaker_id": "2",
          "time": "20210827 17:33"
        },
        {
          "id": "MDRW2100019259.1.7",
          "form": "토속적으로 느껴지는 것도 있고?",
          "original_form": "토속적으로 느껴지는 것도 있고?",
          "speaker_id": "2",
          "time": "20210827 17:34"
        },
        {
          "id": "MDRW2100019259.1.8",
          "form": "서양에서 성경관련 미디어소재같은거 많이내서",
          "original_form": "서양에서 성경관련 미디어소재같은거 많이내서",
          "speaker_id": "1",
          "time": "20210827 17:35"
        },
        {
          "id": "MDRW2100019259.1.9",
          "form": "기독교가 익숙할법도한데",
          "original_form": "기독교가 익숙할법도한데",
          "speaker_id": "1",
          "time": "20210827 17:35"
        },
        {
          "id": "MDRW2100019259.1.10",
          "form": "기독교는....익숙한데 반발감이 있달지 ㅋㅋ",
          "original_form": "기독교는....익숙한데 반발감이 있달지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100019259.1.11",
          "form": "한국기독교?ㅋㅋㅋ",
          "original_form": "한국기독교?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100019259.1.12",
          "form": "예스 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "예스 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:43"
        },
        {
          "id": "MDRW2100019259.1.13",
          "form": "거긴 좀...종교집단이라기보단 광기에 절은 느낌이야",
          "original_form": "거긴 좀...종교집단이라기보단 광기에 절은 느낌이야",
          "speaker_id": "2",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019259.1.14",
          "form": "맹신이랄까",
          "original_form": "맹신이랄까{emoji:☹}",
          "speaker_id": "2",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019259.1.15",
          "form": "맞아 요새",
          "original_form": "맞아 요새",
          "speaker_id": "1",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019259.1.16",
          "form": "이슬람보고도느끼는건데",
          "original_form": "이슬람보고도느끼는건데",
          "speaker_id": "1",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019259.1.17",
          "form": "종교를초월해서 믿는느낌",
          "original_form": "종교를초월해서 믿는느낌",
          "speaker_id": "1",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019259.1.18",
          "form": "ㅁㅈ 그냥 음 신실한 믿음이라기보단",
          "original_form": "ㅁㅈ 그냥 음 신실한 믿음이라기보단",
          "speaker_id": "2",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100019259.1.19",
          "form": "무지성이랄지",
          "original_form": "무지성이랄지",
          "speaker_id": "2",
          "time": "20210827 17:46"
        },
        {
          "id": "MDRW2100019259.1.20",
          "form": "종교가 삶의방식이되면",
          "original_form": "종교가 삶의방식이되면",
          "speaker_id": "1",
          "time": "20210827 17:48"
        },
        {
          "id": "MDRW2100019259.1.21",
          "form": "안되는거잖아",
          "original_form": "안되는거잖아",
          "speaker_id": "1",
          "time": "20210827 17:48"
        },
        {
          "id": "MDRW2100019259.1.22",
          "form": "글치",
          "original_form": "글치",
          "speaker_id": "2",
          "time": "20210827 17:57"
        },
        {
          "id": "MDRW2100019259.1.23",
          "form": "그리고 특정 종교들은 너무 배타적인 입장을 보여서",
          "original_form": "그리고 특정 종교들은 너무 배타적인 입장을 보여서",
          "speaker_id": "2",
          "time": "20210827 17:57"
        },
        {
          "id": "MDRW2100019259.1.24",
          "form": "별로야",
          "original_form": "별로야",
          "speaker_id": "2",
          "time": "20210827 17:57"
        },
        {
          "id": "MDRW2100019259.1.25",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210827 17:57"
        },
        {
          "id": "MDRW2100019259.1.26",
          "form": "자기들만이 정답이란 그게 싫어",
          "original_form": "자기들만이 정답이란 그게 싫어",
          "speaker_id": "2",
          "time": "20210827 17:57"
        },
        {
          "id": "MDRW2100019259.1.27",
          "form": "그냥 나는나 너는너",
          "original_form": "그냥 나는나 너는너",
          "speaker_id": "1",
          "time": "20210827 17:57"
        },
        {
          "id": "MDRW2100019259.1.28",
          "form": "이정도만해도 좋은데",
          "original_form": "이정도만해도 좋은데",
          "speaker_id": "1",
          "time": "20210827 17:58"
        },
        {
          "id": "MDRW2100019259.1.29",
          "form": "ㅁㅈㅁㅈ 근데 a란 답이 아니면 전부 잘못된 거로 보니까",
          "original_form": "ㅁㅈㅁㅈ 근데 a란 답이 아니면 전부 잘못된 거로 보니까",
