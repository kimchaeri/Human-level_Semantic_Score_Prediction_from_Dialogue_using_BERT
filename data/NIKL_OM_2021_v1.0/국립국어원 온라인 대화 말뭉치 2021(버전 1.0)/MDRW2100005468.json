      "utterance": [
        {
          "id": "MDRW2100005468.1.1",
          "form": "밤에 꼭 창문닫고 자기~~",
          "original_form": "밤에 꼭 창문닫고 자기~~",
          "speaker_id": "2",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005468.1.2",
          "form": "나 항상 열고자는데ㅋㅋ",
          "original_form": "나 항상 열고자는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:59"
        },
        {
          "id": "MDRW2100005468.1.3",
          "form": "밤에 비올거 같아",
          "original_form": "밤에 비올거 같아",
          "speaker_id": "2",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005468.1.4",
          "form": "가을장마라잖아",
          "original_form": "가을장마라잖아",
          "speaker_id": "2",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005468.1.5",
          "form": "벌써 가을이야?",
          "original_form": "벌써 가을이야?",
          "speaker_id": "1",
          "time": "20210821 23:13"
        },
        {
          "id": "MDRW2100005468.1.6",
          "form": "8월인데요..",
          "original_form": "8월인데요..",
          "speaker_id": "1",
          "time": "20210821 23:13"
        },
        {
          "id": "MDRW2100005468.1.7",
          "form": "뉴스에서 그런거라구",
          "original_form": "뉴스에서 그런거라구",
          "speaker_id": "2",
          "time": "20210821 23:14"
        },
        {
          "id": "MDRW2100005468.1.8",
          "form": "9월이면 가을이지",
          "original_form": "9월이면 가을이지",
          "speaker_id": "2",
          "time": "20210821 23:14"
        },
        {
          "id": "MDRW2100005468.1.9",
          "form": "지금은!! 8월인데요!!",
          "original_form": "지금은!! 8월인데요!!",
          "speaker_id": "1",
          "time": "20210821 23:14"
        },
        {
          "id": "MDRW2100005468.1.10",
          "form": "곧!! 9월이니까!!",
          "original_form": "곧!! 9월이니까!!",
          "speaker_id": "2",
          "time": "20210821 23:14"
        },
        {
          "id": "MDRW2100005468.1.11",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210821 23:14"
        },
        {
          "id": "MDRW2100005468.1.12",
          "form": "중국이랑 일본에 있던 정체전선이 올라왔다더라",
          "original_form": "중국이랑 일본에 있던 정체전선이 올라왔다더라",
          "speaker_id": "2",
          "time": "20210821 23:14"
        },
        {
          "id": "MDRW2100005468.1.13",
          "form": "그 비 엄청 뿌리던....",
          "original_form": "그 비 엄청 뿌리던....",
          "speaker_id": "2",
          "time": "20210821 23:14"
        },
        {
          "id": "MDRW2100005468.1.14",
          "form": "정체전선이 뭐죠",
          "original_form": "정체전선이 뭐죠",
          "speaker_id": "1",
          "time": "20210821 23:14"
        },
        {
          "id": "MDRW2100005468.1.15",
          "form": "#정체전선 검색이 되면 좋은데",
          "original_form": "#정체전선 검색이 되면 좋은데",
          "speaker_id": "2",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005468.1.16",
          "form": "제가 알아서 찾아보겠습니다",
          "original_form": "제가 알아서 찾아보겠습니다",
          "speaker_id": "1",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005468.1.17",
          "form": "아 이거 그거네",
          "original_form": "아 이거 그거네",
          "speaker_id": "1",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005468.1.18",
          "form": "장마전선도 종류 중 하나야!",
          "original_form": "장마전선도 종류 중 하나야!",
          "speaker_id": "2",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005468.1.19",
          "form": "예전 과학시간네 배운.. 동그랗고 세모난거",
          "original_form": "예전 과학시간네 배운.. 동그랗고 세모난거",
          "speaker_id": "1",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005468.1.20",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005468.1.21",
          "form": "앜ㅋㄲ 맞아 그거!!",
          "original_form": "앜ㅋㄲ 맞아 그거!!",
          "speaker_id": "2",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005468.1.22",
          "form": "한랭전선과 온난전선이 만난.....",
          "original_form": "한랭전선과 온난전선이 만난.....",
          "speaker_id": "2",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005468.1.23",
          "form": "그렇습니다",
          "original_form": "그렇습니다",
          "speaker_id": "1",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005468.1.24",
