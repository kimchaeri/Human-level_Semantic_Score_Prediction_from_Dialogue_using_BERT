        {
          "id": "MDRW2100041665.1.1",
          "form": "물2리터는 정말 못마시겠어요 ㄱㅋ",
          "original_form": "물2리터는 정말 못마시겠어요 ㄱㅋ",
          "speaker_id": "2",
          "time": "20210906 14:11"
        },
        {
          "id": "MDRW2100041665.1.2",
          "form": "저도 드릅게 물 안마신다는 얘기를 들을정도로 안마셔서 ㅋㅋㅋㅋ",
          "original_form": "저도 드릅게 물 안마신다는 얘기를 들을정도로 안마셔서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:11"
        },
        {
          "id": "MDRW2100041665.1.3",
          "form": "저도 절대 못마셔요",
          "original_form": "저도 절대 못마셔요",
          "speaker_id": "1",
          "time": "20210906 14:11"
        },
        {
          "id": "MDRW2100041665.1.4",
          "form": "아이번에",
          "original_form": "아이번에",
          "speaker_id": "2",
          "time": "20210906 14:11"
        },
        {
          "id": "MDRW2100041665.1.5",
          "form": "이리터라니 일리터도 힘든데 ㅋㅋㅋㅋ",
          "original_form": "이리터라니 일리터도 힘든데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:11"
        },
        {
          "id": "MDRW2100041665.1.6",
          "form": "링티를 사서",
          "original_form": "링티를 사서",
          "speaker_id": "2",
          "time": "20210906 14:11"
        },
        {
          "id": "MDRW2100041665.1.7",
          "form": "링티 넣어서 마셔봤는데",
          "original_form": "링티 넣어서 마셔봤는데",
          "speaker_id": "2",
          "time": "20210906 14:11"
        },
        {
          "id": "MDRW2100041665.1.8",
          "form": "아 그거 뭔가 궁금했어요",
          "original_form": "아 그거 뭔가 궁금했어요",
          "speaker_id": "1",
          "time": "20210906 14:11"
        },
        {
          "id": "MDRW2100041665.1.9",
          "form": "그마나 마실만해요^^;/",
          "original_form": "그마나 마실만해요^^;/",
          "speaker_id": "2",
          "time": "20210906 14:11"
        },
        {
          "id": "MDRW2100041665.1.10",
          "form": "무슨 맛나여??",
          "original_form": "무슨 맛나여??",
          "speaker_id": "1",
          "time": "20210906 14:12"
        },
        {
          "id": "MDRW2100041665.1.11",
          "form": "링티 그냥  레몬맛 포카리  복숭아맛 포카리",
          "original_form": "링티 그냥  레몬맛 포카리  복숭아맛 포카리",
          "speaker_id": "2",
          "time": "20210906 14:12"
        },
        {
          "id": "MDRW2100041665.1.12",
          "form": "같아요   ㅋㅋㅋㅋㅋㅋ",
          "original_form": "같아요   ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:12"
        },
        {
          "id": "MDRW2100041665.1.13",
          "form": "아 그런 맛이구나!!",
          "original_form": "아 그런 맛이구나!!",
          "speaker_id": "1",
          "time": "20210906 14:14"
        },
        {
          "id": "MDRW2100041665.1.14",
          "form": "저는 뭐 탄산일까 했어요",
          "original_form": "저는 뭐 탄산일까 했어요",
          "speaker_id": "1",
          "time": "20210906 14:14"
        },
        {
          "id": "MDRW2100041665.1.15",
          "form": "첨가물도 좀들어있기는 했지만...",
          "original_form": "첨가물도 좀들어있기는 했지만...",
          "speaker_id": "2",
          "time": "20210906 14:14"
        },
        {
          "id": "MDRW2100041665.1.16",
          "form": "물못마시는 저에게 큰 도움이 됐어요 ㅋ",
          "original_form": "물못마시는 저에게 큰 도움이 됐어요 ㅋ",
          "speaker_id": "2",
          "time": "20210906 14:15"
        },
        {
          "id": "MDRW2100041665.1.17",
          "form": "ㅋㅋㅋㅋ 아 저는 그런 달달한 맛을 안좋아해서",
          "original_form": "ㅋㅋㅋㅋ 아 저는 그런 달달한 맛을 안좋아해서",
