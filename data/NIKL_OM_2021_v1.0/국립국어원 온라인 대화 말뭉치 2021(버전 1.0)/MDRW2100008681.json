      "utterance": [
        {
          "id": "MDRW2100008681.1.1",
          "form": "가족은 어떻게 되세요?",
          "original_form": "가족은 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210822 17:23"
        },
        {
          "id": "MDRW2100008681.1.2",
          "form": "저희 4명이요",
          "original_form": "저희 4명이요",
          "speaker_id": "2",
          "time": "20210822 17:23"
        },
        {
          "id": "MDRW2100008681.1.3",
          "form": "헉 저희도인데",
          "original_form": "헉 저희도인데",
          "speaker_id": "1",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008681.1.4",
          "form": "딸하나 아들하나!!",
          "original_form": "딸하나 아들하나!!",
          "speaker_id": "1",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008681.1.5",
          "form": "아 저흰 아들만 둘이에요",
          "original_form": "아 저흰 아들만 둘이에요",
          "speaker_id": "2",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008681.1.6",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008681.1.7",
          "form": "힘들지 않으세요? ㅋㅋ",
          "original_form": "힘들지 않으세요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008681.1.8",
          "form": "아무래도 아들둘이 활동적이다보니",
          "original_form": "아무래도 아들둘이 활동적이다보니",
          "speaker_id": "2",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008681.1.9",
          "form": "좀 더 말썽이긴해요",
          "original_form": "좀 더 말썽이긴해요",
          "speaker_id": "2",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008681.1.10",
          "form": "그래도 듬직하시겠다",
          "original_form": "그래도 듬직하시겠다",
          "speaker_id": "1",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008681.1.11",
          "form": "네 좋아요",
          "original_form": "네 좋아요",
          "speaker_id": "2",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008681.1.12",
          "form": "첫째가 딸?",
          "original_form": "첫째가 딸?",
          "speaker_id": "2",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008681.1.13",
          "form": "아뇨 막내가 딸이요!",
          "original_form": "아뇨 막내가 딸이요!",
          "speaker_id": "1",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008681.1.14",
          "form": "근데 둘다 사이가 좋아서",
          "original_form": "근데 둘다 사이가 좋아서",
          "speaker_id": "1",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008681.1.15",
          "form": "잘 지내요",
          "original_form": "잘 지내요",
          "speaker_id": "1",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008681.1.16",
          "form": "남매끼리 안 친한 애들도 많은데",
          "original_form": "남매끼리 안 친한 애들도 많은데",
          "speaker_id": "2",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008681.1.17",
          "form": "좋으시겠어요",
          "original_form": "좋으시겠어요",
          "speaker_id": "2",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008681.1.18",
          "form": "네네 다행이죠",
          "original_form": "네네 다행이죠",
          "speaker_id": "1",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008681.1.19",
          "form": "다같이 사시는건가요?",
          "original_form": "다같이 사시는건가요?",
          "speaker_id": "1",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100008681.1.20",
          "form": "아뇨 아들 하나는 다른 지역에 가 있어요",
          "original_form": "아뇨 아들 하나는 다른 지역에 가 있어요",
          "speaker_id": "2",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100008681.1.21",
          "form": "아 보고싶으시겠어요",
          "original_form": "아 보고싶으시겠어요",
          "speaker_id": "1",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100008681.1.22",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100008681.1.23",
          "form": "근데 한달에 2번 정도 올라와서",
          "original_form": "근데 한달에 2번 정도 올라와서",
          "speaker_id": "2",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100008681.1.24",
          "form": "괜찮아요",
          "original_form": "괜찮아요",
          "speaker_id": "2",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100008681.1.25",
          "form": "음 효자시구나 ㅎㅎ",
          "original_form": "음 효자시구나 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100008681.1.26",
          "form": "그런 편이에요 ㅎㅎㅎ",
          "original_form": "그런 편이에요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 17:27"
        }
      ]
    }
