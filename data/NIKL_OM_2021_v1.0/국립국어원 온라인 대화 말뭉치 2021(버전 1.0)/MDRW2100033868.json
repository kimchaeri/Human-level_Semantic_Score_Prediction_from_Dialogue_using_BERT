      "utterance": [
        {
          "id": "MDRW2100033868.1.1",
          "form": "저승갔다가 왔죠...",
          "original_form": "저승갔다가 왔죠...",
          "speaker_id": "1",
          "time": "20210825 13:40"
        },
        {
          "id": "MDRW2100033868.1.2",
          "form": "헉전 안봐요",
          "original_form": "헉전 안봐요",
          "speaker_id": "2",
          "time": "20210825 13:40"
        },
        {
          "id": "MDRW2100033868.1.3",
          "form": "미장도 어제 쫌 ㅇ오랐어요",
          "original_form": "미장도 어제 쫌 ㅇ오랐어요",
          "speaker_id": "2",
          "time": "20210825 13:40"
        },
        {
          "id": "MDRW2100033868.1.4",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 13:40"
        },
        {
          "id": "MDRW2100033868.1.5",
          "form": "올랐어요",
          "original_form": "올랐어요",
          "speaker_id": "2",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100033868.1.6",
          "form": "아직  나락입니다",
          "original_form": "아직  나락입니다",
          "speaker_id": "2",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100033868.1.7",
          "form": "오늘은..  떨어더라구요...",
          "original_form": "오늘은..  떨어더라구요...",
          "speaker_id": "1",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100033868.1.8",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100033868.1.9",
          "form": "잠깐이죠",
          "original_form": "잠깐이죠",
          "speaker_id": "2",
          "time": "20210825 13:41"
        },
        {
          "id": "MDRW2100033868.1.10",
          "form": "내려가면...오를때도 있겠죠?!ㅋㅋ",
          "original_form": "내려가면...오를때도 있겠죠?!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100033868.1.11",
          "form": "집안일에 회사에 주식에 집에서 정말 바쁘시네요",
          "original_form": "집안일에 회사에 주식에 집에서 정말 바쁘시네요",
          "speaker_id": "2",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100033868.1.12",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100033868.1.13",
          "form": "존버는 승리하죠",
          "original_form": "존버는 승리하죠",
          "speaker_id": "2",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100033868.1.14",
          "form": "저 어제 탈출했어요 ㅋㅋ",
          "original_form": "저 어제 탈출했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100033868.1.15",
          "form": "한개",
          "original_form": "한개",
          "speaker_id": "2",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100033868.1.16",
          "form": "저도 잠깐발담근 삼전...탈출이요ㅋㅋㅋ",
          "original_form": "저도 잠깐발담근 삼전...탈출이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100033868.1.17",
          "form": "오 축하요 ㅎㅎ",
          "original_form": "오 축하요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100033868.1.18",
          "form": "그래서 정신이 하나도 없는거 같애요ㅋㅋㅋ",
          "original_form": "그래서 정신이 하나도 없는거 같애요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100033868.1.19",
          "form": "가정에 보탬되고자 한건데",
          "original_form": "가정에 보탬되고자 한건데",
          "speaker_id": "2",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100033868.1.20",
          "form": "시간쪼개서 ㅋㅋ",
          "original_form": "시간쪼개서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100033868.1.21",
          "form": "그쵸..  근데 마이너스...라서ㅋㄱㅋㄱㄱㄱ",
          "original_form": "그쵸..  근데 마이너스...라서ㅋㄱㅋㄱㄱㄱ",
          "speaker_id": "1",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100033868.1.22",
          "form": "이건뭐 가정에 보탬이 안돼요 ㅋㅋ",
          "original_form": "이건뭐 가정에 보탬이 안돼요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100033868.1.23",
          "form": "이제 다 탈출하면..  공모주만 할려구요",
          "original_form": "이제 다 탈출하면..  공모주만 할려구요",
          "speaker_id": "1",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100033868.1.24",
          "form": "그래서 아이들 학원비하게",
          "original_form": "그래서 아이들 학원비하게",
          "speaker_id": "2",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100033868.1.25",
          "form": "공모주 올인요",
          "original_form": "공모주 올인요",
          "speaker_id": "2",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100033868.1.26",
          "form": "저도저도 똑같애요!",
          "original_form": "저도저도 똑같애요!",
          "speaker_id": "1",
          "time": "20210825 13:44"
        },
        {
          "id": "MDRW2100033868.1.27",
          "form": "요새 그나마 나아요",
          "original_form": "요새 그나마 나아요",
          "speaker_id": "2",
          "time": "20210825 13:44"
        }
      ]
    }
