      "utterance": [
        {
          "id": "MDRW2100003581.1.1",
          "form": "음료수 어떤거 좋아하세요?",
          "original_form": "음료수 어떤거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 02:41"
        },
        {
          "id": "MDRW2100003581.1.2",
          "form": "저는 콜라 좋아해요!",
          "original_form": "저는 콜라 좋아해요!",
          "speaker_id": "1",
          "time": "20210821 02:42"
        },
        {
          "id": "MDRW2100003581.1.3",
          "form": "저두요!",
          "original_form": "저두요!",
          "speaker_id": "2",
          "time": "20210821 02:42"
        },
        {
          "id": "MDRW2100003581.1.4",
          "form": "근데 살찔까봐 다이어트 콜라 마셔요 ㅋㅋ",
          "original_form": "근데 살찔까봐 다이어트 콜라 마셔요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 02:43"
        },
        {
          "id": "MDRW2100003581.1.5",
          "form": "아 저도 최근에 그 맛을 알았어요",
          "original_form": "아 저도 최근에 그 맛을 알았어요",
          "speaker_id": "1",
          "time": "20210821 02:44"
        },
        {
          "id": "MDRW2100003581.1.6",
          "form": "예전에 제로콜라 궁금해서 한번 마셔보고",
          "original_form": "예전에 제로콜라 궁금해서 한번 마셔보고",
          "speaker_id": "1",
          "time": "20210821 02:44"
        },
        {
          "id": "MDRW2100003581.1.7",
          "form": "그니까요 맛있죠!",
          "original_form": "그니까요 맛있죠!",
          "speaker_id": "2",
          "time": "20210821 02:44"
        },
        {
          "id": "MDRW2100003581.1.8",
          "form": "네 ㅋㅋㅋ 그땐 이상하길래 내돈주고 절대 안사먹어야지 했는데",
          "original_form": "네 ㅋㅋㅋ 그땐 이상하길래 내돈주고 절대 안사먹어야지 했는데",
          "speaker_id": "1",
          "time": "20210821 02:45"
        },
        {
          "id": "MDRW2100003581.1.9",
          "form": "감미료맛이 중독이된다니까요 ㅋㅋㅋ!!",
          "original_form": "감미료맛이 중독이된다니까요 ㅋㅋㅋ!!",
          "speaker_id": "2",
          "time": "20210821 02:45"
        },
        {
          "id": "MDRW2100003581.1.10",
          "form": "아 ㅋㅋㅋ저도 처음에 그랬어요 ㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋ저도 처음에 그랬어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 02:45"
        },
        {
          "id": "MDRW2100003581.1.11",
          "form": "얼마전에 다시 먹어보니 괜찮은거예요ㅋㅋㅋ",
          "original_form": "얼마전에 다시 먹어보니 괜찮은거예요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 02:45"
        },
        {
          "id": "MDRW2100003581.1.12",
          "form": "그래서 일주일전에 처음으로",
          "original_form": "그래서 일주일전에 처음으로",
          "speaker_id": "1",
          "time": "20210821 02:45"
        },
        {
          "id": "MDRW2100003581.1.13",
          "form": "오오오 제로콜라의 늪으로 온걸",
          "original_form": "오오오 제로콜라의 늪으로 온걸",
          "speaker_id": "2",
          "time": "20210821 02:45"
        },
        {
          "id": "MDRW2100003581.1.14",
          "form": "제돈주고 사왔어요 ㅋㅋㅋ",
          "original_form": "제돈주고 사왔어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 02:45"
        },
        {
          "id": "MDRW2100003581.1.15",
          "form": "환영합니다 ㅋㅋㅋ",
          "original_form": "환영합니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 02:45"
        },
        {
          "id": "MDRW2100003581.1.16",
          "form": "전 요즘 싹 다 제로로 된 음료만 마셔요",
          "original_form": "전 요즘 싹 다 제로로 된 음료만 마셔요",
          "speaker_id": "2",
          "time": "20210821 02:45"
        },
        {
          "id": "MDRW2100003581.1.17",
          "form": "어떤 게 맛있나요?",
          "original_form": "어떤 게 맛있나요?",
          "speaker_id": "1",
          "time": "20210821 02:45"
        },
        {
          "id": "MDRW2100003581.1.18",
          "form": "전 나랑드 사이다랑 제로콜라 좋아해요",
          "original_form": "전 나랑드 사이다랑 제로콜라 좋아해요",
          "speaker_id": "2",
          "time": "20210821 02:46"
        },
        {
          "id": "MDRW2100003581.1.19",
          "form": "사이다는 나랑드 !",
          "original_form": "사이다는 나랑드 !",
          "speaker_id": "2",
          "time": "20210821 02:46"
        },
        {
          "id": "MDRW2100003581.1.20",
          "form": "헐 저 얼마전에 나랑드랑 고민하다가 제로칠성 사왔는데ㅠㅠ",
          "original_form": "헐 저 얼마전에 나랑드랑 고민하다가 제로칠성 사왔는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 02:46"
        },
        {
          "id": "MDRW2100003581.1.21",
          "form": "아직 먹어보진 않았어요",
          "original_form": "아직 먹어보진 않았어요",
          "speaker_id": "1",
          "time": "20210821 02:46"
        },
        {
          "id": "MDRW2100003581.1.22",
          "form": "제로칠성은 제취향이 아니더라구요",
          "original_form": "제로칠성은 제취향이 아니더라구요",
          "speaker_id": "2",
          "time": "20210821 02:47"
        },
        {
          "id": "MDRW2100003581.1.23",
          "form": "나랑드 괜찮아요!",
          "original_form": "나랑드 괜찮아요!",
          "speaker_id": "2",
          "time": "20210821 02:47"
        },
        {
          "id": "MDRW2100003581.1.24",
          "form": "앗 그런가요?ㅠㅠ",
          "original_form": "앗 그런가요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 02:48"
        },
        {
          "id": "MDRW2100003581.1.25",
          "form": "둘다 안 먹어봤는데 궁금해서 사본거라",
          "original_form": "둘다 안 먹어봤는데 궁금해서 사본거라",
          "speaker_id": "1",
          "time": "20210821 02:48"
        },
        {
          "id": "MDRW2100003581.1.26",
          "form": "다음엔 나랑드 사봐야겠네요",
          "original_form": "다음엔 나랑드 사봐야겠네요",
          "speaker_id": "1",
          "time": "20210821 02:48"
        },
        {
          "id": "MDRW2100003581.1.27",
          "form": "강추강추",
          "original_form": "강추강추",
          "speaker_id": "2",
          "time": "20210821 02:49"
        },
        {
          "id": "MDRW2100003581.1.28",
          "form": "맛이 완전 달라요!",
          "original_form": "맛이 완전 달라요!",
          "speaker_id": "2",
          "time": "20210821 02:49"
        },
        {
          "id": "MDRW2100003581.1.29",
          "form": "근데 제로콜라 마시고 ㅋㅋ 과자먹어요",
          "original_form": "근데 제로콜라 마시고 ㅋㅋ 과자먹어요",
          "speaker_id": "2",
          "time": "20210821 02:49"
        },
        {
          "id": "MDRW2100003581.1.30",
          "form": "제로콜라 마시는 의미 0",
          "original_form": "제로콜라 마시는 의미 0",
          "speaker_id": "2",
          "time": "20210821 02:49"
        },
        {
          "id": "MDRW2100003581.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210821 02:50"
        },
        {
          "id": "MDRW2100003581.1.32",
          "form": "그래도 죄책감을 조금이라도...ㅋㅋㅋ",
          "original_form": "그래도 죄책감을 조금이라도...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 02:50"
        },
        {
          "id": "MDRW2100003581.1.33",
          "form": "맞아요 제가 그 마인드에요",
          "original_form": "맞아요 제가 그 마인드에요",
          "speaker_id": "2",
          "time": "20210821 02:51"
        }
      ]
    }
