      "utterance": [
        {
          "id": "MDRW2100037381.1.1",
          "form": "오토바이 타고 다니고",
          "original_form": "오토바이 타고 다니고",
          "speaker_id": "2",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100037381.1.2",
          "form": "만만한 사람한테는",
          "original_form": "만만한 사람한테는",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100037381.1.3",
          "form": "괜히 무섭게 말한다",
          "original_form": "괜히 무섭게 말한다",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100037381.1.4",
          "form": "이러니까",
          "original_form": "이러니까",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100037381.1.5",
          "form": "비추천 엄청 먹고",
          "original_form": "비추천 엄청 먹고",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100037381.1.6",
          "form": "열폭하지 말라는 말만 들었어요ㅠ",
          "original_form": "열폭하지 말라는 말만 들었어요ㅠ",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100037381.1.7",
          "form": "사람들이 별로 안 믿어 줬나 봐요",
          "original_form": "사람들이 별로 안 믿어 줬나 봐요",
          "speaker_id": "1",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100037381.1.8",
          "form": "그리고 툭하면 싸워서",
          "original_form": "그리고 툭하면 싸워서",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100037381.1.9",
          "form": "맞아요 그냥 나름...잘생겨서 다들 믿고싶지 않았나",
          "original_form": "맞아요 그냥 나름...잘생겨서 다들 믿고싶지 않았나",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100037381.1.10",
          "form": "원래 뻔뻔 하니까 연예인 하는 거 같아요",
          "original_form": "원래 뻔뻔 하니까 연예인 하는 거 같아요",
          "speaker_id": "1",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100037381.1.11",
          "form": "툭하면 싸워서 복도에서",
          "original_form": "툭하면 싸워서 복도에서",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100037381.1.12",
          "form": "맞아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100037381.1.13",
          "form": "자기꺼 벗겨진 실내화 주우러 다니는거 봤어요",
          "original_form": "자기꺼 벗겨진 실내화 주우러 다니는거 봤어요",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100037381.1.14",
          "form": "방송에서는 착한 척 해야 하는 것도 연기했죠",
          "original_form": "방송에서는 착한 척 해야 하는 것도 연기했죠",
          "speaker_id": "1",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100037381.1.15",
          "form": "그니까요,,, 실제로는 하나도 안착해요",
          "original_form": "그니까요,,, 실제로는 하나도 안착해요",
          "speaker_id": "2",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100037381.1.16",
          "form": "그 집안이 잘살아서",
          "original_form": "그 집안이 잘살아서",
          "speaker_id": "2",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100037381.1.17",
          "form": "더 수월하게 연예인 된거예요",
          "original_form": "더 수월하게 연예인 된거예요",
          "speaker_id": "2",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100037381.1.18",
          "form": "누나도 아나운서거든요",
          "original_form": "누나도 아나운서거든요",
          "speaker_id": "2",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100037381.1.19",
          "form": "연예인 되려면 인맥이 많이 필요하니까",
          "original_form": "연예인 되려면 인맥이 많이 필요하니까",
          "speaker_id": "1",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100037381.1.20",
          "form": "맞아요ㅠ 그리고 이젠",
          "original_form": "맞아요ㅠ 그리고 이젠",
          "speaker_id": "2",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100037381.1.21",
          "form": "집안이 잘 살아야 연예인 시켜주는 것 같기도 하고...",
          "original_form": "집안이 잘 살아야 연예인 시켜주는 것 같기도 하고...",
          "speaker_id": "2",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100037381.1.22",
          "form": "돈 많으면 좋죠 머",
          "original_form": "돈 많으면 좋죠 머",
          "speaker_id": "1",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100037381.1.23",
          "form": "넷플릭스 자주 보세요?",
          "original_form": "넷플릭스 자주 보세요?",
          "speaker_id": "1",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100037381.1.24",
          "form": "네네!!",
          "original_form": "네네!!",
          "speaker_id": "2",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100037381.1.25",
          "form": "저 넷플릭스랑 왓챠 둘 다 구독해요 ㅎㅎㅎ",
          "original_form": "저 넷플릭스랑 왓챠 둘 다 구독해요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100037381.1.26",
          "form": "영화 좋아하시나 봐요",
          "original_form": "영화 좋아하시나 봐요",
          "speaker_id": "1",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100037381.1.27",
          "form": "네 영화도 좋아하구... 뭔가 다른 세계에 다녀오는 기분이라!!!",
          "original_form": "네 영화도 좋아하구... 뭔가 다른 세계에 다녀오는 기분이라!!!",
          "speaker_id": "2",
          "time": "20210831 23:07"
        }
      ]
    }
