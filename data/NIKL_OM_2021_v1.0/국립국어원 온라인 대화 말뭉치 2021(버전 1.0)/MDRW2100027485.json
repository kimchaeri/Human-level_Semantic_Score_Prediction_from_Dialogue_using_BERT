      "utterance": [
        {
          "id": "MDRW2100027485.1.1",
          "form": "저컴터사는데 지금",
          "original_form": "저컴터사는데 지금",
          "speaker_id": "2",
          "time": "20210902 13:05"
        },
        {
          "id": "MDRW2100027485.1.2",
          "form": "홍철없는 홍철팀입니당",
          "original_form": "홍철없는 홍철팀입니당",
          "speaker_id": "2",
          "time": "20210902 13:05"
        },
        {
          "id": "MDRW2100027485.1.3",
          "form": "name2은 효율성을",
          "original_form": "&name2&은 효율성을",
          "speaker_id": "1",
          "time": "20210902 13:05"
        },
        {
          "id": "MDRW2100027485.1.4",
          "form": "중요시하시는 것 같아요!",
          "original_form": "중요시하시는 것 같아요!",
          "speaker_id": "1",
          "time": "20210902 13:05"
        },
        {
          "id": "MDRW2100027485.1.5",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋ 너무 웃겨요 홍철 없는 홍철팀 맞는 말이네요",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋ 너무 웃겨요 홍철 없는 홍철팀 맞는 말이네요",
          "speaker_id": "1",
          "time": "20210902 13:05"
        },
        {
          "id": "MDRW2100027485.1.6",
          "form": "본체만 사면끝인뎅",
          "original_form": "본체만 사면끝인뎅",
          "speaker_id": "2",
          "time": "20210902 13:05"
        },
        {
          "id": "MDRW2100027485.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 13:05"
        },
        {
          "id": "MDRW2100027485.1.8",
          "form": "게임이야 머든 다 돌아가니..",
          "original_form": "게임이야 머든 다 돌아가니..",
          "speaker_id": "2",
          "time": "20210902 13:05"
        },
        {
          "id": "MDRW2100027485.1.9",
          "form": "게임을 정말 좋아하시네요!",
          "original_form": "게임을 정말 좋아하시네요!",
          "speaker_id": "1",
          "time": "20210902 13:06"
        },
        {
          "id": "MDRW2100027485.1.10",
          "form": "어떤 사이트 통해서 구매하시나요?",
          "original_form": "어떤 사이트 통해서 구매하시나요?",
          "speaker_id": "1",
          "time": "20210902 13:06"
        },
        {
          "id": "MDRW2100027485.1.11",
          "form": "저는 친구가 견적맞춰줘서",
          "original_form": "저는 친구가 견적맞춰줘서",
          "speaker_id": "2",
          "time": "20210902 13:15"
        },
        {
          "id": "MDRW2100027485.1.12",
          "form": "삽니당",
          "original_form": "삽니당",
          "speaker_id": "2",
          "time": "20210902 13:15"
        },
        {
          "id": "MDRW2100027485.1.13",
          "form": "오버시스템에서 맞췄어요",
          "original_form": "오버시스템에서 맞췄어요",
          "speaker_id": "2",
          "time": "20210902 13:15"
        },
        {
          "id": "MDRW2100027485.1.14",
          "form": "좋은 친구를 두셨네요!!!",
          "original_form": "좋은 친구를 두셨네요!!!",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027485.1.15",
          "form": "오버시스템이라...",
          "original_form": "오버시스템이라...",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027485.1.16",
          "form": "생소하지만 많은 걸 알게되네요 ㅎㅎㅎ",
          "original_form": "생소하지만 많은 걸 알게되네요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027485.1.17",
          "form": "ㅋㅋㅋㅋ 다나와도있고",
          "original_form": "ㅋㅋㅋㅋ 다나와도있고",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027485.1.18",
          "form": "여러싸이트 있는거같아요",
          "original_form": "여러싸이트 있는거같아요",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027485.1.19",
          "form": "저도 여러게에서 맙춰보고했어요",
          "original_form": "저도 여러게에서 맙춰보고했어요",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027485.1.20",
          "form": "오오 가격비교는 역시 여러 군데에서 해야죠!",
          "original_form": "오오 가격비교는 역시 여러 군데에서 해야죠!",
          "speaker_id": "1",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100027485.1.21",
          "form": "저는 텐바이텐에서 광고 메일이 왔길래",
          "original_form": "저는 텐바이텐에서 광고 메일이 왔길래",
          "speaker_id": "1",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100027485.1.22",
          "form": "구경하는데... 요즘은 키보드도 정말 예쁘게 나오네요?",
          "original_form": "구경하는데... 요즘은 키보드도 정말 예쁘게 나오네요?",
          "speaker_id": "1",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100027485.1.23",
          "form": "맞어요 저는그냥엘이디있는 블랙으로 샀어요 ㅋㅋㅋ",
          "original_form": "맞어요 저는그냥엘이디있는 블랙으로 샀어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027485.1.24",
          "form": "name2 역시 시크하시네요!",
          "original_form": "&name2& 역시 시크하시네요!",
          "speaker_id": "1",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100027485.1.25",
          "form": "제 친구들은 아주 그녕",
          "original_form": "제 친구들은 아주 그녕",
          "speaker_id": "1",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100027485.1.26",
          "form": "그냥",
          "original_form": "그냥",
          "speaker_id": "1",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100027485.1.27",
          "form": "알록달록한거 사더라구요 ㅋㅋㅋㅋ",
          "original_form": "알록달록한거 사더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027485.1.28",
          "form": "저는 흰색사면 관리못해서 안대요",
          "original_form": "저는 흰색사면 관리못해서 안대요",
          "speaker_id": "2",
          "time": "20210902 14:58"
        },
        {
          "id": "MDRW2100027485.1.29",
          "form": "흰색은 옷이든 물건이든 관리하기 힘든거같아요 ㅎㅎ",
          "original_form": "흰색은 옷이든 물건이든 관리하기 힘든거같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027485.1.30",
          "form": "맞아여 흰색땀흘리먄",
          "original_form": "맞아여 흰색땀흘리먄",
          "speaker_id": "2",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100027485.1.31",
          "form": "누래지는거 최악..",
          "original_form": "누래지는거 최악..",
          "speaker_id": "2",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100027485.1.32",
          "form": "맞아요!!!!!",
          "original_form": "맞아요!!!!!",
          "speaker_id": "1",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100027485.1.33",
          "form": "운동화도...",
          "original_form": "운동화도...",
          "speaker_id": "1",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100027485.1.34",
          "form": "그래도",
          "original_form": "그래도",
          "speaker_id": "2",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100027485.1.35",
          "form": "근데 저는 왜이렇게 하얀 운동화가 좋은지!!!",
          "original_form": "근데 저는 왜이렇게 하얀 운동화가 좋은지!!!",
          "speaker_id": "1",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100027485.1.36",
