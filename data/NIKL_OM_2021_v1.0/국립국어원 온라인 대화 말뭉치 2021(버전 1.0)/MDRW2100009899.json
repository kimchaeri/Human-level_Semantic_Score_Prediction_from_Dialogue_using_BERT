      "utterance": [
        {
          "id": "MDRW2100009899.1.1",
          "form": "와이프 분은 어떤 분야 쪽 일을 하고 싶어하시나요",
          "original_form": "와이프 분은 어떤 분야 쪽 일을 하고 싶어하시나요",
          "speaker_id": "2",
          "time": "20210823 11:32"
        },
        {
          "id": "MDRW2100009899.1.2",
          "form": "사무직쪽 일 찾고 있어요",
          "original_form": "사무직쪽 일 찾고 있어요",
          "speaker_id": "1",
          "time": "20210823 11:33"
        },
        {
          "id": "MDRW2100009899.1.3",
          "form": "준비가 안되있어서 이제부터 영어점수 만들고",
          "original_form": "준비가 안되있어서 이제부터 영어점수 만들고",
          "speaker_id": "1",
          "time": "20210823 11:33"
        },
        {
          "id": "MDRW2100009899.1.4",
          "form": "공부도 해야해요",
          "original_form": "공부도 해야해요",
          "speaker_id": "1",
          "time": "20210823 11:33"
        },
        {
          "id": "MDRW2100009899.1.5",
          "form": "저도 토익 준비도하고있어요 ㅋㅋ",
          "original_form": "저도 토익 준비도하고있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:34"
        },
        {
          "id": "MDRW2100009899.1.6",
          "form": "공부의 시작은 토익인가 ㅋㅋ",
          "original_form": "공부의 시작은 토익인가 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:34"
        },
        {
          "id": "MDRW2100009899.1.7",
          "form": "가사일은 어떻게 분담하고 계세요?",
          "original_form": "가사일은 어떻게 분담하고 계세요?",
          "speaker_id": "2",
          "time": "20210823 11:34"
        },
        {
          "id": "MDRW2100009899.1.8",
          "form": "딱딱 정해서 하지는 않고",
          "original_form": "딱딱 정해서 하지는 않고",
          "speaker_id": "1",
          "time": "20210823 12:13"
        },
        {
          "id": "MDRW2100009899.1.9",
          "form": "그때 그때 서로 알아서 해요",
          "original_form": "그때 그때 서로 알아서 해요",
          "speaker_id": "1",
          "time": "20210823 12:13"
        },
        {
          "id": "MDRW2100009899.1.10",
          "form": "어제는 제가 화장실청소하고 설거지했어요",
          "original_form": "어제는 제가 화장실청소하고 설거지했어요",
          "speaker_id": "1",
          "time": "20210823 12:13"
        },
        {
          "id": "MDRW2100009899.1.11",
          "form": "저희는 가사분담으로 문제되는건 없어서",
          "original_form": "저희는 가사분담으로 문제되는건 없어서",
          "speaker_id": "1",
          "time": "20210823 12:14"
        },
        {
          "id": "MDRW2100009899.1.12",
          "form": "결혼하셨어요?",
          "original_form": "결혼하셨어요?",
          "speaker_id": "1",
          "time": "20210823 12:14"
        },
        {
          "id": "MDRW2100009899.1.13",
          "form": "아니요 아직 미혼입니다",
          "original_form": "아니요 아직 미혼입니다",
          "speaker_id": "2",
          "time": "20210823 12:30"
        },
        {
          "id": "MDRW2100009899.1.14",
          "form": "저는 엄마랑 살고 있어요 ㅎㅎ",
          "original_form": "저는 엄마랑 살고 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 12:30"
        },
        {
          "id": "MDRW2100009899.1.15",
          "form": "아하 그렇구나",
          "original_form": "아하 그렇구나",
          "speaker_id": "1",
          "time": "20210823 12:33"
        },
        {
          "id": "MDRW2100009899.1.16",
          "form": "저는 성인되면서부터는 쭉 나가살았었는데",
          "original_form": "저는 성인되면서부터는 쭉 나가살았었는데",
          "speaker_id": "1",
          "time": "20210823 12:34"
        },
        {
          "id": "MDRW2100009899.1.17",
          "form": "가끔 부모님이랑 같이 살던 때가 그립네요",
          "original_form": "가끔 부모님이랑 같이 살던 때가 그립네요",
          "speaker_id": "1",
          "time": "20210823 12:34"
        },
        {
          "id": "MDRW2100009899.1.18",
          "form": "저는 나가살았을때도 그럭저럭 잘살았는데",
          "original_form": "저는 나가살았을때도 그럭저럭 잘살았는데",
          "speaker_id": "2",
          "time": "20210823 12:35"
        },
        {
          "id": "MDRW2100009899.1.19",
          "form": "지금은 그저 월세를 안내게 해주셔서 감사합니다란 마음으로 살아요 ㅋㅋ",
          "original_form": "지금은 그저 월세를 안내게 해주셔서 감사합니다란 마음으로 살아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 12:36"
        },
        {
          "id": "MDRW2100009899.1.20",
          "form": "아 월세...저도 지금 월세 내며 사는데",
          "original_form": "아 월세...저도 지금 월세 내며 사는데",
          "speaker_id": "1",
          "time": "20210823 12:38"
        },
        {
          "id": "MDRW2100009899.1.21",
          "form": "정말 최악적이네요",
          "original_form": "정말 최악적이네요",
          "speaker_id": "1",
          "time": "20210823 12:38"
        },
        {
          "id": "MDRW2100009899.1.22",
          "form": "돈이 안모여",
          "original_form": "돈이 안모여",
          "speaker_id": "1",
          "time": "20210823 12:38"
        },
        {
          "id": "MDRW2100009899.1.23",
          "form": "그래서 가사일을 제가 좀 하려고하는데",
          "original_form": "그래서 가사일을 제가 좀 하려고하는데",
          "speaker_id": "2",
          "time": "20210823 12:42"
        },
        {
          "id": "MDRW2100009899.1.24",
          "form": "너무 성에 안 차 하세요 ㅋㅋ",
          "original_form": "너무 성에 안 차 하세요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 12:42"
        },
        {
          "id": "MDRW2100009899.1.25",
          "form": "어쩔 수 없죠 더 도와 드리는 수밖에 ㅎㅎ",
          "original_form": "어쩔 수 없죠 더 도와 드리는 수밖에 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 12:48"
        },
        {
          "id": "MDRW2100009899.1.26",
          "form": "근데 티도 안나는데 사실 썩 잘하지도 못해서요",
          "original_form": "근데 티도 안나는데 사실 썩 잘하지도 못해서요",
          "speaker_id": "2",
          "time": "20210823 14:01"
        },
        {
          "id": "MDRW2100009899.1.27",
          "form": "화장실청소 곰팡이 제거가 힘든것같아요",
          "original_form": "화장실청소 곰팡이 제거가 힘든것같아요",
          "speaker_id": "2",
          "time": "20210823 14:01"
        },
        {
          "id": "MDRW2100009899.1.28",
          "form": "저 바닷가 근처 사는데 곰팡이 제일 골치 아파요",
          "original_form": "저 바닷가 근처 사는데 곰팡이 제일 골치 아파요",
          "speaker_id": "1",
          "time": "20210823 14:05"
        }
      ]
