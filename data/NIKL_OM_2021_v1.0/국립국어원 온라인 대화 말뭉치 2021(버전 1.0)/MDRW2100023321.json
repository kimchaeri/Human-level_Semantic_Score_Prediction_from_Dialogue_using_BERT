      "utterance": [
        {
          "id": "MDRW2100023321.1.1",
          "form": "근데 어떻게 결혼 결심 하셨나요ㅋㅋ",
          "original_form": "근데 어떻게 결혼 결심 하셨나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:22"
        },
        {
          "id": "MDRW2100023321.1.2",
          "form": "엄...시댁에서 갑자기 얼른결혼해라 해서요?ㅋㅋㅋㅋ",
          "original_form": "엄...시댁에서 갑자기 얼른결혼해라 해서요?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:23"
        },
        {
          "id": "MDRW2100023321.1.3",
          "form": "7년사귀니까 이제 빨리하래요.ㅋㅋㅋ",
          "original_form": "7년사귀니까 이제 빨리하래요.ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:23"
        },
        {
          "id": "MDRW2100023321.1.4",
          "form": "너무퐝당하죠?ㅋㅋ",
          "original_form": "너무퐝당하죠?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:23"
        },
        {
          "id": "MDRW2100023321.1.5",
          "form": "ㅋㅋㅋㅋㅋ헉 ㅋㅋㅋ시댁에서 많이 보태주셨나용..",
          "original_form": "ㅋㅋㅋㅋㅋ헉 ㅋㅋㅋ시댁에서 많이 보태주셨나용..",
          "speaker_id": "2",
          "time": "20210830 00:24"
        },
        {
          "id": "MDRW2100023321.1.6",
          "form": "아니욬ㅋㅋㅋㅋ",
          "original_form": "아니욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:26"
        },
        {
          "id": "MDRW2100023321.1.7",
          "form": "대출과함께 저희집은",
          "original_form": "대출과함께 저희집은",
          "speaker_id": "1",
          "time": "20210830 00:26"
        },
        {
          "id": "MDRW2100023321.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ 집사주신쥴",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 집사주신쥴",
          "speaker_id": "2",
          "time": "20210830 00:26"
        },
        {
          "id": "MDRW2100023321.1.9",
          "form": "신한은행과 같이샀어요 ㅋㅋㅋ",
          "original_form": "신한은행과 같이샀어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:27"
        },
        {
          "id": "MDRW2100023321.1.10",
          "form": "그러게요ㅋㅋㅋ",
          "original_form": "그러게요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:27"
        },
        {
          "id": "MDRW2100023321.1.11",
          "form": "아 근데 그때사서 다행이에요! ㅋㅋㅋㅋ",
          "original_form": "아 근데 그때사서 다행이에요! ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:27"
        },
        {
          "id": "MDRW2100023321.1.12",
          "form": "집도안사주실거면서 왜결혼은ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "집도안사주실거면서 왜결혼은ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:27"
        },
        {
          "id": "MDRW2100023321.1.13",
          "form": "집..너무비싸져서 ㅎ",
          "original_form": "집..너무비싸져서 ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:27"
        },
        {
          "id": "MDRW2100023321.1.14",
          "form": "아니에요ㅠㅠ",
          "original_form": "아니에요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 00:27"
        },
        {
          "id": "MDRW2100023321.1.15",
          "form": "저희는",
          "original_form": "저희는",
          "speaker_id": "1",
          "time": "20210830 00:27"
        },
        {
          "id": "MDRW2100023321.1.16",
          "form": "머나먼 남쪽나라라",
          "original_form": "머나먼 남쪽나라라",
          "speaker_id": "1",
          "time": "20210830 00:27"
        },
        {
          "id": "MDRW2100023321.1.17",
          "form": "아하....ㅜㅜㅜ ㅋㅋㅋㅋㅋ 이런 ,,",
          "original_form": "아하....ㅜㅜㅜ ㅋㅋㅋㅋㅋ 이런 ,,",
          "speaker_id": "2",
          "time": "20210830 00:27"
        },
        {
          "id": "MDRW2100023321.1.18",
          "form": "하긴 .ㅡ주변지역 어디든 다  올랐다던데...",
          "original_form": "하긴 .ㅡ주변지역 어디든 다  올랐다던데...",
          "speaker_id": "1",
          "time": "20210830 00:28"
        },
        {
          "id": "MDRW2100023321.1.19",
          "form": "ㅋㅋㅋ 내집만 안오르기 ㅎ 프로포즈는 어케받으셨나요 ㅋㅋ",
          "original_form": "ㅋㅋㅋ 내집만 안오르기 ㅎ 프로포즈는 어케받으셨나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:28"
        },
        {
          "id": "MDRW2100023321.1.20",
          "form": "저 프로포즈 못받았어요ㅠㅠ",
          "original_form": "저 프로포즈 못받았어요ㅠㅠ",
