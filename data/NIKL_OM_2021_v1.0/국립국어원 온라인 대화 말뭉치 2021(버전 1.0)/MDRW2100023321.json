          "time": "20210830 00:30"
        },
        {
          "id": "MDRW2100023321.1.21",
          "form": "지금이라도",
          "original_form": "지금이라도",
          "speaker_id": "1",
          "time": "20210830 00:30"
        },
        {
          "id": "MDRW2100023321.1.22",
          "form": "궁뎅이 차고싶네요 ㅋㅋ",
          "original_form": "궁뎅이 차고싶네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:30"
        },
        {
          "id": "MDRW2100023321.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:32"
        },
        {
          "id": "MDRW2100023321.1.24",
          "form": "뭐라고 안하셨어요?",
          "original_form": "뭐라고 안하셨어요?",
          "speaker_id": "2",
          "time": "20210830 00:32"
        },
        {
