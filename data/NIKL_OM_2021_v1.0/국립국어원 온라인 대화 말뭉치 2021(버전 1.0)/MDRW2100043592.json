      "utterance": [
        {
          "id": "MDRW2100043592.1.1",
          "form": "화장 잘 하시나요?",
          "original_form": "화장 잘 하시나요?",
          "speaker_id": "1",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043592.1.2",
          "form": "최근엔 코로나라 한적이 거의없는고같네요",
          "original_form": "최근엔 코로나라 한적이 거의없는고같네요",
          "speaker_id": "2",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043592.1.3",
          "form": "그냥그냥해요 ㅋㅋ",
          "original_form": "그냥그냥해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043592.1.4",
          "form": "전 잘 못하거든요 ㅠ",
          "original_form": "전 잘 못하거든요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043592.1.5",
          "form": "연습하다 보면 되지않을까요?",
          "original_form": "연습하다 보면 되지않을까요?",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043592.1.6",
          "form": "연구도 필요하고 ㅎㅎ",
          "original_form": "연구도 필요하고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043592.1.7",
          "form": "이게 미술에 재능이 좀 있어야 잘하는 것 같아요  전 ㅠ ㅜ",
          "original_form": "이게 미술에 재능이 좀 있어야 잘하는 것 같아요  전 ㅠ ㅜ",
          "speaker_id": "1",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043592.1.8",
          "form": "음 그건아닌거같아요",
          "original_form": "음 그건아닌거같아요",
          "speaker_id": "2",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043592.1.9",
          "form": "미술 하는 분들이 분명 더 잘하긴하겠지만",
          "original_form": "미술 하는 분들이 분명 더 잘하긴하겠지만",
          "speaker_id": "2",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043592.1.10",
          "form": "그래도 어느정도 하시는군요~",
          "original_form": "그래도 어느정도 하시는군요~",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043592.1.11",
          "form": "전 저한테 뭔 색이 어울리는지도 몰라요 ㅋㅋ",
          "original_form": "전 저한테 뭔 색이 어울리는지도 몰라요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043592.1.12",
          "form": "아니에욬ㅋㅋㅋㅋ 경험이 쌓이면 다 보여요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니에욬ㅋㅋㅋㅋ 경험이 쌓이면 다 보여요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043592.1.13",
          "form": "아니면 그냥 남들이 정해주는거 써고 되고",
          "original_form": "아니면 그냥 남들이 정해주는거 써고 되고",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043592.1.14",
          "form": "테스트를 받으러 가야할까봐요",
          "original_form": "테스트를 받으러 가야할까봐요",
          "speaker_id": "1",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043592.1.15",
          "form": "그런 방법도 좋죠 ㅎㅎ",
          "original_form": "그런 방법도 좋죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100043592.1.16",
          "form": "근데 거기서 해주면 예쁜데 집에와서 해보면 똑같은 느낌이 아닐것 같은 느낌이예요 ㅋㅋ",
          "original_form": "근데 거기서 해주면 예쁜데 집에와서 해보면 똑같은 느낌이 아닐것 같은 느낌이예요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043592.1.17",
          "form": "가서도 공부한다는 느낌으로 가셔야..",
          "original_form": "가서도 공부한다는 느낌으로 가셔야..",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043592.1.18",
          "form": "그렇군요. 이것도 공부해야하다니 ㅠ",
          "original_form": "그렇군요. 이것도 공부해야하다니 ㅠ",
          "speaker_id": "1",
