      "utterance": [
        {
          "id": "MDRW2100036770.1.1",
          "form": "안녕하세요 배민 자주쓰세요?",
          "original_form": "안녕하세요 배민 자주쓰세요?",
          "speaker_id": "1",
          "time": "20210831 11:43"
        },
        {
          "id": "MDRW2100036770.1.2",
          "form": "한달에 두번정도 사용해요!",
          "original_form": "한달에 두번정도 사용해요!",
          "speaker_id": "2",
          "time": "20210831 11:56"
        },
        {
          "id": "MDRW2100036770.1.3",
          "form": "자주쓰세요?",
          "original_form": "자주쓰세요?",
          "speaker_id": "2",
          "time": "20210831 11:56"
        },
        {
          "id": "MDRW2100036770.1.4",
          "form": "아뇨 얼마전에 지웠어요",
          "original_form": "아뇨 얼마전에 지웠어요",
          "speaker_id": "1",
          "time": "20210831 11:56"
        },
        {
          "id": "MDRW2100036770.1.5",
          "form": "폰용량이 없어서 지웠네요 ㅋㅋ ㅜㅜ",
          "original_form": "폰용량이 없어서 지웠네요 ㅋㅋ ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 11:56"
        },
        {
          "id": "MDRW2100036770.1.6",
          "form": "그럼 배달음식은 자주 안드시나요?",
          "original_form": "그럼 배달음식은 자주 안드시나요?",
          "speaker_id": "2",
          "time": "20210831 11:59"
        },
        {
          "id": "MDRW2100036770.1.7",
          "form": "네 부모님이랑 살아서요 글고 시키면 그 업체에 시키죠 배민이없으니까요 ㅜ",
          "original_form": "네 부모님이랑 살아서요 글고 시키면 그 업체에 시키죠 배민이없으니까요 ㅜ",
          "speaker_id": "1",
          "time": "20210831 11:59"
        },
        {
          "id": "MDRW2100036770.1.8",
          "form": "그러시구나 그나마 시킨다면 뭘 제일 자주 시키나요?",
          "original_form": "그러시구나 그나마 시킨다면 뭘 제일 자주 시키나요?",
          "speaker_id": "2",
          "time": "20210831 12:00"
        },
        {
          "id": "MDRW2100036770.1.9",
          "form": "그중에 피자 자주 시켜요",
          "original_form": "그중에 피자 자주 시켜요",
          "speaker_id": "1",
          "time": "20210831 12:01"
        },
        {
          "id": "MDRW2100036770.1.10",
          "form": "햄버거도 거의 사와서 먹으니 안시키고요",
          "original_form": "햄버거도 거의 사와서 먹으니 안시키고요",
          "speaker_id": "1",
          "time": "20210831 12:01"
        },
        {
          "id": "MDRW2100036770.1.11",
          "form": "그러시구나 햄버거는 어디꺼 좋아하세요?",
          "original_form": "그러시구나 햄버거는 어디꺼 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 12:01"
        },
        {
          "id": "MDRW2100036770.1.12",
          "form": "좋기는 버거킹인데 비싸서 주로 맥도날드먹어요",
          "original_form": "좋기는 버거킹인데 비싸서 주로 맥도날드먹어요",
          "speaker_id": "1",
          "time": "20210831 12:02"
        },
        {
          "id": "MDRW2100036770.1.13",
          "form": "저는 버거킹 세일할때만 사먹어요",
          "original_form": "저는 버거킹 세일할때만 사먹어요",
          "speaker_id": "2",
          "time": "20210831 12:02"
        },
        {
          "id": "MDRW2100036770.1.14",
          "form": "맥날도 요즘 많이 올랐죠...",
          "original_form": "맥날도 요즘 많이 올랐죠...",
          "speaker_id": "2",
          "time": "20210831 12:03"
        },
        {
          "id": "MDRW2100036770.1.15",
          "form": "저희집 근처 버거킹은 쿠폰이 안되는 지점이더라구요",
          "original_form": "저희집 근처 버거킹은 쿠폰이 안되는 지점이더라구요",
          "speaker_id": "1",
          "time": "20210831 12:04"
        },
        {
          "id": "MDRW2100036770.1.16",
          "form": "맥날도 마니 올랐죠",
          "original_form": "맥날도 마니 올랐죠",
          "speaker_id": "1",
          "time": "20210831 12:05"
        },
        {
          "id": "MDRW2100036770.1.17",
          "form": "쿠폰이 안되는 지점이구나...불편하시겠다",
          "original_form": "쿠폰이 안되는 지점이구나...불편하시겠다",
          "speaker_id": "2",
          "time": "20210831 12:05"
        },
        {
          "id": "MDRW2100036770.1.18",
          "form": "와퍼주로 드시나요?",
          "original_form": "와퍼주로 드시나요?",
          "speaker_id": "2",
          "time": "20210831 12:05"
        },
        {
          "id": "MDRW2100036770.1.19",
          "form": "아뇨 세일하는거요 ㅋㅋ",
          "original_form": "아뇨 세일하는거요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:06"
        }
      ]
