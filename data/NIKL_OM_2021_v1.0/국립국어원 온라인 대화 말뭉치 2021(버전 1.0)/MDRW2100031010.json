      "utterance": [
        {
          "id": "MDRW2100031010.1.1",
          "form": "나 담주까지 재택근무한다.",
          "original_form": "나 담주까지 재택근무한다.",
          "speaker_id": "1",
          "time": "20210904 15:29"
        },
        {
          "id": "MDRW2100031010.1.2",
          "form": "이틀정도",
          "original_form": "이틀정도",
          "speaker_id": "1",
          "time": "20210904 15:29"
        },
        {
          "id": "MDRW2100031010.1.3",
          "form": "겨우 이틀? 넘하네",
          "original_form": "겨우 이틀? 넘하네",
          "speaker_id": "2",
          "time": "20210904 15:29"
        },
        {
          "id": "MDRW2100031010.1.4",
          "form": "근데 솔직히 개발자는 집에서도 일할 수 있잖아",
          "original_form": "근데 솔직히 개발자는 집에서도 일할 수 있잖아",
          "speaker_id": "2",
          "time": "20210904 15:29"
        },
        {
          "id": "MDRW2100031010.1.5",
          "form": "너네 회사는 왜 뭣하러 자꾸 출근하라고 그러는거야?",
          "original_form": "너네 회사는 왜 뭣하러 자꾸 출근하라고 그러는거야?",
          "speaker_id": "2",
          "time": "20210904 15:29"
        },
        {
          "id": "MDRW2100031010.1.6",
          "form": "회사에 굳이 가야 일이 더 잘돼?",
          "original_form": "회사에 굳이 가야 일이 더 잘돼?",
          "speaker_id": "2",
          "time": "20210904 15:30"
        },
        {
          "id": "MDRW2100031010.1.7",
          "form": "어...",
          "original_form": "어...",
          "speaker_id": "1",
          "time": "20210904 15:30"
        },
        {
          "id": "MDRW2100031010.1.8",
          "form": "프로젝트가 시작단계라서 회의가 많은데",
          "original_form": "프로젝트가 시작단계라서 회의가 많은데",
          "speaker_id": "1",
          "time": "20210904 15:30"
        },
        {
          "id": "MDRW2100031010.1.9",
          "form": "회의실 들어가면 줌도 잘 끊기고 그래서",
          "original_form": "회의실 들어가면 줌도 잘 끊기고 그래서",
          "speaker_id": "1",
          "time": "20210904 15:30"
        },
        {
          "id": "MDRW2100031010.1.10",
          "form": "요즘은 줌? 같은 걸로 화상회의하던데",
          "original_form": "요즘은 줌? 같은 걸로 화상회의하던데",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100031010.1.11",
          "form": "아 회의실 들어가면 끊어진다고ㅡㅡ",
          "original_form": "아 회의실 들어가면 끊어진다고ㅡㅡ",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100031010.1.12",
          "form": "그리고 관리자입장에선 일하는거 안보이니까 그렇겠지",
          "original_form": "그리고 관리자입장에선 일하는거 안보이니까 그렇겠지",
          "speaker_id": "1",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100031010.1.13",
          "form": "응 ㅋㅋㅋ 아 근데 어제 줌하니까 심하게 끊기더라",
          "original_form": "응 ㅋㅋㅋ 아 근데 어제 줌하니까 심하게 끊기더라",
          "speaker_id": "1",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100031010.1.14",
          "form": "이런... 근데 개발자들 회의는 대체로 뭘 함?",
          "original_form": "이런... 근데 개발자들 회의는 대체로 뭘 함?",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100031010.1.15",
          "form": "걍 일정같은거 얘기하나?",
          "original_form": "걍 일정같은거 얘기하나?",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100031010.1.16",
          "form": "원래 줌으로 하긴하는데...우리 회사 기본적으로 디스코드로 이냐기하거든",
          "original_form": "원래 줌으로 하긴하는데...우리 회사 기본적으로 디스코드로 이냐기하거든",
          "speaker_id": "1",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100031010.1.17",
          "form": "응 일정회의",
          "original_form": "응 일정회의",
          "speaker_id": "1",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100031010.1.18",
          "form": "기술적 문제",
          "original_form": "기술적 문제",
          "speaker_id": "1",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100031010.1.19",
          "form": "문제를 말하면 결론은 해결해라지만",
          "original_form": "문제를 말하면 결론은 해결해라지만",
          "speaker_id": "1",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100031010.1.20",
          "form": "문제에 대한 해답제시 노... 그냥 해결하자",
          "original_form": "문제에 대한 해답제시 노... 그냥 해결하자",
          "speaker_id": "1",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100031010.1.21",
          "form": "개발자는 집에서 쉽게 일할 수 있을 줄 알았는데 너 보면 그게 아니넹",
          "original_form": "개발자는 집에서 쉽게 일할 수 있을 줄 알았는데 너 보면 그게 아니넹",
          "speaker_id": "2",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100031010.1.22",
          "form": "원래 할 수 있는데 내가 너무 초보라서 그런가봐",
          "original_form": "원래 할 수 있는데 내가 너무 초보라서 그런가봐",
          "speaker_id": "1",
          "time": "20210904 15:33"
        },
        {
          "id": "MDRW2100031010.1.23",
          "form": "너 이제 경력이 6개월차 개발자던가?",
          "original_form": "너 이제 경력이 6개월차 개발자던가?",
          "speaker_id": "2",
          "time": "20210904 15:33"
        },
        {
          "id": "MDRW2100031010.1.24",
          "form": "응... 너무 슬프다...",
          "original_form": "응... 너무 슬프다...",
          "speaker_id": "1",
          "time": "20210904 15:34"
        },
        {
          "id": "MDRW2100031010.1.25",
          "form": "고생이 많구만",
          "original_form": "고생이 많구만",
          "speaker_id": "2",
          "time": "20210904 15:34"
        },
        {
          "id": "MDRW2100031010.1.26",
          "form": "전에 경리했던 것하고 비교했을때 일의 난이도 차이는 어때",
          "original_form": "전에 경리했던 것하고 비교했을때 일의 난이도 차이는 어때",
          "speaker_id": "2",
          "time": "20210904 15:34"
        },
        {
          "id": "MDRW2100031010.1.27",
          "form": "장단점이 있는거 같애",
          "original_form": "장단점이 있는거 같애",
          "speaker_id": "1",
          "time": "20210904 15:34"
        }
      ]
