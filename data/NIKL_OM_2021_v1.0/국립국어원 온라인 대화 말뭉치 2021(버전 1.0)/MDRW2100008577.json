      "utterance": [
        {
          "id": "MDRW2100008577.1.1",
          "form": "저녁까지?",
          "original_form": "저녁까지?",
          "speaker_id": "2",
          "time": "20210822 16:54"
        },
        {
          "id": "MDRW2100008577.1.2",
          "form": "집에 가는길인데ㅋㅋ",
          "original_form": "집에 가는길인데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:09"
        },
        {
          "id": "MDRW2100008577.1.3",
          "form": "아ㅋㅋ 급 종료",
          "original_form": "아ㅋㅋ 급 종료",
          "speaker_id": "2",
          "time": "20210822 17:09"
        },
        {
          "id": "MDRW2100008577.1.4",
          "form": "첫째는 회먹고싶다하고 신랑은 고기먹고싶다하고 ㅋㅋ",
          "original_form": "첫째는 회먹고싶다하고 신랑은 고기먹고싶다하고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:09"
        },
        {
          "id": "MDRW2100008577.1.5",
          "form": "응 급하게 종료ㅋㅋㅋㅋ",
          "original_form": "응 급하게 종료ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:09"
        },
        {
          "id": "MDRW2100008577.1.6",
          "form": "ㅋㅋㅋ 회사랑",
          "original_form": "ㅋㅋㅋ 회사랑",
          "speaker_id": "2",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008577.1.7",
          "form": "길게는 못있는다",
          "original_form": "길게는 못있는다",
          "speaker_id": "1",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008577.1.8",
          "form": "전부다 비싼거",
          "original_form": "전부다 비싼거",
          "speaker_id": "2",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008577.1.9",
          "form": "먹고싶다카네",
          "original_form": "먹고싶다카네",
          "speaker_id": "2",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008577.1.10",
          "form": "그러니깐ㅋㅋㅋ아휴 ㄱㅋㅋㅋ",
          "original_form": "그러니깐ㅋㅋㅋ아휴 ㄱㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008577.1.11",
          "form": "입이 고오급지다~",
          "original_form": "입이 고오급지다~",
          "speaker_id": "2",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008577.1.12",
          "form": "요새 삼겹살이 땡기는데 너무비싸서 안사고 개기고 있음",
          "original_form": "요새 삼겹살이 땡기는데 너무비싸서 안사고 개기고 있음",
          "speaker_id": "2",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008577.1.13",
          "form": "웃긴다ㅋㅋ왜ㅋㅋㅋㅋㅋ고기먹어라ㅋㅋㅋ",
          "original_form": "웃긴다ㅋㅋ왜ㅋㅋㅋㅋㅋ고기먹어라ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008577.1.14",
          "form": "삼겹살ㅋㅋ",
          "original_form": "삼겹살ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008577.1.15",
          "form": "우리는 주물럭사서 시댁가서 먹고오게ㅋㅋ",
          "original_form": "우리는 주물럭사서 시댁가서 먹고오게ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008577.1.16",
          "form": "지나가는 길이라서ㅋㅋ",
          "original_form": "지나가는 길이라서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008577.1.17",
          "form": "저녁식사 해결하려고ㅋㅋ",
          "original_form": "저녁식사 해결하려고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008577.1.18",
          "form": "좋다앙~~~ 주물럭 담주에 먹어야쥥ㅋ",
          "original_form": "좋다앙~~~ 주물럭 담주에 먹어야쥥ㅋ",
          "speaker_id": "2",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100008577.1.19",
          "form": "회는 물건너갔나",
          "original_form": "회는 물건너갔나",
          "speaker_id": "2",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100008577.1.20",
          "form": "응ㅋㅋ마트왔어. 저녁으로 주물럭 먹지뭐ㅋㅋ",
          "original_form": "응ㅋㅋ마트왔어. 저녁으로 주물럭 먹지뭐ㅋㅋ",
          "speaker_id": "1",
