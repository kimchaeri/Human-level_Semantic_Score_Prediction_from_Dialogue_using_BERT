      "utterance": [
        {
          "id": "MDRW2100047524.1.1",
          "form": "여름에 더워서 마니팔아줬군",
          "original_form": "여름에 더워서 마니팔아줬군",
          "speaker_id": "2",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100047524.1.2",
          "form": "웃긴건 그 커피가",
          "original_form": "웃긴건 그 커피가",
          "speaker_id": "1",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100047524.1.3",
          "form": "체인임",
          "original_form": "체인임",
          "speaker_id": "1",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100047524.1.4",
          "form": "프랜차이즈",
          "original_form": "프랜차이즈",
          "speaker_id": "1",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100047524.1.5",
          "form": "내가 동구사는데",
          "original_form": "내가 동구사는데",
          "speaker_id": "1",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100047524.1.6",
          "form": "중구에하나있고",
          "original_form": "중구에하나있고",
          "speaker_id": "1",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100047524.1.7",
          "form": "북구에 있음",
          "original_form": "북구에 있음",
          "speaker_id": "1",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100047524.1.8",
          "form": "먼데",
          "original_form": "먼데",
          "speaker_id": "2",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100047524.1.9",
          "form": "매머드?",
          "original_form": "매머드?",
          "speaker_id": "2",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100047524.1.10",
          "form": "내가 그집 커피 사먹겠다고",
          "original_form": "내가 그집 커피 사먹겠다고",
          "speaker_id": "1",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100047524.1.11",
          "form": "천씨씨커피",
          "original_form": "천씨씨커피",
          "speaker_id": "1",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100047524.1.12",
          "form": "상호명",
          "original_form": "상호명",
          "speaker_id": "1",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100047524.1.13",
          "form": "동부도서관이 아니라 중부도서관 다닐정도였음",
          "original_form": "동부도서관이 아니라 중부도서관 다닐정도였음",
          "speaker_id": "1",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100047524.1.14",
          "form": "나는 첨듣는디",
          "original_form": "나는 첨듣는디",
          "speaker_id": "2",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100047524.1.15",
          "form": "울산과 진주에만 좀 체인있음",
          "original_form": "울산과 진주에만 좀 체인있음",
          "speaker_id": "1",
          "time": "20210826 19:20"
        },
        {
          "id": "MDRW2100047524.1.16",
          "form": "그 두 도시가 거의 유일하게 체인있음",
          "original_form": "그 두 도시가 거의 유일하게 체인있음",
          "speaker_id": "1",
          "time": "20210826 19:20"
        },
        {
          "id": "MDRW2100047524.1.17",
          "form": "대구인가 부산에 좀있고",
          "original_form": "대구인가 부산에 좀있고",
          "speaker_id": "1",
          "time": "20210826 19:20"
        },
        {
          "id": "MDRW2100047524.1.18",
          "form": "진주랑 울산이 본고장인듯",
          "original_form": "진주랑 울산이 본고장인듯",
          "speaker_id": "1",
          "time": "20210826 19:20"
        },
        {
          "id": "MDRW2100047524.1.19",
          "form": "담엔 크폰먼저",
          "original_form": "담엔 크폰먼저",
          "speaker_id": "2",
          "time": "20210826 19:21"
        },
        {
          "id": "MDRW2100047524.1.20",
          "form": "찾아야긋다",
          "original_form": "찾아야긋다",
          "speaker_id": "2",
          "time": "20210826 19:21"
        },
        {
          "id": "MDRW2100047524.1.21",
          "form": "진주랑 울산에 지점많고",
          "original_form": "진주랑 울산에 지점많고",
          "speaker_id": "1",
          "time": "20210826 19:21"
        },
        {
          "id": "MDRW2100047524.1.22",
          "form": "부산 대구에 한개씩 인가 있고",
          "original_form": "부산 대구에 한개씩 인가 있고",
          "speaker_id": "1",
          "time": "20210826 19:21"
        },
        {
          "id": "MDRW2100047524.1.23",
          "form": "경상도 중심",
          "original_form": "경상도 중심",
          "speaker_id": "1",
          "time": "20210826 19:21"
        },
        {
          "id": "MDRW2100047524.1.24",
          "form": "근데 더 소름돋는건 먼지 알아여?",
          "original_form": "근데 더 소름돋는건 먼지 알아여?",
          "speaker_id": "1",
          "time": "20210826 19:21"
        },
        {
          "id": "MDRW2100047524.1.25",
          "form": "난첨들음",
          "original_form": "난첨들음",
          "speaker_id": "2",
          "time": "20210826 19:21"
        },
        {
          "id": "MDRW2100047524.1.26",
          "form": "내가 북구점도 세번이나 갔다는거임",
          "original_form": "내가 북구점도 세번이나 갔다는거임",
          "speaker_id": "1",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100047524.1.27",
          "form": "근데 걔들도 단한번도 쿠폰안줌",
          "original_form": "근데 걔들도 단한번도 쿠폰안줌",
          "speaker_id": "1",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100047524.1.28",
          "form": "왜그럼",
          "original_form": "왜그럼",
          "speaker_id": "2",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100047524.1.29",
          "form": "안내해야하는데",
          "original_form": "안내해야하는데",
          "speaker_id": "2",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100047524.1.30",
          "form": "심지어 북구점은진짜 멀어서 갈때마다 음료 두개씩 주문했었음 7800씩",
          "original_form": "심지어 북구점은진짜 멀어서 갈때마다 음료 두개씩 주문했었음 7800씩",
          "speaker_id": "1",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100047524.1.31",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100047524.1.32",
          "form": "왜 그렇게카지",
          "original_form": "왜 그렇게카지",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100047524.1.33",
          "form": "나 진짜 정말 너무 분노해서",
          "original_form": "나 진짜 정말 너무 분노해서",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100047524.1.34",
          "form": "그날로 손절함",
          "original_form": "그날로 손절함",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100047524.1.35",
          "form": "진짜 북구점만 생각해도 쿠폰 6개고",
          "original_form": "진짜 북구점만 생각해도 쿠폰 6개고",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100047524.1.36",
          "form": "중구점도 20개는 됐을거임",
          "original_form": "중구점도 20개는 됐을거임",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100047524.1.37",
          "form": "그것도 첨들어봄",
          "original_form": "그것도 첨들어봄",
          "speaker_id": "2",
          "time": "20210827 14:06"
        }
      ]
