      "utterance": [
        {
          "id": "MDRW2100020667.1.1",
          "form": "진짜 너무 맛있어요",
          "original_form": "진짜 너무 맛있어요",
          "speaker_id": "2",
          "time": "20210828 10:57"
        },
        {
          "id": "MDRW2100020667.1.2",
          "form": "왜요 왜 안먹으려했어요",
          "original_form": "왜요 왜 안먹으려했어요",
          "speaker_id": "1",
          "time": "20210828 10:57"
        },
        {
          "id": "MDRW2100020667.1.3",
          "form": "포장이 너무 술안주처럼 생겨서..",
          "original_form": "포장이 너무 술안주처럼 생겨서..",
          "speaker_id": "2",
          "time": "20210828 10:57"
        },
        {
          "id": "MDRW2100020667.1.4",
          "form": "의심했는데",
          "original_form": "의심했는데",
          "speaker_id": "2",
          "time": "20210828 10:57"
        },
        {
          "id": "MDRW2100020667.1.5",
          "form": "그건그래요ㅋㅋㅋㅋ",
          "original_form": "그건그래요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:57"
        },
        {
          "id": "MDRW2100020667.1.6",
          "form": "애주가들의 픽인 이유가 있더라구요",
          "original_form": "애주가들의 픽인 이유가 있더라구요",
          "speaker_id": "2",
          "time": "20210828 10:57"
        },
        {
          "id": "MDRW2100020667.1.7",
          "form": "그리고 나쵸도 좋아해요",
          "original_form": "그리고 나쵸도 좋아해요",
          "speaker_id": "2",
          "time": "20210828 10:57"
        },
        {
          "id": "MDRW2100020667.1.8",
          "form": "나쵸 좋아하시나요",
          "original_form": "나쵸 좋아하시나요",
          "speaker_id": "2",
          "time": "20210828 10:58"
        },
        {
          "id": "MDRW2100020667.1.9",
          "form": "약간 오독오독한게 매력",
          "original_form": "약간 오독오독한게 매력",
          "speaker_id": "1",
          "time": "20210828 10:58"
        },
        {
          "id": "MDRW2100020667.1.10",
          "form": "나초 좋아해요",
          "original_form": "나초 좋아해요",
          "speaker_id": "1",
          "time": "20210828 10:58"
        },
        {
          "id": "MDRW2100020667.1.11",
          "form": "어떤 나초 좋아하세요",
          "original_form": "어떤 나초 좋아하세요",
          "speaker_id": "1",
          "time": "20210828 10:58"
        },
        {
          "id": "MDRW2100020667.1.12",
          "form": "외국꺼? 한국꺼?",
          "original_form": "외국꺼? 한국꺼?",
          "speaker_id": "1",
          "time": "20210828 10:58"
        },
        {
          "id": "MDRW2100020667.1.13",
          "form": "저 종류불문 다 좋아해요",
          "original_form": "저 종류불문 다 좋아해요",
          "speaker_id": "2",
          "time": "20210828 10:58"
        },
        {
          "id": "MDRW2100020667.1.14",
          "form": "외국꺼부터 시작해서",
          "original_form": "외국꺼부터 시작해서",
          "speaker_id": "2",
          "time": "20210828 10:58"
        },
        {
          "id": "MDRW2100020667.1.15",
          "form": "도도한 나쵸까지",
          "original_form": "도도한 나쵸까지",
          "speaker_id": "2",
          "time": "20210828 10:58"
        },
        {
          "id": "MDRW2100020667.1.16",
          "form": "그 전 외국꺼 치즈소스 찍어먹는거 너무 좋아요",
          "original_form": "그 전 외국꺼 치즈소스 찍어먹는거 너무 좋아요",
          "speaker_id": "1",
          "time": "20210828 10:58"
        },
        {
          "id": "MDRW2100020667.1.17",
          "form": "각자의 매력이 있어요",
          "original_form": "각자의 매력이 있어요",
          "speaker_id": "2",
          "time": "20210828 10:58"
        },
        {
          "id": "MDRW2100020667.1.18",
          "form": "소스아시나요",
          "original_form": "소스아시나요",
          "speaker_id": "1",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020667.1.19",
          "form": "맞아요 그거 사오면",
          "original_form": "맞아요 그거 사오면",
          "speaker_id": "2",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020667.1.20",
          "form": "그날은 살찌는 거에요",
          "original_form": "그날은 살찌는 거에요",
          "speaker_id": "2",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020667.1.21",
          "form": "알죠알죠",
          "original_form": "알죠알죠",
          "speaker_id": "2",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020667.1.22",
          "form": "진짜 너무맛있어",
          "original_form": "진짜 너무맛있어",
          "speaker_id": "1",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020667.1.23",
          "form": "그리고 도도한나쵸늣",
          "original_form": "그리고 도도한나쵸늣",
          "speaker_id": "1",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020667.1.24",
          "form": "조합이 짱이에요",
          "original_form": "조합이 짱이에요",
          "speaker_id": "2",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020667.1.25",
          "form": "검정색이 제일 자극젘",
          "original_form": "검정색이 제일 자극젘",
          "speaker_id": "1",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020667.1.26",
          "form": "누가 발견한건지",
          "original_form": "누가 발견한건지",
          "speaker_id": "2",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020667.1.27",
          "form": "헉글쿤요",
          "original_form": "헉글쿤요",
          "speaker_id": "2",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020667.1.28",
          "form": "도도한 나쵸먹으면 손 빨개지더라구요",
          "original_form": "도도한 나쵸먹으면 손 빨개지더라구요",
          "speaker_id": "2",
          "time": "20210828 11:00"
        }
      ]
    }
