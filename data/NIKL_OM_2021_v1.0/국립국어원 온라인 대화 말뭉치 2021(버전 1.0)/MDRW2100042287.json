        },
        {
          "id": "MDRW2100042287.1.2",
          "form": "거리유지없이 사람들 너무많아서",
          "original_form": "거리유지없이 사람들 너무많아서",
          "speaker_id": "1",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100042287.1.3",
          "form": "그러면 다른곳 가지 뭐",
          "original_form": "그러면 다른곳 가지 뭐",
          "speaker_id": "2",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100042287.1.4",
          "form": "중간까지도 못보고 걍 나왔다는데",
          "original_form": "중간까지도 못보고 걍 나왔다는데",
          "speaker_id": "1",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100042287.1.5",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100042287.1.6",
          "form": "그정도로 많다고???",
          "original_form": "그정도로 많다고???",
          "speaker_id": "2",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100042287.1.7",
          "form": "딱봐도 사람들 많을거같긴하다",
          "original_form": "딱봐도 사람들 많을거같긴하다",
          "speaker_id": "1",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100042287.1.8",
          "form": "근데 나 이번에 갔을때도",
          "original_form": "근데 나 이번에 갔을때도",
          "speaker_id": "2",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100042287.1.9",
          "form": "황리단길에 사람 많드라",
          "original_form": "황리단길에 사람 많드라",
          "speaker_id": "2",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100042287.1.10",
          "form": "이런...",
          "original_form": "이런...",
          "speaker_id": "1",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100042287.1.11",
          "form": "그래도",
          "original_form": "그래도",
          "speaker_id": "2",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100042287.1.12",
          "form": "맛있는거 많이 보이던데",
          "original_form": "맛있는거 많이 보이던데",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100042287.1.13",
          "form": "가서 다 먹자구",
          "original_form": "가서 다 먹자구",
