      "utterance": [
        {
          "id": "MDRW2100034763.1.1",
          "form": "음 서울쪽 알려드릴까요?!",
          "original_form": "음 서울쪽 알려드릴까요?!",
          "speaker_id": "1",
          "time": "20210828 19:03"
        },
        {
          "id": "MDRW2100034763.1.2",
          "form": "저 서울 자취 1년차인데",
          "original_form": "저 서울 자취 1년차인데",
          "speaker_id": "2",
          "time": "20210828 19:04"
        },
        {
          "id": "MDRW2100034763.1.3",
          "form": "코로나라 가본곳이 없어요",
          "original_form": "코로나라 가본곳이 없어요",
          "speaker_id": "2",
          "time": "20210828 19:04"
        },
        {
          "id": "MDRW2100034763.1.4",
          "form": "서울쪽 다 좋아요...",
          "original_form": "서울쪽 다 좋아요...",
          "speaker_id": "2",
          "time": "20210828 19:04"
        },
        {
          "id": "MDRW2100034763.1.5",
          "form": "헉 ㅠㅠ 속상",
          "original_form": "헉 ㅠㅠ 속상",
          "speaker_id": "1",
          "time": "20210828 19:04"
        },
        {
          "id": "MDRW2100034763.1.6",
          "form": "일단 여의도!",
          "original_form": "일단 여의도!",
          "speaker_id": "1",
          "time": "20210828 19:04"
        },
        {
          "id": "MDRW2100034763.1.7",
          "form": "하얀 과자 점 이라고 디저트 카페가 있는데",
          "original_form": "하얀 과자 점 이라고 디저트 카페가 있는데",
          "speaker_id": "1",
          "time": "20210828 19:05"
        },
        {
          "id": "MDRW2100034763.1.8",
          "form": "까눌레 정말 맛있구여..",
          "original_form": "까눌레 정말 맛있구여..",
          "speaker_id": "1",
          "time": "20210828 19:05"
        },
        {
          "id": "MDRW2100034763.1.9",
          "form": "여의숲이라고 에그타르트 맛집 있어요",
          "original_form": "여의숲이라고 에그타르트 맛집 있어요",
          "speaker_id": "1",
          "time": "20210828 19:06"
        },
        {
          "id": "MDRW2100034763.1.10",
          "form": "캡쳐중입니다",
          "original_form": "캡쳐중입니다",
          "speaker_id": "2",
          "time": "20210828 19:07"
        },
        {
          "id": "MDRW2100034763.1.11",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:07"
        },
        {
          "id": "MDRW2100034763.1.12",
          "form": "디저트맛집들이네요 너무좋아요",
          "original_form": "디저트맛집들이네요 너무좋아요",
          "speaker_id": "2",
          "time": "20210828 19:07"
        },
        {
          "id": "MDRW2100034763.1.13",
          "form": "그리고 베이글 좋아하시면",
          "original_form": "그리고 베이글 좋아하시면",
          "speaker_id": "1",
          "time": "20210828 19:09"
        },
        {
          "id": "MDRW2100034763.1.14",
          "form": "여의도랑 공덕에 아이엠베이글이라고 있는데 샐러드시키면 베이글 반개가 같이나와요 크림치즈 추가해서 같이 드시면 든든하고 너무 맛있어요",
          "original_form": "여의도랑 공덕에 아이엠베이글이라고 있는데 샐러드시키면 베이글 반개가 같이나와요 크림치즈 추가해서 같이 드시면 든든하고 너무 맛있어요",
          "speaker_id": "1",
          "time": "20210828 19:09"
        },
        {
          "id": "MDRW2100034763.1.15",
          "form": "대박 빵좋아하는 친구데려가야겠어요",
          "original_form": "대박 빵좋아하는 친구데려가야겠어요",
          "speaker_id": "2",
          "time": "20210828 19:11"
        },
        {
          "id": "MDRW2100034763.1.16",
          "form": "밥은 혹시 제가 미쁘동 얘기했었나요?",
          "original_form": "밥은 혹시 제가 미쁘동 얘기했었나요?",
          "speaker_id": "1",
          "time": "20210828 19:11"
        },
        {
          "id": "MDRW2100034763.1.17",
          "form": "네! 메모해뒀어요 덮밥집이요!",
          "original_form": "네! 메모해뒀어요 덮밥집이요!",
          "speaker_id": "2",
          "time": "20210828 19:12"
        },
        {
          "id": "MDRW2100034763.1.18",
          "form": "떡볶이 좋아하세요?",
          "original_form": "떡볶이 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 19:13"
        },
        {
          "id": "MDRW2100034763.1.19",
          "form": "제친구들이 좋아해요!",
          "original_form": "제친구들이 좋아해요!",
          "speaker_id": "2",
          "time": "20210828 19:13"
        },
        {
          "id": "MDRW2100034763.1.20",
          "form": "점수따게 알려주세요",
          "original_form": "점수따게 알려주세요",
          "speaker_id": "2",
          "time": "20210828 19:13"
        },
        {
          "id": "MDRW2100034763.1.21",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:13"
        },
        {
          "id": "MDRW2100034763.1.22",
          "form": "홍대에 또보겠지떡볶이도 맛있다고 들었어요",
          "original_form": "홍대에 또보겠지떡볶이도 맛있다고 들었어요",
          "speaker_id": "1",
          "time": "20210828 19:13"
        },
        {
          "id": "MDRW2100034763.1.23",
          "form": "연어초밥은 홍대 은행골!",
          "original_form": "연어초밥은 홍대 은행골!",
          "speaker_id": "1",
          "time": "20210828 19:14"
        },
        {
          "id": "MDRW2100034763.1.24",
          "form": "홍대쪽 가까워서 자주가게될거 같아요 감사해요!",
          "original_form": "홍대쪽 가까워서 자주가게될거 같아요 감사해요!",
          "speaker_id": "2",
          "time": "20210828 19:14"
        },
        {
          "id": "MDRW2100034763.1.25",
          "form": "헉 연어",
          "original_form": "헉 연어",
          "speaker_id": "2",
          "time": "20210828 19:14"
        },
        {
          "id": "MDRW2100034763.1.26",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210828 19:14"
        },
        {
          "id": "MDRW2100034763.1.27",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210828 19:15"
        },
        {
          "id": "MDRW2100034763.1.28",
          "form": "잠시만요 홍대쪽 머리 더 굴려볼게요",
          "original_form": "잠시만요 홍대쪽 머리 더 굴려볼게요",
          "speaker_id": "1",
          "time": "20210828 19:15"
        },
        {
          "id": "MDRW2100034763.1.29",
          "form": "화이팅...!",
          "original_form": "화이팅...!",
          "speaker_id": "2",
          "time": "20210828 19:16"
        },
        {
          "id": "MDRW2100034763.1.30",
          "form": "염치없지만 굴려주셨으면 하는 맴...",
          "original_form": "염치없지만 굴려주셨으면 하는 맴...",
          "speaker_id": "2",
          "time": "20210828 19:16"
        },
        {
          "id": "MDRW2100034763.1.31",
          "form": "근데 연남동쪽은 저녁에 돌아다니면 너무 좋아요",
          "original_form": "근데 연남동쪽은 저녁에 돌아다니면 너무 좋아요",
          "speaker_id": "1",
          "time": "20210828 19:16"
        },
        {
          "id": "MDRW2100034763.1.32",
          "form": "전 진짜ㅠㅠ 저녁의 연남동 분위기가 정말.. 좋거든요",
          "original_form": "전 진짜ㅠㅠ 저녁의 연남동 분위기가 정말.. 좋거든요",
          "speaker_id": "1",
          "time": "20210828 19:16"
        },
        {
          "id": "MDRW2100034763.1.33",
          "form": "산책하기좋은곳도 너무 최고죠",
          "original_form": "산책하기좋은곳도 너무 최고죠",
          "speaker_id": "2",
          "time": "20210828 19:18"
        },
        {
          "id": "MDRW2100034763.1.34",
          "form": "꼭 가볼께요",
          "original_form": "꼭 가볼께요",
          "speaker_id": "2",
          "time": "20210828 19:18"
        },
        {
          "id": "MDRW2100034763.1.35",
          "form": "연남동 오늘그대와라는 파스타집도 너무 맛있었어요",
          "original_form": "연남동 오늘그대와라는 파스타집도 너무 맛있었어요",
          "speaker_id": "1",
          "time": "20210828 19:18"
        },
        {
          "id": "MDRW2100034763.1.36",
          "form": "파스타먹고 산책나가면 딱이겠네요",
          "original_form": "파스타먹고 산책나가면 딱이겠네요",
          "speaker_id": "2",
          "time": "20210828 19:18"
        },
        {
          "id": "MDRW2100034763.1.37",
          "form": "그쳐 ㅋㅋ 카페도 많아서 구경하기도 좋고",
          "original_form": "그쳐 ㅋㅋ 카페도 많아서 구경하기도 좋고",
          "speaker_id": "1",
          "time": "20210828 19:19"
        }
      ]
    }
