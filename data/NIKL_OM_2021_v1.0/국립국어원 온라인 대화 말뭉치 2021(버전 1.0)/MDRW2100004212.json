        {
          "id": "MDRW2100004212.1.1",
          "form": "해산물 좋아해?",
          "original_form": "해산물 좋아해?",
          "speaker_id": "2",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004212.1.2",
          "form": "응 해산물 좋아",
          "original_form": "응 해산물 좋아",
          "speaker_id": "1",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004212.1.3",
          "form": "맛있어",
          "original_form": "맛있어",
          "speaker_id": "1",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004212.1.4",
          "form": "나는 해산물 보다는 육류...",
          "original_form": "나는 해산물 보다는 육류...",
          "speaker_id": "2",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004212.1.5",
          "form": "고기도 맛있지",
          "original_form": "고기도 맛있지",
          "speaker_id": "1",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004212.1.6",
          "form": "그래도 갑각류는 좋아하는 듯",
          "original_form": "그래도 갑각류는 좋아하는 듯",
          "speaker_id": "2",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004212.1.7",
          "form": "갑각류? 조개 그런?",
          "original_form": "갑각류? 조개 그런?",
          "speaker_id": "1",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004212.1.8",
          "form": "조개 새우 꽃게 오징어",
          "original_form": "조개 새우 꽃게 오징어",
          "speaker_id": "2",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004212.1.9",
          "form": "회는?",
          "original_form": "회는?",
          "speaker_id": "1",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004212.1.10",
          "form": "회도 좋지 맛있어",
          "original_form": "회도 좋지 맛있어",
          "speaker_id": "2",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004212.1.11",
          "form": "회 얘기하니까 물회 먹고 싶다",
          "original_form": "회 얘기하니까 물회 먹고 싶다",
          "speaker_id": "1",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004212.1.12",
          "form": "ㅋㅋㅋ 물회 ㅋㅋ 나는 가시 있는 건 먹기 싫어",
          "original_form": "ㅋㅋㅋ 물회 ㅋㅋ 나는 가시 있는 건 먹기 싫어",
          "speaker_id": "2",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004212.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ 그렇구나",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그렇구나",
          "speaker_id": "1",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004212.1.14",
          "form": "물회는 가시 없지",
          "original_form": "물회는 가시 없지",
          "speaker_id": "1",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004212.1.15",
          "form": "응 물회도 맛있지",
          "original_form": "응 물회도 맛있지",
          "speaker_id": "2",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004212.1.16",
          "form": "햄버거 중에서 어떤거 좋아해?",
          "original_form": "햄버거 중에서 어떤거 좋아해?",
