      "utterance": [
        {
          "id": "MDRW2100020558.1.1",
          "form": "어떤 빵인가왜",
          "original_form": "어떤 빵인가왜",
          "speaker_id": "2",
          "time": "20210828 09:44"
        },
        {
          "id": "MDRW2100020558.1.2",
          "form": "어떤빵을 좋아하시나요",
          "original_form": "어떤빵을 좋아하시나요",
          "speaker_id": "1",
          "time": "20210828 09:44"
        },
        {
          "id": "MDRW2100020558.1.3",
          "form": "아 저 가리지않는데",
          "original_form": "아 저 가리지않는데",
          "speaker_id": "1",
          "time": "20210828 09:44"
        },
        {
          "id": "MDRW2100020558.1.4",
          "form": "소시지든거 좋아합니다",
          "original_form": "소시지든거 좋아합니다",
          "speaker_id": "1",
          "time": "20210828 09:44"
        },
        {
          "id": "MDRW2100020558.1.5",
          "form": "초딩입맛이라ㅎ",
          "original_form": "초딩입맛이라ㅎ",
          "speaker_id": "1",
          "time": "20210828 09:44"
        },
        {
          "id": "MDRW2100020558.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ저는 어릴 때 피자빵 완전 좋아했는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저는 어릴 때 피자빵 완전 좋아했는데",
          "speaker_id": "2",
          "time": "20210828 09:45"
        },
        {
          "id": "MDRW2100020558.1.7",
          "form": "피자빵 그거알죠",
          "original_form": "피자빵 그거알죠",
          "speaker_id": "1",
          "time": "20210828 09:45"
        },
        {
          "id": "MDRW2100020558.1.8",
          "form": "친척분들중에 빵회사 다니시는 분이 계셨거든요",
          "original_form": "친척분들중에 빵회사 다니시는 분이 계셨거든요",
          "speaker_id": "2",
          "time": "20210828 09:45"
        },
        {
          "id": "MDRW2100020558.1.9",
          "form": "소시지들어가는 양은 같은데 햄 더 먆은거 고르려고",
          "original_form": "소시지들어가는 양은 같은데 햄 더 먆은거 고르려고",
          "speaker_id": "1",
          "time": "20210828 09:45"
        },
        {
          "id": "MDRW2100020558.1.10",
          "form": "고민하던거",
          "original_form": "고민하던거",
          "speaker_id": "1",
          "time": "20210828 09:45"
        },
        {
          "id": "MDRW2100020558.1.11",
          "form": "피자빵이나 케익을 엄청 많이 가져다 주셔서",
          "original_form": "피자빵이나 케익을 엄청 많이 가져다 주셔서",
          "speaker_id": "2",
          "time": "20210828 09:45"
        },
        {
          "id": "MDRW2100020558.1.12",
          "form": "빵 회사 다니시는분이계시다니",
          "original_form": "빵 회사 다니시는분이계시다니",
          "speaker_id": "1",
          "time": "20210828 09:45"
        },
        {
          "id": "MDRW2100020558.1.13",
          "form": "그때 원없이 먹었어요",
          "original_form": "그때 원없이 먹었어요",
          "speaker_id": "2",
          "time": "20210828 09:45"
        },
        {
          "id": "MDRW2100020558.1.14",
          "form": "성공하셨네요",
          "original_form": "성공하셨네요",
          "speaker_id": "1",
          "time": "20210828 09:46"
        },
        {
          "id": "MDRW2100020558.1.15",
          "form": "그쵸 동생보다 소시지 많이 든거 먹으려고",
          "original_form": "그쵸 동생보다 소시지 많이 든거 먹으려고",
          "speaker_id": "2",
          "time": "20210828 09:46"
        },
        {
          "id": "MDRW2100020558.1.16",
          "form": "매의 눈으로 살폈죠",
          "original_form": "매의 눈으로 살폈죠",
          "speaker_id": "2",
          "time": "20210828 09:46"
        },
        {
          "id": "MDRW2100020558.1.17",
          "form": "어짜피ㅋㅋ 소시지는 하나들어가는데",
          "original_form": "어짜피ㅋㅋ 소시지는 하나들어가는데",
          "speaker_id": "1",
          "time": "20210828 09:46"
        },
        {
          "id": "MDRW2100020558.1.18",
          "form": "그래서 평생 먹을 피자빵을 그때 다 먹었어요",
          "original_form": "그래서 평생 먹을 피자빵을 그때 다 먹었어요",
          "speaker_id": "2",
          "time": "20210828 09:46"
        },
        {
          "id": "MDRW2100020558.1.19",
          "form": "어릴 땐 그걸 몰라서",
          "original_form": "어릴 땐 그걸 몰라서",
          "speaker_id": "1",
          "time": "20210828 09:46"
        },
        {
          "id": "MDRW2100020558.1.20",
