      "utterance": [
        {
          "id": "MDRW2100025532.1.1",
          "form": "오 안녕안녕",
          "original_form": "오 안녕안녕",
          "speaker_id": "2",
          "time": "20210901 12:53"
        },
        {
          "id": "MDRW2100025532.1.2",
          "form": "저요새 회전문도는데",
          "original_form": "저요새 회전문도는데",
          "speaker_id": "2",
          "time": "20210901 12:54"
        },
        {
          "id": "MDRW2100025532.1.3",
          "form": "아주잘오셨습니다",
          "original_form": "아주잘오셨습니다",
          "speaker_id": "2",
          "time": "20210901 12:54"
        },
        {
          "id": "MDRW2100025532.1.4",
          "form": "요새 뮤지컬봐서 통장이 텅장됐어여",
          "original_form": "요새 뮤지컬봐서 통장이 텅장됐어여",
          "speaker_id": "2",
          "time": "20210901 12:54"
        },
        {
          "id": "MDRW2100025532.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:54"
        },
        {
          "id": "MDRW2100025532.1.6",
          "form": "요즘 회전문 도시는데 또 다른 최애극이 등장하셨나여",
          "original_form": "요즘 회전문 도시는데 또 다른 최애극이 등장하셨나여",
          "speaker_id": "1",
          "time": "20210901 12:54"
        },
        {
          "id": "MDRW2100025532.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210901 12:55"
        },
        {
          "id": "MDRW2100025532.1.8",
          "form": "류지킬은 마음의고향",
          "original_form": "류지킬은 마음의고향",
          "speaker_id": "2",
          "time": "20210901 12:55"
        },
        {
          "id": "MDRW2100025532.1.9",
          "form": "제덕질의시작과끝",
          "original_form": "제덕질의시작과끝",
          "speaker_id": "2",
          "time": "20210901 12:55"
        },
        {
          "id": "MDRW2100025532.1.10",
          "form": "류지킬 은퇴하고 제 뮤덕질도 끝났다고생각했는데",
          "original_form": "류지킬 은퇴하고 제 뮤덕질도 끝났다고생각했는데",
          "speaker_id": "2",
          "time": "20210901 12:55"
        },
        {
          "id": "MDRW2100025532.1.11",
          "form": "다시돌아왔다",
          "original_form": "다시돌아왔다",
          "speaker_id": "2",
          "time": "20210901 12:55"
        },
        {
          "id": "MDRW2100025532.1.12",
          "form": "부-활",
          "original_form": "부-활",
          "speaker_id": "1",
          "time": "20210901 12:55"
        },
        {
          "id": "MDRW2100025532.1.13",
          "form": "행복한 텅장러네요",
          "original_form": "행복한 텅장러네요",
          "speaker_id": "1",
          "time": "20210901 12:56"
        },
        {
          "id": "MDRW2100025532.1.14",
          "form": "약간...",
          "original_form": "약간...",
          "speaker_id": "2",
          "time": "20210901 12:56"
        },
        {
          "id": "MDRW2100025532.1.15",
          "form": "휴덕기였나봐",
          "original_form": "휴덕기였나봐",
          "speaker_id": "2",
          "time": "20210901 12:56"
        },
        {
          "id": "MDRW2100025532.1.16",
          "form": "지킬도 페어는 다 찍으실 기세네요",
          "original_form": "지킬도 페어는 다 찍으실 기세네요",
          "speaker_id": "1",
          "time": "20210901 12:56"
        },
        {
          "id": "MDRW2100025532.1.17",
          "form": "ㅋㄱㄱㄱㅋ아 저는",
          "original_form": "ㅋㄱㄱㄱㅋ아 저는",
          "speaker_id": "2",
          "time": "20210901 12:56"
        },
        {
          "id": "MDRW2100025532.1.18",
          "form": "이번엔 페어따위 신경쓰지않습니다",
          "original_form": "이번엔 페어따위 신경쓰지않습니다",
          "speaker_id": "2",
          "time": "20210901 12:56"
        },
        {
          "id": "MDRW2100025532.1.19",
          "form": "지킬만 류면 됨",
          "original_form": "지킬만 류면 됨",
          "speaker_id": "2",
          "time": "20210901 12:56"
        },
        {
          "id": "MDRW2100025532.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ최애 지킴이",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ최애 지킴이",
          "speaker_id": "1",
          "time": "20210901 12:57"
        },
        {
          "id": "MDRW2100025532.1.21",
          "form": "그게 맞지",
          "original_form": "그게 맞지",
          "speaker_id": "1",
          "time": "20210901 12:57"
        },
        {
          "id": "MDRW2100025532.1.22",
          "form": "그치다른페어는신경안씀",
          "original_form": "그치다른페어는신경안씀",
          "speaker_id": "2",
          "time": "20210901 12:57"
        },
        {
          "id": "MDRW2100025532.1.23",
          "form": "제최애페어는 이미 다 엠마루시 은퇴해써",
          "original_form": "제최애페어는 이미 다 엠마루시 은퇴해써",
          "speaker_id": "2",
          "time": "20210901 12:57"
        },
        {
          "id": "MDRW2100025532.1.24",
          "form": "아직 렌드윅은 안보거지???",
          "original_form": "아직 렌드윅은 안보거지???",
          "speaker_id": "1",
          "time": "20210901 12:58"
        },
        {
          "id": "MDRW2100025532.1.25",
          "form": "언제볼려구",
          "original_form": "언제볼려구",
          "speaker_id": "1",
          "time": "20210901 12:58"
        },
        {
          "id": "MDRW2100025532.1.26",
          "form": "아 5일",
          "original_form": "아 5일",
          "speaker_id": "2",
          "time": "20210901 12:58"
        },
        {
          "id": "MDRW2100025532.1.27",
          "form": "이미 예약해놨죠",
          "original_form": "이미 예약해놨죠",
          "speaker_id": "2",
          "time": "20210901 12:58"
        },
        {
          "id": "MDRW2100025532.1.28",
          "form": "오 드디어",
          "original_form": "오 드디어",
          "speaker_id": "1",
          "time": "20210901 12:58"
        },
        {
          "id": "MDRW2100025532.1.29",
          "form": "잘한다며",
          "original_form": "잘한다며",
          "speaker_id": "1",
          "time": "20210901 12:58"
        },
        {
          "id": "MDRW2100025532.1.30",
          "form": "잘한대",
          "original_form": "잘한대",
          "speaker_id": "2",
          "time": "20210901 12:58"
        },
