          "speaker_id": "1",
          "time": "20210825 07:25"
        },
        {
          "id": "MDRW2100012894.1.4",
          "form": "와요?",
          "original_form": "와요?",
          "speaker_id": "1",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100012894.1.5",
          "form": "아뇨 이제 그쳤네요 ㅎㅎ",
          "original_form": "아뇨 이제 그쳤네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 16:00"
        },
        {
          "id": "MDRW2100012894.1.6",
          "form": "맑아졌어요",
          "original_form": "맑아졌어요",
          "speaker_id": "2",
          "time": "20210825 16:00"
        },
        {
          "id": "MDRW2100012894.1.7",
          "form": "거기는요?",
          "original_form": "거기는요?",
          "speaker_id": "2",
          "time": "20210825 16:00"
        },
        {
          "id": "MDRW2100012894.1.8",
          "form": "흐려요",
          "original_form": "흐려요",
          "speaker_id": "1",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100012894.1.9",
          "form": "아직",
          "original_form": "아직",
          "speaker_id": "1",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100012894.1.10",
          "form": "오그래도 그쳤네요",
          "original_form": "오그래도 그쳤네요",
          "speaker_id": "2",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100012894.1.11",
          "form": "장마가 이대로 가려나 몰라요",
          "original_form": "장마가 이대로 가려나 몰라요",
          "speaker_id": "2",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100012894.1.12",
          "form": "빨래가 안말라요",
          "original_form": "빨래가 안말라요",
          "speaker_id": "2",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100012894.1.13",
          "form": "건조기 돌렸네요",
          "original_form": "건조기 돌렸네요",
          "speaker_id": "2",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100012894.1.14",
          "form": "빨래 어찌 말리셨어요?",
          "original_form": "빨래 어찌 말리셨어요?",
          "speaker_id": "2",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100012894.1.15",
          "form": "제습기돌려요",
          "original_form": "제습기돌려요",
          "speaker_id": "1",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100012894.1.16",
          "form": "아 그럼 빨리 마르죠",
          "original_form": "아 그럼 빨리 마르죠",
          "speaker_id": "2",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100012894.1.17",
          "form": "전 빗물 에 신발 말리기가 넘힘들러요",
          "original_form": "전 빗물 에 신발 말리기가 넘힘들러요",
          "speaker_id": "2",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100012894.1.18",
          "form": "안젖을수도 없고 나갈때 차라리 슬리퍼 신어야 겠어요 ㅋㅌ",
          "original_form": "안젖을수도 없고 나갈때 차라리 슬리퍼 신어야 겠어요 ㅋㅌ",
          "speaker_id": "2",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100012894.1.19",
          "form": "그게 편하죠",
          "original_form": "그게 편하죠",
          "speaker_id": "1",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100012894.1.20",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100012894.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100012894.1.22",
          "form": "지금 좀개어서 이제 빨래하고 에어컨 돌리려고요",
          "original_form": "지금 좀개어서 이제 빨래하고 에어컨 돌리려고요",
          "speaker_id": "2",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100012894.1.23",
          "form": "한번 돌려주면 집이 습도가 낮아져서 좋은거 같아요",
          "original_form": "한번 돌려주면 집이 습도가 낮아져서 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100012894.1.24",
          "form": "내 좋은 제품이죠",
          "original_form": "내 좋은 제품이죠",
          "speaker_id": "1",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100012894.1.25",
          "form": "없었음 여름 장마 못견디죠",
          "original_form": "없었음 여름 장마 못견디죠",
          "speaker_id": "2",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100012894.1.26",
          "form": "획기적이죠",
          "original_form": "획기적이죠",
          "speaker_id": "1",
          "time": "20210825 16:22"
        }
      ]
