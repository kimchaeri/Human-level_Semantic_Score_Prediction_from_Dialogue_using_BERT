      "utterance": [
        {
          "id": "MDRW2100021928.1.1",
          "form": "해산물은 좋아하세요?",
          "original_form": "해산물은 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 23:58"
        },
        {
          "id": "MDRW2100021928.1.2",
          "form": "해산물은 오징어만 좋아해요.. \r\n나머진 징그러워서",
          "original_form": "해산물은 오징어만 좋아해요.. \r\n나머진 징그러워서",
          "speaker_id": "1",
          "time": "20210829 00:00"
        },
        {
          "id": "MDRW2100021928.1.3",
          "form": "오 그러시구나",
          "original_form": "오 그러시구나",
          "speaker_id": "2",
          "time": "20210829 00:03"
        },
        {
          "id": "MDRW2100021928.1.4",
          "form": "고기는 물에 빠진 것도 좋아하세요?",
          "original_form": "고기는 물에 빠진 것도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 00:03"
        },
        {
          "id": "MDRW2100021928.1.5",
          "form": "갈비탕이라든지..",
          "original_form": "갈비탕이라든지..",
          "speaker_id": "2",
          "time": "20210829 00:03"
        },
        {
          "id": "MDRW2100021928.1.6",
          "form": "아.. 맛잘알 이시군요...",
          "original_form": "아.. 맛잘알 이시군요...",
          "speaker_id": "1",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100021928.1.7",
          "form": "탕중의 왕은 갈비탕입죠..  ㅋㅋㅋ",
          "original_form": "탕중의 왕은 갈비탕입죠..  ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:05"
        },
        {
          "id": "MDRW2100021928.1.8",
          "form": "주로 한식 좋아하시나바여?",
          "original_form": "주로 한식 좋아하시나바여?",
          "speaker_id": "1",
          "time": "20210829 00:05"
        },
        {
          "id": "MDRW2100021928.1.9",
          "form": "한국에서 파는 건 거의 좋아해요",
          "original_form": "한국에서 파는 건 거의 좋아해요",
          "speaker_id": "2",
          "time": "20210829 00:05"
        },
        {
          "id": "MDRW2100021928.1.10",
          "form": "한식이 아니어도요",
          "original_form": "한식이 아니어도요",
          "speaker_id": "2",
          "time": "20210829 00:05"
        },
        {
          "id": "MDRW2100021928.1.11",
          "form": "파스타류나 스테이크도 좋아하시나요?",
          "original_form": "파스타류나 스테이크도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210829 00:10"
        },
        {
          "id": "MDRW2100021928.1.12",
          "form": "아 물론 좋아합니당",
          "original_form": "아 물론 좋아합니당",
          "speaker_id": "2",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100021928.1.13",
          "form": "파스타는 오일파스타 제일 좋아해요",
          "original_form": "파스타는 오일파스타 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100021928.1.14",
          "form": "오일파스타는 무맛? 이지 않나여?",
          "original_form": "오일파스타는 무맛? 이지 않나여?",
          "speaker_id": "1",
          "time": "20210829 00:24"
        },
        {
          "id": "MDRW2100021928.1.15",
          "form": "그런가요ㅋㅋㅋ",
          "original_form": "그런가요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:25"
        },
        {
          "id": "MDRW2100021928.1.16",
          "form": "저는 오일파스타 먹으면 다른 재료맛이 잘 느껴져서 좋더라구요",
          "original_form": "저는 오일파스타 먹으면 다른 재료맛이 잘 느껴져서 좋더라구요",
          "speaker_id": "2",
          "time": "20210829 00:25"
        },
        {
          "id": "MDRW2100021928.1.17",
          "form": "마늘이나 조개나 시금치나..",
          "original_form": "마늘이나 조개나 시금치나..",
          "speaker_id": "2",
          "time": "20210829 00:26"
        },
        {
          "id": "MDRW2100021928.1.18",
          "form": "아.. 진짜 미식가이시구나..",
          "original_form": "아.. 진짜 미식가이시구나..",
          "speaker_id": "1",
          "time": "20210829 00:26"
        },
        {
          "id": "MDRW2100021928.1.19",
          "form": "맛집도 많이 다니세요?",
          "original_form": "맛집도 많이 다니세요?",
          "speaker_id": "1",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021928.1.20",
          "form": "기회가 되면요!",
          "original_form": "기회가 되면요!",
          "speaker_id": "2",
          "time": "20210829 00:29"
        },
        {
          "id": "MDRW2100021928.1.21",
          "form": "웨이팅을 즐기지는 않아요",
          "original_form": "웨이팅을 즐기지는 않아요",
          "speaker_id": "2",
          "time": "20210829 00:30"
        },
        {
          "id": "MDRW2100021928.1.22",
          "form": "최근엔 맛집 어디 가보셧어요?",
          "original_form": "최근엔 맛집 어디 가보셧어요?",
          "speaker_id": "1",
          "time": "20210829 00:31"
        },
        {
          "id": "MDRW2100021928.1.23",
          "form": "갈치덮밥집에서 좀 기다렸어요...ㅋㅋㅋㅋㅋ",
          "original_form": "갈치덮밥집에서 좀 기다렸어요...ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:33"
        }
      ]
    }
