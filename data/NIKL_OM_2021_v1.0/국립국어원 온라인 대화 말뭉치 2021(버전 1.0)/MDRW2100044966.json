      "utterance": [
        {
          "id": "MDRW2100044966.1.1",
          "form": "나 오늘 세이라이섬",
          "original_form": "나 오늘 세이라이섬",
          "speaker_id": "2",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044966.1.2",
          "form": "큰 퍼즐 하나",
          "original_form": "큰 퍼즐 하나",
          "speaker_id": "2",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044966.1.3",
          "form": "끝냇어",
          "original_form": "끝냇어",
          "speaker_id": "2",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044966.1.4",
          "form": "오 ㅊㅊ",
          "original_form": "오 ㅊㅊ",
          "speaker_id": "1",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044966.1.5",
          "form": "난 아직",
          "original_form": "난 아직",
          "speaker_id": "1",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044966.1.6",
          "form": "맵 안열엇어...",
          "original_form": "맵 안열엇어...",
          "speaker_id": "1",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100044966.1.7",
          "form": "거기에 진귀한 보물상자랑 화려한 보물상자 두개 짜잘한 보물상자 여러개",
          "original_form": "거기에 진귀한 보물상자랑 화려한 보물상자 두개 짜잘한 보물상자 여러개",
          "speaker_id": "2",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044966.1.8",
          "form": "준다",
          "original_form": "준다",
          "speaker_id": "2",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044966.1.9",
          "form": "아직도??",
          "original_form": "아직도??",
          "speaker_id": "2",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044966.1.10",
          "form": "라이덴 뽑을거면",
          "original_form": "라이덴 뽑을거면",
          "speaker_id": "2",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044966.1.11",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044966.1.12",
          "form": "미리 모아두는것도",
          "original_form": "미리 모아두는것도",
          "speaker_id": "2",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044966.1.13",
          "form": "나쁘지 않아",
          "original_form": "나쁘지 않아",
          "speaker_id": "2",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044966.1.14",
          "form": "아니 와타츠미 신의 눈동자 좀 먹고",
          "original_form": "아니 와타츠미 신의 눈동자 좀 먹고",
          "speaker_id": "1",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044966.1.15",
          "form": "퍼즐 좀 풀다보니...",
          "original_form": "퍼즐 좀 풀다보니...",
          "speaker_id": "1",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044966.1.16",
          "form": "번개씨앗 레벨 얼마나 올렸어?",
          "original_form": "번개씨앗 레벨 얼마나 올렸어?",
          "speaker_id": "1",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100044966.1.17",
          "form": "번개씨앗 지금 16렙일걸?",
          "original_form": "번개씨앗 지금 16렙일걸?",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044966.1.18",
          "form": "아 근데 거기 진짜",
          "original_form": "아 근데 거기 진짜",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
