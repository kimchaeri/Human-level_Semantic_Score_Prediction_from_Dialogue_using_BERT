      "utterance": [
        {
          "id": "MDRW2100004143.1.1",
          "form": "최근에 구매한 꿀템같은거 있으신가요??",
          "original_form": "최근에 구매한 꿀템같은거 있으신가요??",
          "speaker_id": "1",
          "time": "20210821 15:08"
        },
        {
          "id": "MDRW2100004143.1.2",
          "form": "전 아이패드 거치대 샀어요",
          "original_form": "전 아이패드 거치대 샀어요",
          "speaker_id": "2",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004143.1.3",
          "form": "튼튼하진 않은데 세우기 좋아요",
          "original_form": "튼튼하진 않은데 세우기 좋아요",
          "speaker_id": "2",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004143.1.4",
          "form": "오 저도 있어요 ㅋㅋㅋ 목이 전보다 편하죠",
          "original_form": "오 저도 있어요 ㅋㅋㅋ 목이 전보다 편하죠",
          "speaker_id": "1",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004143.1.5",
          "form": "그쵸 진작 살 걸 그랬어요",
          "original_form": "그쵸 진작 살 걸 그랬어요",
          "speaker_id": "2",
          "time": "20210821 17:05"
        },
        {
          "id": "MDRW2100004143.1.6",
          "form": "훨씬 편하더라구요",
          "original_form": "훨씬 편하더라구요",
          "speaker_id": "2",
          "time": "20210821 17:05"
        },
        {
          "id": "MDRW2100004143.1.7",
          "form": "맞아요 ㅋㅋㅋㅋ 숙이지 않아도 되고",
          "original_form": "맞아요 ㅋㅋㅋㅋ 숙이지 않아도 되고",
          "speaker_id": "1",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004143.1.8",
          "form": "펜슬 사용하기에 더 편하더라구요",
          "original_form": "펜슬 사용하기에 더 편하더라구요",
          "speaker_id": "1",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004143.1.9",
          "form": "맞아요 거치대 써보니까 제가 얼마나",
          "original_form": "맞아요 거치대 써보니까 제가 얼마나",
          "speaker_id": "2",
          "time": "20210821 18:12"
        },
        {
          "id": "MDRW2100004143.1.10",
          "form": "거북목처럼 사용했나 생각하게 되더라구요",
          "original_form": "거북목처럼 사용했나 생각하게 되더라구요",
          "speaker_id": "2",
          "time": "20210821 18:12"
        },
        {
          "id": "MDRW2100004143.1.11",
          "form": "공감해요... 폰도 자주하고 이러다보니",
          "original_form": "공감해요... 폰도 자주하고 이러다보니",
          "speaker_id": "1",
          "time": "20210822 13:03"
        },
        {
          "id": "MDRW2100004143.1.12",
          "form": "패드는 보통 영상보고 이러면 사용시간이 길어서",
          "original_form": "패드는 보통 영상보고 이러면 사용시간이 길어서",
          "speaker_id": "1",
          "time": "20210822 13:03"
        },
        {
          "id": "MDRW2100004143.1.13",
          "form": "거치대 필수인거같아요 ㅎㅎ",
          "original_form": "거치대 필수인거같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 13:03"
        },
        {
          "id": "MDRW2100004143.1.14",
          "form": "맞아요 저 오늘 패드 들고 나왔는데 거치대 없어서 그새 불편하네요ㅠ",
          "original_form": "맞아요 저 오늘 패드 들고 나왔는데 거치대 없어서 그새 불편하네요ㅠ",
          "speaker_id": "2",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100004143.1.15",
          "form": "전 요새 반지도 사고 싶어요ㅎㅎ",
          "original_form": "전 요새 반지도 사고 싶어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 15:32"
        },
        {
          "id": "MDRW2100004143.1.16",
          "form": "오 반지 좋죠 ㅋㅋㅋㅋ 저도 늘 반지 해요 어떤 스타일 좋아하세요??",
          "original_form": "오 반지 좋죠 ㅋㅋㅋㅋ 저도 늘 반지 해요 어떤 스타일 좋아하세요??",
          "speaker_id": "1",
          "time": "20210823 09:19"
        },
        {
          "id": "MDRW2100004143.1.17",
          "form": "저 판도라 반지 좋아해요!",
          "original_form": "저 판도라 반지 좋아해요!",
          "speaker_id": "2",
          "time": "20210823 20:10"
        },
        {
          "id": "MDRW2100004143.1.18",
          "form": "요새는 새로 하나 맞출까 생각중이거든요",
          "original_form": "요새는 새로 하나 맞출까 생각중이거든요",
          "speaker_id": "2",
          "time": "20210823 20:11"
        },
        {
          "id": "MDRW2100004143.1.19",
          "form": "오.. 저도 판도라 몇개 있어요",
          "original_form": "오.. 저도 판도라 몇개 있어요",
          "speaker_id": "1",
          "time": "20210823 22:46"
        },
        {
          "id": "MDRW2100004143.1.20",
          "form": "뭔가 근데 다 있을법한 흔한 반지들만 있네요 ㅋㅋㅋㅋ",
          "original_form": "뭔가 근데 다 있을법한 흔한 반지들만 있네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:46"
        },
        {
          "id": "MDRW2100004143.1.21",
          "form": "ㅋㅋㅋ왠지 저희 가진게 비슷할 거 같아요ㅎㅎ",
          "original_form": "ㅋㅋㅋ왠지 저희 가진게 비슷할 거 같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 11:43"
        },
        {
          "id": "MDRW2100004143.1.22",
          "form": "그쵸 ㅋㅋㅋㅋ 저도 그 생각 들어요",
          "original_form": "그쵸 ㅋㅋㅋㅋ 저도 그 생각 들어요",
          "speaker_id": "1",
          "time": "20210824 15:53"
        },
        {
          "id": "MDRW2100004143.1.23",
          "form": "하트반지랑 하트가드링....에 혹시 탄생석도??",
          "original_form": "하트반지랑 하트가드링....에 혹시 탄생석도??",
          "speaker_id": "1",
          "time": "20210824 15:53"
        },
        {
          "id": "MDRW2100004143.1.24",
          "form": "판도라 뭐뭐 가지고 계세요?",
          "original_form": "판도라 뭐뭐 가지고 계세요?",
          "speaker_id": "2",
