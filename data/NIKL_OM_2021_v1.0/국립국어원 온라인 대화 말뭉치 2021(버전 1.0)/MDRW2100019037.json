      "utterance": [
        {
          "id": "MDRW2100019037.1.1",
          "form": "그니까 너무 창피해",
          "original_form": "그니까 너무 창피해",
          "speaker_id": "2",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100019037.1.2",
