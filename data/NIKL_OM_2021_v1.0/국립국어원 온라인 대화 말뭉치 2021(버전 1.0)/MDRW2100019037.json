          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100019037.1.3",
          "form": "대학교학생증에는",
          "original_form": "대학교학생증에는",
          "speaker_id": "1",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100019037.1.4",
          "form": "카드기능없음?",
          "original_form": "카드기능없음?",
          "speaker_id": "1",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100019037.1.5",
          "form": "없을 걸?",
          "original_form": "없을 걸?",
          "speaker_id": "2",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100019037.1.6",
          "form": "없어야 할 걸",
          "original_form": "없어야 할 걸",
          "speaker_id": "2",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100019037.1.7",
          "form": "내 기억으론 없음",
          "original_form": "내 기억으론 없음",
          "speaker_id": "2",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100019037.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ학생증발급받긴함?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ학생증발급받긴함?",
          "speaker_id": "1",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100019037.1.9",
          "form": "작년인가 발급 받고",
          "original_form": "작년인가 발급 받고",
          "speaker_id": "2",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100019037.1.10",
          "form": "지갑에 고이 잠들게 함",
          "original_form": "지갑에 고이 잠들게 함",
          "speaker_id": "2",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100019037.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아",
          "speaker_id": "1",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100019037.1.12",
          "form": "작년에학교가보긴했어?",
          "original_form": "작년에학교가보긴했어?",
          "speaker_id": "1",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100019037.1.13",
          "form": "시험치러 한번 가봄",
          "original_form": "시험치러 한번 가봄",
          "speaker_id": "2",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100019037.1.14",
          "form": "지금까지 입시 포함해서 학교 2번 가봄",
          "original_form": "지금까지 입시 포함해서 학교 2번 가봄",
