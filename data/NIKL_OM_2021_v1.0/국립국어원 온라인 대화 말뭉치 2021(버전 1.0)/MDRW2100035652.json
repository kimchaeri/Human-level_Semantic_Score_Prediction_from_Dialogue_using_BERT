      "utterance": [
        {
          "id": "MDRW2100035652.1.1",
          "form": "그 성범죄 저지른 전 멤버요?",
          "original_form": "그 성범죄 저지른 전 멤버요?",
          "speaker_id": "2",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100035652.1.2",
          "form": "쟤는 클라스가 다르다 .. 이 생각 했어요",
          "original_form": "쟤는 클라스가 다르다 .. 이 생각 했어요",
          "speaker_id": "1",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100035652.1.3",
          "form": "옙...",
          "original_form": "옙...",
          "speaker_id": "1",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100035652.1.4",
          "form": "진짜 악질중에",
          "original_form": "진짜 악질중에",
          "speaker_id": "1",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100035652.1.5",
          "form": "최고 ..",
          "original_form": "최고 ..",
          "speaker_id": "1",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100035652.1.6",
          "form": "그니까여... 중국가서 아주 지세상되니까",
          "original_form": "그니까여... 중국가서 아주 지세상되니까",
          "speaker_id": "2",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100035652.1.7",
          "form": "본색 드러난거죠 뭐...",
          "original_form": "본색 드러난거죠 뭐...",
          "speaker_id": "2",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100035652.1.8",
          "form": "전 몰랐는데 걔가 그리",
          "original_form": "전 몰랐는데 걔가 그리",
          "speaker_id": "1",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100035652.1.9",
          "form": "잘나갔다면서요??",
          "original_form": "잘나갔다면서요??",
          "speaker_id": "1",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100035652.1.10",
          "form": "막 명품 엠버서더하고",
          "original_form": "막 명품 엠버서더하고",
          "speaker_id": "1",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100035652.1.11",
          "form": "저 그거까진 잘 모르겠는데",
          "original_form": "저 그거까진 잘 모르겠는데",
          "speaker_id": "2",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100035652.1.12",
          "form": "걔 탈퇴할때 개인팬이 엄청많았던것 같기도 하고...",
          "original_form": "걔 탈퇴할때 개인팬이 엄청많았던것 같기도 하고...",
          "speaker_id": "2",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100035652.1.13",
          "form": "아 하긴 중국에서 인기가 있으니",
          "original_form": "아 하긴 중국에서 인기가 있으니",
          "speaker_id": "1",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100035652.1.14",
          "form": "둘기가 되서 떠났겠죠 ..",
          "original_form": "둘기가 되서 떠났겠죠 ..",
          "speaker_id": "1",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100035652.1.15",
          "form": "그런애가 구최애면.... 진짜 수치.....",
          "original_form": "그런애가 구최애면.... 진짜 수치.....",
          "speaker_id": "2",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100035652.1.16",
          "form": "와 진짜 최고 수치 아닙니까",
          "original_form": "와 진짜 최고 수치 아닙니까",
          "speaker_id": "1",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100035652.1.17",
          "form": "둘기ㅋㅋㅋㅋ 아니 근데 왜 둘기라고 하는거예뇨?",
          "original_form": "둘기ㅋㅋㅋㅋ 아니 근데 왜 둘기라고 하는거예뇨?",
          "speaker_id": "2",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100035652.1.18",
          "form": "귀소본능을 놀리는건가요?",
          "original_form": "귀소본능을 놀리는건가요?",
          "speaker_id": "2",
          "time": "20210829 22:01"
        },
        {
          "id": "MDRW2100035652.1.19",
          "form": "그거 아맠ㅋㅋ",
          "original_form": "그거 아맠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:01"
        },
        {
          "id": "MDRW2100035652.1.20",
          "form": "비둘기처럼 푸드덕 날랐다고",
          "original_form": "비둘기처럼 푸드덕 날랐다고",
          "speaker_id": "1",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100035652.1.21",
          "form": "둘길껄요?",
          "original_form": "둘길껄요?",
          "speaker_id": "1",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100035652.1.22",
          "form": "아닠ㅋㅋㅋㅋㅋ 그런 단순한 이유...",
          "original_form": "아닠ㅋㅋㅋㅋㅋ 그런 단순한 이유...",
          "speaker_id": "2",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100035652.1.23",
          "form": "진짜 별뜻 없죸ㅋㅋㅋ",
          "original_form": "진짜 별뜻 없죸ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100035652.1.24",
          "form": "그거 아마 또",
          "original_form": "그거 아마 또",
          "speaker_id": "1",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100035652.1.25",
          "form": "그 엑소 세계관 ㅋㅋㅋ",
          "original_form": "그 엑소 세계관 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100035652.1.26",
          "form": "아아악ㅋㅋㅋㅋㅋㅋㅋ 세계관이랑 관련있나요",
          "original_form": "아아악ㅋㅋㅋㅋㅋㅋㅋ 세계관이랑 관련있나요",
          "speaker_id": "2",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100035652.1.27",
          "form": "걔가 비행 담당이라 .. 또",
          "original_form": "걔가 비행 담당이라 .. 또",
          "speaker_id": "1",
          "time": "20210829 22:03"
        }
