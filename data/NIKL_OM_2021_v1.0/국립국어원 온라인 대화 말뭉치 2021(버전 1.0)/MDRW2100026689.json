      "utterance": [
        {
          "id": "MDRW2100026689.1.1",
          "form": "전시회 좋아하세요??",
          "original_form": "전시회 좋아하세요??",
          "speaker_id": "2",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026689.1.2",
          "form": "네네 좋아해요",
          "original_form": "네네 좋아해요",
          "speaker_id": "1",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026689.1.3",
          "form": "특히 사진찍기좋은곳요",
          "original_form": "특히 사진찍기좋은곳요",
          "speaker_id": "1",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026689.1.4",
          "form": "예전에는 보러 가는거 좋아했는데 언제 보러갔는지 기억도 안나네요",
          "original_form": "예전에는 보러 가는거 좋아했는데 언제 보러갔는지 기억도 안나네요",
          "speaker_id": "2",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026689.1.5",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "2",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026689.1.6",
          "form": "그쵸 코로나땜에",
          "original_form": "그쵸 코로나땜에",
          "speaker_id": "1",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026689.1.7",
          "form": "최근에 뭐 보셨어요??",
          "original_form": "최근에 뭐 보셨어요??",
          "speaker_id": "2",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026689.1.8",
          "form": "안간지 오래예요 저도",
          "original_form": "안간지 오래예요 저도",
          "speaker_id": "1",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026689.1.9",
          "form": "ㅜㅜㅜ",
          "original_form": "ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100026689.1.10",
          "form": "저 마지막으로 갔던게 동대문이었던거 같은데",
          "original_form": "저 마지막으로 갔던게 동대문이었던거 같은데",
          "speaker_id": "2",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026689.1.11",
          "form": "동대문 좋죠",
          "original_form": "동대문 좋죠",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026689.1.12",
          "form": "패션디자이너 전시였어요",
          "original_form": "패션디자이너 전시였어요",
          "speaker_id": "2",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026689.1.13",
          "form": "폴 스미스였나...",
          "original_form": "폴 스미스였나...",
          "speaker_id": "2",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026689.1.14",
          "form": "오 패션전시 한번도 안가봐서",
          "original_form": "오 패션전시 한번도 안가봐서",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026689.1.15",
          "form": "신기해요",
          "original_form": "신기해요",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026689.1.16",
          "form": "진짜 오래전인거 같은데 기억이 안나네요",
          "original_form": "진짜 오래전인거 같은데 기억이 안나네요",
          "speaker_id": "2",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100026689.1.17",
          "form": "그냥 비슷하더라고요",
          "original_form": "그냥 비슷하더라고요",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100026689.1.18",
          "form": "전 무슨 계절관련 전시였어요 마지막에 간게",
          "original_form": "전 무슨 계절관련 전시였어요 마지막에 간게",
          "speaker_id": "1",
          "time": "20210901 23:39"
        }
      ]
