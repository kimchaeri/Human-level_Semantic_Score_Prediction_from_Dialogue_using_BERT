      "utterance": [
        {
          "id": "MDRW2100016307.1.1",
          "form": "항공과는 첨보네요",
          "original_form": "항공과는 첨보네요",
          "speaker_id": "1",
          "time": "20210826 11:49"
        },
        {
          "id": "MDRW2100016307.1.2",
          "form": "항공과는 제복 입는다던데 트루?",
          "original_form": "항공과는 제복 입는다던데 트루?",
          "speaker_id": "1",
          "time": "20210826 11:50"
        },
        {
          "id": "MDRW2100016307.1.3",
          "form": "넹 그렇습니당",
          "original_form": "넹 그렇습니당",
          "speaker_id": "2",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016307.1.4",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016307.1.5",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "1",
          "time": "20210826 12:17"
        },
        {
          "id": "MDRW2100016307.1.6",
          "form": "대학같은 느낌보다는 이미 반쯤 직장같은 느낌이겠네요",
          "original_form": "대학같은 느낌보다는 이미 반쯤 직장같은 느낌이겠네요",
          "speaker_id": "1",
          "time": "20210826 12:17"
        },
        {
          "id": "MDRW2100016307.1.7",
          "form": "근데 그렇지도 않아요",
          "original_form": "근데 그렇지도 않아요",
          "speaker_id": "2",
          "time": "20210826 12:20"
        },
        {
          "id": "MDRW2100016307.1.8",
          "form": "그냥 고등학교 교복느낌?",
          "original_form": "그냥 고등학교 교복느낌?",
          "speaker_id": "2",
          "time": "20210826 12:20"
        },
        {
          "id": "MDRW2100016307.1.9",
          "form": "옷 자체는 아니긴 한데 느낌이 그냥",
          "original_form": "옷 자체는 아니긴 한데 느낌이 그냥",
          "speaker_id": "2",
          "time": "20210826 12:20"
        },
        {
          "id": "MDRW2100016307.1.10",
          "form": "거기 군기 빡센편인가요?",
          "original_form": "거기 군기 빡센편인가요?",
          "speaker_id": "1",
          "time": "20210826 12:21"
        },
        {
          "id": "MDRW2100016307.1.11",
          "form": "생각보단 괜찮은 것 같아요",
          "original_form": "생각보단 괜찮은 것 같아요",
          "speaker_id": "2",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016307.1.12",
          "form": "다들 가면 외모 엄청 빡세게 해다니겠다...",
          "original_form": "다들 가면 외모 엄청 빡세게 해다니겠다...",
          "speaker_id": "1",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016307.1.13",
          "form": "아침에 일어나기 싫겠어요ㅋㅋㅋ",
          "original_form": "아침에 일어나기 싫겠어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100016307.1.14",
          "form": "근데 적응하면 머리까지 20분 컷",
          "original_form": "근데 적응하면 머리까지 20분 컷",
          "speaker_id": "2",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016307.1.15",
          "form": "저 화장 10분만에 할 수 있어요",
          "original_form": "저 화장 10분만에 할 수 있어요",
          "speaker_id": "2",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016307.1.16",
          "form": "워...",
          "original_form": "워...",
          "speaker_id": "1",
          "time": "20210826 12:26"
        },
        {
          "id": "MDRW2100016307.1.17",
          "form": "전 남잔데 머리하는데 10분 넘게걸리는데ㅋㅋㅋ",
          "original_form": "전 남잔데 머리하는데 10분 넘게걸리는데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:27"
        },
        {
          "id": "MDRW2100016307.1.18",
          "form": "머리 세우고 다니세요???",
          "original_form": "머리 세우고 다니세요???",
          "speaker_id": "2",
          "time": "20210826 12:27"
        },
        {
          "id": "MDRW2100016307.1.19",
          "form": "아뇨ㅋㅋㅋ 포마드스타일 하거나 가르마 해다녀요",
          "original_form": "아뇨ㅋㅋㅋ 포마드스타일 하거나 가르마 해다녀요",
          "speaker_id": "1",
          "time": "20210826 12:28"
        },
        {
          "id": "MDRW2100016307.1.20",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210826 12:47"
        },
        {
          "id": "MDRW2100016307.1.21",
          "form": "왁스 쓰세요??",
          "original_form": "왁스 쓰세요??",
          "speaker_id": "2",
          "time": "20210826 12:47"
        },
        {
          "id": "MDRW2100016307.1.22",
          "form": "아님 스프레이?",
          "original_form": "아님 스프레이?",
          "speaker_id": "2",
          "time": "20210826 12:47"
        },
        {
          "id": "MDRW2100016307.1.23",
          "form": "둘다 써요",
          "original_form": "둘다 써요",
          "speaker_id": "1",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100016307.1.24",
          "form": "왁스로 질감 내고",
          "original_form": "왁스로 질감 내고",
          "speaker_id": "1",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100016307.1.25",
          "form": "스프레이로 고정",
          "original_form": "스프레이로 고정",
          "speaker_id": "1",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100016307.1.26",
          "form": "오 뭔가 장인의 느낌",
          "original_form": "오 뭔가 장인의 느낌",
          "speaker_id": "2",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100016307.1.27",
          "form": "전 오일이랑 스프레이써요",
          "original_form": "전 오일이랑 스프레이써요",
          "speaker_id": "2",
          "time": "20210826 12:50"
        },
        {
          "id": "MDRW2100016307.1.28",
          "form": "오일은 뭐 호호바같은거 써요?",
          "original_form": "오일은 뭐 호호바같은거 써요?",
          "speaker_id": "1",
          "time": "20210826 12:53"
        },
        {
          "id": "MDRW2100016307.1.29",
          "form": "음 그냥 헤어오일 아무거나..",
          "original_form": "음 그냥 헤어오일 아무거나..",
          "speaker_id": "2",
          "time": "20210826 12:54"
        },
        {
          "id": "MDRW2100016307.1.30",
          "form": "매일 머리할 땐 헤어젤 쓰니까 좀 끈적거리고 그래가지고",
          "original_form": "매일 머리할 땐 헤어젤 쓰니까 좀 끈적거리고 그래가지고",
          "speaker_id": "2",
          "time": "20210826 12:54"
        },
        {
          "id": "MDRW2100016307.1.31",
          "form": "오일로 바꿨어요",
          "original_form": "오일로 바꿨어요",
          "speaker_id": "2",
          "time": "20210826 12:54"
        },
        {
          "id": "MDRW2100016307.1.32",
          "form": "예전에 수염 관리할때 호호바오일 썼었거든요ㅋㅋ",
          "original_form": "예전에 수염 관리할때 호호바오일 썼었거든요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:56"
        },
        {
          "id": "MDRW2100016307.1.33",
          "form": "좋나요???",
          "original_form": "좋나요???",
          "speaker_id": "2",
          "time": "20210826 12:56"
        },
        {
          "id": "MDRW2100016307.1.34",
          "form": "이름 귀엽다 호호바",
          "original_form": "이름 귀엽다 호호바",
          "speaker_id": "2",
          "time": "20210826 12:56"
        },
        {
          "id": "MDRW2100016307.1.35",
          "form": "향이 좋아요",
          "original_form": "향이 좋아요",
          "speaker_id": "1",
          "time": "20210826 13:13"
        }
      ]
    }
