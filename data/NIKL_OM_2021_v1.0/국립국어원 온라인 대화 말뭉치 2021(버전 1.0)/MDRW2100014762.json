      "utterance": [
        {
          "id": "MDRW2100014762.1.1",
          "form": "결혼에대해서 어케생각하세요",
          "original_form": "결혼에대해서 어케생각하세요",
          "speaker_id": "1",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014762.1.2",
          "form": "전 결혼은 해야한다 생각해요",
          "original_form": "전 결혼은 해야한다 생각해요",
          "speaker_id": "2",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014762.1.3",
          "form": "기혼이세요?",
          "original_form": "기혼이세요?",
          "speaker_id": "2",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014762.1.4",
          "form": "아뇨 미혼이요",
          "original_form": "아뇨 미혼이요",
          "speaker_id": "1",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014762.1.5",
          "form": "근데 결혼적령기에왿",
          "original_form": "근데 결혼적령기에왿",
          "speaker_id": "1",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014762.1.6",
          "form": "사실 지났어요",
          "original_form": "사실 지났어요",
          "speaker_id": "1",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014762.1.7",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014762.1.8",
          "form": "아하 전 기혼이요:)",
          "original_form": "아하 전 기혼이요:)",
          "speaker_id": "2",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014762.1.9",
          "form": "결혼하면 뭐가 좋아요?",
          "original_form": "결혼하면 뭐가 좋아요?",
          "speaker_id": "1",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014762.1.10",
          "form": "고민돼요",
          "original_form": "고민돼요",
          "speaker_id": "1",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014762.1.11",
          "form": "결혼하실 여자친구분 있으세요?",
          "original_form": "결혼하실 여자친구분 있으세요?",
          "speaker_id": "2",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014762.1.12",
          "form": "아 전 여자에요",
          "original_form": "아 전 여자에요",
          "speaker_id": "1",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014762.1.13",
          "form": "남친이있긴한데",
          "original_form": "남친이있긴한데",
          "speaker_id": "1",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014762.1.14",
          "form": "고민돼요",
          "original_form": "고민돼요",
          "speaker_id": "1",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014762.1.15",
          "form": "이사람이다!라는 확신이없어서",
          "original_form": "이사람이다!라는 확신이없어서",
          "speaker_id": "1",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014762.1.16",
          "form": "아하ㅋㅋㅋ아이디가 남자 이름이라 남자분인줄요 ㅎㅎ",
          "original_form": "아하ㅋㅋㅋ아이디가 남자 이름이라 남자분인줄요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:45"
        },
        {
          "id": "MDRW2100014762.1.17",
          "form": "어떤 부분때문에 확신이 없으신가요?",
          "original_form": "어떤 부분때문에 확신이 없으신가요?",
          "speaker_id": "2",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014762.1.18",
          "form": "뭔가 결혼해서 죽을때까지 날사랑해줄수있을까",
          "original_form": "뭔가 결혼해서 죽을때까지 날사랑해줄수있을까",
          "speaker_id": "1",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014762.1.19",
          "form": "서로 평생 아껴줄수있을까?",
          "original_form": "서로 평생 아껴줄수있을까?",
          "speaker_id": "1",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014762.1.20",
          "form": "아....그건.....ㅋㅋㅋ절대 확신이 생길 수 없는 부분ㅋㅋㅋㅋ",
          "original_form": "아....그건.....ㅋㅋㅋ절대 확신이 생길 수 없는 부분ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014762.1.21",
          "form": "아 그런가욬ㅋㅋㅋㅋ",
          "original_form": "아 그런가욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014762.1.22",
          "form": "넘 욕심이었네요",
          "original_form": "넘 욕심이었네요",
          "speaker_id": "1",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014762.1.23",
          "form": "그냥 사람 성실하고 착하면 되는거죠?",
          "original_form": "그냥 사람 성실하고 착하면 되는거죠?",
          "speaker_id": "1",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014762.1.24",
          "form": "네 전 술담배 안하고 성실하고 착하고 그런 모습 봤어욕",
          "original_form": "네 전 술담배 안하고 성실하고 착하고 그런 모습 봤어욕",
          "speaker_id": "2",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014762.1.25",
          "form": "근데 가정환경은 보는 게 좋은 것 같아요..",
          "original_form": "근데 가정환경은 보는 게 좋은 것 같아요..",
          "speaker_id": "2",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014762.1.26",
          "form": "아.. 가정환경이라면 가족?아니면 재산?",
          "original_form": "아.. 가정환경이라면 가족?아니면 재산?",
          "speaker_id": "1",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014762.1.27",
          "form": "가족이요. 아니라고 해도 성향을 따라가더라고요",
          "original_form": "가족이요. 아니라고 해도 성향을 따라가더라고요",
          "speaker_id": "2",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014762.1.28",
          "form": "기왕이면 화목한 가정에서 자란 사람이 좋아요",
          "original_form": "기왕이면 화목한 가정에서 자란 사람이 좋아요",
          "speaker_id": "2",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014762.1.29",
          "form": "아 화목한가정",
          "original_form": "아 화목한가정",
          "speaker_id": "1",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014762.1.30",
          "form": "진짜 중요해요!",
          "original_form": "진짜 중요해요!",
          "speaker_id": "2",
          "time": "20210825 21:23"
        }
      ]
    }
