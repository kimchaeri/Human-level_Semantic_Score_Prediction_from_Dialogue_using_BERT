      "utterance": [
        {
          "id": "MDRW2100021307.1.1",
          "form": "티원은 10000명넘고",
          "original_form": "티원은 10000명넘고",
          "speaker_id": "2",
          "time": "20210828 17:28"
        },
        {
          "id": "MDRW2100021307.1.2",
          "form": "담원 겨우2000명임",
          "original_form": "담원 겨우2000명임",
          "speaker_id": "2",
          "time": "20210828 17:28"
        },
        {
          "id": "MDRW2100021307.1.3",
          "form": "담원은",
          "original_form": "담원은",
          "speaker_id": "1",
          "time": "20210828 17:28"
        },
        {
          "id": "MDRW2100021307.1.4",
          "form": "역사가 짧아서 그런가",
          "original_form": "역사가 짧아서 그런가",
          "speaker_id": "1",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100021307.1.5",
          "form": "페이커 하나 존재만으로",
          "original_form": "페이커 하나 존재만으로",
          "speaker_id": "1",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100021307.1.6",
          "form": "압도적이긴하네",
          "original_form": "압도적이긴하네",
          "speaker_id": "1",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100021307.1.7",
          "form": "칸이랑 바텀듀오",
          "original_form": "칸이랑 바텀듀오",
          "speaker_id": "2",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100021307.1.8",
          "form": "인성논란으로",
          "original_form": "인성논란으로",
          "speaker_id": "2",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100021307.1.9",
          "form": "팬떠난것도많고",
          "original_form": "팬떠난것도많고",
          "speaker_id": "2",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100021307.1.10",
          "form": "칸 인성은 원래 유명햇지 ㅋㅋㅋ",
          "original_form": "칸 인성은 원래 유명햇지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:30"
        },
        {
          "id": "MDRW2100021307.1.11",
          "form": "게임하다 패드립하고",
          "original_form": "게임하다 패드립하고",
          "speaker_id": "1",
          "time": "20210828 17:30"
        },
        {
          "id": "MDRW2100021307.1.12",
          "form": "ㅇㅇㅇ그거땜에 그럴수도",
          "original_form": "ㅇㅇㅇ그거땜에 그럴수도",
          "speaker_id": "2",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100021307.1.13",
          "form": "시작한다",
          "original_form": "시작한다",
          "speaker_id": "2",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100021307.1.14",
          "form": "이번 바텀듀오가",
          "original_form": "이번 바텀듀오가",
          "speaker_id": "1",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100021307.1.15",
          "form": "룰루 비하인가?",
          "original_form": "룰루 비하인가?",
          "speaker_id": "1",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100021307.1.16",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100021307.1.17",
          "form": "고스트베릴",
          "original_form": "고스트베릴",
          "speaker_id": "2",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100021307.1.18",
          "form": "순당무",
          "original_form": "순당무",
          "speaker_id": "2",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100021307.1.19",
          "form": "쇼메 라이즈선픽보소",
          "original_form": "쇼메 라이즈선픽보소",
          "speaker_id": "2",
          "time": "20210828 17:34"
        },
        {
          "id": "MDRW2100021307.1.20",
          "form": "둘다 미드선픽이네",
          "original_form": "둘다 미드선픽이네",
          "speaker_id": "2",
          "time": "20210828 17:34"
        },
        {
          "id": "MDRW2100021307.1.21",
          "form": "오 이번엔",
          "original_form": "오 이번엔",
          "speaker_id": "1",
          "time": "20210828 17:35"
        },
        {
          "id": "MDRW2100021307.1.22",
          "form": "미드는 노잼일듯",
          "original_form": "미드는 노잼일듯",
          "speaker_id": "1",
          "time": "20210828 17:35"
        },
        {
          "id": "MDRW2100021307.1.23",
          "form": "카밀 그웬 라이즈 아지르",
          "original_form": "카밀 그웬 라이즈 아지르",
          "speaker_id": "2",
          "time": "20210828 17:35"
        },
        {
          "id": "MDRW2100021307.1.24",
          "form": "재밌겄노",
          "original_form": "재밌겄노",
          "speaker_id": "2",
          "time": "20210828 17:35"
        },
        {
          "id": "MDRW2100021307.1.25",
          "form": "그웬 나옴?",
          "original_form": "그웬 나옴?",
          "speaker_id": "1",
          "time": "20210828 17:36"
        },
        {
          "id": "MDRW2100021307.1.26",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210828 17:36"
        },
        {
          "id": "MDRW2100021307.1.27",
          "form": "음 정석픽만나왓네",
          "original_form": "음 정석픽만나왓네",
          "speaker_id": "1",
          "time": "20210828 17:37"
        },
        {
          "id": "MDRW2100021307.1.28",
          "form": "그웬 비에고 아지르 이즈 레오나",
          "original_form": "그웬 비에고 아지르 이즈 레오나",
          "speaker_id": "2",
          "time": "20210828 17:38"
        },
        {
          "id": "MDRW2100021307.1.29",
          "form": "카밀 트런들 라이즈 애쉬 브라움",
          "original_form": "카밀 트런들 라이즈 애쉬 브라움",
          "speaker_id": "2",
          "time": "20210828 17:38"
        },
        {
          "id": "MDRW2100021307.1.30",
          "form": "정석이네 둘다 그냥",
          "original_form": "정석이네 둘다 그냥",
          "speaker_id": "2",
          "time": "20210828 17:38"
        },
        {
          "id": "MDRW2100021307.1.31",
          "form": "시작한다 이제",
          "original_form": "시작한다 이제",
          "speaker_id": "1",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100021307.1.32",
          "form": "벌써 결승인게 신기하네 ㅋㅋ",
          "original_form": "벌써 결승인게 신기하네 ㅋㅋ",
          "speaker_id": "1",
