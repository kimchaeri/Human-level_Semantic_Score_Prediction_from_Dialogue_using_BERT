      "utterance": [
        {
          "id": "MDRW2100002785.1.1",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002785.1.2",
          "form": "전 도마뱀을 키워요 ㅎㅎ",
          "original_form": "전 도마뱀을 키워요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002785.1.3",
          "form": "우와!! 도마뱀이요?!?!?",
          "original_form": "우와!! 도마뱀이요?!?!?",
          "speaker_id": "2",
          "time": "20210820 16:16"
        },
        {
          "id": "MDRW2100002785.1.4",
          "form": "파충류 좋아하시나보네요~",
          "original_form": "파충류 좋아하시나보네요~",
          "speaker_id": "2",
          "time": "20210820 16:16"
        },
        {
          "id": "MDRW2100002785.1.5",
          "form": "네 비어디 드레곤이요",
          "original_form": "네 비어디 드레곤이요",
          "speaker_id": "1",
          "time": "20210820 16:16"
        },
        {
          "id": "MDRW2100002785.1.6",
          "form": "그게 제가 털 알레르기가 있어서 도마뱀 키우게 됐어요",
          "original_form": "그게 제가 털 알레르기가 있어서 도마뱀 키우게 됐어요",
          "speaker_id": "1",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002785.1.7",
          "form": "녀석들 착해요",
          "original_form": "녀석들 착해요",
          "speaker_id": "1",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002785.1.8",
          "form": "오~~ '들'이라고 하시는 거 보면 한 마리가 아닌가 보네요~~",
          "original_form": "오~~ '들'이라고 하시는 거 보면 한 마리가 아닌가 보네요~~",
          "speaker_id": "2",
          "time": "20210820 16:18"
        },
        {
          "id": "MDRW2100002785.1.9",
          "form": "네 두마리요",
          "original_form": "네 두마리요",
          "speaker_id": "1",
          "time": "20210820 16:19"
        },
        {
          "id": "MDRW2100002785.1.10",
          "form": "name3이 name4이요",
          "original_form": "&name3&이 &name4&이요",
          "speaker_id": "1",
          "time": "20210820 16:20"
        },
        {
          "id": "MDRW2100002785.1.11",
          "form": "오오~~ 이름 특이하네요~//",
          "original_form": "오오~~ 이름 특이하네요~//",
          "speaker_id": "2",
          "time": "20210820 16:24"
        },
        {
          "id": "MDRW2100002785.1.12",
          "form": "오래 키우신거에요?ㅎ",
          "original_form": "오래 키우신거에요?ㅎ",
          "speaker_id": "2",
          "time": "20210820 16:24"
        },
        {
          "id": "MDRW2100002785.1.13",
          "form": "아니뇨 1년반요",
          "original_form": "아니뇨 1년반요",
          "speaker_id": "1",
          "time": "20210820 16:24"
        },
        {
          "id": "MDRW2100002785.1.14",
          "form": "어떤 동물 키우세요?",
          "original_form": "어떤 동물 키우세요?",
          "speaker_id": "1",
          "time": "20210820 16:31"
        },
        {
          "id": "MDRW2100002785.1.15",
          "form": "저는 동물... 키우고 싶은데 아직 마음의 준비단계에 있어요~",
          "original_form": "저는 동물... 키우고 싶은데 아직 마음의 준비단계에 있어요~",
          "speaker_id": "2",
          "time": "20210820 16:39"
        },
        {
          "id": "MDRW2100002785.1.16",
          "form": "아무래도 생명이다보니",
          "original_form": "아무래도 생명이다보니",
          "speaker_id": "2",
          "time": "20210820 16:39"
        },
        {
          "id": "MDRW2100002785.1.17",
          "form": "책임질 수 있을 때까지 신중하려고요",
          "original_form": "책임질 수 있을 때까지 신중하려고요",
          "speaker_id": "2",
          "time": "20210820 16:39"
        },
        {
          "id": "MDRW2100002785.1.18",
          "form": "저도 엄청 고민했어요",
          "original_form": "저도 엄청 고민했어요",
          "speaker_id": "1",
          "time": "20210820 16:43"
        },
        {
          "id": "MDRW2100002785.1.19",
          "form": "십년을 살텐대 내가 책임질수 있을까",
          "original_form": "십년을 살텐대 내가 책임질수 있을까",
          "speaker_id": "1",
          "time": "20210820 16:43"
        },
        {
          "id": "MDRW2100002785.1.20",
          "form": "아플까 아직도 겁나요",
          "original_form": "아플까 아직도 겁나요",
          "speaker_id": "1",
          "time": "20210820 16:43"
        },
        {
          "id": "MDRW2100002785.1.21",
          "form": "징짜..ㅠㅠ 아프면 너무 맘이 힘들거 같어요..",
          "original_form": "징짜..ㅠㅠ 아프면 너무 맘이 힘들거 같어요..",
          "speaker_id": "2",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002785.1.22",
          "form": "말도 안 통하니까.. 어떻게 해줘야할지 눈물만..ㅠ",
          "original_form": "말도 안 통하니까.. 어떻게 해줘야할지 눈물만..ㅠ",
          "speaker_id": "2",
          "time": "20210820 16:54"
        },
        {
          "id": "MDRW2100002785.1.23",
          "form": "예 한번 서로싸워 다쳤는데 정말 놀랐어요 낫는 내내 너무 걱정되서 혼났어요",
          "original_form": "예 한번 서로싸워 다쳤는데 정말 놀랐어요 낫는 내내 너무 걱정되서 혼났어요",
          "speaker_id": "1",
          "time": "20210820 17:00"
        },
        {
          "id": "MDRW2100002785.1.24",
          "form": "도마뱀은 치료 할때도 마땅치 않아요",
          "original_form": "도마뱀은 치료 할때도 마땅치 않아요",
          "speaker_id": "1",
          "time": "20210820 17:01"
        },
        {
          "id": "MDRW2100002785.1.25",
          "form": "강아지를 키우고 싶었는데 비염에 알레르기로 포기했어요",
          "original_form": "강아지를 키우고 싶었는데 비염에 알레르기로 포기했어요",
          "speaker_id": "1",
          "time": "20210820 17:12"
        },
        {
          "id": "MDRW2100002785.1.26",
          "form": "ㅠㅠ 강아지나 고양이 알레르기 있는 분들이 꽤 되더라고요",
          "original_form": "ㅠㅠ 강아지나 고양이 알레르기 있는 분들이 꽤 되더라고요",
          "speaker_id": "2",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002785.1.27",
          "form": "그래도 도마뱀은 털관련 문제는 없어서 다행이겠어요~",
          "original_form": "그래도 도마뱀은 털관련 문제는 없어서 다행이겠어요~",
          "speaker_id": "2",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002785.1.28",
          "form": "네 그래서 털때문에 도마뱀 길러요",
          "original_form": "네 그래서 털때문에 도마뱀 길러요",
