      "utterance": [
        {
          "id": "MDRW2100024693.1.1",
          "form": "여행자 보험이 얼마였는지는 기억이 안 난다",
          "original_form": "여행자 보험이 얼마였는지는 기억이 안 난다",
          "speaker_id": "2",
          "time": "20210831 17:16"
        },
        {
          "id": "MDRW2100024693.1.2",
          "form": "막 엄청 비싸지는 않았을거야",
          "original_form": "막 엄청 비싸지는 않았을거야",
          "speaker_id": "2",
          "time": "20210831 17:16"
        },
        {
          "id": "MDRW2100024693.1.3",
          "form": "넌 도보여행 안 좋아하니?",
          "original_form": "넌 도보여행 안 좋아하니?",
          "speaker_id": "2",
          "time": "20210831 17:17"
        },
        {
          "id": "MDRW2100024693.1.4",
          "form": "응 많이 걷는게 아까워",
          "original_form": "응 많이 걷는게 아까워",
          "speaker_id": "1",
          "time": "20210831 17:17"
        },
        {
          "id": "MDRW2100024693.1.5",
          "form": "이동시간이나 내 체력이나",
          "original_form": "이동시간이나 내 체력이나",
          "speaker_id": "1",
          "time": "20210831 17:17"
        },
        {
          "id": "MDRW2100024693.1.6",
          "form": "더  중요한 활동할",
          "original_form": "더  중요한 활동할",
          "speaker_id": "1",
          "time": "20210831 17:17"
        },
        {
          "id": "MDRW2100024693.1.7",
          "form": "에너지가 없는 느낌",
          "original_form": "에너지가 없는 느낌",
          "speaker_id": "1",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100024693.1.8",
          "form": "아하 그렇구나",
          "original_form": "아하 그렇구나",
          "speaker_id": "2",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100024693.1.9",
          "form": "하긴 여행지 가서 걸어보니까",
          "original_form": "하긴 여행지 가서 걸어보니까",
          "speaker_id": "2",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100024693.1.10",
          "form": "시간이 좀 들긴 하더라",
          "original_form": "시간이 좀 들긴 하더라",
          "speaker_id": "2",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100024693.1.11",
          "form": "하루가 짧은 느낌이었여",
          "original_form": "하루가 짧은 느낌이었여",
          "speaker_id": "2",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100024693.1.12",
          "form": "넌 국내여행 어디 가봤어?",
          "original_form": "넌 국내여행 어디 가봤어?",
          "speaker_id": "2",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100024693.1.13",
          "form": "길거리 구경하면서 걷는게 목적인거랑",
          "original_form": "길거리 구경하면서 걷는게 목적인거랑",
          "speaker_id": "1",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100024693.1.14",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100024693.1.15",
          "form": "순수하게   이동이 목적인건 느낌이 다르더라",
          "original_form": "순수하게   이동이 목적인건 느낌이 다르더라",
          "speaker_id": "1",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100024693.1.16",
          "form": "아 맞아맞아",
          "original_form": "아 맞아맞아",
          "speaker_id": "2",
          "time": "20210831 17:19"
        },
        {
          "id": "MDRW2100024693.1.17",
          "form": "국내는 최근에 경기도간게  마지막이야",
          "original_form": "국내는 최근에 경기도간게  마지막이야",
          "speaker_id": "1",
          "time": "20210831 17:19"
        },
        {
          "id": "MDRW2100024693.1.18",
          "form": "경기도 어디서 놀았니",
          "original_form": "경기도 어디서 놀았니",
          "speaker_id": "2",
          "time": "20210831 17:19"
        },
        {
          "id": "MDRW2100024693.1.19",
          "form": "수원 행궁갔는데",
          "original_form": "수원 행궁갔는데",
          "speaker_id": "1",
          "time": "20210831 17:20"
        },
        {
          "id": "MDRW2100024693.1.20",
          "form": "주변에 기념품샵아니면",
          "original_form": "주변에 기념품샵아니면",
          "speaker_id": "1",
          "time": "20210831 17:20"
        },
        {
          "id": "MDRW2100024693.1.21",
          "form": "카페더라",
          "original_form": "카페더라",
          "speaker_id": "1",
          "time": "20210831 17:20"
        },
        {
          "id": "MDRW2100024693.1.22",
          "form": "기념품 샀니?",
          "original_form": "기념품 샀니?",
          "speaker_id": "2",
          "time": "20210831 17:21"
        },
        {
          "id": "MDRW2100024693.1.23",
          "form": "안샀어 죄다 머리핀아니면 다꾸용 스티커더라",
          "original_form": "안샀어 죄다 머리핀아니면 다꾸용 스티커더라",
          "speaker_id": "1",
          "time": "20210831 17:21"
        },
        {
          "id": "MDRW2100024693.1.24",
          "form": "ㅋㅋㅋㅋㅋ쓸데없는 기념품이네...",
          "original_form": "ㅋㅋㅋㅋㅋ쓸데없는 기념품이네...",
          "speaker_id": "2",
          "time": "20210831 17:22"
        },
        {
          "id": "MDRW2100024693.1.25",
          "form": "카페도 어찌나 비싸던지",
          "original_form": "카페도 어찌나 비싸던지",
          "speaker_id": "1",
          "time": "20210831 17:22"
        },
        {
          "id": "MDRW2100024693.1.26",
          "form": "카페도 가봤어?",
          "original_form": "카페도 가봤어?",
          "speaker_id": "2",
          "time": "20210831 17:22"
        },
        {
          "id": "MDRW2100024693.1.27",
          "form": "ㄷ음료한잔에 7~9천원이야",
          "original_form": "ㄷ음료한잔에 7~9천원이야",
          "speaker_id": "1",
          "time": "20210831 17:22"
        }
      ]
    }
  ]
