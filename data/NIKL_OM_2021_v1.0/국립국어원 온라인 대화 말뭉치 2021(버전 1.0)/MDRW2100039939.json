      "utterance": [
        {
          "id": "MDRW2100039939.1.1",
          "form": "구내식당도 집밥만 못한지라 얼마 먹다가 헛살만 찌는 것 같더라고요",
          "original_form": "구내식당도 집밥만 못한지라 얼마 먹다가 헛살만 찌는 것 같더라고요",
          "speaker_id": "2",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100039939.1.2",
          "form": "3년 되었어요",
          "original_form": "3년 되었어요",
          "speaker_id": "2",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100039939.1.3",
          "form": "ㅎㅎ 회사 근처에 좋은 백반집 하나 있음 좋은데 성에 안차네요",
          "original_form": "ㅎㅎ 회사 근처에 좋은 백반집 하나 있음 좋은데 성에 안차네요",
          "speaker_id": "1",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100039939.1.4",
          "form": "첫 직장인가요",
          "original_form": "첫 직장인가요",
          "speaker_id": "1",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100039939.1.5",
          "form": "아니요 일한지는 12년차 되었어요",
          "original_form": "아니요 일한지는 12년차 되었어요",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100039939.1.6",
          "form": "아ㅎ 베테랑이시군요!",
          "original_form": "아ㅎ 베테랑이시군요!",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100039939.1.7",
          "form": "이직은 몇번 하셨어요?",
          "original_form": "이직은 몇번 하셨어요?",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100039939.1.8",
          "form": "이번이 두번째에요",
          "original_form": "이번이 두번째에요",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100039939.1.9",
          "form": "오늘도 불금이다 곧 추석이다 하며 보냈어요",
          "original_form": "오늘도 불금이다 곧 추석이다 하며 보냈어요",
          "speaker_id": "2",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100039939.1.10",
          "form": "ㅎㅎ  직장인 맘 다 같죠ㅎㅎ",
          "original_form": "ㅎㅎ  직장인 맘 다 같죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100039939.1.11",
          "form": "맞아요 받은 월급만큼만 일하자!",
          "original_form": "맞아요 받은 월급만큼만 일하자!",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100039939.1.12",
          "form": "다짐하면서 ㅎㅎ",
          "original_form": "다짐하면서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:27"
        },
        {
          "id": "MDRW2100039939.1.13",
          "form": "ㅎㅎ합법적 월급루팡이쥬ㅎㅎ",
          "original_form": "ㅎㅎ합법적 월급루팡이쥬ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100039939.1.14",
          "form": "네 ㅋㅋㅋ맞아요",
          "original_form": "네 ㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100039939.1.15",
          "form": "다음주부터는 야근도 생기는데",
          "original_form": "다음주부터는 야근도 생기는데",
          "speaker_id": "2",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100039939.1.16",
          "form": "쉬엄쉬엄해야겠어요",
          "original_form": "쉬엄쉬엄해야겠어요",
          "speaker_id": "2",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100039939.1.17",
          "form": "추석전 타이트하네요ㅜ",
          "original_form": "추석전 타이트하네요ㅜ",
          "speaker_id": "1",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100039939.1.18",
          "form": "추석 떡값은 나오나요",
          "original_form": "추석 떡값은 나오나요",
          "speaker_id": "1",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100039939.1.19",
          "form": "네 다행이 현찰로 나와요",
          "original_form": "네 다행이 현찰로 나와요",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100039939.1.20",
          "form": "그것도 기다려야죠 진짜 떡값정도지만 ㅋㅋ",
          "original_form": "그것도 기다려야죠 진짜 떡값정도지만 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100039939.1.21",
          "form": "그게 어딥니까ㅜ",
          "original_form": "그게 어딥니까ㅜ",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100039939.1.22",
          "form": "우린...안 줍니다ㅜ",
          "original_form": "우린...안 줍니다ㅜ",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100039939.1.23",
          "form": "설마 아무것도 안나오나요",
          "original_form": "설마 아무것도 안나오나요",
          "speaker_id": "2",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100039939.1.24",
          "form": "햄이라도 줘야죠",
          "original_form": "햄이라도 줘야죠",
          "speaker_id": "2",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100039939.1.25",
          "form": "나온적이 잘",
          "original_form": "나온적이 잘",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100039939.1.26",
          "form": "식용류라도...",
          "original_form": "식용류라도...",
          "speaker_id": "2",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100039939.1.27",
          "form": "아 선물은 오죠",
          "original_form": "아 선물은 오죠",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100039939.1.28",
          "form": "저도 딱 그정도에요 ㅎㅎ",
          "original_form": "저도 딱 그정도에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100039939.1.29",
          "form": "협찬받은 기업서...",
          "original_form": "협찬받은 기업서...",
          "speaker_id": "1",
          "time": "20210903 23:35"
        }
      ]
    }
