      "utterance": [
        {
          "id": "MDRW2100025581.1.1",
          "form": "최근에 샀던 물품있나요",
          "original_form": "최근에 샀던 물품있나요",
          "speaker_id": "1",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100025581.1.2",
          "form": "최근에 구입한 물품이 있나요?",
          "original_form": "최근에 구입한 물품이 있나요?",
          "speaker_id": "1",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100025581.1.3",
          "form": "음~~다욧식품이랑",
          "original_form": "음~~다욧식품이랑",
          "speaker_id": "2",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100025581.1.4",
          "form": "애들간식",
          "original_form": "애들간식",
          "speaker_id": "2",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100025581.1.5",
          "form": "오 디이어트~~좋죠",
          "original_form": "오 디이어트~~좋죠",
          "speaker_id": "1",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100025581.1.6",
          "form": "닭 살",
          "original_form": "닭 살",
          "speaker_id": "2",
          "time": "20210901 15:33"
        },
        {
          "id": "MDRW2100025581.1.7",
          "form": "그게 안적히네ㅜ",
          "original_form": "그게 안적히네ㅜ",
          "speaker_id": "2",
          "time": "20210901 15:33"
        },
        {
          "id": "MDRW2100025581.1.8",
          "form": "이런거저런거",
          "original_form": "이런거저런거",
          "speaker_id": "2",
          "time": "20210901 15:33"
        },
        {
          "id": "MDRW2100025581.1.9",
          "form": "저도 두부하고 토마토 샀어요ㅋㅋㅋ",
          "original_form": "저도 두부하고 토마토 샀어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:49"
        },
        {
          "id": "MDRW2100025581.1.10",
          "form": "300그램 두팩인데",
          "original_form": "300그램 두팩인데",
          "speaker_id": "1",
          "time": "20210901 15:49"
        },
        {
          "id": "MDRW2100025581.1.11",
          "form": "보통 한 끼에 110그램 정도 먹는거 같아서",
          "original_form": "보통 한 끼에 110그램 정도 먹는거 같아서",
          "speaker_id": "1",
          "time": "20210901 15:49"
        },
        {
          "id": "MDRW2100025581.1.12",
          "form": "나도 두부사야하는데~~",
          "original_form": "나도 두부사야하는데~~",
          "speaker_id": "2",
          "time": "20210901 15:49"
        },
        {
          "id": "MDRW2100025581.1.13",
          "form": "두부 3번 나눠 먹으려구요",
          "original_form": "두부 3번 나눠 먹으려구요",
          "speaker_id": "1",
          "time": "20210901 15:49"
        },
        {
          "id": "MDRW2100025581.1.14",
          "form": "샤인마토먹고싶다",
          "original_form": "샤인마토먹고싶다",
          "speaker_id": "2",
          "time": "20210901 15:49"
        },
        {
          "id": "MDRW2100025581.1.15",
          "form": "큰거?",
          "original_form": "큰거?",
          "speaker_id": "2",
          "time": "20210901 15:49"
        },
        {
          "id": "MDRW2100025581.1.16",
          "form": "손두부?",
          "original_form": "손두부?",
          "speaker_id": "2",
          "time": "20210901 15:50"
        },
        {
          "id": "MDRW2100025581.1.17",
          "form": "아녀 그냥 두부",
          "original_form": "아녀 그냥 두부",
          "speaker_id": "1",
          "time": "20210901 15:50"
        },
        {
          "id": "MDRW2100025581.1.18",
          "form": "샤인머스켓 사지",
          "original_form": "샤인머스켓 사지",
          "speaker_id": "2",
          "time": "20210901 15:50"
        },
        {
          "id": "MDRW2100025581.1.19",
          "form": "먹고싶은데",
          "original_form": "먹고싶은데",
          "speaker_id": "2",
          "time": "20210901 15:50"
        },
        {
          "id": "MDRW2100025581.1.20",
          "form": "다져서 먹으면 저는 순두부 아니어도 괜찮더라구요",
          "original_form": "다져서 먹으면 저는 순두부 아니어도 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210901 15:50"
        },
        {
          "id": "MDRW2100025581.1.21",
          "form": "샤인머스켓 샀는디",
          "original_form": "샤인머스켓 샀는디",
          "speaker_id": "1",
          "time": "20210901 15:50"
        },
        {
          "id": "MDRW2100025581.1.22",
          "form": "내일씻어둬라",
          "original_form": "내일씻어둬라",
          "speaker_id": "2",
          "time": "20210901 15:50"
        },
        {
          "id": "MDRW2100025581.1.23",
          "form": "전에 산건 할인 안하길래 1.5키로 29900원",
          "original_form": "전에 산건 할인 안하길래 1.5키로 29900원",
          "speaker_id": "1",
          "time": "20210901 15:50"
        },
        {
          "id": "MDRW2100025581.1.24",
          "form": "계란삶고",
          "original_form": "계란삶고",
          "speaker_id": "2",
          "time": "20210901 15:50"
        },
        {
          "id": "MDRW2100025581.1.25",
          "form": "계란있나?",
          "original_form": "계란있나?",
          "speaker_id": "2",
          "time": "20210901 15:50"
        },
        {
          "id": "MDRW2100025581.1.26",
          "form": "사는김에 깜빠리 토마토랑 파프리카 할인하길래 같이 샀쥬",
          "original_form": "사는김에 깜빠리 토마토랑 파프리카 할인하길래 같이 샀쥬",
          "speaker_id": "1",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100025581.1.27",
          "form": "계란은 지금 삶는 즁~",
          "original_form": "계란은 지금 삶는 즁~",
          "speaker_id": "1",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100025581.1.28",
          "form": "난하루한개만 무면되겠더라",
          "original_form": "난하루한개만 무면되겠더라",
          "speaker_id": "2",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100025581.1.29",
          "form": "4시10분쯤에 불 끌거에요",
          "original_form": "4시10분쯤에 불 끌거에요",
          "speaker_id": "1",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100025581.1.30",
          "form": "파프리카 스틱해두라",
          "original_form": "파프리카 스틱해두라",
          "speaker_id": "2",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100025581.1.31",
          "form": "오이하고",
          "original_form": "오이하고",
          "speaker_id": "2",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100025581.1.32",
          "form": "아니ㅋㅋㅋㅋ누가보면 저 주방이모인뎁쇼ㅋㅋㅋㅋ",
          "original_form": "아니ㅋㅋㅋㅋ누가보면 저 주방이모인뎁쇼ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100025581.1.33",
          "form": "계란 불 끄고 같이 해두려구요",
          "original_form": "계란 불 끄고 같이 해두려구요",
          "speaker_id": "1",
          "time": "20210901 15:52"
        },
        {
          "id": "MDRW2100025581.1.34",
          "form": "이모 계란삶아두고",
          "original_form": "이모 계란삶아두고",
          "speaker_id": "2",
          "time": "20210901 15:52"
        },
        {
          "id": "MDRW2100025581.1.35",
          "form": "오이스틱파프리카스틱",
          "original_form": "오이스틱파프리카스틱",
          "speaker_id": "2",
          "time": "20210901 15:52"
        },
        {
          "id": "MDRW2100025581.1.36",
          "form": "파프리카 할인하는거 산거라 얼른 먹어야해요",
          "original_form": "파프리카 할인하는거 산거라 얼른 먹어야해요",
          "speaker_id": "1",
          "time": "20210901 15:52"
        },
        {
          "id": "MDRW2100025581.1.37",
          "form": "안좋은거가?",
          "original_form": "안좋은거가?",
          "speaker_id": "2",
          "time": "20210901 15:52"
        },
        {
          "id": "MDRW2100025581.1.38",
          "form": "겉은 말짱해보이는데 살짝 말랑해지긴 했어요",
          "original_form": "겉은 말짱해보이는데 살짝 말랑해지긴 했어요",
          "speaker_id": "1",
          "time": "20210901 15:52"
        },
        {
          "id": "MDRW2100025581.1.39",
          "form": "이번주 내로 다 먹으면 되겠쥬",
          "original_form": "이번주 내로 다 먹으면 되겠쥬",
          "speaker_id": "1",
          "time": "20210901 15:53"
        },
        {
          "id": "MDRW2100025581.1.40",
          "form": "몇개샀는데?",
          "original_form": "몇개샀는데?",
          "speaker_id": "2",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100025581.1.41",
          "form": "파프리카는 3개요",
          "original_form": "파프리카는 3개요",
          "speaker_id": "1",
          "time": "20210901 15:55"
        },
        {
          "id": "MDRW2100025581.1.42",
          "form": "3개?3봉아니고",
          "original_form": "3개?3봉아니고",
          "speaker_id": "2",
          "time": "20210901 15:55"
        },
        {
          "id": "MDRW2100025581.1.43",
          "form": "3개면 다묵겠네",
          "original_form": "3개면 다묵겠네",
          "speaker_id": "2",
          "time": "20210901 15:55"
        },
        {
          "id": "MDRW2100025581.1.44",
          "form": "맞죠 금방 먹을거 같아요",
          "original_form": "맞죠 금방 먹을거 같아요",
          "speaker_id": "1",
          "time": "20210901 15:56"
        }
