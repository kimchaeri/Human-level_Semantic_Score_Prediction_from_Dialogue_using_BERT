      "utterance": [
        {
          "id": "MDRW2100033084.1.1",
          "form": "식음료... 중에서도 음료 하니까 생각나는 거 있음",
          "original_form": "식음료... 중에서도 음료 하니까 생각나는 거 있음",
          "speaker_id": "1",
          "time": "20210906 15:47"
        },
        {
          "id": "MDRW2100033084.1.2",
          "form": "요즘 액상과당 줄인다고 다들 난리인 것 같음",
          "original_form": "요즘 액상과당 줄인다고 다들 난리인 것 같음",
          "speaker_id": "1",
          "time": "20210906 15:48"
        },
        {
          "id": "MDRW2100033084.1.3",
          "form": "액상과당 안 좋긴 하지",
          "original_form": "액상과당 안 좋긴 하지",
          "speaker_id": "2",
          "time": "20210906 15:48"
        },
        {
          "id": "MDRW2100033084.1.4",
          "form": "그래서 제품 여러 종류로 나오지 않나?",
          "original_form": "그래서 제품 여러 종류로 나오지 않나?",
          "speaker_id": "2",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100033084.1.5",
          "form": "다이어트 때문에 강제적으로 못 먹게 된 나의 액상과당",
          "original_form": "다이어트 때문에 강제적으로 못 먹게 된 나의 액상과당",
          "speaker_id": "1",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100033084.1.6",
          "form": "맞아 당 없는 아이스티... 뭐 그런 거",
          "original_form": "맞아 당 없는 아이스티... 뭐 그런 거",
          "speaker_id": "1",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100033084.1.7",
          "form": "당 대체 감미료들....",
          "original_form": "당 대체 감미료들....",
          "speaker_id": "1",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100033084.1.8",
          "form": "당 없는 아이스티 먹어봤어??",
          "original_form": "당 없는 아이스티 먹어봤어??",
          "speaker_id": "2",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100033084.1.9",
          "form": "아니?? 근데 맛있어 보임",
          "original_form": "아니?? 근데 맛있어 보임",
          "speaker_id": "1",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100033084.1.10",
          "form": "그리고 요즘 인기 있는 것... 크로플",
          "original_form": "그리고 요즘 인기 있는 것... 크로플",
          "speaker_id": "1",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100033084.1.11",
          "form": "나 크로플 사랑하잖아",
          "original_form": "나 크로플 사랑하잖아",
          "speaker_id": "2",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100033084.1.12",
          "form": "아이스크림 크로플...",
          "original_form": "아이스크림 크로플...",
          "speaker_id": "2",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100033084.1.13",
          "form": "저번에 사 먹어 봤는데... 눈물 남",
          "original_form": "저번에 사 먹어 봤는데... 눈물 남",
          "speaker_id": "1",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100033084.1.14",
          "form": "요즘 크로플에 꽂힌 상태",
          "original_form": "요즘 크로플에 꽂힌 상태",
          "speaker_id": "1",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100033084.1.15",
          "form": "ㄹㅇ 근데 들고 먹기엔 불편한듯",
          "original_form": "ㄹㅇ 근데 들고 먹기엔 불편한듯",
          "speaker_id": "2",
          "time": "20210906 15:51"
        },
        {
          "id": "MDRW2100033084.1.16",
          "form": "잘라서 먹게 칼도 줬으면",
          "original_form": "잘라서 먹게 칼도 줬으면",
          "speaker_id": "2",
          "time": "20210906 15:51"
        },
        {
          "id": "MDRW2100033084.1.17",
          "form": "우리 학교 앞 카페는 칼 주던디",
          "original_form": "우리 학교 앞 카페는 칼 주던디",
          "speaker_id": "1",
          "time": "20210906 15:51"
        },
        {
          "id": "MDRW2100033084.1.18",
          "form": "우리는 안 주더라 프랜차이즈라 그런가",
          "original_form": "우리는 안 주더라 프랜차이즈라 그런가",
          "speaker_id": "2",
          "time": "20210906 15:51"
        },
        {
          "id": "MDRW2100033084.1.19",
          "form": "프랜차이즈 못됐네",
          "original_form": "프랜차이즈 못됐네",
          "speaker_id": "1",
          "time": "20210906 15:51"
        },
        {
          "id": "MDRW2100033084.1.20",
          "form": "그리고 다들 에어프라이어에 뭔가를 돌려 먹음",
          "original_form": "그리고 다들 에어프라이어에 뭔가를 돌려 먹음",
          "speaker_id": "1",
          "time": "20210906 15:52"
        },
        {
          "id": "MDRW2100033084.1.21",
          "form": "일단 넣고 보던데",
          "original_form": "일단 넣고 보던데",
          "speaker_id": "2",
          "time": "20210906 15:52"
        },
        {
          "id": "MDRW2100033084.1.22",
          "form": "와플 기계도 인기있었지",
          "original_form": "와플 기계도 인기있었지",
          "speaker_id": "2",
          "time": "20210906 15:52"
        },
        {
          "id": "MDRW2100033084.1.23",
          "form": "에어프라이어용 치킨텐더... 피카츄 돈가스... 버팔로 윙....",
          "original_form": "에어프라이어용 치킨텐더... 피카츄 돈가스... 버팔로 윙....",
          "speaker_id": "1",
          "time": "20210906 15:52"
        },
        {
          "id": "MDRW2100033084.1.24",
          "form": "와플 기계에 볶음밥 넣잖아",
          "original_form": "와플 기계에 볶음밥 넣잖아",
          "speaker_id": "1",
          "time": "20210906 15:52"
        },
        {
          "id": "MDRW2100033084.1.25",
          "form": "삼겹살 넣고 ㅋ",
          "original_form": "삼겹살 넣고 ㅋ",
          "speaker_id": "1",
          "time": "20210906 15:52"
        },
        {
          "id": "MDRW2100033084.1.26",
          "form": "ㅋ 와플인데 이제 한국식 와플",
          "original_form": "ㅋ 와플인데 이제 한국식 와플",
