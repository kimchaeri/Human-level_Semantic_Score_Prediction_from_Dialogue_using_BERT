      "utterance": [
        {
          "id": "MDRW2100003806.1.1",
          "form": "마지막 연애가 언제신가요?",
          "original_form": "마지막 연애가 언제신가요?",
          "speaker_id": "1",
          "time": "20210821 12:18"
        },
        {
          "id": "MDRW2100003806.1.2",
          "form": "지금 연애중이에요ㅎㅎ",
          "original_form": "지금 연애중이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 12:19"
        },
        {
          "id": "MDRW2100003806.1.3",
          "form": "2년차입니다",
          "original_form": "2년차입니다",
          "speaker_id": "2",
          "time": "20210821 12:19"
        },
        {
          "id": "MDRW2100003806.1.4",
          "form": "엇 저도 그런데 년수도 똑같네요!",
          "original_form": "엇 저도 그런데 년수도 똑같네요!",
          "speaker_id": "1",
          "time": "20210821 12:19"
        },
        {
          "id": "MDRW2100003806.1.5",
          "form": "헐대박ㅋㅋㅋ",
          "original_form": "헐대박ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 12:20"
        },
        {
          "id": "MDRW2100003806.1.6",
          "form": "잘맞으세요?",
          "original_form": "잘맞으세요?",
          "speaker_id": "2",
          "time": "20210821 12:20"
        },
        {
          "id": "MDRW2100003806.1.7",
          "form": "전 남친이랑 진짜 안맞는데 맞춰가며 지내거든요",
          "original_form": "전 남친이랑 진짜 안맞는데 맞춰가며 지내거든요",
          "speaker_id": "2",
          "time": "20210821 12:20"
        },
        {
          "id": "MDRW2100003806.1.8",
          "form": "저는 어제 여행 다녀왔는데",
          "original_form": "저는 어제 여행 다녀왔는데",
          "speaker_id": "1",
          "time": "20210821 12:20"
        },
        {
          "id": "MDRW2100003806.1.9",
          "form": "안맞아도 맞추는 거죠 뭐 ㅠㅠㅠ 2년이면 다들 그런 것 같아요 흑",
          "original_form": "안맞아도 맞추는 거죠 뭐 ㅠㅠㅠ 2년이면 다들 그런 것 같아요 흑",
          "speaker_id": "1",
          "time": "20210821 12:20"
        },
        {
          "id": "MDRW2100003806.1.10",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210821 12:21"
        },
        {
          "id": "MDRW2100003806.1.11",
          "form": "헐 저도",
          "original_form": "헐 저도",
          "speaker_id": "2",
          "time": "20210821 12:21"
        },
        {
          "id": "MDRW2100003806.1.12",
          "form": "저 제주도 갔다왔어요",
          "original_form": "저 제주도 갔다왔어요",
          "speaker_id": "2",
          "time": "20210821 12:21"
        },
        {
          "id": "MDRW2100003806.1.13",
          "form": "뭐야ㅋㅋ소름돋아",
          "original_form": "뭐야ㅋㅋ소름돋아",
          "speaker_id": "2",
          "time": "20210821 12:21"
        },
        {
          "id": "MDRW2100003806.1.14",
          "form": "어제왔어요",
          "original_form": "어제왔어요",
          "speaker_id": "2",
          "time": "20210821 12:21"
        },
        {
          "id": "MDRW2100003806.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ앜ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ앜ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 12:21"
        },
        {
          "id": "MDRW2100003806.1.16",
          "form": "해수욕장 폐쇄던데 괜찮았나요?ㅠㅠ",
          "original_form": "해수욕장 폐쇄던데 괜찮았나요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 12:21"
        },
        {
          "id": "MDRW2100003806.1.17",
          "form": "사람 겁나 많아요^^...",
          "original_form": "사람 겁나 많아요^^...",
          "speaker_id": "2",
          "time": "20210821 12:22"
        },
        {
          "id": "MDRW2100003806.1.18",
          "form": "남친이 집돌이라서",
          "original_form": "남친이 집돌이라서",
          "speaker_id": "2",
          "time": "20210821 12:22"
        },
        {
          "id": "MDRW2100003806.1.19",
          "form": "거의 드라이브만했어요",
          "original_form": "거의 드라이브만했어요",
          "speaker_id": "2",
          "time": "20210821 12:22"
        },
        {
          "id": "MDRW2100003806.1.20",
          "form": "저랑 진짜 안맞음",
          "original_form": "저랑 진짜 안맞음",
          "speaker_id": "2",
          "time": "20210821 12:22"
        },
        {
          "id": "MDRW2100003806.1.21",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 12:22"
        },
        {
          "id": "MDRW2100003806.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 12:22"
        },
        {
          "id": "MDRW2100003806.1.23",
          "form": "그럴 수 있어요",
          "original_form": "그럴 수 있어요",
          "speaker_id": "1",
          "time": "20210821 12:22"
        },
        {
          "id": "MDRW2100003806.1.24",
          "form": "제 남친도집돌인데 여행 한정해서는 좀 돌아다녀요",
          "original_form": "제 남친도집돌인데 여행 한정해서는 좀 돌아다녀요",
          "speaker_id": "1",
          "time": "20210821 12:22"
        },
        {
          "id": "MDRW2100003806.1.25",
          "form": "근데 저희는 뚜벅이라 드라이브 부럽네요ㅠㅠ",
          "original_form": "근데 저희는 뚜벅이라 드라이브 부럽네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 12:22"
        },
        {
          "id": "MDRW2100003806.1.26",
          "form": "아 남친분도 아직 면허 없으세요?",
          "original_form": "아 남친분도 아직 면허 없으세요?",
          "speaker_id": "2",
          "time": "20210821 12:23"
        },
        {
          "id": "MDRW2100003806.1.27",
          "form": "남자친구는 이제 직장인인데 아직 면허도 없고 차도없어요 ㅎㅎㅎ",
          "original_form": "남자친구는 이제 직장인인데 아직 면허도 없고 차도없어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 12:24"
        },
        {
          "id": "MDRW2100003806.1.28",
          "form": "이제 따려는 것 같은데 잘 모르겠어요 ㅎㅎㅎ",
          "original_form": "이제 따려는 것 같은데 잘 모르겠어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 12:24"
        },
        {
          "id": "MDRW2100003806.1.29",
          "form": "저는 아직 대학생이에요!",
          "original_form": "저는 아직 대학생이에요!",
          "speaker_id": "1",
          "time": "20210821 12:24"
        },
        {
          "id": "MDRW2100003806.1.30",
          "form": "아 아직 사회초년생 커플이시구나",
          "original_form": "아 아직 사회초년생 커플이시구나",
          "speaker_id": "2",
          "time": "20210821 12:25"
        },
        {
          "id": "MDRW2100003806.1.31",
          "form": "남친분 차 꼭 사라고하세요",
          "original_form": "남친분 차 꼭 사라고하세요",
          "speaker_id": "2",
          "time": "20210821 12:25"
        },
        {
          "id": "MDRW2100003806.1.32",
          "form": "연애하는데 차있으면 차원이 달라요",
          "original_form": "연애하는데 차있으면 차원이 달라요",
          "speaker_id": "2",
          "time": "20210821 12:25"
        },
        {
          "id": "MDRW2100003806.1.33",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210821 12:25"
        },
        {
          "id": "MDRW2100003806.1.34",
          "form": "마음 같아선 이거 넘 보여주고 싶네요 ㅎㅎㅎ",
          "original_form": "마음 같아선 이거 넘 보여주고 싶네요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 12:26"
        },
        {
          "id": "MDRW2100003806.1.35",
          "form": "근데 또 경차는 싫다고 해서 아마 좀 걸릴 것 같아요 ㅠㅠ",
          "original_form": "근데 또 경차는 싫다고 해서 아마 좀 걸릴 것 같아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 12:26"
        }
      ]
