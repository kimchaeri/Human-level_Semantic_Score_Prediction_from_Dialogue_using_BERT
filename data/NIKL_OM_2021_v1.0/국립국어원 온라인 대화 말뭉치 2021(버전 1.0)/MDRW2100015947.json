      "utterance": [
        {
          "id": "MDRW2100015947.1.1",
          "form": "너는 알바 많이 해봤어?",
          "original_form": "너는 알바 많이 해봤어?",
          "speaker_id": "2",
          "time": "20210826 03:30"
        },
        {
          "id": "MDRW2100015947.1.2",
          "form": "나 엄청 다양하게 해봤지",
          "original_form": "나 엄청 다양하게 해봤지",
          "speaker_id": "1",
          "time": "20210826 03:30"
        },
        {
          "id": "MDRW2100015947.1.3",
          "form": "어떤거해봤는데?",
          "original_form": "어떤거해봤는데?",
          "speaker_id": "2",
          "time": "20210826 03:31"
        },
        {
          "id": "MDRW2100015947.1.4",
          "form": "알다시피 장기로는 태권도랑 물리치료 했었지",
          "original_form": "알다시피 장기로는 태권도랑 물리치료 했었지",
          "speaker_id": "1",
          "time": "20210826 03:31"
        },
        {
          "id": "MDRW2100015947.1.5",
          "form": "그거는 알고 있지",
          "original_form": "그거는 알고 있지",
          "speaker_id": "2",
          "time": "20210826 03:31"
        },
        {
          "id": "MDRW2100015947.1.6",
          "form": "단기가 궁금한거야?",
          "original_form": "단기가 궁금한거야?",
          "speaker_id": "1",
          "time": "20210826 03:31"
        },
        {
          "id": "MDRW2100015947.1.7",
          "form": "단기를 많이 했다는거잖아",
          "original_form": "단기를 많이 했다는거잖아",
          "speaker_id": "2",
          "time": "20210826 03:31"
        },
        {
          "id": "MDRW2100015947.1.8",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "1",
          "time": "20210826 03:31"
        },
        {
          "id": "MDRW2100015947.1.9",
          "form": "단기로는 뭐했는데",
          "original_form": "단기로는 뭐했는데",
          "speaker_id": "2",
          "time": "20210826 03:31"
        },
        {
          "id": "MDRW2100015947.1.10",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210826 03:31"
        },
        {
          "id": "MDRW2100015947.1.11",
          "form": "부스운영이나 마라톤 스탭, 행사 스탭, 식기세척, 웨딩홀 등등",
          "original_form": "부스운영이나 마라톤 스탭, 행사 스탭, 식기세척, 웨딩홀 등등",
          "speaker_id": "1",
          "time": "20210826 03:32"
        },
        {
          "id": "MDRW2100015947.1.12",
          "form": "많이도 했네",
          "original_form": "많이도 했네",
          "speaker_id": "2",
          "time": "20210826 03:32"
        },
        {
          "id": "MDRW2100015947.1.13",
          "form": "지금 말한것보다 훨씬 많이 했지",
          "original_form": "지금 말한것보다 훨씬 많이 했지",
          "speaker_id": "1",
          "time": "20210826 03:32"
        },
        {
          "id": "MDRW2100015947.1.14",
          "form": "가장 기억에 남는 알바는 뭐야?",
          "original_form": "가장 기억에 남는 알바는 뭐야?",
          "speaker_id": "2",
          "time": "20210826 03:32"
        },
        {
          "id": "MDRW2100015947.1.15",
          "form": "아무래도 제일 오래한 태권도",
          "original_form": "아무래도 제일 오래한 태권도",
          "speaker_id": "1",
          "time": "20210826 03:32"
        },
        {
          "id": "MDRW2100015947.1.16",
          "form": "그럼 제일 재밌었던거는?",
          "original_form": "그럼 제일 재밌었던거는?",
          "speaker_id": "2",
          "time": "20210826 03:33"
        },
        {
          "id": "MDRW2100015947.1.17",
          "form": "방청객이지",
          "original_form": "방청객이지",
          "speaker_id": "1",
          "time": "20210826 03:33"
        },
        {
          "id": "MDRW2100015947.1.18",
          "form": "방청객은 뭐하는거야?",
          "original_form": "방청객은 뭐하는거야?",
          "speaker_id": "2",
          "time": "20210826 03:33"
        },
        {
          "id": "MDRW2100015947.1.19",
          "form": "그냥 앉아서 재밌는거 보면서 박수치면 돼",
          "original_form": "그냥 앉아서 재밌는거 보면서 박수치면 돼",
          "speaker_id": "1",
          "time": "20210826 03:33"
        },
        {
          "id": "MDRW2100015947.1.20",
          "form": "돈은 얼마나 받아?",
          "original_form": "돈은 얼마나 받아?",
          "speaker_id": "2",
          "time": "20210826 03:33"
        },
        {
          "id": "MDRW2100015947.1.21",
          "form": "15000원이었나",
          "original_form": "15000원이었나",
          "speaker_id": "1",
          "time": "20210826 03:33"
        },
        {
          "id": "MDRW2100015947.1.22",
          "form": "많이주지는 않네",
          "original_form": "많이주지는 않네",
          "speaker_id": "2",
          "time": "20210826 03:33"
        },
        {
          "id": "MDRW2100015947.1.23",
          "form": "그렇지 아무래도",
          "original_form": "그렇지 아무래도",
          "speaker_id": "1",
          "time": "20210826 03:33"
        },
        {
          "id": "MDRW2100015947.1.24",
          "form": "그래도 재밌는거 보면서 돈버니까 좋았겠네",
          "original_form": "그래도 재밌는거 보면서 돈버니까 좋았겠네",
          "speaker_id": "2",
          "time": "20210826 03:34"
        },
        {
          "id": "MDRW2100015947.1.25",
          "form": "맞아 완전 꿀이었지",
          "original_form": "맞아 완전 꿀이었지",
          "speaker_id": "1",
          "time": "20210826 03:34"
        }
      ]
    }
