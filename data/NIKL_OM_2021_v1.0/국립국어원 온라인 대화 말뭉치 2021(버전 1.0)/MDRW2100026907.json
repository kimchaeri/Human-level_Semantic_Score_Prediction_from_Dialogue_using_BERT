      "utterance": [
        {
          "id": "MDRW2100026907.1.1",
          "form": "안녕하세요 밤이라 야식이 땡기네요",
          "original_form": "안녕하세요 밤이라 야식이 땡기네요",
          "speaker_id": "2",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100026907.1.2",
          "form": "그러게요 저도 야식 먹고싶어요ㅠ",
          "original_form": "그러게요 저도 야식 먹고싶어요ㅠ",
          "speaker_id": "1",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100026907.1.3",
          "form": "좋아하는 음식 있으세요??",
          "original_form": "좋아하는 음식 있으세요??",
          "speaker_id": "2",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100026907.1.4",
          "form": "저 떡볶이 너무 좋아해요ㅠㅠ 떡볶이랑 순대!!!",
          "original_form": "저 떡볶이 너무 좋아해요ㅠㅠ 떡볶이랑 순대!!!",
          "speaker_id": "1",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100026907.1.5",
          "form": "헉 저도 분식 좋아해요ㅠㅠ 거의 사랑하는 수준ㅎㅎ",
          "original_form": "헉 저도 분식 좋아해요ㅠㅠ 거의 사랑하는 수준ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 01:10"
        },
        {
          "id": "MDRW2100026907.1.6",
          "form": "요즘엔 귀찮아서 배달로 분식 많이 시켜먹는거 같아용",
          "original_form": "요즘엔 귀찮아서 배달로 분식 많이 시켜먹는거 같아용",
          "speaker_id": "1",
          "time": "20210902 01:11"
        },
        {
          "id": "MDRW2100026907.1.7",
          "form": "네 그러게요",
          "original_form": "네 그러게요",
          "speaker_id": "2",
          "time": "20210902 01:11"
        },
        {
          "id": "MDRW2100026907.1.8",
          "form": "배달음식이 아무래도 간편하니까요",
          "original_form": "배달음식이 아무래도 간편하니까요",
          "speaker_id": "2",
          "time": "20210902 01:11"
        },
        {
          "id": "MDRW2100026907.1.9",
          "form": "배달음식하면 짜장면도 맛있는데ㅎㅎ",
          "original_form": "배달음식하면 짜장면도 맛있는데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 01:12"
        },
        {
          "id": "MDRW2100026907.1.10",
          "form": "그쵸 중국집도 맛있죠ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그쵸 중국집도 맛있죠ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:12"
        },
        {
          "id": "MDRW2100026907.1.11",
          "form": "중화요리에는 청량한 음료가 잘 어울리는거 같아요",
          "original_form": "중화요리에는 청량한 음료가 잘 어울리는거 같아요",
          "speaker_id": "2",
          "time": "20210902 01:13"
        },
        {
          "id": "MDRW2100026907.1.12",
          "form": "맞아요 청량하거나 아예 도수가 센",
          "original_form": "맞아요 청량하거나 아예 도수가 센",
          "speaker_id": "1",
          "time": "20210902 01:13"
        },
        {
          "id": "MDRW2100026907.1.13",
          "form": "그래서 중국 술들이 도수가 세거나 깔끔한가봅니다",
          "original_form": "그래서 중국 술들이 도수가 세거나 깔끔한가봅니다",
          "speaker_id": "2",
          "time": "20210902 01:13"
        },
        {
          "id": "MDRW2100026907.1.14",
          "form": "그러네요 정말 ㅎ",
          "original_form": "그러네요 정말 ㅎ",
          "speaker_id": "1",
          "time": "20210902 01:13"
        },
        {
          "id": "MDRW2100026907.1.15",
          "form": "내일 점심으로는 정말 분식이나 중화요리 먹어야겠네요ㅋㅋ",
          "original_form": "내일 점심으로는 정말 분식이나 중화요리 먹어야겠네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 01:14"
        },
        {
          "id": "MDRW2100026907.1.16",
          "form": "좋은 결정이십니다ㅋㅋ 전 다이어트만 아니었어도...",
          "original_form": "좋은 결정이십니다ㅋㅋ 전 다이어트만 아니었어도...",
          "speaker_id": "2",
          "time": "20210902 01:14"
        },
        {
          "id": "MDRW2100026907.1.17",
          "form": "다이어트 하시는구나ㅠㅠ",
          "original_form": "다이어트 하시는구나ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 01:14"
        }
      ]
