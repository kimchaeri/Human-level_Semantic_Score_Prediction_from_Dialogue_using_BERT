      "utterance": [
        {
          "id": "MDRW2100025836.1.1",
          "form": "어제 127 영상 봤어??",
          "original_form": "어제 127 영상 봤어??",
          "speaker_id": "1",
          "time": "20210902 12:09"
        },
        {
          "id": "MDRW2100025836.1.2",
          "form": "그 유튜브에 뜬건 아직 다 안봤는데 완전 시트콤재질이던에",
          "original_form": "그 유튜브에 뜬건 아직 다 안봤는데 완전 시트콤재질이던에",
          "speaker_id": "1",
          "time": "20210902 12:09"
        },
        {
          "id": "MDRW2100025836.1.3",
          "form": "나 그거 유튜브에 뜬 건 아직 못보고",
          "original_form": "나 그거 유튜브에 뜬 건 아직 못보고",
          "speaker_id": "2",
          "time": "20210902 12:13"
        },
        {
          "id": "MDRW2100025836.1.4",
          "form": "트위터에 짤막하게 개인별로 뜬 건 봤어",
          "original_form": "트위터에 짤막하게 개인별로 뜬 건 봤어",
          "speaker_id": "2",
          "time": "20210902 12:13"
        },
        {
          "id": "MDRW2100025836.1.5",
          "form": "나 지금 보는중인데",
          "original_form": "나 지금 보는중인데",
          "speaker_id": "1",
          "time": "20210902 12:13"
        },
        {
          "id": "MDRW2100025836.1.6",
          "form": "그냥... 거의 미국 하이틴 드라마야",
          "original_form": "그냥... 거의 미국 하이틴 드라마야",
          "speaker_id": "1",
          "time": "20210902 12:13"
        },
        {
          "id": "MDRW2100025836.1.7",
          "form": "하... 그럴것같아",
          "original_form": "하... 그럴것같아",
          "speaker_id": "2",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100025836.1.8",
          "form": "썸네일부터 미쳤어",
          "original_form": "썸네일부터 미쳤어",
          "speaker_id": "2",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100025836.1.9",
          "form": "쟈니 마크 재현... 영어로 말해",
          "original_form": "쟈니 마크 재현... 영어로 말해",
          "speaker_id": "1",
          "time": "20210902 12:27"
        },
        {
          "id": "MDRW2100025836.1.10",
          "form": "거의 영어듣기 평가 하는중",
          "original_form": "거의 영어듣기 평가 하는중",
          "speaker_id": "1",
          "time": "20210902 12:28"
        },
        {
          "id": "MDRW2100025836.1.11",
          "form": "아 좀 이따가 봐야지",
          "original_form": "아 좀 이따가 봐야지",
          "speaker_id": "2",
          "time": "20210902 14:32"
        },
        {
          "id": "MDRW2100025836.1.12",
          "form": "지금봐 제발",
          "original_form": "지금봐 제발",
          "speaker_id": "1",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100025836.1.13",
          "form": "해찬이가 너무 귀여워",
          "original_form": "해찬이가 너무 귀여워",
          "speaker_id": "1",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100025836.1.14",
          "form": "아님 27:57초부터 한 20초만 봐",
          "original_form": "아님 27:57초부터 한 20초만 봐",
          "speaker_id": "1",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100025836.1.15",
          "form": "네가 좋아할듯",
          "original_form": "네가 좋아할듯",
          "speaker_id": "1",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100025836.1.16",
          "form": "하 아냐 풀로 볼거야 사실 짤 조금조금은 살짝씩 탐라에 떠서 봤어",
          "original_form": "하 아냐 풀로 볼거야 사실 짤 조금조금은 살짝씩 탐라에 떠서 봤어",
          "speaker_id": "2",
          "time": "20210902 14:37"
        },
        {
          "id": "MDRW2100025836.1.17",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "1",
          "time": "20210902 14:38"
        },
        {
          "id": "MDRW2100025836.1.18",
          "form": "한글 자막키고 보는거 추천할게",
          "original_form": "한글 자막키고 보는거 추천할게",
          "speaker_id": "1",
          "time": "20210902 14:38"
        },
        {
          "id": "MDRW2100025836.1.19",
          "form": "아 한글 자막 있어?",
          "original_form": "아 한글 자막 있어?",
          "speaker_id": "2",
          "time": "20210902 16:19"
        },
        {
          "id": "MDRW2100025836.1.20",
          "form": "있을걸",
          "original_form": "있을걸",
          "speaker_id": "1",
          "time": "20210902 16:21"
        },
        {
          "id": "MDRW2100025836.1.21",
          "form": "있다",
          "original_form": "있다",
          "speaker_id": "1",
          "time": "20210902 16:21"
        },
        {
          "id": "MDRW2100025836.1.22",
          "form": "나 다봤어",
          "original_form": "나 다봤어",
          "speaker_id": "2",
          "time": "20210902 18:17"
        },
        {
          "id": "MDRW2100025836.1.23",
          "form": "대박이야",
          "original_form": "대박이야",
          "speaker_id": "2",
          "time": "20210902 18:17"
        },
        {
          "id": "MDRW2100025836.1.24",
          "form": "황홀해",
          "original_form": "황홀해",
          "speaker_id": "2",
          "time": "20210902 18:17"
        },
        {
          "id": "MDRW2100025836.1.25",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210902 18:18"
        },
        {
          "id": "MDRW2100025836.1.26",
          "form": "완전 미국 시트콤같아",
          "original_form": "완전 미국 시트콤같아",
