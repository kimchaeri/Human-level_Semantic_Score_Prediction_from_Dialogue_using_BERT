      "utterance": [
        {
          "id": "MDRW2100008126.1.1",
          "form": "혹시 키우시는 동물 있으세요?",
          "original_form": "혹시 키우시는 동물 있으세요?",
          "speaker_id": "1",
          "time": "20210822 13:26"
        },
        {
          "id": "MDRW2100008126.1.2",
          "form": "동물 지금은 안키워요..키우시나요",
          "original_form": "동물 지금은 안키워요..키우시나요",
          "speaker_id": "2",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100008126.1.3",
          "form": "옛날에 토끼는 있었네요",
          "original_form": "옛날에 토끼는 있었네요",
          "speaker_id": "2",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100008126.1.4",
          "form": "저희집은 강아지 키워요",
          "original_form": "저희집은 강아지 키워요",
          "speaker_id": "1",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100008126.1.5",
          "form": "강쥐 부러워요.. 어떤 친구인가요",
          "original_form": "강쥐 부러워요.. 어떤 친구인가요",
          "speaker_id": "2",
          "time": "20210822 13:38"
        },
        {
          "id": "MDRW2100008126.1.6",
          "form": "되게 귀여워요",
          "original_form": "되게 귀여워요",
          "speaker_id": "1",
          "time": "20210822 13:55"
        },
        {
          "id": "MDRW2100008126.1.7",
          "form": "시츄 좋아하세요?",
          "original_form": "시츄 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 13:56"
        },
        {
          "id": "MDRW2100008126.1.8",
          "form": "시츄 진짜 귀엽죠 제일 귀여운듯요",
          "original_form": "시츄 진짜 귀엽죠 제일 귀여운듯요",
          "speaker_id": "2",
          "time": "20210822 13:59"
        },
        {
          "id": "MDRW2100008126.1.9",
          "form": "옛날에 닌텐도로 개키우는거 했는데",
          "original_form": "옛날에 닌텐도로 개키우는거 했는데",
          "speaker_id": "2",
          "time": "20210822 14:00"
        },
        {
          "id": "MDRW2100008126.1.10",
          "form": "시츄키웠던 기억이 나네요 사랑스럽",
          "original_form": "시츄키웠던 기억이 나네요 사랑스럽",
          "speaker_id": "2",
          "time": "20210822 14:00"
        },
        {
          "id": "MDRW2100008126.1.11",
          "form": "혹시 키우고 싶은 동물 있으세요?",
          "original_form": "혹시 키우고 싶은 동물 있으세요?",
          "speaker_id": "1",
          "time": "20210822 14:07"
        },
        {
          "id": "MDRW2100008126.1.12",
          "form": "고양이나 강쥐 키우고 싶죠",
          "original_form": "고양이나 강쥐 키우고 싶죠",
          "speaker_id": "2",
          "time": "20210822 14:12"
        },
        {
          "id": "MDRW2100008126.1.13",
          "form": "강쥐 키우면 어떤가요",
          "original_form": "강쥐 키우면 어떤가요",
          "speaker_id": "2",
          "time": "20210822 14:12"
        },
        {
          "id": "MDRW2100008126.1.14",
          "form": "강아지가 털은 좀 날리는데",
          "original_form": "강아지가 털은 좀 날리는데",
          "speaker_id": "1",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008126.1.15",
          "form": "그래도 귀여워서 괜찮은거같아요",
          "original_form": "그래도 귀여워서 괜찮은거같아요",
          "speaker_id": "1",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008126.1.16",
          "form": "고양이나 강아지중에 좋아하시는 종 있으세요?",
          "original_form": "고양이나 강아지중에 좋아하시는 종 있으세요?",
          "speaker_id": "1",
          "time": "20210822 14:54"
        },
        {
          "id": "MDRW2100008126.1.17",
          "form": "딱히 아는건 없는데",
          "original_form": "딱히 아는건 없는데",
          "speaker_id": "2",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100008126.1.18",
          "form": "군견류 멋지더라고요 까만거",
          "original_form": "군견류 멋지더라고요 까만거",
          "speaker_id": "2",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100008126.1.19",
          "form": "지인님은요",
          "original_form": "지인님은요",
          "speaker_id": "2",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100008126.1.20",
          "form": "저는 요즘 진돗개도 좋은거같아요",
          "original_form": "저는 요즘 진돗개도 좋은거같아요",
          "speaker_id": "1",
          "time": "20210822 14:57"
        },
        {
          "id": "MDRW2100008126.1.21",
          "form": "진돗개..좋죠",
          "original_form": "진돗개..좋죠",
          "speaker_id": "2",
          "time": "20210822 14:57"
        },
        {
          "id": "MDRW2100008126.1.22",
          "form": "대형견만의 매력이 있는듯요",
          "original_form": "대형견만의 매력이 있는듯요",
          "speaker_id": "2",
          "time": "20210822 14:58"
        },
        {
          "id": "MDRW2100008126.1.23",
          "form": "햄스터나 물고기같은 작은동물도 좋지않아요?",
          "original_form": "햄스터나 물고기같은 작은동물도 좋지않아요?",
          "speaker_id": "1",
          "time": "20210822 14:58"
        },
        {
          "id": "MDRW2100008126.1.24",
          "form": "좋죠 것도 결국 번거로워서 못 키우겠지만",
          "original_form": "좋죠 것도 결국 번거로워서 못 키우겠지만",
          "speaker_id": "2",
          "time": "20210822 14:59"
