      "utterance": [
        {
          "id": "MDRW2100038771.1.1",
          "form": "퇴근하삼?",
          "original_form": "퇴근하삼?",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100038771.1.2",
          "form": "전철이쥬",
          "original_form": "전철이쥬",
          "speaker_id": "2",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100038771.1.3",
          "form": "ㅋㅋ아빠 오늘 주사맞으신거 앎?",
          "original_form": "ㅋㅋ아빠 오늘 주사맞으신거 앎?",
          "speaker_id": "1",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100038771.1.4",
          "form": "읭? 담주아니고?",
          "original_form": "읭? 담주아니고?",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100038771.1.5",
          "form": "일주일 미뤄졌다고 엄마가 그랬는데?",
          "original_form": "일주일 미뤄졌다고 엄마가 그랬는데?",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100038771.1.6",
          "form": "불편하신곳은 없으시대?",
          "original_form": "불편하신곳은 없으시대?",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100038771.1.7",
          "form": "ㅋㅋ 엄마가 담주",
          "original_form": "ㅋㅋ 엄마가 담주",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100038771.1.8",
          "form": "아빤 말을 안하시지ㅋㅋ",
          "original_form": "아빤 말을 안하시지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100038771.1.9",
          "form": "그때 아빠가 8월말이라고 하셨는데",
          "original_form": "그때 아빠가 8월말이라고 하셨는데",
          "speaker_id": "2",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100038771.1.10",
          "form": "9월초였군 일주일이 아니고 2,3일 미뤄진거였네",
          "original_form": "9월초였군 일주일이 아니고 2,3일 미뤄진거였네",
          "speaker_id": "2",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100038771.1.11",
          "form": "ㅋㅋ 아리까리",
          "original_form": "ㅋㅋ 아리까리",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100038771.1.12",
          "form": "괜찮으셔서 다행이여",
          "original_form": "괜찮으셔서 다행이여",
          "speaker_id": "2",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100038771.1.13",
          "form": "토요일에 빨래 할거야?",
          "original_form": "토요일에 빨래 할거야?",
          "speaker_id": "2",
          "time": "20210902 22:20"
        },
        {
          "id": "MDRW2100038771.1.14",
          "form": "ㅋㅋ낼 몇시 접종?",
          "original_form": "ㅋㅋ낼 몇시 접종?",
          "speaker_id": "1",
          "time": "20210902 22:20"
        },
        {
          "id": "MDRW2100038771.1.15",
          "form": "빨래는 왜 묻는거죠?",
          "original_form": "빨래는 왜 묻는거죠?",
          "speaker_id": "1",
          "time": "20210902 22:20"
        },
        {
          "id": "MDRW2100038771.1.16",
          "form": "낼 4시",
          "original_form": "낼 4시",
          "speaker_id": "2",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100038771.1.17",
          "form": "비가 안온다고 하니 빨래요정이 움직일라나 싶어서 ㅋㅋㅋㅋㅋ",
          "original_form": "비가 안온다고 하니 빨래요정이 움직일라나 싶어서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100038771.1.18",
          "form": "ㅋㅋ대청소 예정이었으나.. 내가 피아노 선반만할겨",
          "original_form": "ㅋㅋ대청소 예정이었으나.. 내가 피아노 선반만할겨",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100038771.1.19",
          "form": "빨래 날좋음 당근해야지",
          "original_form": "빨래 날좋음 당근해야지",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100038771.1.20",
          "form": "웅 그렇고만ㅋㅋㅋㅋ",
          "original_form": "웅 그렇고만ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100038771.1.21",
          "form": "당연합니다",
          "original_form": "당연합니다",
          "speaker_id": "1",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100038771.1.22",
          "form": "낼 당신의 컨디션에 따라",
          "original_form": "낼 당신의 컨디션에 따라",
          "speaker_id": "1",
          "time": "20210902 22:26"
        }
