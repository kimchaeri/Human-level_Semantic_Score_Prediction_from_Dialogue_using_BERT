      "utterance": [
        {
          "id": "MDRW2100012276.1.1",
          "form": "여권에도 저렴하게 하는 방법이 있다는거 알아",
          "original_form": "여권에도 저렴하게 하는 방법이 있다는거 알아",
          "speaker_id": "1",
          "time": "20210824 23:57"
        },
        {
          "id": "MDRW2100012276.1.2",
          "form": "뭐 여권도",
          "original_form": "뭐 여권도",
          "speaker_id": "2",
          "time": "20210824 23:57"
        },
        {
          "id": "MDRW2100012276.1.3",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210824 23:57"
        },
        {
          "id": "MDRW2100012276.1.4",
          "form": "헐 대박이네 뭔데",
          "original_form": "헐 대박이네 뭔데",
          "speaker_id": "2",
          "time": "20210824 23:57"
        },
        {
          "id": "MDRW2100012276.1.5",
          "form": "여권에는 일반여권이랑 알뜰여권이 있거든",
          "original_form": "여권에는 일반여권이랑 알뜰여권이 있거든",
          "speaker_id": "1",
          "time": "20210824 23:58"
        },
        {
          "id": "MDRW2100012276.1.6",
          "form": "2종류네",
          "original_form": "2종류네",
          "speaker_id": "2",
          "time": "20210824 23:58"
        },
        {
          "id": "MDRW2100012276.1.7",
          "form": "엉 거기서 또 유효기간이 5년 10년인게 있어",
          "original_form": "엉 거기서 또 유효기간이 5년 10년인게 있어",
          "speaker_id": "1",
          "time": "20210824 23:58"
        },
        {
          "id": "MDRW2100012276.1.8",
          "form": "어 이건 알아",
          "original_form": "어 이건 알아",
          "speaker_id": "2",
          "time": "20210824 23:58"
        },
        {
          "id": "MDRW2100012276.1.9",
          "form": "알뜰여권을 하면 일반여권보다 3,000원 저렴",
          "original_form": "알뜰여권을 하면 일반여권보다 3,000원 저렴",
          "speaker_id": "1",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100012276.1.10",
          "form": "차이는 없는거야?",
          "original_form": "차이는 없는거야?",
          "speaker_id": "2",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100012276.1.11",
          "form": "엉  그냥 여권종이의 장수가 틀림",
          "original_form": "엉  그냥 여권종이의 장수가 틀림",
          "speaker_id": "1",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100012276.1.12",
          "form": "그렇구나 장수는 괜찮아",
          "original_form": "그렇구나 장수는 괜찮아",
          "speaker_id": "2",
          "time": "20210825 00:00"
        },
        {
          "id": "MDRW2100012276.1.13",
          "form": "일반여권은 48면 알뜰여권24면",
          "original_form": "일반여권은 48면 알뜰여권24면",
          "speaker_id": "1",
          "time": "20210825 00:00"
        },
        {
          "id": "MDRW2100012276.1.14",
          "form": "하긴 여행 10년동안 얼마가겠어",
          "original_form": "하긴 여행 10년동안 얼마가겠어",
          "speaker_id": "2",
          "time": "20210825 00:00"
        },
        {
          "id": "MDRW2100012276.1.15",
          "form": "응 여행 자주 안다니면 그냥 두껍지 않은 알뜰여권이 좋지",
          "original_form": "응 여행 자주 안다니면 그냥 두껍지 않은 알뜰여권이 좋지",
