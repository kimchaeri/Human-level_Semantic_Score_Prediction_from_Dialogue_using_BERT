      "utterance": [
        {
          "id": "MDRW2100022835.1.1",
          "form": "나 저녁",
          "original_form": "나 저녁",
          "speaker_id": "2",
          "time": "20210829 18:18"
        },
        {
          "id": "MDRW2100022835.1.2",
          "form": "콤나물밥 먹음",
          "original_form": "콤나물밥 먹음",
          "speaker_id": "2",
          "time": "20210829 18:18"
        },
        {
          "id": "MDRW2100022835.1.3",
          "form": "근데 생각보다 음식이 안먹혀서 쉬다 먹다 쉬다했어ㅎ",
          "original_form": "근데 생각보다 음식이 안먹혀서 쉬다 먹다 쉬다했어ㅎ",
          "speaker_id": "2",
          "time": "20210829 18:19"
        },
        {
          "id": "MDRW2100022835.1.4",
          "form": "콩나물밥? 건강식이네 ㅋㅋ",
          "original_form": "콩나물밥? 건강식이네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:19"
        },
        {
          "id": "MDRW2100022835.1.5",
          "form": "왜? 맛이없었어?",
          "original_form": "왜? 맛이없었어?",
          "speaker_id": "1",
          "time": "20210829 18:19"
        },
        {
          "id": "MDRW2100022835.1.6",
          "form": "아니 맛보다 뭔가 벅차다? 안먹히더라",
          "original_form": "아니 맛보다 뭔가 벅차다? 안먹히더라",
          "speaker_id": "2",
          "time": "20210829 18:19"
        },
        {
          "id": "MDRW2100022835.1.7",
          "form": "그래도 맛나게 먹긴했어ㅎ",
          "original_form": "그래도 맛나게 먹긴했어ㅎ",
          "speaker_id": "2",
          "time": "20210829 18:19"
        },
        {
          "id": "MDRW2100022835.1.8",
          "form": "입맛이 없었나?",
          "original_form": "입맛이 없었나?",
          "speaker_id": "1",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022835.1.9",
          "form": "아니ㅎ 입맛보단 먹고싶고 맛도 괜찮았는데 점심소화가 덜 된건지 안먹히더라 숨찬느낌  비슷하게ㅎ",
          "original_form": "아니ㅎ 입맛보단 먹고싶고 맛도 괜찮았는데 점심소화가 덜 된건지 안먹히더라 숨찬느낌  비슷하게ㅎ",
          "speaker_id": "2",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022835.1.10",
          "form": "아 점심을 늦게 먹지않았나?",
          "original_form": "아 점심을 늦게 먹지않았나?",
          "speaker_id": "1",
          "time": "20210829 18:24"
        },
        {
          "id": "MDRW2100022835.1.11",
          "form": "그래서 그런가봐",
          "original_form": "그래서 그런가봐",
          "speaker_id": "1",
          "time": "20210829 18:24"
        },
        {
          "id": "MDRW2100022835.1.12",
          "form": "늦께 먹었나ㅎ 그럴지도ㅎㅎ",
          "original_form": "늦께 먹었나ㅎ 그럴지도ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 18:24"
        },
        {
          "id": "MDRW2100022835.1.13",
          "form": "좀 움직여보려고 해",
          "original_form": "좀 움직여보려고 해",
          "speaker_id": "2",
          "time": "20210829 18:25"
        },
        {
          "id": "MDRW2100022835.1.14",
          "form": "소화되나  어쩌나 싶어서ㅎ",
          "original_form": "소화되나  어쩌나 싶어서ㅎ",
          "speaker_id": "2",
          "time": "20210829 18:25"
        },
        {
          "id": "MDRW2100022835.1.15",
          "form": "저녁은?",
          "original_form": "저녁은?",
          "speaker_id": "2",
          "time": "20210829 18:25"
        },
        {
          "id": "MDRW2100022835.1.16",
          "form": "ㅇㅇ걸으면 소화될듯해",
          "original_form": "ㅇㅇ걸으면 소화될듯해",
          "speaker_id": "1",
          "time": "20210829 18:25"
        },
        {
          "id": "MDRW2100022835.1.17",
          "form": "난 문어초무침해서 먹었당",
          "original_form": "난 문어초무침해서 먹었당",
          "speaker_id": "1",
          "time": "20210829 18:26"
        },
        {
          "id": "MDRW2100022835.1.18",
          "form": "색다른메뉴먹었네\r\n문어 한마리 샀오?",
          "original_form": "색다른메뉴먹었네\r\n문어 한마리 샀오?",
          "speaker_id": "2",
          "time": "20210829 18:26"
        },
        {
          "id": "MDRW2100022835.1.19",
          "form": "아니 전에 있던거ㅈ엄마가 냉동실에얼려둠",
          "original_form": "아니 전에 있던거ㅈ엄마가 냉동실에얼려둠",
          "speaker_id": "1",
          "time": "20210829 18:28"
        },
        {
          "id": "MDRW2100022835.1.20",
          "form": "별식이였네ㅎ",
          "original_form": "별식이였네ㅎ",
          "speaker_id": "2",
          "time": "20210829 18:39"
        },
        {
          "id": "MDRW2100022835.1.21",
          "form": "문어 쫄깃 새콤해?",
          "original_form": "문어 쫄깃 새콤해?",
          "speaker_id": "2",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100022835.1.22",
          "form": "양념은 맛있는데 문어가 오래되서 ㅈ ㅣㄹ기더라",
          "original_form": "양념은 맛있는데 문어가 오래되서 ㅈ ㅣㄹ기더라",
          "speaker_id": "1",
          "time": "20210829 18:45"
        },
        {
          "id": "MDRW2100022835.1.23",
          "form": "양념 잘하셨네ㅎㅎ",
          "original_form": "양념 잘하셨네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 18:45"
        },
        {
          "id": "MDRW2100022835.1.24",
          "form": "문어 먹은지 좀 된듯",
          "original_form": "문어 먹은지 좀 된듯",
          "speaker_id": "2",
          "time": "20210829 18:46"
        }
      ]
    }
