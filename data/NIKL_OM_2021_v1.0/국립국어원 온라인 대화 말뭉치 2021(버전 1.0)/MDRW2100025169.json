      "utterance": [
        {
          "id": "MDRW2100025169.1.1",
          "form": "너는 sns를 하니?",
          "original_form": "너는 sns를 하니?",
          "speaker_id": "1",
          "time": "20210901 00:06"
        },
        {
          "id": "MDRW2100025169.1.2",
          "form": "인스타그램 하지",
          "original_form": "인스타그램 하지",
          "speaker_id": "2",
          "time": "20210901 00:06"
        },
        {
          "id": "MDRW2100025169.1.3",
          "form": "다음 카페도 하고",
          "original_form": "다음 카페도 하고",
          "speaker_id": "2",
          "time": "20210901 00:07"
        },
        {
          "id": "MDRW2100025169.1.4",
          "form": "나도 sns는 인스타만하고",
          "original_form": "나도 sns는 인스타만하고",
          "speaker_id": "1",
          "time": "20210901 00:07"
        },
        {
          "id": "MDRW2100025169.1.5",
          "form": "커뮤는 다음카페랑",
          "original_form": "커뮤는 다음카페랑",
          "speaker_id": "1",
          "time": "20210901 00:07"
        },
        {
          "id": "MDRW2100025169.1.6",
          "form": "가끔 트위터해",
          "original_form": "가끔 트위터해",
          "speaker_id": "1",
          "time": "20210901 00:07"
        },
        {
          "id": "MDRW2100025169.1.7",
          "form": "이것저것 많이 하네",
          "original_form": "이것저것 많이 하네",
          "speaker_id": "2",
          "time": "20210901 00:07"
        },
        {
          "id": "MDRW2100025169.1.8",
          "form": "그래서 니가 소식에 빠르구나",
          "original_form": "그래서 니가 소식에 빠르구나",
          "speaker_id": "2",
          "time": "20210901 00:07"
        },
        {
          "id": "MDRW2100025169.1.9",
          "form": "요즘 쭉빵이 망해서",
          "original_form": "요즘 쭉빵이 망해서",
          "speaker_id": "1",
          "time": "20210901 00:08"
        },
        {
          "id": "MDRW2100025169.1.10",
          "form": "다른곳을 찾느라 좀힘들었어",
          "original_form": "다른곳을 찾느라 좀힘들었어",
          "speaker_id": "1",
          "time": "20210901 00:08"
        },
        {
          "id": "MDRW2100025169.1.11",
          "form": "너도 그카페를 했니?",
          "original_form": "너도 그카페를 했니?",
          "speaker_id": "1",
          "time": "20210901 00:08"
        },
        {
          "id": "MDRW2100025169.1.12",
          "form": "했었어",
          "original_form": "했었어",
          "speaker_id": "2",
          "time": "20210901 00:08"
        },
        {
          "id": "MDRW2100025169.1.13",
          "form": "이런저런 정보 많아서 좋았는데 망해서 아쉬웠지",
          "original_form": "이런저런 정보 많아서 좋았는데 망해서 아쉬웠지",
          "speaker_id": "2",
          "time": "20210901 00:08"
        },
        {
          "id": "MDRW2100025169.1.14",
          "form": "맞아 딴곳은 아직 성에 안차는거같아",
          "original_form": "맞아 딴곳은 아직 성에 안차는거같아",
          "speaker_id": "1",
          "time": "20210901 00:09"
        },
        {
          "id": "MDRW2100025169.1.15",
          "form": "거기가 최고였는데",
          "original_form": "거기가 최고였는데",
          "speaker_id": "1",
          "time": "20210901 00:09"
        },
        {
          "id": "MDRW2100025169.1.16",
          "form": "결국은 나도 카페는 못찾고 인기글만 보잖아",
          "original_form": "결국은 나도 카페는 못찾고 인기글만 보잖아",
          "speaker_id": "2",
          "time": "20210901 00:09"
        },
        {
          "id": "MDRW2100025169.1.17",
          "form": "인기글 요즘 도탁스나",
          "original_form": "인기글 요즘 도탁스나",
          "speaker_id": "1",
          "time": "20210901 00:10"
        },
        {
          "id": "MDRW2100025169.1.18",
          "form": "이종께 많이올라와서 재미가없어",
          "original_form": "이종께 많이올라와서 재미가없어",
          "speaker_id": "1",
          "time": "20210901 00:10"
        },
        {
          "id": "MDRW2100025169.1.19",
          "form": "아 맞아",
          "original_form": "아 맞아",
          "speaker_id": "2",
          "time": "20210901 00:10"
        },
        {
          "id": "MDRW2100025169.1.20",
          "form": "좋은 카페 안 생기나",
          "original_form": "좋은 카페 안 생기나",
          "speaker_id": "2",
          "time": "20210901 00:10"
        },
        {
          "id": "MDRW2100025169.1.21",
          "form": "파생카페로 우동탕생겼는데",
          "original_form": "파생카페로 우동탕생겼는데",
          "speaker_id": "1",
          "time": "20210901 00:11"
        },
        {
          "id": "MDRW2100025169.1.22",
          "form": "한번 가입해봐",
          "original_form": "한번 가입해봐",
          "speaker_id": "1",
          "time": "20210901 00:11"
        },
        {
          "id": "MDRW2100025169.1.23",
          "form": "나는 아예 다른 커뮤 둘러봤는데",
          "original_form": "나는 아예 다른 커뮤 둘러봤는데",
          "speaker_id": "2",
          "time": "20210901 00:11"
        },
        {
          "id": "MDRW2100025169.1.24",
          "form": "다른데는 너무 폐쇄적이더라",
          "original_form": "다른데는 너무 폐쇄적이더라",
          "speaker_id": "2",
          "time": "20210901 00:11"
        },
        {
          "id": "MDRW2100025169.1.25",
          "form": "가입 자체가 힘들어",
          "original_form": "가입 자체가 힘들어",
          "speaker_id": "2",
          "time": "20210901 00:11"
        },
        {
          "id": "MDRW2100025169.1.26",
          "form": "맞아 막상 가입해도",
          "original_form": "맞아 막상 가입해도",
          "speaker_id": "1",
          "time": "20210901 00:11"
        },
        {
          "id": "MDRW2100025169.1.27",
          "form": "등업이 너무 어려워",
          "original_form": "등업이 너무 어려워",
          "speaker_id": "1",
          "time": "20210901 00:11"
        }
      ]
    }
  ]
