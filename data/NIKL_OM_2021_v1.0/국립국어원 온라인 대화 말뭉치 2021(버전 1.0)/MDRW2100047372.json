      "utterance": [
        {
          "id": "MDRW2100047372.1.1",
          "form": "최근 영화 보셨나요?",
          "original_form": "최근 영화 보셨나요?",
          "speaker_id": "1",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100047372.1.2",
          "form": "최근 본 영화 있나요?",
          "original_form": "최근 본 영화 있나요?",
          "speaker_id": "1",
          "time": "20210826 14:02"
        },
        {
          "id": "MDRW2100047372.1.3",
          "form": "랑종을 봤어요",
          "original_form": "랑종을 봤어요",
          "speaker_id": "2",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100047372.1.4",
          "form": "name1님은요,",
          "original_form": "&name1&님은요,",
          "speaker_id": "2",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100047372.1.5",
          "form": "저는 모가디슈를 봤어요",
          "original_form": "저는 모가디슈를 봤어요",
          "speaker_id": "1",
          "time": "20210826 14:06"
        },
        {
          "id": "MDRW2100047372.1.6",
          "form": "모가디슈 평 좋던데",
          "original_form": "모가디슈 평 좋던데",
          "speaker_id": "2",
          "time": "20210826 14:08"
        },
        {
          "id": "MDRW2100047372.1.7",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "2",
          "time": "20210826 14:08"
        },
        {
          "id": "MDRW2100047372.1.8",
          "form": "네 재밌었어요 ^^",
          "original_form": "네 재밌었어요 ^^",
          "speaker_id": "1",
          "time": "20210826 14:08"
        },
        {
          "id": "MDRW2100047372.1.9",
          "form": "ㅎㅎ 볼까말까 했었는데",
          "original_form": "ㅎㅎ 볼까말까 했었는데",
          "speaker_id": "2",
          "time": "20210826 14:09"
        },
        {
          "id": "MDRW2100047372.1.10",
          "form": "지금은 극장에서 내렸으려나요",
          "original_form": "지금은 극장에서 내렸으려나요",
          "speaker_id": "2",
          "time": "20210826 14:09"
        },
        {
          "id": "MDRW2100047372.1.11",
          "form": "전 재밌어서 주변에 추천도 했네요^^",
          "original_form": "전 재밌어서 주변에 추천도 했네요^^",
          "speaker_id": "1",
          "time": "20210826 14:09"
        },
        {
          "id": "MDRW2100047372.1.12",
          "form": "어 아직 안 내렸네요",
          "original_form": "어 아직 안 내렸네요",
          "speaker_id": "2",
          "time": "20210826 14:10"
        },
        {
          "id": "MDRW2100047372.1.13",
          "form": "코로나로 개봉영화가 적으니 텀이 긴가봐요",
          "original_form": "코로나로 개봉영화가 적으니 텀이 긴가봐요",
          "speaker_id": "2",
          "time": "20210826 14:10"
        },
        {
          "id": "MDRW2100047372.1.14",
          "form": "아직 극장 걸려있을 때 봐야겠어요",
          "original_form": "아직 극장 걸려있을 때 봐야겠어요",
          "speaker_id": "2",
          "time": "20210826 14:10"
        },
        {
          "id": "MDRW2100047372.1.15",
          "form": "극장에 사람은 많던가요?",
          "original_form": "극장에 사람은 많던가요?",
          "speaker_id": "2",
          "time": "20210826 14:12"
        },
        {
          "id": "MDRW2100047372.1.16",
          "form": "그러게요 공연계도 코로나때문에 타격이 크겠어요",
          "original_form": "그러게요 공연계도 코로나때문에 타격이 크겠어요",
          "speaker_id": "1",
          "time": "20210826 14:12"
        },
        {
          "id": "MDRW2100047372.1.17",
          "form": "네 그와중에 제가 예약한 공연들은 그래도 관객점유율이 높은데",
          "original_form": "네 그와중에 제가 예약한 공연들은 그래도 관객점유율이 높은데",
          "speaker_id": "2",
          "time": "20210826 14:13"
        },
        {
          "id": "MDRW2100047372.1.18",
          "form": "실험적인 공연은 정말 어렵겠어요",
          "original_form": "실험적인 공연은 정말 어렵겠어요",
          "speaker_id": "2",
          "time": "20210826 14:14"
        },
        {
          "id": "MDRW2100047372.1.19",
          "form": "어떤 공연 예약하셨나요",
          "original_form": "어떤 공연 예약하셨나요",
          "speaker_id": "1",
          "time": "20210826 14:14"
        },
        {
          "id": "MDRW2100047372.1.20",
          "form": "조드윅이요",
          "original_form": "조드윅이요",
          "speaker_id": "2",
          "time": "20210826 14:16"
        },
        {
          "id": "MDRW2100047372.1.21",
          "form": "와우 대박이네요~^^",
          "original_form": "와우 대박이네요~^^",
          "speaker_id": "1",
          "time": "20210826 14:17"
        },
        {
          "id": "MDRW2100047372.1.22",
          "form": "그전에 시카고도 사람 많았고요",
          "original_form": "그전에 시카고도 사람 많았고요",
