      "utterance": [
        {
          "id": "MDRW2100036984.1.1",
          "form": "지금 갑자기 내리고 있어요!!",
          "original_form": "지금 갑자기 내리고 있어요!!",
          "speaker_id": "2",
          "time": "20210831 16:31"
        },
        {
          "id": "MDRW2100036984.1.2",
          "form": "엄청 많이 내리네요ㅠ",
          "original_form": "엄청 많이 내리네요ㅠ",
          "speaker_id": "2",
          "time": "20210831 16:31"
        },
        {
          "id": "MDRW2100036984.1.3",
          "form": "저희는 방금 호우주의보 문자도 왔어요",
          "original_form": "저희는 방금 호우주의보 문자도 왔어요",
          "speaker_id": "1",
          "time": "20210831 16:31"
        },
        {
          "id": "MDRW2100036984.1.4",
          "form": "우와ㄷㄷ",
          "original_form": "우와ㄷㄷ",
          "speaker_id": "2",
          "time": "20210831 16:33"
        },
        {
          "id": "MDRW2100036984.1.5",
          "form": "밖이신가요?!",
          "original_form": "밖이신가요?!",
          "speaker_id": "2",
          "time": "20210831 16:34"
        },
        {
          "id": "MDRW2100036984.1.6",
          "form": "저는 지금 직장인데",
          "original_form": "저는 지금 직장인데",
          "speaker_id": "1",
          "time": "20210831 16:34"
        },
        {
          "id": "MDRW2100036984.1.7",
          "form": "퇴근길이 걱정되네요ㅜ",
          "original_form": "퇴근길이 걱정되네요ㅜ",
          "speaker_id": "1",
          "time": "20210831 16:34"
        },
        {
          "id": "MDRW2100036984.1.8",
          "form": "비가 좀 잦아들면 좋으련만",
          "original_form": "비가 좀 잦아들면 좋으련만",
          "speaker_id": "1",
          "time": "20210831 16:34"
        },
        {
          "id": "MDRW2100036984.1.9",
          "form": "앗ㅠㅠ 진짜요ㅡㅜㅠ",
          "original_form": "앗ㅠㅠ 진짜요ㅡㅜㅠ",
          "speaker_id": "2",
          "time": "20210831 16:35"
        },
        {
          "id": "MDRW2100036984.1.10",
          "form": "퇴근하시기전에 그치면 좋겠어요!",
          "original_form": "퇴근하시기전에 그치면 좋겠어요!",
          "speaker_id": "2",
          "time": "20210831 16:35"
        },
        {
          "id": "MDRW2100036984.1.11",
          "form": "소나기여야하는데ㅠ",
          "original_form": "소나기여야하는데ㅠ",
          "speaker_id": "2",
          "time": "20210831 16:35"
        },
        {
          "id": "MDRW2100036984.1.12",
          "form": "퇴근하면서도 폭우였어요ㅠ",
          "original_form": "퇴근하면서도 폭우였어요ㅠ",
          "speaker_id": "1",
          "time": "20210831 20:37"
        },
        {
          "id": "MDRW2100036984.1.13",
          "form": "세상에.. 잘 도착하셨어요?ㅡㅠ",
          "original_form": "세상에.. 잘 도착하셨어요?ㅡㅠ",
          "speaker_id": "2",
          "time": "20210831 20:40"
        },
        {
          "id": "MDRW2100036984.1.14",
          "form": "네 다행히 동료분이 근처까지 차 태워주시고",
          "original_form": "네 다행히 동료분이 근처까지 차 태워주시고",
          "speaker_id": "1",
          "time": "20210831 20:40"
        },
        {
          "id": "MDRW2100036984.1.15",
          "form": "가족이 픽업해서",
          "original_form": "가족이 픽업해서",
          "speaker_id": "1",
          "time": "20210831 20:40"
        },
        {
          "id": "MDRW2100036984.1.16",
          "form": "아우ㅠ 다행이에요 정말",
          "original_form": "아우ㅠ 다행이에요 정말",
          "speaker_id": "2",
          "time": "20210831 20:42"
        },
        {
          "id": "MDRW2100036984.1.17",
          "form": "새벽에도 비가 엄청내린다더라구요",
          "original_form": "새벽에도 비가 엄청내린다더라구요",
          "speaker_id": "2",
          "time": "20210831 20:43"
        },
        {
          "id": "MDRW2100036984.1.18",
          "form": "name2님 동네는 비 많이 오지 않나요?",
          "original_form": "&name2&님 동네는 비 많이 오지 않나요?",
          "speaker_id": "1",
          "time": "20210831 20:43"
        },
        {
          "id": "MDRW2100036984.1.19",
          "form": "지금은 안오지만 새벽에 엄청 내린다고 경고뜨더라구요",
          "original_form": "지금은 안오지만 새벽에 엄청 내린다고 경고뜨더라구요",
          "speaker_id": "2",
          "time": "20210831 20:46"
        },
        {
          "id": "MDRW2100036984.1.20",
          "form": "아 구름이 이동하고 있다더라고요",
          "original_form": "아 구름이 이동하고 있다더라고요",
          "speaker_id": "1",
          "time": "20210831 21:20"
        },
        {
          "id": "MDRW2100036984.1.21",
          "form": "세상에ㅜㅠ",
          "original_form": "세상에ㅜㅠ",
