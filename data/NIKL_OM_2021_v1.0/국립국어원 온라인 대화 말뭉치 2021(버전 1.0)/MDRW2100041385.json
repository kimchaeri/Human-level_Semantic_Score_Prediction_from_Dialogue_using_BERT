      "utterance": [
        {
          "id": "MDRW2100041385.1.1",
          "form": "영화는 잘 안보게되요",
          "original_form": "영화는 잘 안보게되요",
          "speaker_id": "2",
          "time": "20210905 22:55"
        },
        {
          "id": "MDRW2100041385.1.2",
          "form": "좋아하는 연예인은요?",
          "original_form": "좋아하는 연예인은요?",
          "speaker_id": "1",
          "time": "20210905 22:56"
        },
        {
          "id": "MDRW2100041385.1.3",
          "form": "요즘 정경호가 멋잇더라고요",
          "original_form": "요즘 정경호가 멋잇더라고요",
          "speaker_id": "2",
          "time": "20210905 23:00"
        },
        {
          "id": "MDRW2100041385.1.4",
          "form": "아 슬의 보세요?",
          "original_form": "아 슬의 보세요?",
          "speaker_id": "1",
          "time": "20210905 23:00"
        },
        {
          "id": "MDRW2100041385.1.5",
          "form": "네 보세요?",
          "original_form": "네 보세요?",
          "speaker_id": "2",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100041385.1.6",
          "form": "저는 아직이용 ㅠㅠ",
          "original_form": "저는 아직이용 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100041385.1.7",
          "form": "슬의 3인가 하지 않아요?",
          "original_form": "슬의 3인가 하지 않아요?",
          "speaker_id": "1",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100041385.1.8",
          "form": "2요 거의 끝나가요",
          "original_form": "2요 거의 끝나가요",
          "speaker_id": "2",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100041385.1.9",
          "form": "아하 아쉬우시겠어요ㅠㅠ 해피엔딩이겠죠?!",
          "original_form": "아하 아쉬우시겠어요ㅠㅠ 해피엔딩이겠죠?!",
          "speaker_id": "1",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100041385.1.10",
          "form": "예측을 못하겠네요ㅋㅋ",
          "original_form": "예측을 못하겠네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100041385.1.11",
          "form": "헉 요즘 막 전개가 스펙타클한가봐요",
          "original_form": "헉 요즘 막 전개가 스펙타클한가봐요",
          "speaker_id": "1",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100041385.1.12",
          "form": "그렇긴 한데 3없다면 잘되겠죠ㅋ",
          "original_form": "그렇긴 한데 3없다면 잘되겠죠ㅋ",
          "speaker_id": "2",
          "time": "20210905 23:05"
        },
        {
          "id": "MDRW2100041385.1.13",
          "form": "그래도 3 나오면 좋겠어요 ㅋㅋㅋ 우리나라에 계속 하는 시리즈 드라마 있었으면...",
          "original_form": "그래도 3 나오면 좋겠어요 ㅋㅋㅋ 우리나라에 계속 하는 시리즈 드라마 있었으면...",
          "speaker_id": "1",
          "time": "20210905 23:09"
        },
        {
          "id": "MDRW2100041385.1.14",
          "form": "맞아요 그럼 좋겠어요",
          "original_form": "맞아요 그럼 좋겠어요",
          "speaker_id": "2",
