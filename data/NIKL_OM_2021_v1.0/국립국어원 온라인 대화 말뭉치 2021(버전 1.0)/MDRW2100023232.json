      "utterance": [
        {
          "id": "MDRW2100023232.1.1",
          "form": "난 올스타전은 한 번 가봤어",
          "original_form": "난 올스타전은 한 번 가봤어",
          "speaker_id": "2",
          "time": "20210829 23:05"
        },
        {
          "id": "MDRW2100023232.1.2",
          "form": "오 재미있었겠다",
          "original_form": "오 재미있었겠다",
          "speaker_id": "1",
          "time": "20210829 23:06"
        },
        {
          "id": "MDRW2100023232.1.3",
          "form": "응 일반 경기랑 다른 느낌이었어",
          "original_form": "응 일반 경기랑 다른 느낌이었어",
          "speaker_id": "2",
          "time": "20210829 23:06"
        },
        {
          "id": "MDRW2100023232.1.4",
          "form": "반쯤 예능이잖아ㅋㅋㅋㅋ",
          "original_form": "반쯤 예능이잖아ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100023232.1.5",
          "form": "ㅋㅋㅋㅋ 그렇지",
          "original_form": "ㅋㅋㅋㅋ 그렇지",
          "speaker_id": "2",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100023232.1.6",
          "form": "언제 가봤는데?",
          "original_form": "언제 가봤는데?",
          "speaker_id": "1",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100023232.1.7",
          "form": "예전에 친구랑 가봣어",
          "original_form": "예전에 친구랑 가봣어",
          "speaker_id": "2",
          "time": "20210829 23:07"
        },
        {
          "id": "MDRW2100023232.1.8",
          "form": "제일 기억에 남는 선수 누구야?",
          "original_form": "제일 기억에 남는 선수 누구야?",
          "speaker_id": "1",
          "time": "20210829 23:08"
        },
        {
          "id": "MDRW2100023232.1.9",
          "form": "잘 기억은 안나는데 박병호?",
          "original_form": "잘 기억은 안나는데 박병호?",
          "speaker_id": "2",
          "time": "20210829 23:08"
        },
        {
          "id": "MDRW2100023232.1.10",
          "form": "박병호 요즘 어디에 있어?",
          "original_form": "박병호 요즘 어디에 있어?",
          "speaker_id": "1",
          "time": "20210829 23:09"
        },
        {
          "id": "MDRW2100023232.1.11",
          "form": "최근에 주장직 내려놨다는데",
          "original_form": "최근에 주장직 내려놨다는데",
          "speaker_id": "2",
          "time": "20210829 23:09"
        },
        {
          "id": "MDRW2100023232.1.12",
          "form": "계속 넥센 이야?",
          "original_form": "계속 넥센 이야?",
          "speaker_id": "1",
          "time": "20210829 23:09"
        },
        {
          "id": "MDRW2100023232.1.13",
          "form": "응 키움",
          "original_form": "응 키움",
          "speaker_id": "2",
          "time": "20210829 23:10"
        },
        {
          "id": "MDRW2100023232.1.14",
          "form": "요즘은 못하나?",
          "original_form": "요즘은 못하나?",
          "speaker_id": "1",
          "time": "20210829 23:10"
        },
        {
          "id": "MDRW2100023232.1.15",
          "form": "아 야구장 마렵다",
          "original_form": "아 야구장 마렵다",
          "speaker_id": "2",
          "time": "20210830 23:13"
        }
      ]
