      "utterance": [
        {
          "id": "MDRW2100016705.1.1",
          "form": "어떤 음식 좋아하세요??",
          "original_form": "어떤 음식 좋아하세요??",
          "speaker_id": "2",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100016705.1.2",
          "form": "저는 요새한창 브리또에빠졌어요~",
          "original_form": "저는 요새한창 브리또에빠졌어요~",
          "speaker_id": "1",
          "time": "20210826 15:30"
        },
        {
          "id": "MDRW2100016705.1.3",
          "form": "헐 전 파니니랑 샌드위치요 ㅠ",
          "original_form": "헐 전 파니니랑 샌드위치요 ㅠ",
          "speaker_id": "2",
          "time": "20210827 12:17"
        },
        {
          "id": "MDRW2100016705.1.4",
          "form": "너무 맛있어요 ㅠ",
          "original_form": "너무 맛있어요 ㅠ",
          "speaker_id": "2",
          "time": "20210827 12:17"
        },
        {
          "id": "MDRW2100016705.1.5",
          "form": "오 파니니랑 샌듸치 저도 참좋아해요! 혹시 맛집이 있나요??",
          "original_form": "오 파니니랑 샌듸치 저도 참좋아해요! 혹시 맛집이 있나요??",
          "speaker_id": "1",
          "time": "20210827 12:18"
        },
        {
          "id": "MDRW2100016705.1.6",
          "form": "저희 집 주변이라 ㅠㅜ",
          "original_form": "저희 집 주변이라 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210827 12:19"
        },
        {
          "id": "MDRW2100016705.1.7",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210827 12:19"
        },
        {
          "id": "MDRW2100016705.1.8",
          "form": "혹시 서울 사시나용??",
          "original_form": "혹시 서울 사시나용??",
          "speaker_id": "2",
          "time": "20210827 12:19"
        },
        {
          "id": "MDRW2100016705.1.9",
          "form": "아~ 개인사업장인가보군요!",
          "original_form": "아~ 개인사업장인가보군요!",
          "speaker_id": "1",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100016705.1.10",
          "form": "안타깝게도 전 부산살아요ㅜ",
          "original_form": "안타깝게도 전 부산살아요ㅜ",
          "speaker_id": "1",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100016705.1.11",
          "form": "체인점이긴 한데",
          "original_form": "체인점이긴 한데",
          "speaker_id": "2",
          "time": "20210827 12:23"
        },
        {
          "id": "MDRW2100016705.1.12",
          "form": "아이엠베이글이라고",
          "original_form": "아이엠베이글이라고",
          "speaker_id": "2",
          "time": "20210827 12:23"
        },
        {
          "id": "MDRW2100016705.1.13",
          "form": "맛있더라구용",
          "original_form": "맛있더라구용",
          "speaker_id": "2",
          "time": "20210827 12:23"
        },
        {
          "id": "MDRW2100016705.1.14",
          "form": "그리고 저 서브웨이 엄청 좋아해요",
          "original_form": "그리고 저 서브웨이 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210827 12:23"
        },
        {
          "id": "MDRW2100016705.1.15",
          "form": "아이엠베이글! 메모해둬야겠어요~",
          "original_form": "아이엠베이글! 메모해둬야겠어요~",
          "speaker_id": "1",
          "time": "20210827 12:24"
        },
        {
          "id": "MDRW2100016705.1.16",
          "form": "저두 서브웨이 좋아해요! 브레드는 플랫만 먹어요 ㅋㅋ",
          "original_form": "저두 서브웨이 좋아해요! 브레드는 플랫만 먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:24"
        },
        {
          "id": "MDRW2100016705.1.17",
          "form": "헐 진짜요??",
          "original_form": "헐 진짜요??",
          "speaker_id": "2",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100016705.1.18",
          "form": "전 허니오트랑 위트!!",
          "original_form": "전 허니오트랑 위트!!",
          "speaker_id": "2",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100016705.1.19",
          "form": "너무 맛있어요 ㅠ",
          "original_form": "너무 맛있어요 ㅠ",
          "speaker_id": "2",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100016705.1.20",
          "form": "무슨 조합 좋아하세요??",
          "original_form": "무슨 조합 좋아하세요??",
          "speaker_id": "2",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100016705.1.21",
          "form": "오! 저는 치킨들어간거 다좋아해요! 아침에먹으면 무조건 에그마요~ 소스는 랜치랑 어니언이요 ㅎㅎ",
          "original_form": "오! 저는 치킨들어간거 다좋아해요! 아침에먹으면 무조건 에그마요~ 소스는 랜치랑 어니언이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 18:24"
        },
        {
          "id": "MDRW2100016705.1.22",
          "form": "헐 에그마요 ㅠㅠㅜ",
          "original_form": "헐 에그마요 ㅠㅠㅜ",
          "speaker_id": "2",
          "time": "20210827 19:22"
        },
        {
          "id": "MDRW2100016705.1.23",
          "form": "너무 맛있죠 ㅠㅜ",
          "original_form": "너무 맛있죠 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210827 19:22"
        },
        {
          "id": "MDRW2100016705.1.24",
          "form": "갑자기 먹고 싶네요 ㅠㅠ",
          "original_form": "갑자기 먹고 싶네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 19:23"
        },
        {
          "id": "MDRW2100016705.1.25",
          "form": "에그마요 집에서 따라하려해도 그맛그대로는 안나는거같아요 ㅋㅋ",
          "original_form": "에그마요 집에서 따라하려해도 그맛그대로는 안나는거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:05"
        },
        {
          "id": "MDRW2100016705.1.26",
          "form": "밖에서 사먹는 게 최고에요.. 맛은..",
          "original_form": "밖에서 사먹는 게 최고에요.. 맛은..",
          "speaker_id": "2",
          "time": "20210827 23:42"
        }
      ]
