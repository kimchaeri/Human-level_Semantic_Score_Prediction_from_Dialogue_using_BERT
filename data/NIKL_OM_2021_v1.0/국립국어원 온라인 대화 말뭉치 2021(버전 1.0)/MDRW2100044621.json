      "utterance": [
        {
          "id": "MDRW2100044621.1.1",
          "form": "안녕하세요 라온님",
          "original_form": "안녕하세요 라온님",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044621.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044621.1.3",
          "form": "반려동물 키우시나용?",
          "original_form": "반려동물 키우시나용?",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044621.1.4",
          "form": "네네! 고양이 한마리 키우고 있어요",
          "original_form": "네네! 고양이 한마리 키우고 있어요",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044621.1.5",
          "form": "헐!!! 이거 왜 사진 기능 없나요 ㅠㅠㅠㅠㅠ 귀엽겠죠ㅠㅠ",
          "original_form": "헐!!! 이거 왜 사진 기능 없나요 ㅠㅠㅠㅠㅠ 귀엽겠죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044621.1.6",
          "form": "맞아요 사진 첨부 기능 없어서 너무 아쉬워요ㅜㅜㅜ",
          "original_form": "맞아요 사진 첨부 기능 없어서 너무 아쉬워요ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044621.1.7",
          "form": "저희집 고양이 동네방네 자랑하고 다니고 싶은데ㅜㅜㅜ",
          "original_form": "저희집 고양이 동네방네 자랑하고 다니고 싶은데ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044621.1.8",
          "form": "ㅠㅠㅠㅠ 글자로 자랑 해주세요ㅠㅋㅋㅋㅋㅋ 무슨색이에요?",
          "original_form": "ㅠㅠㅠㅠ 글자로 자랑 해주세요ㅠㅋㅋㅋㅋㅋ 무슨색이에요?",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044621.1.9",
          "form": "코숏 치즈태비에요!!",
          "original_form": "코숏 치즈태비에요!!",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044621.1.10",
          "form": "네발 다 흰양말 신었구 꼬리 끝도 우유에 퐁당했어요ㅜㅜ",
          "original_form": "네발 다 흰양말 신었구 꼬리 끝도 우유에 퐁당했어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044621.1.11",
          "form": "솜방망이도 토실토실해가지고 맞으면 얼마나 아픈지 몰라요..",
          "original_form": "솜방망이도 토실토실해가지고 맞으면 얼마나 아픈지 몰라요..",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044621.1.12",
          "form": "꺄아아앙앙가가각",
          "original_form": "꺄아아앙앙가가각",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044621.1.13",
          "form": "설마 애기 이름이 라온이에요?",
          "original_form": "설마 애기 이름이 라온이에요?",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044621.1.14",
          "form": "앗 맞아요!!",
          "original_form": "앗 맞아요!!",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044621.1.15",
          "form": "저희 고양이 이름에서 닉 따왔어요 ㅋㅋㅋㅋ",
          "original_form": "저희 고양이 이름에서 닉 따왔어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044621.1.16",
          "form": "설명해주신거랑 이름이 비슷한 분위기에요ㅠ 귀여워요",
          "original_form": "설명해주신거랑 이름이 비슷한 분위기에요ㅠ 귀여워요",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044621.1.17",
          "form": "흰양말 우유 퐁당 말고는 뮤슨색이에요?",
          "original_form": "흰양말 우유 퐁당 말고는 뮤슨색이에요?",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044621.1.18",
          "form": "치즈냥인데 노란색보단 갈색에 가까운 치즈에요!",
          "original_form": "치즈냥인데 노란색보단 갈색에 가까운 치즈에요!",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044621.1.19",
          "form": "그래서 무늬도 진한 갈색으로 잡혀있어요 ㅋㅋㅋ",
          "original_form": "그래서 무늬도 진한 갈색으로 잡혀있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044621.1.20",
          "form": "ㅠㅠㅠ귀여워요... 눈 색은 무슨 색인가요?",
          "original_form": "ㅠㅠㅠ귀여워요... 눈 색은 무슨 색인가요?",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044621.1.21",
          "form": "귀여워요ㅠㅠㅠ 눈색은 어떤색이에요?",
          "original_form": "귀여워요ㅠㅠㅠ 눈색은 어떤색이에요?",
          "speaker_id": "1",
          "time": "20210907 23:42"
        }
      ]
