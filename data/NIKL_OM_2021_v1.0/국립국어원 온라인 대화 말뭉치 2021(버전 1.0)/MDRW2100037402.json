      "utterance": [
        {
          "id": "MDRW2100037402.1.1",
          "form": "개학 축하해",
          "original_form": "개학 축하해",
          "speaker_id": "2",
          "time": "20210831 23:17"
        },
        {
          "id": "MDRW2100037402.1.2",
          "form": "오늘 학교 가?",
          "original_form": "오늘 학교 가?",
          "speaker_id": "2",
          "time": "20210831 23:17"
        },
        {
          "id": "MDRW2100037402.1.3",
          "form": "ㅠㅠ너무 싫다 너는 시험공부할때 어디서 주로해?",
          "original_form": "ㅠㅠ너무 싫다 너는 시험공부할때 어디서 주로해?",
          "speaker_id": "1",
          "time": "20210831 23:17"
        },
        {
          "id": "MDRW2100037402.1.4",
          "form": "응 수업이 하나 있어",
          "original_form": "응 수업이 하나 있어",
          "speaker_id": "1",
          "time": "20210831 23:18"
        },
        {
          "id": "MDRW2100037402.1.5",
          "form": "나는 어디서든 하지",
          "original_form": "나는 어디서든 하지",
          "speaker_id": "2",
          "time": "20210831 23:18"
        },
        {
          "id": "MDRW2100037402.1.6",
          "form": "위치가 중요한 게 아냐",
          "original_form": "위치가 중요한 게 아냐",
          "speaker_id": "2",
          "time": "20210831 23:18"
        },
        {
          "id": "MDRW2100037402.1.7",
          "form": "도서관이랑 카페중에는 어디를 선호해?",
          "original_form": "도서관이랑 카페중에는 어디를 선호해?",
          "speaker_id": "1",
          "time": "20210831 23:18"
        },
        {
          "id": "MDRW2100037402.1.8",
          "form": "도서관에서 하다가 질리면 카페에 가",
          "original_form": "도서관에서 하다가 질리면 카페에 가",
          "speaker_id": "2",
          "time": "20210831 23:18"
        },
        {
          "id": "MDRW2100037402.1.9",
          "form": "기분전환이 돼",
          "original_form": "기분전환이 돼",
          "speaker_id": "2",
          "time": "20210831 23:18"
        },
        {
          "id": "MDRW2100037402.1.10",
          "form": "꼭 추천할게",
          "original_form": "꼭 추천할게",
          "speaker_id": "2",
          "time": "20210831 23:18"
        },
        {
          "id": "MDRW2100037402.1.11",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210831 23:18"
        },
        {
          "id": "MDRW2100037402.1.12",
          "form": "올해는 알찬 학교생활을 할거야?",
          "original_form": "올해는 알찬 학교생활을 할거야?",
          "speaker_id": "2",
          "time": "20210831 23:18"
        },
        {
          "id": "MDRW2100037402.1.13",
          "form": "근데 내일 수업이 하나뿐인거야?",
          "original_form": "근데 내일 수업이 하나뿐인거야?",
          "speaker_id": "2",
          "time": "20210831 23:18"
        },
        {
          "id": "MDRW2100037402.1.14",
          "form": "흠 그건 미래의 내가 할 일이야",
          "original_form": "흠 그건 미래의 내가 할 일이야",
          "speaker_id": "1",
          "time": "20210831 23:18"
        },
        {
          "id": "MDRW2100037402.1.15",
          "form": "아쉽다...",
          "original_form": "아쉽다...",
          "speaker_id": "2",
          "time": "20210831 23:18"
        },
        {
          "id": "MDRW2100037402.1.16",
          "form": "응 근데 나머지시간에도 할일은 있어",
          "original_form": "응 근데 나머지시간에도 할일은 있어",
          "speaker_id": "1",
          "time": "20210831 23:18"
        },
        {
          "id": "MDRW2100037402.1.17",
          "form": "좀 더 수업계획을 열심히 짜봐!",
          "original_form": "좀 더 수업계획을 열심히 짜봐!",
          "speaker_id": "2",
          "time": "20210831 23:19"
        },
        {
          "id": "MDRW2100037402.1.18",
          "form": "응 ㅠㅠ 그런데 벌써 싫다",
          "original_form": "응 ㅠㅠ 그런데 벌써 싫다",
          "speaker_id": "1",
          "time": "20210831 23:19"
        },
        {
          "id": "MDRW2100037402.1.19",
          "form": "학생이 학교와 수업에 전념해야지.",
          "original_form": "학생이 학교와 수업에 전념해야지.",
          "speaker_id": "2",
          "time": "20210831 23:19"
        }
      ]
