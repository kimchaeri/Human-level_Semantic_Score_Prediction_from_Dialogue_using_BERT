      "utterance": [
        {
          "id": "MDRW2100045693.1.1",
          "form": "name1님 요리 잘하세요?",
          "original_form": "&name1&님 요리 잘하세요?",
          "speaker_id": "2",
          "time": "20210908 05:32"
        },
        {
          "id": "MDRW2100045693.1.2",
          "form": "빠른 음식얘기",
          "original_form": "빠른 음식얘기",
          "speaker_id": "1",
          "time": "20210908 05:32"
        },
        {
          "id": "MDRW2100045693.1.3",
          "form": "전 한식같은거 못하고",
          "original_form": "전 한식같은거 못하고",
          "speaker_id": "1",
          "time": "20210908 05:32"
        },
        {
          "id": "MDRW2100045693.1.4",
          "form": "적당히 가끔 따라하는건 어떻게든해욬ㅋㅋㅋ",
          "original_form": "적당히 가끔 따라하는건 어떻게든해욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:32"
        },
        {
          "id": "MDRW2100045693.1.5",
          "form": "어제 그릭복숭아 해먹었어요",
          "original_form": "어제 그릭복숭아 해먹었어요",
          "speaker_id": "1",
          "time": "20210908 05:32"
        },
        {
          "id": "MDRW2100045693.1.6",
          "form": "오 복숭아 안에 그릭요거트 넣는거요?",
          "original_form": "오 복숭아 안에 그릭요거트 넣는거요?",
          "speaker_id": "2",
          "time": "20210908 05:33"
        },
        {
          "id": "MDRW2100045693.1.7",
          "form": "넵 기술적으로 넣진 못하고",
          "original_form": "넵 기술적으로 넣진 못하고",
          "speaker_id": "1",
          "time": "20210908 05:34"
        },
        {
          "id": "MDRW2100045693.1.8",
          "form": "복숭아 송송썰어서 그릭요거트 얹고 그래놀라 투척!",
          "original_form": "복숭아 송송썰어서 그릭요거트 얹고 그래놀라 투척!",
          "speaker_id": "1",
          "time": "20210908 05:34"
        },
        {
          "id": "MDRW2100045693.1.9",
          "form": "시럽도 뿌리면 완성!",
          "original_form": "시럽도 뿌리면 완성!",
          "speaker_id": "1",
          "time": "20210908 05:34"
        },
        {
          "id": "MDRW2100045693.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ 무슨 시럽이에요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 무슨 시럽이에요?",
          "speaker_id": "2",
          "time": "20210908 05:36"
        },
        {
          "id": "MDRW2100045693.1.11",
          "form": "메이플시럽넣었는데",
          "original_form": "메이플시럽넣었는데",
          "speaker_id": "1",
          "time": "20210908 05:37"
        },
        {
          "id": "MDRW2100045693.1.12",
          "form": "올리고당같은것도 괜찮을 것 같아요!",
          "original_form": "올리고당같은것도 괜찮을 것 같아요!",
          "speaker_id": "1",
          "time": "20210908 05:37"
        },
        {
          "id": "MDRW2100045693.1.13",
          "form": "엄청 부지런하시네요",
          "original_form": "엄청 부지런하시네요",
          "speaker_id": "2",
          "time": "20210908 05:38"
        },
        {
          "id": "MDRW2100045693.1.14",
          "form": "저는 귀찮아서 못할거같아요ㅠㅋㅋㅋㅋㅋ",
          "original_form": "저는 귀찮아서 못할거같아요ㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:39"
        },
        {
          "id": "MDRW2100045693.1.15",
          "form": "누가 해주면 받아먹을줄이나 알지ㅠ 게을러요",
          "original_form": "누가 해주면 받아먹을줄이나 알지ㅠ 게을러요",
          "speaker_id": "2",
          "time": "20210908 05:39"
        },
        {
          "id": "MDRW2100045693.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ받아먹고 칭찬해주는것도 능력이에요 사실",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ받아먹고 칭찬해주는것도 능력이에요 사실",
          "speaker_id": "1",
          "time": "20210908 05:39"
        },
        {
          "id": "MDRW2100045693.1.17",
          "form": "저는 그걸 잘 못해섴ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 그걸 잘 못해섴ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:39"
        },
        {
          "id": "MDRW2100045693.1.18",
          "form": "아이고... 이렇게 좋은말씀을..ㅋㅋㅋㅋ 훈훈허네요 감사합니다",
          "original_form": "아이고... 이렇게 좋은말씀을..ㅋㅋㅋㅋ 훈훈허네요 감사합니다",
          "speaker_id": "2",
          "time": "20210908 05:43"
        },
        {
          "id": "MDRW2100045693.1.19",
          "form": "복숭아는 맛있었어요?",
          "original_form": "복숭아는 맛있었어요?",
          "speaker_id": "2",
          "time": "20210908 05:43"
        },
        {
          "id": "MDRW2100045693.1.20",
          "form": "원래 단 복숭아였겠죠?",
          "original_form": "원래 단 복숭아였겠죠?",
          "speaker_id": "2",
          "time": "20210908 05:43"
        },
        {
          "id": "MDRW2100045693.1.21",
          "form": "넵 황도복숭아!!",
          "original_form": "넵 황도복숭아!!",
          "speaker_id": "1",
          "time": "20210908 05:44"
        },
        {
          "id": "MDRW2100045693.1.22",
          "form": "노란 물복 너무 맛있었어요!!!",
          "original_form": "노란 물복 너무 맛있었어요!!!",
          "speaker_id": "1",
          "time": "20210908 05:44"
        },
        {
          "id": "MDRW2100045693.1.23",
          "form": "딱복도 좋아하지만 그릭복숭아엔 역시 물복인 것 같아욬ㅋㅋㅋㅋ",
          "original_form": "딱복도 좋아하지만 그릭복숭아엔 역시 물복인 것 같아욬ㅋㅋㅋㅋ",
