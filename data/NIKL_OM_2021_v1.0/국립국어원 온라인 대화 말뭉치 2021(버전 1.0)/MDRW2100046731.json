        {
          "id": "MDRW2100046731.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211105 00:06"
        },
        {
          "id": "MDRW2100046731.1.2",
          "form": "안녕하세요! 점심 먹고 올게요! 맛점하세요!",
          "original_form": "안녕하세요! 점심 먹고 올게요! 맛점하세요!",
          "speaker_id": "2",
          "time": "20211105 00:14"
        },
        {
          "id": "MDRW2100046731.1.3",
          "form": "게임 좋아하시나요?",
          "original_form": "게임 좋아하시나요?",
          "speaker_id": "2",
          "time": "20211104 13:18"
        },
        {
          "id": "MDRW2100046731.1.4",
          "form": "네 매일해요^^",
          "original_form": "네 매일해요^^",
          "speaker_id": "1",
          "time": "20211104 13:19"
        },
        {
          "id": "MDRW2100046731.1.5",
          "form": "무슨 게임 하세요? ㅎㅎ",
          "original_form": "무슨 게임 하세요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 13:23"
        },
        {
          "id": "MDRW2100046731.1.6",
          "form": "리니지요",
          "original_form": "리니지요",
          "speaker_id": "1",
          "time": "20211104 13:24"
        },
        {
          "id": "MDRW2100046731.1.7",
          "form": "리니지가 rpg였죠?",
          "original_form": "리니지가 rpg였죠?",
          "speaker_id": "2",
          "time": "20211104 13:28"
        },
        {
          "id": "MDRW2100046731.1.8",
          "form": "혹시 현질도 하시나요 ㅋㅋㅋ",
          "original_form": "혹시 현질도 하시나요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 13:28"
        },
        {
          "id": "MDRW2100046731.1.9",
          "form": "안해요 ㅎㅎ현질은 돈이 아까워요",
          "original_form": "안해요 ㅎㅎ현질은 돈이 아까워요",
          "speaker_id": "1",
          "time": "20211104 13:29"
        },
        {
          "id": "MDRW2100046731.1.10",
          "form": "현질은 안하시는군요!",
          "original_form": "현질은 안하시는군요!",
          "speaker_id": "2",
          "time": "20211104 13:40"
        },
        {
          "id": "MDRW2100046731.1.11",
          "form": "저도 현질은 진짜 안하는 편인데",
          "original_form": "저도 현질은 진짜 안하는 편인데",
          "speaker_id": "2",
          "time": "20211104 13:40"
        },
        {
          "id": "MDRW2100046731.1.12",
          "form": "메이플 하니까 현질하고 싶더라구요 ㅋㅋㅋ",
          "original_form": "메이플 하니까 현질하고 싶더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 13:40"
        },
        {
          "id": "MDRW2100046731.1.13",
          "form": "메이플 하시는군요",
          "original_form": "메이플 하시는군요",
          "speaker_id": "1",
          "time": "20211104 13:41"
        },
        {
          "id": "MDRW2100046731.1.14",
          "form": "메이플 재밌나요?",
          "original_form": "메이플 재밌나요?",
          "speaker_id": "1",
          "time": "20211104 13:41"
        },
        {
          "id": "MDRW2100046731.1.15",
          "form": "재미가 딱히 있는건 아닌데",
          "original_form": "재미가 딱히 있는건 아닌데",
          "speaker_id": "2",
          "time": "20211104 13:41"
        },
        {
          "id": "MDRW2100046731.1.16",
          "form": "묘하게 매력이 있어요 ㅋㅋㅋㅋ",
          "original_form": "묘하게 매력이 있어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 13:41"
        },
        {
          "id": "MDRW2100046731.1.17",
          "form": "그렇군요 메이플은 어떤게임인가요?",
          "original_form": "그렇군요 메이플은 어떤게임인가요?",
          "speaker_id": "1",
          "time": "20211104 13:42"
        },
        {
          "id": "MDRW2100046731.1.18",
          "form": "메이플도 따지자면 rpg 쪽이에요!",
          "original_form": "메이플도 따지자면 rpg 쪽이에요!",
          "speaker_id": "2",
          "time": "20211104 13:43"
        },
        {
          "id": "MDRW2100046731.1.19",
          "form": "렙업하고 아이템 끼고 ㅋㅋㅋ",
