      "utterance": [
        {
          "id": "MDRW2100038466.1.1",
          "form": "징그러운거",
          "original_form": "징그러운거",
          "speaker_id": "2",
          "time": "20210902 13:56"
        },
        {
          "id": "MDRW2100038466.1.2",
          "form": "근데 번개 결계도 있고",
          "original_form": "근데 번개 결계도 있고",
          "speaker_id": "2",
          "time": "20210902 13:56"
        },
        {
          "id": "MDRW2100038466.1.3",
          "form": "자세히 안봤어....",
          "original_form": "자세히 안봤어....",
          "speaker_id": "2",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100038466.1.4",
          "form": "해파리 같은거?",
          "original_form": "해파리 같은거?",
          "speaker_id": "1",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100038466.1.5",
          "form": "ㅇㅇ그런 애들",
          "original_form": "ㅇㅇ그런 애들",
          "speaker_id": "2",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100038466.1.6",
          "form": "그리고 새로운 몹 있다",
          "original_form": "그리고 새로운 몹 있다",
          "speaker_id": "2",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100038466.1.7",
          "form": "걔넨 이상하게 생긴건 아닌데",
          "original_form": "걔넨 이상하게 생긴건 아닌데",
          "speaker_id": "2",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100038466.1.8",
          "form": "섬 분위기가 워낙 싸해서",
          "original_form": "섬 분위기가 워낙 싸해서",
          "speaker_id": "2",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100038466.1.9",
          "form": "묘해",
          "original_form": "묘해",
          "speaker_id": "2",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100038466.1.10",
          "form": "ㅇㅎ 난 와타츠미 섬에서",
          "original_form": "ㅇㅎ 난 와타츠미 섬에서",
          "speaker_id": "1",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100038466.1.11",
          "form": "웬 물풍선?같이 생긴 몹 봤는데",
          "original_form": "웬 물풍선?같이 생긴 몹 봤는데",
          "speaker_id": "1",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100038466.1.12",
          "form": "어 그거 맞을듯?",
          "original_form": "어 그거 맞을듯?",
          "speaker_id": "2",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100038466.1.13",
          "form": "강공격 안하면",
          "original_form": "강공격 안하면",
          "speaker_id": "1",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100038466.1.14",
          "form": "오히려 체력이 회복되더라;;",
          "original_form": "오히려 체력이 회복되더라;;",
          "speaker_id": "1",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100038466.1.15",
          "form": "그 원소 충전되면",
          "original_form": "그 원소 충전되면",
          "speaker_id": "2",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100038466.1.16",
          "form": "더 강해지고 더 크게 터진다",
          "original_form": "더 강해지고 더 크게 터진다",
          "speaker_id": "2",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100038466.1.17",
          "form": "ㅇㅇㅇ 걔 맞아",
          "original_form": "ㅇㅇㅇ 걔 맞아",
          "speaker_id": "1",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100038466.1.18",
          "form": "뭐 이런ㅈ설명",
          "original_form": "뭐 이런ㅈ설명",
          "speaker_id": "2",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100038466.1.19",
          "form": "ㅇㅇ그럼 걔야",
          "original_form": "ㅇㅇ그럼 걔야",
          "speaker_id": "2",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100038466.1.20",
          "form": "근데 바위타입도 있어",
          "original_form": "근데 바위타입도 있어",
          "speaker_id": "2",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100038466.1.21",
          "form": "아 그래???",
          "original_form": "아 그래???",
          "speaker_id": "1",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100038466.1.22",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100038466.1.23",
          "form": "바위는 진짜 골때릴거같은데",
          "original_form": "바위는 진짜 골때릴거같은데",
          "speaker_id": "1",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100038466.1.24",
          "form": "내가 때려보진 않고",
          "original_form": "내가 때려보진 않고",
          "speaker_id": "2",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100038466.1.25",
          "form": "급히 마늘 수급만 하고 튀어서",
          "original_form": "급히 마늘 수급만 하고 튀어서",
          "speaker_id": "2",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100038466.1.26",
          "form": "잘은 모르겠지만",
          "original_form": "잘은 모르겠지만",
          "speaker_id": "2",
          "time": "20210902 14:00"
        },
        {
          "id": "MDRW2100038466.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:11"
        },
        {
          "id": "MDRW2100038466.1.28",
          "form": "난 세이라이섬은 팁대로",
          "original_form": "난 세이라이섬은 팁대로",
          "speaker_id": "1",
          "time": "20210902 14:11"
        },
        {
          "id": "MDRW2100038466.1.29",
          "form": "모험가 퀘스트 다 끝낸담에 가려고...",
          "original_form": "모험가 퀘스트 다 끝낸담에 가려고...",
          "speaker_id": "1",
          "time": "20210902 14:11"
        },
        {
          "id": "MDRW2100038466.1.30",
          "form": "번개 또맞긴 싫어...",
          "original_form": "번개 또맞긴 싫어...",
          "speaker_id": "1",
          "time": "20210902 14:11"
        },
        {
          "id": "MDRW2100038466.1.31",
          "form": "아니 난",
          "original_form": "아니 난",
          "speaker_id": "2",
          "time": "20210902 14:11"
        },
        {
          "id": "MDRW2100038466.1.32",
          "form": "라이덴이 갑자기",
          "original_form": "라이덴이 갑자기",
          "speaker_id": "2",
          "time": "20210902 14:11"
        },
        {
          "id": "MDRW2100038466.1.33",
          "form": "나와서",
          "original_form": "나와서",
          "speaker_id": "2",
          "time": "20210902 14:11"
        },
        {
          "id": "MDRW2100038466.1.34",
          "form": "80연인데 나온거라",
          "original_form": "80연인데 나온거라",
          "speaker_id": "2",
          "time": "20210902 14:12"
        }
      ]
