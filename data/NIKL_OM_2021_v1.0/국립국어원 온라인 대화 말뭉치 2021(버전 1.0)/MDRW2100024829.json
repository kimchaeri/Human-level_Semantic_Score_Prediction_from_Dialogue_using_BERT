      "utterance": [
        {
          "id": "MDRW2100024829.1.1",
          "form": "오늘 뭐먹었어?",
          "original_form": "오늘 뭐먹었어?",
          "speaker_id": "1",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100024829.1.2",
          "form": "오늘 꼬기랑 메론",
          "original_form": "오늘 꼬기랑 메론",
          "speaker_id": "2",
          "time": "20210902 19:09"
        },
        {
          "id": "MDRW2100024829.1.3",
          "form": "고기? 무슨 고기?",
          "original_form": "고기? 무슨 고기?",
          "speaker_id": "1",
          "time": "20210902 19:11"
        },
        {
          "id": "MDRW2100024829.1.4",
          "form": "오리고기!",
          "original_form": "오리고기!",
          "speaker_id": "2",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100024829.1.5",
          "form": "헐 머스타드 찍어서 먹었어?",
          "original_form": "헐 머스타드 찍어서 먹었어?",
          "speaker_id": "1",
          "time": "20210902 19:13"
        },
        {
          "id": "MDRW2100024829.1.6",
          "form": "아니아니 볶은거나와서 그냥 먹었지",
          "original_form": "아니아니 볶은거나와서 그냥 먹었지",
          "speaker_id": "2",
          "time": "20210902 19:15"
        },
        {
          "id": "MDRW2100024829.1.7",
          "form": "아하 나도 이따 고기 먹을거야",
          "original_form": "아하 나도 이따 고기 먹을거야",
          "speaker_id": "1",
          "time": "20210902 19:17"
        },
        {
          "id": "MDRW2100024829.1.8",
          "form": "뭐먹어",
          "original_form": "뭐먹어",
          "speaker_id": "2",
          "time": "20210902 19:18"
        },
        {
          "id": "MDRW2100024829.1.9",
          "form": "한우 쌈싸먹어야지",
          "original_form": "한우 쌈싸먹어야지",
          "speaker_id": "1",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100024829.1.10",
          "form": "좋은거 먹는다",
          "original_form": "좋은거 먹는다",
          "speaker_id": "2",
          "time": "20210902 19:22"
        },
        {
          "id": "MDRW2100024829.1.11",
          "form": "좋겠더",
          "original_form": "좋겠더",
          "speaker_id": "2",
          "time": "20210902 19:22"
        },
        {
          "id": "MDRW2100024829.1.12",
          "form": "다",
          "original_form": "다",
          "speaker_id": "2",
          "time": "20210902 19:22"
        },
        {
          "id": "MDRW2100024829.1.13",
          "form": "다먹고 복숭아 먹을거야",
          "original_form": "다먹고 복숭아 먹을거야",
          "speaker_id": "1",
          "time": "20210902 19:23"
        },
        {
          "id": "MDRW2100024829.1.14",
          "form": "너는 물복파야 딱복파야?",
          "original_form": "너는 물복파야 딱복파야?",
          "speaker_id": "1",
          "time": "20210902 19:23"
        },
        {
          "id": "MDRW2100024829.1.15",
          "form": "나 둘다좋아",
          "original_form": "나 둘다좋아",
          "speaker_id": "2",
          "time": "20210902 19:23"
        },
        {
          "id": "MDRW2100024829.1.16",
          "form": "다잘먹거든",
          "original_form": "다잘먹거든",
          "speaker_id": "2",
          "time": "20210902 19:23"
        },
        {
          "id": "MDRW2100024829.1.17",
          "form": "넌 뭔데?",
          "original_form": "넌 뭔데?",
          "speaker_id": "2",
          "time": "20210902 19:23"
        },
        {
          "id": "MDRW2100024829.1.18",
          "form": "나는 물복파라서 물렁물렁 복숭아 한접시 먹을거야",
          "original_form": "나는 물복파라서 물렁물렁 복숭아 한접시 먹을거야",
          "speaker_id": "1",
          "time": "20210902 19:23"
        },
        {
          "id": "MDRW2100024829.1.19",
          "form": "우아 많이먹는다 돼지네",
          "original_form": "우아 많이먹는다 돼지네",
          "speaker_id": "2",
          "time": "20210902 19:24"
        },
        {
          "id": "MDRW2100024829.1.20",
          "form": "돼지는 아닌데 맛있잖아",
          "original_form": "돼지는 아닌데 맛있잖아",
          "speaker_id": "1",
          "time": "20210902 19:25"
        },
        {
          "id": "MDRW2100024829.1.21",
          "form": "그건 맞지 근데 또 너무 물이면안돼 어느정도 물복숭아여야대",
          "original_form": "그건 맞지 근데 또 너무 물이면안돼 어느정도 물복숭아여야대",
          "speaker_id": "2",
          "time": "20210902 19:33"
        },
        {
          "id": "MDRW2100024829.1.22",
          "form": "적당한 복숭아야!",
          "original_form": "적당한 복숭아야!",
          "speaker_id": "1",
          "time": "20210902 19:35"
        },
        {
          "id": "MDRW2100024829.1.23",
          "form": "맛있어",
          "original_form": "맛있어",
          "speaker_id": "1",
          "time": "20210902 19:35"
        },
        {
          "id": "MDRW2100024829.1.24",
          "form": "그치그치",
          "original_form": "그치그치",
          "speaker_id": "2",
          "time": "20210902 19:39"
        },
        {
          "id": "MDRW2100024829.1.25",
          "form": "음식 뭐좋아해",
          "original_form": "음식 뭐좋아해",
          "speaker_id": "2",
          "time": "20210902 19:39"
        },
        {
          "id": "MDRW2100024829.1.26",
          "form": "나는 떡볶이가 제일 좋아",
          "original_form": "나는 떡볶이가 제일 좋아",
          "speaker_id": "1",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100024829.1.27",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210902 19:49"
        },
        {
          "id": "MDRW2100024829.1.28",
          "form": "난 돈가스 치킨",
          "original_form": "난 돈가스 치킨",
          "speaker_id": "2",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100024829.1.29",
          "form": "떡볶이 싫어하는 여자없고 돈가스 싫어하는 남자 없다는 말이 진짠가보네",
          "original_form": "떡볶이 싫어하는 여자없고 돈가스 싫어하는 남자 없다는 말이 진짠가보네",
          "speaker_id": "1",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100024829.1.30",
          "form": "맞지 짱이다 짱",
          "original_form": "맞지 짱이다 짱",
          "speaker_id": "2",
          "time": "20210902 19:52"
        }
      ]
    }
  ]
