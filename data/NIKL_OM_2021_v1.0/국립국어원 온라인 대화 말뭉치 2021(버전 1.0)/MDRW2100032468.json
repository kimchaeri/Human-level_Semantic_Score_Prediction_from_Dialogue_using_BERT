        {
          "id": "MDRW2100032468.1.1",
          "form": "김치랑 참치랑 진미채볶음",
          "original_form": "김치랑 참치랑 진미채볶음",
          "speaker_id": "2",
          "time": "20210905 18:41"
        },
        {
          "id": "MDRW2100032468.1.2",
          "form": "나는 냉장고에 지금 반찬이 별로 없어 ㅎㅎ",
          "original_form": "나는 냉장고에 지금 반찬이 별로 없어 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 18:42"
        },
        {
          "id": "MDRW2100032468.1.3",
          "form": "나도 별로 없어 ㅋㅋ",
          "original_form": "나도 별로 없어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:42"
        },
        {
          "id": "MDRW2100032468.1.4",
          "form": "요즘 맨날 시켜 먹어서",
          "original_form": "요즘 맨날 시켜 먹어서",
          "speaker_id": "1",
          "time": "20210905 18:42"
        },
        {
          "id": "MDRW2100032468.1.5",
          "form": "최근에 뭐 시켜먹었어?",
          "original_form": "최근에 뭐 시켜먹었어?",
          "speaker_id": "2",
          "time": "20210905 18:42"
        },
        {
          "id": "MDRW2100032468.1.6",
          "form": "햄버거 치킨 피자",
          "original_form": "햄버거 치킨 피자",
          "speaker_id": "1",
          "time": "20210905 18:42"
        },
        {
          "id": "MDRW2100032468.1.7",
          "form": "햄버거 먹고 싶다",
          "original_form": "햄버거 먹고 싶다",
          "speaker_id": "2",
          "time": "20210905 18:43"
        },
        {
          "id": "MDRW2100032468.1.8",
          "form": "맘스터치 먹었는데",
          "original_form": "맘스터치 먹었는데",
          "speaker_id": "1",
          "time": "20210905 18:43"
        },
        {
          "id": "MDRW2100032468.1.9",
          "form": "무슨 메뉴 먹었는데?",
          "original_form": "무슨 메뉴 먹었는데?",
          "speaker_id": "2",
          "time": "20210905 18:43"
        },
        {
          "id": "MDRW2100032468.1.10",
          "form": "화이트갈릭 버거랑 싸이버거",
          "original_form": "화이트갈릭 버거랑 싸이버거",
          "speaker_id": "1",
          "time": "20210905 18:43"
        },
        {
          "id": "MDRW2100032468.1.11",
          "form": "맛있었겠다 거기 감튀 맛있지",
          "original_form": "맛있었겠다 거기 감튀 맛있지",
          "speaker_id": "2",
          "time": "20210905 18:44"
        },
        {
          "id": "MDRW2100032468.1.12",
          "form": "화이트 갈릭 버거 소스가 맛있어",
          "original_form": "화이트 갈릭 버거 소스가 맛있어",
          "speaker_id": "1",
          "time": "20210905 18:44"
        },
        {
          "id": "MDRW2100032468.1.13",
          "form": "거기 치킨도 매콤해서 맛있어",
          "original_form": "거기 치킨도 매콤해서 맛있어",
