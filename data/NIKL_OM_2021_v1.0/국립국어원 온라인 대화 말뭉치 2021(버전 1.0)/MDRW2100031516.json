      "utterance": [
        {
          "id": "MDRW2100031516.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210904 22:19"
        },
        {
          "id": "MDRW2100031516.1.2",
          "form": "저녁 뭐 드셨나요?",
          "original_form": "저녁 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210904 22:19"
        },
        {
          "id": "MDRW2100031516.1.3",
          "form": "저는 곱창 먹었어요",
          "original_form": "저는 곱창 먹었어요",
          "speaker_id": "1",
          "time": "20210904 22:20"
        },
        {
          "id": "MDRW2100031516.1.4",
          "form": "name2님은 뭐 드셨나요?",
          "original_form": "&name2&님은 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210904 22:20"
        },
        {
          "id": "MDRW2100031516.1.5",
          "form": "저는 깐풍기 먹었어요!",
          "original_form": "저는 깐풍기 먹었어요!",
          "speaker_id": "2",
          "time": "20210905 00:25"
        },
        {
          "id": "MDRW2100031516.1.6",
          "form": "곱창 좋아하시나요?",
          "original_form": "곱창 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210905 00:25"
        },
        {
          "id": "MDRW2100031516.1.7",
          "form": "네 곱창 좋아해요\r\n깐풍기는 직접 만드신건가요?",
          "original_form": "네 곱창 좋아해요\r\n깐풍기는 직접 만드신건가요?",
          "speaker_id": "1",
          "time": "20210905 00:29"
        },
        {
          "id": "MDRW2100031516.1.8",
          "form": "아뇨 배달 시켜먹었어요 ㅎㅎ",
          "original_form": "아뇨 배달 시켜먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 00:32"
        },
        {
          "id": "MDRW2100031516.1.9",
          "form": "소/돼지곱창 중 뭐 좋아하세요?",
          "original_form": "소/돼지곱창 중 뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 00:32"
        },
        {
          "id": "MDRW2100031516.1.10",
          "form": "저는 돼지요",
          "original_form": "저는 돼지요",
          "speaker_id": "1",
          "time": "20210905 00:34"
        },
        {
          "id": "MDRW2100031516.1.11",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210905 00:34"
        },
        {
          "id": "MDRW2100031516.1.12",
          "form": "전 사실 곱창 안 먹어봤어요 아직...ㅎㅎㅠ",
          "original_form": "전 사실 곱창 안 먹어봤어요 아직...ㅎㅎㅠ",
          "speaker_id": "2",
          "time": "20210905 00:43"
        },
        {
          "id": "MDRW2100031516.1.13",
          "form": "돼지곱창은 어떤가요?",
          "original_form": "돼지곱창은 어떤가요?",
          "speaker_id": "2",
          "time": "20210905 00:43"
        },
        {
          "id": "MDRW2100031516.1.14",
          "form": "사실 조금 질기고 기름먹는 맛이 느껴지기도 하는데 맛있을 때가 많아서 먹어요ㅋㅋㅋㅋ",
          "original_form": "사실 조금 질기고 기름먹는 맛이 느껴지기도 하는데 맛있을 때가 많아서 먹어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:44"
        },
        {
          "id": "MDRW2100031516.1.15",
          "form": "ㅋㅋㅋㅋㅋ그렇군요",
          "original_form": "ㅋㅋㅋㅋㅋ그렇군요",
          "speaker_id": "2",
          "time": "20210905 15:35"
        },
        {
          "id": "MDRW2100031516.1.16",
          "form": "나중에 한번 먹어 봐야겠어요.",
          "original_form": "나중에 한번 먹어 봐야겠어요.",
          "speaker_id": "2",
          "time": "20210905 15:35"
        },
        {
          "id": "MDRW2100031516.1.17",
          "form": "깐풍기 좋아하시면 중국음식은 대부분 다 좋아하시나요?",
          "original_form": "깐풍기 좋아하시면 중국음식은 대부분 다 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210905 15:38"
        },
        {
          "id": "MDRW2100031516.1.18",
          "form": "네! 별로 음식 가리는 건 없어요",
          "original_form": "네! 별로 음식 가리는 건 없어요",
          "speaker_id": "2",
          "time": "20210906 12:54"
        },
        {
          "id": "MDRW2100031516.1.19",
          "form": "중국음식 좋아하시나요?",
          "original_form": "중국음식 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210906 12:55"
        },
        {
          "id": "MDRW2100031516.1.20",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "1",
          "time": "20210906 13:40"
        },
        {
          "id": "MDRW2100031516.1.21",
          "form": "짜장면이랑 짬뽕 중에 뭘 더 좋아하시나요?",
          "original_form": "짜장면이랑 짬뽕 중에 뭘 더 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210906 13:41"
        },
        {
          "id": "MDRW2100031516.1.22",
