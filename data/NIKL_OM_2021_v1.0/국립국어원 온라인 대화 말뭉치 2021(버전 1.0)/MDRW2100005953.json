      "utterance": [
        {
          "id": "MDRW2100005953.1.1",
          "form": "어떻게글케 부지런하시죠?",
          "original_form": "어떻게글케 부지런하시죠?",
          "speaker_id": "1",
          "time": "20210907 17:26"
        },
        {
          "id": "MDRW2100005953.1.2",
          "form": "먹고 살기가 힘들어서 그런가봐요 ㅋㅋㅋ",
          "original_form": "먹고 살기가 힘들어서 그런가봐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:44"
        },
        {
          "id": "MDRW2100005953.1.3",
          "form": "보험도 근데 사실",
          "original_form": "보험도 근데 사실",
          "speaker_id": "2",
          "time": "20210907 17:45"
        },
        {
          "id": "MDRW2100005953.1.4",
          "form": "제꺼 보험 설계하려다가",
          "original_form": "제꺼 보험 설계하려다가",
          "speaker_id": "2",
          "time": "20210907 17:45"
        },
        {
          "id": "MDRW2100005953.1.5",
          "form": "이해가 안되어가지구, 그냥 이걸 내가.. 시험을 쳐서 자격딴담에 내가 하는게 낫겠다 라구 생각이 들어서",
          "original_form": "이해가 안되어가지구, 그냥 이걸 내가.. 시험을 쳐서 자격딴담에 내가 하는게 낫겠다 라구 생각이 들어서",
          "speaker_id": "2",
          "time": "20210907 17:45"
        },
        {
          "id": "MDRW2100005953.1.6",
          "form": "시작하게 되었던거거든용",
          "original_form": "시작하게 되었던거거든용",
          "speaker_id": "2",
          "time": "20210907 17:45"
        },
        {
          "id": "MDRW2100005953.1.7",
          "form": "재미를 느껴서 그런가봐요!",
          "original_form": "재미를 느껴서 그런가봐요!",
          "speaker_id": "2",
          "time": "20210907 17:45"
        },
        {
          "id": "MDRW2100005953.1.8",
          "form": "우와 ... 어케구하세여  고객은요",
          "original_form": "우와 ... 어케구하세여  고객은요",
          "speaker_id": "1",
          "time": "20210907 17:49"
        },
        {
          "id": "MDRW2100005953.1.9",
          "form": "아는 사람 통하거나~",
          "original_form": "아는 사람 통하거나~",
          "speaker_id": "2",
          "time": "20210907 17:50"
        },
        {
          "id": "MDRW2100005953.1.10",
          "form": "토스에 보험설계사용 앱이 따로 있어요. 그렇게 구하고있어요",
          "original_form": "토스에 보험설계사용 앱이 따로 있어요. 그렇게 구하고있어요",
          "speaker_id": "2",
          "time": "20210907 17:50"
        },
        {
          "id": "MDRW2100005953.1.11",
          "form": "앜ㅋㅋㅋㅋㅋ 신기하당",
          "original_form": "앜ㅋㅋㅋㅋㅋ 신기하당",
          "speaker_id": "1",
          "time": "20210907 17:51"
        },
        {
          "id": "MDRW2100005953.1.12",
          "form": "실비 드시려고 시작하셧나여??",
          "original_form": "실비 드시려고 시작하셧나여??",
          "speaker_id": "1",
          "time": "20210907 17:51"
        },
        {
          "id": "MDRW2100005953.1.13",
          "form": "네네 맞아요 ㅋㅋㅋ",
          "original_form": "네네 맞아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:52"
        },
        {
          "id": "MDRW2100005953.1.14",
          "form": "지금은 한달에 한두건 정도 계약하는것같아요",
          "original_form": "지금은 한달에 한두건 정도 계약하는것같아요",
          "speaker_id": "2",
          "time": "20210907 17:52"
        },
        {
          "id": "MDRW2100005953.1.15",
          "form": "진짜 완전 부업",
          "original_form": "진짜 완전 부업",
          "speaker_id": "2",
          "time": "20210907 17:52"
        },
        {
          "id": "MDRW2100005953.1.16",
          "form": "궁금한거 있으면 여쭤보셔욤! 진짜 하루종일도 가능",
          "original_form": "궁금한거 있으면 여쭤보셔욤! 진짜 하루종일도 가능",
          "speaker_id": "2",
          "time": "20210907 17:52"
        },
        {
          "id": "MDRW2100005953.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:52"
        },
        {
          "id": "MDRW2100005953.1.18",
          "form": "왘ㅋㅋㅋ 그건 근데 그럼 보험사에서 수수료를 받는거에요? ㅋㅋㅋㅋ",
          "original_form": "왘ㅋㅋㅋ 그건 근데 그럼 보험사에서 수수료를 받는거에요? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:53"
        },
        {
          "id": "MDRW2100005953.1.19",
          "form": "네네! 보험사마다 다르긴 한데",
          "original_form": "네네! 보험사마다 다르긴 한데",
          "speaker_id": "2",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100005953.1.20",
          "form": "계약금액의 %로 받구~",
          "original_form": "계약금액의 %로 받구~",
          "speaker_id": "2",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100005953.1.21",
          "form": "12~36개월 안에 그 사람이 해지를 하면",
          "original_form": "12~36개월 안에 그 사람이 해지를 하면",
          "speaker_id": "2",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100005953.1.22",
          "form": "받았던 금액을 비율대로 토해내야 해요",
          "original_form": "받았던 금액을 비율대로 토해내야 해요",
          "speaker_id": "2",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100005953.1.23",
          "form": "우왘ㅋㅋㅋㅋ  진짜 name2님은 혹시 mbti가 무엇이신욬ㅋㅋ",
          "original_form": "우왘ㅋㅋㅋㅋ  진짜 &name2&님은 혹시 mbti가 무엇이신욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:56"
        },
        {
          "id": "MDRW2100005953.1.24",
          "form": "저 다시 찾아보니깐",
          "original_form": "저 다시 찾아보니깐",
          "speaker_id": "2",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100005953.1.25",
          "form": "ENTJ 로 나왔던것같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "ENTJ 로 나왔던것같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100005953.1.26",
          "form": "ㅋㅋㅋㅋㅋ 쓰리잡 잘어울려요 ㅋㅋㅋ 지짜대단하심",
          "original_form": "ㅋㅋㅋㅋㅋ 쓰리잡 잘어울려요 ㅋㅋㅋ 지짜대단하심",
          "speaker_id": "1",
          "time": "20210907 18:03"
        },
        {
          "id": "MDRW2100005953.1.27",
          "form": "실비는 몇세대 드셧어요? ㅋㅋ",
          "original_form": "실비는 몇세대 드셧어요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:03"
        },
        {
          "id": "MDRW2100005953.1.28",
          "form": "저 2세대꺼 들었었는데",
          "original_form": "저 2세대꺼 들었었는데",
          "speaker_id": "2",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100005953.1.29",
          "form": "좀 잔뜩 타먹고...",
          "original_form": "좀 잔뜩 타먹고...",
          "speaker_id": "2",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100005953.1.30",
          "form": "3세대로 갈아탔어욤",
          "original_form": "3세대로 갈아탔어욤",
          "speaker_id": "2",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100005953.1.31",
          "form": "7월에 4세대가 나왔었으니",
          "original_form": "7월에 4세대가 나왔었으니",
          "speaker_id": "2",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100005953.1.32",
          "form": "그 이전에 호다닥",
          "original_form": "그 이전에 호다닥",
          "speaker_id": "2",
          "time": "20210907 18:05"
        },
        {
          "id": "MDRW2100005953.1.33",
          "form": "갈아타는게 조앗던거엿어요? ㅋㅋㅋ",
          "original_form": "갈아타는게 조앗던거엿어요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:07"
        },
        {
          "id": "MDRW2100005953.1.34",
          "form": "앞에거일수록 좋다들어는데",
          "original_form": "앞에거일수록 좋다들어는데",
          "speaker_id": "1",
          "time": "20210907 18:07"
        },
        {
          "id": "MDRW2100005953.1.35",
          "form": "좋긴 좋은데",
          "original_form": "좋긴 좋은데",
          "speaker_id": "2",
          "time": "20210907 18:11"
        },
        {
          "id": "MDRW2100005953.1.36",
          "form": "그 이전 실비들이 1년마다 갱신이 되는데",
          "original_form": "그 이전 실비들이 1년마다 갱신이 되는데",
          "speaker_id": "2",
          "time": "20210907 18:12"
        },
        {
          "id": "MDRW2100005953.1.37",
          "form": "1년마다 실비가 갱신되서 또 달라져요",
          "original_form": "1년마다 실비가 갱신되서 또 달라져요",
