      "utterance": [
        {
          "id": "MDRW2100036440.1.1",
          "form": "네.....지금 실내인데 비와요...",
          "original_form": "네.....지금 실내인데 비와요...",
          "speaker_id": "2",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100036440.1.2",
          "form": "ㅠ.^ ...... 복학할 날만 기다리며",
          "original_form": "ㅠ.^ ...... 복학할 날만 기다리며",
          "speaker_id": "1",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100036440.1.3",
          "form": "저희 학교 주변에는 마라탕 집만 3개거든요",
          "original_form": "저희 학교 주변에는 마라탕 집만 3개거든요",
          "speaker_id": "1",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100036440.1.4",
          "form": "맨날 다른 거 먹을 수 있단 말예요!!@",
          "original_form": "맨날 다른 거 먹을 수 있단 말예요!!@",
          "speaker_id": "1",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100036440.1.5",
          "form": "와 진짜 최곤데요....ㅠㅠㅠ다 조금씩 즈마장 량이 다른지 마라스런 맛이 다르잖아요",
          "original_form": "와 진짜 최곤데요....ㅠㅠㅠ다 조금씩 즈마장 량이 다른지 마라스런 맛이 다르잖아요",
          "speaker_id": "2",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100036440.1.6",
          "form": "꼬소한거는 여기 맵고 깔끔한건 저기~",
          "original_form": "꼬소한거는 여기 맵고 깔끔한건 저기~",
          "speaker_id": "2",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100036440.1.7",
          "form": "그르니까요! 심지어 하나는 사골 베이스라",
          "original_form": "그르니까요! 심지어 하나는 사골 베이스라",
          "speaker_id": "1",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100036440.1.8",
          "form": "해장하러 가면... ^.^",
          "original_form": "해장하러 가면... ^.^",
          "speaker_id": "1",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100036440.1.9",
          "form": "끝내줘요",
          "original_form": "끝내줘요",
          "speaker_id": "1",
          "time": "20210830 23:23"
        },
        {
          "id": "MDRW2100036440.1.10",
          "form": "크아...ㅠㅠㅠㅠㅠ",
          "original_form": "크아...ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100036440.1.11",
          "form": "아....진짜 너무 마라탕 고파요....",
          "original_form": "아....진짜 너무 마라탕 고파요....",
          "speaker_id": "2",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100036440.1.12",
          "form": "계란 볶음밥이랑 꿔바러우 같이 시켜서",
          "original_form": "계란 볶음밥이랑 꿔바러우 같이 시켜서",
          "speaker_id": "2",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100036440.1.13",
          "form": "와구와구 먹고 싶어요...",
          "original_form": "와구와구 먹고 싶어요...",
          "speaker_id": "2",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100036440.1.14",
          "form": "오마이갓 name2님 정막 정말 담번에",
          "original_form": "오마이갓 &name2&님 정막 정말 담번에",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036440.1.15",
          "form": "저랑 촨커 가요",
          "original_form": "저랑 촨커 가요",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036440.1.16",
          "form": "거기 계란볶음밥 진쩌",
          "original_form": "거기 계란볶음밥 진쩌",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036440.1.17",
          "form": "환상이에요",
          "original_form": "환상이에요",
          "speaker_id": "1",
          "time": "20210830 23:25"
        },
        {
          "id": "MDRW2100036440.1.18",
          "form": "와 진짜요???ㅠㅠㅠ하긴 꿔바러우 맛집인디 요리 기깔나게 잘하시겟군요ㅠㅠㅠ",
          "original_form": "와 진짜요???ㅠㅠㅠ하긴 꿔바러우 맛집인디 요리 기깔나게 잘하시겟군요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 23:27"
        },
        {
          "id": "MDRW2100036440.1.19",
          "form": "name2님 양고기도 드세요?",
          "original_form": "&name2&님 양고기도 드세요?",
          "speaker_id": "1",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100036440.1.20",
          "form": "거기 양꼬치도...",
          "original_form": "거기 양꼬치도...",
          "speaker_id": "1",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100036440.1.21",
          "form": "말해모해..",
          "original_form": "말해모해..",
          "speaker_id": "1",
          "time": "20210830 23:28"
        },
        {
          "id": "MDRW2100036440.1.22",
          "form": "와 진짜 양고기 디비져요...ㅠㅠ",
          "original_form": "와 진짜 양고기 디비져요...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100036440.1.23",
          "form": "마라탕은 소넣는걸 선호하긴하는데",
          "original_form": "마라탕은 소넣는걸 선호하긴하는데",
          "speaker_id": "2",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100036440.1.24",
          "form": "양꼬치 진심 없어서 못먹죠ㅠ",
          "original_form": "양꼬치 진심 없어서 못먹죠ㅠ",
          "speaker_id": "2",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100036440.1.25",
          "form": "와 진짜 중식의 천국 아니에요???",
          "original_form": "와 진짜 중식의 천국 아니에요???",
          "speaker_id": "2",
          "time": "20210830 23:30"
        },
        {
          "id": "MDRW2100036440.1.26",
          "form": "헐 저도 마라탕은 절대 소 양은 구워서 주의자입니닷",
          "original_form": "헐 저도 마라탕은 절대 소 양은 구워서 주의자입니닷",
          "speaker_id": "1",
          "time": "20210830 23:33"
        },
        {
          "id": "MDRW2100036440.1.27",
          "form": "맞아요 촨커 그",
          "original_form": "맞아요 촨커 그",
          "speaker_id": "1",
          "time": "20210830 23:33"
        },
        {
          "id": "MDRW2100036440.1.28",
          "form": "푸딘코? 그 사람이 홍보해서",
          "original_form": "푸딘코? 그 사람이 홍보해서",
          "speaker_id": "1",
          "time": "20210830 23:33"
        },
        {
          "id": "MDRW2100036440.1.29",
          "form": "엄청 유명해져서 ^...^ 정작 학교 학생들은 못 가는",
          "original_form": "엄청 유명해져서 ^...^ 정작 학교 학생들은 못 가는",
          "speaker_id": "1",
          "time": "20210830 23:33"
        },
        {
          "id": "MDRW2100036440.1.30",
          "form": "맛집이 됐어요",
          "original_form": "맛집이 됐어요",
          "speaker_id": "1",
          "time": "20210830 23:33"
        },
        {
          "id": "MDRW2100036440.1.31",
          "form": "근데 진짜 제 인생 맛집",
          "original_form": "근데 진짜 제 인생 맛집",
          "speaker_id": "1",
          "time": "20210830 23:34"
        },
        {
          "id": "MDRW2100036440.1.32",
          "form": "꼭 쳐보새요",
          "original_form": "꼭 쳐보새요",
          "speaker_id": "1",
          "time": "20210830 23:34"
        },
        {
          "id": "MDRW2100036440.1.33",
          "form": "아놔....ㅠㅠㅠ",
          "original_form": "아놔....ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 23:34"
        },
        {
          "id": "MDRW2100036440.1.34",
          "form": "꼭 가볼게여ㅠㅠ",
          "original_form": "꼭 가볼게여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 23:34"
        },
        {
          "id": "MDRW2100036440.1.35",
          "form": "푸딘코가 뭔지 모르겟지만 푸틴같이 기분 나쁜 네이밍이네요ㅠ",
          "original_form": "푸딘코가 뭔지 모르겟지만 푸틴같이 기분 나쁜 네이밍이네요ㅠ",
          "speaker_id": "2",
          "time": "20210830 23:34"
        },
        {
          "id": "MDRW2100036440.1.36",
          "form": "네 -.- 제 원수입니다요",
          "original_form": "네 -.- 제 원수입니다요",
          "speaker_id": "1",
          "time": "20210830 23:35"
        },
