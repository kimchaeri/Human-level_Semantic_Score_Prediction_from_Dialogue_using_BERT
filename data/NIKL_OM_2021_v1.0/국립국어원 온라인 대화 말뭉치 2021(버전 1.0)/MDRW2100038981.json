      "utterance": [
        {
          "id": "MDRW2100038981.1.1",
          "form": "아니어 하지만 2주후에 맞아요 ㅎㅎ",
          "original_form": "아니어 하지만 2주후에 맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100038981.1.2",
          "form": "저는 일차맞았어요",
          "original_form": "저는 일차맞았어요",
          "speaker_id": "1",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100038981.1.3",
          "form": "부작용은 없으셨나요? 요즘 부자용이 걱정돼요",
          "original_form": "부작용은 없으셨나요? 요즘 부자용이 걱정돼요",
          "speaker_id": "2",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100038981.1.4",
          "form": "저는 팔좀 뻐근한거 빼고는 괜찮았어요",
          "original_form": "저는 팔좀 뻐근한거 빼고는 괜찮았어요",
          "speaker_id": "1",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100038981.1.5",
          "form": "백신 어떤거 맞으세요?",
          "original_form": "백신 어떤거 맞으세요?",
          "speaker_id": "1",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100038981.1.6",
          "form": "전 화이자 또는 모더나로 아직도 나와있어요",
          "original_form": "전 화이자 또는 모더나로 아직도 나와있어요",
          "speaker_id": "2",
          "time": "20210903 00:13"
        },
        {
          "id": "MDRW2100038981.1.7",
          "form": "그래서 모르겠어요,, 아마 화이자아닐까요?",
          "original_form": "그래서 모르겠어요,, 아마 화이자아닐까요?",
          "speaker_id": "2",
          "time": "20210903 00:13"
        },
        {
          "id": "MDRW2100038981.1.8",
          "form": "아 며칠전에 알려주더라구요",
          "original_form": "아 며칠전에 알려주더라구요",
          "speaker_id": "1",
          "time": "20210903 00:13"
        },
        {
          "id": "MDRW2100038981.1.9",
          "form": "모더나 수급이 안되서 화이자일거 같아요",
          "original_form": "모더나 수급이 안되서 화이자일거 같아요",
          "speaker_id": "1",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100038981.1.10",
          "form": "전 아직 안나왔어요 name1님은 화이자였나요?",
          "original_form": "전 아직 안나왔어요 &name1&님은 화이자였나요?",
          "speaker_id": "2",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100038981.1.11",
          "form": "역시 그죠?",
          "original_form": "역시 그죠?",
          "speaker_id": "2",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100038981.1.12",
          "form": "전 화이자요 ㅎㅎ",
          "original_form": "전 화이자요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 00:14"
        },
        {
          "id": "MDRW2100038981.1.13",
          "form": "저도 부작용걱정했는데",
          "original_form": "저도 부작용걱정했는데",
          "speaker_id": "1",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100038981.1.14",
          "form": "괜찮더라구요",
          "original_form": "괜찮더라구요",
          "speaker_id": "1",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100038981.1.15",
          "form": "전 혹시 몰라서 생리기간 피해서 맞으려고 좀 뒤로 미뤘어요",
          "original_form": "전 혹시 몰라서 생리기간 피해서 맞으려고 좀 뒤로 미뤘어요",
          "speaker_id": "2",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100038981.1.16",
          "form": "그게 나을거같아요",
          "original_form": "그게 나을거같아요",
          "speaker_id": "1",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100038981.1.17",
          "form": "백신부작용에 생리불순도 있대요",
          "original_form": "백신부작용에 생리불순도 있대요",
          "speaker_id": "1",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100038981.1.18",
          "form": "네 글도 잠시 안나오는거면 괜찮은데 계속 피가 흐르는건 곤란해요",
          "original_form": "네 글도 잠시 안나오는거면 괜찮은데 계속 피가 흐르는건 곤란해요",
          "speaker_id": "2",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100038981.1.19",
          "form": "그래도",
          "original_form": "그래도",
          "speaker_id": "2",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100038981.1.20",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100038981.1.21",
          "form": "그래도 대부분 일시적이래요",
          "original_form": "그래도 대부분 일시적이래요",
          "speaker_id": "1",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100038981.1.22",
          "form": "ㅠㅠ 평생 하혈하는거면 진짜,, 곤란,,",
          "original_form": "ㅠㅠ 평생 하혈하는거면 진짜,, 곤란,,",
          "speaker_id": "2",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100038981.1.23",
          "form": "그건 진짜 국가소송감이죠ㅜ",
