      "utterance": [
        {
          "id": "MDRW2100024877.1.1",
          "form": "여행은 좋아하시나요?",
          "original_form": "여행은 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210831 20:09"
        },
        {
          "id": "MDRW2100024877.1.2",
          "form": "넼ㅋㅋ.조아해용",
          "original_form": "넼ㅋㅋ.조아해용",
          "speaker_id": "2",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100024877.1.3",
          "form": "혹시 추천해주실 여행지가 있으신가요?",
          "original_form": "혹시 추천해주실 여행지가 있으신가요?",
          "speaker_id": "1",
          "time": "20210831 21:23"
        },
        {
          "id": "MDRW2100024877.1.4",
          "form": "저는 제주도용..ㅋㅋ 제주도 한달 있고싶어요ㅠㅋㅋㅋ",
          "original_form": "저는 제주도용..ㅋㅋ 제주도 한달 있고싶어요ㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100024877.1.5",
          "form": "제주도!\r\n어디곳이 좋나요?",
          "original_form": "제주도!\r\n어디곳이 좋나요?",
          "speaker_id": "1",
          "time": "20210831 21:44"
        },
        {
          "id": "MDRW2100024877.1.6",
          "form": "ㅋㅋㅋㅋ 다 좋은거같아요 어딜가도...어디든 바다가 잇어서 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 다 좋은거같아요 어딜가도...어디든 바다가 잇어서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100024877.1.7",
          "form": "전 아직 한번 밖에 못가봤는데 우연히 성산일출봉 근처 바다에서",
          "original_form": "전 아직 한번 밖에 못가봤는데 우연히 성산일출봉 근처 바다에서",
          "speaker_id": "1",
          "time": "20210831 22:03"
        },
        {
          "id": "MDRW2100024877.1.8",
          "form": "돌고래봤어요ㅎㅅ",
          "original_form": "돌고래봤어요ㅎㅅ",
          "speaker_id": "1",
          "time": "20210831 22:03"
        },
        {
          "id": "MDRW2100024877.1.9",
          "form": "너무 좋더라구요~!",
          "original_form": "너무 좋더라구요~!",
          "speaker_id": "1",
          "time": "20210831 22:03"
        },
        {
          "id": "MDRW2100024877.1.10",
          "form": "헐 ..진짜조앗겟다ㅠㅠㅠㅠ",
          "original_form": "헐 ..진짜조앗겟다ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 22:07"
        },
        {
          "id": "MDRW2100024877.1.11",
          "form": "네!\r\n복권을 사야한다고 하던데 못사서 아쉽더라구요",
          "original_form": "네!\r\n복권을 사야한다고 하던데 못사서 아쉽더라구요",
          "speaker_id": "1",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100024877.1.12",
          "form": "그래도 너무 좋은 장면을 봐서ㅎㅎ",
          "original_form": "그래도 너무 좋은 장면을 봐서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100024877.1.13",
          "form": "제주도는 또 가보고 싶네요",
          "original_form": "제주도는 또 가보고 싶네요",
          "speaker_id": "1",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100024877.1.14",
          "form": "글게요 진짜 기억에 남겠어요",
          "original_form": "글게요 진짜 기억에 남겠어요",
          "speaker_id": "2",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100024877.1.15",
          "form": "요즘 제주도 넘 인기있어졌어요 코로나때문에 다들 해외를 못가니 ㅋㅋㅋ",
          "original_form": "요즘 제주도 넘 인기있어졌어요 코로나때문에 다들 해외를 못가니 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100024877.1.16",
          "form": "랜트카비용이...",
          "original_form": "랜트카비용이...",
          "speaker_id": "2",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100024877.1.17",
          "form": "제주도는 정말 너무 비싸요ㅜㅜ",
          "original_form": "제주도는 정말 너무 비싸요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100024877.1.18",
          "form": "그만큼 좋기도 하구요ㅜ",
          "original_form": "그만큼 좋기도 하구요ㅜ",
          "speaker_id": "1",
          "time": "20210831 22:18"
        },
        {
          "id": "MDRW2100024877.1.19",
          "form": "맞아용... 가고싶다당장 ㅠㅠ",
          "original_form": "맞아용... 가고싶다당장 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100024877.1.20",
          "form": "제주도에서 맛있는 음식은 드셨어요?",
          "original_form": "제주도에서 맛있는 음식은 드셨어요?",
          "speaker_id": "1",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100024877.1.21",
          "form": "아근데 생각보다...너무 관광에 치우친데가 많아그런가?",
          "original_form": "아근데 생각보다...너무 관광에 치우친데가 많아그런가?",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100024877.1.22",
          "form": "다 맛있진 않았어요",
          "original_form": "다 맛있진 않았어요",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100024877.1.23",
          "form": "그냥 흑돼지랑 전복죽 이런건 맛있더라구요 ㅋㅋㅋ",
          "original_form": "그냥 흑돼지랑 전복죽 이런건 맛있더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100024877.1.24",
          "form": "그쵸?\r\n저도 아직은 제주도에서 맛있는 음식을 못먹었어요ㅜㅜ",
          "original_form": "그쵸?\r\n저도 아직은 제주도에서 맛있는 음식을 못먹었어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 22:33"
        }
      ]
    }
