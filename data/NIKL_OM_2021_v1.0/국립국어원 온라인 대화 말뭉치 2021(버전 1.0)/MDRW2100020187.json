      "utterance": [
        {
          "id": "MDRW2100020187.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020187.1.2",
          "form": "최근에 스포츠 경기 본적 있으세요?",
          "original_form": "최근에 스포츠 경기 본적 있으세요?",
          "speaker_id": "1",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020187.1.3",
          "form": "그럼요 그럼요",
          "original_form": "그럼요 그럼요",
          "speaker_id": "2",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020187.1.4",
          "form": "얼마 전 올림픽에서",
          "original_form": "얼마 전 올림픽에서",
          "speaker_id": "2",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020187.1.5",
          "form": "양궁을 숨죽여 봤답니다",
          "original_form": "양궁을 숨죽여 봤답니다",
          "speaker_id": "2",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020187.1.6",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020187.1.7",
          "form": "앗 저두요!",
          "original_form": "앗 저두요!",
          "speaker_id": "1",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020187.1.8",
          "form": "양궁 경기 중에 기억에 남는거 있으세요?",
          "original_form": "양궁 경기 중에 기억에 남는거 있으세요?",
          "speaker_id": "1",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020187.1.9",
          "form": "기억에 남는 스포츠 선수라든지요 ㅎㅎ",
          "original_form": "기억에 남는 스포츠 선수라든지요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020187.1.10",
          "form": "저는 원래 관심이 없었는데",
          "original_form": "저는 원래 관심이 없었는데",
          "speaker_id": "2",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020187.1.11",
          "form": "안산 선수 때문애",
          "original_form": "안산 선수 때문애",
          "speaker_id": "2",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020187.1.12",
          "form": "진심이 되어버렸습니다",
          "original_form": "진심이 되어버렸습니다",
          "speaker_id": "2",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020187.1.13",
          "form": "어떻게 쏠 때마다",
          "original_form": "어떻게 쏠 때마다",
          "speaker_id": "2",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020187.1.14",
          "form": "10점인지",
          "original_form": "10점인지",
          "speaker_id": "2",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020187.1.15",
          "form": "어머 저두 그래요",
          "original_form": "어머 저두 그래요",
          "speaker_id": "1",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020187.1.16",
          "form": "살면서 양궁 본 적 손에 꼽는데 진짜 신기할 정도로 몰입되더라구요",
          "original_form": "살면서 양궁 본 적 손에 꼽는데 진짜 신기할 정도로 몰입되더라구요",
          "speaker_id": "1",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020187.1.17",
          "form": "저도요 저도요",
          "original_form": "저도요 저도요",
          "speaker_id": "2",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020187.1.18",
          "form": "그리고 안산 선수가",
          "original_form": "그리고 안산 선수가",
          "speaker_id": "2",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020187.1.19",
          "form": "지금 20살이라는게",
          "original_form": "지금 20살이라는게",
          "speaker_id": "2",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020187.1.20",
          "form": "21살인가,,",
          "original_form": "21살인가,,",
          "speaker_id": "2",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020187.1.21",
          "form": "정말 놀라워요... 언니라고 불러야할 것 같은데...",
          "original_form": "정말 놀라워요... 언니라고 불러야할 것 같은데...",
          "speaker_id": "2",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020187.1.22",
          "form": "팍팍한 현생에 이렇게 멋진 한국 여선수 보니까 어찌나 자랑스럽고 좋던지요 ㅎㅎㅎ",
          "original_form": "팍팍한 현생에 이렇게 멋진 한국 여선수 보니까 어찌나 자랑스럽고 좋던지요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020187.1.23",
          "form": "맞아요 진짜",
          "original_form": "맞아요 진짜",
          "speaker_id": "2",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020187.1.24",
          "form": "우리나라 올림픽 금메달",
          "original_form": "우리나라 올림픽 금메달",
          "speaker_id": "2",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020187.1.25",
          "form": "절반을 안산 선수가 땄잖아요",
          "original_form": "절반을 안산 선수가 땄잖아요",
          "speaker_id": "2",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020187.1.26",
          "form": "아 그리고 배구도",
          "original_form": "아 그리고 배구도",
          "speaker_id": "2",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020187.1.27",
          "form": "멋있으면 다 언니에요 ㅎㅎ\r\n이런 경기는 직관하러 갔으면 좋았을걸 싶어요 그죠",
          "original_form": "멋있으면 다 언니에요 ㅎㅎ\r\n이런 경기는 직관하러 갔으면 좋았을걸 싶어요 그죠",
          "speaker_id": "1",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020187.1.28",
          "form": "보지는 않았지만",
          "original_form": "보지는 않았지만",
          "speaker_id": "2",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020187.1.29",
          "form": "맞아요!!!",
          "original_form": "맞아요!!!",
          "speaker_id": "2",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020187.1.30",
          "form": "배구도 보진 않았지만",
          "original_form": "배구도 보진 않았지만",
          "speaker_id": "2",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020187.1.31",
          "form": "여자 배구를 그냥 배구로 하고",
          "original_form": "여자 배구를 그냥 배구로 하고",
          "speaker_id": "2",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020187.1.32",
          "form": "남자 배구를 남자 배구라고 하면 좋겠어요",
          "original_form": "남자 배구를 남자 배구라고 하면 좋겠어요",
          "speaker_id": "2",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020187.1.33",
          "form": "그러게 말이에요!",
          "original_form": "그러게 말이에요!",
          "speaker_id": "1",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020187.1.34",
          "form": "name2과 관심사도 비슷한것 같고",
          "original_form": "&name2&과 관심사도 비슷한것 같고",
          "speaker_id": "1",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020187.1.35",
          "form": "스포츠 경기에 대해 생각도 나눠봐서 넘 좋네요~",
          "original_form": "스포츠 경기에 대해 생각도 나눠봐서 넘 좋네요~",
          "speaker_id": "1",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020187.1.36",
          "form": "맞아요 앞으로도 대화 자주 합시다!",
          "original_form": "맞아요 앞으로도 대화 자주 합시다!",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020187.1.37",
          "form": "저도 이번 기회에",
          "original_form": "저도 이번 기회에",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020187.1.38",
          "form": "운동을 해야겠다는 생각이 들었어요",
          "original_form": "운동을 해야겠다는 생각이 들었어요",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020187.1.39",
          "form": "ㅎㅎ 좋아요 스포츠 보면서 \r\n운동해야겠단 멋진 생각이셔요!",
          "original_form": "ㅎㅎ 좋아요 스포츠 보면서 \r\n운동해야겠단 멋진 생각이셔요!",
          "speaker_id": "1",
          "time": "20210828 00:16"
        },
        {
          "id": "MDRW2100020187.1.40",
          "form": "저 예전애",
          "original_form": "저 예전애",
          "speaker_id": "2",
          "time": "20210828 00:17"
        },
        {
          "id": "MDRW2100020187.1.41",
          "form": "예전에 평창올림픽 때",
          "original_form": "예전에 평창올림픽 때",
          "speaker_id": "2",
          "time": "20210828 00:17"
        },
        {
          "id": "MDRW2100020187.1.42",
          "form": "패럴림픽 자원봉사활동으로",
          "original_form": "패럴림픽 자원봉사활동으로",
          "speaker_id": "2",
          "time": "20210828 00:17"
        },
        {
          "id": "MDRW2100020187.1.43",
          "form": "갈 수 있었는데",
          "original_form": "갈 수 있었는데",
          "speaker_id": "2",
          "time": "20210828 00:17"
        },
        {
          "id": "MDRW2100020187.1.44",
          "form": "휴학해야해서 안간거 정말 후회 중이에요",
          "original_form": "휴학해야해서 안간거 정말 후회 중이에요",
          "speaker_id": "2",
          "time": "20210828 00:17"
        },
        {
          "id": "MDRW2100020187.1.45",
          "form": "으아 안타까워요ㅠㅠ",
          "original_form": "으아 안타까워요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 00:18"
        },
        {
          "id": "MDRW2100020187.1.46",
          "form": "담에도 그런 멋진 기회가 찾아오길 바라요",
          "original_form": "담에도 그런 멋진 기회가 찾아오길 바라요",
          "speaker_id": "1",
          "time": "20210828 00:18"
        }
      ]
