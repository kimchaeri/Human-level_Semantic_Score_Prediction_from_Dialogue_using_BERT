      "utterance": [
        {
          "id": "MDRW2100035665.1.1",
          "form": "저는 뜨반파 입니다..",
          "original_form": "저는 뜨반파 입니다..",
          "speaker_id": "2",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100035665.1.2",
          "form": "저는 후자에용ㅋㅋㅋㅋ",
          "original_form": "저는 후자에용ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100035665.1.3",
          "form": "겨울에 아무리 실내라도 반팔 못입겠던데",
          "original_form": "겨울에 아무리 실내라도 반팔 못입겠던데",
          "speaker_id": "1",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100035665.1.4",
          "form": "이게 사람마다 반반으로 갈리더라구요ㅋㅋㅋ",
          "original_form": "이게 사람마다 반반으로 갈리더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100035665.1.5",
          "form": "왜냐면 옷이 몸에 휘감기는걸 참지못하는 자연인입니다",
          "original_form": "왜냐면 옷이 몸에 휘감기는걸 참지못하는 자연인입니다",
          "speaker_id": "2",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100035665.1.6",
          "form": "아하!",
          "original_form": "아하!",
          "speaker_id": "1",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100035665.1.7",
          "form": "막 너무 다 덮는 옷 입으면",
          "original_form": "막 너무 다 덮는 옷 입으면",
          "speaker_id": "1",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100035665.1.8",
          "form": "답답함 느끼시는군여",
          "original_form": "답답함 느끼시는군여",
          "speaker_id": "1",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100035665.1.9",
          "form": "네네ㅠ",
          "original_form": "네네ㅠ",
          "speaker_id": "2",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100035665.1.10",
          "form": "패딩은 생존템이지만요",
          "original_form": "패딩은 생존템이지만요",
          "speaker_id": "2",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100035665.1.11",
          "form": "겨울에 코트입으세용 패딩입으세용?",
          "original_form": "겨울에 코트입으세용 패딩입으세용?",
          "speaker_id": "1",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100035665.1.12",
          "form": "저 멋낸다고 코트입은게 한 5년전 같아요...ㅋㅋㅋ",
          "original_form": "저 멋낸다고 코트입은게 한 5년전 같아요...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100035665.1.13",
          "form": "패딩파입니다 살아야하니까요",
          "original_form": "패딩파입니다 살아야하니까요",
          "speaker_id": "2",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100035665.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035665.1.15",
          "form": "코트는 지인짜 가끔",
          "original_form": "코트는 지인짜 가끔",
          "speaker_id": "2",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035665.1.16",
          "form": "롱패딩은 생존템이죠",
          "original_form": "롱패딩은 생존템이죠",
          "speaker_id": "1",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035665.1.17",
          "form": "출근은 패딩이죠",
          "original_form": "출근은 패딩이죠",
          "speaker_id": "2",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035665.1.18",
          "form": "ㅋㅋㅋㅋㅋ 패딩이 진짜",
          "original_form": "ㅋㅋㅋㅋㅋ 패딩이 진짜",
          "speaker_id": "1",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035665.1.19",
          "form": "엉덩이까지 따뜻하고",
          "original_form": "엉덩이까지 따뜻하고",
          "speaker_id": "1",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035665.1.20",
          "form": "허벅지도 따듯하고",
          "original_form": "허벅지도 따듯하고",
          "speaker_id": "1",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035665.1.21",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "2",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035665.1.22",
          "form": "오리들에게 감사해요",
          "original_form": "오리들에게 감사해요",
          "speaker_id": "2",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035665.1.23",
          "form": "앜ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035665.1.24",
          "form": "패딩 최고에요",
          "original_form": "패딩 최고에요",
          "speaker_id": "2",
          "time": "20210829 22:13"
        }
      ]
    }
