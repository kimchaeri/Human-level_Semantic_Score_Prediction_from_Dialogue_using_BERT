      "utterance": [
        {
          "id": "MDRW2100038647.1.1",
          "form": "안녕하세요 식사하셨나요?",
          "original_form": "안녕하세요 식사하셨나요?",
          "speaker_id": "1",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100038647.1.2",
          "form": "안녕하세요! 저는 비빔밥 먹었어요! 저녁 드셨나요?",
          "original_form": "안녕하세요! 저는 비빔밥 먹었어요! 저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100038647.1.3",
          "form": "이제 집가서 먹으려고요!",
          "original_form": "이제 집가서 먹으려고요!",
          "speaker_id": "1",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100038647.1.4",
          "form": "일찍 드셨네요!",
          "original_form": "일찍 드셨네요!",
          "speaker_id": "1",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100038647.1.5",
          "form": "네! ㅎㅎ 뭘 드실건가요?",
          "original_form": "네! ㅎㅎ 뭘 드실건가요?",
          "speaker_id": "2",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100038647.1.6",
          "form": "토스트해먹을거같아요",
          "original_form": "토스트해먹을거같아요",
          "speaker_id": "1",
          "time": "20210902 18:35"
        },
        {
          "id": "MDRW2100038647.1.7",
          "form": "누룽지나ㅋㅋ",
          "original_form": "누룽지나ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:35"
        },
        {
          "id": "MDRW2100038647.1.8",
          "form": "간단하게 드시는군요!! ㅎㅎ",
          "original_form": "간단하게 드시는군요!! ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:35"
        },
        {
          "id": "MDRW2100038647.1.9",
          "form": "점심을 과하게 먹어서요ㅎㅎ 치즈볶음밥",
          "original_form": "점심을 과하게 먹어서요ㅎㅎ 치즈볶음밥",
          "speaker_id": "1",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100038647.1.10",
          "form": "치즈는 사랑입니다! 치즈 넣으면 다 맛있는거 같아요 ㅎㅎ",
          "original_form": "치즈는 사랑입니다! 치즈 넣으면 다 맛있는거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:38"
        },
        {
          "id": "MDRW2100038647.1.11",
          "form": "까망베르랑 체다 둘다 넣었어요ㅋㅋ",
          "original_form": "까망베르랑 체다 둘다 넣었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:39"
        },
        {
          "id": "MDRW2100038647.1.12",
          "form": "까망베르 치즈도 쭉쭉 늘어나나요?!!",
          "original_form": "까망베르 치즈도 쭉쭉 늘어나나요?!!",
          "speaker_id": "2",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100038647.1.13",
          "form": "흘러내리는 정도??",
          "original_form": "흘러내리는 정도??",
          "speaker_id": "1",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100038647.1.14",
          "form": "브리치즈구이처럼 해먹으려고요",
          "original_form": "브리치즈구이처럼 해먹으려고요",
          "speaker_id": "1",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100038647.1.15",
          "form": "크 상상만해도 맛있네요!!",
          "original_form": "크 상상만해도 맛있네요!!",
          "speaker_id": "2",
          "time": "20210902 18:59"
        },
        {
          "id": "MDRW2100038647.1.16",
          "form": "토스트에 치즈 넣어 먹어도 맛있겠어요",
          "original_form": "토스트에 치즈 넣어 먹어도 맛있겠어요",
          "speaker_id": "2",
          "time": "20210902 18:59"
        },
        {
          "id": "MDRW2100038647.1.17",
          "form": "네 그릴드치즈!!! 너무 맛있어요!!!",
          "original_form": "네 그릴드치즈!!! 너무 맛있어요!!!",
          "speaker_id": "1",
          "time": "20210902 19:25"
        },
        {
          "id": "MDRW2100038647.1.18",
          "form": "카야잼토스트도 넘 맛있더라구요!!!",
          "original_form": "카야잼토스트도 넘 맛있더라구요!!!",
          "speaker_id": "2",
          "time": "20210902 19:45"
        },
        {
          "id": "MDRW2100038647.1.19",
          "form": "진짜 토스트는 다 맛있어요!!",
          "original_form": "진짜 토스트는 다 맛있어요!!",
          "speaker_id": "1",
