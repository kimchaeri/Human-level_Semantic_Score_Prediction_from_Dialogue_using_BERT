      "utterance": [
        {
          "id": "MDRW2100035710.1.1",
          "form": "그럼 내일부터는 강제 아메리카노네요ㅜ",
          "original_form": "그럼 내일부터는 강제 아메리카노네요ㅜ",
          "speaker_id": "1",
          "time": "20210829 23:05"
        },
        {
          "id": "MDRW2100035710.1.2",
          "form": "저녁 드셨나요",
          "original_form": "저녁 드셨나요",
          "speaker_id": "1",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100035710.1.3",
          "form": "오늘부터 진짜 다이어트에욤 ㅋㅋ 힘내자 나자신 ㅋㅋㅋ",
          "original_form": "오늘부터 진짜 다이어트에욤 ㅋㅋ 힘내자 나자신 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:19"
        },
        {
          "id": "MDRW2100035710.1.4",
          "form": "오늘 저녁은...설마 쉐이크?",
          "original_form": "오늘 저녁은...설마 쉐이크?",
          "speaker_id": "1",
          "time": "20210830 22:20"
        },
        {
          "id": "MDRW2100035710.1.5",
          "form": "방금 뭔가 제 위를 지나가긴했어요 ㅋㅋ",
          "original_form": "방금 뭔가 제 위를 지나가긴했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:20"
        },
        {
          "id": "MDRW2100035710.1.6",
          "form": "ㅜㅇㅜ 위에서 피눈물이 흐르네요",
          "original_form": "ㅜㅇㅜ 위에서 피눈물이 흐르네요",
          "speaker_id": "1",
          "time": "20210830 22:21"
        },
        {
          "id": "MDRW2100035710.1.7",
          "form": "제가 무슨맛 다이어트 쉐이크를 샀는지 말씀 드렸던가요",
          "original_form": "제가 무슨맛 다이어트 쉐이크를 샀는지 말씀 드렸던가요",
          "speaker_id": "2",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100035710.1.8",
          "form": "단백질...?",
          "original_form": "단백질...?",
          "speaker_id": "1",
          "time": "20210830 22:25"
        },
        {
          "id": "MDRW2100035710.1.9",
          "form": "단백질맛이 지금도 의미불명이에요ㅎㅎ",
          "original_form": "단백질맛이 지금도 의미불명이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 22:25"
        },
        {
          "id": "MDRW2100035710.1.10",
          "form": "민트초코맛이요 ㅋㅋ",
          "original_form": "민트초코맛이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 08:41"
        },
        {
          "id": "MDRW2100035710.1.11",
          "form": "대체 왜 이런 새로운 시도를 했을까ㅜㅜ",
          "original_form": "대체 왜 이런 새로운 시도를 했을까ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 08:41"
        },
        {
          "id": "MDRW2100035710.1.12",
          "form": "아니ㅜ 민초는 배신하지 않는데ㅜ",
          "original_form": "아니ㅜ 민초는 배신하지 않는데ㅜ",
          "speaker_id": "1",
          "time": "20210831 08:44"
        },
        {
          "id": "MDRW2100035710.1.13",
          "form": "저는 민초를 좋아하는게 아니라",
          "original_form": "저는 민초를 좋아하는게 아니라",
          "speaker_id": "2",
          "time": "20210831 09:39"
        },
        {
          "id": "MDRW2100035710.1.14",
          "form": "민트초코크림을 좋아한게 아닐까요 ㅋㅋㅋ",
          "original_form": "민트초코크림을 좋아한게 아닐까요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:39"
        },
        {
          "id": "MDRW2100035710.1.15",
          "form": "아니ㅎㅎ 차이가 뭔가욧!",
          "original_form": "아니ㅎㅎ 차이가 뭔가욧!",
          "speaker_id": "1",
          "time": "20210831 09:40"
        },
        {
          "id": "MDRW2100035710.1.16",
          "form": "민초단으로서 맛이 진짜 궁금하네요",
          "original_form": "민초단으로서 맛이 진짜 궁금하네요",
          "speaker_id": "1",
          "time": "20210831 09:40"
        },
        {
          "id": "MDRW2100035710.1.17",
          "form": "닝닝한 민초맛이요",
          "original_form": "닝닝한 민초맛이요",
          "speaker_id": "2",
          "time": "20210831 09:41"
        },
        {
          "id": "MDRW2100035710.1.18",
          "form": "크림 맛은 빠진ㅋㅋ",
          "original_form": "크림 맛은 빠진ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:42"
        },
        {
          "id": "MDRW2100035710.1.19",
          "form": "물탄 민초인가요...",
          "original_form": "물탄 민초인가요...",
          "speaker_id": "1",
          "time": "20210831 09:42"
        },
        {
          "id": "MDRW2100035710.1.20",
          "form": "아니ㅋㅋㅋ 당도는 어때요!",
          "original_form": "아니ㅋㅋㅋ 당도는 어때요!",
          "speaker_id": "1",
          "time": "20210831 09:42"
        }
      ]
