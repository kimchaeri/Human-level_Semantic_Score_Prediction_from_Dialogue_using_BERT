        {
          "id": "MDRW2100031758.1.1",
          "form": "한식중식일식 어떤게 제일좋으세요?",
          "original_form": "한식중식일식 어떤게 제일좋으세요?",
          "speaker_id": "1",
          "time": "20210905 00:07"
        },
        {
          "id": "MDRW2100031758.1.2",
          "form": "한식이요",
          "original_form": "한식이요",
          "speaker_id": "2",
          "time": "20210905 00:07"
        },
        {
          "id": "MDRW2100031758.1.3",
          "form": "님은요 ?",
          "original_form": "님은요 ?",
          "speaker_id": "2",
          "time": "20210905 00:07"
        },
        {
          "id": "MDRW2100031758.1.4",
          "form": "저도한식이요!!",
          "original_form": "저도한식이요!!",
          "speaker_id": "1",
          "time": "20210905 00:08"
        },
        {
          "id": "MDRW2100031758.1.5",
          "form": "한국인은 밥심이죠 ㅎㅎ",
          "original_form": "한국인은 밥심이죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 00:08"
        },
        {
          "id": "MDRW2100031758.1.6",
          "form": "맞아요! 밥!탄수화물최고",
          "original_form": "맞아요! 밥!탄수화물최고",
          "speaker_id": "1",
          "time": "20210905 00:08"
        },
        {
          "id": "MDRW2100031758.1.7",
          "form": "그럼 찌개/국 어떤게 더좋으세요?",
          "original_form": "그럼 찌개/국 어떤게 더좋으세요?",
          "speaker_id": "1",
          "time": "20210905 00:08"
        },
        {
          "id": "MDRW2100031758.1.8",
          "form": "둘다 좋아해요",
          "original_form": "둘다 좋아해요",
          "speaker_id": "2",
          "time": "20210905 00:08"
        },
        {
          "id": "MDRW2100031758.1.9",
          "form": "김치찌개 / 된장찌개 뭐가더좋으세요?",
          "original_form": "김치찌개 / 된장찌개 뭐가더좋으세요?",
          "speaker_id": "1",
          "time": "20210905 00:09"
        },
        {
          "id": "MDRW2100031758.1.10",
          "form": "김치찌개 좋아해요",
          "original_form": "김치찌개 좋아해요",
