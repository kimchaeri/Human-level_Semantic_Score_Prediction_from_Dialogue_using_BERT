      "utterance": [
        {
          "id": "MDRW2100016643.1.1",
          "form": "머리 단발로 자를거야?",
          "original_form": "머리 단발로 자를거야?",
          "speaker_id": "1",
          "time": "20210826 14:22"
        },
        {
          "id": "MDRW2100016643.1.2",
          "form": "응 언니는?",
          "original_form": "응 언니는?",
          "speaker_id": "2",
          "time": "20210826 14:22"
        },
        {
          "id": "MDRW2100016643.1.3",
          "form": "난 칼단발",
          "original_form": "난 칼단발",
          "speaker_id": "2",
          "time": "20210826 14:22"
        },
        {
          "id": "MDRW2100016643.1.4",
          "form": "나는 머리 기장 좀 자를까 싶어서",
          "original_form": "나는 머리 기장 좀 자를까 싶어서",
          "speaker_id": "1",
          "time": "20210826 14:22"
        },
        {
          "id": "MDRW2100016643.1.5",
          "form": "난 숱이 없어서 그나마 풍성해보이는 단발..",
          "original_form": "난 숱이 없어서 그나마 풍성해보이는 단발..",
          "speaker_id": "2",
          "time": "20210826 14:23"
        },
        {
          "id": "MDRW2100016643.1.6",
          "form": "칼단발 이쁠듯",
          "original_form": "칼단발 이쁠듯",
          "speaker_id": "1",
          "time": "20210826 14:23"
        },
        {
          "id": "MDRW2100016643.1.7",
          "form": "부럽다 풍성인들",
          "original_form": "부럽다 풍성인들",
          "speaker_id": "2",
          "time": "20210826 14:23"
        },
        {
          "id": "MDRW2100016643.1.8",
          "form": "근데 머리 한번 짧게자르 넘 기르기 힘들어서ㅜㅠ",
          "original_form": "근데 머리 한번 짧게자르 넘 기르기 힘들어서ㅜㅠ",
          "speaker_id": "1",
          "time": "20210826 14:23"
        },
        {
          "id": "MDRW2100016643.1.9",
          "form": "풍성인들ㅋㅋ",
          "original_form": "풍성인들ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:23"
        },
        {
          "id": "MDRW2100016643.1.10",
          "form": "왜? 숱많으면 노상관이야 뭔들...",
          "original_form": "왜? 숱많으면 노상관이야 뭔들...",
          "speaker_id": "2",
          "time": "20210826 14:23"
        },
        {
          "id": "MDRW2100016643.1.11",
          "form": "기르는데 오래걸리잖아ㅠㅜ",
          "original_form": "기르는데 오래걸리잖아ㅠㅜ",
          "speaker_id": "1",
          "time": "20210826 14:24"
        },
        {
          "id": "MDRW2100016643.1.12",
          "form": "자르고 싶다가도 못 자르게씀",
          "original_form": "자르고 싶다가도 못 자르게씀",
          "speaker_id": "1",
          "time": "20210826 14:24"
        },
        {
          "id": "MDRW2100016643.1.13",
          "form": "난 빡빡 밀고 머리숱 두배 생긴다면 오케이할거야",
          "original_form": "난 빡빡 밀고 머리숱 두배 생긴다면 오케이할거야",
          "speaker_id": "2",
          "time": "20210826 14:25"
        },
        {
          "id": "MDRW2100016643.1.14",
          "form": "있을때 아껴줘",
          "original_form": "있을때 아껴줘",
          "speaker_id": "2",
          "time": "20210826 14:25"
        },
        {
          "id": "MDRW2100016643.1.15",
          "form": "조눗이넼ㄱㄱ",
          "original_form": "조눗이넼ㄱㄱ",
          "speaker_id": "1",
          "time": "20210826 14:25"
        },
        {
          "id": "MDRW2100016643.1.16",
          "form": "그래 있을때 조심해야지",
          "original_form": "그래 있을때 조심해야지",
          "speaker_id": "1",
          "time": "20210826 14:25"
        },
        {
          "id": "MDRW2100016643.1.17",
          "form": "머리 자주 묶는것도 삼가래",
          "original_form": "머리 자주 묶는것도 삼가래",
          "speaker_id": "2",
          "time": "20210826 14:25"
        },
        {
          "id": "MDRW2100016643.1.18",
          "form": "아니 펌하고 싶은데 백수라ㅠㅜ",
          "original_form": "아니 펌하고 싶은데 백수라ㅠㅜ",
          "speaker_id": "1",
          "time": "20210826 14:25"
        },
        {
          "id": "MDRW2100016643.1.19",
          "form": "머리 당겨지면서 빠진대",
          "original_form": "머리 당겨지면서 빠진대",
          "speaker_id": "2",
          "time": "20210826 14:25"
        },
        {
          "id": "MDRW2100016643.1.20",
          "form": "난 펌 염색 다신 안해도 좋으니까 풍성 한번만",
          "original_form": "난 펌 염색 다신 안해도 좋으니까 풍성 한번만",
          "speaker_id": "2",
          "time": "20210826 14:26"
        },
        {
          "id": "MDRW2100016643.1.21",
          "form": "머리 항상 묶고 있는데 무섭다",
          "original_form": "머리 항상 묶고 있는데 무섭다",
          "speaker_id": "1",
          "time": "20210826 14:26"
        },
        {
          "id": "MDRW2100016643.1.22",
          "form": "세게 당기는것만 조심해",
          "original_form": "세게 당기는것만 조심해",
          "speaker_id": "2",
          "time": "20210826 14:27"
        },
        {
          "id": "MDRW2100016643.1.23",
          "form": "너 예전에 숱 엄청 많았는데",
          "original_form": "너 예전에 숱 엄청 많았는데",
          "speaker_id": "1",
          "time": "20210826 14:27"
        },
        {
          "id": "MDRW2100016643.1.24",
          "form": "그러게 다 어디갔어 누가 파먹었나봐",
          "original_form": "그러게 다 어디갔어 누가 파먹었나봐",
          "speaker_id": "2",
          "time": "20210826 14:27"
        }
      ]
    }
  ]
