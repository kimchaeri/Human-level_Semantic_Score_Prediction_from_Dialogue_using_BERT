      "utterance": [
        {
          "id": "MDRW2100012221.1.1",
          "form": "넌 반려동물 키워?",
          "original_form": "넌 반려동물 키워?",
          "speaker_id": "2",
          "time": "20210824 20:27"
        },
        {
          "id": "MDRW2100012221.1.2",
          "form": "웅 우리집 강아지 너무 귀여운 것 같아",
          "original_form": "웅 우리집 강아지 너무 귀여운 것 같아",
          "speaker_id": "1",
          "time": "20210824 20:27"
        },
        {
          "id": "MDRW2100012221.1.3",
          "form": "오 강아지 키우는 구나",
          "original_form": "오 강아지 키우는 구나",
          "speaker_id": "2",
          "time": "20210824 20:27"
        },
        {
          "id": "MDRW2100012221.1.4",
          "form": "이름이 모야",
          "original_form": "이름이 모야",
          "speaker_id": "2",
          "time": "20210824 20:27"
        },
        {
          "id": "MDRW2100012221.1.5",
          "form": "name3이야",
          "original_form": "&name3&이야",
          "speaker_id": "1",
          "time": "20210824 20:28"
        },
        {
          "id": "MDRW2100012221.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ우와 이름 귀엽다!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ우와 이름 귀엽다!",
          "speaker_id": "2",
          "time": "20210824 20:28"
        },
        {
          "id": "MDRW2100012221.1.7",
          "form": "얼굴도 귀여워",
          "original_form": "얼굴도 귀여워",
          "speaker_id": "1",
          "time": "20210824 20:28"
        },
        {
          "id": "MDRW2100012221.1.8",
          "form": "무슨 종이야?",
          "original_form": "무슨 종이야?",
          "speaker_id": "2",
          "time": "20210824 20:30"
        },
        {
          "id": "MDRW2100012221.1.9",
          "form": "푸들이야",
          "original_form": "푸들이야",
          "speaker_id": "1",
          "time": "20210824 20:30"
        },
        {
          "id": "MDRW2100012221.1.10",
          "form": "그런데 요즘 건강이 안좋아서 걱정이야",
          "original_form": "그런데 요즘 건강이 안좋아서 걱정이야",
          "speaker_id": "1",
          "time": "20210824 20:30"
        },
        {
          "id": "MDRW2100012221.1.11",
          "form": "헐 왜?ㅠ",
          "original_form": "헐 왜?ㅠ",
          "speaker_id": "2",
          "time": "20210824 20:30"
        },
        {
          "id": "MDRW2100012221.1.12",
          "form": "나이가 들어서 시력, 청력, 관절, 기관지 전부 다 기능이 안좋아졌어",
          "original_form": "나이가 들어서 시력, 청력, 관절, 기관지 전부 다 기능이 안좋아졌어",
          "speaker_id": "1",
          "time": "20210824 20:31"
        },
        {
          "id": "MDRW2100012221.1.13",
          "form": "아 진짜?나랑이 몇 살인데?",
          "original_form": "아 진짜?나랑이 몇 살인데?",
          "speaker_id": "2",
          "time": "20210824 20:31"
        },
        {
          "id": "MDRW2100012221.1.14",
          "form": "14살이야 그래도 나이에 비해서는 건강한거래",
          "original_form": "14살이야 그래도 나이에 비해서는 건강한거래",
          "speaker_id": "1",
          "time": "20210824 20:31"
        },
        {
          "id": "MDRW2100012221.1.15",
          "form": "헐 14살이면 나이 진짜 많다!",
          "original_form": "헐 14살이면 나이 진짜 많다!",
          "speaker_id": "2",
          "time": "20210824 20:32"
        },
        {
          "id": "MDRW2100012221.1.16",
          "form": "그래도 나 닮아서 동안이라 밖에 나가면 다들 애기인줄 알아",
          "original_form": "그래도 나 닮아서 동안이라 밖에 나가면 다들 애기인줄 알아",
          "speaker_id": "1",
          "time": "20210824 20:32"
        },
        {
          "id": "MDRW2100012221.1.17",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 20:32"
        },
        {
          "id": "MDRW2100012221.1.18",
          "form": "name3하고 추억 얘기해줄 수 있어?",
          "original_form": "&name3&하고 추억 얘기해줄 수 있어?",
          "speaker_id": "2",
          "time": "20210824 20:32"
        },
        {
          "id": "MDRW2100012221.1.19",
          "form": "음 최근에는 name3랑 같이 인생네컷 찍었어",
          "original_form": "음 최근에는 &name3&랑 같이 인생네컷 찍었어",
          "speaker_id": "1",
          "time": "20210824 20:33"
        },
        {
          "id": "MDRW2100012221.1.20",
          "form": "이상하게 나왔지만 그래도 재밌었어",
          "original_form": "이상하게 나왔지만 그래도 재밌었어",
          "speaker_id": "1",
          "time": "20210824 20:33"
        },
        {
          "id": "MDRW2100012221.1.21",
          "form": "우와~너가 name3 사랑으로 잘 키우는 것 깉아",
          "original_form": "우와~너가 &name3& 사랑으로 잘 키우는 것 깉아",
          "speaker_id": "2",
          "time": "20210824 20:33"
        },
        {
          "id": "MDRW2100012221.1.22",
          "form": "name3는 무슨 간식 좋아해?",
          "original_form": "&name3&는 무슨 간식 좋아해?",
          "speaker_id": "2",
          "time": "20210824 20:33"
        },
        {
          "id": "MDRW2100012221.1.23",
          "form": "name3 결석도 있어서.. 개껌같은거는 못먹고 당근 좋아해!",
          "original_form": "&name3& 결석도 있어서.. 개껌같은거는 못먹고 당근 좋아해!",
          "speaker_id": "1",
          "time": "20210824 20:34"
        },
        {
          "id": "MDRW2100012221.1.24",
          "form": "헐ㅜㅜㅜ결석이라니",
          "original_form": "헐ㅜㅜㅜ결석이라니",
          "speaker_id": "2",
          "time": "20210824 20:34"
        },
        {
          "id": "MDRW2100012221.1.25",
          "form": "당근?",
          "original_form": "당근?",
          "speaker_id": "2",
          "time": "20210824 20:34"
        },
        {
          "id": "MDRW2100012221.1.26",
          "form": "name3 나랑 다르게 편식을 안해 상추도 좋아하고 다 좋아해",
          "original_form": "&name3& 나랑 다르게 편식을 안해 상추도 좋아하고 다 좋아해",
          "speaker_id": "1",
          "time": "20210824 20:35"
        },
        {
          "id": "MDRW2100012221.1.27",
          "form": "이야name3가 나보다 낫다",
          "original_form": "이야&name3&가 나보다 낫다",
          "speaker_id": "2",
          "time": "20210824 20:35"
        },
        {
          "id": "MDRW2100012221.1.28",
          "form": "알러지는 있어?",
          "original_form": "알러지는 있어?",
          "speaker_id": "2",
          "time": "20210824 20:35"
        },
        {
          "id": "MDRW2100012221.1.29",
          "form": "그리고 내가 아침마다 녹즙을 먹는데 녹즙먹고 남은거 몇방울 물에 타주면 좋아햐",
          "original_form": "그리고 내가 아침마다 녹즙을 먹는데 녹즙먹고 남은거 몇방울 물에 타주면 좋아햐",
          "speaker_id": "1",
          "time": "20210824 20:35"
        },
        {
          "id": "MDRW2100012221.1.30",
          "form": "알러지는 없어",
          "original_form": "알러지는 없어",
          "speaker_id": "1",
          "time": "20210824 20:35"
        },
        {
          "id": "MDRW2100012221.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ녹즙",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ녹즙",
          "speaker_id": "2",
          "time": "20210824 20:35"
        },
        {
          "id": "MDRW2100012221.1.32",
          "form": "name3하고는 모하고 놀아?",
          "original_form": "&name3&하고는 모하고 놀아?",
          "speaker_id": "2",
          "time": "20210824 20:35"
        },
        {
          "id": "MDRW2100012221.1.33",
          "form": "산책 주로하지",
          "original_form": "산책 주로하지",
