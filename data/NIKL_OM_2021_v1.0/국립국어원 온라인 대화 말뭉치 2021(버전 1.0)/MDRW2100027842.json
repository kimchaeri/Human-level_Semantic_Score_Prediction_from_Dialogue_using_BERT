      "utterance": [
        {
          "id": "MDRW2100027842.1.1",
          "form": "책 요즘 어떤거 읽으세요??",
          "original_form": "책 요즘 어떤거 읽으세요??",
          "speaker_id": "1",
          "time": "20210902 15:34"
        },
        {
          "id": "MDRW2100027842.1.2",
          "form": "소설을 자주읽는거같아요",
          "original_form": "소설을 자주읽는거같아요",
          "speaker_id": "2",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100027842.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100027842.1.4",
          "form": "저는 소설도 읽구요",
          "original_form": "저는 소설도 읽구요",
          "speaker_id": "1",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100027842.1.5",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100027842.1.6",
          "form": "베스트셀러 위주로 많이 보기도 해요ㅋㅋ",
          "original_form": "베스트셀러 위주로 많이 보기도 해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100027842.1.7",
          "form": "최근엔 뭐 읽으셨어요",
          "original_form": "최근엔 뭐 읽으셨어요",
          "speaker_id": "2",
          "time": "20210902 15:36"
        },
        {
          "id": "MDRW2100027842.1.8",
          "form": "한낮의 우울이요ㅎㅎ",
          "original_form": "한낮의 우울이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 15:36"
        },
        {
          "id": "MDRW2100027842.1.9",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210902 15:36"
        },
        {
          "id": "MDRW2100027842.1.10",
          "form": "저는 김려령소설이요ㅎㅎ 약간 드라마같은느낌이었던거같아요",
          "original_form": "저는 김려령소설이요ㅎㅎ 약간 드라마같은느낌이었던거같아요",
          "speaker_id": "2",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100027842.1.11",
          "form": "한낮의우울은 어떠셨나요",
          "original_form": "한낮의우울은 어떠셨나요",
          "speaker_id": "2",
          "time": "20210902 15:38"
        },
        {
          "id": "MDRW2100027842.1.12",
          "form": "우울증에 대해 이해 하는데 조금 도움이 됐어요ㅎㅎ",
          "original_form": "우울증에 대해 이해 하는데 조금 도움이 됐어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100027842.1.13",
          "form": "김려령소설은 거의 안읽어본거같아요",
          "original_form": "김려령소설은 거의 안읽어본거같아요",
          "speaker_id": "1",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100027842.1.14",
          "form": "추천할만한가요??",
          "original_form": "추천할만한가요??",
          "speaker_id": "1",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100027842.1.15",
          "form": "음..반반인거같아요ㅋㅋ",
          "original_form": "음..반반인거같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100027842.1.16",
          "form": "책은 주로 어떻게보시나요?",
          "original_form": "책은 주로 어떻게보시나요?",
          "speaker_id": "2",
          "time": "20210902 15:43"
        },
        {
          "id": "MDRW2100027842.1.17",
          "form": "실물책으로 보세요?",
          "original_form": "실물책으로 보세요?",
          "speaker_id": "2",
          "time": "20210902 15:43"
        },
        {
          "id": "MDRW2100027842.1.18",
          "form": "한동안 그러다가 코시국 이후로는 전자책으로 정착했어요",
          "original_form": "한동안 그러다가 코시국 이후로는 전자책으로 정착했어요",
          "speaker_id": "1",
          "time": "20210902 15:48"
        },
        {
          "id": "MDRW2100027842.1.19",
          "form": "요즘 전자책 잘 나오더라구요",
          "original_form": "요즘 전자책 잘 나오더라구요",
          "speaker_id": "2",
          "time": "20210902 15:51"
        },
        {
          "id": "MDRW2100027842.1.20",
          "form": "오디오북도 편하고 좋은거같아요",
          "original_form": "오디오북도 편하고 좋은거같아요",
          "speaker_id": "2",
          "time": "20210902 15:51"
        },
        {
          "id": "MDRW2100027842.1.21",
          "form": "맞아요ㅎㅎ 요새는 어플도 많구",
          "original_form": "맞아요ㅎㅎ 요새는 어플도 많구",
          "speaker_id": "1",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100027842.1.22",
          "form": "그러니까요ㅎㅎ",
          "original_form": "그러니까요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 15:56"
        },
        {
          "id": "MDRW2100027842.1.23",
          "form": "뭔가 쓰는 어플이 있으세요?",
          "original_form": "뭔가 쓰는 어플이 있으세요?",
          "speaker_id": "2",
