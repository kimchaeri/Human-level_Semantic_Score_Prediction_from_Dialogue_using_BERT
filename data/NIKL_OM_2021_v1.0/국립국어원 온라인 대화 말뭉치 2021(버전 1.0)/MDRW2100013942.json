      "utterance": [
        {
          "id": "MDRW2100013942.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013942.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 16:32"
        },
        {
          "id": "MDRW2100013942.1.3",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "1",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013942.1.4",
          "form": "네 저는 죠스떡볶이 점심으로 먹었어요",
          "original_form": "네 저는 죠스떡볶이 점심으로 먹었어요",
          "speaker_id": "2",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013942.1.5",
          "form": "오 떡볶이 맛있겠네요",
          "original_form": "오 떡볶이 맛있겠네요",
          "speaker_id": "1",
          "time": "20210825 16:36"
        },
        {
          "id": "MDRW2100013942.1.6",
          "form": "혹시 엽떡도 좋아하시나요?",
          "original_form": "혹시 엽떡도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 16:36"
        },
        {
          "id": "MDRW2100013942.1.7",
          "form": "네 엽떡도 좋아해요!",
          "original_form": "네 엽떡도 좋아해요!",
          "speaker_id": "2",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013942.1.8",
          "form": "떡볶이는 다 좋은 것 같아요",
          "original_form": "떡볶이는 다 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210825 16:38"
        },
        {
          "id": "MDRW2100013942.1.9",
          "form": "저 떡볶이 맛집도 찾아가고 그러는데",
          "original_form": "저 떡볶이 맛집도 찾아가고 그러는데",
          "speaker_id": "2",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013942.1.10",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013942.1.11",
          "form": "우와 떡볶이 진짜 좋아하시나봐요ㅎㅎ",
          "original_form": "우와 떡볶이 진짜 좋아하시나봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013942.1.12",
          "form": "저는 홍대 쪽 떡볶이 맛집 가봤어요",
          "original_form": "저는 홍대 쪽 떡볶이 맛집 가봤어요",
          "speaker_id": "1",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013942.1.13",
          "form": "홍대! 맛집 이름이 뭐예요?",
          "original_form": "홍대! 맛집 이름이 뭐예요?",
          "speaker_id": "2",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013942.1.14",
          "form": "또보겠지 떡볶이집이요!",
          "original_form": "또보겠지 떡볶이집이요!",
          "speaker_id": "1",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013942.1.15",
          "form": "또 추천할만한 맛집 있나요?",
          "original_form": "또 추천할만한 맛집 있나요?",
          "speaker_id": "1",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013942.1.16",
          "form": "아 가봤어요 저도ㅎㅎ 거기 감자랑 맛있더라거요",
          "original_form": "아 가봤어요 저도ㅎㅎ 거기 감자랑 맛있더라거요",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013942.1.17",
          "form": "홍대쪽음 잘 모르겠고",
          "original_form": "홍대쪽음 잘 모르겠고",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013942.1.18",
          "form": "용산에 현선이네 떡볶이랑 아차산역에 신토불이 떡볶이 좋아해요",
          "original_form": "용산에 현선이네 떡볶이랑 아차산역에 신토불이 떡볶이 좋아해요",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013942.1.19",
          "form": "광명선매떡볶이가 최애인데 거긴 저희집에선 좀 멀어서",
          "original_form": "광명선매떡볶이가 최애인데 거긴 저희집에선 좀 멀어서",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013942.1.20",
          "form": "와 진짜 많이 가보셨네요",
          "original_form": "와 진짜 많이 가보셨네요",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013942.1.21",
          "form": "좋은정보 감사드려요",
          "original_form": "좋은정보 감사드려요",
          "speaker_id": "1",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100013942.1.22",
          "form": "전 항상 배달로만 먹어서",
          "original_form": "전 항상 배달로만 먹어서",
          "speaker_id": "1",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100013942.1.23",
          "form": "배달이 편하죠! 엽떡 주로 드세요?",
          "original_form": "배달이 편하죠! 엽떡 주로 드세요?",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100013942.1.24",
          "form": "넵 저는 엽떡 중간맛 반반이용",
          "original_form": "넵 저는 엽떡 중간맛 반반이용",
          "speaker_id": "1",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100013942.1.25",
          "form": "엽떡은 뭔가 중독성아 있는것 같아요 주기적으로 찾게되더라고요ㅋㅋㅋ",
          "original_form": "엽떡은 뭔가 중독성아 있는것 같아요 주기적으로 찾게되더라고요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:57"
        },
        {
          "id": "MDRW2100013942.1.26",
          "form": "맞아요ㅋ 지금도 엽떡 먹고싶네요",
          "original_form": "맞아요ㅋ 지금도 엽떡 먹고싶네요",
          "speaker_id": "1",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100013942.1.27",
          "form": "name1은 저녁 뭐드실건가용",
          "original_form": "&name1&은 저녁 뭐드실건가용",
          "speaker_id": "1",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100013942.1.28",
          "form": "뭘먹을지 고민이에요ㅎㅎ",
          "original_form": "뭘먹을지 고민이에요ㅎㅎ",
