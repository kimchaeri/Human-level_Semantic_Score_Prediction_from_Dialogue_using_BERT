      "utterance": [
        {
          "id": "MDRW2100021482.1.1",
          "form": "게임 하시는 거 있으세요?",
          "original_form": "게임 하시는 거 있으세요?",
          "speaker_id": "1",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100021482.1.2",
          "form": "딱히 하는건 없고요 매일 한번씩 지뢰찾기정도해요ㅎ",
          "original_form": "딱히 하는건 없고요 매일 한번씩 지뢰찾기정도해요ㅎ",
          "speaker_id": "2",
          "time": "20210828 19:29"
        },
        {
          "id": "MDRW2100021482.1.3",
          "form": "지뢰찾기 오랜만이네요 ㅋㅋㅋㅋ 예전엔 자주 했는데",
          "original_form": "지뢰찾기 오랜만이네요 ㅋㅋㅋㅋ 예전엔 자주 했는데",
          "speaker_id": "1",
          "time": "20210828 19:41"
        },
        {
          "id": "MDRW2100021482.1.4",
          "form": "저는 폰게임만 하고있어요",
          "original_form": "저는 폰게임만 하고있어요",
          "speaker_id": "1",
          "time": "20210828 19:41"
        },
        {
          "id": "MDRW2100021482.1.5",
          "form": "어떤 폰게임이요?",
          "original_form": "어떤 폰게임이요?",
          "speaker_id": "2",
          "time": "20210828 19:42"
        },
        {
          "id": "MDRW2100021482.1.6",
          "form": "머지멘션이라구 물건 합치는ㅋㅋ 그런거랑 카드게임이요!",
          "original_form": "머지멘션이라구 물건 합치는ㅋㅋ 그런거랑 카드게임이요!",
          "speaker_id": "1",
          "time": "20210828 19:43"
        },
        {
          "id": "MDRW2100021482.1.7",
          "form": "어제는 사천성 새로 받아서 하고있어요",
          "original_form": "어제는 사천성 새로 받아서 하고있어요",
          "speaker_id": "1",
          "time": "20210828 19:43"
        },
        {
          "id": "MDRW2100021482.1.8",
          "form": "오 퍼즐게임류 좋아하시나요??",
          "original_form": "오 퍼즐게임류 좋아하시나요??",
          "speaker_id": "2",
          "time": "20210828 20:02"
        },
        {
          "id": "MDRW2100021482.1.9",
          "form": "네네 ㅋㅋㅋ 퍼즐게임이랑 방탈출 게임도 좋아해요!",
          "original_form": "네네 ㅋㅋㅋ 퍼즐게임이랑 방탈출 게임도 좋아해요!",
          "speaker_id": "1",
          "time": "20210828 20:04"
        },
        {
          "id": "MDRW2100021482.1.10",
          "form": "저도 사실 퍼즐게임에 환장해요ㅋㅋ 한번 손대면 거의 날밤새서 자제중이지만요ㅋㅋㅋ",
          "original_form": "저도 사실 퍼즐게임에 환장해요ㅋㅋ 한번 손대면 거의 날밤새서 자제중이지만요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:05"
        },
        {
          "id": "MDRW2100021482.1.11",
          "form": "ㅋㅋㅋㅋ뭐 하셨었어요?",
          "original_form": "ㅋㅋㅋㅋ뭐 하셨었어요?",
          "speaker_id": "1",
          "time": "20210828 20:06"
        },
        {
          "id": "MDRW2100021482.1.12",
          "form": "저는 그 캔디크러쉬 회사 게임거 종류별로 했었어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 그 캔디크러쉬 회사 게임거 종류별로 했었어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:06"
        },
        {
          "id": "MDRW2100021482.1.13",
          "form": "저도 캔디크러시사가 했었는데 아침마다 눈이 퀭해졌었네요ㅋㅋ 비슷한 카카오 게임이 프렌즈팝콘였던가요? 그거도 했어요ㅎ",
          "original_form": "저도 캔디크러시사가 했었는데 아침마다 눈이 퀭해졌었네요ㅋㅋ 비슷한 카카오 게임이 프렌즈팝콘였던가요? 그거도 했어요ㅎ",
          "speaker_id": "2",
          "time": "20210828 20:25"
        },
        {
          "id": "MDRW2100021482.1.14",
          "form": "맞아요 맞아요 프렌즈 팝콘도 하다가 안 하구",
          "original_form": "맞아요 맞아요 프렌즈 팝콘도 하다가 안 하구",
          "speaker_id": "1",
          "time": "20210828 20:26"
        },
        {
          "id": "MDRW2100021482.1.15",
          "form": "프렌즈 사천성 타운 이렇게 지금 하고 있어요 ㅋㅋㅋㅋ",
          "original_form": "프렌즈 사천성 타운 이렇게 지금 하고 있어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100021482.1.16",
          "form": "한 번 하면 진짜 하트 다 떨어져도 사서 하고ㅠ 그랬었네요",
          "original_form": "한 번 하면 진짜 하트 다 떨어져도 사서 하고ㅠ 그랬었네요",
          "speaker_id": "1",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100021482.1.17",
          "form": "네 저도 하트사고 구걸도 하고 장난 아니었죠ㅎㅎ 카카오게임은 팝콘? 이후로 안햇는데 찾아보니 비슷한게 많네요",
          "original_form": "네 저도 하트사고 구걸도 하고 장난 아니었죠ㅎㅎ 카카오게임은 팝콘? 이후로 안햇는데 찾아보니 비슷한게 많네요",
          "speaker_id": "2",
          "time": "20210828 20:29"
        },
        {
          "id": "MDRW2100021482.1.18",
          "form": "네 엄청 나오더라고요 ㅋㅋㅋ 비슷비슷하게",
          "original_form": "네 엄청 나오더라고요 ㅋㅋㅋ 비슷비슷하게",
          "speaker_id": "1",
          "time": "20210828 20:30"
        },
        {
          "id": "MDRW2100021482.1.19",
          "form": "그만큼 하는 사람이 있나봐요 ㅋㅋ",
          "original_form": "그만큼 하는 사람이 있나봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:30"
        }
      ]
    }
