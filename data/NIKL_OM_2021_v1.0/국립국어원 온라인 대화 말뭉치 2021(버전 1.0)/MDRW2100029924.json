        {
          "id": "MDRW2100029924.1.1",
          "form": "집안일 해야죠",
          "original_form": "집안일 해야죠",
          "speaker_id": "2",
          "time": "20210903 16:37"
        },
        {
          "id": "MDRW2100029924.1.2",
          "form": "분담해야죠",
          "original_form": "분담해야죠",
          "speaker_id": "1",
          "time": "20210903 16:49"
        },
        {
          "id": "MDRW2100029924.1.3",
          "form": "뭐 그냥 알아서 보이는거 해서요",
          "original_form": "뭐 그냥 알아서 보이는거 해서요",
          "speaker_id": "2",
          "time": "20210903 16:55"
        },
        {
          "id": "MDRW2100029924.1.4",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100029924.1.5",
          "form": "청소도 쉽지않죠",
          "original_form": "청소도 쉽지않죠",
          "speaker_id": "2",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100029924.1.6",
          "form": "화장실전담이요",
          "original_form": "화장실전담이요",
          "speaker_id": "1",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100029924.1.7",
          "form": "거긴 특히 힘들어요",
          "original_form": "거긴 특히 힘들어요",
          "speaker_id": "2",
          "time": "20210903 17:46"
        },
        {
          "id": "MDRW2100029924.1.8",
          "form": "화장실청소가 가장 힘들죠",
          "original_form": "화장실청소가 가장 힘들죠",
          "speaker_id": "1",
          "time": "20210903 17:47"
        },
        {
          "id": "MDRW2100029924.1.9",
          "form": "깨끗이 안돼요",
          "original_form": "깨끗이 안돼요",
          "speaker_id": "2",
          "time": "20210903 17:50"
        },
        {
          "id": "MDRW2100029924.1.10",
          "form": "락스냄새싫어요",
          "original_form": "락스냄새싫어요",
          "speaker_id": "1",
          "time": "20210903 17:53"
        },
        {
          "id": "MDRW2100029924.1.11",
          "form": "곰팡이없애려면 써야하니까요",
          "original_form": "곰팡이없애려면 써야하니까요",
          "speaker_id": "2",
          "time": "20210903 17:54"
        },
        {
          "id": "MDRW2100029924.1.12",
          "form": "머리도아프고",
          "original_form": "머리도아프고",
          "speaker_id": "1",
          "time": "20210903 17:55"
        },
        {
          "id": "MDRW2100029924.1.13",
          "form": "청소가 힘드네요",
          "original_form": "청소가 힘드네요",
          "speaker_id": "2",
          "time": "20210903 17:55"
        },
        {
          "id": "MDRW2100029924.1.14",
          "form": "마스크껴도 안됨",
          "original_form": "마스크껴도 안됨",
