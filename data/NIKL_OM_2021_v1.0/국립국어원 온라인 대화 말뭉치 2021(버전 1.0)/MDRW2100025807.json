      "utterance": [
        {
          "id": "MDRW2100025807.1.1",
          "form": "내 생일 전에 코스트코 갈 일 있어?",
          "original_form": "내 생일 전에 코스트코 갈 일 있어?",
          "speaker_id": "2",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100025807.1.2",
          "form": "니 생일 때 주말이라 사람 많아서 코스트코 못 갈 거 같은데 왜",
          "original_form": "니 생일 때 주말이라 사람 많아서 코스트코 못 갈 거 같은데 왜",
          "speaker_id": "1",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100025807.1.3",
          "form": "아니 커피빈 케이크 먹고 싶은데",
          "original_form": "아니 커피빈 케이크 먹고 싶은데",
          "speaker_id": "2",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100025807.1.4",
          "form": "정관엔 없어져서 찾아보니까 코스트코에서 십분 거리에 있다길래",
          "original_form": "정관엔 없어져서 찾아보니까 코스트코에서 십분 거리에 있다길래",
          "speaker_id": "2",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100025807.1.5",
          "form": "아 거긴 갈 수 있지",
          "original_form": "아 거긴 갈 수 있지",
          "speaker_id": "1",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100025807.1.6",
          "form": "코스트코는 주말에 사람 많아서 가기 힘든거고",
          "original_form": "코스트코는 주말에 사람 많아서 가기 힘든거고",
          "speaker_id": "1",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100025807.1.7",
          "form": "근데 거기 가려고 그냥 가기엔 너무 멀어서",
          "original_form": "근데 거기 가려고 그냥 가기엔 너무 멀어서",
          "speaker_id": "2",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100025807.1.8",
          "form": "그정도는 갈 수 있다",
          "original_form": "그정도는 갈 수 있다",
          "speaker_id": "1",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100025807.1.9",
          "form": "거의 한시간인데 좀 그래서",
          "original_form": "거의 한시간인데 좀 그래서",
          "speaker_id": "2",
          "time": "20210901 17:13"
        },
        {
          "id": "MDRW2100025807.1.10",
          "form": "한 번인데 갔다올 수 있다",
          "original_form": "한 번인데 갔다올 수 있다",
          "speaker_id": "1",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100025807.1.11",
          "form": "맛있는 거 먹어라",
          "original_form": "맛있는 거 먹어라",
          "speaker_id": "1",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100025807.1.12",
          "form": "오 웬일ㅋㅋ짱 좋",
          "original_form": "오 웬일ㅋㅋ짱 좋",
          "speaker_id": "2",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100025807.1.13",
          "form": "이따 아저씨 옮 물어봐",
          "original_form": "이따 아저씨 옮 물어봐",
          "speaker_id": "1",
          "time": "20210901 17:17"
        },
        {
          "id": "MDRW2100025807.1.14",
          "form": "ㅇㅇ 넘 멀어서 아님 투썸 꺼 먹을라했는데",
          "original_form": "ㅇㅇ 넘 멀어서 아님 투썸 꺼 먹을라했는데",
          "speaker_id": "2",
          "time": "20210901 17:17"
        },
        {
          "id": "MDRW2100025807.1.15",
          "form": "투썸 맛 없더라 먹고 싶은 거 먹어",
          "original_form": "투썸 맛 없더라 먹고 싶은 거 먹어",
