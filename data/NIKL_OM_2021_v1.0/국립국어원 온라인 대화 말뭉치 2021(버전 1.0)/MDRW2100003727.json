      "utterance": [
        {
          "id": "MDRW2100003727.1.1",
          "form": "안녕하세요 혹시 집안일에 적극적이신가요?",
          "original_form": "안녕하세요 혹시 집안일에 적극적이신가요?",
          "speaker_id": "1",
          "time": "20210821 11:18"
        },
        {
          "id": "MDRW2100003727.1.2",
          "form": "네 근데 해도해도 티가 안나네요ㅠ",
          "original_form": "네 근데 해도해도 티가 안나네요ㅠ",
          "speaker_id": "2",
          "time": "20210821 11:18"
        },
        {
          "id": "MDRW2100003727.1.3",
          "form": "저는 자취하느라 제가 다 하는데 집안일 쉽지않아요ㅠㅠ",
          "original_form": "저는 자취하느라 제가 다 하는데 집안일 쉽지않아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 11:18"
        },
        {
          "id": "MDRW2100003727.1.4",
          "form": "그쵸 매일매일 쓸고 닦는거 너무 귀찮아요ㅎㅎ",
          "original_form": "그쵸 매일매일 쓸고 닦는거 너무 귀찮아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 11:19"
        },
        {
          "id": "MDRW2100003727.1.5",
          "form": "저는 설거지가 너무 귀찮더라고요 흑흑",
          "original_form": "저는 설거지가 너무 귀찮더라고요 흑흑",
          "speaker_id": "1",
          "time": "20210821 11:21"
        },
        {
          "id": "MDRW2100003727.1.6",
          "form": "매일 해야해서요ㅠㅠ",
          "original_form": "매일 해야해서요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 11:21"
        },
        {
          "id": "MDRW2100003727.1.7",
          "form": "무슨 집안일을 가장 싫어하세요?",
          "original_form": "무슨 집안일을 가장 싫어하세요?",
          "speaker_id": "1",
          "time": "20210821 11:21"
        },
        {
          "id": "MDRW2100003727.1.8",
          "form": "저도 설거지..그리고 욕실청소ㅠㅠ",
          "original_form": "저도 설거지..그리고 욕실청소ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 11:22"
        },
        {
          "id": "MDRW2100003727.1.9",
          "form": "한번 놓치면 진짜 보기싫어져서",
          "original_form": "한번 놓치면 진짜 보기싫어져서",
          "speaker_id": "2",
          "time": "20210821 11:22"
        },
        {
          "id": "MDRW2100003727.1.10",
          "form": "헉 욕실청소도 힘들죠",
          "original_form": "헉 욕실청소도 힘들죠",
          "speaker_id": "1",
          "time": "20210821 11:22"
        },
        {
          "id": "MDRW2100003727.1.11",
          "form": "부모님이랑 같이 지내시나요?",
          "original_form": "부모님이랑 같이 지내시나요?",
          "speaker_id": "1",
          "time": "20210821 11:23"
        },
        {
          "id": "MDRW2100003727.1.12",
          "form": "넹 본가로 들어왔는데 일은 안줄어드네요ㅋㅋ",
          "original_form": "넹 본가로 들어왔는데 일은 안줄어드네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 11:23"
        },
        {
          "id": "MDRW2100003727.1.13",
          "form": "그쵸 ㅎㅎㅎ",
          "original_form": "그쵸 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 11:24"
        },
        {
          "id": "MDRW2100003727.1.14",
          "form": "집이 넓을 수록 집안일이 더 힘들 것 같아요",
          "original_form": "집이 넓을 수록 집안일이 더 힘들 것 같아요",
          "speaker_id": "1",
          "time": "20210821 11:24"
        },
        {
          "id": "MDRW2100003727.1.15",
          "form": "그니까요ㅎㅎ 이제 계절바뀌면 이불이랑 다 대청소 하자고 하시는데 벌써 귀찮음요ㅠ",
          "original_form": "그니까요ㅎㅎ 이제 계절바뀌면 이불이랑 다 대청소 하자고 하시는데 벌써 귀찮음요ㅠ",
          "speaker_id": "2",
          "time": "20210821 11:25"
        },
        {
          "id": "MDRW2100003727.1.16",
          "form": "악 벌써힘들어요ㅠㅠㅠ",
          "original_form": "악 벌써힘들어요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 11:25"
        },
        {
          "id": "MDRW2100003727.1.17",
          "form": "이불 빨래 발로 하면 너무 힘든데ㅠㅠ",
          "original_form": "이불 빨래 발로 하면 너무 힘든데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 11:26"
        },
        {
          "id": "MDRW2100003727.1.18",
          "form": "다행히 저희는 맡기는데 그것도 일이라..ㅠㅠ",
          "original_form": "다행히 저희는 맡기는데 그것도 일이라..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 11:26"
        },
        {
          "id": "MDRW2100003727.1.19",
          "form": "다행이네요 ㅎㅎ",
          "original_form": "다행이네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 11:26"
        }
      ]
