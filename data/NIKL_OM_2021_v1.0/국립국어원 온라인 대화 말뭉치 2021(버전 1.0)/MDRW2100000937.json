      "utterance": [
        {
          "id": "MDRW2100000937.1.1",
          "form": "오늘 날씨 시원하더라구요",
          "original_form": "오늘 날씨 시원하더라구요",
          "speaker_id": "1",
          "time": "20210819 20:47"
        },
        {
          "id": "MDRW2100000937.1.2",
          "form": "맞아요 그래서 요즘 가볍게입고 밖에 많이 나가요",
          "original_form": "맞아요 그래서 요즘 가볍게입고 밖에 많이 나가요",
          "speaker_id": "2",
          "time": "20210819 20:48"
        },
        {
          "id": "MDRW2100000937.1.3",
          "form": "일기예보도 요즘 맞는것같아 좋아요",
          "original_form": "일기예보도 요즘 맞는것같아 좋아요",
          "speaker_id": "2",
          "time": "20210819 20:48"
        },
        {
          "id": "MDRW2100000937.1.4",
          "form": "그니까요 근데 소나기가 가끔 내려서 우산필수",
          "original_form": "그니까요 근데 소나기가 가끔 내려서 우산필수",
          "speaker_id": "1",
          "time": "20210819 20:48"
        },
        {
          "id": "MDRW2100000937.1.5",
          "form": "저는 이제 우산 안들고 다녀도 돼요",
          "original_form": "저는 이제 우산 안들고 다녀도 돼요",
          "speaker_id": "2",
          "time": "20210819 20:49"
        },
        {
          "id": "MDRW2100000937.1.6",
          "form": "오오 저는 항상 가방에 작은우산을..",
          "original_form": "오오 저는 항상 가방에 작은우산을..",
          "speaker_id": "1",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000937.1.7",
          "form": "동선이 같아서 비가오면 피할곳을 찾아뒀어요",
          "original_form": "동선이 같아서 비가오면 피할곳을 찾아뒀어요",
          "speaker_id": "2",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000937.1.8",
          "form": "큰 표지판, 앵무새 집",
          "original_form": "큰 표지판, 앵무새 집",
          "speaker_id": "2",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000937.1.9",
          "form": "ㅋㄱㅋ 좋네요",
          "original_form": "ㅋㄱㅋ 좋네요",
          "speaker_id": "1",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000937.1.10",
          "form": "비피할곳 알아놓으면 대처하기편하죠",
          "original_form": "비피할곳 알아놓으면 대처하기편하죠",
          "speaker_id": "1",
          "time": "20210819 20:51"
        },
        {
          "id": "MDRW2100000937.1.11",
          "form": "근데 요즘 누가 따라해서 열받아요",
          "original_form": "근데 요즘 누가 따라해서 열받아요",
          "speaker_id": "2",
          "time": "20210819 20:51"
        },
        {
          "id": "MDRW2100000937.1.12",
          "form": "왜요..?",
          "original_form": "왜요..?",
          "speaker_id": "1",
          "time": "20210819 20:51"
        },
        {
          "id": "MDRW2100000937.1.13",
          "form": "비를 피하는 제 모습이 안락해보였나봐요",
          "original_form": "비를 피하는 제 모습이 안락해보였나봐요",
          "speaker_id": "2",
          "time": "20210819 20:51"
        },
        {
          "id": "MDRW2100000937.1.14",
          "form": "아하 ㅋㅋㅋ 요즘 옷차림은 아직 반팔이죠?",
          "original_form": "아하 ㅋㅋㅋ 요즘 옷차림은 아직 반팔이죠?",
          "speaker_id": "1",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000937.1.15",
          "form": "네 맞아요 반팔입기 딱좋은날씨",
          "original_form": "네 맞아요 반팔입기 딱좋은날씨",
          "speaker_id": "2",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000937.1.16",
          "form": "요즘 옷차림이 어떻게되세요",
          "original_form": "요즘 옷차림이 어떻게되세요",
          "speaker_id": "2",
          "time": "20210819 20:53"
        },
        {
          "id": "MDRW2100000937.1.17",
          "form": "저도 반팔입고 반바지죠",
          "original_form": "저도 반팔입고 반바지죠",
          "speaker_id": "1",
          "time": "20210819 20:53"
        },
        {
          "id": "MDRW2100000937.1.18",
          "form": "낮에는 더워서 아직 긴팔은 무리",
          "original_form": "낮에는 더워서 아직 긴팔은 무리",
          "speaker_id": "1",
          "time": "20210819 20:54"
        },
        {
          "id": "MDRW2100000937.1.19",
          "form": "요즘 크롭티가 유행인것같아요",
          "original_form": "요즘 크롭티가 유행인것같아요",
          "speaker_id": "2",
          "time": "20210819 20:54"
        },
        {
          "id": "MDRW2100000937.1.20",
          "form": "다시 여름 아이템이 된것같아요",
          "original_form": "다시 여름 아이템이 된것같아요",
          "speaker_id": "2",
          "time": "20210819 20:54"
        },
        {
          "id": "MDRW2100000937.1.21",
          "form": "저는 크롭티 안좋아해서..",
          "original_form": "저는 크롭티 안좋아해서..",
          "speaker_id": "1",
          "time": "20210819 20:54"
        }
