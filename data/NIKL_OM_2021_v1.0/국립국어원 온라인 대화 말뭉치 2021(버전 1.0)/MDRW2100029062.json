      "utterance": [
        {
          "id": "MDRW2100029062.1.1",
          "form": "음 취미가 거의없어요호",
          "original_form": "음 취미가 거의없어요호",
          "speaker_id": "2",
          "time": "20210902 23:29"
        },
        {
          "id": "MDRW2100029062.1.2",
          "form": "에구 현생이 벅차신가부내요",
          "original_form": "에구 현생이 벅차신가부내요",
          "speaker_id": "1",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100029062.1.3",
          "form": "아무래도 애기한테 온정신을 다 쏳으니깐",
          "original_form": "아무래도 애기한테 온정신을 다 쏳으니깐",
          "speaker_id": "2",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100029062.1.4",
          "form": "저한테는 무관심해요ㅎㅎ",
          "original_form": "저한테는 무관심해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100029062.1.5",
          "form": "아하 애기라는 최대의 공통 관심사가 있네요!",
          "original_form": "아하 애기라는 최대의 공통 관심사가 있네요!",
          "speaker_id": "1",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100029062.1.6",
          "form": "그렇게 말해주셔섶감사요ㅎㅎ",
          "original_form": "그렇게 말해주셔섶감사요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100029062.1.7",
          "form": "애기얘기 많이 하면 아내분도 좋아하실거 같아요 :)",
          "original_form": "애기얘기 많이 하면 아내분도 좋아하실거 같아요 :)",
          "speaker_id": "1",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029062.1.8",
          "form": "애기랑 많이 놀아줘요ㅎㅎ",
          "original_form": "애기랑 많이 놀아줘요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029062.1.9",
          "form": "근데 벌써 떠났음요ㅎㅎ",
          "original_form": "근데 벌써 떠났음요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029062.1.10",
          "form": "앵 누가요?",
          "original_form": "앵 누가요?",
          "speaker_id": "1",
          "time": "20210902 23:38"
        },
        {
          "id": "MDRW2100029062.1.11",
          "form": "아아 마음이ㅎㅎ",
          "original_form": "아아 마음이ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100029062.1.12",
          "form": "ㅋㅋㅋ 원래 사람마음이 좋다가 싫다가 그러잖아요",
          "original_form": "ㅋㅋㅋ 원래 사람마음이 좋다가 싫다가 그러잖아요",
          "speaker_id": "1",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029062.1.13",
          "form": "ㅎㅎㅎ 이혼 곧해요ㅎㅎ",
          "original_form": "ㅎㅎㅎ 이혼 곧해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029062.1.14",
          "form": "어이구야 애기는 누가 데려가나요",
          "original_form": "어이구야 애기는 누가 데려가나요",
          "speaker_id": "1",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029062.1.15",
          "form": "아내가 키워요ㅎ",
          "original_form": "아내가 키워요ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100029062.1.16",
          "form": "애기 잘 크길 바라요ㅠㅠ 양육비도 잘 보내주시구요",
          "original_form": "애기 잘 크길 바라요ㅠㅠ 양육비도 잘 보내주시구요",
          "speaker_id": "1",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100029062.1.17",
          "form": "네네 그래야죠ㅎ",
          "original_form": "네네 그래야죠ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100029062.1.18",
          "form": "앱테크해서 양육 조금이라더 보태야죠ㅎ",
          "original_form": "앱테크해서 양육 조금이라더 보태야죠ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
