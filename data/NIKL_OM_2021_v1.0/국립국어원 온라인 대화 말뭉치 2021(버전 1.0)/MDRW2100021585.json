      "utterance": [
        {
          "id": "MDRW2100021585.1.1",
          "form": "집안은 화목한 편이세요?",
          "original_form": "집안은 화목한 편이세요?",
          "speaker_id": "1",
          "time": "20210828 20:38"
        },
        {
          "id": "MDRW2100021585.1.2",
          "form": "내 그럭저럭 화목한 편인거 같아요",
          "original_form": "내 그럭저럭 화목한 편인거 같아요",
          "speaker_id": "2",
          "time": "20210828 20:46"
        },
        {
          "id": "MDRW2100021585.1.3",
          "form": "다행이네요 ㅎㅎ",
          "original_form": "다행이네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 20:50"
        },
        {
          "id": "MDRW2100021585.1.4",
          "form": "형제자매 있어요?",
          "original_form": "형제자매 있어요?",
          "speaker_id": "1",
          "time": "20210828 20:50"
        },
        {
          "id": "MDRW2100021585.1.5",
          "form": "네ㅋㅋㅋ 있어요",
          "original_form": "네ㅋㅋㅋ 있어요",
          "speaker_id": "2",
          "time": "20210828 20:53"
        },
        {
          "id": "MDRW2100021585.1.6",
          "form": "오 형제자매 어떻게 되세요?",
          "original_form": "오 형제자매 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210828 20:56"
        },
        {
          "id": "MDRW2100021585.1.7",
          "form": "언니하나 있어요",
          "original_form": "언니하나 있어요",
          "speaker_id": "2",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100021585.1.8",
          "form": "언니랑 친해요?",
          "original_form": "언니랑 친해요?",
          "speaker_id": "1",
          "time": "20210828 20:59"
        },
        {
          "id": "MDRW2100021585.1.9",
          "form": "네 정말 친해요ㅎㅎ 앤디님은 형제자매 있으세요??",
          "original_form": "네 정말 친해요ㅎㅎ 앤디님은 형제자매 있으세요??",
          "speaker_id": "2",
          "time": "20210828 21:02"
        },
        {
          "id": "MDRW2100021585.1.10",
          "form": "네 저도 누나 한 명 있어요!",
          "original_form": "네 저도 누나 한 명 있어요!",
          "speaker_id": "1",
          "time": "20210828 21:07"
        },
        {
          "id": "MDRW2100021585.1.11",
          "form": "근데 누나가 연애하느라 바빠서 요즘 연락 잘안해요 ㅜㅜ",
          "original_form": "근데 누나가 연애하느라 바빠서 요즘 연락 잘안해요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 21:07"
        },
        {
          "id": "MDRW2100021585.1.12",
          "form": "연애ㅋㅋ 어쩔수없죠ㅎ 나이차이는 많이 나시나요?",
          "original_form": "연애ㅋㅋ 어쩔수없죠ㅎ 나이차이는 많이 나시나요?",
          "speaker_id": "2",
          "time": "20210828 21:17"
        },
        {
          "id": "MDRW2100021585.1.13",
          "form": "많이 안나서 그런지 잘 맞더라구요",
          "original_form": "많이 안나서 그런지 잘 맞더라구요",
          "speaker_id": "1",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100021585.1.14",
          "form": "매일 만나는 거보면 이러다 결혼할 거 같기도 해요",
          "original_form": "매일 만나는 거보면 이러다 결혼할 거 같기도 해요",
          "speaker_id": "1",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100021585.1.15",
          "form": "누나분이 좋은 남친 만나셨길 바래요ㅎㅎ",
          "original_form": "누나분이 좋은 남친 만나셨길 바래요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 21:32"
        },
        {
          "id": "MDRW2100021585.1.16",
          "form": "지금 잘해주고 있다고 그러더라구요",
          "original_form": "지금 잘해주고 있다고 그러더라구요",
          "speaker_id": "1",
          "time": "20210828 21:34"
        },
        {
          "id": "MDRW2100021585.1.17",
          "form": "곧 하나의 가족이 탄생할듯 한데 행복하길...",
          "original_form": "곧 하나의 가족이 탄생할듯 한데 행복하길...",
          "speaker_id": "1",
          "time": "20210828 21:34"
        }
      ]
