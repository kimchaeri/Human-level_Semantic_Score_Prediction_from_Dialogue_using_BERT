      "utterance": [
        {
          "id": "MDRW2100025539.1.1",
          "form": "고양이나 강아지 좋아하세요",
          "original_form": "고양이나 강아지 좋아하세요",
          "speaker_id": "2",
          "time": "20210901 13:01"
        },
        {
          "id": "MDRW2100025539.1.2",
          "form": "전 둘다 좋아해요",
          "original_form": "전 둘다 좋아해요",
          "speaker_id": "2",
          "time": "20210901 13:01"
        },
        {
          "id": "MDRW2100025539.1.3",
          "form": "키우지는 않아요",
          "original_form": "키우지는 않아요",
          "speaker_id": "2",
          "time": "20210901 13:01"
        },
        {
          "id": "MDRW2100025539.1.4",
          "form": "저도 좋아하는데 랜선으로만 키워요ㅎ",
          "original_form": "저도 좋아하는데 랜선으로만 키워요ㅎ",
          "speaker_id": "1",
          "time": "20210901 14:13"
        },
        {
          "id": "MDRW2100025539.1.5",
          "form": "개는 보고만 있어도 좋아요",
          "original_form": "개는 보고만 있어도 좋아요",
          "speaker_id": "1",
          "time": "20210901 14:14"
        },
        {
          "id": "MDRW2100025539.1.6",
          "form": "저두요 유튭으로 아이들 지켜봐요",
          "original_form": "저두요 유튭으로 아이들 지켜봐요",
          "speaker_id": "2",
          "time": "20210901 14:14"
        },
        {
          "id": "MDRW2100025539.1.7",
          "form": "털도 안날리고요ㅋㅋ",
          "original_form": "털도 안날리고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:16"
        },
        {
          "id": "MDRW2100025539.1.8",
          "form": "맞아요 ㅋㅋㅋ대리만족",
          "original_form": "맞아요 ㅋㅋㅋ대리만족",
          "speaker_id": "2",
          "time": "20210901 14:16"
        },
        {
          "id": "MDRW2100025539.1.9",
          "form": "산책시켜주기 힘들어요 ㅠ",
          "original_form": "산책시켜주기 힘들어요 ㅠ",
          "speaker_id": "2",
          "time": "20210901 14:17"
        },
        {
          "id": "MDRW2100025539.1.10",
          "form": "일땜에 아이들 돌봐줄 시간이 없어서요",
          "original_form": "일땜에 아이들 돌봐줄 시간이 없어서요",
          "speaker_id": "2",
          "time": "20210901 14:17"
        },
        {
          "id": "MDRW2100025539.1.11",
          "form": "산책은..",
          "original_form": "산책은..",
          "speaker_id": "1",
          "time": "20210901 14:17"
        },
        {
          "id": "MDRW2100025539.1.12",
          "form": "저 스스로도 잘 안시켜줘서ㅋㅋ",
          "original_form": "저 스스로도 잘 안시켜줘서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:17"
        },
        {
          "id": "MDRW2100025539.1.13",
          "form": "앗 저두요 ㅋㅋ동지시네요",
          "original_form": "앗 저두요 ㅋㅋ동지시네요",
          "speaker_id": "2",
          "time": "20210901 14:18"
        },
        {
          "id": "MDRW2100025539.1.14",
          "form": "길냥이 보면 한번씩 편의점에서 간식 사서 받쳐요",
          "original_form": "길냥이 보면 한번씩 편의점에서 간식 사서 받쳐요",
          "speaker_id": "2",
          "time": "20210901 14:18"
        },
        {
          "id": "MDRW2100025539.1.15",
          "form": "그걸로 만족중이랍니다",
          "original_form": "그걸로 만족중이랍니다",
          "speaker_id": "2",
          "time": "20210901 14:18"
        },
        {
          "id": "MDRW2100025539.1.16",
          "form": "어떤 고양이 종류 좋아하세요",
          "original_form": "어떤 고양이 종류 좋아하세요",
          "speaker_id": "2",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100025539.1.17",
          "form": "음 지금은 치즈요!",
          "original_form": "음 지금은 치즈요!",
          "speaker_id": "1",
          "time": "20210902 10:42"
        },
        {
          "id": "MDRW2100025539.1.18",
          "form": "치즈가 순하더라구요",
          "original_form": "치즈가 순하더라구요",
          "speaker_id": "1",
          "time": "20210902 10:42"
        },
        {
          "id": "MDRW2100025539.1.19",
          "form": "치즈좋아요",
          "original_form": "치즈좋아요",
          "speaker_id": "2",
          "time": "20210902 10:42"
        },
        {
          "id": "MDRW2100025539.1.20",
          "form": "얼룩이도 좋아요",
          "original_form": "얼룩이도 좋아요",
          "speaker_id": "2",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100025539.1.21",
          "form": "턱시도도 좋아요",
          "original_form": "턱시도도 좋아요",
          "speaker_id": "2",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100025539.1.22",
          "form": "크 요새 눈썹있는 흰냥이도 좋아요",
          "original_form": "크 요새 눈썹있는 흰냥이도 좋아요",
          "speaker_id": "1",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100025539.1.23",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100025539.1.24",
          "form": "눈써비",
          "original_form": "눈써비",
          "speaker_id": "2",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100025539.1.25",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100025539.1.26",
          "form": "하악거리는것도 귀엽더라구요ㅋㅋ",
          "original_form": "하악거리는것도 귀엽더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100025539.1.27",
          "form": "하찮고 귀여워요",
          "original_form": "하찮고 귀여워요",
          "speaker_id": "2",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100025539.1.28",
          "form": "조그만 아기고양이도 좋아해요",
          "original_form": "조그만 아기고양이도 좋아해요",
          "speaker_id": "2",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100025539.1.29",
          "form": "쪼꾸미",
          "original_form": "쪼꾸미",
          "speaker_id": "2",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100025539.1.30",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100025539.1.31",
          "form": "어제는 점남이 나왔는데",
          "original_form": "어제는 점남이 나왔는데",
          "speaker_id": "1",
          "time": "20210902 10:48"
        },
        {
          "id": "MDRW2100025539.1.32",
          "form": "귀여워서 동영상캡쳐해뒀어요ㅋㅋ",
          "original_form": "귀여워서 동영상캡쳐해뒀어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:48"
        },
        {
          "id": "MDRW2100025539.1.33",
          "form": "보내주세요",
          "original_form": "보내주세요",
          "speaker_id": "2",
          "time": "20210902 10:49"
        }
      ]
    }
  ]
