      "utterance": [
        {
          "id": "MDRW2100006086.1.1",
          "form": "운도 능력이죠ㅋㅋ 저도 거성이형 좋아했어요ㅎㅎ",
          "original_form": "운도 능력이죠ㅋㅋ 저도 거성이형 좋아했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100006086.1.2",
          "form": "그리고 본성은 착해서",
          "original_form": "그리고 본성은 착해서",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100006086.1.3",
          "form": "지르고 나중에 전전긍긍",
          "original_form": "지르고 나중에 전전긍긍",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100006086.1.4",
          "form": "밎아요 거성이형 미담이 많이 돌죠ㅎㅎㅎ",
          "original_form": "밎아요 거성이형 미담이 많이 돌죠ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100006086.1.5",
          "form": "볼매에요 ㅎㅎ",
          "original_form": "볼매에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100006086.1.6",
          "form": "사람 잘챙기고",
          "original_form": "사람 잘챙기고",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100006086.1.7",
          "form": "ㅋㅋ 무도할때가 그립네요ㅋㅋ",
          "original_form": "ㅋㅋ 무도할때가 그립네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100006086.1.8",
          "form": "그니까요 놀뭐에서 할때 오시징",
          "original_form": "그니까요 놀뭐에서 할때 오시징",
          "speaker_id": "1",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100006086.1.9",
          "form": "전에 놀뭐에 치킨 튀길때도 거성이형 진짜 웃겼죠ㅋㅋㅋ",
          "original_form": "전에 놀뭐에 치킨 튀길때도 거성이형 진짜 웃겼죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100006086.1.10",
          "form": "네 궁시렁거리면서 함ㅋㅋㅋ",
          "original_form": "네 궁시렁거리면서 함ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100006086.1.11",
          "form": "치킨 소스 만들때 레시피는 쳐다도 안보고 대충대충ㅋㅋㅋㅋㅋㅋ",
          "original_form": "치킨 소스 만들때 레시피는 쳐다도 안보고 대충대충ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100006086.1.12",
          "form": "그러다가 쿠사리먹으면 시무룩ㅋㅋㅋ",
          "original_form": "그러다가 쿠사리먹으면 시무룩ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006086.1.13",
          "form": "쿠사리 먹을때 거성이형 표정 진짜 웃기죠ㅋㅋ",
          "original_form": "쿠사리 먹을때 거성이형 표정 진짜 웃기죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006086.1.14",
          "form": "진짜 매덩입니다",
          "original_form": "진짜 매덩입니다",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100006086.1.15",
