      "utterance": [
        {
          "id": "MDRW2100027133.1.1",
          "form": "스포츠는 음 평소 챙겨보시나요?",
          "original_form": "스포츠는 음 평소 챙겨보시나요?",
          "speaker_id": "2",
          "time": "20210902 10:18"
        },
        {
          "id": "MDRW2100027133.1.2",
          "form": "아니요 ㅋㅋ큐ㅠㅠ 사실 올림픽도 잘 안봤어요",
          "original_form": "아니요 ㅋㅋ큐ㅠㅠ 사실 올림픽도 잘 안봤어요",
          "speaker_id": "1",
          "time": "20210902 10:18"
        },
        {
          "id": "MDRW2100027133.1.3",
          "form": "name2님은 좋아하세요??",
          "original_form": "&name2&님은 좋아하세요??",
          "speaker_id": "1",
          "time": "20210902 10:18"
        },
        {
          "id": "MDRW2100027133.1.4",
          "form": "저도 이번올림픽 솔직히 잘 안봤어요",
          "original_form": "저도 이번올림픽 솔직히 잘 안봤어요",
          "speaker_id": "2",
          "time": "20210902 10:20"
        },
        {
          "id": "MDRW2100027133.1.5",
          "form": "집안식구들은 어떠세요?",
          "original_form": "집안식구들은 어떠세요?",
          "speaker_id": "2",
          "time": "20210902 10:20"
        },
        {
          "id": "MDRW2100027133.1.6",
          "form": "남동생이나 남편이나 아버지시라던가..",
          "original_form": "남동생이나 남편이나 아버지시라던가..",
          "speaker_id": "2",
          "time": "20210902 10:21"
        },
        {
          "id": "MDRW2100027133.1.7",
          "form": "오빠가 야구 좋아해서 코로나 터지기 전엔 종종 보러 갔어요 ㅋㅋㅋ",
          "original_form": "오빠가 야구 좋아해서 코로나 터지기 전엔 종종 보러 갔어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:21"
        },
        {
          "id": "MDRW2100027133.1.8",
          "form": "야구 좋아하세요??",
          "original_form": "야구 좋아하세요??",
          "speaker_id": "1",
          "time": "20210902 10:21"
        },
        {
          "id": "MDRW2100027133.1.9",
          "form": "야구 좋아해요",
          "original_form": "야구 좋아해요",
          "speaker_id": "2",
          "time": "20210902 10:23"
        },
        {
          "id": "MDRW2100027133.1.10",
          "form": "어느팀 응원하세요?",
          "original_form": "어느팀 응원하세요?",
          "speaker_id": "2",
          "time": "20210902 10:23"
        },
        {
          "id": "MDRW2100027133.1.11",
          "form": "저는 야구 잘 모르긴한데요 오빠 때문에 키움 응원했어요 ㅋㅋㅋ",
          "original_form": "저는 야구 잘 모르긴한데요 오빠 때문에 키움 응원했어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:26"
        },
        {
          "id": "MDRW2100027133.1.12",
          "form": "name2 님은 어느팀 응원하시나요??",
          "original_form": "&name2& 님은 어느팀 응원하시나요??",
          "speaker_id": "1",
          "time": "20210902 10:26"
        },
        {
          "id": "MDRW2100027133.1.13",
          "form": "저는 삼성라이온즈요",
          "original_form": "저는 삼성라이온즈요",
          "speaker_id": "2",
          "time": "20210902 10:27"
        },
        {
          "id": "MDRW2100027133.1.14",
          "form": "제가 대학을 대구에서 나와서 ㅎㅎ",
          "original_form": "제가 대학을 대구에서 나와서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:27"
        },
        {
          "id": "MDRW2100027133.1.15",
          "form": "대학때 야구장 가봤어요 ㅋㅋ",
          "original_form": "대학때 야구장 가봤어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:28"
        },
        {
          "id": "MDRW2100027133.1.16",
          "form": "역시 지역연고팀을...!!!",
          "original_form": "역시 지역연고팀을...!!!",
          "speaker_id": "1",
          "time": "20210902 10:30"
        },
        {
          "id": "MDRW2100027133.1.17",
          "form": "야구 티비에서 보는거랑 직관이랑 느낌 완전 다르지않아요???",
          "original_form": "야구 티비에서 보는거랑 직관이랑 느낌 완전 다르지않아요???",
          "speaker_id": "1",
          "time": "20210902 10:30"
        },
        {
          "id": "MDRW2100027133.1.18",
          "form": "야구는 좀 그런거있는거같아요ㅋㅋ",
          "original_form": "야구는 좀 그런거있는거같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100027133.1.19",
          "form": "저는생각보다 응원하는게 재밌었던기억이나요 ㅋㅋㅋ",
          "original_form": "저는생각보다 응원하는게 재밌었던기억이나요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100027133.1.20",
          "form": "맞아요!!!",
          "original_form": "맞아요!!!",
          "speaker_id": "1",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100027133.1.21",
          "form": "응원봉 두드리면서요 막 ㅋㅋㅋ",
          "original_form": "응원봉 두드리면서요 막 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100027133.1.22",
          "form": "그리고 분명 모르는 노래였는데",
          "original_form": "그리고 분명 모르는 노래였는데",
          "speaker_id": "2",
          "time": "20210902 10:34"
        },
        {
          "id": "MDRW2100027133.1.23",
          "form": "게임 끝날때는 다 알고 나오게되고 ㅋㅋ",
          "original_form": "게임 끝날때는 다 알고 나오게되고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:34"
        },
        {
          "id": "MDRW2100027133.1.24",
          "form": "ㅋㅋㅋㅋㅋ맞아요 진짜",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 진짜",
          "speaker_id": "1",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100027133.1.25",
          "form": "막 계속 떠오르는데",
          "original_form": "막 계속 떠오르는데",
          "speaker_id": "1",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100027133.1.26",
          "form": "그러다가 상대팀 노래가 더 좋으면 상대팀 노래만 떠오르더라구요...",
          "original_form": "그러다가 상대팀 노래가 더 좋으면 상대팀 노래만 떠오르더라구요...",
          "speaker_id": "1",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100027133.1.27",
          "form": "후크송이라고 해야하나 중독성이 진짜 ㅋㅋ",
          "original_form": "후크송이라고 해야하나 중독성이 진짜 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100027133.1.28",
          "form": "요새는 밖에서음식못사가겠죠",
          "original_form": "요새는 밖에서음식못사가겠죠",
          "speaker_id": "2",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027133.1.29",
          "form": "안에 다 매점이있어서",
          "original_form": "안에 다 매점이있어서",
          "speaker_id": "2",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027133.1.30",
          "form": "그러게요ㅠㅠㅠㅠ 야구장은 매점 음식 먹는 재미인데요 ㅋㅋㅋ",
          "original_form": "그러게요ㅠㅠㅠㅠ 야구장은 매점 음식 먹는 재미인데요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027133.1.31",
          "form": "요새 직관 가능해졌을까요??",
          "original_form": "요새 직관 가능해졌을까요??",
          "speaker_id": "1",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027133.1.32",
          "form": "저는 진짜 옛날이라 ㅋㅋ",
          "original_form": "저는 진짜 옛날이라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:49"
        },
        {
          "id": "MDRW2100027133.1.33",
          "form": "사실 저도요 ㅋㅋㅋㅋ",
          "original_form": "사실 저도요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027133.1.34",
          "form": "매점이 아직 막 많이없었거든요",
          "original_form": "매점이 아직 막 많이없었거든요",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027133.1.35",
          "form": "구단도 오래됐고 구장도 오래됐고 ㅋㅋ",
          "original_form": "구단도 오래됐고 구장도 오래됐고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027133.1.36",
          "form": "아 ㅇ0ㅇ",
          "original_form": "아 ㅇ0ㅇ",
          "speaker_id": "1",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027133.1.37",
          "form": "그러네요 구장 컨디션을 엄청 타는 거 같더라구요",
          "original_form": "그러네요 구장 컨디션을 엄청 타는 거 같더라구요",
          "speaker_id": "1",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027133.1.38",
          "form": "그래서 밖에서사서들어가도 딱히 제재안했었는데",
          "original_form": "그래서 밖에서사서들어가도 딱히 제재안했었는데",
          "speaker_id": "2",
          "time": "20210902 10:51"
        },
        {
          "id": "MDRW2100027133.1.39",
          "form": "저 고척돔이랑 목동 이렇게만 가봤는데 새건물은 매점많고 푸드코트도 있고 그런데 목동은 마트에서 맥주 공수하고 그랬어요",
          "original_form": "저 고척돔이랑 목동 이렇게만 가봤는데 새건물은 매점많고 푸드코트도 있고 그런데 목동은 마트에서 맥주 공수하고 그랬어요",
          "speaker_id": "1",
          "time": "20210902 10:51"
        },
        {
          "id": "MDRW2100027133.1.40",
          "form": "맞아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:51"
        },
        {
          "id": "MDRW2100027133.1.41",
          "form": "그때먹었더음식이진짜 맛있었어요 ㅋㅋㅋ",
