        {
          "id": "MDRW2100020080.1.1",
          "form": "연애중이신가요ㅎㅎ",
          "original_form": "연애중이신가요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100020080.1.2",
          "form": "네 연애중이예요",
          "original_form": "네 연애중이예요",
          "speaker_id": "1",
          "time": "20210828 09:41"
        },
        {
          "id": "MDRW2100020080.1.3",
          "form": "결혼하셨다고 했죠!",
          "original_form": "결혼하셨다고 했죠!",
          "speaker_id": "1",
          "time": "20210828 09:41"
        },
        {
          "id": "MDRW2100020080.1.4",
          "form": "연애 행복하신가요",
          "original_form": "연애 행복하신가요",
          "speaker_id": "2",
          "time": "20210828 11:57"
        },
        {
          "id": "MDRW2100020080.1.5",
          "form": "네 좋죠",
          "original_form": "네 좋죠",
          "speaker_id": "1",
          "time": "20210828 12:27"
        },
        {
          "id": "MDRW2100020080.1.6",
          "form": "결혼 행복하신가요?",
          "original_form": "결혼 행복하신가요?",
          "speaker_id": "1",
          "time": "20210828 12:27"
        },
        {
          "id": "MDRW2100020080.1.7",
          "form": "네 장단점이 있지만",
          "original_form": "네 장단점이 있지만",
          "speaker_id": "2",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100020080.1.8",
          "form": "장점이 더 큰듯해요",
          "original_form": "장점이 더 큰듯해요",
          "speaker_id": "2",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100020080.1.9",
          "form": "어떤 장점이 있나요?",
          "original_form": "어떤 장점이 있나요?",
          "speaker_id": "1",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100020080.1.10",
          "form": "확실히 뭔가 안정적이에요",
          "original_form": "확실히 뭔가 안정적이에요",
          "speaker_id": "2",
          "time": "20210828 18:01"
        },
        {
          "id": "MDRW2100020080.1.11",
          "form": "그럼 혹시 단점도 물어봐도 될까요?",
          "original_form": "그럼 혹시 단점도 물어봐도 될까요?",
          "speaker_id": "1",
          "time": "20210828 18:05"
        },
        {
          "id": "MDRW2100020080.1.12",
          "form": "ㅋㅋ옆에 계속 있어요",
          "original_form": "ㅋㅋ옆에 계속 있어요",
          "speaker_id": "2",
          "time": "20210829 06:55"
        },
        {
          "id": "MDRW2100020080.1.13",
          "form": "장점이자 단점이군요ㅋㅋ",
          "original_form": "장점이자 단점이군요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:37"
        },
        {
          "id": "MDRW2100020080.1.14",
          "form": "얼마나 되셨어요?",
          "original_form": "얼마나 되셨어요?",
          "speaker_id": "1",
          "time": "20210829 10:37"
        },
        {
          "id": "MDRW2100020080.1.15",
          "form": "저 작년 11월이오ㅎㅎ",
          "original_form": "저 작년 11월이오ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 11:45"
        },
        {
          "id": "MDRW2100020080.1.16",
          "form": "이분이다 하게되신 계기가 있나요?",
          "original_form": "이분이다 하게되신 계기가 있나요?",
          "speaker_id": "1",
          "time": "20210829 11:51"
        },
