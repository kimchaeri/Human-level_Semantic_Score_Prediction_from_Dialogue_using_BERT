        },
        {
          "id": "MDRW2100031664.1.2",
          "form": "부산에서 도심인데",
          "original_form": "부산에서 도심인데",
          "speaker_id": "2",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100031664.1.3",
          "form": "그렇게 더러우면",
          "original_form": "그렇게 더러우면",
          "speaker_id": "2",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100031664.1.4",
          "form": "다른곳은...",
          "original_form": "다른곳은...",
          "speaker_id": "2",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100031664.1.5",
          "form": "ㅎㅎ....",
          "original_form": "ㅎㅎ....",
          "speaker_id": "1",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100031664.1.6",
          "form": "싱가포르는",
          "original_form": "싱가포르는",
          "speaker_id": "1",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100031664.1.7",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100031664.1.8",
          "form": "그 건물짓는거 허가할때",
          "original_form": "그 건물짓는거 허가할때",
          "speaker_id": "1",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100031664.1.9",
          "form": "비슷비슷한 건물은 아예 허가 못받아서",
          "original_form": "비슷비슷한 건물은 아예 허가 못받아서",
          "speaker_id": "1",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100031664.1.10",
          "form": "특이한건물들 진짜 많던데",
          "original_form": "특이한건물들 진짜 많던데",
          "speaker_id": "1",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100031664.1.11",
          "form": "헐 대박",
