        {
          "id": "MDRW2100031664.1.22",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100031664.1.23",
          "form": "좀 디자인을 천시하는느낌?",
          "original_form": "좀 디자인을 천시하는느낌?",
