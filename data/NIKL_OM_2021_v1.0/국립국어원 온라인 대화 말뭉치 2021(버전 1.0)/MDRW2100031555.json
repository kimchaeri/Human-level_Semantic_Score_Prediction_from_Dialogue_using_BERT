      "utterance": [
        {
          "id": "MDRW2100031555.1.1",
          "form": "반팔은 샀니",
          "original_form": "반팔은 샀니",
          "speaker_id": "2",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100031555.1.2",
          "form": "월요일에 도착할것같네ㅋㅋ",
          "original_form": "월요일에 도착할것같네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100031555.1.3",
          "form": "일단 하나만 결제함ㅋㅋ",
          "original_form": "일단 하나만 결제함ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100031555.1.4",
          "form": "왜 그랬니 ㅋㅋㅋ",
          "original_form": "왜 그랬니 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:58"
        },
        {
          "id": "MDRW2100031555.1.5",
          "form": "으응 하나 더 결제할게...ㅋㅋㅋ",
          "original_form": "으응 하나 더 결제할게...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100031555.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100031555.1.7",
          "form": "가을되니까 베이지 넘 사고싶",
          "original_form": "가을되니까 베이지 넘 사고싶",
          "speaker_id": "2",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100031555.1.8",
          "form": "잘 어울리는 사람들 부럽다",
          "original_form": "잘 어울리는 사람들 부럽다",
          "speaker_id": "2",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100031555.1.9",
          "form": "난 안어울려도 입는다ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "난 안어울려도 입는다ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:05"
        },
        {
          "id": "MDRW2100031555.1.10",
          "form": "없는 집안 우환 소환 색깔 흑",
          "original_form": "없는 집안 우환 소환 색깔 흑",
          "speaker_id": "2",
          "time": "20210904 23:18"
        },
        {
          "id": "MDRW2100031555.1.11",
          "form": "우환ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "우환ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031555.1.12",
          "form": "티 하나 마저 결제 했다ㅋㅋ",
          "original_form": "티 하나 마저 결제 했다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100031555.1.13",
          "form": "베이지 니트 이런 포근한 너낌 원츄다 ㅋㅋㅋ",
          "original_form": "베이지 니트 이런 포근한 너낌 원츄다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:56"
        },
        {
          "id": "MDRW2100031555.1.14",
          "form": "샀군요",
          "original_form": "샀군요",
          "speaker_id": "2",
          "time": "20210904 23:56"
        },
        {
          "id": "MDRW2100031555.1.15",
          "form": "네 샀습니다  택배 아저씨가 보고싶군요ㅋㅋ",
          "original_form": "네 샀습니다  택배 아저씨가 보고싶군요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 11:32"
        },
        {
          "id": "MDRW2100031555.1.16",
          "form": "마져 살려면 추석 오기전에 얼른 사야해",
          "original_form": "마져 살려면 추석 오기전에 얼른 사야해",
          "speaker_id": "2",
          "time": "20210905 12:59"
        },
        {
          "id": "MDRW2100031555.1.17",
          "form": "응 반팔입을 날이 얼마남지않았다ㅋㅋ",
          "original_form": "응 반팔입을 날이 얼마남지않았다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:04"
        },
        {
          "id": "MDRW2100031555.1.18",
          "form": "마져 레이어드 해도 한계가 곧 ㅋㅋ",
          "original_form": "마져 레이어드 해도 한계가 곧 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:46"
        },
        {
          "id": "MDRW2100031555.1.19",
          "form": "그래 이너로 입어도 이번달이 한계겠지 ㅋㅋ",
          "original_form": "그래 이너로 입어도 이번달이 한계겠지 ㅋㅋ",
