      "utterance": [
        {
          "id": "MDRW2100003952.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210821 14:07"
        },
        {
          "id": "MDRW2100003952.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 14:08"
        },
        {
          "id": "MDRW2100003952.1.3",
          "form": "무슨동물키워요?",
          "original_form": "무슨동물키워요?",
          "speaker_id": "1",
          "time": "20210821 14:08"
        },
        {
          "id": "MDRW2100003952.1.4",
          "form": "강아지요",
          "original_form": "강아지요",
          "speaker_id": "2",
          "time": "20210821 14:09"
        },
        {
          "id": "MDRW2100003952.1.5",
          "form": "귀엽겠다",
          "original_form": "귀엽겠다",
          "speaker_id": "1",
          "time": "20210821 14:09"
        },
        {
          "id": "MDRW2100003952.1.6",
          "form": "오래키웠어요?",
          "original_form": "오래키웠어요?",
          "speaker_id": "1",
          "time": "20210821 14:09"
        },
        {
          "id": "MDRW2100003952.1.7",
          "form": "9년 정도요?",
          "original_form": "9년 정도요?",
          "speaker_id": "2",
          "time": "20210821 14:09"
        },
        {
          "id": "MDRW2100003952.1.8",
          "form": "와 오래키우셨네요 진짜",
          "original_form": "와 오래키우셨네요 진짜",
          "speaker_id": "1",
          "time": "20210821 14:10"
        },
        {
          "id": "MDRW2100003952.1.9",
          "form": "산책 본인이 시켜주나요?",
          "original_form": "산책 본인이 시켜주나요?",
          "speaker_id": "1",
          "time": "20210821 14:10"
        },
        {
          "id": "MDRW2100003952.1.10",
          "form": "요즘 코로나 때문에 자주는 못시켜요",
          "original_form": "요즘 코로나 때문에 자주는 못시켜요",
          "speaker_id": "2",
          "time": "20210821 14:10"
        },
        {
          "id": "MDRW2100003952.1.11",
          "form": "그렇군요..아쉽네여",
          "original_form": "그렇군요..아쉽네여",
          "speaker_id": "1",
          "time": "20210821 14:11"
        },
        {
          "id": "MDRW2100003952.1.12",
          "form": "길가다가 산책하는 강아쥐 보이면",
          "original_form": "길가다가 산책하는 강아쥐 보이면",
          "speaker_id": "1",
          "time": "20210821 14:11"
        },
        {
          "id": "MDRW2100003952.1.13",
          "form": "저도모르게 웃게되더라고요",
          "original_form": "저도모르게 웃게되더라고요",
          "speaker_id": "1",
          "time": "20210821 14:11"
        },
        {
          "id": "MDRW2100003952.1.14",
          "form": "너무기여워요",
          "original_form": "너무기여워요",
          "speaker_id": "1",
          "time": "20210821 14:11"
        },
        {
          "id": "MDRW2100003952.1.15",
          "form": "맞아요ㅎㅋㅋ",
          "original_form": "맞아요ㅎㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:15"
        },
        {
          "id": "MDRW2100003952.1.16",
          "form": "강아지가 말은 잘 들어요?",
          "original_form": "강아지가 말은 잘 들어요?",
          "speaker_id": "1",
          "time": "20210821 14:18"
        },
        {
          "id": "MDRW2100003952.1.17",
          "form": "막 손달라고하면 손도 주고 그러나여",
          "original_form": "막 손달라고하면 손도 주고 그러나여",
          "speaker_id": "1",
          "time": "20210821 14:18"
        },
        {
          "id": "MDRW2100003952.1.18",
          "form": "넿",
          "original_form": "넿",
          "speaker_id": "2",
          "time": "20210821 14:20"
        },
        {
          "id": "MDRW2100003952.1.19",
          "form": "똑똑한 강아지네요",
          "original_form": "똑똑한 강아지네요",
          "speaker_id": "1",
          "time": "20210821 14:25"
        },
        {
          "id": "MDRW2100003952.1.20",
          "form": "훈련 잘시켰나봐용",
          "original_form": "훈련 잘시켰나봐용",
          "speaker_id": "1",
          "time": "20210821 14:26"
        }
