        {
          "id": "MDRW2100023964.1.1",
          "form": "마스크팩 산거 왔드아",
          "original_form": "마스크팩 산거 왔드아",
          "speaker_id": "2",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100023964.1.2",
          "form": "오오 기념으로 하나 붙여라ㅋㅋ",
          "original_form": "오오 기념으로 하나 붙여라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:25"
        },
        {
          "id": "MDRW2100023964.1.3",
          "form": "귀찮네 근데 ㅋㅋㅋㅋ",
          "original_form": "귀찮네 근데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100023964.1.4",
          "form": "난 오늘도 모델링팩 했다ㅋㅋ",
          "original_form": "난 오늘도 모델링팩 했다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100023964.1.5",
          "form": "큰 용기내서 팩하는중 ㅋㅋㅋ",
          "original_form": "큰 용기내서 팩하는중 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:06"
        },
        {
          "id": "MDRW2100023964.1.6",
          "form": "거의 안 빼먹고 매일 해? 모델링팩",
          "original_form": "거의 안 빼먹고 매일 해? 모델링팩",
          "speaker_id": "2",
          "time": "20210830 22:06"
        },
        {
          "id": "MDRW2100023964.1.7",
          "form": "이틀에 한번ㅋㅋ 비타민 앰플이 순수비타민 함량이 높아서 자극 생길 수 있으니 이틀에 한번씩만 바르라 해서ㅋㅋ",
          "original_form": "이틀에 한번ㅋㅋ 비타민 앰플이 순수비타민 함량이 높아서 자극 생길 수 있으니 이틀에 한번씩만 바르라 해서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:12"
        },
        {
          "id": "MDRW2100023964.1.8",
          "form": "것도 대단하구만 ㅋㅋㅋㅋ",
          "original_form": "것도 대단하구만 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100023964.1.9",
          "form": "깨 더 안생길라믄 으쯜 수 없네ㅋㅋㅋ",
          "original_form": "깨 더 안생길라믄 으쯜 수 없네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100023964.1.10",
          "form": "트러블 언제 싹 없어지나 ㅠ",
          "original_form": "트러블 언제 싹 없어지나 ㅠ",
          "speaker_id": "2",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100023964.1.11",
          "form": "입원해서 항생제 맞으니 싹 없어지든뎈ㅋㅋㅋㅋㅋ",
          "original_form": "입원해서 항생제 맞으니 싹 없어지든뎈ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100023964.1.12",
          "form": "ㅋ....ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋ....ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100023964.1.13",
          "form": "도움 안 되는 비법이네",
          "original_form": "도움 안 되는 비법이네",
          "speaker_id": "2",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100023964.1.14",
          "form": "응 나도 그다지 추천은 안함ㅋㅋㅋ",
          "original_form": "응 나도 그다지 추천은 안함ㅋㅋㅋ",
