        {
          "id": "MDRW2100011674.1.1",
          "form": "피부과 다니고 싶다",
          "original_form": "피부과 다니고 싶다",
          "speaker_id": "1",
          "time": "20210824 15:00"
        },
        {
          "id": "MDRW2100011674.1.2",
          "form": "나도 피부과",
          "original_form": "나도 피부과",
          "speaker_id": "2",
          "time": "20210824 15:00"
        },
        {
          "id": "MDRW2100011674.1.3",
          "form": "근데 아는 엄마가 피부얇아진다고",
          "original_form": "근데 아는 엄마가 피부얇아진다고",
          "speaker_id": "2",
          "time": "20210824 15:00"
        },
        {
          "id": "MDRW2100011674.1.4",
          "form": "뭐하고싶은데?",
          "original_form": "뭐하고싶은데?",
          "speaker_id": "1",
          "time": "20210824 15:00"
        },
        {
          "id": "MDRW2100011674.1.5",
          "form": "나는 피부가 얇아서 다니지 말라더라",
          "original_form": "나는 피부가 얇아서 다니지 말라더라",
          "speaker_id": "2",
          "time": "20210824 15:00"
        },
        {
          "id": "MDRW2100011674.1.6",
          "form": "나도 피부얇아진다는 얘기들었다",
          "original_form": "나도 피부얇아진다는 얘기들었다",
          "speaker_id": "1",
          "time": "20210824 15:00"
        },
        {
          "id": "MDRW2100011674.1.7",
          "form": "그러면 안하는게 좋데",
          "original_form": "그러면 안하는게 좋데",
          "speaker_id": "2",
          "time": "20210824 15:01"
        },
        {
          "id": "MDRW2100011674.1.8",
          "form": "서울대 의사가..그래서 나이들어서 하래했데",
          "original_form": "서울대 의사가..그래서 나이들어서 하래했데",
          "speaker_id": "1",
          "time": "20210824 15:01"
        },
        {
          "id": "MDRW2100011674.1.9",
          "form": "그래서 피부과는 포기",
          "original_form": "그래서 피부과는 포기",
          "speaker_id": "2",
          "time": "20210824 15:01"
        },
        {
          "id": "MDRW2100011674.1.10",
          "form": "그래도 하면 이쁘잖아",
          "original_form": "그래도 하면 이쁘잖아",
          "speaker_id": "1",
          "time": "20210824 15:01"
        },
        {
          "id": "MDRW2100011674.1.11",
          "form": "피부얇아진다고 나이들어서?",
          "original_form": "피부얇아진다고 나이들어서?",
          "speaker_id": "2",
          "time": "20210824 15:01"
        },
        {
          "id": "MDRW2100011674.1.12",
          "form": "아니 나이들어서 최대한 늦게 하라고",
          "original_form": "아니 나이들어서 최대한 늦게 하라고",
          "speaker_id": "1",
          "time": "20210824 15:01"
        },
        {
          "id": "MDRW2100011674.1.13",
          "form": "보톡스나 맞고 싶네",
          "original_form": "보톡스나 맞고 싶네",
          "speaker_id": "2",
          "time": "20210824 15:01"
        },
        {
          "id": "MDRW2100011674.1.14",
          "form": "왜 이유가?",
          "original_form": "왜 이유가?",
          "speaker_id": "2",
          "time": "20210824 15:01"
        },
        {
          "id": "MDRW2100011674.1.15",
          "form": "보톡스는 효과가 미비한든",
          "original_form": "보톡스는 효과가 미비한든",
          "speaker_id": "1",
          "time": "20210824 15:02"
        },
        {
          "id": "MDRW2100011674.1.16",
          "form": "효과 좋드라",
          "original_form": "효과 좋드라",
          "speaker_id": "2",
          "time": "20210824 15:02"
        },
        {
          "id": "MDRW2100011674.1.17",
          "form": "자꾸하면 얇아지니깐 최대한 늦게하라고",
          "original_form": "자꾸하면 얇아지니깐 최대한 늦게하라고",
          "speaker_id": "1",
          "time": "20210824 15:02"
        },
