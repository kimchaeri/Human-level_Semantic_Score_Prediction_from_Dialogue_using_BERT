      "utterance": [
        {
          "id": "MDRW2100002666.1.1",
          "form": "너 방청소는 자주 하는 편이야?",
          "original_form": "너 방청소는 자주 하는 편이야?",
          "speaker_id": "1",
          "time": "20210820 14:35"
        },
        {
          "id": "MDRW2100002666.1.2",
          "form": "방청소는 자주하지",
          "original_form": "방청소는 자주하지",
          "speaker_id": "2",
          "time": "20210820 14:37"
        },
        {
          "id": "MDRW2100002666.1.3",
          "form": "특히 머리카락이 길어서 자주 빠져서",
          "original_form": "특히 머리카락이 길어서 자주 빠져서",
          "speaker_id": "2",
          "time": "20210820 14:37"
        },
        {
          "id": "MDRW2100002666.1.4",
          "form": "어쩔수없이 자주하지",
          "original_form": "어쩔수없이 자주하지",
          "speaker_id": "2",
          "time": "20210820 14:37"
        },
        {
          "id": "MDRW2100002666.1.5",
          "form": "난 머리카락이 길어서 청소 안하면 바로 귀신의 집이 되던데",
          "original_form": "난 머리카락이 길어서 청소 안하면 바로 귀신의 집이 되던데",
          "speaker_id": "1",
          "time": "20210820 14:38"
        },
        {
          "id": "MDRW2100002666.1.6",
          "form": "맞아 그래서",
          "original_form": "맞아 그래서",
          "speaker_id": "2",
          "time": "20210820 14:40"
        },
        {
          "id": "MDRW2100002666.1.7",
          "form": "청소를 맨날하게됨",
          "original_form": "청소를 맨날하게됨",
          "speaker_id": "2",
          "time": "20210820 14:40"
        },
        {
          "id": "MDRW2100002666.1.8",
          "form": "청소좀 하라고..",
          "original_form": "청소좀 하라고..",
          "speaker_id": "1",
          "time": "20210820 14:42"
        },
        {
          "id": "MDRW2100002666.1.9",
          "form": "귀찮아서 안하면 엄마가 들어오셔서 그러시더라",
          "original_form": "귀찮아서 안하면 엄마가 들어오셔서 그러시더라",
          "speaker_id": "1",
          "time": "20210820 14:42"
        },
        {
          "id": "MDRW2100002666.1.10",
          "form": "넌 안그래?",
          "original_form": "넌 안그래?",
          "speaker_id": "1",
          "time": "20210820 14:43"
        },
        {
          "id": "MDRW2100002666.1.11",
          "form": "나도 그래",
          "original_form": "나도 그래",
          "speaker_id": "2",
          "time": "20210820 14:44"
        },
        {
          "id": "MDRW2100002666.1.12",
          "form": "가끔 엄마가 오셔서 청소하고 가심",
          "original_form": "가끔 엄마가 오셔서 청소하고 가심",
          "speaker_id": "2",
          "time": "20210820 14:44"
        },
        {
          "id": "MDRW2100002666.1.13",
          "form": "생각해보니 넌 고양이도 키워서 청소 더 자주 하겠네",
          "original_form": "생각해보니 넌 고양이도 키워서 청소 더 자주 하겠네",
          "speaker_id": "1",
          "time": "20210820 14:46"
        },
        {
          "id": "MDRW2100002666.1.14",
          "form": "맞아 고양이털이 많아서",
          "original_form": "맞아 고양이털이 많아서",
          "speaker_id": "2",
          "time": "20210820 14:46"
        },
        {
          "id": "MDRW2100002666.1.15",
          "form": "청소 자주하지",
          "original_form": "청소 자주하지",
          "speaker_id": "2",
          "time": "20210820 14:46"
        },
        {
          "id": "MDRW2100002666.1.16",
          "form": "그럼 먼지같은것도 별로 없겠네?",
          "original_form": "그럼 먼지같은것도 별로 없겠네?",
          "speaker_id": "1",
          "time": "20210820 14:56"
        },
        {
          "id": "MDRW2100002666.1.17",
          "form": "응응 단지",
          "original_form": "응응 단지",
          "speaker_id": "2",
          "time": "20210820 14:57"
        },
        {
          "id": "MDRW2100002666.1.18",
          "form": "금방 쌓이지",
          "original_form": "금방 쌓이지",
          "speaker_id": "2",
          "time": "20210820 14:57"
        },
        {
          "id": "MDRW2100002666.1.19",
          "form": "흑",
          "original_form": "흑",
          "speaker_id": "2",
          "time": "20210820 14:57"
        },
        {
          "id": "MDRW2100002666.1.20",
          "form": "난 내 기관지에 안좋으면서도 귀찮아서 안하는 스타일인지라",
          "original_form": "난 내 기관지에 안좋으면서도 귀찮아서 안하는 스타일인지라",
          "speaker_id": "1",
          "time": "20210820 14:59"
        },
        {
          "id": "MDRW2100002666.1.21",
          "form": "어떤 의미로 너가 부러워",
          "original_form": "어떤 의미로 너가 부러워",
          "speaker_id": "1",
          "time": "20210820 14:59"
        },
        {
          "id": "MDRW2100002666.1.22",
          "form": "귀찮아도 해야될수 밖에 없잔아",
          "original_form": "귀찮아도 해야될수 밖에 없잔아",
          "speaker_id": "1",
          "time": "20210820 14:59"
        },
        {
          "id": "MDRW2100002666.1.23",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210820 15:01"
        },
        {
          "id": "MDRW2100002666.1.24",
          "form": "나도 방청소나 해야겠다~",
          "original_form": "나도 방청소나 해야겠다~",
          "speaker_id": "1",
          "time": "20210820 15:06"
        },
        {
          "id": "MDRW2100002666.1.25",
          "form": "방청소 나는 오늘했지",
          "original_form": "방청소 나는 오늘했지",
          "speaker_id": "2",
          "time": "20210820 15:08"
        },
        {
          "id": "MDRW2100002666.1.26",
          "form": "머리카락이 기니 잘 보이더라 바닥에 아주 ㅋㅋ",
          "original_form": "머리카락이 기니 잘 보이더라 바닥에 아주 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:08"
        },
        {
          "id": "MDRW2100002666.1.27",
          "form": "나도 그럴거 같네",
          "original_form": "나도 그럴거 같네",
          "speaker_id": "1",
          "time": "20210820 15:08"
        },
        {
          "id": "MDRW2100002666.1.28",
          "form": "청소 전쟁하러 간다!",
          "original_form": "청소 전쟁하러 간다!",
          "speaker_id": "1",
          "time": "20210820 15:08"
        }
      ]
    }
  ]
