      "utterance": [
        {
          "id": "MDRW2100019953.1.1",
          "form": "여행좋아하시나요??",
          "original_form": "여행좋아하시나요??",
          "speaker_id": "1",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019953.1.2",
          "form": "네 여행은 좋아하는데 집돌이라서 많이 해보진 못 했어요 ㅠ",
          "original_form": "네 여행은 좋아하는데 집돌이라서 많이 해보진 못 했어요 ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019953.1.3",
          "form": "아하! 맞아요 ㅋㅋ 집순이들은 여행계획짜는건좋아하는데 막상가려면 귀찮아서,,",
          "original_form": "아하! 맞아요 ㅋㅋ 집순이들은 여행계획짜는건좋아하는데 막상가려면 귀찮아서,,",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019953.1.4",
          "form": "원래 여행가는거보다",
          "original_form": "원래 여행가는거보다",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019953.1.5",
          "form": "계획짜는게 더 재밌는 법",
          "original_form": "계획짜는게 더 재밌는 법",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019953.1.6",
          "form": "맞아요 ㅋㅋ 여행전날이 젤 재밌어요 ㄹㅇ",
          "original_form": "맞아요 ㅋㅋ 여행전날이 젤 재밌어요 ㄹㅇ",
          "speaker_id": "1",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019953.1.7",
          "form": "ㅋㅋㅋㅋㅋ 근데 계획 열심히짜도 계획대로 여행은 안 하는것 같아요",
          "original_form": "ㅋㅋㅋㅋㅋ 근데 계획 열심히짜도 계획대로 여행은 안 하는것 같아요",
          "speaker_id": "2",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019953.1.8",
          "form": "맞아요 ㅋㅋ 그냥 대략적으로만 짜는게 좋은거같아요",
          "original_form": "맞아요 ㅋㅋ 그냥 대략적으로만 짜는게 좋은거같아요",
          "speaker_id": "1",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019953.1.9",
          "form": "인정인정 ㅋㅋ",
          "original_form": "인정인정 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100019953.1.10",
          "form": "최근에 여행 가신적 있으세요??",
          "original_form": "최근에 여행 가신적 있으세요??",
          "speaker_id": "2",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100019953.1.11",
          "form": "최근엔 못갔죠 ㅠㅠ 코로롱터지고 암때두안갔어욮ㅋㅋ",
          "original_form": "최근엔 못갔죠 ㅠㅠ 코로롱터지고 암때두안갔어욮ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100019953.1.12",
          "form": "ㅠ 코로롱 얼른 끝나야할텐데 끝날 기미가 안 보여요",
          "original_form": "ㅠ 코로롱 얼른 끝나야할텐데 끝날 기미가 안 보여요",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100019953.1.13",
          "form": "전 이번 여름휴가때 안동 다녀왔어요 ㅎ",
          "original_form": "전 이번 여름휴가때 안동 다녀왔어요 ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100019953.1.14",
          "form": "오오 안동! 찜닭드셨나여??",
          "original_form": "오오 안동! 찜닭드셨나여??",
          "speaker_id": "1",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100019953.1.15",
          "form": "오 안동 찜닭 아시는구나! ㅋㅋ",
          "original_form": "오 안동 찜닭 아시는구나! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100019953.1.16",
          "form": "네 찜닭 먹었어요!!!!!!",
          "original_form": "네 찜닭 먹었어요!!!!!!",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100019953.1.17",
          "form": "오 !! 맛있었겠네요//",
          "original_form": "오 !! 맛있었겠네요//",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100019953.1.18",
          "form": "지금 야식으로 찜닭먹고싶어요 ㅠ",
          "original_form": "지금 야식으로 찜닭먹고싶어요 ㅠ",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100019953.1.19",
          "form": "모르시나요??",
          "original_form": "모르시나요??",
          "speaker_id": "2",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100019953.1.20",
          "form": "맛 있게 먹으면 0칼로리입니다",
          "original_form": "맛 있게 먹으면 0칼로리입니다",
          "speaker_id": "2",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100019953.1.21",
          "form": "맞아요 ㅋㅋㅋㅋㅋ 근데 근방에 찜닭하는곳이없네요..ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ 근데 근방에 찜닭하는곳이없네요..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100019953.1.22",
          "form": "ㅠㅜ 그럼 오늘은 참으시는걸로!",
          "original_form": "ㅠㅜ 그럼 오늘은 참으시는걸로!",
          "speaker_id": "2",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100019953.1.23",
          "form": "어쩔수없죠뭐 ㅜㅜ 찜닭은 대체품이없으니!",
          "original_form": "어쩔수없죠뭐 ㅜㅜ 찜닭은 대체품이없으니!",
          "speaker_id": "1",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100019953.1.24",
          "form": "맞아여 ㅋㅋ",
          "original_form": "맞아여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100019953.1.25",
          "form": "찜닭 하는 집이 없어서 못 먹은게 아니라",
          "original_form": "찜닭 하는 집이 없어서 못 먹은게 아니라",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100019953.1.26",
          "form": "야식 땡기는걸 참은걸로 해여 ㅋㅋ",
          "original_form": "야식 땡기는걸 참은걸로 해여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100019953.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ 좋아여~ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 좋아여~ㅋㅋㅋ",
