      "utterance": [
        {
          "id": "MDRW2100031147.1.1",
          "form": "나도 뮤지컬은 한번 보러 가보고 싶다",
          "original_form": "나도 뮤지컬은 한번 보러 가보고 싶다",
          "speaker_id": "2",
          "time": "20210904 18:05"
        },
        {
          "id": "MDRW2100031147.1.2",
          "form": "그치그치",
          "original_form": "그치그치",
          "speaker_id": "1",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100031147.1.3",
          "form": "맘마미아 한번 보고 싶음",
          "original_form": "맘마미아 한번 보고 싶음",
          "speaker_id": "1",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100031147.1.4",
          "form": "나는 요번에 했던 시카고 ㅋㅋ",
          "original_form": "나는 요번에 했던 시카고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:17"
        },
        {
          "id": "MDRW2100031147.1.5",
          "form": "오 유명한거다",
          "original_form": "오 유명한거다",
          "speaker_id": "1",
          "time": "20210904 18:26"
        },
        {
          "id": "MDRW2100031147.1.6",
          "form": "왜 보고싶은데?",
          "original_form": "왜 보고싶은데?",
          "speaker_id": "1",
          "time": "20210904 18:26"
        },
        {
          "id": "MDRW2100031147.1.7",
          "form": "클립 같은거 보니까 재밌어보였어서 ㅋㅋㅋ",
          "original_form": "클립 같은거 보니까 재밌어보였어서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:48"
        },
        {
          "id": "MDRW2100031147.1.8",
          "form": "클립을 찾아보면 어떠케해...",
          "original_form": "클립을 찾아보면 어떠케해...",
          "speaker_id": "1",
          "time": "20210904 19:13"
        },
        {
          "id": "MDRW2100031147.1.9",
          "form": "내가 찾아본게 아니라구..",
          "original_form": "내가 찾아본게 아니라구..",
          "speaker_id": "2",
          "time": "20210904 19:13"
        },
        {
          "id": "MDRW2100031147.1.10",
          "form": "트위터가 보여주던걸..",
          "original_form": "트위터가 보여주던걸..",
          "speaker_id": "2",
          "time": "20210904 19:13"
        },
        {
          "id": "MDRW2100031147.1.11",
          "form": "틔터가 나빴네 그런건 스포일러 당하면 안돼는데",
          "original_form": "틔터가 나빴네 그런건 스포일러 당하면 안돼는데",
          "speaker_id": "1",
          "time": "20210904 19:19"
        },
        {
          "id": "MDRW2100031147.1.12",
          "form": "ㅋㅋㅋㅋ 아냐 근데 내용은 없고",
          "original_form": "ㅋㅋㅋㅋ 아냐 근데 내용은 없고",
          "speaker_id": "2",
          "time": "20210904 19:31"
        },
        {
          "id": "MDRW2100031147.1.13",
          "form": "배우들 연기 비교 이런것만 봤어 ㅋㅋㅋ",
          "original_form": "배우들 연기 비교 이런것만 봤어 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:31"
        },
        {
          "id": "MDRW2100031147.1.14",
          "form": "아니 글쎜ㅋㅋㅋ 그게 안된단 것이니라",
          "original_form": "아니 글쎜ㅋㅋㅋ 그게 안된단 것이니라",
          "speaker_id": "1",
          "time": "20210904 19:33"
        },
        {
          "id": "MDRW2100031147.1.15",
          "form": "에잌ㅋㅋㅋ",
          "original_form": "에잌ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:34"
        },
        {
          "id": "MDRW2100031147.1.16",
          "form": "시카고 보니까 아직 하긴 하던데 이쪽 지역 공연은 이미 다 끝난 것도 같고",
          "original_form": "시카고 보니까 아직 하긴 하던데 이쪽 지역 공연은 이미 다 끝난 것도 같고",
          "speaker_id": "2",
          "time": "20210904 19:35"
        },
        {
          "id": "MDRW2100031147.1.17",
          "form": "그냥 외국에 가서 현지거 보자",
          "original_form": "그냥 외국에 가서 현지거 보자",
          "speaker_id": "1",
          "time": "20210904 19:37"
        },
        {
          "id": "MDRW2100031147.1.18",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:37"
        },
        {
          "id": "MDRW2100031147.1.19",
          "form": "눈으로만 보고 오기 ㅋㅋㅋ",
          "original_form": "눈으로만 보고 오기 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:37"
        },
        {
          "id": "MDRW2100031147.1.20",
          "form": "엌ㅋㅋㅋㅋㅋ",
          "original_form": "엌ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 19:39"
        },
        {
          "id": "MDRW2100031147.1.21",
          "form": "귀로도 들어!",
          "original_form": "귀로도 들어!",
          "speaker_id": "1",
          "time": "20210904 19:39"
        },
        {
          "id": "MDRW2100031147.1.22",
          "form": "귀가 꽉 막혀서 그건 안된데 ㅋㅋㅋㅋ",
          "original_form": "귀가 꽉 막혀서 그건 안된데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:40"
        },
        {
          "id": "MDRW2100031147.1.23",
          "form": "그럼 음악만 듣자",
          "original_form": "그럼 음악만 듣자",
          "speaker_id": "1",
          "time": "20210904 19:42"
        },
        {
          "id": "MDRW2100031147.1.24",
          "form": "내용 스포당하고ㅠ가면 그런데",
          "original_form": "내용 스포당하고ㅠ가면 그런데",
          "speaker_id": "1",
          "time": "20210904 19:42"
        },
        {
          "id": "MDRW2100031147.1.25",
          "form": "ㅋㅋㅋㅋ 어쩔수없어ㅜ",
          "original_form": "ㅋㅋㅋㅋ 어쩔수없어ㅜ",
          "speaker_id": "2",
          "time": "20210904 19:43"
        },
        {
          "id": "MDRW2100031147.1.26",
          "form": "하... 할쑤이ㅛ다",
          "original_form": "하... 할쑤이ㅛ다",
          "speaker_id": "1",
          "time": "20210904 19:43"
        }
      ]
    }
