      "utterance": [
        {
          "id": "MDRW2100020442.1.1",
          "form": "맞아요 저는 그래도 동료들이 좋아서",
          "original_form": "맞아요 저는 그래도 동료들이 좋아서",
          "speaker_id": "2",
          "time": "20210828 05:55"
        },
        {
          "id": "MDRW2100020442.1.2",
          "form": "오 다행이다",
          "original_form": "오 다행이다",
          "speaker_id": "1",
          "time": "20210828 05:55"
        },
        {
          "id": "MDRW2100020442.1.3",
          "form": "만족하고 있어요 ㅋㅋ",
          "original_form": "만족하고 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:55"
        },
        {
          "id": "MDRW2100020442.1.4",
          "form": "동료 좋은것도 복인데",
          "original_form": "동료 좋은것도 복인데",
          "speaker_id": "1",
          "time": "20210828 05:55"
        },
        {
          "id": "MDRW2100020442.1.5",
          "form": "복받으셨네용ㅋㅋ",
          "original_form": "복받으셨네용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 05:55"
        },
        {
          "id": "MDRW2100020442.1.6",
          "form": "무던한 걸 무기로 이해관계에 굳이 끼어들지않아서 ㅋㅋㅋ",
          "original_form": "무던한 걸 무기로 이해관계에 굳이 끼어들지않아서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:56"
        },
        {
          "id": "MDRW2100020442.1.7",
          "form": "눈새인양",
          "original_form": "눈새인양",
          "speaker_id": "2",
          "time": "20210828 05:56"
        },
        {
          "id": "MDRW2100020442.1.8",
          "form": "살아가면 편하져ㅋㅋㅋ",
          "original_form": "살아가면 편하져ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:56"
        },
        {
          "id": "MDRW2100020442.1.9",
          "form": "아하 그런거 좋아요ㅋㅋㅋ",
          "original_form": "아하 그런거 좋아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 05:56"
        },
        {
          "id": "MDRW2100020442.1.10",
          "form": "다 아는데 모르는척하는게",
          "original_form": "다 아는데 모르는척하는게",
          "speaker_id": "1",
          "time": "20210828 05:56"
        },
        {
          "id": "MDRW2100020442.1.11",
          "form": "더 편해요",
          "original_form": "더 편해요",
          "speaker_id": "1",
          "time": "20210828 05:56"
        },
        {
          "id": "MDRW2100020442.1.12",
          "form": "대학생때 아르바이트 해보셨어용??",
          "original_form": "대학생때 아르바이트 해보셨어용??",
          "speaker_id": "1",
          "time": "20210828 05:56"
        },
        {
          "id": "MDRW2100020442.1.13",
          "form": "맞아요 ㅋㅋㅋ무조건 모르는척 아닌척 ㅠㅠ",
          "original_form": "맞아요 ㅋㅋㅋ무조건 모르는척 아닌척 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 05:56"
        },
        {
          "id": "MDRW2100020442.1.14",
          "form": "거의 사생활 얘기도 안하려고 하고",
          "original_form": "거의 사생활 얘기도 안하려고 하고",
          "speaker_id": "2",
          "time": "20210828 05:57"
        },
        {
          "id": "MDRW2100020442.1.15",
          "form": "나중에 보면 좀 부풀려져있고 해서 ㅋㅋ",
          "original_form": "나중에 보면 좀 부풀려져있고 해서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:57"
        },
        {
          "id": "MDRW2100020442.1.16",
          "form": "네네ㅋㅋㅋ 난 아무것도 몰라요~~ 저도 그러고 다녔어요",
          "original_form": "네네ㅋㅋㅋ 난 아무것도 몰라요~~ 저도 그러고 다녔어요",
          "speaker_id": "1",
          "time": "20210828 05:57"
        },
        {
          "id": "MDRW2100020442.1.17",
          "form": "맞아욬ㅋㅋㅋ 소설이 되어있어요..",
          "original_form": "맞아욬ㅋㅋㅋ 소설이 되어있어요..",
          "speaker_id": "1",
          "time": "20210828 05:57"
        },
        {
          "id": "MDRW2100020442.1.18",
          "form": "대학때 아르바이트",
          "original_form": "대학때 아르바이트",
          "speaker_id": "2",
          "time": "20210828 05:57"
        },
        {
          "id": "MDRW2100020442.1.19",
          "form": "경마장에서 했어요",
          "original_form": "경마장에서 했어요",
          "speaker_id": "2",
          "time": "20210828 05:57"
        },
        {
          "id": "MDRW2100020442.1.20",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210828 05:57"
        },
        {
          "id": "MDRW2100020442.1.21",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "1",
          "time": "20210828 05:57"
        },
        {
          "id": "MDRW2100020442.1.22",
          "form": "완전 이색 아르바이트네요",
          "original_form": "완전 이색 아르바이트네요",
          "speaker_id": "1",
          "time": "20210828 05:58"
        },
        {
          "id": "MDRW2100020442.1.23",
          "form": "말 뛰는 거 보면서 ㅋㅋ",
          "original_form": "말 뛰는 거 보면서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:58"
        },
        {
          "id": "MDRW2100020442.1.24",
          "form": "민원실에서 고객 응대했어요",
          "original_form": "민원실에서 고객 응대했어요",
          "speaker_id": "2",
          "time": "20210828 05:58"
        },
        {
          "id": "MDRW2100020442.1.25",
          "form": "와 그냥 편견이지만 경마장 아르바이트면",
          "original_form": "와 그냥 편견이지만 경마장 아르바이트면",
          "speaker_id": "1",
          "time": "20210828 05:58"
        },
        {
          "id": "MDRW2100020442.1.26",
          "form": "시급 쎌것같아요",
          "original_form": "시급 쎌것같아요",
          "speaker_id": "1",
          "time": "20210828 05:58"
        },
        {
          "id": "MDRW2100020442.1.27",
          "form": "좀 꿀 알바였어요 ㅋㅋㅋ",
          "original_form": "좀 꿀 알바였어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 05:58"
        },
        {
          "id": "MDRW2100020442.1.28",
          "form": "와ㅠㅠ 부럽다",
          "original_form": "와ㅠㅠ 부럽다",
          "speaker_id": "1",
          "time": "20210828 05:58"
        },
        {
          "id": "MDRW2100020442.1.29",
          "form": "어 시급도 나름 괜트찮았어요 ㅋㅋ",
          "original_form": "어 시급도 나름 괜트찮았어요 ㅋㅋ",
