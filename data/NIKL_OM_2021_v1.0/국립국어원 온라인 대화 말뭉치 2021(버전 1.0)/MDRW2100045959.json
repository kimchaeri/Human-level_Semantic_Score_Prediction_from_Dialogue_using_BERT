      "utterance": [
        {
          "id": "MDRW2100045959.1.1",
          "form": "으어 출근싫어",
          "original_form": "으어 출근싫어",
          "speaker_id": "2",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100045959.1.2",
          "form": "카톡 업그레이드 끝났다",
          "original_form": "카톡 업그레이드 끝났다",
          "speaker_id": "2",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100045959.1.3",
          "form": "왜 하필지금해서 바쁜데 두번 일하게",
          "original_form": "왜 하필지금해서 바쁜데 두번 일하게",
          "speaker_id": "2",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100045959.1.4",
          "form": "ㅏㄴ드냐",
          "original_form": "ㅏㄴ드냐",
          "speaker_id": "2",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100045959.1.5",
          "form": "맞아...",
          "original_form": "맞아...",
          "speaker_id": "1",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045959.1.6",
          "form": "직장인에게 이시간이 젤 바쁜 타이밍인데 ㅡㅡ",
          "original_form": "직장인에게 이시간이 젤 바쁜 타이밍인데 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045959.1.7",
          "form": "그니까.. 눈치챙겨 카카오",
          "original_form": "그니까.. 눈치챙겨 카카오",
          "speaker_id": "2",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045959.1.8",
          "form": "(?)",
          "original_form": "(?)",
          "speaker_id": "2",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045959.1.9",
          "form": "맞아... 지금 출근하느라 다들 예민하다규",
          "original_form": "맞아... 지금 출근하느라 다들 예민하다규",
          "speaker_id": "1",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045959.1.10",
          "form": "ㅋㅋ큐ㅠㅠ",
          "original_form": "ㅋㅋ큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:09"
        },
        {
          "id": "MDRW2100045959.1.11",
          "form": "설마 나만 업그레이드했니?(카카오짤짤",
          "original_form": "설마 나만 업그레이드했니?(카카오짤짤",
          "speaker_id": "2",
          "time": "20210908 09:09"
        },
        {
          "id": "MDRW2100045959.1.12",
          "form": "바쁜데 날 농락했어!",
          "original_form": "바쁜데 날 농락했어!",
          "speaker_id": "2",
          "time": "20210908 09:09"
        },
        {
          "id": "MDRW2100045959.1.13",
          "form": "일단 난 업그레이드 안했어 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "일단 난 업그레이드 안했어 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:09"
        },
        {
          "id": "MDRW2100045959.1.14",
          "form": "그래도 10시 출근이라 좀 낫겠다",
          "original_form": "그래도 10시 출근이라 좀 낫겠다",
          "speaker_id": "1",
          "time": "20210908 09:09"
        },
        {
          "id": "MDRW2100045959.1.15",
          "form": "너도 출근러잖아!(",
          "original_form": "너도 출근러잖아!(",
          "speaker_id": "2",
          "time": "20210908 09:09"
        },
        {
          "id": "MDRW2100045959.1.16",
          "form": "ㅋㅋㅋ ㅇㅇ.. 9시출근이었으면",
          "original_form": "ㅋㅋㅋ ㅇㅇ.. 9시출근이었으면",
          "speaker_id": "2",
          "time": "20210908 09:09"
        },
        {
          "id": "MDRW2100045959.1.17",
          "form": "한시간 반 잡고 나갔어야할걸",
          "original_form": "한시간 반 잡고 나갔어야할걸",
          "speaker_id": "2",
          "time": "20210908 09:09"
        },
        {
          "id": "MDRW2100045959.1.18",
          "form": "차가 엄청 막히는 곳이라",
          "original_form": "차가 엄청 막히는 곳이라",
          "speaker_id": "2",
          "time": "20210908 09:09"
        },
        {
          "id": "MDRW2100045959.1.19",
          "form": "아 그쪽이 많이 막혀?",
          "original_form": "아 그쪽이 많이 막혀?",
          "speaker_id": "1",
          "time": "20210908 09:10"
        },
        {
          "id": "MDRW2100045959.1.20",
          "form": "ㅇㅇ 그쪽에 정부청사부터 시청 법원 등등 다 있어서",
          "original_form": "ㅇㅇ 그쪽에 정부청사부터 시청 법원 등등 다 있어서",
          "speaker_id": "2",
          "time": "20210908 09:10"
        },
        {
          "id": "MDRW2100045959.1.21",
          "form": "직장인 천지라",
          "original_form": "직장인 천지라",
          "speaker_id": "2",
          "time": "20210908 09:10"
        },
        {
          "id": "MDRW2100045959.1.22",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210908 09:10"
        },
        {
          "id": "MDRW2100045959.1.23",
          "form": "와 막힐만하네 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "와 막힐만하네 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:12"
        },
        {
          "id": "MDRW2100045959.1.24",
          "form": "물론 그동네 사는.사람들도 있겠지만 아마 밖에서 오는 사람들이 더 많을거라",
          "original_form": "물론 그동네 사는.사람들도 있겠지만 아마 밖에서 오는 사람들이 더 많을거라",
          "speaker_id": "2",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100045959.1.25",
          "form": "ㅇㅇ 그 고개 넘어가는 길목이 있는데",
          "original_form": "ㅇㅇ 그 고개 넘어가는 길목이 있는데",
          "speaker_id": "2",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100045959.1.26",
          "form": "거긴...(절래",
          "original_form": "거긴...(절래",
          "speaker_id": "2",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100045959.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 나같으면",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 나같으면",
          "speaker_id": "1",
          "time": "20210908 09:14"
        },
        {
          "id": "MDRW2100045959.1.28",
          "form": "그냥 회사 근처에",
          "original_form": "그냥 회사 근처에",
          "speaker_id": "1",
          "time": "20210908 09:14"
        },
        {
          "id": "MDRW2100045959.1.29",
          "form": "전세 알아볼듯",
          "original_form": "전세 알아볼듯",
          "speaker_id": "1",
          "time": "20210908 09:14"
        },
        {
          "id": "MDRW2100045959.1.30",
          "form": "안그래도 일하기 싫은데...",
          "original_form": "안그래도 일하기 싫은데...",
          "speaker_id": "1",
          "time": "20210908 09:14"
        },
        {
          "id": "MDRW2100045959.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:15"
        },
        {
          "id": "MDRW2100045959.1.32",
          "form": "진짜.. 오래 일하는 사람이면 근처라도 알아볼듯",
          "original_form": "진짜.. 오래 일하는 사람이면 근처라도 알아볼듯",
          "speaker_id": "2",
          "time": "20210908 09:15"
        },
        {
          "id": "MDRW2100045959.1.33",
          "form": "아니면 지하철 다니는 동네로",
          "original_form": "아니면 지하철 다니는 동네로",
          "speaker_id": "2",
          "time": "20210908 09:15"
        },
        {
          "id": "MDRW2100045959.1.34",
          "form": "거긴 지하철 다니거든...",
          "original_form": "거긴 지하철 다니거든...",
          "speaker_id": "2",
          "time": "20210908 09:15"
        },
        {
          "id": "MDRW2100045959.1.35",
          "form": "그정도면 지하철도 지옥철일거야...",
          "original_form": "그정도면 지하철도 지옥철일거야...",
          "speaker_id": "1",
          "time": "20210908 09:15"
        },
        {
          "id": "MDRW2100045959.1.36",
          "form": "우리 회사 7호선인대",
          "original_form": "우리 회사 7호선인대",
          "speaker_id": "1",
          "time": "20210908 09:15"
        },
        {
          "id": "MDRW2100045959.1.37",
