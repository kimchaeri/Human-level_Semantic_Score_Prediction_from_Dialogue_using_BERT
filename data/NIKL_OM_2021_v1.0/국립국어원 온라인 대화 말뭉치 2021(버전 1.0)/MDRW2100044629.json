      "utterance": [
        {
          "id": "MDRW2100044629.1.1",
          "form": "르누아르님은 무슨계절 좋아하세요?",
          "original_form": "르누아르님은 무슨계절 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 01:33"
        },
        {
          "id": "MDRW2100044629.1.2",
          "form": "저는 여름 제일 좋아해요 ㅋㅋ",
          "original_form": "저는 여름 제일 좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:33"
        },
        {
          "id": "MDRW2100044629.1.3",
          "form": "뭔가 벅차는 느낌 그런 걸 좋아해서 ㅋㅋ",
          "original_form": "뭔가 벅차는 느낌 그런 걸 좋아해서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:34"
        },
        {
          "id": "MDRW2100044629.1.4",
          "form": "더위에 강하신가요?",
          "original_form": "더위에 강하신가요?",
          "speaker_id": "1",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100044629.1.5",
          "form": "추위보단 덜타요!!",
          "original_form": "추위보단 덜타요!!",
          "speaker_id": "2",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100044629.1.6",
          "form": "근데 양산은 꼭 써요 ㅋㅋ",
          "original_form": "근데 양산은 꼭 써요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100044629.1.7",
          "form": "양산 중요하죠 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "양산 중요하죠 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100044629.1.8",
          "form": "생존템 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "생존템 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100044629.1.9",
          "form": "양산없으면 녹아내림ㅋㅋㅋㅋㅋㅋ",
          "original_form": "양산없으면 녹아내림ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100044629.1.10",
          "form": "그쵸.. 양산 쓰고나면 없던 시절로 절대 못 돌아가요 ㅋㅋㅋ",
          "original_form": "그쵸.. 양산 쓰고나면 없던 시절로 절대 못 돌아가요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100044629.1.11",
          "form": "정수리가 타는 느낌..",
          "original_form": "정수리가 타는 느낌..",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100044629.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ다시돌아갈수없어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ다시돌아갈수없어요",
          "speaker_id": "1",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100044629.1.13",
          "form": "ㅋㅋㅋㅋ 진짜 진작부터 쓸걸.. 잘만 쓰면 선크림보다 효과는 더 좋다네요",
          "original_form": "ㅋㅋㅋㅋ 진짜 진작부터 쓸걸.. 잘만 쓰면 선크림보다 효과는 더 좋다네요",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100044629.1.14",
          "form": "저도 처음양산써보고",
          "original_form": "저도 처음양산써보고",
          "speaker_id": "1",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100044629.1.15",
          "form": "쓰지않은지난날들을 후회햇어욬ㅋㅋㅋㅋ",
          "original_form": "쓰지않은지난날들을 후회햇어욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:49"
        },
        {
          "id": "MDRW2100044629.1.16",
          "form": "그쵸.. 앞으로 더 많이 써야돼요 그래도 예전보단 많이 대중화된거 같아요",
          "original_form": "그쵸.. 앞으로 더 많이 써야돼요 그래도 예전보단 많이 대중화된거 같아요",
          "speaker_id": "2",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100044629.1.17",
          "form": "맞아요 은근보이더라구요",
          "original_form": "맞아요 은근보이더라구요",
          "speaker_id": "1",
          "time": "20210908 01:54"
        },
        {
          "id": "MDRW2100044629.1.18",
          "form": "이제 여름도 끝물 같네요 가을이 점점 오는 느낌 ㅎㅎ",
          "original_form": "이제 여름도 끝물 같네요 가을이 점점 오는 느낌 ㅎㅎ",
