      "utterance": [
        {
          "id": "MDRW2100015514.1.1",
          "form": "맞아 눈병도 별로 안생기고",
          "original_form": "맞아 눈병도 별로 안생기고",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015514.1.2",
          "form": "그러네 환절기에 꼭 눈병 유행했었던 거 같은데",
          "original_form": "그러네 환절기에 꼭 눈병 유행했었던 거 같은데",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015514.1.3",
          "form": "그러니까 그건 좋아",
          "original_form": "그러니까 그건 좋아",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015514.1.4",
          "form": "열심히 손 씻고 소독까지 하니까 그런가봐",
          "original_form": "열심히 손 씻고 소독까지 하니까 그런가봐",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015514.1.5",
          "form": "그게 아무래도 젤 크지",
          "original_form": "그게 아무래도 젤 크지",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015514.1.6",
          "form": "그놈에 코로나만 잡히면 좋을 텐데",
          "original_form": "그놈에 코로나만 잡히면 좋을 텐데",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015514.1.7",
          "form": "우리도 영국처럼 위드 코로나 할 수 있으려나",
          "original_form": "우리도 영국처럼 위드 코로나 할 수 있으려나",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015514.1.8",
          "form": "같이 살기 싫은데",
          "original_form": "같이 살기 싫은데",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015514.1.9",
          "form": "근데 없어지지 않을거 같은데",
          "original_form": "근데 없어지지 않을거 같은데",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015514.1.10",
          "form": "약간 감기 같은 존재가 되는 건가?",
          "original_form": "약간 감기 같은 존재가 되는 건가?",
          "speaker_id": "1",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100015514.1.11",
          "form": "독감 같은거지",
          "original_form": "독감 같은거지",
          "speaker_id": "2",
          "time": "20210825 23:26"
        },
        {
          "id": "MDRW2100015514.1.12",
          "form": "전문가들도 부정적이던데",
          "original_form": "전문가들도 부정적이던데",
          "speaker_id": "2",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100015514.1.13",
          "form": "전문가들도 잡히진 않을 거래?",
          "original_form": "전문가들도 잡히진 않을 거래?",
          "speaker_id": "1",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100015514.1.14",
          "form": "가능성이 희박하다고 들었어 ㅠㅜ",
          "original_form": "가능성이 희박하다고 들었어 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100015514.1.15",
          "form": "지금 돌아가는 거 보면 어려워 보이기는 한다",
          "original_form": "지금 돌아가는 거 보면 어려워 보이기는 한다",
          "speaker_id": "1",
          "time": "20210825 23:29"
        }
      ]
