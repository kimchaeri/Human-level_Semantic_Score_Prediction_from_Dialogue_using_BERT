        {
          "id": "MDRW2100002086.1.1",
          "form": "유투브 많이보세여?",
          "original_form": "유투브 많이보세여?",
          "speaker_id": "1",
          "time": "20210819 23:28"
        },
        {
          "id": "MDRW2100002086.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210819 23:29"
        },
        {
          "id": "MDRW2100002086.1.3",
          "form": "주로 어떤 류?",
          "original_form": "주로 어떤 류?",
          "speaker_id": "1",
          "time": "20210819 23:29"
        },
        {
          "id": "MDRW2100002086.1.4",
          "form": "연예인요",
          "original_form": "연예인요",
          "speaker_id": "2",
          "time": "20210819 23:29"
        },
        {
          "id": "MDRW2100002086.1.5",
          "form": "댄스동영상 같은거도 보세여?",
          "original_form": "댄스동영상 같은거도 보세여?",
          "speaker_id": "1",
          "time": "20210819 23:30"
        },
        {
          "id": "MDRW2100002086.1.6",
          "form": "개콘 나온사람들 개인채널 많이 하더라구요",
          "original_form": "개콘 나온사람들 개인채널 많이 하더라구요",
          "speaker_id": "2",
          "time": "20210819 23:30"
        },
        {
          "id": "MDRW2100002086.1.7",
          "form": "아 맞아요 허안나 술방도 재밌어요",
          "original_form": "아 맞아요 허안나 술방도 재밌어요",
          "speaker_id": "1",
          "time": "20210819 23:30"
        },
        {
          "id": "MDRW2100002086.1.8",
          "form": "ㅋㅋ 개콘 없어지고 유투브로 보는 것 같아요",
          "original_form": "ㅋㅋ 개콘 없어지고 유투브로 보는 것 같아요",
          "speaker_id": "2",
          "time": "20210819 23:31"
        },
        {
          "id": "MDRW2100002086.1.9",
          "form": "맞아요 개그동영상 잼나죠",
          "original_form": "맞아요 개그동영상 잼나죠",
          "speaker_id": "1",
          "time": "20210819 23:31"
        },
        {
          "id": "MDRW2100002086.1.10",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "2",
          "time": "20210819 23:32"
        },
        {
          "id": "MDRW2100002086.1.11",
          "form": "넷플 추천작 있나요",
          "original_form": "넷플 추천작 있나요",
          "speaker_id": "1",
          "time": "20210819 23:32"
        },
        {
          "id": "MDRW2100002086.1.12",
          "form": "모가디슈 잼난데요",
          "original_form": "모가디슈 잼난데요",
