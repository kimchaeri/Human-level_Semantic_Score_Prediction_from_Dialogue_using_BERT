      "utterance": [
        {
          "id": "MDRW2100015063.1.1",
          "form": "유산소 운동해요!",
          "original_form": "유산소 운동해요!",
          "speaker_id": "2",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100015063.1.2",
          "form": "그래봐야 산책(조깅)이지만..",
          "original_form": "그래봐야 산책(조깅)이지만..",
          "speaker_id": "2",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100015063.1.3",
          "form": "와 저는 코로나 터지기 전까지 수영했었는데",
          "original_form": "와 저는 코로나 터지기 전까지 수영했었는데",
          "speaker_id": "1",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100015063.1.4",
          "form": "못해서 아쉬워요",
          "original_form": "못해서 아쉬워요",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015063.1.5",
          "form": "와~ 수영!",
          "original_form": "와~ 수영!",
          "speaker_id": "2",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015063.1.6",
          "form": "저 수영 배우다가 포기했어요",
          "original_form": "저 수영 배우다가 포기했어요",
          "speaker_id": "2",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015063.1.7",
          "form": "산책 좋죠 그냥 시간날때 하시나요?",
          "original_form": "산책 좋죠 그냥 시간날때 하시나요?",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015063.1.8",
          "form": "보통 퇴근 후 저녁 먹고 바로 해요",
          "original_form": "보통 퇴근 후 저녁 먹고 바로 해요",
          "speaker_id": "2",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015063.1.9",
          "form": "아 정말요? 저두 사실 1년밖에 못했어요",
          "original_form": "아 정말요? 저두 사실 1년밖에 못했어요",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015063.1.10",
          "form": "공원 산책하면서 귀여운 멍뭉이 봐요",
          "original_form": "공원 산책하면서 귀여운 멍뭉이 봐요",
          "speaker_id": "2",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015063.1.11",
          "form": "아 진짜 부지런하시다",
          "original_form": "아 진짜 부지런하시다",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015063.1.12",
          "form": "맞아요 딱 그때 산책하면",
          "original_form": "맞아요 딱 그때 산책하면",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015063.1.13",
          "form": "여름엔 안했어요 ㅋㅋㅋㅋ 봄가을겨울 한정..",
          "original_form": "여름엔 안했어요 ㅋㅋㅋㅋ 봄가을겨울 한정..",
          "speaker_id": "2",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015063.1.14",
          "form": "공원에 많더라구요",
          "original_form": "공원에 많더라구요",
          "speaker_id": "1",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015063.1.15",
          "form": "여름은 진짜 너무 쪄죽죠 ㅠㅠ",
          "original_form": "여름은 진짜 너무 쪄죽죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015063.1.16",
          "form": "그리고 따릉이도 열심히 탔어요!",
          "original_form": "그리고 따릉이도 열심히 탔어요!",
          "speaker_id": "2",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015063.1.17",
          "form": "실내에서 운동해야해요",
          "original_form": "실내에서 운동해야해요",
          "speaker_id": "1",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015063.1.18",
          "form": "저 백신2차 맞으면 가을 중순쯤이라서 그때 따릉이 타려구요~",
          "original_form": "저 백신2차 맞으면 가을 중순쯤이라서 그때 따릉이 타려구요~",
          "speaker_id": "2",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015063.1.19",
          "form": "마침 집 앞에 대여소 생겨서 출퇴근도 가능할 것 같아서요",
          "original_form": "마침 집 앞에 대여소 생겨서 출퇴근도 가능할 것 같아서요",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100015063.1.20",
          "form": "따릉이 가끔 타면 진짜 재밌죠 ㅋㅋ",
          "original_form": "따릉이 가끔 타면 진짜 재밌죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100015063.1.21",
          "form": "1시간권 두번 다신 안끊을거에요..",
          "original_form": "1시간권 두번 다신 안끊을거에요..",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100015063.1.22",
          "form": "와 대박 저는 킥보드 탔는데 운동 위해서 따릉이로 바꿔야겠어요",
          "original_form": "와 대박 저는 킥보드 탔는데 운동 위해서 따릉이로 바꿔야겠어요",
          "speaker_id": "1",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100015063.1.23",
          "form": "어찌나 시간 금방 가던지 대여소 찾느라 데이터 다 써요 맨날",
          "original_form": "어찌나 시간 금방 가던지 대여소 찾느라 데이터 다 써요 맨날",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100015063.1.24",
          "form": "아 왜요??",
          "original_form": "아 왜요??",
          "speaker_id": "1",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100015063.1.25",
          "form": "공유 킥보드요?ㅎㅎ",
          "original_form": "공유 킥보드요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100015063.1.26",
          "form": "네 ㅋㅋㅋ근데 좀 저는 걱정되더라고요 운동도 안되니까",
          "original_form": "네 ㅋㅋㅋ근데 좀 저는 걱정되더라고요 운동도 안되니까",
          "speaker_id": "1",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100015063.1.27",
          "form": "안하고 걷던가 따릉이 타야겠어요",
          "original_form": "안하고 걷던가 따릉이 타야겠어요",
          "speaker_id": "1",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100015063.1.28",
          "form": "ㅠㅠ다음엔 따릉이로 운동하길 바라며 대화 마쳐요!",
          "original_form": "ㅠㅠ다음엔 따릉이로 운동하길 바라며 대화 마쳐요!",
          "speaker_id": "2",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100015063.1.29",
          "form": "주제 막 바꾸셔도 돼요 ㅋㅋ",
          "original_form": "주제 막 바꾸셔도 돼요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100015063.1.30",
          "form": "혹시 식단관리도 하세요???",
          "original_form": "혹시 식단관리도 하세요???",
          "speaker_id": "1",
          "time": "20210825 21:46"
        }
      ]
    }
