          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044277.1.3",
          "form": "아 ㅋㅋㅋ봅니다 봐여",
          "original_form": "아 ㅋㅋㅋ봅니다 봐여",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044277.1.4",
          "form": "열시반에 시작함~",
          "original_form": "열시반에 시작함~",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044277.1.5",
          "form": "대기즁~",
          "original_form": "대기즁~",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044277.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ본방사수하나??",
          "original_form": "ㅋㅋㅋㅋㅋㅋ본방사수하나??",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044277.1.7",
          "form": "ㅋㅋㅋㅋ할라구",
          "original_form": "ㅋㅋㅋㅋ할라구",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044277.1.8",
          "form": "안그럼 넘 늦게 끝나",
          "original_form": "안그럼 넘 늦게 끝나",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044277.1.9",
          "form": "어느팀 젤 조아해",
          "original_form": "어느팀 젤 조아해",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044277.1.10",
          "form": "난 아마 ygx?",
          "original_form": "난 아마 ygx?",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044277.1.11",
          "form": "오 그렇군 리정 원픽인가여",
          "original_form": "오 그렇군 리정 원픽인가여",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044277.1.12",
          "form": "리정이 죻아요",
          "original_form": "리정이 죻아요",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044277.1.13",
          "form": "네넹 제 원픽인듯",
          "original_form": "네넹 제 원픽인듯",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044277.1.14",
          "form": "ㅋㅋㅋㅋ리정 짖짜 실력파",
          "original_form": "ㅋㅋㅋㅋ리정 짖짜 실력파",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044277.1.15",
          "form": "나도 리정좋음",
          "original_form": "나도 리정좋음",
          "speaker_id": "1",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044277.1.16",
          "form": "ㅋㅋㅋㅋ어느팀 젤 좋아햐",
          "original_form": "ㅋㅋㅋㅋ어느팀 젤 좋아햐",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044277.1.17",
          "form": "모니카쌤도 조음 꺄륵",
          "original_form": "모니카쌤도 조음 꺄륵",
          "speaker_id": "1",
