      "utterance": [
        {
          "id": "MDRW2100039802.1.1",
          "form": "우리 노래 한 줄씩 이어 불러요",
          "original_form": "우리 노래 한 줄씩 이어 불러요",
          "speaker_id": "1",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039802.1.2",
          "form": "무슨 노래 할래요",
          "original_form": "무슨 노래 할래요",
          "speaker_id": "1",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039802.1.3",
          "form": "거부 없음",
          "original_form": "거부 없음",
          "speaker_id": "1",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039802.1.4",
          "form": "님이 먼저 시작하시면",
          "original_form": "님이 먼저 시작하시면",
          "speaker_id": "2",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039802.1.5",
          "form": "제가 눈치껏",
          "original_form": "제가 눈치껏",
          "speaker_id": "2",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039802.1.6",
          "form": "따라가졸게요",
          "original_form": "따라가졸게요",
          "speaker_id": "2",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039802.1.7",
          "form": "이상한노래 금지",
          "original_form": "이상한노래 금지",
          "speaker_id": "2",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039802.1.8",
          "form": "가슴이 데인 것처럼",
          "original_form": "가슴이 데인 것처럼",
          "speaker_id": "1",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039802.1.9",
          "form": "정신이 너무 없어",
          "original_form": "정신이 너무 없어",
          "speaker_id": "2",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039802.1.10",
          "form": "땡입니다",
          "original_form": "땡입니다",
          "speaker_id": "1",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039802.1.11",
          "form": "당신 이민자예요?",
          "original_form": "당신 이민자예요?",
          "speaker_id": "1",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039802.1.12",
          "form": "ne.",
          "original_form": "ne.",
          "speaker_id": "2",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039802.1.13",
          "form": "눈물이 마른것처럼",
          "original_form": "눈물이 마른것처럼",
          "speaker_id": "2",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039802.1.14",
          "form": "님 작사가 세요?",
          "original_form": "님 작사가 세요?",
          "speaker_id": "1",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039802.1.15",
          "form": "헤어지는 내가 너무나 그립지만",
          "original_form": "헤어지는 내가 너무나 그립지만",
          "speaker_id": "2",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039802.1.16",
          "form": "원곡 망치지마세요",
          "original_form": "원곡 망치지마세요",
          "speaker_id": "1",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039802.1.17",
          "form": "가슴을 데인것처럼,,,,",
          "original_form": "가슴을 데인것처럼,,,,",
          "speaker_id": "1",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039802.1.18",
          "form": "혼지불러보세요",
          "original_form": "혼지불러보세요",
          "speaker_id": "2",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039802.1.19",
          "form": "눈물에 패인 것처럼,,,,~~~~~~",
          "original_form": "눈물에 패인 것처럼,,,,~~~~~~",
          "speaker_id": "1",
          "time": "20210903 21:40"
        },
        {
