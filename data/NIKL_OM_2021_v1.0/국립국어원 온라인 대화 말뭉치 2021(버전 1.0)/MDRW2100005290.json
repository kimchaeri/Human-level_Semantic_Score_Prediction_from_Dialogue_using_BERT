      "utterance": [
        {
          "id": "MDRW2100005290.1.1",
          "form": "이번 여름 옥수수나 복숭아는 안 먹고 지나갔네",
          "original_form": "이번 여름 옥수수나 복숭아는 안 먹고 지나갔네",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005290.1.2",
          "form": "옥수수 먹었잖아",
          "original_form": "옥수수 먹었잖아",
          "speaker_id": "1",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005290.1.3",
          "form": "초당",
          "original_form": "초당",
          "speaker_id": "1",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005290.1.4",
          "form": "찰옥수수 말이여~",
          "original_form": "찰옥수수 말이여~",
          "speaker_id": "2",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005290.1.5",
          "form": "아아 찰옥수수",
          "original_form": "아아 찰옥수수",
          "speaker_id": "1",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005290.1.6",
          "form": "아직 철이잖아 옥수수 함 살까?",
          "original_form": "아직 철이잖아 옥수수 함 살까?",
          "speaker_id": "1",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005290.1.7",
          "form": "누가 옥수수 쪄서 알맹이만 주면 좋겠다ㅋㅋ",
          "original_form": "누가 옥수수 쪄서 알맹이만 주면 좋겠다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005290.1.8",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005290.1.9",
          "form": "그런것도 찾아보면 있을지 몰라",
          "original_form": "그런것도 찾아보면 있을지 몰라",
          "speaker_id": "1",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005290.1.10",
          "form": "냉동 찰옥수수 알곡 이런거",
          "original_form": "냉동 찰옥수수 알곡 이런거",
          "speaker_id": "1",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005290.1.11",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005290.1.12",
          "form": "갓 삶은 것 먹고싶은데",
          "original_form": "갓 삶은 것 먹고싶은데",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005290.1.13",
          "form": "욕심은 끝이 없다",
          "original_form": "욕심은 끝이 없다",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005290.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005290.1.15",
          "form": "그건 욕심이지",
          "original_form": "그건 욕심이지",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005290.1.16",
          "form": "찰옥수수 함 시켜먹자 뉴슈가 넣고 쪄서",
          "original_form": "찰옥수수 함 시켜먹자 뉴슈가 넣고 쪄서",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005290.1.17",
          "form": "그래 여름 지나기 전에",
          "original_form": "그래 여름 지나기 전에",
          "speaker_id": "2",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005290.1.18",
          "form": "여름 가기 전에 먹어야할 음식이 또 있나?",
          "original_form": "여름 가기 전에 먹어야할 음식이 또 있나?",
          "speaker_id": "2",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005290.1.19",
          "form": "민어?",
          "original_form": "민어?",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005290.1.20",
          "form": "아 민어는",
          "original_form": "아 민어는",
          "speaker_id": "2",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005290.1.21",
          "form": "말복기념 민어챙기기",
          "original_form": "말복기념 민어챙기기",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005290.1.22",
          "form": "담주에 꼭 먹자",
          "original_form": "담주에 꼭 먹자",
          "speaker_id": "2",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005290.1.23",
          "form": "말복은 이미 열흘이나 지났지만",
          "original_form": "말복은 이미 열흘이나 지났지만",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005290.1.24",
          "form": "그려 ㅋㅋ 담주에 푸룬도 올텐데",
          "original_form": "그려 ㅋㅋ 담주에 푸룬도 올텐데",
          "speaker_id": "1",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005290.1.25",
          "form": "그거 맛있음 좋겠네",
          "original_form": "그거 맛있음 좋겠네",
          "speaker_id": "1",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005290.1.26",
          "form": "복달임 음식은 먹는다!",
          "original_form": "복달임 음식은 먹는다!",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005290.1.27",
          "form": "말복이 열흘 전이라니",
          "original_form": "말복이 열흘 전이라니",
          "speaker_id": "1",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005290.1.28",
          "form": "시간 잘간자",
          "original_form": "시간 잘간자",
          "speaker_id": "1",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005290.1.29",
          "form": "푸룬푸룬~",
          "original_form": "푸룬푸룬~",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005290.1.30",
          "form": "간다",
          "original_form": "간다",
          "speaker_id": "1",
          "time": "20210821 21:50"
        }
      ]
    }
  ]
