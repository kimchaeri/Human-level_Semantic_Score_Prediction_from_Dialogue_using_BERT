      "utterance": [
        {
          "id": "MDRW2100043124.1.1",
          "form": "유투브보시나요",
          "original_form": "유투브보시나요",
          "speaker_id": "1",
          "time": "20210907 14:36"
        },
        {
          "id": "MDRW2100043124.1.2",
          "form": "네 유튜브 자주 봐요",
          "original_form": "네 유튜브 자주 봐요",
          "speaker_id": "2",
          "time": "20210907 14:37"
        },
        {
          "id": "MDRW2100043124.1.3",
          "form": "어떤거보세요",
          "original_form": "어떤거보세요",
          "speaker_id": "1",
          "time": "20210907 14:39"
        },
        {
          "id": "MDRW2100043124.1.4",
          "form": "그냥 예능클립 올라온 거 봐요",
          "original_form": "그냥 예능클립 올라온 거 봐요",
          "speaker_id": "2",
          "time": "20210907 14:40"
        },
        {
          "id": "MDRW2100043124.1.5",
          "form": "요즘에 예능 통으로 보기 힘들더라구요 ㅋㅋ",
          "original_form": "요즘에 예능 통으로 보기 힘들더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:40"
        },
        {
          "id": "MDRW2100043124.1.6",
          "form": "ㅎㅎ좋아하는연예인있으세요",
          "original_form": "ㅎㅎ좋아하는연예인있으세요",
          "speaker_id": "1",
          "time": "20210907 14:40"
        },
        {
          "id": "MDRW2100043124.1.7",
          "form": "아뇨 지금은 없어요 ㅋㅋ",
          "original_form": "아뇨 지금은 없어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:41"
        },
        {
          "id": "MDRW2100043124.1.8",
          "form": "ㅎㅎㅎ넷플구독하세요!",
          "original_form": "ㅎㅎㅎ넷플구독하세요!",
          "speaker_id": "1",
          "time": "20210907 14:43"
        },
        {
          "id": "MDRW2100043124.1.9",
          "form": "넷플 계속 고민하고 있는데 저번에 한달무료 쓸때 생각보다 잘 못써가지고 그냥 해지했거든요",
          "original_form": "넷플 계속 고민하고 있는데 저번에 한달무료 쓸때 생각보다 잘 못써가지고 그냥 해지했거든요",
          "speaker_id": "2",
          "time": "20210907 14:47"
        },
        {
          "id": "MDRW2100043124.1.10",
          "form": "요즘에 뭐가 재미있나요?",
          "original_form": "요즘에 뭐가 재미있나요?",
          "speaker_id": "2",
          "time": "20210907 14:48"
        },
        {
          "id": "MDRW2100043124.1.11",
          "form": "ㅎㅎ저는에밀리파리에가다재밌게봤어요",
          "original_form": "ㅎㅎ저는에밀리파리에가다재밌게봤어요",
          "speaker_id": "1",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100043124.1.12",
          "form": "오 드라마인가요?",
          "original_form": "오 드라마인가요?",
          "speaker_id": "2",
          "time": "20210907 14:51"
        },
        {
          "id": "MDRW2100043124.1.13",
          "form": "시즌제드라마인데 재밌더라고요~",
          "original_form": "시즌제드라마인데 재밌더라고요~",
          "speaker_id": "1",
          "time": "20210907 14:52"
        },
        {
          "id": "MDRW2100043124.1.14",
          "form": "어떤 배우가 나오나요??",
          "original_form": "어떤 배우가 나오나요??",
          "speaker_id": "2",
          "time": "20210907 14:53"
        },
        {
          "id": "MDRW2100043124.1.15",
          "form": "음 외국배운데",
          "original_form": "음 외국배운데",
          "speaker_id": "1",
          "time": "20210907 14:57"
        },
        {
          "id": "MDRW2100043124.1.16",
          "form": "이뻐요",
          "original_form": "이뻐요",
          "speaker_id": "1",
          "time": "20210907 14:57"
        },
        {
          "id": "MDRW2100043124.1.17",
          "form": "한번 검색해봐야겠네요 ㅋㅋㅋ",
          "original_form": "한번 검색해봐야겠네요 ㅋㅋㅋ",
