      "utterance": [
        {
          "id": "MDRW2100027207.1.1",
          "form": "학교요즘",
          "original_form": "학교요즘",
          "speaker_id": "2",
          "time": "20210902 10:31"
        },
        {
          "id": "MDRW2100027207.1.2",
          "form": "줌으로 수업하는곳이 많죠",
          "original_form": "줌으로 수업하는곳이 많죠",
          "speaker_id": "2",
          "time": "20210902 10:31"
        },
        {
          "id": "MDRW2100027207.1.3",
          "form": "학교는 안간지오래됐죠",
          "original_form": "학교는 안간지오래됐죠",
          "speaker_id": "1",
          "time": "20210902 10:31"
        },
        {
          "id": "MDRW2100027207.1.4",
          "form": "좋아하는 과목은요",
          "original_form": "좋아하는 과목은요",
          "speaker_id": "1",
          "time": "20210902 10:31"
        },
        {
          "id": "MDRW2100027207.1.5",
          "form": "어제 초딩들 개학했어요",
          "original_form": "어제 초딩들 개학했어요",
          "speaker_id": "2",
          "time": "20210902 10:31"
        },
        {
          "id": "MDRW2100027207.1.6",
          "form": "국민학교였지요",
          "original_form": "국민학교였지요",
          "speaker_id": "1",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100027207.1.7",
          "form": "전 잘수있는과목 좋앙ㅂ",
          "original_form": "전 잘수있는과목 좋앙ㅂ",
          "speaker_id": "2",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100027207.1.8",
          "form": "국민학교다넜지만",
          "original_form": "국민학교다넜지만",
          "speaker_id": "2",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100027207.1.9",
          "form": "물리요?",
          "original_form": "물리요?",
          "speaker_id": "1",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100027207.1.10",
          "form": "졸업은 초등이죠",
          "original_form": "졸업은 초등이죠",
          "speaker_id": "2",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100027207.1.11",
          "form": "잊지마세요",
          "original_form": "잊지마세요",
          "speaker_id": "2",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100027207.1.12",
          "form": "국민아니었나요",
          "original_form": "국민아니었나요",
          "speaker_id": "1",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100027207.1.13",
          "form": "마지막 국민",
          "original_form": "마지막 국민",
          "speaker_id": "1",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100027207.1.14",
          "form": "졸업은 초등으로 했어요",
          "original_form": "졸업은 초등으로 했어요",
          "speaker_id": "2",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100027207.1.15",
          "form": "수학이 싫습니다",
          "original_form": "수학이 싫습니다",
          "speaker_id": "1",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100027207.1.16",
          "form": "딱 바꼈어요",
          "original_form": "딱 바꼈어요",
          "speaker_id": "2",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100027207.1.17",
          "form": "6년내내국민",
          "original_form": "6년내내국민",
          "speaker_id": "2",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100027207.1.18",
          "form": "사회나오니 더 필요없어요",
          "original_form": "사회나오니 더 필요없어요",
          "speaker_id": "1",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100027207.1.19",
          "form": "졸업은초등학교",
          "original_form": "졸업은초등학교",
          "speaker_id": "2",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100027207.1.20",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100027207.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100027207.1.22",
          "form": "영어가도움되지요",
          "original_form": "영어가도움되지요",
          "speaker_id": "2",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100027207.1.23",
          "form": "지구과학은 왜 배웠는지",
          "original_form": "지구과학은 왜 배웠는지",
          "speaker_id": "1",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100027207.1.24",
          "form": "외국어가 중요해요",
          "original_form": "외국어가 중요해요",
          "speaker_id": "2",
          "time": "20210902 10:33"
        }
      ]
