        {
          "id": "MDRW2100033192.1.1",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210906 18:40"
        },
        {
          "id": "MDRW2100033192.1.2",
          "form": "11번가에서 제가 가장 찾고싶었던거 찾아서..",
          "original_form": "11번가에서 제가 가장 찾고싶었던거 찾아서..",
          "speaker_id": "2",
          "time": "20210906 18:40"
        },
        {
          "id": "MDRW2100033192.1.3",
          "form": "튀김우동 오뚜기꺼 엄청 찾았거든요 ㅠㅠ",
          "original_form": "튀김우동 오뚜기꺼 엄청 찾았거든요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 18:40"
        },
        {
          "id": "MDRW2100033192.1.4",
          "form": "그거 편의점에 컵라면으로 파는거 봤는데",
          "original_form": "그거 편의점에 컵라면으로 파는거 봤는데",
          "speaker_id": "1",
          "time": "20210906 18:41"
        },
        {
          "id": "MDRW2100033192.1.5",
          "form": "쉽게구할수있어요",
          "original_form": "쉽게구할수있어요",
          "speaker_id": "1",
          "time": "20210906 18:41"
        },
        {
          "id": "MDRW2100033192.1.6",
          "form": "저희 지역은 인기가없어서 그런지",
          "original_form": "저희 지역은 인기가없어서 그런지",
          "speaker_id": "2",
          "time": "20210906 18:42"
        },
        {
          "id": "MDRW2100033192.1.7",
          "form": "농심 튀김우동만 팔고",
          "original_form": "농심 튀김우동만 팔고",
          "speaker_id": "2",
          "time": "20210906 18:42"
        },
        {
          "id": "MDRW2100033192.1.8",
          "form": "오뚜기는 일체 안팔더라고요",
          "original_form": "오뚜기는 일체 안팔더라고요",
          "speaker_id": "2",
          "time": "20210906 18:42"
        },
        {
          "id": "MDRW2100033192.1.9",
          "form": "피시방에서만 간혹 봐서 그런지 더욱 먹고싶었는데",
          "original_form": "피시방에서만 간혹 봐서 그런지 더욱 먹고싶었는데",
          "speaker_id": "2",
          "time": "20210906 18:42"
        },
        {
          "id": "MDRW2100033192.1.10",
          "form": "11번가에는 바로 있어서 사서 먹고있어요",
          "original_form": "11번가에는 바로 있어서 사서 먹고있어요",
          "speaker_id": "2",
          "time": "20210906 18:42"
        },
        {
          "id": "MDRW2100033192.1.11",
          "form": "오!!역시 온라인주문은없는게없네요",
          "original_form": "오!!역시 온라인주문은없는게없네요",
          "speaker_id": "1",
          "time": "20210906 18:49"
        },
        {
          "id": "MDRW2100033192.1.12",
          "form": "맛있죵",
          "original_form": "맛있죵",
          "speaker_id": "1",
          "time": "20210906 18:50"
        },
        {
          "id": "MDRW2100033192.1.13",
          "form": "맞아요 ㅠㅠㅠㅠㅠ 너무 맛있어요",
          "original_form": "맞아요 ㅠㅠㅠㅠㅠ 너무 맛있어요",
          "speaker_id": "2",
          "time": "20210906 18:50"
        },
        {
          "id": "MDRW2100033192.1.14",
          "form": "피시방에서 처음먹어봤었는데",
          "original_form": "피시방에서 처음먹어봤었는데",
          "speaker_id": "2",
          "time": "20210906 18:51"
        },
        {
          "id": "MDRW2100033192.1.15",
          "form": "그 국물이 진짜 진국인지라..",
          "original_form": "그 국물이 진짜 진국인지라..",
          "speaker_id": "2",
          "time": "20210906 18:51"
        },
        {
          "id": "MDRW2100033192.1.16",
          "form": "굿굿!!!",
          "original_form": "굿굿!!!",
          "speaker_id": "1",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100033192.1.17",
          "form": "기억에남으니깐 또 찾는거죵",
          "original_form": "기억에남으니깐 또 찾는거죵",
          "speaker_id": "1",
          "time": "20210906 18:53"
        },
        {
          "id": "MDRW2100033192.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210906 19:18"
        },
        {
          "id": "MDRW2100033192.1.19",
          "form": "저는 지금 치킨뜯고있어요",
          "original_form": "저는 지금 치킨뜯고있어요",
          "speaker_id": "1",
          "time": "20210906 19:47"
        },
        {
          "id": "MDRW2100033192.1.20",
          "form": "오..맛있겠네요!",
          "original_form": "오..맛있겠네요!",
          "speaker_id": "2",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100033192.1.21",
          "form": "행복하게먹고있어요",
          "original_form": "행복하게먹고있어요",
          "speaker_id": "1",
          "time": "20210906 19:56"
        },
        {
          "id": "MDRW2100033192.1.22",
          "form": "소스도 아까 KFC소스에다가 먹는다고 하셨죠?!",
          "original_form": "소스도 아까 KFC소스에다가 먹는다고 하셨죠?!",
          "speaker_id": "2",
          "time": "20210906 19:58"
        },
        {
          "id": "MDRW2100033192.1.23",
          "form": "진짜 맛있겠네요 ㅠㅠ",
          "original_form": "진짜 맛있겠네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 19:58"
        },
        {
          "id": "MDRW2100033192.1.24",
          "form": "어제는 치킨을 먹고",
          "original_form": "어제는 치킨을 먹고",
          "speaker_id": "1",
          "time": "20210907 06:53"
        },
        {
          "id": "MDRW2100033192.1.25",
