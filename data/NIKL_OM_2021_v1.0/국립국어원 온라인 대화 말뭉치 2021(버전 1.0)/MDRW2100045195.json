      "utterance": [
        {
          "id": "MDRW2100045195.1.1",
          "form": "잘라파고스 얼른 망했으면",
          "original_form": "잘라파고스 얼른 망했으면",
          "speaker_id": "2",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045195.1.2",
          "form": "거기는 진짜 망조들어보이는데",
          "original_form": "거기는 진짜 망조들어보이는데",
          "speaker_id": "1",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045195.1.3",
          "form": "아 요새는 한국 잘베껴서",
          "original_form": "아 요새는 한국 잘베껴서",
          "speaker_id": "1",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045195.1.4",
          "form": "다시 새롭게 트레이닝하던데",
          "original_form": "다시 새롭게 트레이닝하던데",
          "speaker_id": "1",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045195.1.5",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045195.1.6",
          "form": "ㅋㅋㅋㅋ 한국도 표절에서 자유로운 나라는 아니지만",
          "original_form": "ㅋㅋㅋㅋ 한국도 표절에서 자유로운 나라는 아니지만",
          "speaker_id": "2",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045195.1.7",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045195.1.8",
          "form": "그래도 일본이 그동안 잘라파고스 돼있던게 커서",
          "original_form": "그래도 일본이 그동안 잘라파고스 돼있던게 커서",
          "speaker_id": "2",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045195.1.9",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045195.1.10",
          "form": "마자",
          "original_form": "마자",
          "speaker_id": "1",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045195.1.11",
          "form": "따라오려면",
          "original_form": "따라오려면",
          "speaker_id": "2",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045195.1.12",
          "form": "시간 더",
          "original_form": "시간 더",
          "speaker_id": "2",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045195.1.13",
          "form": "거기도 완전",
          "original_form": "거기도 완전",
          "speaker_id": "1",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045195.1.14",
          "form": "걸릴 느낌",
          "original_form": "걸릴 느낌",
          "speaker_id": "2",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045195.1.15",
          "form": "어린애율동을",
          "original_form": "어린애율동을",
          "speaker_id": "1",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045195.1.16",
          "form": "잘한다ㅏㄹ한다 잘한다",
          "original_form": "잘한다ㅏㄹ한다 잘한다",
          "speaker_id": "1",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045195.1.17",
          "form": "이랬으니",
          "original_form": "이랬으니",
          "speaker_id": "1",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045195.1.18",
          "form": "이게어디쉽게 바뀌겠어",
          "original_form": "이게어디쉽게 바뀌겠어",
          "speaker_id": "1",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045195.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:14"
        },
        {
          "id": "MDRW2100045195.1.20",
          "form": "우리나라 아이돌들 기본만 해도",
          "original_form": "우리나라 아이돌들 기본만 해도",
          "speaker_id": "2",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045195.1.21",
          "form": "거기선 아티스트 취급할 정도였으니ㅋㅋㅋㅋ",
          "original_form": "거기선 아티스트 취급할 정도였으니ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045195.1.22",
          "form": "중국은 가사가 멋진 중국 까리한 중국 중국최고",
          "original_form": "중국은 가사가 멋진 중국 까리한 중국 중국최고",
          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045195.1.23",
          "form": "역사속에 우리는 언제나 짱이었고 모두가 우리를 우러러봐 중국짱",
          "original_form": "역사속에 우리는 언제나 짱이었고 모두가 우리를 우러러봐 중국짱",
          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045195.1.24",
          "form": "이모양이니",
          "original_form": "이모양이니",
          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045195.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045195.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ양쪽나라 다 심각하네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ양쪽나라 다 심각하네",
          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045195.1.27",
          "form": "단군할아버지머했어여",
          "original_form": "단군할아버지머했어여",
          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045195.1.28",
          "form": "동북아시아의 비극",
          "original_form": "동북아시아의 비극",
          "speaker_id": "2",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045195.1.29",
          "form": "왜 이런곳에 부동산 투기를 하신거에요",
          "original_form": "왜 이런곳에 부동산 투기를 하신거에요",
          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045195.1.30",
          "form": "어메이징 동북아시아",
          "original_form": "어메이징 동북아시아",
          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045195.1.31",
          "form": "노래도 다 이상하고",
          "original_form": "노래도 다 이상하고",
          "speaker_id": "1",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045195.1.32",
          "form": "솔직히 일본 ㅠ지컬도 거지같아",
          "original_form": "솔직히 일본 ㅠ지컬도 거지같아",
          "speaker_id": "1",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045195.1.33",
          "form": "다들 엔카부르고이어",
          "original_form": "다들 엔카부르고이어",
          "speaker_id": "1",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045195.1.34",
          "form": "코창력",
          "original_form": "코창력",
          "speaker_id": "1",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045195.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ극단 시키인가 거긴 좀 유명한거같던데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ극단 시키인가 거긴 좀 유명한거같던데",
          "speaker_id": "2",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045195.1.36",
          "form": "아니면 다카라즈카",
          "original_form": "아니면 다카라즈카",
          "speaker_id": "2",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045195.1.37",
          "form": "사계가 유명한건",
          "original_form": "사계가 유명한건",
          "speaker_id": "1",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045195.1.38",
          "form": "제가봤을대는",
          "original_form": "제가봤을대는",
          "speaker_id": "1",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045195.1.39",
          "form": "브로드웨이 최신극을 잘 사와서그래",
          "original_form": "브로드웨이 최신극을 잘 사와서그래",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045195.1.40",
          "form": "지금은 한물간듯",
          "original_form": "지금은 한물간듯",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045195.1.41",
          "form": "아그러고보니까",
          "original_form": "아그러고보니까",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045195.1.42",
          "form": "성냥팔이소녀의 재림이라는 망작이있는데",
          "original_form": "성냥팔이소녀의 재림이라는 망작이있는데",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045195.1.43",
          "form": "그 영화가 진짜 돈이 어어어엉어엄청 투ㅏ를 받아서 그당시 전례없는 100억 작품이었나",
          "original_form": "그 영화가 진짜 돈이 어어어엉어엄청 투ㅏ를 받아서 그당시 전례없는 100억 작품이었나",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045195.1.44",
          "form": "그랬었음",
          "original_form": "그랬었음",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045195.1.45",
          "form": "근데 그게진짜 폭망한거",
          "original_form": "근데 그게진짜 폭망한거",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045195.1.46",
          "form": "그래서 당시 충무로 샷다내릴뻔했는데",
          "original_form": "그래서 당시 충무로 샷다내릴뻔했는데",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045195.1.47",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ투자자들이 그걸 다 뮤에 했다고...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ투자자들이 그걸 다 뮤에 했다고...",
