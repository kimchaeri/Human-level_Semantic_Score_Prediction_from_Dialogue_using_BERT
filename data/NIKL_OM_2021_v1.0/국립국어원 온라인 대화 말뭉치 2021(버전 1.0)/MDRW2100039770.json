      "utterance": [
        {
          "id": "MDRW2100039770.1.1",
          "form": "요즘날씨 죽인다",
          "original_form": "요즘날씨 죽인다",
          "speaker_id": "2",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100039770.1.2",
          "form": "왜? 시원해서?",
          "original_form": "왜? 시원해서?",
          "speaker_id": "1",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100039770.1.3",
          "form": "하늘이 넘 이뻐 깨끗하고 구름도 예술이고",
          "original_form": "하늘이 넘 이뻐 깨끗하고 구름도 예술이고",
          "speaker_id": "2",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100039770.1.4",
          "form": "나 오늘 하늘 한번도 안봤지롱",
          "original_form": "나 오늘 하늘 한번도 안봤지롱",
          "speaker_id": "1",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039770.1.5",
          "form": "자연이 주는 기쁨을 느껴야해\r\n계절마다 주는 느낌이다르잖아",
          "original_form": "자연이 주는 기쁨을 느껴야해\r\n계절마다 주는 느낌이다르잖아",
          "speaker_id": "2",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039770.1.6",
          "form": "계절마다 다 짜증나!",
          "original_form": "계절마다 다 짜증나!",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039770.1.7",
          "form": "가을은 춥지도 덥지도 않아서 좋아",
          "original_form": "가을은 춥지도 덥지도 않아서 좋아",
          "speaker_id": "2",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039770.1.8",
          "form": "무슨 소리야 가을 없어졌는데",
          "original_form": "무슨 소리야 가을 없어졌는데",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039770.1.9",
          "form": "지금이 가을이야\r\n밤엔 선선 낮엔 쨍쨍",
          "original_form": "지금이 가을이야\r\n밤엔 선선 낮엔 쨍쨍",
          "speaker_id": "2",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100039770.1.10",
          "form": "요즘 사람들 반팔 입어 긴팔 입어",
          "original_form": "요즘 사람들 반팔 입어 긴팔 입어",
          "speaker_id": "1",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100039770.1.11",
          "form": "아직은 반팔이 많아",
          "original_form": "아직은 반팔이 많아",
          "speaker_id": "2",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100039770.1.12",
          "form": "근데 요즘 미세먼지가 좀 적다",
          "original_form": "근데 요즘 미세먼지가 좀 적다",
          "speaker_id": "1",
          "time": "20210903 21:22"
        },
        {
          "id": "MDRW2100039770.1.13",
          "form": "맞아 코로나무서워 도망갔나봐",
          "original_form": "맞아 코로나무서워 도망갔나봐",
          "speaker_id": "2",
          "time": "20210903 21:22"
        },
        {
          "id": "MDRW2100039770.1.14",
          "form": "내일 비온대?",
          "original_form": "내일 비온대?",
          "speaker_id": "1",
          "time": "20210903 21:22"
        },
        {
          "id": "MDRW2100039770.1.15",
          "form": "일기예보를 보거라",
          "original_form": "일기예보를 보거라",
          "speaker_id": "2",
          "time": "20210903 21:22"
        },
        {
          "id": "MDRW2100039770.1.16",
          "form": "보기 싫으니까 물어봤지",
          "original_form": "보기 싫으니까 물어봤지",
          "speaker_id": "1",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100039770.1.17",
          "form": "엄마도 그러네",
          "original_form": "엄마도 그러네",
          "speaker_id": "2",
          "time": "20210903 21:23"
        }
      ]
