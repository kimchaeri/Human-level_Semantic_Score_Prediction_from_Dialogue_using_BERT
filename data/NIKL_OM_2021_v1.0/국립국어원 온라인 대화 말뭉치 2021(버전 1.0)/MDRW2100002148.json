      "utterance": [
        {
          "id": "MDRW2100002148.1.1",
          "form": "반려동물키우시나요",
          "original_form": "반려동물키우시나요",
          "speaker_id": "1",
          "time": "20210820 00:02"
        },
        {
          "id": "MDRW2100002148.1.2",
          "form": "네 강아지 한마리 키워요",
          "original_form": "네 강아지 한마리 키워요",
          "speaker_id": "2",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100002148.1.3",
          "form": "우와 강아지 엄청귀엽겠어요",
          "original_form": "우와 강아지 엄청귀엽겠어요",
          "speaker_id": "1",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100002148.1.4",
          "form": "제가 키우는 건 아니고 동생이 키우는거지만 가끔 보니깐요",
          "original_form": "제가 키우는 건 아니고 동생이 키우는거지만 가끔 보니깐요",
          "speaker_id": "2",
          "time": "20210820 00:05"
        },
        {
          "id": "MDRW2100002148.1.5",
          "form": "name1님은 반려동물 키우나요?",
          "original_form": "&name1&님은 반려동물 키우나요?",
          "speaker_id": "2",
          "time": "20210820 00:05"
        },
        {
          "id": "MDRW2100002148.1.6",
          "form": "아하그렇군여",
          "original_form": "아하그렇군여",
          "speaker_id": "1",
          "time": "20210820 00:05"
        },
        {
          "id": "MDRW2100002148.1.7",
          "form": "네 전 고양이키워요",
          "original_form": "네 전 고양이키워요",
          "speaker_id": "1",
          "time": "20210820 00:05"
        },
        {
          "id": "MDRW2100002148.1.8",
          "form": "사진첩들어가면 전부 냥이사진들로만 ㅎㅎ",
          "original_form": "사진첩들어가면 전부 냥이사진들로만 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 00:05"
        },
        {
          "id": "MDRW2100002148.1.9",
          "form": "몇마리 키워요?",
          "original_form": "몇마리 키워요?",
          "speaker_id": "2",
          "time": "20210820 00:06"
        },
        {
          "id": "MDRW2100002148.1.10",
          "form": "원래 반려동물 키우면 사진첩 가득 사진 많아요",
          "original_form": "원래 반려동물 키우면 사진첩 가득 사진 많아요",
          "speaker_id": "2",
          "time": "20210820 00:06"
        },
        {
          "id": "MDRW2100002148.1.11",
          "form": "1마리에요 ㅎㅎ",
          "original_form": "1마리에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 00:07"
        },
        {
          "id": "MDRW2100002148.1.12",
          "form": "네 매번 새롭게귀엽고 이뻐요 ㅋㅋㅋㅋㅋ",
          "original_form": "네 매번 새롭게귀엽고 이뻐요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:07"
        },
        {
          "id": "MDRW2100002148.1.13",
          "form": "몇살이에요? 어떻게 생겼어요?",
          "original_form": "몇살이에요? 어떻게 생겼어요?",
          "speaker_id": "2",
          "time": "20210820 00:09"
        },
        {
          "id": "MDRW2100002148.1.14",
          "form": "우리집 개는 갈색푸들이에요",
          "original_form": "우리집 개는 갈색푸들이에요",
          "speaker_id": "2",
          "time": "20210820 00:09"
        },
        {
          "id": "MDRW2100002148.1.15",
          "form": "코숏에 치즈태비에요 노랑색털이 여기저기묻어있어여",
          "original_form": "코숏에 치즈태비에요 노랑색털이 여기저기묻어있어여",
          "speaker_id": "1",
          "time": "20210820 00:10"
        },
        {
          "id": "MDRW2100002148.1.16",
          "form": "푸들 넘귀엽져ㅜ빵실빵실",
          "original_form": "푸들 넘귀엽져ㅜ빵실빵실",
          "speaker_id": "1",
          "time": "20210820 00:10"
        },
        {
          "id": "MDRW2100002148.1.17",
          "form": "꺄!!! 치즈냥이구나 요즘 고양이 짤 봐서 정감가더라구요",
          "original_form": "꺄!!! 치즈냥이구나 요즘 고양이 짤 봐서 정감가더라구요",
          "speaker_id": "2",
          "time": "20210820 00:12"
        },
        {
          "id": "MDRW2100002148.1.18",
          "form": "지금 미용 안해서 정말 털보아저씨같아요",
          "original_form": "지금 미용 안해서 정말 털보아저씨같아요",
          "speaker_id": "2",
          "time": "20210820 00:12"
        },
        {
          "id": "MDRW2100002148.1.19",
          "form": "ㅋㅋㅋㅋㅋ털보아저씨",
          "original_form": "ㅋㅋㅋㅋㅋ털보아저씨",
          "speaker_id": "1",
          "time": "20210820 00:15"
        },
        {
          "id": "MDRW2100002148.1.20",
          "form": "강쥐들은 주기적으로 미용한번씩해주져?",
          "original_form": "강쥐들은 주기적으로 미용한번씩해주져?",
          "speaker_id": "1",
          "time": "20210820 00:15"
        },
        {
          "id": "MDRW2100002148.1.21",
          "form": "두달이나 석달에 한번 한대요",
          "original_form": "두달이나 석달에 한번 한대요",
          "speaker_id": "2",
          "time": "20210820 00:16"
        },
        {
          "id": "MDRW2100002148.1.22",
          "form": "그 돈도 무시못하겠어요ㅜㅜ",
          "original_form": "그 돈도 무시못하겠어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 00:17"
        },
        {
          "id": "MDRW2100002148.1.23",
          "form": "미용하면 뉘집 강아지인지 모르겠더라구요 너무 멀끔해서요",
          "original_form": "미용하면 뉘집 강아지인지 모르겠더라구요 너무 멀끔해서요",
          "speaker_id": "2",
          "time": "20210820 00:17"
        },
        {
          "id": "MDRW2100002148.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ깜직이로 변신",
          "original_form": "ㅋㅋㅋㅋㅋㅋ깜직이로 변신",
          "speaker_id": "1",
          "time": "20210820 00:17"
        },
        {
          "id": "MDRW2100002148.1.25",
          "form": "고양이들도 털관리하죠?",
          "original_form": "고양이들도 털관리하죠?",
          "speaker_id": "2",
          "time": "20210820 00:17"
        },
        {
          "id": "MDRW2100002148.1.26",
          "form": "네 장묘종인애들은 관리해주는데요",
          "original_form": "네 장묘종인애들은 관리해주는데요",
          "speaker_id": "1",
          "time": "20210820 00:17"
        },
        {
          "id": "MDRW2100002148.1.27",
          "form": "저희애는 장묘종이아니라 지가관리알아서해요 ㅋㅋㅋ",
          "original_form": "저희애는 장묘종이아니라 지가관리알아서해요 ㅋㅋㅋ",
          "speaker_id": "1",
