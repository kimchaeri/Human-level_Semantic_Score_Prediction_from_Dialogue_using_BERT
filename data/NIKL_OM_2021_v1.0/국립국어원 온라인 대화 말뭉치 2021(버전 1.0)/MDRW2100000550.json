      "utterance": [
        {
          "id": "MDRW2100000550.1.1",
          "form": "멍멍이키워?",
          "original_form": "멍멍이키워?",
          "speaker_id": "1",
          "time": "20210819 16:48"
        },
        {
          "id": "MDRW2100000550.1.2",
          "form": "아니 너는?",
          "original_form": "아니 너는?",
          "speaker_id": "2",
          "time": "20210819 16:48"
        },
        {
          "id": "MDRW2100000550.1.3",
          "form": "나는 멍멍이 키워",
          "original_form": "나는 멍멍이 키워",
          "speaker_id": "1",
          "time": "20210819 16:48"
        },
        {
          "id": "MDRW2100000550.1.4",
          "form": "포메?",
          "original_form": "포메?",
          "speaker_id": "2",
          "time": "20210819 16:49"
        },
        {
          "id": "MDRW2100000550.1.5",
          "form": "아니아니 믹스랑 말티즈",
          "original_form": "아니아니 믹스랑 말티즈",
          "speaker_id": "1",
          "time": "20210819 16:49"
        },
        {
          "id": "MDRW2100000550.1.6",
          "form": "넌 반려동물 안키우는거야?",
          "original_form": "넌 반려동물 안키우는거야?",
          "speaker_id": "1",
          "time": "20210819 16:49"
        },
        {
          "id": "MDRW2100000550.1.7",
          "form": "응 지금은 안키우고 예전에는 포메 키웠었어",
          "original_form": "응 지금은 안키우고 예전에는 포메 키웠었어",
          "speaker_id": "2",
          "time": "20210819 16:52"
        },
        {
          "id": "MDRW2100000550.1.8",
          "form": "반려견 이름이 뭐야?",
          "original_form": "반려견 이름이 뭐야?",
          "speaker_id": "2",
          "time": "20210819 17:06"
        },
        {
          "id": "MDRW2100000550.1.9",
          "form": "뭉치!",
          "original_form": "뭉치!",
          "speaker_id": "1",
          "time": "20210819 17:18"
        },
        {
          "id": "MDRW2100000550.1.10",
          "form": "진짜?",
          "original_form": "진짜?",
          "speaker_id": "2",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000550.1.11",
          "form": "응응ㅋㅋㅋ",
          "original_form": "응응ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000550.1.12",
          "form": "애기때사고뭉치였어서",
          "original_form": "애기때사고뭉치였어서",
          "speaker_id": "1",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000550.1.13",
          "form": "대박이네~",
          "original_form": "대박이네~",
          "speaker_id": "2",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000550.1.14",
          "form": "뭉치라고지었어",
          "original_form": "뭉치라고지었어",
          "speaker_id": "1",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000550.1.15",
          "form": "다른 애는?",
          "original_form": "다른 애는?",
          "speaker_id": "2",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000550.1.16",
          "form": "납득이",
          "original_form": "납득이",
          "speaker_id": "1",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000550.1.17",
          "form": "우리집개들이름 특이하짘ㅋ",
          "original_form": "우리집개들이름 특이하짘ㅋ",
          "speaker_id": "1",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000550.1.18",
          "form": "응 ㅋㅋ 왜 납득이야?",
          "original_form": "응 ㅋㅋ 왜 납득이야?",
          "speaker_id": "2",
          "time": "20210819 17:24"
        },
        {
          "id": "MDRW2100000550.1.19",
          "form": "건축학개론 할때 데리고와서ㅋㅋㅋ",
          "original_form": "건축학개론 할때 데리고와서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:48"
        },
        {
          "id": "MDRW2100000550.1.20",
          "form": "그럼 나이가 꽤 있겠네",
          "original_form": "그럼 나이가 꽤 있겠네",
          "speaker_id": "2",
          "time": "20210819 17:49"
        },
        {
          "id": "MDRW2100000550.1.21",
          "form": "열살 됬나?",
          "original_form": "열살 됬나?",
          "speaker_id": "1",
          "time": "20210819 17:52"
        },
        {
          "id": "MDRW2100000550.1.22",
          "form": "할아버지야ㅋㅋ",
          "original_form": "할아버지야ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:52"
        },
        {
          "id": "MDRW2100000550.1.23",
          "form": "건강해?",
          "original_form": "건강해?",
          "speaker_id": "2",
          "time": "20210819 17:52"
        },
        {
          "id": "MDRW2100000550.1.24",
          "form": "응응 너무 건강해서 탈이야",
          "original_form": "응응 너무 건강해서 탈이야",
          "speaker_id": "1",
          "time": "20210819 17:53"
        },
        {
          "id": "MDRW2100000550.1.25",
          "form": "밥도 너무 잘먹어ㅋㅋ",
          "original_form": "밥도 너무 잘먹어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:53"
        },
        {
          "id": "MDRW2100000550.1.26",
          "form": "다행이네~",
          "original_form": "다행이네~",
          "speaker_id": "2",
          "time": "20210819 17:54"
        },
        {
          "id": "MDRW2100000550.1.27",
          "form": "응응 마쟈 예전에 포메 키웠을때 이름뭐여써",
          "original_form": "응응 마쟈 예전에 포메 키웠을때 이름뭐여써",
          "speaker_id": "1",
          "time": "20210819 17:58"
        },
        {
          "id": "MDRW2100000550.1.28",
          "form": "꼬망이",
          "original_form": "꼬망이",
          "speaker_id": "2",
          "time": "20210819 18:01"
        },
        {
          "id": "MDRW2100000550.1.29",
          "form": "너무 귀여운대??",
          "original_form": "너무 귀여운대??",
          "speaker_id": "1",
          "time": "20210819 18:02"
        },
        {
          "id": "MDRW2100000550.1.30",
          "form": "왜 꼬망이야ㅋㅋ",
          "original_form": "왜 꼬망이야ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 18:02"
        },
        {
          "id": "MDRW2100000550.1.31",
          "form": "꼬마라서 ㅎ",
          "original_form": "꼬마라서 ㅎ",
          "speaker_id": "2",
          "time": "20210819 18:03"
        },
        {
          "id": "MDRW2100000550.1.32",
          "form": "헐 넘귀엽닼ㅋ",
          "original_form": "헐 넘귀엽닼ㅋ",
          "speaker_id": "1",
          "time": "20210819 18:04"
        }
      ]
    }
