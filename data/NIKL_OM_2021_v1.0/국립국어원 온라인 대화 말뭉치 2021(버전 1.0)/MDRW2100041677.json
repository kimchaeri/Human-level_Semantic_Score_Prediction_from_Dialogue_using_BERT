      "utterance": [
        {
          "id": "MDRW2100041677.1.1",
          "form": "매운거 좋아햐요?",
          "original_form": "매운거 좋아햐요?",
          "speaker_id": "1",
          "time": "20210906 14:23"
        },
        {
          "id": "MDRW2100041677.1.2",
          "form": "오 네! 매운거 먹는거 좋아해요!",
          "original_form": "오 네! 매운거 먹는거 좋아해요!",
          "speaker_id": "2",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100041677.1.3",
          "form": "최애라면 불닭입니다ㅎㅎ",
          "original_form": "최애라면 불닭입니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100041677.1.4",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "1",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100041677.1.5",
          "form": "불닭진짜 맛있죠",
          "original_form": "불닭진짜 맛있죠",
          "speaker_id": "1",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100041677.1.6",
          "form": "오리지널파ㅡ",
          "original_form": "오리지널파ㅡ",
          "speaker_id": "1",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100041677.1.7",
          "form": "신가요?",
          "original_form": "신가요?",
          "speaker_id": "1",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100041677.1.8",
          "form": "네!! 오리지널밖에 안사먹어요ㅋㅋㅋ",
          "original_form": "네!! 오리지널밖에 안사먹어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100041677.1.9",
          "form": "근데 체다치즈 하나 넣어먹어요 그럼 꾸덕해서 더 취향저격이더라구요!",
          "original_form": "근데 체다치즈 하나 넣어먹어요 그럼 꾸덕해서 더 취향저격이더라구요!",
          "speaker_id": "2",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100041677.1.10",
          "form": "헐 저도요저도뇨",
          "original_form": "헐 저도요저도뇨",
          "speaker_id": "1",
          "time": "20210906 16:22"
        },
        {
          "id": "MDRW2100041677.1.11",
          "form": "체다치즈 넣으면 진짜 너무 맛나요",
          "original_form": "체다치즈 넣으면 진짜 너무 맛나요",
          "speaker_id": "1",
          "time": "20210906 16:22"
        },
        {
          "id": "MDRW2100041677.1.12",
          "form": "밥 비벼먹어도 두배맛나고",
          "original_form": "밥 비벼먹어도 두배맛나고",
          "speaker_id": "1",
          "time": "20210906 16:22"
        },
        {
          "id": "MDRW2100041677.1.13",
          "form": "와우 맞아요!!!! 진짜 훨배 맛있어요ㅠㅠㅠ",
          "original_form": "와우 맞아요!!!! 진짜 훨배 맛있어요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 16:34"
        },
        {
          "id": "MDRW2100041677.1.14",
          "form": "불닭만큼은 모짜렐라보다 체다가 최고ㅠㅠㅠ",
          "original_form": "불닭만큼은 모짜렐라보다 체다가 최고ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 16:35"
        },
        {
          "id": "MDRW2100041677.1.15",
          "form": "그럼 불닭 봉지로 드세요 컵으로 드세요??",
          "original_form": "그럼 불닭 봉지로 드세요 컵으로 드세요??",
          "speaker_id": "2",
          "time": "20210906 16:35"
        },
        {
          "id": "MDRW2100041677.1.16",
          "form": "컵을 더 자주 먹긴하는데",
          "original_form": "컵을 더 자주 먹긴하는데",
          "speaker_id": "1",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100041677.1.17",
          "form": "봉지가 더 맛난거같아요 ㅠ",
          "original_form": "봉지가 더 맛난거같아요 ㅠ",
          "speaker_id": "1",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100041677.1.18",
          "form": "모짜렐라는.. 가끔 쭈욱 늘어나느거 보고싶을때만 넣는거죠 ㅎ",
          "original_form": "모짜렐라는.. 가끔 쭈욱 늘어나느거 보고싶을때만 넣는거죠 ㅎ",
          "speaker_id": "1",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100041677.1.19",
          "form": "아항 저는 맨날 봉지로 먹어요! 마지막에 치즈 넣고 졸이듯이 먹으면 뭔가 소스도 더 잘 배이는 것 같고ㅎㅎㅋㅋㅋ",
          "original_form": "아항 저는 맨날 봉지로 먹어요! 마지막에 치즈 넣고 졸이듯이 먹으면 뭔가 소스도 더 잘 배이는 것 같고ㅎㅎㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100041677.1.20",
          "form": "맞아요ㅋㅋㅋ 그리고 모짜렐라 맛이 그렇게 잘 느껴지는 것 같지도 않더라구요,,",
          "original_form": "맞아요ㅋㅋㅋ 그리고 모짜렐라 맛이 그렇게 잘 느껴지는 것 같지도 않더라구요,,",
          "speaker_id": "2",
          "time": "20210906 16:52"
        },
        {
          "id": "MDRW2100041677.1.21",
          "form": "맞아요 ㅋㅋㅋㅋ 맛잇어보이는대 은근 맛이 안느겨디는",
          "original_form": "맞아요 ㅋㅋㅋㅋ 맛잇어보이는대 은근 맛이 안느겨디는",
          "speaker_id": "1",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100041677.1.22",
          "form": "저는 끓여먹기 귀찮아서 컵라면 먹는거라 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 끓여먹기 귀찮아서 컵라면 먹는거라 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100041677.1.23",
          "form": "사실 면도 봉지가 더 맛난거같아요 도톰해가지구",
          "original_form": "사실 면도 봉지가 더 맛난거같아요 도톰해가지구",
          "speaker_id": "1",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100041677.1.24",
          "form": "앜ㅋㅋㅋㅋㅋ 간편한건 컵라면 못이기긴해요ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ 간편한건 컵라면 못이기긴해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:04"
        },
        {
          "id": "MDRW2100041677.1.25",
          "form": "저두요! 면이 봉지가ㅜ더 취향이긴해요 ㅎㅎ",
          "original_form": "저두요! 면이 봉지가ㅜ더 취향이긴해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 23:04"
        },
        {
          "id": "MDRW2100041677.1.26",
          "form": "근데 봉지가 더 싸고 양도 많아서 좋아요ㅋㅋㅋ",
          "original_form": "근데 봉지가 더 싸고 양도 많아서 좋아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 23:05"
        },
        {
          "id": "MDRW2100041677.1.27",
          "form": "소스도 더 넉넉한것같구 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "소스도 더 넉넉한것같구 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:00"
        },
        {
          "id": "MDRW2100041677.1.28",
          "form": "아 얘기하다보니까 넘 땡겨요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 얘기하다보니까 넘 땡겨요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:00"
        },
        {
          "id": "MDRW2100041677.1.29",
          "form": "ㅋㅋㅋㅋㅋ 불닭은 한번먹어도 다음날 또 생각나요,,",
          "original_form": "ㅋㅋㅋㅋㅋ 불닭은 한번먹어도 다음날 또 생각나요,,",
          "speaker_id": "2",
          "time": "20210907 12:23"
        },
        {
          "id": "MDRW2100041677.1.30",
          "form": "다른 불닭은 안드셔보셨어요??",
          "original_form": "다른 불닭은 안드셔보셨어요??",
          "speaker_id": "2",
          "time": "20210907 12:23"
        },
        {
          "id": "MDRW2100041677.1.31",
          "form": "까르보불닭도 좋아하고 치즈불닭도 좋아햐요!!",
          "original_form": "까르보불닭도 좋아하고 치즈불닭도 좋아햐요!!",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100041677.1.32",
          "form": "name2님으뇨?",
          "original_form": "&name2&님으뇨?",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100041677.1.33",
          "form": "오옷 저도 그 두개가 그다음으로 좋아요!!",
          "original_form": "오옷 저도 그 두개가 그다음으로 좋아요!!",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100041677.1.34",
