      "utterance": [
        {
          "id": "MDRW2100008322.1.1",
          "form": "어젠 치과가느라 서울갔더니 넘 피곤했어요",
          "original_form": "어젠 치과가느라 서울갔더니 넘 피곤했어요",
          "speaker_id": "2",
          "time": "20210822 15:05"
        },
        {
          "id": "MDRW2100008322.1.2",
          "form": "그런데 잠도 많이 못주무시고 늘 피곤하시겠어요..",
          "original_form": "그런데 잠도 많이 못주무시고 늘 피곤하시겠어요..",
          "speaker_id": "1",
          "time": "20210822 15:06"
        },
        {
          "id": "MDRW2100008322.1.3",
          "form": "네 늘 어깨가 무겁네요",
          "original_form": "네 늘 어깨가 무겁네요",
          "speaker_id": "2",
          "time": "20210822 15:06"
        },
        {
          "id": "MDRW2100008322.1.4",
          "form": "힘이 없어요",
          "original_form": "힘이 없어요",
          "speaker_id": "2",
          "time": "20210822 15:06"
        },
        {
          "id": "MDRW2100008322.1.5",
          "form": "낮잠 좀 자고 싶어요",
          "original_form": "낮잠 좀 자고 싶어요",
          "speaker_id": "2",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100008322.1.6",
          "form": "부럽습니다 ㅎㅎ",
          "original_form": "부럽습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100008322.1.7",
          "form": "어\r\n\r\n요새 저 사춘기 에도 안난",
          "original_form": "어\r\n\r\n요새 저 사춘기 에도 안난",
          "speaker_id": "2",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100008322.1.8",
          "form": "뾰루지가 났어요 ㅋㅋ",
          "original_form": "뾰루지가 났어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100008322.1.9",
          "form": "왕만해요 ㅋㅋ",
          "original_form": "왕만해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100008322.1.10",
          "form": "일주일에 너무 안자다보니 주말에 한번씩 누워있다보면 잠들때가 있더라구요",
          "original_form": "일주일에 너무 안자다보니 주말에 한번씩 누워있다보면 잠들때가 있더라구요",
          "speaker_id": "1",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100008322.1.11",
          "form": "헉.. 그것도 수면 부족으로 생기신거 아닐까요?ㅠㅠ",
          "original_form": "헉.. 그것도 수면 부족으로 생기신거 아닐까요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008322.1.12",
          "form": "아 그럼 꿀잠이죠",
          "original_form": "아 그럼 꿀잠이죠",
          "speaker_id": "2",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008322.1.13",
          "form": "그럴지도 몰라요",
          "original_form": "그럴지도 몰라요",
          "speaker_id": "2",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008322.1.14",
          "form": "마스크 때문인가 했는데 아닌거 같아요",
          "original_form": "마스크 때문인가 했는데 아닌거 같아요",
          "speaker_id": "2",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008322.1.15",
          "form": "화장품도 잘 안쓰는데 ㅋㅋ",
          "original_form": "화장품도 잘 안쓰는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008322.1.16",
          "form": "피부 관리 하세요?",
          "original_form": "피부 관리 하세요?",
          "speaker_id": "2",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008322.1.17",
          "form": "저 요새 미간에 보톡스 맞고 싶어요",
          "original_form": "저 요새 미간에 보톡스 맞고 싶어요",
          "speaker_id": "2",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008322.1.18",
          "form": "아... 저 저저번달에 보톡스 맞구 왔어요 ㅎㅎㅎㅎ",
          "original_form": "아... 저 저저번달에 보톡스 맞구 왔어요 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008322.1.19",
          "form": "오 정말요",
          "original_form": "오 정말요",
          "speaker_id": "2",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008322.1.20",
          "form": "몇달가요?",
          "original_form": "몇달가요?",
          "speaker_id": "2",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008322.1.21",
          "form": "어디 맞았어요?",
          "original_form": "어디 맞았어요?",
          "speaker_id": "2",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008322.1.22",
          "form": "많이 아픈가요?",
          "original_form": "많이 아픈가요?",
          "speaker_id": "2",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008322.1.23",
          "form": "3달정도 된거 같은데 윗쪽에는 이미 조금 풀렸어요 ㅠㅠ",
          "original_form": "3달정도 된거 같은데 윗쪽에는 이미 조금 풀렸어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008322.1.24",
          "form": "또 다시 맞으러 가야할 것 같애요..",
          "original_form": "또 다시 맞으러 가야할 것 같애요..",
          "speaker_id": "1",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008322.1.25",
          "form": "아 빨리 풀리네요",
          "original_form": "아 빨리 풀리네요",
          "speaker_id": "2",
          "time": "20210822 15:14"
        },
        {
          "id": "MDRW2100008322.1.26",
          "form": "저 미간에 주름이 팍",
          "original_form": "저 미간에 주름이 팍",
          "speaker_id": "2",
          "time": "20210822 15:14"
        },
        {
          "id": "MDRW2100008322.1.27",
          "form": "생겼어요",
          "original_form": "생겼어요",
          "speaker_id": "2",
          "time": "20210822 15:14"
        },
        {
          "id": "MDRW2100008322.1.28",
          "form": "전 이마 , 눈가 미간 이렇게 많아서...",
          "original_form": "전 이마 , 눈가 미간 이렇게 많아서...",
          "speaker_id": "1",
          "time": "20210822 15:15"
        },
        {
          "id": "MDRW2100008322.1.29",
          "form": "아 그러시군요",
          "original_form": "아 그러시군요",
          "speaker_id": "2",
          "time": "20210822 15:15"
        },
        {
          "id": "MDRW2100008322.1.30",
          "form": "거의 얼굴로 말해요ㄱ 거든요 ㅠㅠㅠ",
          "original_form": "거의 얼굴로 말해요ㄱ 거든요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:15"
        },
        {
          "id": "MDRW2100008322.1.31",
          "form": "다 하신거예요?",
          "original_form": "다 하신거예요?",
          "speaker_id": "2",
          "time": "20210822 15:15"
        },
        {
          "id": "MDRW2100008322.1.32",
          "form": "저도 사실 많지만 ㅋㅋ",
          "original_form": "저도 사실 많지만 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:15"
        },
        {
          "id": "MDRW2100008322.1.33",
          "form": "미간만 있다 할께요 ㅋㅋ",
          "original_form": "미간만 있다 할께요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:15"
        },
        {
          "id": "MDRW2100008322.1.34",
          "form": "그러다보니 최소 이마 미간은 꼭 맞아야해요..",
          "original_form": "그러다보니 최소 이마 미간은 꼭 맞아야해요..",
          "speaker_id": "1",
          "time": "20210822 15:15"
        },
        {
          "id": "MDRW2100008322.1.35",
          "form": "인상이 미간때문에 이상할까봐ㅠ하려고요",
          "original_form": "인상이 미간때문에 이상할까봐ㅠ하려고요",
          "speaker_id": "2",
          "time": "20210822 15:16"
        },
        {
          "id": "MDRW2100008322.1.36",
          "form": "안그럼 깊은 주름생긴다니 안맞을 수가 없더라구요 ㅠㅠ",
          "original_form": "안그럼 깊은 주름생긴다니 안맞을 수가 없더라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:16"
        },
        {
          "id": "MDRW2100008322.1.37",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 15:16"
        },
        {
          "id": "MDRW2100008322.1.38",
          "form": "깊어지려해요 ㅜㅜ",
          "original_form": "깊어지려해요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 15:16"
        },
        {
          "id": "MDRW2100008322.1.39",
          "form": "아 슬프네요",
          "original_form": "아 슬프네요",
          "speaker_id": "2",
          "time": "20210822 15:16"
        },
        {
          "id": "MDRW2100008322.1.40",
          "form": "나이가 직격탄으로 왔어요",
          "original_form": "나이가 직격탄으로 왔어요",
          "speaker_id": "2",
          "time": "20210822 15:16"
        },
        {
          "id": "MDRW2100008322.1.41",
          "form": "깊어지기 전에 맞아야한대요",
          "original_form": "깊어지기 전에 맞아야한대요",
          "speaker_id": "1",
          "time": "20210822 15:17"
        }
      ]
