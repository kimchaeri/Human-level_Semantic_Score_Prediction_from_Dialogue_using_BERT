      "utterance": [
        {
          "id": "MDRW2100027857.1.1",
          "form": "동호회나 모임 하시나요",
          "original_form": "동호회나 모임 하시나요",
          "speaker_id": "1",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100027857.1.2",
          "form": "게임 동호회 하고 있어요",
          "original_form": "게임 동호회 하고 있어요",
          "speaker_id": "2",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100027857.1.3",
          "form": "만나지는 못하고 채팅으로만요",
          "original_form": "만나지는 못하고 채팅으로만요",
          "speaker_id": "2",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100027857.1.4",
          "form": "무슨 겜 동회회요?",
          "original_form": "무슨 겜 동회회요?",
          "speaker_id": "1",
          "time": "20210902 15:46"
        },
        {
          "id": "MDRW2100027857.1.5",
          "form": "한국에서는 섭종한 모바일 게임이요...ㅎㅎ",
          "original_form": "한국에서는 섭종한 모바일 게임이요...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100027857.1.6",
          "form": "헉 뭐에요 그게?",
          "original_form": "헉 뭐에요 그게?",
          "speaker_id": "1",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027857.1.7",
          "form": "하얀고양이 프로젝트라고",
          "original_form": "하얀고양이 프로젝트라고",
          "speaker_id": "2",
          "time": "20210902 15:51"
        },
        {
          "id": "MDRW2100027857.1.8",
          "form": "일본게임이에요",
          "original_form": "일본게임이에요",
          "speaker_id": "2",
          "time": "20210902 15:51"
        },
        {
          "id": "MDRW2100027857.1.9",
          "form": "헛아예 첨 들어봐요",
          "original_form": "헛아예 첨 들어봐요",
          "speaker_id": "1",
          "time": "20210902 16:03"
        },
        {
          "id": "MDRW2100027857.1.10",
          "form": "그럴 수도 있어요 워낙 마이너 게임이라",
          "original_form": "그럴 수도 있어요 워낙 마이너 게임이라",
          "speaker_id": "2",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027857.1.11",
          "form": "네ㅋ 일본에서는 계속 플레이가능한가봐요",
          "original_form": "네ㅋ 일본에서는 계속 플레이가능한가봐요",
          "speaker_id": "1",
          "time": "20210902 16:17"
        },
        {
          "id": "MDRW2100027857.1.12",
          "form": "네ㅎㅎ 일본은 아직 서비스하고 있어요",
          "original_form": "네ㅎㅎ 일본은 아직 서비스하고 있어요",
          "speaker_id": "2",
          "time": "20210902 16:20"
        },
        {
          "id": "MDRW2100027857.1.13",
          "form": "일판 접속 하시나요?",
          "original_form": "일판 접속 하시나요?",
          "speaker_id": "1",
          "time": "20210902 16:31"
        },
        {
          "id": "MDRW2100027857.1.14",
          "form": "예전엔 했었는데 지금은 뜸하게 하고 있어요",
          "original_form": "예전엔 했었는데 지금은 뜸하게 하고 있어요",
          "speaker_id": "2",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100027857.1.15",
          "form": "동호회 분들이랑은 오래 뵀던 분들이라 다른 얘기하고 있어요ㅋㅋ",
          "original_form": "동호회 분들이랑은 오래 뵀던 분들이라 다른 얘기하고 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100027857.1.16",
          "form": "근데 실제로는 모임 안 가졌어요?",
          "original_form": "근데 실제로는 모임 안 가졌어요?",
          "speaker_id": "1",
          "time": "20210902 16:34"
        },
        {
          "id": "MDRW2100027857.1.17",
          "form": "코로나 이전에는 몇번 가지기도 했는데",
          "original_form": "코로나 이전에는 몇번 가지기도 했는데",
          "speaker_id": "2",
          "time": "20210902 16:37"
        },
        {
          "id": "MDRW2100027857.1.18",
          "form": "아무래도 사는 동네도 멀고 코로나 때문에 안만난지 오래됐어요",
          "original_form": "아무래도 사는 동네도 멀고 코로나 때문에 안만난지 오래됐어요",
