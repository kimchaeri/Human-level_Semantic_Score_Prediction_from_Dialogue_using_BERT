      "utterance": [
        {
          "id": "MDRW2100037003.1.1",
          "form": "비가 오면 특별히 땡기는 음식이 있으신가요?",
          "original_form": "비가 오면 특별히 땡기는 음식이 있으신가요?",
          "speaker_id": "2",
          "time": "20210831 17:05"
        },
        {
          "id": "MDRW2100037003.1.2",
          "form": "전 짬뽕이랑 김치전이요",
          "original_form": "전 짬뽕이랑 김치전이요",
          "speaker_id": "1",
          "time": "20210831 17:06"
        },
        {
          "id": "MDRW2100037003.1.3",
          "form": "name1님은여",
          "original_form": "&name1&님은여",
          "speaker_id": "1",
          "time": "20210831 17:06"
        },
        {
          "id": "MDRW2100037003.1.4",
          "form": "헉 저도 김치전이요!!!",
          "original_form": "헉 저도 김치전이요!!!",
          "speaker_id": "2",
          "time": "20210831 17:06"
        },
        {
          "id": "MDRW2100037003.1.5",
          "form": "사실 맛있는 건 언제나 좋지만",
          "original_form": "사실 맛있는 건 언제나 좋지만",
          "speaker_id": "2",
          "time": "20210831 17:06"
        },
        {
          "id": "MDRW2100037003.1.6",
          "form": "라면도 땡기구요....^ㅇ^",
          "original_form": "라면도 땡기구요....^ㅇ^",
          "speaker_id": "2",
          "time": "20210831 17:06"
        },
        {
          "id": "MDRW2100037003.1.7",
          "form": "그쳐 크으",
          "original_form": "그쳐 크으",
          "speaker_id": "1",
          "time": "20210831 17:06"
        },
        {
          "id": "MDRW2100037003.1.8",
          "form": "아근데 김밥 무서워서 못먹겠더라구요",
          "original_form": "아근데 김밥 무서워서 못먹겠더라구요",
          "speaker_id": "2",
          "time": "20210831 17:06"
        },
        {
          "id": "MDRW2100037003.1.9",
          "form": "얼마전에 그 뉴스.... 들으셨죠!!!",
          "original_form": "얼마전에 그 뉴스.... 들으셨죠!!!",
          "speaker_id": "2",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100037003.1.10",
          "form": "아 뉴스봤어요 저도ㅜㅜ",
          "original_form": "아 뉴스봤어요 저도ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100037003.1.11",
          "form": "저 김밥완전좋아하는데ㅜㅜ",
          "original_form": "저 김밥완전좋아하는데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100037003.1.12",
          "form": "저도요...",
          "original_form": "저도요...",
          "speaker_id": "2",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100037003.1.13",
          "form": "특히 참치김밥...",
          "original_form": "특히 참치김밥...",
          "speaker_id": "2",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100037003.1.14",
          "form": "재수학원 다닐 때",
          "original_form": "재수학원 다닐 때",
          "speaker_id": "2",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100037003.1.15",
          "form": "친구들이랑 자습시간에 몰래 아침에",
          "original_form": "친구들이랑 자습시간에 몰래 아침에",
          "speaker_id": "2",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100037003.1.16",
          "form": "참깨라면이랑 참치김밥 먹던게 생각나네요",
          "original_form": "참깨라면이랑 참치김밥 먹던게 생각나네요",
          "speaker_id": "2",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100037003.1.17",
          "form": "그 몰래먹는맛이 더맛있져",
          "original_form": "그 몰래먹는맛이 더맛있져",
          "speaker_id": "1",
          "time": "20210831 17:08"
        },
        {
          "id": "MDRW2100037003.1.18",
          "form": "맞아요!!!! 헤헤",
          "original_form": "맞아요!!!! 헤헤",
          "speaker_id": "2",
          "time": "20210831 17:09"
        },
        {
          "id": "MDRW2100037003.1.19",
          "form": "혹시 라면도 좋아하시나요?",
          "original_form": "혹시 라면도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210831 17:09"
        },
        {
          "id": "MDRW2100037003.1.20",
          "form": "네 완전좋아해요",
          "original_form": "네 완전좋아해요",
          "speaker_id": "1",
          "time": "20210831 17:10"
        },
        {
          "id": "MDRW2100037003.1.21",
          "form": "요즘은 진라면순한맛이 꽂혔고",
          "original_form": "요즘은 진라면순한맛이 꽂혔고",
          "speaker_id": "1",
          "time": "20210831 17:10"
        },
        {
          "id": "MDRW2100037003.1.22",
          "form": "name1님은여",
          "original_form": "&name1&님은여",
          "speaker_id": "1",
          "time": "20210831 17:10"
        },
        {
          "id": "MDRW2100037003.1.23",
          "form": "저도 완전 좋아하죠",
          "original_form": "저도 완전 좋아하죠",
          "speaker_id": "2",
          "time": "20210831 17:10"
        },
        {
          "id": "MDRW2100037003.1.24",
          "form": "오오 진라면 순한맛!!!",
          "original_form": "오오 진라면 순한맛!!!",
          "speaker_id": "2",
          "time": "20210831 17:10"
        },
        {
          "id": "MDRW2100037003.1.25",
          "form": "저도 순한맛 좋아하는데",
          "original_form": "저도 순한맛 좋아하는데",
          "speaker_id": "2",
          "time": "20210831 17:10"
        },
        {
