      "utterance": [
        {
          "id": "MDRW2100036824.1.1",
          "form": "손열음이랑 또 누구더라...",
          "original_form": "손열음이랑 또 누구더라...",
          "speaker_id": "2",
          "time": "20210831 13:04"
        },
        {
          "id": "MDRW2100036824.1.2",
          "form": "유명한 피아니스트 또 있었는데",
          "original_form": "유명한 피아니스트 또 있었는데",
          "speaker_id": "2",
          "time": "20210831 13:04"
        },
        {
          "id": "MDRW2100036824.1.3",
          "form": "그 분 연주회도 티켓팅하기 힘들다 하더라구요 ",
          "original_form": "그 분 연주회도 티켓팅하기 힘들다 하더라구요 ",
          "speaker_id": "2",
          "time": "20210831 13:04"
        },
        {
          "id": "MDRW2100036824.1.4",
          "form": "조성진요",
          "original_form": "조성진요",
          "speaker_id": "1",
          "time": "20210831 13:05"
        },
        {
          "id": "MDRW2100036824.1.5",
          "form": "혹시 썸타는 분이세요?",
          "original_form": "혹시 썸타는 분이세요?",
          "speaker_id": "1",
          "time": "20210831 13:05"
        },
        {
          "id": "MDRW2100036824.1.6",
          "form": "아맞다 조성진이었어요 ㅎㅎㅎ",
          "original_form": "아맞다 조성진이었어요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 13:07"
        },
        {
          "id": "MDRW2100036824.1.7",
          "form": "아뇨 그냥 친구예요!",
          "original_form": "아뇨 그냥 친구예요!",
          "speaker_id": "2",
          "time": "20210831 13:07"
        },
        {
          "id": "MDRW2100036824.1.8",
          "form": "썸이면 대신 예매해달라는거 같아서요 ㅋㅋㅋ",
          "original_form": "썸이면 대신 예매해달라는거 같아서요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:09"
        },
        {
          "id": "MDRW2100036824.1.9",
          "form": "조성진은 진짜 빨리 나가요 그거는 유료멤버십 가입해야 살수있을거에요",
          "original_form": "조성진은 진짜 빨리 나가요 그거는 유료멤버십 가입해야 살수있을거에요",
          "speaker_id": "1",
          "time": "20210831 13:09"
        },
        {
          "id": "MDRW2100036824.1.10",
          "form": "아 ㅋㅋㅋㅋㅋ 그런걸수도 있군요 ㅋㅋㅋㅋㅋㅋ 저는 진짜 친구 이야기였어욬ㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋ 그런걸수도 있군요 ㅋㅋㅋㅋㅋㅋ 저는 진짜 친구 이야기였어욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:11"
        },
        {
          "id": "MDRW2100036824.1.11",
          "form": "와 그렇군요",
          "original_form": "와 그렇군요",
          "speaker_id": "2",
          "time": "20210831 13:12"
        },
        {
          "id": "MDRW2100036824.1.12",
          "form": "name1님 정말 클래식에 일가견 있으신 분이시네요!",
          "original_form": "&name1&님 정말 클래식에 일가견 있으신 분이시네요!",
          "speaker_id": "2",
          "time": "20210831 13:12"
        },
        {
          "id": "MDRW2100036824.1.13",
          "form": "저는 문화생활이라고 해봤자",
          "original_form": "저는 문화생활이라고 해봤자",
          "speaker_id": "2",
          "time": "20210831 13:12"
        },
        {
          "id": "MDRW2100036824.1.14",
          "form": "지난달에 크루엘라랑 블랙위도우 보고 온 것만 생각나네요 ㅎㅎ",
          "original_form": "지난달에 크루엘라랑 블랙위도우 보고 온 것만 생각나네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 13:12"
        },
        {
          "id": "MDRW2100036824.1.15",
          "form": "저는 진짜 클래식만 보거든요 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 진짜 클래식만 보거든요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 13:13"
        },
        {
          "id": "MDRW2100036824.1.16",
          "form": "말씀하신건 영화에요?",
          "original_form": "말씀하신건 영화에요?",
          "speaker_id": "1",
          "time": "20210831 13:13"
        },
        {
          "id": "MDRW2100036824.1.17",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210831 13:17"
        },
        {
          "id": "MDRW2100036824.1.18",
          "form": "아 영화에는 정말 관심이 없으시군요 ㅎㅎㅎ",
          "original_form": "아 영화에는 정말 관심이 없으시군요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 13:17"
        },
        {
          "id": "MDRW2100036824.1.19",
          "form": "저는 영화를 보고싶던데",
          "original_form": "저는 영화를 보고싶던데",
          "speaker_id": "1",
          "time": "20210831 13:17"
        },
        {
          "id": "MDRW2100036824.1.20",
          "form": "안보기시작하니 진짜 관심이 없어서요",
          "original_form": "안보기시작하니 진짜 관심이 없어서요",
          "speaker_id": "1",
          "time": "20210831 13:17"
        },
        {
          "id": "MDRW2100036824.1.21",
          "form": "오오 그러시군요!!!",
          "original_form": "오오 그러시군요!!!",
          "speaker_id": "2",
          "time": "20210831 13:22"
        },
        {
          "id": "MDRW2100036824.1.22",
          "form": "그래도 본인 취향이 뚜렷하면",
          "original_form": "그래도 본인 취향이 뚜렷하면",
          "speaker_id": "2",
          "time": "20210831 13:22"
        },
        {
          "id": "MDRW2100036824.1.23",
          "form": "즐겁고 싶으실 때 편하시겠어요!!!",
          "original_form": "즐겁고 싶으실 때 편하시겠어요!!!",
          "speaker_id": "2",
          "time": "20210831 13:23"
        },
        {
          "id": "MDRW2100036824.1.24",
          "form": "네 그건 그래요",
          "original_form": "네 그건 그래요",
          "speaker_id": "1",
          "time": "20210831 13:23"
        },
        {
          "id": "MDRW2100036824.1.25",
          "form": "다른 사람이 영화말할때 할말이 없는게 문제지만요",
          "original_form": "다른 사람이 영화말할때 할말이 없는게 문제지만요",
          "speaker_id": "1",
          "time": "20210831 13:23"
        },
        {
          "id": "MDRW2100036824.1.26",
          "form": "그럴 수도 있죠 ㅎㅎ",
          "original_form": "그럴 수도 있죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 13:23"
        },
        {
          "id": "MDRW2100036824.1.27",
          "form": "오케스트라도 보신 적 있나요?",
          "original_form": "오케스트라도 보신 적 있나요?",
          "speaker_id": "2",
          "time": "20210831 13:24"
        },
        {
          "id": "MDRW2100036824.1.28",
          "form": "네 오케스트라 연주도봤어요",
          "original_form": "네 오케스트라 연주도봤어요",
          "speaker_id": "1",
          "time": "20210831 13:25"
        },
        {
          "id": "MDRW2100036824.1.29",
          "form": "오케스트라와 피아노의 매력은",
          "original_form": "오케스트라와 피아노의 매력은",
          "speaker_id": "2",
          "time": "20210831 13:26"
        },
        {
          "id": "MDRW2100036824.1.30",
          "form": "비유하자면,,,",
          "original_form": "비유하자면,,,",
          "speaker_id": "2",
          "time": "20210831 13:26"
        },
        {
          "id": "MDRW2100036824.1.31",
          "form": "종합선물세트와 단품 같은 느낌이려나요 ",
