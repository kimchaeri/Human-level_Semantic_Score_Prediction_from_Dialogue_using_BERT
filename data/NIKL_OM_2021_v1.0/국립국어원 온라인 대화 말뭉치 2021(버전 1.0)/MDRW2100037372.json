      "utterance": [
        {
          "id": "MDRW2100037372.1.1",
          "form": "공작도시라고 이것도 막장일듯",
          "original_form": "공작도시라고 이것도 막장일듯",
          "speaker_id": "2",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100037372.1.2",
          "form": "물 들어올때 노 저야야죠",
          "original_form": "물 들어올때 노 저야야죠",
          "speaker_id": "1",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100037372.1.3",
          "form": "또 치정이예요?",
          "original_form": "또 치정이예요?",
          "speaker_id": "1",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100037372.1.4",
          "form": "이건 부자들 세계",
          "original_form": "이건 부자들 세계",
          "speaker_id": "2",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100037372.1.5",
          "form": "부자들 또 막장이 많죠ㅋ",
          "original_form": "부자들 또 막장이 많죠ㅋ",
          "speaker_id": "1",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100037372.1.6",
          "form": "정재계를 뒤흔드는 성진그룹 미술관을 배경으로",
          "original_form": "정재계를 뒤흔드는 성진그룹 미술관을 배경으로",
          "speaker_id": "2",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100037372.1.7",
          "form": "ㅋㅋ재벌들의 암투를 그린 내용",
          "original_form": "ㅋㅋ재벌들의 암투를 그린 내용",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037372.1.8",
          "form": "오~주연배우들이 좋다",
          "original_form": "오~주연배우들이 좋다",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037372.1.9",
          "form": "누구예요?",
          "original_form": "누구예요?",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037372.1.10",
          "form": "수애다 수애 미술관이랑 어울림",
          "original_form": "수애다 수애 미술관이랑 어울림",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037372.1.11",
          "form": "드레수애 오랜만이네",
          "original_form": "드레수애 오랜만이네",
          "speaker_id": "1",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100037372.1.12",
          "form": "김강우도 나오고",
          "original_form": "김강우도 나오고",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100037372.1.13",
          "form": "진짜 배우들은 괜찮네요",
          "original_form": "진짜 배우들은 괜찮네요",
          "speaker_id": "1",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100037372.1.14",
          "form": "수애는 정말 고급진 이미지",
          "original_form": "수애는 정말 고급진 이미지",
          "speaker_id": "2",
          "time": "20210831 22:59"
        }
