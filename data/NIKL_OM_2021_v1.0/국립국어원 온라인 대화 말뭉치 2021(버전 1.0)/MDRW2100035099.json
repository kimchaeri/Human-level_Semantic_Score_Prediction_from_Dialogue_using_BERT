        {
          "id": "MDRW2100035099.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "2",
          "time": "20210829 02:38"
        },
        {
          "id": "MDRW2100035099.1.2",
          "form": "스위치 게임좀 같이 골라줄래",
          "original_form": "스위치 게임좀 같이 골라줄래",
          "speaker_id": "1",
          "time": "20210829 02:39"
        },
        {
          "id": "MDRW2100035099.1.3",
          "form": "닌텐도 스위치 말하는 거니",
          "original_form": "닌텐도 스위치 말하는 거니",
          "speaker_id": "2",
          "time": "20210829 02:39"
        },
        {
          "id": "MDRW2100035099.1.4",
          "form": "맞아 일본게임",
          "original_form": "맞아 일본게임",
          "speaker_id": "1",
          "time": "20210829 02:39"
        },
        {
          "id": "MDRW2100035099.1.5",
          "form": "지금은 어떤 게임 가지고 있는데",
          "original_form": "지금은 어떤 게임 가지고 있는데",
          "speaker_id": "2",
          "time": "20210829 02:39"
        },
        {
          "id": "MDRW2100035099.1.6",
          "form": "나는 저스트댄스밖에 없어",
          "original_form": "나는 저스트댄스밖에 없어",
          "speaker_id": "1",
          "time": "20210829 02:39"
        },
        {
          "id": "MDRW2100035099.1.7",
          "form": "요즘 링 피트도 많이 한데",
          "original_form": "요즘 링 피트도 많이 한데",
          "speaker_id": "2",
          "time": "20210829 02:39"
        },
        {
          "id": "MDRW2100035099.1.8",
          "form": "맞아맞아 그래서 3개의 후보 중 하나야",
          "original_form": "맞아맞아 그래서 3개의 후보 중 하나야",
          "speaker_id": "1",
          "time": "20210829 02:39"
        },
        {
          "id": "MDRW2100035099.1.9",
          "form": "젤다라는 것도 유명하다던데",
          "original_form": "젤다라는 것도 유명하다던데",
          "speaker_id": "2",
          "time": "20210829 02:39"
        },
        {
          "id": "MDRW2100035099.1.10",
          "form": "그건 내취향이 아니라서 후보에 없어",
          "original_form": "그건 내취향이 아니라서 후보에 없어",
          "speaker_id": "1",
          "time": "20210829 02:40"
        },
        {
          "id": "MDRW2100035099.1.11",
          "form": "다른 후보는 어떤 거예요",
          "original_form": "다른 후보는 어떤 거예요",
          "speaker_id": "2",
          "time": "20210829 02:40"
        },
        {
          "id": "MDRW2100035099.1.12",
          "form": "리듬게임을 사고 싶어",
          "original_form": "리듬게임을 사고 싶어",
          "speaker_id": "1",
          "time": "20210829 02:40"
        },
        {
          "id": "MDRW2100035099.1.13",
          "form": "저스트 댄스 말고는 뭐가 있는지 잘 모르겠다",
          "original_form": "저스트 댄스 말고는 뭐가 있는지 잘 모르겠다",
          "speaker_id": "2",
          "time": "20210829 02:40"
        },
        {
          "id": "MDRW2100035099.1.14",
          "form": "사게 되면 알려줄게",
          "original_form": "사게 되면 알려줄게",
          "speaker_id": "1",
          "time": "20210829 02:40"
