      "utterance": [
        {
          "id": "MDRW2100022676.1.1",
          "form": "아이고 그쵸 ㅠㅠ 어떤 부분이 제일 힘드세요?",
          "original_form": "아이고 그쵸 ㅠㅠ 어떤 부분이 제일 힘드세요?",
          "speaker_id": "2",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022676.1.2",
          "form": "이게 결과물에 비해서",
          "original_form": "이게 결과물에 비해서",
          "speaker_id": "1",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100022676.1.3",
          "form": "작업 시간은 몇배는 기본으로 들고",
          "original_form": "작업 시간은 몇배는 기본으로 들고",
          "speaker_id": "1",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100022676.1.4",
          "form": "그러다보면 밤낮 바뀌는건 기본이고ㅠ",
          "original_form": "그러다보면 밤낮 바뀌는건 기본이고ㅠ",
          "speaker_id": "1",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100022676.1.5",
          "form": "그렇겠네요...",
          "original_form": "그렇겠네요...",
          "speaker_id": "2",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022676.1.6",
          "form": "돈도 별로 안주나요?",
          "original_form": "돈도 별로 안주나요?",
          "speaker_id": "2",
          "time": "20210829 18:20"
        },
        {
          "id": "MDRW2100022676.1.7",
          "form": "그렇....죠...",
          "original_form": "그렇....죠...",
          "speaker_id": "1",
          "time": "20210829 20:28"
        },
        {
          "id": "MDRW2100022676.1.8",
          "form": "이쪽 계열이 돈이 되게 짜요ㅠ",
          "original_form": "이쪽 계열이 돈이 되게 짜요ㅠ",
          "speaker_id": "1",
          "time": "20210829 20:28"
        },
        {
          "id": "MDRW2100022676.1.9",
          "form": "근데 그만큼 원하는건 많고..",
          "original_form": "근데 그만큼 원하는건 많고..",
          "speaker_id": "1",
          "time": "20210829 20:28"
        },
        {
          "id": "MDRW2100022676.1.10",
          "form": "시간당으로 주는게 아니라 건당이라",
          "original_form": "시간당으로 주는게 아니라 건당이라",
          "speaker_id": "1",
          "time": "20210829 20:28"
        },
        {
          "id": "MDRW2100022676.1.11",
          "form": "사람마다 다르지만 기본적으로 걸리는 시간이 있으니까..",
          "original_form": "사람마다 다르지만 기본적으로 걸리는 시간이 있으니까..",
          "speaker_id": "1",
          "time": "20210829 20:28"
        },
        {
          "id": "MDRW2100022676.1.12",
          "form": "영상은 편집빨이 반 이상인데 좀 후하게 주면 좋겠네요ㅠ",
          "original_form": "영상은 편집빨이 반 이상인데 좀 후하게 주면 좋겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210829 20:43"
        },
        {
          "id": "MDRW2100022676.1.13",
          "form": "그러면 주마다 정해진 개수가 있는 거예요?",
          "original_form": "그러면 주마다 정해진 개수가 있는 거예요?",
          "speaker_id": "2",
          "time": "20210829 20:43"
        },
        {
          "id": "MDRW2100022676.1.14",
          "form": "그건 아니고",
          "original_form": "그건 아니고",
          "speaker_id": "1",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100022676.1.15",
          "form": "하는만큼이요!",
          "original_form": "하는만큼이요!",
          "speaker_id": "1",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100022676.1.16",
          "form": "이거는 아마 계약마다 다를거예요~",
          "original_form": "이거는 아마 계약마다 다를거예요~",
          "speaker_id": "1",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100022676.1.17",
          "form": "아하 하는만큼 더 받는 거군요",
          "original_form": "아하 하는만큼 더 받는 거군요",
          "speaker_id": "2",
          "time": "20210829 23:01"
        },
        {
          "id": "MDRW2100022676.1.18",
          "form": "더 힘들다,,",
          "original_form": "더 힘들다,,",
          "speaker_id": "2",
          "time": "20210829 23:01"
        },
        {
          "id": "MDRW2100022676.1.19",
          "form": "지금도 밤새 작업중인데...",
          "original_form": "지금도 밤새 작업중인데...",
          "speaker_id": "1",
          "time": "20210830 08:06"
        },
        {
          "id": "MDRW2100022676.1.20",
          "form": "ㅜㅜ이직 고민중입니다",
          "original_form": "ㅜㅜ이직 고민중입니다",
          "speaker_id": "1",
          "time": "20210830 08:06"
        },
        {
          "id": "MDRW2100022676.1.21",
          "form": "아이구 좋은 선택 하시길 바랄게요 ㅠㅠ",
          "original_form": "아이구 좋은 선택 하시길 바랄게요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 11:28"
        },
        {
          "id": "MDRW2100022676.1.22",
          "form": "고민만 두달째 하고잇어요....",
          "original_form": "고민만 두달째 하고잇어요....",
          "speaker_id": "1",
          "time": "20210901 06:17"
        },
        {
          "id": "MDRW2100022676.1.23",
          "form": "이직도 쉽지 않아서ㅜ",
          "original_form": "이직도 쉽지 않아서ㅜ",
          "speaker_id": "1",
          "time": "20210901 06:17"
        },
        {
          "id": "MDRW2100022676.1.24",
          "form": "이직하면 업계도 바꾸시나요?",
          "original_form": "이직하면 업계도 바꾸시나요?",
          "speaker_id": "2",
          "time": "20210901 16:31"
        },
        {
          "id": "MDRW2100022676.1.25",
          "form": "아무래도... 여긴 좁아서",
          "original_form": "아무래도... 여긴 좁아서",
          "speaker_id": "1",
          "time": "20210902 00:13"
        },
        {
          "id": "MDRW2100022676.1.26",
          "form": "아예 직종을 바꾸려구요",
          "original_form": "아예 직종을 바꾸려구요",
          "speaker_id": "1",
          "time": "20210902 00:13"
        },
        {
          "id": "MDRW2100022676.1.27",
          "form": "아하.. 생각해두신 게 있나요?",
          "original_form": "아하.. 생각해두신 게 있나요?",
          "speaker_id": "2",
          "time": "20210902 00:40"
        }
      ]
    }
