      "utterance": [
        {
          "id": "MDRW2100011861.1.1",
          "form": "아 전 아직 잘 모르겠는데",
          "original_form": "아 전 아직 잘 모르겠는데",
          "speaker_id": "2",
          "time": "20210824 16:50"
        },
        {
          "id": "MDRW2100011861.1.2",
          "form": "스파게티 해먹을까 생각중이에요",
          "original_form": "스파게티 해먹을까 생각중이에요",
          "speaker_id": "2",
          "time": "20210824 16:50"
        },
        {
          "id": "MDRW2100011861.1.3",
          "form": "스파게티가 생각보다 해먹기 간편한 편이죠??",
          "original_form": "스파게티가 생각보다 해먹기 간편한 편이죠??",
          "speaker_id": "1",
          "time": "20210824 16:50"
        },
        {
          "id": "MDRW2100011861.1.4",
          "form": "네 소스만 있으면 면만 끓이면 돼서",
          "original_form": "네 소스만 있으면 면만 끓이면 돼서",
          "speaker_id": "2",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100011861.1.5",
          "form": "자주 해먹어요!",
          "original_form": "자주 해먹어요!",
          "speaker_id": "2",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100011861.1.6",
          "form": "저도 곧 자취를 시작하는데",
          "original_form": "저도 곧 자취를 시작하는데",
          "speaker_id": "1",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100011861.1.7",
          "form": "스파게티 자주 해먹을 것 같아요",
          "original_form": "스파게티 자주 해먹을 것 같아요",
          "speaker_id": "1",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100011861.1.8",
          "form": "또 간편하게 해먹을 수 있는 음식 뭐가있나요?",
          "original_form": "또 간편하게 해먹을 수 있는 음식 뭐가있나요?",
          "speaker_id": "1",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100011861.1.9",
          "form": "완전 간단하게 계란 볶음밥도 자주 먹어요",
          "original_form": "완전 간단하게 계란 볶음밥도 자주 먹어요",
          "speaker_id": "2",
          "time": "20210824 16:51"
        },
        {
          "id": "MDRW2100011861.1.10",
          "form": "요즘 계란 가격이 비싸다던데",
          "original_form": "요즘 계란 가격이 비싸다던데",
          "speaker_id": "1",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100011861.1.11",
          "form": "얼마정도하나요??",
          "original_form": "얼마정도하나요??",
          "speaker_id": "1",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100011861.1.12",
          "form": "날계란에 밥 비벼서 파기름이랑 굴소스에 볶는거요",
          "original_form": "날계란에 밥 비벼서 파기름이랑 굴소스에 볶는거요",
          "speaker_id": "2",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100011861.1.13",
          "form": "흠",
          "original_form": "흠",
          "speaker_id": "2",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100011861.1.14",
          "form": "비싼 건 아는데 가격은 잘 모르겠어요!",
          "original_form": "비싼 건 아는데 가격은 잘 모르겠어요!",
          "speaker_id": "2",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100011861.1.15",
          "form": "음 굴소스 정말 만능이죠",
          "original_form": "음 굴소스 정말 만능이죠",
          "speaker_id": "1",
          "time": "20210824 16:53"
        },
        {
          "id": "MDRW2100011861.1.16",
          "form": "집에 하나 구매해둬야겠네요",
          "original_form": "집에 하나 구매해둬야겠네요",
          "speaker_id": "1",
          "time": "20210824 16:53"
        },
        {
          "id": "MDRW2100011861.1.17",
          "form": "맞아요 꽤 자주 쓰더라구요",
          "original_form": "맞아요 꽤 자주 쓰더라구요",
          "speaker_id": "2",
          "time": "20210824 16:53"
        },
        {
          "id": "MDRW2100011861.1.18",
          "form": "에어프라이기는 생각보다 쓰기 귀찮은가요?",
          "original_form": "에어프라이기는 생각보다 쓰기 귀찮은가요?",
          "speaker_id": "1",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100011861.1.19",
          "form": "채소 볶은거에 굴소스 넣어도 맛있어요",
          "original_form": "채소 볶은거에 굴소스 넣어도 맛있어요",
          "speaker_id": "2",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100011861.1.20",
          "form": "음 귀찮진 않은데",
          "original_form": "음 귀찮진 않은데",
          "speaker_id": "2",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100011861.1.21",
          "form": "에어프라이어 돌리면 뭔가.. 좀 지나면 음식이 딱딱하고 뻣뻣해져서",
          "original_form": "에어프라이어 돌리면 뭔가.. 좀 지나면 음식이 딱딱하고 뻣뻣해져서",
          "speaker_id": "2",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100011861.1.22",
          "form": "별로 좋아하진 않아요",
          "original_form": "별로 좋아하진 않아요",
          "speaker_id": "2",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100011861.1.23",
          "form": "아아 무슨느낌인지 알 것 같아요",
          "original_form": "아아 무슨느낌인지 알 것 같아요",
          "speaker_id": "1",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100011861.1.24",
          "form": "친구들은 잘 쓰더라구요!",
          "original_form": "친구들은 잘 쓰더라구요!",
          "speaker_id": "2",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100011861.1.25",
          "form": "조절하기도 조금 힘들죠",
          "original_form": "조절하기도 조금 힘들죠",
          "speaker_id": "1",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100011861.1.26",
          "form": "저도 잘 태워먹어요",
          "original_form": "저도 잘 태워먹어요",
          "speaker_id": "1",
          "time": "20210824 16:54"
        }
      ]
    }
  ]
