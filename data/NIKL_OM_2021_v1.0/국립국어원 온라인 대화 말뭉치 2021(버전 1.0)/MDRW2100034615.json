      "utterance": [
        {
          "id": "MDRW2100034615.1.1",
          "form": "혼자 사시나요?",
          "original_form": "혼자 사시나요?",
          "speaker_id": "1",
          "time": "20210828 16:22"
        },
        {
          "id": "MDRW2100034615.1.2",
          "form": "네 ㅋㅋ 님은요?",
          "original_form": "네 ㅋㅋ 님은요?",
          "speaker_id": "2",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100034615.1.3",
          "form": "저는 부모님하고 살아요 ㅋㅋ",
          "original_form": "저는 부모님하고 살아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100034615.1.4",
          "form": "자취하면 재밌겠어요",
          "original_form": "자취하면 재밌겠어요",
          "speaker_id": "1",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100034615.1.5",
          "form": "아 돈들어요,,,",
          "original_form": "아 돈들어요,,,",
          "speaker_id": "2",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100034615.1.6",
          "form": "아 그건 그렇겠네요 ㅜㅜ",
          "original_form": "아 그건 그렇겠네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100034615.1.7",
          "form": "재미보단 돈과 노동,, 그거 빼면 재미있죠",
          "original_form": "재미보단 돈과 노동,, 그거 빼면 재미있죠",
          "speaker_id": "2",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100034615.1.8",
          "form": "자취하면 돈모으기 힘들단 말을 봤어요",
          "original_form": "자취하면 돈모으기 힘들단 말을 봤어요",
          "speaker_id": "1",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100034615.1.9",
          "form": "뭐든 장단점이 있죠",
          "original_form": "뭐든 장단점이 있죠",
          "speaker_id": "1",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100034615.1.10",
          "form": "ㅠㅠ 할수 없죠 뭐,,",
          "original_form": "ㅠㅠ 할수 없죠 뭐,,",
          "speaker_id": "2",
          "time": "20210828 16:25"
        },
        {
          "id": "MDRW2100034615.1.11",
          "form": "ㅜㅜ그러게요",
          "original_form": "ㅜㅜ그러게요",
          "speaker_id": "1",
          "time": "20210828 16:25"
        },
        {
          "id": "MDRW2100034615.1.12",
          "form": "예전엔 자취가 로망이었어요",
          "original_form": "예전엔 자취가 로망이었어요",
          "speaker_id": "1",
          "time": "20210828 16:25"
        },
        {
          "id": "MDRW2100034615.1.13",
          "form": "그럼 하세요!ㅋㅋ 대신 전세금이,,",
          "original_form": "그럼 하세요!ㅋㅋ 대신 전세금이,,",
          "speaker_id": "2",
          "time": "20210828 16:26"
        },
        {
          "id": "MDRW2100034615.1.14",
          "form": "월세가,,",
          "original_form": "월세가,,",
          "speaker_id": "2",
          "time": "20210828 16:26"
        },
        {
          "id": "MDRW2100034615.1.15",
          "form": "하 비용이 부담스러워서료 ㅜㅜ",
          "original_form": "하 비용이 부담스러워서료 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 16:26"
        },
        {
          "id": "MDRW2100034615.1.16",
          "form": "집안일도 자신없고 마음만 있어요 로망만 ㅋㅋㅋ",
          "original_form": "집안일도 자신없고 마음만 있어요 로망만 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:26"
        },
        {
          "id": "MDRW2100034615.1.17",
          "form": "아끼는 돈에 로망을 가지시면 욕망이 사라지실거에요 ㅠㅠㅋ",
          "original_form": "아끼는 돈에 로망을 가지시면 욕망이 사라지실거에요 ㅠㅠㅋ",
          "speaker_id": "2",
          "time": "20210828 16:26"
        },
        {
          "id": "MDRW2100034615.1.18",
          "form": "앜ㅋㅋㅋㅋㅋㅋ 그런 방법이 있군욬ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ 그런 방법이 있군욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:26"
        }
      ]
