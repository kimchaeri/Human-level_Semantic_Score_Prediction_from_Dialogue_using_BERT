      "utterance": [
        {
          "id": "MDRW2100047725.1.1",
          "form": "출근하셧나요?",
          "original_form": "출근하셧나요?",
          "speaker_id": "1",
          "time": "20210827 10:29"
        },
        {
          "id": "MDRW2100047725.1.2",
          "form": "출근하셧나요?",
          "original_form": "출근하셧나요?",
          "speaker_id": "1",
          "time": "20210827 10:33"
        },
        {
          "id": "MDRW2100047725.1.3",
          "form": "네 지금 가는길이에요",
          "original_form": "네 지금 가는길이에요",
          "speaker_id": "2",
          "time": "20210827 10:37"
        },
        {
          "id": "MDRW2100047725.1.4",
          "form": "저는 진작 출근해서 일하고잇어요",
          "original_form": "저는 진작 출근해서 일하고잇어요",
          "speaker_id": "1",
          "time": "20210827 10:39"
        },
        {
          "id": "MDRW2100047725.1.5",
          "form": "출근시간 여유로우시네욤",
          "original_form": "출근시간 여유로우시네욤",
          "speaker_id": "1",
          "time": "20210827 10:39"
        },
        {
          "id": "MDRW2100047725.1.6",
          "form": "ㅋㅋㅋ 집에 비새서ㅜㅜㅡㅜㅜ",
          "original_form": "ㅋㅋㅋ 집에 비새서ㅜㅜㅡㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 10:39"
        },
        {
          "id": "MDRW2100047725.1.7",
          "form": "본다구요",
          "original_form": "본다구요",
          "speaker_id": "2",
          "time": "20210827 10:40"
        },
        {
          "id": "MDRW2100047725.1.8",
          "form": "앗ㅜㅜ 비가많이왓나봐요ㅜㅜ",
          "original_form": "앗ㅜㅜ 비가많이왓나봐요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 10:40"
        },
        {
          "id": "MDRW2100047725.1.9",
          "form": "네 좀씩 새요 그래서 좀 늦었어요",
          "original_form": "네 좀씩 새요 그래서 좀 늦었어요",
          "speaker_id": "2",
          "time": "20210827 10:44"
        },
        {
          "id": "MDRW2100047725.1.10",
          "form": "주말에는 머하시나요",
          "original_form": "주말에는 머하시나요",
          "speaker_id": "2",
          "time": "20210828 20:11"
        },
        {
          "id": "MDRW2100047725.1.11",
          "form": "오늘 트레이더스에서 장보고왓어요",
          "original_form": "오늘 트레이더스에서 장보고왓어요",
          "speaker_id": "1",
          "time": "20210828 20:11"
        },
        {
          "id": "MDRW2100047725.1.12",
          "form": "오 매우 실용적인 활동이네요",
          "original_form": "오 매우 실용적인 활동이네요",
          "speaker_id": "2",
          "time": "20210828 20:12"
        },
        {
          "id": "MDRW2100047725.1.13",
          "form": "낮엔 덥더라구요",
          "original_form": "낮엔 덥더라구요",
          "speaker_id": "1",
          "time": "20210828 20:12"
        },
        {
          "id": "MDRW2100047725.1.14",
          "form": "주말에는 쉬시나요",
          "original_form": "주말에는 쉬시나요",
          "speaker_id": "2",
          "time": "20210828 20:12"
        },
        {
          "id": "MDRW2100047725.1.15",
          "form": "네 ㅎㅎ\n토욜 잘보내셧어요?",
          "original_form": "네 ㅎㅎ\n토욜 잘보내셧어요?",
          "speaker_id": "1",
          "time": "20210828 20:12"
        },
        {
          "id": "MDRW2100047725.1.16",
          "form": "네 암것두안하고 누워만있고싶네요 ㅎㅎ",
          "original_form": "네 암것두안하고 누워만있고싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 20:13"
        },
        {
          "id": "MDRW2100047725.1.17",
          "form": "그게젤좋져 저두 지금 누웟어요 ㅜ",
          "original_form": "그게젤좋져 저두 지금 누웟어요 ㅜ",
          "speaker_id": "1",
          "time": "20210828 20:14"
        },
        {
          "id": "MDRW2100047725.1.18",
          "form": "장봐오셨으니 저녁짓는일이 또 남아있지않나요",
          "original_form": "장봐오셨으니 저녁짓는일이 또 남아있지않나요",
          "speaker_id": "2",
          "time": "20210828 20:15"
        },
        {
          "id": "MDRW2100047725.1.19",
          "form": "남친이 장금이라서 수육 기다리고잇어요 ㅎㅎ",
          "original_form": "남친이 장금이라서 수육 기다리고잇어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 20:17"
        }
      ]
