      "utterance": [
        {
          "id": "MDRW2100002513.1.1",
          "form": "사고싶은물건있으신가요?",
          "original_form": "사고싶은물건있으신가요?",
          "speaker_id": "2",
          "time": "20210820 13:06"
        },
        {
          "id": "MDRW2100002513.1.2",
          "form": "사고싶은건 많은데 요즘 관심 있는건 플립핸드폰이요",
          "original_form": "사고싶은건 많은데 요즘 관심 있는건 플립핸드폰이요",
          "speaker_id": "1",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002513.1.3",
          "form": "아 플립 진짜 이번에 넘 이쁘더라구여",
          "original_form": "아 플립 진짜 이번에 넘 이쁘더라구여",
          "speaker_id": "2",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100002513.1.4",
          "form": "색상어떤게 젤 맘에드세요",
          "original_form": "색상어떤게 젤 맘에드세요",
          "speaker_id": "2",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100002513.1.5",
          "form": "그쵸~ 무난하게 크림요",
          "original_form": "그쵸~ 무난하게 크림요",
          "speaker_id": "1",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100002513.1.6",
          "form": "전 바이올렛이 넘맘에들더라구여",
          "original_form": "전 바이올렛이 넘맘에들더라구여",
          "speaker_id": "2",
          "time": "20210820 14:01"
        },
        {
          "id": "MDRW2100002513.1.7",
          "form": "크림도 색상이 오묘하게이뿌돈데",
          "original_form": "크림도 색상이 오묘하게이뿌돈데",
          "speaker_id": "2",
          "time": "20210820 14:01"
        },
        {
          "id": "MDRW2100002513.1.8",
          "form": "바이올렛도 이쁜데 너무 튀니까 오래못쓸까바요ㅋ",
          "original_form": "바이올렛도 이쁜데 너무 튀니까 오래못쓸까바요ㅋ",
          "speaker_id": "1",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100002513.1.9",
          "form": "전 케이스를 자주바꿔서 튀어도 케이스끼우면 그만이더라구요 ㅋㅋㅋㅋㅋ",
          "original_form": "전 케이스를 자주바꿔서 튀어도 케이스끼우면 그만이더라구요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:04"
        },
        {
          "id": "MDRW2100002513.1.10",
          "form": "크림도 케이스끼우고 엄청이쁠것같아요",
          "original_form": "크림도 케이스끼우고 엄청이쁠것같아요",
          "speaker_id": "2",
          "time": "20210820 14:04"
        },
        {
          "id": "MDRW2100002513.1.11",
          "form": "요번에 워치4도 엄청좋아보이던데",
          "original_form": "요번에 워치4도 엄청좋아보이던데",
          "speaker_id": "2",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002513.1.12",
          "form": "삼성열일하네요 ㅋㅋㅋ",
          "original_form": "삼성열일하네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002513.1.13",
          "form": "아!!! 그렇네요 케이스ㅋㅋ 가지고 싶은것 투성이에요ㅠㅜ",
          "original_form": "아!!! 그렇네요 케이스ㅋㅋ 가지고 싶은것 투성이에요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210820 14:10"
        },
        {
          "id": "MDRW2100002513.1.14",
          "form": "맞아요 워치이번에 인바디도 측정가능하다고하더라구여",
          "original_form": "맞아요 워치이번에 인바디도 측정가능하다고하더라구여",
          "speaker_id": "2",
          "time": "20210820 14:16"
        },
        {
          "id": "MDRW2100002513.1.15",
          "form": "본것같아요 정말 기술이 날로 발전 하는것 같아요",
          "original_form": "본것같아요 정말 기술이 날로 발전 하는것 같아요",
          "speaker_id": "1",
          "time": "20210820 14:17"
        },
        {
          "id": "MDRW2100002513.1.16",
          "form": "그니까요 기술느는 속도를 못따라갈정도에요 ㅋㅋㅋ",
          "original_form": "그니까요 기술느는 속도를 못따라갈정도에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:25"
        },
        {
          "id": "MDRW2100002513.1.17",
          "form": "10년뒤면 또 얼마나 바뀔지",
          "original_form": "10년뒤면 또 얼마나 바뀔지",
          "speaker_id": "2",
          "time": "20210820 14:25"
        },
        {
          "id": "MDRW2100002513.1.18",
          "form": "최근에 구매하신 물건은 어떤게 있으신가요?ㅎ",
          "original_form": "최근에 구매하신 물건은 어떤게 있으신가요?ㅎ",
          "speaker_id": "1",
          "time": "20210820 14:27"
        },
        {
          "id": "MDRW2100002513.1.19",
          "form": "완전 최근엔 음악씨디구입했구 아 옷도 한벌샀어요 ㅋㅋㅋ",
          "original_form": "완전 최근엔 음악씨디구입했구 아 옷도 한벌샀어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:28"
        }
      ]
