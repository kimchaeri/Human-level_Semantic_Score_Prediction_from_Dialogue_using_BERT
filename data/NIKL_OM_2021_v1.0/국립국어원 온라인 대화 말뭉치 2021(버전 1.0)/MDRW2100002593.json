      "utterance": [
        {
          "id": "MDRW2100002593.1.1",
          "form": "나 최근들어서 사고 싶은게 많아졌는데 넌 사고 싶은거 없어?",
          "original_form": "나 최근들어서 사고 싶은게 많아졌는데 넌 사고 싶은거 없어?",
          "speaker_id": "1",
          "time": "20210820 13:50"
        },
        {
          "id": "MDRW2100002593.1.2",
          "form": "옷!",
          "original_form": "옷!",
          "speaker_id": "2",
          "time": "20210820 13:50"
        },
        {
          "id": "MDRW2100002593.1.3",
          "form": "옷사고싶어",
          "original_form": "옷사고싶어",
          "speaker_id": "2",
          "time": "20210820 13:50"
        },
        {
          "id": "MDRW2100002593.1.4",
          "form": "어떤 옷으로 사고 싶은데?",
          "original_form": "어떤 옷으로 사고 싶은데?",
          "speaker_id": "1",
          "time": "20210820 13:50"
        },
        {
          "id": "MDRW2100002593.1.5",
          "form": "이제 슬슬 가을옷사야지",
          "original_form": "이제 슬슬 가을옷사야지",
          "speaker_id": "2",
          "time": "20210820 13:51"
        },
        {
          "id": "MDRW2100002593.1.6",
          "form": "가디건이나",
          "original_form": "가디건이나",
          "speaker_id": "2",
          "time": "20210820 13:51"
        },
        {
          "id": "MDRW2100002593.1.7",
          "form": "좀 얇은 긴팔?",
          "original_form": "좀 얇은 긴팔?",
          "speaker_id": "2",
          "time": "20210820 13:51"
        },
        {
          "id": "MDRW2100002593.1.8",
          "form": "그리고 슬랙스 정도",
          "original_form": "그리고 슬랙스 정도",
          "speaker_id": "2",
          "time": "20210820 13:51"
        },
        {
          "id": "MDRW2100002593.1.9",
          "form": "난 가디건같은건 정전기 날것 같던데",
          "original_form": "난 가디건같은건 정전기 날것 같던데",
          "speaker_id": "1",
          "time": "20210820 13:52"
        },
        {
          "id": "MDRW2100002593.1.10",
          "form": "넌 괜찮아?",
          "original_form": "넌 괜찮아?",
          "speaker_id": "1",
          "time": "20210820 13:52"
        },
        {
          "id": "MDRW2100002593.1.11",
          "form": "가디건이 정전기보다는",
          "original_form": "가디건이 정전기보다는",
          "speaker_id": "2",
          "time": "20210820 13:53"
        },
        {
          "id": "MDRW2100002593.1.12",
          "form": "고양이 털갈이 땜에 털 묻는게 좀 걱정이긴한데",
          "original_form": "고양이 털갈이 땜에 털 묻는게 좀 걱정이긴한데",
          "speaker_id": "2",
          "time": "20210820 13:53"
        },
        {
          "id": "MDRW2100002593.1.13",
          "form": "그래도 딱히? 큰 걱정은 없어",
          "original_form": "그래도 딱히? 큰 걱정은 없어",
          "speaker_id": "2",
          "time": "20210820 13:53"
        },
        {
          "id": "MDRW2100002593.1.14",
          "form": "털갈이 할때 돌돌이 쓰면 좋긴한데",
          "original_form": "털갈이 할때 돌돌이 쓰면 좋긴한데",
          "speaker_id": "1",
          "time": "20210820 13:53"
        },
        {
          "id": "MDRW2100002593.1.15",
          "form": "옷에 맨날 하는것도 한계가 있지",
          "original_form": "옷에 맨날 하는것도 한계가 있지",
          "speaker_id": "1",
          "time": "20210820 13:54"
        },
        {
          "id": "MDRW2100002593.1.16",
          "form": "그치 힘들기도 하구",
          "original_form": "그치 힘들기도 하구",
          "speaker_id": "2",
          "time": "20210820 13:54"
        },
        {
          "id": "MDRW2100002593.1.17",
          "form": "특히 검은색 옷에 털이 자주 묻어서 좀 골칫거리야",
          "original_form": "특히 검은색 옷에 털이 자주 묻어서 좀 골칫거리야",
          "speaker_id": "2",
          "time": "20210820 13:54"
        },
        {
          "id": "MDRW2100002593.1.18",
          "form": "검은색을 사지말아야하나 고민중이기도하고",
          "original_form": "검은색을 사지말아야하나 고민중이기도하고",
          "speaker_id": "2",
          "time": "20210820 13:54"
        },
        {
          "id": "MDRW2100002593.1.19",
          "form": "회색 어때",
          "original_form": "회색 어때",
          "speaker_id": "1",
          "time": "20210820 13:54"
        },
        {
          "id": "MDRW2100002593.1.20",
          "form": "회색 좋지",
          "original_form": "회색 좋지",
          "speaker_id": "2",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002593.1.21",
          "form": "그래서 만약 산다면",
          "original_form": "그래서 만약 산다면",
          "speaker_id": "2",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002593.1.22",
          "form": "회색계열 살거같음!",
          "original_form": "회색계열 살거같음!",
          "speaker_id": "2",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002593.1.23",
          "form": "흰색도 좋은데 생각해보니 무리겠구나",
          "original_form": "흰색도 좋은데 생각해보니 무리겠구나",
          "speaker_id": "1",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002593.1.24",
          "form": "다른 색깔로 살 생각은 있어?",
          "original_form": "다른 색깔로 살 생각은 있어?",
          "speaker_id": "1",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002593.1.25",
          "form": "개인적으로",
          "original_form": "개인적으로",
          "speaker_id": "2",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002593.1.26",
          "form": "좀 밝은색깔 계열도 도전하고싶긴하지",
          "original_form": "좀 밝은색깔 계열도 도전하고싶긴하지",
          "speaker_id": "2",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002593.1.27",
          "form": "파란색이나 초록색?",
          "original_form": "파란색이나 초록색?",
          "speaker_id": "2",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002593.1.28",
          "form": "초록색은 얼굴이 죽을 수도 있어서 추천은 못하는데",
          "original_form": "초록색은 얼굴이 죽을 수도 있어서 추천은 못하는데",
          "speaker_id": "1",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002593.1.29",
          "form": "파란색은 나쁘지 않아보여",
          "original_form": "파란색은 나쁘지 않아보여",
          "speaker_id": "1",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002593.1.30",
          "form": "겨울에 초록색 맨투맨 입었는데 나름 괜찮긴하더라고",
          "original_form": "겨울에 초록색 맨투맨 입었는데 나름 괜찮긴하더라고",
          "speaker_id": "2",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002593.1.31",
          "form": "파란색은 사보고 초록색은 고려해봐야겠다",
          "original_form": "파란색은 사보고 초록색은 고려해봐야겠다",
          "speaker_id": "2",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002593.1.32",
          "form": "괜찮으면 어울린다는건데",
          "original_form": "괜찮으면 어울린다는건데",
          "speaker_id": "1",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002593.1.33",
          "form": "난 초록색이 안맞더라고",
          "original_form": "난 초록색이 안맞더라고",
          "speaker_id": "1",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002593.1.34",
          "form": "분홍색 초록색 보라색 이렇게 나한테 안맞더라",
          "original_form": "분홍색 초록색 보라색 이렇게 나한테 안맞더라",
          "speaker_id": "1",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002593.1.35",
          "form": "안어울리는건 앵간하면",
          "original_form": "안어울리는건 앵간하면",
          "speaker_id": "2",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002593.1.36",
          "form": "안보는게 낫지",
          "original_form": "안보는게 낫지",
          "speaker_id": "2",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002593.1.37",
          "form": "진짜 무난한건 흰색 검은색 회색이니",
          "original_form": "진짜 무난한건 흰색 검은색 회색이니",
          "speaker_id": "2",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002593.1.38",
          "form": "그거위주로 사는게 좋을거같아",
          "original_form": "그거위주로 사는게 좋을거같아",
          "speaker_id": "2",
          "time": "20210820 13:59"
        }
      ]
    }
