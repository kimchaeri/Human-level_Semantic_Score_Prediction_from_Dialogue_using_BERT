      "utterance": [
        {
          "id": "MDRW2100027057.1.1",
          "form": "유튜브는 머보시나요??",
          "original_form": "유튜브는 머보시나요??",
          "speaker_id": "1",
          "time": "20210902 08:24"
        },
        {
          "id": "MDRW2100027057.1.2",
          "form": "저는 게임 여행 먹방",
          "original_form": "저는 게임 여행 먹방",
          "speaker_id": "2",
          "time": "20210902 08:44"
        },
        {
          "id": "MDRW2100027057.1.3",
          "form": "넷플릭스는이용하나요??",
          "original_form": "넷플릭스는이용하나요??",
          "speaker_id": "1",
          "time": "20210902 08:50"
        },
        {
          "id": "MDRW2100027057.1.4",
          "form": "넷플릭스 파티 안하면 너무 비싼거 같아요 ㅠㅠ",
          "original_form": "넷플릭스 파티 안하면 너무 비싼거 같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 08:51"
        },
        {
          "id": "MDRW2100027057.1.5",
          "form": "맙어요...너무 부담스러운돈...ㅎ",
          "original_form": "맙어요...너무 부담스러운돈...ㅎ",
          "speaker_id": "1",
          "time": "20210902 08:51"
        },
        {
          "id": "MDRW2100027057.1.6",
          "form": "어떤 유투브 보시나요?",
          "original_form": "어떤 유투브 보시나요?",
          "speaker_id": "2",
          "time": "20210902 08:51"
        },
        {
          "id": "MDRW2100027057.1.7",
          "form": "저는 유튜브 게임이나 동물많이봐요",
          "original_form": "저는 유튜브 게임이나 동물많이봐요",
          "speaker_id": "1",
          "time": "20210902 08:52"
        },
        {
          "id": "MDRW2100027057.1.8",
          "form": "오 어떤 게임 많이 보세요",
          "original_form": "오 어떤 게임 많이 보세요",
          "speaker_id": "2",
          "time": "20210902 08:52"
        },
        {
          "id": "MDRW2100027057.1.9",
          "form": "배그랑 폴가이즈??",
          "original_form": "배그랑 폴가이즈??",
          "speaker_id": "1",
          "time": "20210902 08:53"
        },
        {
          "id": "MDRW2100027057.1.10",
          "form": "어몽어스도 많니봤어요",
          "original_form": "어몽어스도 많니봤어요",
          "speaker_id": "1",
          "time": "20210902 08:53"
        },
        {
          "id": "MDRW2100027057.1.11",
          "form": "오 배그는 실제로 하면 심장 떨어져요",
          "original_form": "오 배그는 실제로 하면 심장 떨어져요",
          "speaker_id": "2",
          "time": "20210902 08:53"
        },
        {
          "id": "MDRW2100027057.1.12",
          "form": "맞아요 저도 하면서 잘놀래요..",
          "original_form": "맞아요 저도 하면서 잘놀래요..",
          "speaker_id": "1",
          "time": "20210902 08:53"
        },
        {
          "id": "MDRW2100027057.1.13",
          "form": "폴가이즈는",
          "original_form": "폴가이즈는",
          "speaker_id": "2",
          "time": "20210902 08:55"
        },
        {
          "id": "MDRW2100027057.1.14",
          "form": "스위치에 나오면할려구요",
          "original_form": "스위치에 나오면할려구요",
          "speaker_id": "2",
          "time": "20210902 08:56"
        },
        {
          "id": "MDRW2100027057.1.15",
          "form": "헐 나온데요?",
          "original_form": "헐 나온데요?",
          "speaker_id": "1",
          "time": "20210902 09:00"
        },
        {
          "id": "MDRW2100027057.1.16",
          "form": "그런가바요",
          "original_form": "그런가바요",
          "speaker_id": "2",
          "time": "20210902 09:01"
        },
        {
          "id": "MDRW2100027057.1.17",
          "form": "자주 보는 유투브 뭐 있으시요",
          "original_form": "자주 보는 유투브 뭐 있으시요",
          "speaker_id": "2",
          "time": "20210902 13:00"
        },
        {
          "id": "MDRW2100027057.1.18",
          "form": "있으세요",
