      "utterance": [
        {
          "id": "MDRW2100017111.1.1",
          "form": "요즘 교양예능 위주로 보는데",
          "original_form": "요즘 교양예능 위주로 보는데",
          "speaker_id": "1",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100017111.1.2",
          "form": "표리부동은 뭔가 무서워요",
          "original_form": "표리부동은 뭔가 무서워요",
          "speaker_id": "1",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100017111.1.3",
          "form": "ㅇㅇ 맞아요 표교수님이랑 이수정교수님 두분이 따로 하시는거죠?",
          "original_form": "ㅇㅇ 맞아요 표교수님이랑 이수정교수님 두분이 따로 하시는거죠?",
          "speaker_id": "2",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017111.1.4",
          "form": "같은 사건인데 좀더 무섭던",
          "original_form": "같은 사건인데 좀더 무섭던",
          "speaker_id": "2",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017111.1.5",
          "form": "어제꺼도 자기 전에 보려다가 무섭다고해서",
          "original_form": "어제꺼도 자기 전에 보려다가 무섭다고해서",
          "speaker_id": "1",
          "time": "20210826 19:03"
        },
        {
          "id": "MDRW2100017111.1.6",
          "form": "안봤어요ㅋㅋ",
          "original_form": "안봤어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:03"
        },
        {
          "id": "MDRW2100017111.1.7",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:04"
        },
        {
          "id": "MDRW2100017111.1.8",
          "form": "티벤이 이제 축구 중계해서 담주 슬의 쉬잖아요",
          "original_form": "티벤이 이제 축구 중계해서 담주 슬의 쉬잖아요",
          "speaker_id": "1",
          "time": "20210826 19:04"
        },
        {
          "id": "MDRW2100017111.1.9",
          "form": "밤에 자기전에 보기엔 좀 ㅋㅋ",
          "original_form": "밤에 자기전에 보기엔 좀 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:04"
        },
        {
          "id": "MDRW2100017111.1.10",
          "form": "아 글쿤요",
          "original_form": "아 글쿤요",
          "speaker_id": "2",
          "time": "20210826 19:04"
        },
        {
          "id": "MDRW2100017111.1.11",
          "form": "이동국이랑 배성재 ㅋㅋ",
          "original_form": "이동국이랑 배성재 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017111.1.12",
          "form": "아 축덕 유투버? 이번에 야구팀하나 골라서 팬한다고",
          "original_form": "아 축덕 유투버? 이번에 야구팀하나 골라서 팬한다고",
          "speaker_id": "2",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017111.1.13",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017111.1.14",
          "form": "헐 왜 지금",
          "original_form": "헐 왜 지금",
          "speaker_id": "1",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017111.1.15",
          "form": "야구 인식이 안 좋을 때ㅋㅋ",
          "original_form": "야구 인식이 안 좋을 때ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100017111.1.16",
          "form": "글게요 ㅎㅎㅎ",
          "original_form": "글게요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017111.1.17",
          "form": "뭔가 도전?",
          "original_form": "뭔가 도전?",
          "speaker_id": "2",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017111.1.18",
          "form": "오히려 인식이 안좋아서? 관심가지라고?",
          "original_form": "오히려 인식이 안좋아서? 관심가지라고?",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017111.1.19",
          "form": "요즘 스포츠 컨텐츠는 골프가 대세같은데",
          "original_form": "요즘 스포츠 컨텐츠는 골프가 대세같은데",
          "speaker_id": "1",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017111.1.20",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017111.1.21",
          "form": "별로 인기는 모르겠어요",
          "original_form": "별로 인기는 모르겠어요",
          "speaker_id": "1",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017111.1.22",
          "form": "프로도 엄청 생겼어요",
          "original_form": "프로도 엄청 생겼어요",
          "speaker_id": "2",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017111.1.23",
          "form": "근데 패널?이 너무 비호예요",
          "original_form": "근데 패널?이 너무 비호예요",
          "speaker_id": "2",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017111.1.24",
          "form": "양씨 형제ㅡㅡ",
          "original_form": "양씨 형제ㅡㅡ",
          "speaker_id": "2",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017111.1.25",
          "form": "ㅇㅇ 이렇게까지 생길일인가 싶어요",
          "original_form": "ㅇㅇ 이렇게까지 생길일인가 싶어요",
          "speaker_id": "1",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017111.1.26",
          "form": "아무리 골프 인구가 늘었다지만",
          "original_form": "아무리 골프 인구가 늘었다지만",
          "speaker_id": "1",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100017111.1.27",
          "form": "글게요",
          "original_form": "글게요",
          "speaker_id": "2",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100017111.1.28",
          "form": "티빙 예능으로도 이수근이랑 강호동 신동엽이 골프 하더라고요",
          "original_form": "티빙 예능으로도 이수근이랑 강호동 신동엽이 골프 하더라고요",
          "speaker_id": "1",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017111.1.29",
          "form": "긴거 입음 덥고\r\n짧은거 입음 춥고",
          "original_form": "긴거 입음 덥고\r\n짧은거 입음 춥고",
          "speaker_id": "2",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017111.1.30",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017111.1.31",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017111.1.32",
          "form": "거기도 쫌",
          "original_form": "거기도 쫌",
          "speaker_id": "2",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017111.1.33",
          "form": "일반인들 연애예능보다는 낫지만ㅋㅋ",
          "original_form": "일반인들 연애예능보다는 낫지만ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017111.1.34",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017111.1.35",
          "form": "전 진짜 일반인 연애는 노관심",
          "original_form": "전 진짜 일반인 연애는 노관심",
          "speaker_id": "1",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017111.1.36",
          "form": "글킨한데 진짜 너무 고인물",
          "original_form": "글킨한데 진짜 너무 고인물",
          "speaker_id": "2",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017111.1.37",
          "form": "일반인 연애하는 프로 있어요?",
          "original_form": "일반인 연애하는 프로 있어요?",
          "speaker_id": "2",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017111.1.38",
          "form": "티빙은ㅇㅇ 신서유기 멤버들",
          "original_form": "티빙은ㅇㅇ 신서유기 멤버들",
          "speaker_id": "1",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017111.1.39",
          "form": "환승연애라고",
          "original_form": "환승연애라고",
          "speaker_id": "1",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017111.1.40",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017111.1.41",
          "form": "헤어진 커플들 나오는건데",
          "original_form": "헤어진 커플들 나오는건데",
          "speaker_id": "1",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017111.1.42",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017111.1.43",
          "form": "은근 인기 많아요",
          "original_form": "은근 인기 많아요",
          "speaker_id": "1",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017111.1.44",
          "form": "글쿤요",
          "original_form": "글쿤요",
          "speaker_id": "2",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017111.1.45",
          "form": "각자 애딸린 남녀?",
          "original_form": "각자 애딸린 남녀?",
          "speaker_id": "2",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017111.1.46",
          "form": "제가 1도 안봤는데 조금 알정도ㅋㅋ",
          "original_form": "제가 1도 안봤는데 조금 알정도ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017111.1.47",
          "form": "아니요ㅋㅋㅋ",
          "original_form": "아니요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017111.1.48",
          "form": "그게 그건가요?",
          "original_form": "그게 그건가요?",
          "speaker_id": "2",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017111.1.49",
          "form": "걍 사귀기만 했어요ㅋㅋㅋ",
          "original_form": "걍 사귀기만 했어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017111.1.50",
          "form": "아님 돌싱들 연애하는거 또있던가요",
          "original_form": "아님 돌싱들 연애하는거 또있던가요",
          "speaker_id": "2",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017111.1.51",
          "form": "없을걸요?",
          "original_form": "없을걸요?",
          "speaker_id": "1",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100017111.1.52",
          "form": "전남편 전부인 연애하는걸 보는건 이상할거같은데",
          "original_form": "전남편 전부인 연애하는걸 보는건 이상할거같은데",
          "speaker_id": "1",
          "time": "20210826 19:20"
        },
        {
          "id": "MDRW2100017111.1.53",
          "form": "저게 전남친 전여친들 모아놓고 한집에 살게하는거던데",
          "original_form": "저게 전남친 전여친들 모아놓고 한집에 살게하는거던데",
          "speaker_id": "1",
          "time": "20210826 19:20"
        }
      ]
