      "utterance": [
        {
          "id": "MDRW2100046701.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "1",
          "time": "20211105 00:32"
        },
        {
          "id": "MDRW2100046701.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211105 00:34"
        },
        {
          "id": "MDRW2100046701.1.3",
          "form": "운동 좋아하시나요~?",
          "original_form": "운동 좋아하시나요~?",
          "speaker_id": "2",
          "time": "20211105 00:34"
        },
        {
          "id": "MDRW2100046701.1.4",
          "form": "네 운동 좋아해요",
          "original_form": "네 운동 좋아해요",
          "speaker_id": "1",
          "time": "20211105 00:34"
        },
        {
          "id": "MDRW2100046701.1.5",
          "form": "오 헬스장도 다니시는건가요?!",
          "original_form": "오 헬스장도 다니시는건가요?!",
          "speaker_id": "2",
          "time": "20211105 00:38"
        },
        {
          "id": "MDRW2100046701.1.6",
          "form": "최근까지 다녔었는데 코로나 때문에 잠깐 쉬는 중이에요",
          "original_form": "최근까지 다녔었는데 코로나 때문에 잠깐 쉬는 중이에요",
          "speaker_id": "1",
          "time": "20211105 00:38"
        },
        {
          "id": "MDRW2100046701.1.7",
          "form": "아 ㅜㅜ 코로나",
          "original_form": "아 ㅜㅜ 코로나",
          "speaker_id": "2",
          "time": "20211105 00:39"
        },
        {
          "id": "MDRW2100046701.1.8",
          "form": "제 주변에도",
          "original_form": "제 주변에도",
          "speaker_id": "2",
          "time": "20211105 00:39"
        },
        {
          "id": "MDRW2100046701.1.9",
          "form": "코로나 터지고.. 헬스장 못다니고 있는 친구들이 너무 많아요",
          "original_form": "코로나 터지고.. 헬스장 못다니고 있는 친구들이 너무 많아요",
          "speaker_id": "2",
          "time": "20211105 00:40"
        },
        {
          "id": "MDRW2100046701.1.10",
          "form": "실제로 헬스장에서 집단감염이 많이",
          "original_form": "실제로 헬스장에서 집단감염이 많이",
          "speaker_id": "2",
          "time": "20211105 00:40"
        },
        {
          "id": "MDRW2100046701.1.11",
          "form": "일어나더라구요",
          "original_form": "일어나더라구요",
          "speaker_id": "2",
          "time": "20211105 00:40"
        },
        {
          "id": "MDRW2100046701.1.12",
          "form": "제 친구도 밀접접촉자 분류되서 격리 몇 번 당하더니 도저히 못가겠다고 ㅋㅋ ㅠㅠ",
          "original_form": "제 친구도 밀접접촉자 분류되서 격리 몇 번 당하더니 도저히 못가겠다고 ㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20211105 00:40"
        },
        {
          "id": "MDRW2100046701.1.13",
          "form": "맞아요 운동기구가 한 공간 안에 있어서 밀접하게 운동할 수 밖에 없더라고요",
          "original_form": "맞아요 운동기구가 한 공간 안에 있어서 밀접하게 운동할 수 밖에 없더라고요",
          "speaker_id": "1",
          "time": "20211105 00:40"
        },
        {
          "id": "MDRW2100046701.1.14",
          "form": "네 ㅠㅠㅠ 건강해지려고 갔다가 건강에 위협을 느끼고 돌아오는 아이러니함 흑흑",
          "original_form": "네 ㅠㅠㅠ 건강해지려고 갔다가 건강에 위협을 느끼고 돌아오는 아이러니함 흑흑",
          "speaker_id": "2",
          "time": "20211105 00:43"
        },
        {
          "id": "MDRW2100046701.1.15",
          "form": "저도 가고싶은데 못가고있어요",
          "original_form": "저도 가고싶은데 못가고있어요",
          "speaker_id": "2",
          "time": "20211105 00:43"
        },
        {
          "id": "MDRW2100046701.1.16",
          "form": "그래서 집 앞 걷기만 하는중 ㅠ",
          "original_form": "그래서 집 앞 걷기만 하는중 ㅠ",
          "speaker_id": "2",
          "time": "20211105 00:43"
        },
        {
          "id": "MDRW2100046701.1.17",
          "form": "가끔 홈트 따라하고 ㅎㅎ",
          "original_form": "가끔 홈트 따라하고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211105 00:44"
        },
        {
          "id": "MDRW2100046701.1.18",
          "form": "맞아요 저도 그냥 홈트하거나 근처 산책하면서 운동하는 중이에요",
          "original_form": "맞아요 저도 그냥 홈트하거나 근처 산책하면서 운동하는 중이에요",
          "speaker_id": "1",
          "time": "20211105 00:44"
        },
        {
          "id": "MDRW2100046701.1.19",
          "form": "얼른 코로나 종결되었으면 좋겠어요",
          "original_form": "얼른 코로나 종결되었으면 좋겠어요",
          "speaker_id": "1",
          "time": "20211105 00:44"
        },
        {
          "id": "MDRW2100046701.1.20",
          "form": "맞아요 ㅠㅠ 근데 코로나는 뭔가 끝이 안날거같고",
          "original_form": "맞아요 ㅠㅠ 근데 코로나는 뭔가 끝이 안날거같고",
          "speaker_id": "2",
          "time": "20211105 00:45"
        },
        {
          "id": "MDRW2100046701.1.21",
          "form": "감기처럼 생각하는 문화로",
          "original_form": "감기처럼 생각하는 문화로",
          "speaker_id": "2",
          "time": "20211105 00:45"
        },
        {
          "id": "MDRW2100046701.1.22",
          "form": "바뀌지 않을까..ㅎㅎ",
          "original_form": "바뀌지 않을까..ㅎㅎ",
          "speaker_id": "2",
          "time": "20211105 00:45"
        },
        {
          "id": "MDRW2100046701.1.23",
          "form": "마스크는 계속 써야될 거 같고 느낌이;;",
          "original_form": "마스크는 계속 써야될 거 같고 느낌이;;",
          "speaker_id": "2",
          "time": "20211105 00:45"
        },
        {
          "id": "MDRW2100046701.1.24",
          "form": "맞아요 솔직히 코로나 바이러스 종결된다고 해도 어떤 새로운 바이러스가 또 나타날지 모르는 세상이 된 것 같아요",
          "original_form": "맞아요 솔직히 코로나 바이러스 종결된다고 해도 어떤 새로운 바이러스가 또 나타날지 모르는 세상이 된 것 같아요",
          "speaker_id": "1",
          "time": "20211105 00:45"
        },
        {
          "id": "MDRW2100046701.1.25",
          "form": "맞아요 코로나 끝났다고 안심할게 아닌거같아요 정말;;\r\n",
          "original_form": "맞아요 코로나 끝났다고 안심할게 아닌거같아요 정말;;\r\n",
          "speaker_id": "2",
          "time": "20211105 00:46"
        }
      ]
