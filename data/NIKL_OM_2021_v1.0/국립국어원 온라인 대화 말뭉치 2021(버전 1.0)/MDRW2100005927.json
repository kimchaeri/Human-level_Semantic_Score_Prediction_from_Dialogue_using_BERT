      "utterance": [
        {
          "id": "MDRW2100005927.1.1",
          "form": "코로나라 집에 있는 시간이 많아져서",
          "original_form": "코로나라 집에 있는 시간이 많아져서",
          "speaker_id": "1",
          "time": "20210907 16:46"
        },
        {
          "id": "MDRW2100005927.1.2",
          "form": "저는 피포페인팅을 해보고 있거든요 name2은 해보고계신거 있나요?",
          "original_form": "저는 피포페인팅을 해보고 있거든요 &name2&은 해보고계신거 있나요?",
          "speaker_id": "1",
          "time": "20210907 16:46"
        },
        {
          "id": "MDRW2100005927.1.3",
          "form": "피포페인팅!!! 해보고싶은데",
          "original_form": "피포페인팅!!! 해보고싶은데",
          "speaker_id": "2",
          "time": "20210907 16:47"
        },
        {
          "id": "MDRW2100005927.1.4",
          "form": "어떠신가용???????",
          "original_form": "어떠신가용???????",
          "speaker_id": "2",
          "time": "20210907 16:47"
        },
        {
          "id": "MDRW2100005927.1.5",
          "form": "음악들으면서 멍하게 할수있어서 좋아요",
          "original_form": "음악들으면서 멍하게 할수있어서 좋아요",
          "speaker_id": "1",
          "time": "20210907 16:47"
        },
        {
          "id": "MDRW2100005927.1.6",
          "form": "점점 색이 더해지는걸보면서 뿌듯하기도하구요",
          "original_form": "점점 색이 더해지는걸보면서 뿌듯하기도하구요",
          "speaker_id": "1",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100005927.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100005927.1.8",
          "form": "한작품에 시간 얼마나 걸리셨어요?",
          "original_form": "한작품에 시간 얼마나 걸리셨어요?",
          "speaker_id": "2",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100005927.1.9",
          "form": "한참 몰아서 할때는 주말 이틀동안 다했구요",
          "original_form": "한참 몰아서 할때는 주말 이틀동안 다했구요",
          "speaker_id": "1",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100005927.1.10",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100005927.1.11",
          "form": "하나에 얼마정도해요?",
          "original_form": "하나에 얼마정도해요?",
          "speaker_id": "2",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100005927.1.12",
          "form": "지금하는건 천천히 하고있어서 한달쯤되었어요",
          "original_form": "지금하는건 천천히 하고있어서 한달쯤되었어요",
          "speaker_id": "1",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100005927.1.13",
          "form": "싼건 만원도 있을껄요?",
          "original_form": "싼건 만원도 있을껄요?",
          "speaker_id": "1",
          "time": "20210907 16:49"
        },
        {
          "id": "MDRW2100005927.1.14",
          "form": "오오괜찮네요!",
          "original_form": "오오괜찮네요!",
          "speaker_id": "2",
          "time": "20210907 16:51"
        },
        {
          "id": "MDRW2100005927.1.15",
          "form": "저는 원룸이라 둘곳없을까봐ㅜㅜㅜ 못 시켜봤어요",
          "original_form": "저는 원룸이라 둘곳없을까봐ㅜㅜㅜ 못 시켜봤어요",
          "speaker_id": "2",
          "time": "20210907 16:51"
        },
        {
          "id": "MDRW2100005927.1.16",
          "form": "미니멀리즘을꿈꿔서..",
          "original_form": "미니멀리즘을꿈꿔서..",
          "speaker_id": "2",
          "time": "20210907 16:52"
        },
        {
          "id": "MDRW2100005927.1.17",
          "form": "아하 그러시면 조금 안 맞으시겠다",
          "original_form": "아하 그러시면 조금 안 맞으시겠다",
          "speaker_id": "1",
          "time": "20210907 16:54"
        },
        {
          "id": "MDRW2100005927.1.18",
          "form": "집에서 하시게된 취미있으세요?",
          "original_form": "집에서 하시게된 취미있으세요?",
          "speaker_id": "1",
          "time": "20210907 16:54"
        },
        {
          "id": "MDRW2100005927.1.19",
          "form": "음...자기관리요..!?",
          "original_form": "음...자기관리요..!?",
          "speaker_id": "2",
          "time": "20210907 16:55"
        },
        {
          "id": "MDRW2100005927.1.20",
          "form": "스킨케어나",
          "original_form": "스킨케어나",
          "speaker_id": "2",
          "time": "20210907 16:55"
        },
        {
          "id": "MDRW2100005927.1.21",
          "form": "메이크업공부같은거요!",
          "original_form": "메이크업공부같은거요!",
          "speaker_id": "2",
          "time": "20210907 16:55"
        },
        {
          "id": "MDRW2100005927.1.22",
          "form": "운동은 홈트영상보면서 하시나요?",
          "original_form": "운동은 홈트영상보면서 하시나요?",
          "speaker_id": "1",
          "time": "20210907 16:55"
        },
        {
          "id": "MDRW2100005927.1.23",
          "form": "헬스장 피티받았는데 반깁스를 발에해서",
          "original_form": "헬스장 피티받았는데 반깁스를 발에해서",
          "speaker_id": "2",
          "time": "20210907 16:57"
        },
        {
          "id": "MDRW2100005927.1.24",
          "form": "지금 한달째 쉬는 중입니다ㅠㅠ",
          "original_form": "지금 한달째 쉬는 중입니다ㅠㅠ",
