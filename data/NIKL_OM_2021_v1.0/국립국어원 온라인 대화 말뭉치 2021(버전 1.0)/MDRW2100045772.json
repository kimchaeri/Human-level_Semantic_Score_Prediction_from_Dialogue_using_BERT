        {
          "id": "MDRW2100045772.1.2",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210908 06:56"
        },
        {
          "id": "MDRW2100045772.1.3",
          "form": "한식인가요?",
          "original_form": "한식인가요?",
          "speaker_id": "2",
          "time": "20210908 06:56"
        },
        {
          "id": "MDRW2100045772.1.4",
          "form": "아니에용",
          "original_form": "아니에용",
          "speaker_id": "1",
          "time": "20210908 06:57"
        },
        {
          "id": "MDRW2100045772.1.5",
          "form": "양식인가요?",
          "original_form": "양식인가요?",
          "speaker_id": "2",
          "time": "20210908 06:58"
        },
        {
          "id": "MDRW2100045772.1.6",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210908 06:58"
        },
        {
          "id": "MDRW2100045772.1.7",
          "form": "맞습니다",
          "original_form": "맞습니다",
          "speaker_id": "1",
          "time": "20210908 06:58"
        },
        {
          "id": "MDRW2100045772.1.8",
          "form": "빵이 들어가나요?",
          "original_form": "빵이 들어가나요?",
          "speaker_id": "2",
          "time": "20210908 07:03"
        },
        {
          "id": "MDRW2100045772.1.9",
          "form": "샌드위치인가요?",
          "original_form": "샌드위치인가요?",
          "speaker_id": "2",
          "time": "20210908 07:05"
        },
        {
          "id": "MDRW2100045772.1.10",
          "form": "안들어가요 대신에 그 엇비슷한 주재료러유댜충 비슷하게 생겼여요",
          "original_form": "안들어가요 대신에 그 엇비슷한 주재료러유댜충 비슷하게 생겼여요",
          "speaker_id": "1",
          "time": "20210908 07:05"
        },
        {
          "id": "MDRW2100045772.1.11",
          "form": "음.. 브리또같은걸까요? 아님 또띠아?",
          "original_form": "음.. 브리또같은걸까요? 아님 또띠아?",
          "speaker_id": "2",
          "time": "20210908 07:10"
        },
        {
          "id": "MDRW2100045772.1.12",
          "form": "아녀!",
          "original_form": "아녀!",
          "speaker_id": "1",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100045772.1.13",
          "form": "빵의 주재료가 모죵",
          "original_form": "빵의 주재료가 모죵",
          "speaker_id": "1",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100045772.1.14",
          "form": "그거랑 비슷하게 생긴거요",
          "original_form": "그거랑 비슷하게 생긴거요",
          "speaker_id": "1",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100045772.1.15",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100045772.1.16",
          "form": "밀가루로 만든 무언가를 넣었는데 빵이 아닌 그런느낌일까욬ㅋㅋㅋㅋ",
          "original_form": "밀가루로 만든 무언가를 넣었는데 빵이 아닌 그런느낌일까욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:25"
        },
        {
          "id": "MDRW2100045772.1.17",
          "form": "크래커?",
          "original_form": "크래커?",
          "speaker_id": "2",
          "time": "20210908 07:25"
        },
        {
          "id": "MDRW2100045772.1.18",
          "form": "아뇨 아뇨 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아뇨 아뇨 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:28"
        },
        {
          "id": "MDRW2100045772.1.19",
          "form": "빵의 주재료거 뭐에요",
          "original_form": "빵의 주재료거 뭐에요",
          "speaker_id": "1",
          "time": "20210908 07:28"
        },
        {
          "id": "MDRW2100045772.1.20",
          "form": "가공전!",
          "original_form": "가공전!",
          "speaker_id": "1",
          "time": "20210908 07:28"
        },
        {
          "id": "MDRW2100045772.1.21",
          "form": "밀?",
          "original_form": "밀?",
          "speaker_id": "2",
          "time": "20210908 07:29"
        },
        {
          "id": "MDRW2100045772.1.22",
          "form": "미궁으로 빠져버리네옄ㅋㅋㅋ큐ㅠㅠㅠ",
          "original_form": "미궁으로 빠져버리네옄ㅋㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 07:29"
        },
        {
          "id": "MDRW2100045772.1.23",
          "form": "밀가루 달걀 버터 이스트 소금 설탕...",
          "original_form": "밀가루 달걀 버터 이스트 소금 설탕...",
