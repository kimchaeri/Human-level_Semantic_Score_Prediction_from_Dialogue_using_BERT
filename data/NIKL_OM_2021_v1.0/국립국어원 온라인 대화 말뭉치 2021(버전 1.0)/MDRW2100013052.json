      "utterance": [
        {
          "id": "MDRW2100013052.1.1",
          "form": "나두ㅋㅋ아침에",
          "original_form": "나두ㅋㅋ아침에",
          "speaker_id": "2",
          "time": "20210825 11:41"
        },
        {
          "id": "MDRW2100013052.1.2",
          "form": "대충 두부야채전먹고 나옴ㅋ",
          "original_form": "대충 두부야채전먹고 나옴ㅋ",
          "speaker_id": "2",
          "time": "20210825 11:42"
        },
        {
          "id": "MDRW2100013052.1.3",
          "form": "샐러드가지고 밥되나?",
          "original_form": "샐러드가지고 밥되나?",
          "speaker_id": "2",
          "time": "20210825 11:42"
        },
        {
          "id": "MDRW2100013052.1.4",
          "form": "그게 점심이가?",
          "original_form": "그게 점심이가?",
          "speaker_id": "2",
          "time": "20210825 11:42"
        },
        {
          "id": "MDRW2100013052.1.5",
          "form": "설마?",
          "original_form": "설마?",
          "speaker_id": "2",
          "time": "20210825 11:42"
        },
        {
          "id": "MDRW2100013052.1.6",
          "form": "ㅋㅋㅋㅋ 아니 샐러드만 있는게 아니고",
          "original_form": "ㅋㅋㅋㅋ 아니 샐러드만 있는게 아니고",
          "speaker_id": "1",
          "time": "20210825 11:42"
        },
        {
          "id": "MDRW2100013052.1.7",
          "form": "아아ㅋㅋㅋ",
          "original_form": "아아ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:43"
        },
        {
          "id": "MDRW2100013052.1.8",
          "form": "새우샐러드 및 목살 볶음밥",
          "original_form": "새우샐러드 및 목살 볶음밥",
          "speaker_id": "1",
          "time": "20210825 11:43"
        },
        {
          "id": "MDRW2100013052.1.9",
          "form": "고기 ㅋㅋ",
          "original_form": "고기 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:43"
        },
        {
          "id": "MDRW2100013052.1.10",
          "form": "고기 먹어야제",
          "original_form": "고기 먹어야제",
          "speaker_id": "1",
          "time": "20210825 11:43"
        },
        {
          "id": "MDRW2100013052.1.11",
          "form": "중요하다",
          "original_form": "중요하다",
          "speaker_id": "1",
          "time": "20210825 11:43"
        },
        {
          "id": "MDRW2100013052.1.12",
          "form": "그래그래 그렇게 얘기해야제",
          "original_form": "그래그래 그렇게 얘기해야제",
          "speaker_id": "2",
          "time": "20210825 11:43"
        },
        {
          "id": "MDRW2100013052.1.13",
          "form": "목살 볶음밥에",
          "original_form": "목살 볶음밥에",
          "speaker_id": "1",
          "time": "20210825 11:43"
        },
        {
          "id": "MDRW2100013052.1.14",
          "form": "풀만있는줄",
          "original_form": "풀만있는줄",
          "speaker_id": "2",
          "time": "20210825 11:43"
        },
        {
          "id": "MDRW2100013052.1.15",
          "form": "목살 추가까지 했음",
          "original_form": "목살 추가까지 했음",
          "speaker_id": "1",
          "time": "20210825 11:43"
        },
        {
          "id": "MDRW2100013052.1.16",
          "form": "미안 ㅋㅋㅋㄱㄱ",
          "original_form": "미안 ㅋㅋㅋㄱㄱ",
          "speaker_id": "1",
          "time": "20210825 11:43"
        },
        {
          "id": "MDRW2100013052.1.17",
          "form": "그라이ㅋㅋㅋ",
          "original_form": "그라이ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:43"
        },
        {
          "id": "MDRW2100013052.1.18",
          "form": "오해했제?? ㅋㅋㅋㄱ",
          "original_form": "오해했제?? ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210825 11:43"
        },
        {
          "id": "MDRW2100013052.1.19",
          "form": "달걀도 추가했다 ㅋㅋㅋㅋㅋ",
          "original_form": "달걀도 추가했다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:43"
        },
        {
          "id": "MDRW2100013052.1.20",
          "form": "그래 오해했네ㅋㅋ",
          "original_form": "그래 오해했네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:43"
        },
        {
          "id": "MDRW2100013052.1.21",
          "form": "놀랬다 막이럼ㅋㄱㅋ",
          "original_form": "놀랬다 막이럼ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210825 11:43"
        },
        {
          "id": "MDRW2100013052.1.22",
          "form": "두부야채전 먹고",
          "original_form": "두부야채전 먹고",
          "speaker_id": "1",
          "time": "20210825 11:43"
        },
        {
          "id": "MDRW2100013052.1.23",
          "form": "어디갔노??",
          "original_form": "어디갔노??",
          "speaker_id": "1",
          "time": "20210825 11:43"
        },
        {
          "id": "MDRW2100013052.1.24",
          "form": "오늘 저녁은 뭐 먹니??",
          "original_form": "오늘 저녁은 뭐 먹니??",
          "speaker_id": "1",
          "time": "20210826 15:41"
        },
        {
          "id": "MDRW2100013052.1.25",
          "form": "매일 반찬 걱정이구만",
          "original_form": "매일 반찬 걱정이구만",
