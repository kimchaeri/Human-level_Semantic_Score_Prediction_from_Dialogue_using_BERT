      "utterance": [
        {
          "id": "MDRW2100015619.1.1",
          "form": "요즘 하시는 게임있나요",
          "original_form": "요즘 하시는 게임있나요",
          "speaker_id": "1",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015619.1.2",
          "form": "요즘 게임 잘 안하는 중이지만",
          "original_form": "요즘 게임 잘 안하는 중이지만",
          "speaker_id": "2",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015619.1.3",
          "form": "게임 얘기는 지겹게 들어요",
          "original_form": "게임 얘기는 지겹게 들어요",
          "speaker_id": "2",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015619.1.4",
          "form": "롤이랑 피파랑 배그랑 뭐 이것저것",
          "original_form": "롤이랑 피파랑 배그랑 뭐 이것저것",
          "speaker_id": "2",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015619.1.5",
          "form": "게임 잘 안하시잖아요",
          "original_form": "게임 잘 안하시잖아요",
          "speaker_id": "2",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100015619.1.6",
          "form": "네 저는 잘 안해요 동생이 요즘 하는게있던데",
          "original_form": "네 저는 잘 안해요 동생이 요즘 하는게있던데",
          "speaker_id": "1",
          "time": "20210826 07:01"
        },
        {
          "id": "MDRW2100015619.1.7",
          "form": "오 동생은 무슨 게임을 하나요",
          "original_form": "오 동생은 무슨 게임을 하나요",
          "speaker_id": "2",
          "time": "20210826 07:27"
        },
        {
          "id": "MDRW2100015619.1.8",
          "form": "뭐였는지모르겠어요 ㅋㅋㅋ물어봤는데",
          "original_form": "뭐였는지모르겠어요 ㅋㅋㅋ물어봤는데",
          "speaker_id": "1",
          "time": "20210826 07:42"
        },
        {
          "id": "MDRW2100015619.1.9",
          "form": "저는 마리오 이런 고전 게임 좋아해요 name2님은요?",
          "original_form": "저는 마리오 이런 고전 게임 좋아해요 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210826 07:43"
        },
        {
          "id": "MDRW2100015619.1.10",
          "form": "저도 닌텐도 게임 좋아해요. 마리오 시리즈 재밌는게 많아요",
          "original_form": "저도 닌텐도 게임 좋아해요. 마리오 시리즈 재밌는게 많아요",
          "speaker_id": "2",
          "time": "20210826 08:19"
        },
        {
          "id": "MDRW2100015619.1.11",
          "form": "최근까지는 동물의 숲을 열심히 했어요",
          "original_form": "최근까지는 동물의 숲을 열심히 했어요",
          "speaker_id": "2",
          "time": "20210826 08:19"
        },
        {
          "id": "MDRW2100015619.1.12",
          "form": "아 맞다 동물의숲",
          "original_form": "아 맞다 동물의숲",
          "speaker_id": "1",
          "time": "20210826 08:44"
        },
        {
          "id": "MDRW2100015619.1.13",
          "form": "농사짓고 집꾸미고 열심히 사는 게임 좋아해요",
          "original_form": "농사짓고 집꾸미고 열심히 사는 게임 좋아해요",
          "speaker_id": "2",
          "time": "20210826 08:53"
        },
        {
          "id": "MDRW2100015619.1.14",
          "form": "게임에서 노는게 아니라 일해요",
          "original_form": "게임에서 노는게 아니라 일해요",
          "speaker_id": "2",
          "time": "20210826 08:53"
        },
        {
          "id": "MDRW2100015619.1.15",
          "form": "게임에서도 일을 하는군요",
          "original_form": "게임에서도 일을 하는군요",
          "speaker_id": "1",
          "time": "20210826 09:21"
        },
        {
          "id": "MDRW2100015619.1.16",
          "form": "부지런합니다!",
          "original_form": "부지런합니다!",
          "speaker_id": "1",
          "time": "20210826 09:21"
        },
        {
          "id": "MDRW2100015619.1.17",
          "form": "베짱이가 되어야합니다",
          "original_form": "베짱이가 되어야합니다",
          "speaker_id": "2",
          "time": "20210826 09:23"
        },
        {
          "id": "MDRW2100015619.1.18",
          "form": "어릴땐 게임 많이 했는데 이제 게임하려면 체력이 필요하네요",
          "original_form": "어릴땐 게임 많이 했는데 이제 게임하려면 체력이 필요하네요",
          "speaker_id": "2",
          "time": "20210826 09:23"
        },
        {
          "id": "MDRW2100015619.1.19",
          "form": "ㅋㅋㅋㅋ너무 슬픈 어른의 삶",
          "original_form": "ㅋㅋㅋㅋ너무 슬픈 어른의 삶",
          "speaker_id": "2",
          "time": "20210826 09:24"
        },
        {
          "id": "MDRW2100015619.1.20",
          "form": "게임하기도 피곤하네요",
          "original_form": "게임하기도 피곤하네요",
          "speaker_id": "1",
          "time": "20210826 11:02"
        },
        {
          "id": "MDRW2100015619.1.21",
          "form": "근데 수업에 게임을 접목하려는 노력을 계속 하는 중이라",
          "original_form": "근데 수업에 게임을 접목하려는 노력을 계속 하는 중이라",
          "speaker_id": "2",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100015619.1.22",
          "form": "게임에 관심을 기울여야해요",
          "original_form": "게임에 관심을 기울여야해요",
          "speaker_id": "2",
          "time": "20210826 11:04"
        },
        {
          "id": "MDRW2100015619.1.23",
          "form": "맞아요 게임스",
          "original_form": "맞아요 게임스",
          "speaker_id": "1",
          "time": "20210826 11:35"
        },
        {
          "id": "MDRW2100015619.1.24",
          "form": "카훗 이런거?",
          "original_form": "카훗 이런거?",
          "speaker_id": "1",
          "time": "20210826 11:35"
        },
        {
          "id": "MDRW2100015619.1.25",
          "form": "와 천재에요?",
          "original_form": "와 천재에요?",
