      "utterance": [
        {
          "id": "MDRW2100027391.1.1",
          "form": "오 추천 해주새요",
          "original_form": "오 추천 해주새요",
          "speaker_id": "2",
          "time": "20210902 12:13"
        },
        {
          "id": "MDRW2100027391.1.2",
          "form": "신작어떠세요",
          "original_form": "신작어떠세요",
          "speaker_id": "1",
          "time": "20210902 12:13"
        },
        {
          "id": "MDRW2100027391.1.3",
          "form": "마리오게임이요!",
          "original_form": "마리오게임이요!",
          "speaker_id": "1",
          "time": "20210902 12:14"
        },
        {
          "id": "MDRW2100027391.1.4",
          "form": "오 와리오 인가 그거요?",
          "original_form": "오 와리오 인가 그거요?",
          "speaker_id": "2",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100027391.1.5",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100027391.1.6",
          "form": "그거 옛버전 재밌었는데",
          "original_form": "그거 옛버전 재밌었는데",
          "speaker_id": "2",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100027391.1.7",
          "form": "저도  했어요",
          "original_form": "저도  했어요",
          "speaker_id": "1",
          "time": "20210902 12:16"
        },
        {
          "id": "MDRW2100027391.1.8",
          "form": "ㅋㅋㅋㅋㅋ 맞아요 ㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ 맞아요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 12:16"
        },
        {
          "id": "MDRW2100027391.1.9",
          "form": "리듬도",
          "original_form": "리듬도",
          "speaker_id": "2",
          "time": "20210902 12:16"
        },
        {
          "id": "MDRW2100027391.1.10",
          "form": "재밌지 않나요",
          "original_form": "재밌지 않나요",
          "speaker_id": "2",
          "time": "20210902 12:16"
        },
        {
          "id": "MDRW2100027391.1.11",
          "form": "오 재미있겠어요 ㅠㅠ",
          "original_form": "오 재미있겠어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027391.1.12",
          "form": "스위티 버전으로도 내줬으면 좋겠어요",
          "original_form": "스위티 버전으로도 내줬으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210902 12:37"
        },
        {
          "id": "MDRW2100027391.1.13",
          "form": "스위치",
          "original_form": "스위치",
          "speaker_id": "2",
          "time": "20210902 12:37"
        },
        {
          "id": "MDRW2100027391.1.14",
          "form": "너무좋을꺼같아요",
          "original_form": "너무좋을꺼같아요",
          "speaker_id": "1",
          "time": "20210902 12:42"
        },
        {
          "id": "MDRW2100027391.1.15",
          "form": "모바일게임 재밋는거 없을까요",
          "original_form": "모바일게임 재밋는거 없을까요",
          "speaker_id": "2",
          "time": "20210902 12:44"
        },
        {
          "id": "MDRW2100027391.1.16",
          "form": "음...저도 모바일은 잘모르겠어요",
          "original_form": "음...저도 모바일은 잘모르겠어요",
          "speaker_id": "1",
          "time": "20210902 12:54"
        },
        {
          "id": "MDRW2100027391.1.17",
          "form": "저는 원신 해봤었는데",
          "original_form": "저는 원신 해봤었는데",
