      "utterance": [
        {
          "id": "MDRW2100017045.1.1",
          "form": "네 책임감도 있고.. 은근 자상해요 ㅎ",
          "original_form": "네 책임감도 있고.. 은근 자상해요 ㅎ",
          "speaker_id": "1",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100017045.1.2",
          "form": "오오 ㅋㅋㅋㅋ부럽습니당ㅋㅋㅋㅋ",
          "original_form": "오오 ㅋㅋㅋㅋ부럽습니당ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100017045.1.3",
          "form": "저는 아직 결혼생각이없긴한데",
          "original_form": "저는 아직 결혼생각이없긴한데",
          "speaker_id": "2",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100017045.1.4",
          "form": "나중에 늙었을때 생각하면 좀 무섭긴해요",
          "original_form": "나중에 늙었을때 생각하면 좀 무섭긴해요",
          "speaker_id": "2",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100017045.1.5",
          "form": "같이할 사람있다는게 참 좋을 것 같아요",
          "original_form": "같이할 사람있다는게 참 좋을 것 같아요",
          "speaker_id": "2",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100017045.1.6",
          "form": "네 인연만나시면 이쁜사랑하시고 결혼하세요",
          "original_form": "네 인연만나시면 이쁜사랑하시고 결혼하세요",
          "speaker_id": "1",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017045.1.7",
          "form": "넹 ㅋㅋㅋㅋ 덕담 감사합니다!!!",
          "original_form": "넹 ㅋㅋㅋㅋ 덕담 감사합니다!!!",
          "speaker_id": "2",
          "time": "20210826 17:36"
        },
        {
          "id": "MDRW2100017045.1.8",
          "form": "연애는 얼마하시고 결혼하셨나요??",
          "original_form": "연애는 얼마하시고 결혼하셨나요??",
          "speaker_id": "2",
          "time": "20210826 17:36"
        },
        {
          "id": "MDRW2100017045.1.9",
          "form": "5년하고 결혼했어요",
          "original_form": "5년하고 결혼했어요",
          "speaker_id": "1",
          "time": "20210826 17:37"
        },
        {
          "id": "MDRW2100017045.1.10",
          "form": "우왕 ㅋㅋㅋㅋ 동갑이신가요?!",
          "original_form": "우왕 ㅋㅋㅋㅋ 동갑이신가요?!",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017045.1.11",
          "form": "네 동갑이요\r\n2010년에 결혼했어요",
          "original_form": "네 동갑이요\r\n2010년에 결혼했어요",
          "speaker_id": "1",
          "time": "20210826 17:48"
        },
        {
          "id": "MDRW2100017045.1.12",
          "form": "오 그럼 벌써 12년차신건가 우와 ..",
          "original_form": "오 그럼 벌써 12년차신건가 우와 ..",
          "speaker_id": "2",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100017045.1.13",
          "form": "네 ㅎ 지금은 친구같아요 ㅎ 동갑이니 말편하게하궁ㅅ",
          "original_form": "네 ㅎ 지금은 친구같아요 ㅎ 동갑이니 말편하게하궁ㅅ",
          "speaker_id": "1",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100017045.1.14",
          "form": "하구요",
          "original_form": "하구요",
          "speaker_id": "1",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100017045.1.15",
          "form": "친구같은거 진짜 좋을거같아요!! ㅋㅋㅋㅋㅋ",
          "original_form": "친구같은거 진짜 좋을거같아요!! ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017045.1.16",
          "form": "저도 편한사람만나구 싶어요 ㅋㅋㅋㅋ",
          "original_form": "저도 편한사람만나구 싶어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017045.1.17",
          "form": "근데 좋아하는 사람생기면 로봇이 된답니당.. ㅋㅋㅋ",
          "original_form": "근데 좋아하는 사람생기면 로봇이 된답니당.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017045.1.18",
          "form": "네 편해서 좋았어요",
          "original_form": "네 편해서 좋았어요",
          "speaker_id": "1",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017045.1.19",
          "form": "맞아요ㅎㅎ 결혼상대자로는...편안한게 제일중요한거 같아요",
          "original_form": "맞아요ㅎㅎ 결혼상대자로는...편안한게 제일중요한거 같아요",
          "speaker_id": "1",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017045.1.20",
          "form": "아니 근데 관심이있으면 굳어버리게돼요 ㅋㅋㅋㅋ 관심없는사람에게만 편하고 ㅋㅋㅋㅋ 제 성격을 개조해야 하겄죠?ㅋㅋㅋㅋ",
          "original_form": "아니 근데 관심이있으면 굳어버리게돼요 ㅋㅋㅋㅋ 관심없는사람에게만 편하고 ㅋㅋㅋㅋ 제 성격을 개조해야 하겄죠?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:49"
        },
        {
          "id": "MDRW2100017045.1.21",
          "form": "저도 그래요 ㅜ 전 신랑이 첨에 너무좋지않았어요 \r\n그래서 더편했겠네요 ㅎ",
          "original_form": "저도 그래요 ㅜ 전 신랑이 첨에 너무좋지않았어요 \r\n그래서 더편했겠네요 ㅎ",
          "speaker_id": "1",
