      "utterance": [
        {
          "id": "MDRW2100032653.1.1",
          "form": "저는 요즘 미용과건겅목적으로",
          "original_form": "저는 요즘 미용과건겅목적으로",
          "speaker_id": "1",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032653.1.2",
          "form": "다이어트를하고있엉ㆍ",
          "original_form": "다이어트를하고있엉ㆍ",
          "speaker_id": "1",
          "time": "20210905 21:51"
        },
        {
          "id": "MDRW2100032653.1.3",
          "form": "다이어트 어떻게 하고있어요??",
          "original_form": "다이어트 어떻게 하고있어요??",
          "speaker_id": "2",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100032653.1.4",
          "form": "식단딘이어트하고있어요",
          "original_form": "식단딘이어트하고있어요",
          "speaker_id": "1",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100032653.1.5",
          "form": "다이어트",
          "original_form": "다이어트",
          "speaker_id": "1",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100032653.1.6",
          "form": "탄수화물줄이기요",
          "original_form": "탄수화물줄이기요",
          "speaker_id": "1",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100032653.1.7",
          "form": "오 그런걸 뭐라고 하던데, 키토제닉식단이던가요?",
          "original_form": "오 그런걸 뭐라고 하던데, 키토제닉식단이던가요?",
          "speaker_id": "2",
          "time": "20210905 21:54"
        },
        {
          "id": "MDRW2100032653.1.8",
          "form": "맞아요 키토제닉^^",
          "original_form": "맞아요 키토제닉^^",
          "speaker_id": "1",
          "time": "20210905 21:54"
        },
        {
          "id": "MDRW2100032653.1.9",
          "form": "식단만바꿔조도 조금씩 빠지드라구요",
          "original_form": "식단만바꿔조도 조금씩 빠지드라구요",
          "speaker_id": "1",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100032653.1.10",
          "form": "식단 바꾸는거 어렵던데 대단해요",
          "original_form": "식단 바꾸는거 어렵던데 대단해요",
          "speaker_id": "2",
          "time": "20210905 21:58"
        },
        {
          "id": "MDRW2100032653.1.11",
          "form": "탄수화물 어떻게 줄이죠? 밥 빵 국수 등...",
          "original_form": "탄수화물 어떻게 줄이죠? 밥 빵 국수 등...",
          "speaker_id": "2",
          "time": "20210905 21:58"
        },
        {
          "id": "MDRW2100032653.1.12",
          "form": "밥빵국수를줄이는데",
          "original_form": "밥빵국수를줄이는데",
          "speaker_id": "1",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032653.1.13",
          "form": "대체식품이있더라구요",
          "original_form": "대체식품이있더라구요",
          "speaker_id": "1",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032653.1.14",
          "form": "밥대신 곤약쌀섞고",
          "original_form": "밥대신 곤약쌀섞고",
          "speaker_id": "1",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032653.1.15",
          "form": "빵대신 단백질빵",
          "original_form": "빵대신 단백질빵",
          "speaker_id": "1",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032653.1.16",
          "form": "국수대신 두부면등등으로 바꾸면돼요",
          "original_form": "국수대신 두부면등등으로 바꾸면돼요",
          "speaker_id": "1",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032653.1.17",
          "form": "와 완전 새로운 세상 이야기같아요",
          "original_form": "와 완전 새로운 세상 이야기같아요",
          "speaker_id": "2",
          "time": "20210905 22:19"
        },
        {
          "id": "MDRW2100032653.1.18",
          "form": "곤약쌀은 어떨지 상상이 안돼요 ㅎㅎ",
          "original_form": "곤약쌀은 어떨지 상상이 안돼요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 22:19"
        },
        {
          "id": "MDRW2100032653.1.19",
          "form": "처음에 습관들이는게힘들었는데이제익숙해지니 할만해졋어요",
          "original_form": "처음에 습관들이는게힘들었는데이제익숙해지니 할만해졋어요",
          "speaker_id": "1",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100032653.1.20",
          "form": "레시피가 많드라그요^^",
          "original_form": "레시피가 많드라그요^^",
          "speaker_id": "1",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100032653.1.21",
          "form": "어떤 맛이에요??",
          "original_form": "어떤 맛이에요??",
          "speaker_id": "2",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100032653.1.22",
          "form": "일반쌀과조금섞어서",
          "original_form": "일반쌀과조금섞어서",
          "speaker_id": "1",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100032653.1.23",
          "form": "맛과향은없고 톡톡터지는식감이에요",
          "original_form": "맛과향은없고 톡톡터지는식감이에요",
          "speaker_id": "1",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100032653.1.24",
          "form": "그냥 곤약밥으로만 먹진 안나봐요??",
          "original_form": "그냥 곤약밥으로만 먹진 안나봐요??",
          "speaker_id": "2",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100032653.1.25",
          "form": "네 그러면배탈이나고 영양이넘부족하대요",
          "original_form": "네 그러면배탈이나고 영양이넘부족하대요",
          "speaker_id": "1",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032653.1.26",
          "form": "아 그런 이유때문이군요",
          "original_form": "아 그런 이유때문이군요",
          "speaker_id": "2",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032653.1.27",
          "form": "단백질 빵은 많이 들어봤는데 맛이 어떤가요?",
          "original_form": "단백질 빵은 많이 들어봤는데 맛이 어떤가요?",
          "speaker_id": "2",
          "time": "20210905 22:36"
        },
        {
          "id": "MDRW2100032653.1.28",
          "form": "조금 퍽퍽하긴하지만 먹을만해요^^",
          "original_form": "조금 퍽퍽하긴하지만 먹을만해요^^",
