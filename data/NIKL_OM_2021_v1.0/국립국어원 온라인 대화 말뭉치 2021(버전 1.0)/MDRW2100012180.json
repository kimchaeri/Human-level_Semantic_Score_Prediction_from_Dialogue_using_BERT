      "utterance": [
        {
          "id": "MDRW2100012180.1.1",
          "form": "아 그분책읽었는데 생각보다 별로더라구..",
          "original_form": "아 그분책읽었는데 생각보다 별로더라구..",
          "speaker_id": "2",
          "time": "20210824 20:48"
        },
        {
          "id": "MDRW2100012180.1.2",
          "form": "무조건 재밌는건 아니였나봐",
          "original_form": "무조건 재밌는건 아니였나봐",
          "speaker_id": "2",
          "time": "20210824 20:48"
        },
        {
          "id": "MDRW2100012180.1.3",
          "form": "ㅋㅋㅋㅋ웅웅~",
          "original_form": "ㅋㅋㅋㅋ웅웅~",
          "speaker_id": "1",
          "time": "20210824 21:11"
        },
        {
          "id": "MDRW2100012180.1.4",
          "form": "히가시노게이고가 원래 추리쪽으로 유명했던 사람이라서",
          "original_form": "히가시노게이고가 원래 추리쪽으로 유명했던 사람이라서",
          "speaker_id": "1",
          "time": "20210824 21:11"
        },
        {
          "id": "MDRW2100012180.1.5",
          "form": "오히려 나미화 나왔을때 찐팬들이 좀 놀럈었지~",
          "original_form": "오히려 나미화 나왔을때 찐팬들이 좀 놀럈었지~",
          "speaker_id": "1",
          "time": "20210824 21:11"
        },
        {
          "id": "MDRW2100012180.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:52"
        },
        {
          "id": "MDRW2100012180.1.7",
          "form": "추리만 좋았던것같아",
          "original_form": "추리만 좋았던것같아",
          "speaker_id": "2",
          "time": "20210825 14:52"
        },
        {
          "id": "MDRW2100012180.1.8",
          "form": "다른책은 생각보단 그저그래서",
          "original_form": "다른책은 생각보단 그저그래서",
          "speaker_id": "2",
          "time": "20210825 14:53"
        },
        {
          "id": "MDRW2100012180.1.9",
          "form": "실망했어ㅜㅜ",
          "original_form": "실망했어ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 14:53"
        },
        {
          "id": "MDRW2100012180.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ안맞는 문체가 있지~~ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ안맞는 문체가 있지~~ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100012180.1.11",
          "form": "뭐 읽었었는데??",
          "original_form": "뭐 읽었었는데??",
          "speaker_id": "1",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100012180.1.12",
          "form": "독소소설",
          "original_form": "독소소설",
          "speaker_id": "2",
          "time": "20210825 15:58"
        },
        {
          "id": "MDRW2100012180.1.13",
          "form": "그냥 내용이 별로였어...",
          "original_form": "그냥 내용이 별로였어...",
          "speaker_id": "2",
          "time": "20210825 15:58"
        },
        {
          "id": "MDRW2100012180.1.14",
          "form": "뻔한데 재미없는내용",
          "original_form": "뻔한데 재미없는내용",
          "speaker_id": "2",
          "time": "20210825 15:58"
        },
        {
          "id": "MDRW2100012180.1.15",
          "form": "ㅋㅋㅋㅋㅋ이런..ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ이런..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100012180.1.16",
          "form": "방과후 추천//",
          "original_form": "방과후 추천//",
          "speaker_id": "1",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100012180.1.17",
          "form": "이게 처녀작인데 꽤 흥미로워~~",
          "original_form": "이게 처녀작인데 꽤 흥미로워~~",
          "speaker_id": "1",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100012180.1.18",
          "form": "오 방과후",
          "original_form": "오 방과후",
          "speaker_id": "2",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100012180.1.19",
          "form": "다음번에 도서관가게되면 빌려봐야겠다",
          "original_form": "다음번에 도서관가게되면 빌려봐야겠다",
          "speaker_id": "2",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100012180.1.20",
          "form": "옹옹~ 고거는 재밌을거야~",
          "original_form": "옹옹~ 고거는 재밌을거야~",
          "speaker_id": "1",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100012180.1.21",
          "form": "예헤",
          "original_form": "예헤",
          "speaker_id": "2",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100012180.1.22",
          "form": "조만간 마음의양식을 채우러 가야겠구만",
          "original_form": "조만간 마음의양식을 채우러 가야겠구만",
          "speaker_id": "2",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100012180.1.23",
          "form": "아주 좋은 생각~//",
          "original_form": "아주 좋은 생각~//",
          "speaker_id": "1",
          "time": "20210826 13:19"
        },
        {
          "id": "MDRW2100012180.1.24",
          "form": "ㅋㅋㅋㅋ언니는 요즘에 책 어떻게읽어요?",
          "original_form": "ㅋㅋㅋㅋ언니는 요즘에 책 어떻게읽어요?",
          "speaker_id": "2",
          "time": "20210826 13:39"
        },
        {
          "id": "MDRW2100012180.1.25",
          "form": "사서?",
          "original_form": "사서?",
          "speaker_id": "2",
          "time": "20210826 13:39"
        },
        {
          "id": "MDRW2100012180.1.26",
          "form": "도서관에서 빌려 읽고 괜찮으면 구입//",
          "original_form": "도서관에서 빌려 읽고 괜찮으면 구입//",
          "speaker_id": "1",
          "time": "20210826 14:10"
        },
        {
          "id": "MDRW2100012180.1.27",
          "form": "수욜점심시간 이용해서 도서관갔다오고 있어~ㅋㅋ",
          "original_form": "수욜점심시간 이용해서 도서관갔다오고 있어~ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:10"
        },
        {
          "id": "MDRW2100012180.1.28",
          "form": "헉 대단하다",
          "original_form": "헉 대단하다",
          "speaker_id": "2",
          "time": "20210827 08:56"
        },
        {
          "id": "MDRW2100012180.1.29",
          "form": "회사근처에 도서관이 있나보네요",
          "original_form": "회사근처에 도서관이 있나보네요",
          "speaker_id": "2",
          "time": "20210827 08:56"
        },
        {
          "id": "MDRW2100012180.1.30",
          "form": "아!! 집 근처요~ㅋㅋ",
          "original_form": "아!! 집 근처요~ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:29"
        },
        {
