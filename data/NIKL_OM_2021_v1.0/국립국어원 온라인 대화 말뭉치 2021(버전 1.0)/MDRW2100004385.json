      "utterance": [
        {
          "id": "MDRW2100004385.1.1",
          "form": "게임도 좋아하시나요?",
          "original_form": "게임도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210821 16:17"
        },
        {
          "id": "MDRW2100004385.1.2",
          "form": "전에는 자주했어요",
          "original_form": "전에는 자주했어요",
          "speaker_id": "1",
          "time": "20210821 16:23"
        },
        {
          "id": "MDRW2100004385.1.3",
          "form": "아 저는 게임은 잘 안해서",
          "original_form": "아 저는 게임은 잘 안해서",
          "speaker_id": "2",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004385.1.4",
          "form": "너무 푹빠지는거같아서 요즘은 잘안하구요ㅎ",
          "original_form": "너무 푹빠지는거같아서 요즘은 잘안하구요ㅎ",
          "speaker_id": "1",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004385.1.5",
          "form": "주로 주변사람들이 하는 것 구경하는 편이예요",
          "original_form": "주로 주변사람들이 하는 것 구경하는 편이예요",
          "speaker_id": "2",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004385.1.6",
          "form": "요즘은 뭐가 유명한가요?",
          "original_form": "요즘은 뭐가 유명한가요?",
          "speaker_id": "1",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004385.1.7",
          "form": "맞아요 게임에 빠져들면",
          "original_form": "맞아요 게임에 빠져들면",
          "speaker_id": "2",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004385.1.8",
          "form": "일상이 힘들죠",
          "original_form": "일상이 힘들죠",
          "speaker_id": "2",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004385.1.9",
          "form": "시간이 훅가요",
          "original_form": "시간이 훅가요",
          "speaker_id": "1",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004385.1.10",
          "form": "최근에는 주변인들도 게임을 많이 안해서",
          "original_form": "최근에는 주변인들도 게임을 많이 안해서",
          "speaker_id": "2",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004385.1.11",
          "form": "저도요ㅎ",
          "original_form": "저도요ㅎ",
          "speaker_id": "1",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004385.1.12",
          "form": "요즘 집콕하는 시간이 많으니 게임하다보면 너무 빠져들까봐 다들 자제하는 것 같아요",
          "original_form": "요즘 집콕하는 시간이 많으니 게임하다보면 너무 빠져들까봐 다들 자제하는 것 같아요",
          "speaker_id": "2",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004385.1.13",
          "form": "저도 그래서 안해요ㅎ",
          "original_form": "저도 그래서 안해요ㅎ",
          "speaker_id": "1",
          "time": "20210821 16:49"
        },
        {
          "id": "MDRW2100004385.1.14",
          "form": "저는 온라인 게임보다는 보드게임이 더 좋더라고요 요즘은",
          "original_form": "저는 온라인 게임보다는 보드게임이 더 좋더라고요 요즘은",
