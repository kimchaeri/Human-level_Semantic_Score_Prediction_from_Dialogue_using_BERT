        {
          "id": "MDRW2100035093.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "2",
          "time": "20210829 02:30"
        },
        {
          "id": "MDRW2100035093.1.2",
          "form": "나 빨리 학교 가고싶어",
          "original_form": "나 빨리 학교 가고싶어",
          "speaker_id": "1",
          "time": "20210829 02:30"
        },
        {
          "id": "MDRW2100035093.1.3",
          "form": "개강이 다가오고 있군요",
          "original_form": "개강이 다가오고 있군요",
          "speaker_id": "2",
          "time": "20210829 02:30"
        },
        {
          "id": "MDRW2100035093.1.4",
          "form": "긴 긴 방학이엇어",
          "original_form": "긴 긴 방학이엇어",
          "speaker_id": "1",
          "time": "20210829 02:30"
        },
        {
          "id": "MDRW2100035093.1.5",
          "form": "방학이 길면 좋은 거 아닌가",
          "original_form": "방학이 길면 좋은 거 아닌가",
          "speaker_id": "2",
          "time": "20210829 02:30"
        },
        {
          "id": "MDRW2100035093.1.6",
          "form": "학교에 있는 친구들이 그리워",
          "original_form": "학교에 있는 친구들이 그리워",
          "speaker_id": "1",
          "time": "20210829 02:30"
        },
        {
          "id": "MDRW2100035093.1.7",
          "form": "친구들이 보고 싶구나 하긴",
          "original_form": "친구들이 보고 싶구나 하긴",
          "speaker_id": "2",
          "time": "20210829 02:30"
        },
        {
          "id": "MDRW2100035093.1.8",
          "form": "코로나로 한번도 못만났거든",
          "original_form": "코로나로 한번도 못만났거든",
          "speaker_id": "1",
          "time": "20210829 02:31"
        },
        {
          "id": "MDRW2100035093.1.9",
          "form": "아 그렇구나 아쉽네",
          "original_form": "아 그렇구나 아쉽네",
          "speaker_id": "2",
          "time": "20210829 02:31"
        },
        {
          "id": "MDRW2100035093.1.10",
          "form": "빨리 동아리 활동도 해야지",
          "original_form": "빨리 동아리 활동도 해야지",
          "speaker_id": "1",
          "time": "20210829 02:31"
        },
        {
          "id": "MDRW2100035093.1.11",
          "form": "코로나 인데 대면할 수 있나",
          "original_form": "코로나 인데 대면할 수 있나",
          "speaker_id": "2",
          "time": "20210829 02:31"
        },
        {
          "id": "MDRW2100035093.1.12",
          "form": "아직 한달간은 비대면 확정이야",
          "original_form": "아직 한달간은 비대면 확정이야",
          "speaker_id": "1",
          "time": "20210829 02:31"
        },
        {
          "id": "MDRW2100035093.1.13",
          "form": "빨리 되면 수업했으면 좋겠다",
          "original_form": "빨리 되면 수업했으면 좋겠다",
          "speaker_id": "2",
          "time": "20210829 02:31"
        },
        {
          "id": "MDRW2100035093.1.14",
          "form": "대면",
          "original_form": "대면",
          "speaker_id": "2",
          "time": "20210829 02:31"
        },
        {
          "id": "MDRW2100035093.1.15",
          "form": "맞아 얼른 학교 가고 싶어 정말",
          "original_form": "맞아 얼른 학교 가고 싶어 정말",
