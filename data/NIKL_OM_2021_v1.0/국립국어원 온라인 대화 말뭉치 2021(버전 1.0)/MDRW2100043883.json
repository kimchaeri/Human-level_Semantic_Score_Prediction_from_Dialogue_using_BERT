      "utterance": [
        {
          "id": "MDRW2100043883.1.1",
          "form": "혹시환승연애보시나요",
          "original_form": "혹시환승연애보시나요",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043883.1.2",
          "form": "아니요! 그게뭔가요?",
          "original_form": "아니요! 그게뭔가요?",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043883.1.3",
          "form": "앗ㅎㅎ하트시그널같은예능이에요",
          "original_form": "앗ㅎㅎ하트시그널같은예능이에요",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043883.1.4",
          "form": "아하 챙겨보시나요?",
          "original_form": "아하 챙겨보시나요?",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043883.1.5",
          "form": "ㅎㅎ넹",
          "original_form": "ㅎㅎ넹",
          "speaker_id": "1",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043883.1.6",
          "form": "보면설레요",
          "original_form": "보면설레요",
          "speaker_id": "1",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043883.1.7",
          "form": "앜ㅋㅋㅋㅋ 달달한가봐요",
          "original_form": "앜ㅋㅋㅋㅋ 달달한가봐요",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043883.1.8",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043883.1.9",
          "form": "유투브보세요?",
          "original_form": "유투브보세요?",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043883.1.10",
          "form": "보죠~ 저는 요즘 스트릿우먼파이터를 열심히 봅니드..",
          "original_form": "보죠~ 저는 요즘 스트릿우먼파이터를 열심히 봅니드..",
          "speaker_id": "2",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043883.1.11",
          "form": "어떤건가요",
          "original_form": "어떤건가요",
          "speaker_id": "1",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043883.1.12",
          "form": "여자 댄서분들 나와서 경쟁하는거예요!",
          "original_form": "여자 댄서분들 나와서 경쟁하는거예요!",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043883.1.13",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100043883.1.14",
          "form": "헐들어본것같아요",
          "original_form": "헐들어본것같아요",
          "speaker_id": "1",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100043883.1.15",
          "form": "오디션?프로?",
          "original_form": "오디션?프로?",
          "speaker_id": "1",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100043883.1.16",
          "form": "오디션..이랑 비슷할수있죠 심사위원도 있고?",
          "original_form": "오디션..이랑 비슷할수있죠 심사위원도 있고?",
          "speaker_id": "2",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100043883.1.17",
          "form": "근데 참가자가 전원 프로 댄서에요!",
          "original_form": "근데 참가자가 전원 프로 댄서에요!",
          "speaker_id": "2",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100043883.1.18",
          "form": "와아",
          "original_form": "와아",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043883.1.19",
          "form": "보는재미가있겠어요",
          "original_form": "보는재미가있겠어요",
