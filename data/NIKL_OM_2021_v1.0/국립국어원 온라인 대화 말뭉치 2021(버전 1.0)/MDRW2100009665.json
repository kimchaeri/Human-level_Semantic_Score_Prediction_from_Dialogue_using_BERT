      "utterance": [
        {
          "id": "MDRW2100009665.1.1",
          "form": "기숙사 살아본 적 있어?",
          "original_form": "기숙사 살아본 적 있어?",
          "speaker_id": "1",
          "time": "20210823 02:41"
        },
        {
          "id": "MDRW2100009665.1.2",
          "form": "응 살아본적 있어",
          "original_form": "응 살아본적 있어",
          "speaker_id": "2",
          "time": "20210823 02:49"
        },
        {
          "id": "MDRW2100009665.1.3",
          "form": "기숙사 살때 어땠어?",
          "original_form": "기숙사 살때 어땠어?",
          "speaker_id": "1",
          "time": "20210823 02:50"
        },
        {
          "id": "MDRW2100009665.1.4",
          "form": "기숙사 사는 다른 친구들이 있어서 재밌었어",
          "original_form": "기숙사 사는 다른 친구들이 있어서 재밌었어",
          "speaker_id": "2",
          "time": "20210823 02:50"
        },
        {
          "id": "MDRW2100009665.1.5",
          "form": "그렇구나 힘든 점은 없었어?",
          "original_form": "그렇구나 힘든 점은 없었어?",
          "speaker_id": "1",
          "time": "20210823 02:51"
        },
        {
          "id": "MDRW2100009665.1.6",
          "form": "힘든점도 있긴 했지",
          "original_form": "힘든점도 있긴 했지",
          "speaker_id": "2",
          "time": "20210823 02:53"
        },
        {
          "id": "MDRW2100009665.1.7",
          "form": "맞아 나도 살았었는데 청소하는게 힘들었어",
          "original_form": "맞아 나도 살았었는데 청소하는게 힘들었어",
          "speaker_id": "1",
          "time": "20210823 02:54"
        },
        {
          "id": "MDRW2100009665.1.8",
          "form": "너도 살았었구나",
          "original_form": "너도 살았었구나",
          "speaker_id": "2",
          "time": "20210823 02:55"
        },
        {
          "id": "MDRW2100009665.1.9",
          "form": "응응 1년 살았었어",
          "original_form": "응응 1년 살았었어",
          "speaker_id": "1",
          "time": "20210823 02:56"
        },
        {
          "id": "MDRW2100009665.1.10",
          "form": "몇명이서 같이 살았어?",
          "original_form": "몇명이서 같이 살았어?",
          "speaker_id": "1",
          "time": "20210823 02:56"
        },
        {
          "id": "MDRW2100009665.1.11",
          "form": "나 포함 2명이었어",
          "original_form": "나 포함 2명이었어",
          "speaker_id": "2",
          "time": "20210823 02:58"
        },
        {
          "id": "MDRW2100009665.1.12",
          "form": "넌 몇명이랑 같이 살았어?",
          "original_form": "넌 몇명이랑 같이 살았어?",
          "speaker_id": "2",
          "time": "20210823 02:58"
        },
        {
          "id": "MDRW2100009665.1.13",
          "form": "나도 두명이었어!!",
          "original_form": "나도 두명이었어!!",
          "speaker_id": "1",
          "time": "20210823 02:58"
        },
        {
          "id": "MDRW2100009665.1.14",
          "form": "요즘에는 두명이 많이 사는 것 같아",
          "original_form": "요즘에는 두명이 많이 사는 것 같아",
          "speaker_id": "1",
          "time": "20210823 02:58"
        },
        {
          "id": "MDRW2100009665.1.15",
          "form": "아무래도 4명은 너무 많으니까",
          "original_form": "아무래도 4명은 너무 많으니까",
          "speaker_id": "2",
          "time": "20210823 02:59"
        },
        {
          "id": "MDRW2100009665.1.16",
          "form": "맞아 4명은 너무 불편할 것 같아",
          "original_form": "맞아 4명은 너무 불편할 것 같아",
