      "utterance": [
        {
          "id": "MDRW2100019354.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210827 18:14"
        },
        {
          "id": "MDRW2100019354.1.2",
          "form": "이번여름에 여행은 갔다오셨나요?",
          "original_form": "이번여름에 여행은 갔다오셨나요?",
          "speaker_id": "1",
          "time": "20210827 18:15"
        },
        {
          "id": "MDRW2100019354.1.3",
          "form": "전 내일 고성놀러가용",
          "original_form": "전 내일 고성놀러가용",
          "speaker_id": "2",
          "time": "20210827 18:37"
        },
        {
          "id": "MDRW2100019354.1.4",
          "form": "고성이요?",
          "original_form": "고성이요?",
          "speaker_id": "1",
          "time": "20210827 18:37"
        },
        {
          "id": "MDRW2100019354.1.5",
          "form": "혹시 서핑하러?",
          "original_form": "혹시 서핑하러?",
          "speaker_id": "1",
          "time": "20210827 18:38"
        },
        {
          "id": "MDRW2100019354.1.6",
          "form": "아뇽 그냥 바닷가앞에서 바베큐하러요 ㅎㅎ",
          "original_form": "아뇽 그냥 바닷가앞에서 바베큐하러요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 18:44"
        },
        {
          "id": "MDRW2100019354.1.7",
          "form": "아 멀리 가시네요ㅎ",
          "original_form": "아 멀리 가시네요ㅎ",
          "speaker_id": "1",
          "time": "20210827 18:48"
        },
        {
          "id": "MDRW2100019354.1.8",
          "form": "좋겠다~",
          "original_form": "좋겠다~",
          "speaker_id": "1",
          "time": "20210827 18:48"
        },
        {
          "id": "MDRW2100019354.1.9",
          "form": "친구들이랑?",
          "original_form": "친구들이랑?",
          "speaker_id": "1",
          "time": "20210827 18:49"
        },
        {
          "id": "MDRW2100019354.1.10",
          "form": "아뇨 남편이랑요 ㅎㅎ",
          "original_form": "아뇨 남편이랑요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:08"
        },
        {
          "id": "MDRW2100019354.1.11",
          "form": "아~ 여름휴간데 좀 늦게가시네요?",
          "original_form": "아~ 여름휴간데 좀 늦게가시네요?",
          "speaker_id": "1",
          "time": "20210827 19:15"
        },
        {
          "id": "MDRW2100019354.1.12",
          "form": "아 그건아니구 그냥 주말겸 생일겸 놀러가는거예용 ㅎㅎ",
          "original_form": "아 그건아니구 그냥 주말겸 생일겸 놀러가는거예용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:19"
        },
        {
          "id": "MDRW2100019354.1.13",
          "form": "그렇군요ㅎ 저도 내년엔 고성쪽 가보려구요",
          "original_form": "그렇군요ㅎ 저도 내년엔 고성쪽 가보려구요",
          "speaker_id": "1",
          "time": "20210827 19:36"
        },
        {
          "id": "MDRW2100019354.1.14",
          "form": "요즘 서핑에 빠져서ㅎ",
          "original_form": "요즘 서핑에 빠져서ㅎ",
          "speaker_id": "1",
          "time": "20210827 19:36"
        },
        {
          "id": "MDRW2100019354.1.15",
          "form": "오 멋지네용 잘타세요?",
          "original_form": "오 멋지네용 잘타세요?",
          "speaker_id": "2",
          "time": "20210827 19:36"
        },
        {
          "id": "MDRW2100019354.1.16",
          "form": "아녀ㅋ 서린이에요",
          "original_form": "아녀ㅋ 서린이에요",
          "speaker_id": "1",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019354.1.17",
          "form": "서핑어린이ㅎㅎ",
          "original_form": "서핑어린이ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019354.1.18",
          "form": "배운지 얼마안되서 잘 못타요",
          "original_form": "배운지 얼마안되서 잘 못타요",
          "speaker_id": "1",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019354.1.19",
          "form": "재미있다보니 이번여름에 만리포만 4번간거 같아요ㅎ",
          "original_form": "재미있다보니 이번여름에 만리포만 4번간거 같아요ㅎ",
          "speaker_id": "1",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100019354.1.20",
          "form": "오 대박 ㅎㅎ 제남편은 여기서 수상스키 타용",
          "original_form": "오 대박 ㅎㅎ 제남편은 여기서 수상스키 타용",
          "speaker_id": "2",
          "time": "20210827 19:47"
        },
        {
          "id": "MDRW2100019354.1.21",
          "form": "오 레저. 좋아하시나보다 같이 안타구요?",
          "original_form": "오 레저. 좋아하시나보다 같이 안타구요?",
          "speaker_id": "1",
          "time": "20210827 19:50"
        },
        {
          "id": "MDRW2100019354.1.22",
          "form": "네네 저는 안타용 ㅎㅎ",
          "original_form": "네네 저는 안타용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:56"
        }
      ]
