      "utterance": [
        {
          "id": "MDRW2100012774.1.1",
          "form": "또 드라마 뭐있을까요?",
          "original_form": "또 드라마 뭐있을까요?",
          "speaker_id": "2",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100012774.1.2",
          "form": "예능은 저번에 이야기하셨고ㅎㅎ",
          "original_form": "예능은 저번에 이야기하셨고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100012774.1.3",
          "form": "드라마는 대작이 많은데ㅎ",
          "original_form": "드라마는 대작이 많은데ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100012774.1.4",
          "form": "동백이죠 ㅋ",
          "original_form": "동백이죠 ㅋ",
          "speaker_id": "1",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100012774.1.5",
          "form": "그거 전 아직안봤어요ㅎㅎ",
          "original_form": "그거 전 아직안봤어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:03"
        },
        {
          "id": "MDRW2100012774.1.6",
          "form": "엄청 재밌다면서요ㅎ",
          "original_form": "엄청 재밌다면서요ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:03"
        },
        {
          "id": "MDRW2100012774.1.7",
          "form": "꼭보셔요 ㅋ",
          "original_form": "꼭보셔요 ㅋ",
          "speaker_id": "1",
          "time": "20210825 01:04"
        },
        {
          "id": "MDRW2100012774.1.8",
          "form": "전 주말에 순천 드라마세트장갔다왔는데 동백이 셋트장 있던데요ㅎ",
          "original_form": "전 주말에 순천 드라마세트장갔다왔는데 동백이 셋트장 있던데요ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:04"
        },
        {
          "id": "MDRW2100012774.1.9",
          "form": "저도 드라마 안좋아하는 편이라",
          "original_form": "저도 드라마 안좋아하는 편이라",
          "speaker_id": "1",
          "time": "20210825 01:04"
        },
        {
          "id": "MDRW2100012774.1.10",
          "form": "본게 손에 꼽아요",
          "original_form": "본게 손에 꼽아요",
          "speaker_id": "1",
          "time": "20210825 01:04"
        },
        {
          "id": "MDRW2100012774.1.11",
          "form": "그거 보면 재밌어서... 밤샘할것같네요ㅎㅎ",
          "original_form": "그거 보면 재밌어서... 밤샘할것같네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:04"
        },
        {
          "id": "MDRW2100012774.1.12",
          "form": "시간날때 봐야겟네요ㅎㅎ",
          "original_form": "시간날때 봐야겟네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:04"
        },
        {
          "id": "MDRW2100012774.1.13",
          "form": "공효진 너무 이쁜듯요 ㅎ",
          "original_form": "공효진 너무 이쁜듯요 ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:04"
        },
        {
          "id": "MDRW2100012774.1.14",
          "form": "강하늘이 정말 무대뽀로 나와요 ㅎㅎ",
          "original_form": "강하늘이 정말 무대뽀로 나와요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 01:05"
        },
        {
          "id": "MDRW2100012774.1.15",
          "form": "근데 진짜 소화 잘한 ㅎㅎㅎ",
          "original_form": "근데 진짜 소화 잘한 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 01:05"
        },
        {
          "id": "MDRW2100012774.1.16",
          "form": "미혼모 공효진을 사랑하는 강하늘 대충 스토리  ㅎ",
          "original_form": "미혼모 공효진을 사랑하는 강하늘 대충 스토리  ㅎ",
          "speaker_id": "1",
          "time": "20210825 01:06"
        },
        {
          "id": "MDRW2100012774.1.17",
          "form": "중간중간에 한번본것같애요ㅎ",
          "original_form": "중간중간에 한번본것같애요ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:06"
        },
        {
          "id": "MDRW2100012774.1.18",
          "form": "네네ㅎㅎ",
          "original_form": "네네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:06"
        },
        {
          "id": "MDRW2100012774.1.19",
          "form": "음 근데 전",
          "original_form": "음 근데 전",
          "speaker_id": "1",
          "time": "20210825 01:06"
        },
        {
          "id": "MDRW2100012774.1.20",
          "form": "거기서 김강훈이 참 ㅎㅎ",
          "original_form": "거기서 김강훈이 참 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 01:06"
        },
        {
          "id": "MDRW2100012774.1.21",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:06"
        },
        {
          "id": "MDRW2100012774.1.22",
          "form": "공효진 아들로 나온 아역이요",
          "original_form": "공효진 아들로 나온 아역이요",
          "speaker_id": "1",
          "time": "20210825 01:06"
        },
        {
          "id": "MDRW2100012774.1.23",
          "form": "꼭봐보겟습니당ㅎㅎ",
          "original_form": "꼭봐보겟습니당ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:06"
        },
        {
          "id": "MDRW2100012774.1.24",
          "form": "연기 잘하더라구요 ㅎㅎ",
          "original_form": "연기 잘하더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 01:06"
        },
        {
          "id": "MDRW2100012774.1.25",
          "form": "응팔",
          "original_form": "응팔",
          "speaker_id": "1",
          "time": "20210825 01:07"
        },
        {
          "id": "MDRW2100012774.1.26",
          "form": "오ㅎㅎ 응팔ㅎㅎ",
          "original_form": "오ㅎㅎ 응팔ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:07"
        },
        {
          "id": "MDRW2100012774.1.27",
          "form": "응칠은 또 안봄여  ㅋ",
          "original_form": "응칠은 또 안봄여  ㅋ",
          "speaker_id": "1",
          "time": "20210825 01:07"
        },
        {
          "id": "MDRW2100012774.1.28",
          "form": "응칠... 왜 나쁜말이먀",
          "original_form": "응칠... 왜 나쁜말이먀",
          "speaker_id": "2",
          "time": "20210825 01:07"
        },
        {
          "id": "MDRW2100012774.1.29",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:07"
        },
        {
          "id": "MDRW2100012774.1.30",
          "form": "필터링 ㅋㅋㅋ",
          "original_form": "필터링 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:07"
        },
        {
          "id": "MDRW2100012774.1.31",
          "form": "응팔은 솔직히 제가 살던 때가 아니라ㅎㅎ",
          "original_form": "응팔은 솔직히 제가 살던 때가 아니라ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:08"
        },
        {
          "id": "MDRW2100012774.1.32",
          "form": "덕선이 연기때메 ㅋ",
          "original_form": "덕선이 연기때메 ㅋ",
          "speaker_id": "1",
          "time": "20210825 01:08"
        },
        {
          "id": "MDRW2100012774.1.33",
          "form": "맞아요ㅎㅎㅎ",
          "original_form": "맞아요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:08"
        },
        {
          "id": "MDRW2100012774.1.34",
          "form": "덕선이ㅎㅎ 성동일하고 이일화ㅎㅎㅎ",
          "original_form": "덕선이ㅎㅎ 성동일하고 이일화ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:09"
        },
        {
          "id": "MDRW2100012774.1.35",
          "form": "응칠은 분명 내세대 이긴 한데",
          "original_form": "응칠은 분명 내세대 이긴 한데",
          "speaker_id": "1",
          "time": "20210825 01:09"
        },
        {
          "id": "MDRW2100012774.1.36",
          "form": "글죠ㅎ",
          "original_form": "글죠ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:09"
        },
        {
          "id": "MDRW2100012774.1.37",
          "form": "뭔가 재메가ㅜ없더라구여",
          "original_form": "뭔가 재메가ㅜ없더라구여",
          "speaker_id": "1",
          "time": "20210825 01:09"
        },
        {
          "id": "MDRW2100012774.1.38",
          "form": "근데 응팔에비하면...",
          "original_form": "근데 응팔에비하면...",
          "speaker_id": "2",
          "time": "20210825 01:09"
        },
        {
          "id": "MDRW2100012774.1.39",
          "form": "재미없어요ㅎㅎ",
          "original_form": "재미없어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:09"
        },
        {
          "id": "MDRW2100012774.1.40",
          "form": "슬생 뻬곤",
          "original_form": "슬생 뻬곤",
          "speaker_id": "1",
          "time": "20210825 01:09"
        },
        {
          "id": "MDRW2100012774.1.41",
          "form": "드라만 끝 ㅋ",
          "original_form": "드라만 끝 ㅋ",
          "speaker_id": "1",
          "time": "20210825 01:09"
        },
        {
          "id": "MDRW2100012774.1.42",
          "form": "아마 스토리는 응칠이 엄청 전개도 빠르고ㄹ",
          "original_form": "아마 스토리는 응칠이 엄청 전개도 빠르고ㄹ",
          "speaker_id": "2",
          "time": "20210825 01:09"
        },
        {
          "id": "MDRW2100012774.1.43",
          "form": "ㅎㅎ 그럼 유튜브는 보세요?",
          "original_form": "ㅎㅎ 그럼 유튜브는 보세요?",
          "speaker_id": "2",
          "time": "20210825 01:10"
        },
        {
          "id": "MDRW2100012774.1.44",
          "form": "저도 안보긴하는데ㅎ 이벤트나 가끔 둘러보고 코인 정도요~",
          "original_form": "저도 안보긴하는데ㅎ 이벤트나 가끔 둘러보고 코인 정도요~",
          "speaker_id": "2",
          "time": "20210825 01:10"
        }
      ]
