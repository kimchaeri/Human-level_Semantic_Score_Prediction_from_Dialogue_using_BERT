      "utterance": [
        {
          "id": "MDRW2100023650.1.1",
          "form": "그니까 맛집은 달라",
          "original_form": "그니까 맛집은 달라",
          "speaker_id": "2",
          "time": "20210830 13:44"
        },
        {
          "id": "MDRW2100023650.1.2",
          "form": "맞아 괜히 맛집이 아니야",
          "original_form": "맞아 괜히 맛집이 아니야",
          "speaker_id": "1",
          "time": "20210830 13:45"
        },
        {
          "id": "MDRW2100023650.1.3",
          "form": "저녁은 뭐 먹을거야?",
          "original_form": "저녁은 뭐 먹을거야?",
          "speaker_id": "2",
          "time": "20210830 13:45"
        },
        {
          "id": "MDRW2100023650.1.4",
          "form": "어제 치킨먹어서 그거 남은거 먹을듯? 너는?",
          "original_form": "어제 치킨먹어서 그거 남은거 먹을듯? 너는?",
          "speaker_id": "1",
          "time": "20210830 13:46"
        },
        {
          "id": "MDRW2100023650.1.5",
          "form": "무슨 치킨먹었어?",
          "original_form": "무슨 치킨먹었어?",
          "speaker_id": "2",
          "time": "20210830 13:46"
        },
        {
          "id": "MDRW2100023650.1.6",
          "form": "처갓집! 핫슈림프 첨먹었는데 괜찮더라?",
          "original_form": "처갓집! 핫슈림프 첨먹었는데 괜찮더라?",
          "speaker_id": "1",
          "time": "20210830 13:46"
        },
        {
          "id": "MDRW2100023650.1.7",
          "form": "어제도 배민으로 시켰어?\n누가 내꺼 로그인했던데?ㅋㅋㅋ",
          "original_form": "어제도 배민으로 시켰어?\n누가 내꺼 로그인했던데?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:47"
        },
        {
          "id": "MDRW2100023650.1.8",
          "form": "아 로그인 알림가?ㅋㅋㅋ 쿠폰 얼마인가 볼라고ㅋㅋㅋㅋㅋ",
          "original_form": "아 로그인 알림가?ㅋㅋㅋ 쿠폰 얼마인가 볼라고ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:47"
        },
        {
          "id": "MDRW2100023650.1.9",
          "form": "근데 처갓집 자체 쿠폰있어서 그걸로 했어ㅋㅋ",
          "original_form": "근데 처갓집 자체 쿠폰있어서 그걸로 했어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:47"
        },
        {
          "id": "MDRW2100023650.1.10",
          "form": "핫슈프림! 맛있겠다 그냥 슈프림이랑 다른가?",
          "original_form": "핫슈프림! 맛있겠다 그냥 슈프림이랑 다른가?",
          "speaker_id": "2",
          "time": "20210830 13:47"
        },
        {
          "id": "MDRW2100023650.1.11",
          "form": "응 오더라ㅋㅋㄱ오호! 딱이네",
          "original_form": "응 오더라ㅋㅋㄱ오호! 딱이네",
          "speaker_id": "2",
          "time": "20210830 13:47"
        },
        {
          "id": "MDRW2100023650.1.12",
          "form": "응 양념이 매워! 근데 난 핫양념이 그 특유의 맛이 있거든? 캡사이신",
          "original_form": "응 양념이 매워! 근데 난 핫양념이 그 특유의 맛이 있거든? 캡사이신",
          "speaker_id": "1",
          "time": "20210830 13:47"
        },
        {
          "id": "MDRW2100023650.1.13",
          "form": "응!",
          "original_form": "응!",
          "speaker_id": "2",
          "time": "20210830 13:48"
        },
        {
          "id": "MDRW2100023650.1.14",
          "form": "그게 맛없는데 슈프림양념이 있어서 나쁘지않았어",
          "original_form": "그게 맛없는데 슈프림양념이 있어서 나쁘지않았어",
          "speaker_id": "1",
          "time": "20210830 13:48"
        },
        {
          "id": "MDRW2100023650.1.15",
          "form": "그냥 양념에 슈프림하면 느끼해서 얼마 못먹거든",
          "original_form": "그냥 양념에 슈프림하면 느끼해서 얼마 못먹거든",
          "speaker_id": "1",
          "time": "20210830 13:48"
        },
        {
          "id": "MDRW2100023650.1.16",
          "form": "마자 슈프림 느끼하더라",
          "original_form": "마자 슈프림 느끼하더라",
          "speaker_id": "2",
          "time": "20210830 13:48"
        },
        {
          "id": "MDRW2100023650.1.17",
          "form": "핫은 안느끼하니까 매운거 느끼한거",
          "original_form": "핫은 안느끼하니까 매운거 느끼한거",
          "speaker_id": "1",
          "time": "20210830 13:48"
        }
