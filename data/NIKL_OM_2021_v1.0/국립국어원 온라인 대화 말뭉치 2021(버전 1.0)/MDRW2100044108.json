        },
        {
          "id": "MDRW2100044108.1.2",
          "form": "유튜브 보세요?",
          "original_form": "유튜브 보세요?",
          "speaker_id": "1",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044108.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044108.1.4",
          "form": "저는 티빙 봐요",
          "original_form": "저는 티빙 봐요",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044108.1.5",
          "form": "유튜브 보시나요?",
          "original_form": "유튜브 보시나요?",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044108.1.6",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044108.1.7",
          "form": "티빙 뭐보새요?",
          "original_form": "티빙 뭐보새요?",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044108.1.8",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044108.1.9",
          "form": "유튭봐요ㅎㅎ",
          "original_form": "유튭봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044108.1.10",
          "form": "한창 알쓸범잡보고",
          "original_form": "한창 알쓸범잡보고",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044108.1.11",
          "form": "꼬꼬무도 봤어요",
          "original_form": "꼬꼬무도 봤어요",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044108.1.12",
          "form": "아 재밌죠",
          "original_form": "아 재밌죠",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044108.1.13",
          "form": "유튜브에서 어떤거 보세요?",
          "original_form": "유튜브에서 어떤거 보세요?",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044108.1.14",
          "form": "박지선교수님 멋있어요",
          "original_form": "박지선교수님 멋있어요",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044108.1.15",
          "form": "저는 5분순삳..",
          "original_form": "저는 5분순삳..",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044108.1.16",
          "form": "이런거요",
          "original_form": "이런거요",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044108.1.17",
          "form": "드라마 요약프로인가요?",
          "original_form": "드라마 요약프로인가요?",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044108.1.18",
          "form": "아니요 예능이요ㅎㅎ",
          "original_form": "아니요 예능이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044108.1.19",
          "form": "어떤 예능인가요?",
          "original_form": "어떤 예능인가요?",
          "speaker_id": "2",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044108.1.20",
          "form": "런닝맨",
          "original_form": "런닝맨",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044108.1.21",
          "form": "무도",
          "original_form": "무도",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044108.1.22",
          "form": "이런거요ㅎㅎ",
          "original_form": "이런거요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044108.1.23",
          "form": "짧게 재밌는 부분만 보는거예요",
          "original_form": "짧게 재밌는 부분만 보는거예요",
          "speaker_id": "1",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044108.1.24",
          "form": "ㅇㅎ 실패하지 않는 조합이네요 ㅋㅋㅋ",
          "original_form": "ㅇㅎ 실패하지 않는 조합이네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044108.1.25",
          "form": "그죠",
          "original_form": "그죠",
          "speaker_id": "1",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044108.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044108.1.27",
          "form": "재밌어요",
          "original_form": "재밌어요",
          "speaker_id": "1",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044108.1.28",
          "form": "최근에 알쓸범잡 어떤거 보셨어요?",
          "original_form": "최근에 알쓸범잡 어떤거 보셨어요?",
          "speaker_id": "1",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044108.1.29",
          "form": "저 위조지폐편이요 ㅋㅋㅋ",
          "original_form": "저 위조지폐편이요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044108.1.30",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044108.1.31",
          "form": "천원짜리는 단가가 안나와서 오천원짜리 위조했다던 ㅋㅋㅋ",
          "original_form": "천원짜리는 단가가 안나와서 오천원짜리 위조했다던 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044108.1.32",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044108.1.33",
          "form": "그거는 못본거네요",
          "original_form": "그거는 못본거네요",
