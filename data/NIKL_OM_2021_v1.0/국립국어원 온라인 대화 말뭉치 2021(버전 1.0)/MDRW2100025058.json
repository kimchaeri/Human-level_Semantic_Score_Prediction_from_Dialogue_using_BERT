      "utterance": [
        {
          "id": "MDRW2100025058.1.1",
          "form": "집안일하시는 거 좋아하시나요??",
          "original_form": "집안일하시는 거 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210901 15:25"
        },
        {
          "id": "MDRW2100025058.1.2",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "2",
          "time": "20210901 15:39"
        },
        {
          "id": "MDRW2100025058.1.3",
          "form": "그래도 개중에 나은건 있어요 ㅋㅋ",
          "original_form": "그래도 개중에 나은건 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:39"
        },
        {
          "id": "MDRW2100025058.1.4",
          "form": "어떤거요??",
          "original_form": "어떤거요??",
          "speaker_id": "1",
          "time": "20210901 15:43"
        },
        {
          "id": "MDRW2100025058.1.5",
          "form": "설거지요 ㅋㅋ",
          "original_form": "설거지요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:03"
        },
        {
          "id": "MDRW2100025058.1.6",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210901 16:03"
        },
        {
          "id": "MDRW2100025058.1.7",
          "form": "저는 웬만한 집안일은 다 좋아해요! 적성에 맞나봐요 ㅋㅋㅋ",
          "original_form": "저는 웬만한 집안일은 다 좋아해요! 적성에 맞나봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:05"
        },
        {
          "id": "MDRW2100025058.1.8",
          "form": "오와 ㅋㅋㅋㅋ",
          "original_form": "오와 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:12"
        },
        {
          "id": "MDRW2100025058.1.9",
          "form": "저는 그 기다려야되는 빨래 같은게 귀찮아요",
          "original_form": "저는 그 기다려야되는 빨래 같은게 귀찮아요",
          "speaker_id": "2",
          "time": "20210901 16:12"
        },
        {
          "id": "MDRW2100025058.1.10",
          "form": "설거지처럼 바로 해치우는게 좋아요 ㅋㅋㅋ",
          "original_form": "설거지처럼 바로 해치우는게 좋아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:13"
        },
        {
          "id": "MDRW2100025058.1.11",
          "form": "엇 그러면 빨래 돌리는동안 설거지나 방청소하시는거 추천해요!",
          "original_form": "엇 그러면 빨래 돌리는동안 설거지나 방청소하시는거 추천해요!",
          "speaker_id": "1",
          "time": "20210901 16:26"
        },
        {
          "id": "MDRW2100025058.1.12",
          "form": "저도 그렇게 하고 있어요ㅋㅋㅋ",
          "original_form": "저도 그렇게 하고 있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:26"
        },
        {
          "id": "MDRW2100025058.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100025058.1.14",
          "form": "그러네요",
          "original_form": "그러네요",
          "speaker_id": "2",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100025058.1.15",
          "form": "네버엔딩집안일이에요",
          "original_form": "네버엔딩집안일이에요",
          "speaker_id": "2",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100025058.1.16",
          "form": "그니까요ㅠ",
          "original_form": "그니까요ㅠ",
          "speaker_id": "1",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100025058.1.17",
          "form": "음쓰가 제일 싫어요",
          "original_form": "음쓰가 제일 싫어요",
          "speaker_id": "1",
          "time": "20210901 17:16"
        },
        {
          "id": "MDRW2100025058.1.18",
          "form": "맞아요 정말",
          "original_form": "맞아요 정말",
          "speaker_id": "2",
          "time": "20210901 17:38"
        },
        {
          "id": "MDRW2100025058.1.19",
          "form": "음식물처리기 하나 사고싶어요",
          "original_form": "음식물처리기 하나 사고싶어요",
          "speaker_id": "2",
          "time": "20210901 17:38"
        },
        {
          "id": "MDRW2100025058.1.20",
          "form": "아 음식물처리기 진짜요!!",
          "original_form": "아 음식물처리기 진짜요!!",
          "speaker_id": "1",
          "time": "20210901 17:40"
        },
        {
          "id": "MDRW2100025058.1.21",
          "form": "스타일러도 사고싶구요ㅠ",
          "original_form": "스타일러도 사고싶구요ㅠ",
          "speaker_id": "1",
          "time": "20210901 17:41"
        },
        {
          "id": "MDRW2100025058.1.22",
          "form": "ㅎㅎ모든걸 갈아버림 짱편할거같아오",
          "original_form": "ㅎㅎ모든걸 갈아버림 짱편할거같아오",
          "speaker_id": "2",
          "time": "20210901 17:46"
        },
        {
          "id": "MDRW2100025058.1.23",
          "form": "집에서는 주로 어떤 활동하시나요??",
          "original_form": "집에서는 주로 어떤 활동하시나요??",
          "speaker_id": "1",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100025058.1.24",
          "form": "퇴근하고나면 운동하거나 공부요!",
          "original_form": "퇴근하고나면 운동하거나 공부요!",
