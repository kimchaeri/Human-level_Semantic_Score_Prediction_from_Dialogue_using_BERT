      "utterance": [
        {
          "id": "MDRW2100019822.1.1",
          "form": "닉네임이 name2시네여..",
          "original_form": "닉네임이 &name2&시네여..",
          "speaker_id": "1",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100019822.1.2",
          "form": "로제 넘 맛있죠.....",
          "original_form": "로제 넘 맛있죠.....",
          "speaker_id": "1",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100019822.1.3",
          "form": "ㅋㅋㅋㅋ 네 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 네 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019822.1.4",
          "form": "어디거좋아하세요",
          "original_form": "어디거좋아하세요",
          "speaker_id": "2",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019822.1.5",
          "form": "저는 삼첩분식이요",
          "original_form": "저는 삼첩분식이요",
          "speaker_id": "1",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019822.1.6",
          "form": "크흐 넘 맛있어요",
          "original_form": "크흐 넘 맛있어요",
          "speaker_id": "1",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019822.1.7",
          "form": "어디거좋아하세여",
          "original_form": "어디거좋아하세여",
          "speaker_id": "1",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019822.1.8",
          "form": "거기 사이드가 최고에요",
          "original_form": "거기 사이드가 최고에요",
          "speaker_id": "2",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019822.1.9",
          "form": "저는 신전이요!",
          "original_form": "저는 신전이요!",
          "speaker_id": "2",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019822.1.10",
          "form": "신전로제는",
          "original_form": "신전로제는",
          "speaker_id": "1",
          "time": "20210827 22:27"
        },
        {
          "id": "MDRW2100019822.1.11",
          "form": "점포 복불복이 심하대서",
          "original_form": "점포 복불복이 심하대서",
          "speaker_id": "1",
          "time": "20210827 22:27"
        },
        {
          "id": "MDRW2100019822.1.12",
          "form": "아직 못먹어봤어요ㅠㅠ",
          "original_form": "아직 못먹어봤어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 22:27"
        },
        {
          "id": "MDRW2100019822.1.13",
          "form": "아 그렇단 말이있던데다행히",
          "original_form": "아 그렇단 말이있던데다행히",
          "speaker_id": "2",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019822.1.14",
          "form": "저희동네는 다 맛있더라구요",
          "original_form": "저희동네는 다 맛있더라구요",
          "speaker_id": "2",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019822.1.15",
          "form": "도전해보세요!!",
          "original_form": "도전해보세요!!",
          "speaker_id": "2",
          "time": "20210827 22:29"
        },
        {
          "id": "MDRW2100019822.1.16",
          "form": "후후 조만간 그래야겠어요",
          "original_form": "후후 조만간 그래야겠어요",
          "speaker_id": "1",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100019822.1.17",
          "form": "요즘 태리로제 거기도 맛있다던데여!",
          "original_form": "요즘 태리로제 거기도 맛있다던데여!",
          "speaker_id": "1",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100019822.1.18",
          "form": "다 그러더라구요! 전 안먹어봤어요 ㅠㅜ",
          "original_form": "다 그러더라구요! 전 안먹어봤어요 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100019822.1.19",
          "form": "닉값 하셔야져!!",
          "original_form": "닉값 하셔야져!!",
          "speaker_id": "1",
          "time": "20210827 22:40"
        },
        {
          "id": "MDRW2100019822.1.20",
          "form": "얼른 먹어보세영!",
          "original_form": "얼른 먹어보세영!",
          "speaker_id": "1",
          "time": "20210827 22:40"
        },
        {
          "id": "MDRW2100019822.1.21",
          "form": "하 분발해야겠네요 반성합니다..",
          "original_form": "하 분발해야겠네요 반성합니다..",
          "speaker_id": "2",
          "time": "20210827 22:40"
        },
        {
          "id": "MDRW2100019822.1.22",
          "form": "거긴 파스타사리도 된다더라구여",
          "original_form": "거긴 파스타사리도 된다더라구여",
          "speaker_id": "2",
          "time": "20210827 22:41"
        },
        {
          "id": "MDRW2100019822.1.23",
          "form": "맞아여 근데 거기",
          "original_form": "맞아여 근데 거기",
          "speaker_id": "1",
          "time": "20210827 22:41"
        },
        {
          "id": "MDRW2100019822.1.24",
          "form": "투움바떡볶이랑",
          "original_form": "투움바떡볶이랑",
          "speaker_id": "1",
          "time": "20210827 22:41"
        },
        {
          "id": "MDRW2100019822.1.25",
          "form": "로제떡볶이가 맛차이가 별로 안 난다고 하더라구요...",
          "original_form": "로제떡볶이가 맛차이가 별로 안 난다고 하더라구요...",
          "speaker_id": "1",
          "time": "20210827 22:41"
        },
        {
          "id": "MDRW2100019822.1.26",
          "form": "그리고 응떡로제는 진짜 별로예요...",
          "original_form": "그리고 응떡로제는 진짜 별로예요...",
          "speaker_id": "1",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019822.1.27",
          "form": "헐그래요??",
          "original_form": "헐그래요??",
          "speaker_id": "2",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019822.1.28",
          "form": "투움바랑 차이가없다니 흠...",
          "original_form": "투움바랑 차이가없다니 흠...",
          "speaker_id": "2",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019822.1.29",
          "form": "응떡도 있는지 첨 알았는데 별로라니...ㅠㅠ",
          "original_form": "응떡도 있는지 첨 알았는데 별로라니...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019822.1.30",
          "form": "저는 심지어 응떡이 떡볶이 최애인데도 그랫어요..",
          "original_form": "저는 심지어 응떡이 떡볶이 최애인데도 그랫어요..",
          "speaker_id": "1",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100019822.1.31",
          "form": "헐 ㅠㅠ 오리지널은 맛있는데 로제는 못하나봐요",
          "original_form": "헐 ㅠㅠ 오리지널은 맛있는데 로제는 못하나봐요",
          "speaker_id": "2",
          "time": "20210827 22:45"
        }
      ]
