      "utterance": [
        {
          "id": "MDRW2100045121.1.1",
          "form": "막 집에 거기 케이크 포장도해갔어 왜이래",
          "original_form": "막 집에 거기 케이크 포장도해갔어 왜이래",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045121.1.2",
          "form": "나는 아니었나봐 설마",
          "original_form": "나는 아니었나봐 설마",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045121.1.3",
          "form": "아니야..  너도포장해갔어..",
          "original_form": "아니야..  너도포장해갔어..",
          "speaker_id": "1",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045121.1.4",
          "form": "나중에 친구들에게 기억하는지 물어봐야겠다",
          "original_form": "나중에 친구들에게 기억하는지 물어봐야겠다",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045121.1.5",
          "form": "나만이럴순없어",
          "original_form": "나만이럴순없어",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045121.1.6",
          "form": "넌좀 많은걸기억못하는데 라멘집은 기억남?",
          "original_form": "넌좀 많은걸기억못하는데 라멘집은 기억남?",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045121.1.7",
          "form": "라멘집이 있었어?",
          "original_form": "라멘집이 있었어?",
          "speaker_id": "2",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045121.1.8",
          "form": "아 먹었던것같기도하고",
          "original_form": "아 먹었던것같기도하고",
          "speaker_id": "2",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045121.1.9",
          "form": "아 왜이래 ㅋㅋㅋㅋㅋ 막학년에 종종먹음",
          "original_form": "아 왜이래 ㅋㅋㅋㅋㅋ 막학년에 종종먹음",
          "speaker_id": "1",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045121.1.10",
          "form": "그니까 언니가 매워했던것도같고",
          "original_form": "그니까 언니가 매워했던것도같고",
          "speaker_id": "2",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045121.1.11",
          "form": "어 맞어 언니 맨날 제일 순한맛",
          "original_form": "어 맞어 언니 맨날 제일 순한맛",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045121.1.12",
          "form": "내가 먹었으면 너네랑 먹었겠지 그치응응",
          "original_form": "내가 먹었으면 너네랑 먹었겠지 그치응응",
          "speaker_id": "2",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045121.1.13",
          "form": "먹었다는것만 기억남ㅋㅋ",
          "original_form": "먹었다는것만 기억남ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045121.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 라멘도 두집이었어",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 라멘도 두집이었어",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045121.1.15",
          "form": "양 끝과 끝에",
          "original_form": "양 끝과 끝에",
          "speaker_id": "1",
          "time": "20210908 00:58"
        },
        {
          "id": "MDRW2100045121.1.16",
          "form": "한곳은 탄탄멘 위주 한곳은 소유라멘 미소라멘 위주 ㅋㅋ",
          "original_form": "한곳은 탄탄멘 위주 한곳은 소유라멘 미소라멘 위주 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045121.1.17",
          "form": "너 혹시 로봇이니",
          "original_form": "너 혹시 로봇이니",
          "speaker_id": "2",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045121.1.18",
          "form": "어쩜 이런걸 기억하지",
          "original_form": "어쩜 이런걸 기억하지",
          "speaker_id": "2",
          "time": "20210908 01:00"
