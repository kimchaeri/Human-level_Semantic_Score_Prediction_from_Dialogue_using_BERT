      "utterance": [
        {
          "id": "MDRW2100025990.1.1",
          "form": "오늘 뭐 드셨어요?",
          "original_form": "오늘 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100025990.1.2",
          "form": "점심은 마라탕 먹었어요",
          "original_form": "점심은 마라탕 먹었어요",
          "speaker_id": "1",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025990.1.3",
          "form": "점심 뭐드셨어요?",
          "original_form": "점심 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025990.1.4",
          "form": "오늘 라면에 햇반요!",
          "original_form": "오늘 라면에 햇반요!",
          "speaker_id": "2",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025990.1.5",
          "form": "진매",
          "original_form": "진매",
          "speaker_id": "2",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025990.1.6",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025990.1.7",
          "form": "혼자지내니 배달음식 시키기가 어려워요",
          "original_form": "혼자지내니 배달음식 시키기가 어려워요",
          "speaker_id": "2",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025990.1.8",
          "form": "진매 맛있죠",
          "original_form": "진매 맛있죠",
          "speaker_id": "1",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100025990.1.9",
          "form": "혼자 사시면 그럴 거 같아요...",
          "original_form": "혼자 사시면 그럴 거 같아요...",
          "speaker_id": "1",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100025990.1.10",
          "form": "최소 금액 때문에 조금은 배달이 안 되니까...",
          "original_form": "최소 금액 때문에 조금은 배달이 안 되니까...",
          "speaker_id": "1",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100025990.1.11",
          "form": "저녁은 뭐가 좋을까 고민중입니당",
          "original_form": "저녁은 뭐가 좋을까 고민중입니당",
          "speaker_id": "2",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100025990.1.12",
          "form": "저는 미역국이랑 먹으려고요",
          "original_form": "저는 미역국이랑 먹으려고요",
          "speaker_id": "1",
          "time": "20210901 18:45"
        },
        {
          "id": "MDRW2100025990.1.13",
          "form": "우왕 맛있겠어요",
          "original_form": "우왕 맛있겠어요",
          "speaker_id": "2",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100025990.1.14",
          "form": "저는 3가지팩 만원짜리 반찬으로 때워야겠어요",
          "original_form": "저는 3가지팩 만원짜리 반찬으로 때워야겠어요",
          "speaker_id": "2",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100025990.1.15",
          "form": "반찬 가게에서 사오시는 건가요?",
          "original_form": "반찬 가게에서 사오시는 건가요?",
          "speaker_id": "1",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100025990.1.16",
          "form": "네 근처에 제주 킹마트라고 웃긴 이름의 마트가 있어서",
          "original_form": "네 근처에 제주 킹마트라고 웃긴 이름의 마트가 있어서",
          "speaker_id": "2",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100025990.1.17",
          "form": "거기서 사와요",
          "original_form": "거기서 사와요",
          "speaker_id": "2",
          "time": "20210901 18:47"
        },
        {
          "id": "MDRW2100025990.1.18",
          "form": "주로 어떤 반찬 사오세요?",
          "original_form": "주로 어떤 반찬 사오세요?",
          "speaker_id": "1",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100025990.1.19",
          "form": "흠 메추리알 장조림 일순위구요",
          "original_form": "흠 메추리알 장조림 일순위구요",
          "speaker_id": "2",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100025990.1.20",
          "form": "나머지는 랜덤이예여",
          "original_form": "나머지는 랜덤이예여",
          "speaker_id": "2",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100025990.1.21",
          "form": "장조림 너무 맛있죠!",
          "original_form": "장조림 너무 맛있죠!",
          "speaker_id": "1",
          "time": "20210901 18:50"
        }
      ]
