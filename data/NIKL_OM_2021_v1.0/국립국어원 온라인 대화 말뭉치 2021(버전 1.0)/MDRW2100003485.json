      "utterance": [
        {
          "id": "MDRW2100003485.1.1",
          "form": "너 근데 학교에서 요즘에 머해?",
          "original_form": "너 근데 학교에서 요즘에 머해?",
          "speaker_id": "1",
          "time": "20210825 08:26"
        },
        {
          "id": "MDRW2100003485.1.2",
          "form": "2학기엔 할것두 없자나",
          "original_form": "2학기엔 할것두 없자나",
          "speaker_id": "1",
          "time": "20210825 08:26"
        },
        {
          "id": "MDRW2100003485.1.3",
          "form": "글킨하지...",
          "original_form": "글킨하지...",
          "speaker_id": "2",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100003485.1.4",
          "form": "그래서할게없어서 탭들고가",
          "original_form": "그래서할게없어서 탭들고가",
          "speaker_id": "2",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100003485.1.5",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100003485.1.6",
          "form": "양띵 볼라고?",
          "original_form": "양띵 볼라고?",
          "speaker_id": "1",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100003485.1.7",
          "form": "엉 언니야가보라매",
          "original_form": "엉 언니야가보라매",
          "speaker_id": "2",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100003485.1.8",
          "form": "그거보고  또",
          "original_form": "그거보고  또",
          "speaker_id": "2",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100003485.1.9",
          "form": "수능공부하거..그러자0",
          "original_form": "수능공부하거..그러자0",
          "speaker_id": "2",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100003485.1.10",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100003485.1.11",
          "form": "흑흑 파이팅",
          "original_form": "흑흑 파이팅",
          "speaker_id": "1",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100003485.1.12",
          "form": "할수잇다 아자아잣",
          "original_form": "할수잇다 아자아잣",
          "speaker_id": "1",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100003485.1.13",
          "form": "고맙슈",
          "original_form": "고맙슈",
          "speaker_id": "2",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100003485.1.14",
          "form": "나도얼른 끝내고싶다",
          "original_form": "나도얼른 끝내고싶다",
          "speaker_id": "2",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100003485.1.15",
          "form": "할수잇다잉",
          "original_form": "할수잇다잉",
          "speaker_id": "1",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100003485.1.16",
          "form": "꾸준히만 한다믄",
          "original_form": "꾸준히만 한다믄",
          "speaker_id": "1",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100003485.1.17",
          "form": "안될게업다",
          "original_form": "안될게업다",
          "speaker_id": "1",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100003485.1.18",
          "form": "헤헷마자",
          "original_form": "헤헷마자",
          "speaker_id": "2",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100003485.1.19",
          "form": "나도 열시미 국어공부해야지",
          "original_form": "나도 열시미 국어공부해야지",
          "speaker_id": "2",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100003485.1.20",
          "form": "국어가 문법이 너무어랴워",
          "original_form": "국어가 문법이 너무어랴워",
          "speaker_id": "2",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100003485.1.21",
          "form": "문법은 해도해도",
          "original_form": "문법은 해도해도",
          "speaker_id": "1",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100003485.1.22",
          "form": "까먹는게 문법이어ㅠ",
          "original_form": "까먹는게 문법이어ㅠ",
          "speaker_id": "1",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100003485.1.23",
          "form": "나도 너무싫엇어",
          "original_form": "나도 너무싫엇어",
          "speaker_id": "1",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100003485.1.24",
          "form": "흐흑....",
          "original_form": "흐흑....",
          "speaker_id": "2",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100003485.1.25",
