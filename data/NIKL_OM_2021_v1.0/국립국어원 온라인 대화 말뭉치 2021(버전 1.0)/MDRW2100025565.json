        {
          "id": "MDRW2100025565.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 13:30"
        },
        {
          "id": "MDRW2100025565.1.2",
          "form": "맛점하셨나요?",
          "original_form": "맛점하셨나요?",
          "speaker_id": "2",
          "time": "20210901 13:31"
        },
        {
          "id": "MDRW2100025565.1.3",
          "form": "네 삼계탕 먹었습니다",
          "original_form": "네 삼계탕 먹었습니다",
          "speaker_id": "1",
          "time": "20210901 13:53"
        },
        {
          "id": "MDRW2100025565.1.4",
          "form": "날씨랑딱이네요",
          "original_form": "날씨랑딱이네요",
          "speaker_id": "2",
          "time": "20210901 14:03"
        },
        {
          "id": "MDRW2100025565.1.5",
          "form": "전 커피랑 번하고 앙버터 먹었더니 배부른데 헛배부른 느낌이네요",
          "original_form": "전 커피랑 번하고 앙버터 먹었더니 배부른데 헛배부른 느낌이네요",
          "speaker_id": "2",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100025565.1.6",
          "form": "뭔가 많~이 드셨는데 ㅎㅎ",
          "original_form": "뭔가 많~이 드셨는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 14:11"
        },
        {
          "id": "MDRW2100025565.1.7",
          "form": "죽기전에 먹고싶을 정도로 좋아하는 음식있나요?",
          "original_form": "죽기전에 먹고싶을 정도로 좋아하는 음식있나요?",
          "speaker_id": "2",
          "time": "20210901 16:07"
        },
        {
          "id": "MDRW2100025565.1.8",
          "form": "죽기 전에 연어회요",
          "original_form": "죽기 전에 연어회요",
          "speaker_id": "1",
          "time": "20210901 16:41"
        },
        {
          "id": "MDRW2100025565.1.9",
          "form": "연어파티할거에요",
          "original_form": "연어파티할거에요",
          "speaker_id": "1",
          "time": "20210901 16:41"
        },
        {
          "id": "MDRW2100025565.1.10",
          "form": "연어파티 진정 파뤼피플이시군요",
          "original_form": "연어파티 진정 파뤼피플이시군요",
          "speaker_id": "2",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100025565.1.11",
          "form": "연어무제한 좋아하시겠어요?",
          "original_form": "연어무제한 좋아하시겠어요?",
          "speaker_id": "2",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100025565.1.12",
          "form": "네 ㅋㅋ 근데 또 막상 그럼",
          "original_form": "네 ㅋㅋ 근데 또 막상 그럼",
          "speaker_id": "1",
          "time": "20210901 16:53"
        },
        {
          "id": "MDRW2100025565.1.13",
          "form": "많이 못먹습니다",
          "original_form": "많이 못먹습니다",
          "speaker_id": "1",
          "time": "20210901 16:53"
        },
        {
          "id": "MDRW2100025565.1.14",
          "form": "어릴땐 무제한 이나 뷔폐 좋아했는데",
          "original_form": "어릴땐 무제한 이나 뷔폐 좋아했는데",
          "speaker_id": "2",
          "time": "20210901 17:30"
        },
        {
          "id": "MDRW2100025565.1.15",
          "form": "요즘엔 양껏못먹겠더라구요",
          "original_form": "요즘엔 양껏못먹겠더라구요",
          "speaker_id": "2",
          "time": "20210901 17:30"
        },
        {
          "id": "MDRW2100025565.1.16",
          "form": "맞아요 어릴때나 먹었죠 ㅠㅠ",
          "original_form": "맞아요 어릴때나 먹었죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 18:07"
        },
        {
          "id": "MDRW2100025565.1.17",
          "form": "걍 단품시키는게 더 이득",
          "original_form": "걍 단품시키는게 더 이득",
          "speaker_id": "2",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100025565.1.18",
          "form": "맞아요 막 하나씩 얼마 계산하는데 ㅋㅋㅋ",
          "original_form": "맞아요 막 하나씩 얼마 계산하는데 ㅋㅋㅋ",
