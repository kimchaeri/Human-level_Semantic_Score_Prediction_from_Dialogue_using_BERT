      "utterance": [
        {
          "id": "MDRW2100027514.1.1",
          "form": "달달말고는 딱히없는거같기도하고",
          "original_form": "달달말고는 딱히없는거같기도하고",
          "speaker_id": "2",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100027514.1.2",
          "form": "결혼은 어때? 언제 하고 싶다 이런거",
          "original_form": "결혼은 어때? 언제 하고 싶다 이런거",
          "speaker_id": "1",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100027514.1.3",
          "form": "음... 잘모르겠어요",
          "original_form": "음... 잘모르겠어요",
          "speaker_id": "2",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100027514.1.4",
          "form": "전그 오래사귄애랑",
          "original_form": "전그 오래사귄애랑",
          "speaker_id": "2",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100027514.1.5",
          "form": "결혼할줄알았는데",
          "original_form": "결혼할줄알았는데",
          "speaker_id": "2",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100027514.1.6",
          "form": "아니였더라구요 ㅋㅋㅋㅋㅋ",
          "original_form": "아니였더라구요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027514.1.7",
          "form": "아하ㅠㅠ 그런거 있죠",
          "original_form": "아하ㅠㅠ 그런거 있죠",
          "speaker_id": "1",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027514.1.8",
          "form": "그런거보면 두렵기도 하고 ㅋㅋㅋㅋㅋ",
          "original_form": "그런거보면 두렵기도 하고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027514.1.9",
          "form": "진짜 전 첫남친이랑 결혼할줄ㅋㅋ",
          "original_form": "진짜 전 첫남친이랑 결혼할줄ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027514.1.10",
          "form": "근데 막 아니고 ㅋㅋㅋㅋ",
          "original_form": "근데 막 아니고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027514.1.11",
          "form": "너무 뭘 몰랐죠 ㅠㅠㅋㅋㅋ",
          "original_form": "너무 뭘 몰랐죠 ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027514.1.12",
          "form": "그니깐요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그니깐요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027514.1.13",
          "form": "연애하면서 배우게 되는거같아요",
          "original_form": "연애하면서 배우게 되는거같아요",
          "speaker_id": "2",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027514.1.14",
          "form": "배우고 결심해도 뒤돌면 까먹고",
          "original_form": "배우고 결심해도 뒤돌면 까먹고",
          "speaker_id": "2",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027514.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027514.1.16",
          "form": "예전에는 연애하면 당연히 별일 없음 오래가고 결혼하는거 아닌가 했는뎈ㅋㅋ",
          "original_form": "예전에는 연애하면 당연히 별일 없음 오래가고 결혼하는거 아닌가 했는뎈ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027514.1.17",
          "form": "이젠 뭐.. 해탈..ㅋㅋ",
          "original_form": "이젠 뭐.. 해탈..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027514.1.18",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "2",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027514.1.19",
          "form": "맞아요 연애 그래도 해보니까 그것도 늘더라구요",
          "original_form": "맞아요 연애 그래도 해보니까 그것도 늘더라구요",
          "speaker_id": "1",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027514.1.20",
          "form": "으아 너무 어려워요 사람 맘이ㅠㅠ",
          "original_form": "으아 너무 어려워요 사람 맘이ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027514.1.21",
          "form": "저도 내가 맞추면 안싸울텐데 이랬는데",
          "original_form": "저도 내가 맞추면 안싸울텐데 이랬는데",
          "speaker_id": "2",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027514.1.22",
          "form": "아니였어요",
          "original_form": "아니였어요",
          "speaker_id": "2",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027514.1.23",
          "form": "맞춰도 싸우는건 매한가지였죠",
          "original_form": "맞춰도 싸우는건 매한가지였죠",
          "speaker_id": "2",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027514.1.24",
          "form": "너무 쉽게 할려했나바요 ㅋㅋ",
          "original_form": "너무 쉽게 할려했나바요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027514.1.25",
          "form": "날먹",
          "original_form": "날먹",
          "speaker_id": "2",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027514.1.26",
          "form": "앜ㅋㅋㅋ 진짜 근데 맞추는거 어렵지 않아요?",
          "original_form": "앜ㅋㅋㅋ 진짜 근데 맞추는거 어렵지 않아요?",
          "speaker_id": "1",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100027514.1.27",
          "form": "맞아요 진짜 잘 모르겠고",
          "original_form": "맞아요 진짜 잘 모르겠고",
          "speaker_id": "2",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100027514.1.28",
          "form": "지금남자친구도5년을알았는데",
          "original_form": "지금남자친구도5년을알았는데",
          "speaker_id": "2",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027514.1.29",
          "form": "연애하니 또 다른 사람같고",
          "original_form": "연애하니 또 다른 사람같고",
          "speaker_id": "2",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027514.1.30",
          "form": "그래요!",
          "original_form": "그래요!",
          "speaker_id": "2",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027514.1.31",
          "form": "에고ㅠㅠ 고생하셨어요",
          "original_form": "에고ㅠㅠ 고생하셨어요",
          "speaker_id": "1",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027514.1.32",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027514.1.33",
          "form": "지인일때랑 애인일때랑 넘 달라요",
          "original_form": "지인일때랑 애인일때랑 넘 달라요",
          "speaker_id": "1",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027514.1.34",
          "form": "맞어요 맞아요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞어요 맞아요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027514.1.35",
          "form": "저도 친구에서 연인된 적 있었는데 너무 허무하게 헤어졌어요 ㅋㅋㅋ",
          "original_form": "저도 친구에서 연인된 적 있었는데 너무 허무하게 헤어졌어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027514.1.36",
          "form": "왜헤어지셨어요??",
          "original_form": "왜헤어지셨어요??",
          "speaker_id": "2",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027514.1.37",
          "form": "연애가 젤 알 수 없고 ㅋㅋㅋㅋ",
          "original_form": "연애가 젤 알 수 없고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027514.1.38",
          "form": "그냥 제가 아파도 아프지마라 입만 털고 와보지도 않고 만나자는 말도 없고 게임만하고",
          "original_form": "그냥 제가 아파도 아프지마라 입만 털고 와보지도 않고 만나자는 말도 없고 게임만하고",
          "speaker_id": "1",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027514.1.39",
          "form": "그래서 헤어졌죠 으으으",
          "original_form": "그래서 헤어졌죠 으으으",
          "speaker_id": "1",
          "time": "20210902 13:27"
        }
      ]
    }
