      "utterance": [
        {
          "id": "MDRW2100014087.1.1",
          "form": "카페 가면 보통 뭐 마셔요?",
          "original_form": "카페 가면 보통 뭐 마셔요?",
          "speaker_id": "1",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014087.1.2",
          "form": "녹차라떼마셔요",
          "original_form": "녹차라떼마셔요",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014087.1.3",
          "form": "커피를 좋아하지않아서요",
          "original_form": "커피를 좋아하지않아서요",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014087.1.4",
          "form": "저는 아아메요",
          "original_form": "저는 아아메요",
          "speaker_id": "1",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014087.1.5",
          "form": "카페인 예민하세요?",
          "original_form": "카페인 예민하세요?",
          "speaker_id": "1",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014087.1.6",
          "form": "어떤음료 좋아해요?",
          "original_form": "어떤음료 좋아해요?",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014087.1.7",
          "form": "저는 안 단 음료 좋아해요",
          "original_form": "저는 안 단 음료 좋아해요",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014087.1.8",
          "form": "그런데 케이크를 먹습니다",
          "original_form": "그런데 케이크를 먹습니다",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014087.1.9",
          "form": "저도 단음료 가능하면 안마시려고하는데",
          "original_form": "저도 단음료 가능하면 안마시려고하는데",
          "speaker_id": "2",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014087.1.10",
          "form": "그러면 먹을게 아메리카노에요ㅋㅋ",
          "original_form": "그러면 먹을게 아메리카노에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014087.1.11",
          "form": "치즈게이크 좋아요",
          "original_form": "치즈게이크 좋아요",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014087.1.12",
          "form": "요즘은 케이크도 그림 그려서 만드는 케이크가 있더라구요",
          "original_form": "요즘은 케이크도 그림 그려서 만드는 케이크가 있더라구요",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014087.1.13",
          "form": "특별한행사같은거 있을때 구입하기 좋을것같아요",
          "original_form": "특별한행사같은거 있을때 구입하기 좋을것같아요",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014087.1.14",
          "form": "어머니 생신 때 앙금케이크 맞춰본 적 있어요",
          "original_form": "어머니 생신 때 앙금케이크 맞춰본 적 있어요",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014087.1.15",
          "form": "예뻐서 사진찍기 좋더라구요",
          "original_form": "예뻐서 사진찍기 좋더라구요",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014087.1.16",
          "form": "저녁식사드셨어요?",
          "original_form": "저녁식사드셨어요?",
          "speaker_id": "2",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014087.1.17",
          "form": "네 샌드위치 먹었어요",
          "original_form": "네 샌드위치 먹었어요",
          "speaker_id": "1",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014087.1.18",
          "form": "ㅎㅎ 어떤 샌드위치요?",
          "original_form": "ㅎㅎ 어떤 샌드위치요?",
          "speaker_id": "2",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100014087.1.19",
          "form": "베이컨에그요ㅋㅋ",
          "original_form": "베이컨에그요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100014087.1.20",
          "form": "식사 하셨어요?",
          "original_form": "식사 하셨어요?",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014087.1.21",
          "form": "넹 저는 알밥먹었어요",
          "original_form": "넹 저는 알밥먹었어요",
          "speaker_id": "2",
          "time": "20210825 20:03"
        },
        {
          "id": "MDRW2100014087.1.22",
          "form": "얘기 들으니 저도 먹고싶네요",
          "original_form": "얘기 들으니 저도 먹고싶네요",
