        {
          "id": "MDRW2100046621.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211103 22:30"
        },
        {
          "id": "MDRW2100046621.1.2",
          "form": "안녕하세욤",
          "original_form": "안녕하세욤",
          "speaker_id": "2",
          "time": "20211103 22:33"
        },
        {
          "id": "MDRW2100046621.1.3",
          "form": "뭐하고계세요?",
          "original_form": "뭐하고계세요?",
          "speaker_id": "2",
          "time": "20211103 22:33"
        },
        {
          "id": "MDRW2100046621.1.4",
          "form": "숙제하고 있네요ㅜㅜ",
          "original_form": "숙제하고 있네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20211103 22:33"
        },
        {
          "id": "MDRW2100046621.1.5",
          "form": "학생이세요?!",
          "original_form": "학생이세요?!",
          "speaker_id": "2",
          "time": "20211103 22:34"
        },
        {
          "id": "MDRW2100046621.1.6",
          "form": "아니요 알바 중인데 숙제가 있네요",
          "original_form": "아니요 알바 중인데 숙제가 있네요",
          "speaker_id": "1",
          "time": "20211103 22:34"
        },
        {
          "id": "MDRW2100046621.1.7",
          "form": "헛 알바하는 곳에서 숙제를 내줘요..?",
          "original_form": "헛 알바하는 곳에서 숙제를 내줘요..?",
          "speaker_id": "2",
          "time": "20211103 22:34"
        },
        {
          "id": "MDRW2100046621.1.8",
          "form": "일배우고 있어서요ㅜㅜ",
          "original_form": "일배우고 있어서요ㅜㅜ",
          "speaker_id": "1",
          "time": "20211103 22:35"
        },
        {
          "id": "MDRW2100046621.1.9",
          "form": "아 힘드시겠다 ㅠㅠ",
          "original_form": "아 힘드시겠다 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211103 22:35"
        },
        {
          "id": "MDRW2100046621.1.10",
          "form": "어떤 분야인지 여쭤봐도 되나요?",
          "original_form": "어떤 분야인지 여쭤봐도 되나요?",
          "speaker_id": "2",
          "time": "20211103 22:35"
        },
        {
          "id": "MDRW2100046621.1.11",
          "form": "넵 ㅜㅜ보안서 제출해서 자세한건 안되네요ㅜㅜ",
          "original_form": "넵 ㅜㅜ보안서 제출해서 자세한건 안되네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20211103 22:36"
        },
        {
          "id": "MDRW2100046621.1.12",
          "form": "아하 저녁은 드셨어요?",
          "original_form": "아하 저녁은 드셨어요?",
          "speaker_id": "2",
          "time": "20211103 22:37"
        },
        {
          "id": "MDRW2100046621.1.13",
          "form": "넵  라면으로 끼니를 떼웠습니다!",
          "original_form": "넵  라면으로 끼니를 떼웠습니다!",
          "speaker_id": "1",
          "time": "20211103 22:37"
        },
        {
          "id": "MDRW2100046621.1.14",
          "form": "늦은 시간까지 일하시는군요",
          "original_form": "늦은 시간까지 일하시는군요",
          "speaker_id": "1",
          "time": "20211103 22:38"
        },
        {
          "id": "MDRW2100046621.1.15",
          "form": "헉 든든하게 먹고 일하셔야죠!",
          "original_form": "헉 든든하게 먹고 일하셔야죠!",
          "speaker_id": "2",
          "time": "20211103 22:40"
        },
        {
          "id": "MDRW2100046621.1.16",
          "form": "우리 둘 다 힘내요 ㅠㅠ",
          "original_form": "우리 둘 다 힘내요 ㅠㅠ",
          "speaker_id": "2",
