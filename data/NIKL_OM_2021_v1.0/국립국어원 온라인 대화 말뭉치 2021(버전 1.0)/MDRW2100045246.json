      "utterance": [
        {
          "id": "MDRW2100045246.1.1",
          "form": "밥먹으면서 가볍게 보기도 좋아요! ㅋㅋ",
          "original_form": "밥먹으면서 가볍게 보기도 좋아요! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045246.1.2",
          "form": "개그의 장점이죠",
          "original_form": "개그의 장점이죠",
          "speaker_id": "2",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045246.1.3",
          "form": "나름 로맨스도 있어요",
          "original_form": "나름 로맨스도 있어요",
          "speaker_id": "2",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045246.1.4",
          "form": "4시즌이면 딱이네요",
          "original_form": "4시즌이면 딱이네요",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045246.1.5",
          "form": "완결 땅땅인가여 더 나올예정인가여",
          "original_form": "완결 땅땅인가여 더 나올예정인가여",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045246.1.6",
          "form": "꽉닫힌 해피엔딩으로 완결 땅땅이요 ㅜ",
          "original_form": "꽉닫힌 해피엔딩으로 완결 땅땅이요 ㅜ",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045246.1.7",
          "form": "아쉽지만 보내줘야죠..",
          "original_form": "아쉽지만 보내줘야죠..",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045246.1.8",
          "form": "오 좋아여 팬들은 좀아쉽겠어요",
          "original_form": "오 좋아여 팬들은 좀아쉽겠어요",
          "speaker_id": "1",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045246.1.9",
          "form": "시즌이 너무 길면 보기싫은데",
          "original_form": "시즌이 너무 길면 보기싫은데",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045246.1.10",
          "form": "전 완결도안났는데 시즌종료시킨거 기다리고있어요 ㅋㅋㅋㅋ....넷플아....",
          "original_form": "전 완결도안났는데 시즌종료시킨거 기다리고있어요 ㅋㅋㅋㅋ....넷플아....",
          "speaker_id": "1",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045246.1.11",
          "form": "좋아하는 작품은 길게하길 바라는 맘이있어요 ㅋㅋ",
          "original_form": "좋아하는 작품은 길게하길 바라는 맘이있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045246.1.12",
          "form": "맞아요ㅜㅜㅜ좀길게길게",
          "original_form": "맞아요ㅜㅜㅜ좀길게길게",
          "speaker_id": "1",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045246.1.13",
          "form": "넷플 요즘 그런 깡패짓 많이하죠!!",
          "original_form": "넷플 요즘 그런 깡패짓 많이하죠!!",
          "speaker_id": "2",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045246.1.14",
          "form": "넘 싫어요",
          "original_form": "넘 싫어요",
          "speaker_id": "2",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045246.1.15",
          "form": "네ㅜㅜㅜ나름인기있었는데",
          "original_form": "네ㅜㅜㅜ나름인기있었는데",
          "speaker_id": "1",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045246.1.16",
          "form": "여성주연작도 많이 캔슬하고 ㅜㅜ",
          "original_form": "여성주연작도 많이 캔슬하고 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045246.1.17",
          "form": "무슨작품인가요?",
          "original_form": "무슨작품인가요?",
          "speaker_id": "2",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045246.1.18",
          "form": "마이너작이라고ㅜㅜ픅흑",
          "original_form": "마이너작이라고ㅜㅜ픅흑",
          "speaker_id": "1",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045246.1.19",
          "form": "더크젠틀리의 전체론적탐정사무소 에요",
          "original_form": "더크젠틀리의 전체론적탐정사무소 에요",
          "speaker_id": "1",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045246.1.20",
          "form": "이거 세계관 개탄탄한데ㅜ 넘아쉬워요",
          "original_form": "이거 세계관 개탄탄한데ㅜ 넘아쉬워요",
          "speaker_id": "1",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045246.1.21",
          "form": "아 저 그거 추천받은적 있어요",
          "original_form": "아 저 그거 추천받은적 있어요",
          "speaker_id": "2",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100045246.1.22",
          "form": "가벼운 추리물? 살짝 웃긴 맞나요?",
          "original_form": "가벼운 추리물? 살짝 웃긴 맞나요?",
          "speaker_id": "2",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045246.1.23",
          "form": "덕후들 심장뛰게하는 ㅋㅋㅋㅋㅋ",
          "original_form": "덕후들 심장뛰게하는 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100045246.1.24",
          "form": "궁금해지내요",
          "original_form": "궁금해지내요",
          "speaker_id": "2",
          "time": "20210908 01:33"
        },
        {
          "id": "MDRW2100045246.1.25",
          "form": "안그래도 요즘 볼게 없어서",
          "original_form": "안그래도 요즘 볼게 없어서",
          "speaker_id": "2",
          "time": "20210908 01:33"
        },
        {
          "id": "MDRW2100045246.1.26",
          "form": "네 좀웃기고 병맛요소들어가는데 또 어두운요소도있고  능력들이나 세계관이 탄탄해요",
          "original_form": "네 좀웃기고 병맛요소들어가는데 또 어두운요소도있고  능력들이나 세계관이 탄탄해요",
          "speaker_id": "1",
          "time": "20210908 01:33"
        },
        {
          "id": "MDRW2100045246.1.27",
          "form": "세계관 탄탄한거 좋아요!",
          "original_form": "세계관 탄탄한거 좋아요!",
          "speaker_id": "2",
          "time": "20210908 01:33"
        }
      ]
    }
  ]
