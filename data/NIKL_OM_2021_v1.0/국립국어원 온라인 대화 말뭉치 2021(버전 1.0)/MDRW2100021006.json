      "utterance": [
        {
          "id": "MDRW2100021006.1.1",
          "form": "중고거래하시나요?",
          "original_form": "중고거래하시나요?",
          "speaker_id": "2",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100021006.1.2",
          "form": "네 당근마켓 종종합니다",
          "original_form": "네 당근마켓 종종합니다",
          "speaker_id": "1",
          "time": "20210828 16:34"
        },
        {
          "id": "MDRW2100021006.1.3",
          "form": "오! 저도 시작하려고 깔았어요",
          "original_form": "오! 저도 시작하려고 깔았어요",
          "speaker_id": "2",
          "time": "20210828 16:40"
        },
        {
          "id": "MDRW2100021006.1.4",
          "form": "안쓰는게많아서",
          "original_form": "안쓰는게많아서",
          "speaker_id": "1",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100021006.1.5",
          "form": "엄청헐값에 파니까",
          "original_form": "엄청헐값에 파니까",
          "speaker_id": "1",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100021006.1.6",
          "form": "팔리더라고요ㅎㅎ",
          "original_form": "팔리더라고요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100021006.1.7",
          "form": "오 헐값에 팔아야하는거군요..",
          "original_form": "오 헐값에 팔아야하는거군요..",
          "speaker_id": "2",
          "time": "20210828 16:42"
        },
        {
          "id": "MDRW2100021006.1.8",
          "form": "왠지 안팔리더라구요..",
          "original_form": "왠지 안팔리더라구요..",
          "speaker_id": "2",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100021006.1.9",
          "form": "천원에 팔고그래요ㅋㅋㅋ",
          "original_form": "천원에 팔고그래요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100021006.1.10",
          "form": "헠ㅋㅋㅋ 진짜 헐값이네요",
          "original_form": "헠ㅋㅋㅋ 진짜 헐값이네요",
          "speaker_id": "2",
          "time": "20210828 16:45"
        },
        {
          "id": "MDRW2100021006.1.11",
          "form": "안팔린 이유가 확실해지네요",
          "original_form": "안팔린 이유가 확실해지네요",
          "speaker_id": "2",
          "time": "20210828 16:46"
        },
        {
          "id": "MDRW2100021006.1.12",
          "form": "친구들이 그냥 기부하라고ㅋㅋㅋ",
          "original_form": "친구들이 그냥 기부하라고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021006.1.13",
          "form": "진짜 기부수준이시네요",
          "original_form": "진짜 기부수준이시네요",
          "speaker_id": "2",
          "time": "20210828 16:50"
        },
        {
          "id": "MDRW2100021006.1.14",
          "form": "동네 근처면 제가 싹쓸이하고싶어요ㅋㅋ",
          "original_form": "동네 근처면 제가 싹쓸이하고싶어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:50"
        },
        {
          "id": "MDRW2100021006.1.15",
          "form": "아진짜,,, ㅋㄱㅋㄱㅋㅋㅋㅋㅋ",
          "original_form": "아진짜,,, ㅋㄱㅋㄱㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100021006.1.16",
          "form": "요즘 뭐 구매하신거있으세요?!",
          "original_form": "요즘 뭐 구매하신거있으세요?!",
          "speaker_id": "1",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100021006.1.17",
          "form": "저는 책선물받은게 제일 최신물건",
          "original_form": "저는 책선물받은게 제일 최신물건",
          "speaker_id": "1",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100021006.1.18",
          "form": "저 키링 재료요!",
          "original_form": "저 키링 재료요!",
          "speaker_id": "2",
          "time": "20210828 16:55"
        },
        {
          "id": "MDRW2100021006.1.19",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210828 16:55"
        },
        {
          "id": "MDRW2100021006.1.20",
          "form": "직접만드세요?",
          "original_form": "직접만드세요?",
          "speaker_id": "1",
          "time": "20210828 16:55"
        },
        {
          "id": "MDRW2100021006.1.21",
          "form": "금손!!",
          "original_form": "금손!!",
          "speaker_id": "1",
          "time": "20210828 16:55"
        },
        {
          "id": "MDRW2100021006.1.22",
          "form": "친구들끼리 단체로 맞춰서 선물하려고 샀는데",
          "original_form": "친구들끼리 단체로 맞춰서 선물하려고 샀는데",
          "speaker_id": "2",
          "time": "20210828 16:55"
        },
        {
          "id": "MDRW2100021006.1.23",
          "form": "제걸ㅈ안샀어욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "제걸ㅈ안샀어욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:55"
        },
        {
          "id": "MDRW2100021006.1.24",
          "form": "앗 아님니당 비슷하게..ㅋㅋ",
          "original_form": "앗 아님니당 비슷하게..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:55"
        },
        {
          "id": "MDRW2100021006.1.25",
