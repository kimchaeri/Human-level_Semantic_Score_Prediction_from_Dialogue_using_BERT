      "utterance": [
        {
          "id": "MDRW2100040581.1.1",
          "form": "요리 좋아하세요?",
          "original_form": "요리 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100040581.1.2",
          "form": "저 하는건 좋아해요!!",
          "original_form": "저 하는건 좋아해요!!",
          "speaker_id": "2",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100040581.1.3",
          "form": "청소보다는 요리파!!",
          "original_form": "청소보다는 요리파!!",
          "speaker_id": "2",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100040581.1.4",
          "form": "오 저는 남편이 주로 요리를 하고 저는 설거지를 하거든요",
          "original_form": "오 저는 남편이 주로 요리를 하고 저는 설거지를 하거든요",
          "speaker_id": "1",
          "time": "20210904 22:56"
        },
        {
          "id": "MDRW2100040581.1.5",
          "form": "라면도 계량컵으로 해야돼요",
          "original_form": "라면도 계량컵으로 해야돼요",
          "speaker_id": "1",
          "time": "20210904 22:57"
        },
        {
          "id": "MDRW2100040581.1.6",
          "form": "전 청소는 안해도 설거지는 해요 ㅋㅋㅋ",
          "original_form": "전 청소는 안해도 설거지는 해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:57"
        },
        {
          "id": "MDRW2100040581.1.7",
          "form": "역시 수학과...??",
          "original_form": "역시 수학과...??",
          "speaker_id": "2",
          "time": "20210904 22:57"
        },
        {
          "id": "MDRW2100040581.1.8",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:57"
        },
        {
          "id": "MDRW2100040581.1.9",
          "form": "안그럼 한강이되거나 짜글이가 되어서요 ㅋㅋ",
          "original_form": "안그럼 한강이되거나 짜글이가 되어서요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:57"
        },
        {
          "id": "MDRW2100040581.1.10",
          "form": "계량해서 먹는게 맛있대요",
          "original_form": "계량해서 먹는게 맛있대요",
          "speaker_id": "2",
          "time": "20210904 22:57"
        },
        {
          "id": "MDRW2100040581.1.11",
          "form": "정석으로 드시네요",
          "original_form": "정석으로 드시네요",
          "speaker_id": "2",
          "time": "20210904 22:57"
        },
        {
          "id": "MDRW2100040581.1.12",
          "form": "무슨 라면 좋아하세요??",
          "original_form": "무슨 라면 좋아하세요??",
          "speaker_id": "2",
          "time": "20210904 22:59"
        },
        {
          "id": "MDRW2100040581.1.13",
          "form": "짜장범벅, 비빔면, 불닭볶음면을 좋아합니다. 국물라면은 참깨라면이요",
          "original_form": "짜장범벅, 비빔면, 불닭볶음면을 좋아합니다. 국물라면은 참깨라면이요",
          "speaker_id": "1",
          "time": "20210904 22:59"
        },
        {
          "id": "MDRW2100040581.1.14",
          "form": "저는 요즘에 스낵면 나가사키짬뽕 오모리 좋아해요!!",
          "original_form": "저는 요즘에 스낵면 나가사키짬뽕 오모리 좋아해요!!",
          "speaker_id": "2",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100040581.1.15",
          "form": "불닭볶음면은 소스로 먹고 있어요 ㅎㅎㅎ",
          "original_form": "불닭볶음면은 소스로 먹고 있어요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100040581.1.16",
          "form": "아 이시간에 라면이야기라니...",
          "original_form": "아 이시간에 라면이야기라니...",
          "speaker_id": "1",
          "time": "20210904 23:01"
        },
        {
          "id": "MDRW2100040581.1.17",
          "form": "넘나 맛날것 같아요",
          "original_form": "넘나 맛날것 같아요",
          "speaker_id": "1",
          "time": "20210904 23:01"
        },
        {
          "id": "MDRW2100040581.1.18",
          "form": "오모리는 뭔가요",
          "original_form": "오모리는 뭔가요",
          "speaker_id": "1",
          "time": "20210904 23:01"
        },
        {
          "id": "MDRW2100040581.1.19",
          "form": "지에스에서 파는 오모리 라면 있거든요?",
          "original_form": "지에스에서 파는 오모리 라면 있거든요?",
          "speaker_id": "2",
          "time": "20210904 23:01"
        },
        {
          "id": "MDRW2100040581.1.20",
          "form": "김치 들어있는건데 맛있어요",
          "original_form": "김치 들어있는건데 맛있어요",
          "speaker_id": "2",
          "time": "20210904 23:01"
        },
        {
          "id": "MDRW2100040581.1.21",
          "form": "오 궁금한데요? 사봐야겠어요",
          "original_form": "오 궁금한데요? 사봐야겠어요",
          "speaker_id": "1",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100040581.1.22",
          "form": "라면에 김치가 들어가면 맛이 없을수가 없을듯합니다",
          "original_form": "라면에 김치가 들어가면 맛이 없을수가 없을듯합니다",
          "speaker_id": "1",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100040581.1.23",
          "form": "진짜 맛있어요 한번 도전해보세요",
          "original_form": "진짜 맛있어요 한번 도전해보세요",
          "speaker_id": "2",
          "time": "20210904 23:04"
        },
        {
          "id": "MDRW2100040581.1.24",
          "form": "제가 뭘 이렇게 적극적으로 추천 못하는데 오모리는 추천 가능합니닼ㅋㅋㅋㅋ",
          "original_form": "제가 뭘 이렇게 적극적으로 추천 못하는데 오모리는 추천 가능합니닼ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:05"
        },
        {
          "id": "MDRW2100040581.1.25",
          "form": "오 그정도인가요? 조만간 도전해볼게요 추천감사합니다",
          "original_form": "오 그정도인가요? 조만간 도전해볼게요 추천감사합니다",
          "speaker_id": "1",
          "time": "20210904 23:05"
        }
      ]
    }
