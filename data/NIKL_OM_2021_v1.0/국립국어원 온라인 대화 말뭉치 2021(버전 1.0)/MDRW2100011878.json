      "utterance": [
        {
          "id": "MDRW2100011878.1.1",
          "form": "으어어 저녁 언제머글거야?",
          "original_form": "으어어 저녁 언제머글거야?",
          "speaker_id": "1",
          "time": "20210824 17:21"
        },
        {
          "id": "MDRW2100011878.1.2",
          "form": "궁금쓰",
          "original_form": "궁금쓰",
          "speaker_id": "1",
          "time": "20210824 17:21"
        },
        {
          "id": "MDRW2100011878.1.3",
          "form": "음 7시쯤..?",
          "original_form": "음 7시쯤..?",
          "speaker_id": "2",
          "time": "20210824 17:21"
        },
        {
          "id": "MDRW2100011878.1.4",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "1",
          "time": "20210824 17:21"
        },
        {
          "id": "MDRW2100011878.1.5",
          "form": "빠르면 6시반?",
          "original_form": "빠르면 6시반?",
          "speaker_id": "2",
          "time": "20210824 17:21"
        },
        {
          "id": "MDRW2100011878.1.6",
          "form": "세상에나",
          "original_form": "세상에나",
          "speaker_id": "1",
          "time": "20210824 17:21"
        },
        {
          "id": "MDRW2100011878.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:22"
        },
        {
          "id": "MDRW2100011878.1.8",
          "form": "오늘은 정녕",
          "original_form": "오늘은 정녕",
          "speaker_id": "1",
          "time": "20210824 17:22"
        },
        {
          "id": "MDRW2100011878.1.9",
          "form": "지코바",
          "original_form": "지코바",
          "speaker_id": "1",
          "time": "20210824 17:22"
        },
        {
          "id": "MDRW2100011878.1.10",
          "form": "아니야?",
          "original_form": "아니야?",
          "speaker_id": "1",
          "time": "20210824 17:22"
        },
        {
          "id": "MDRW2100011878.1.11",
          "form": "웅...",
          "original_form": "웅...",
          "speaker_id": "2",
          "time": "20210824 17:22"
        },
        {
          "id": "MDRW2100011878.1.12",
          "form": "정녕..?",
          "original_form": "정녕..?",
          "speaker_id": "1",
          "time": "20210824 17:22"
        },
        {
          "id": "MDRW2100011878.1.13",
          "form": "그래..",
          "original_form": "그래..",
          "speaker_id": "1",
          "time": "20210824 17:22"
        },
        {
          "id": "MDRW2100011878.1.14",
          "form": "그러고보니 밥없지않아...?",
          "original_form": "그러고보니 밥없지않아...?",
          "speaker_id": "2",
          "time": "20210824 17:22"
        },
        {
          "id": "MDRW2100011878.1.15",
          "form": "그래??",
          "original_form": "그래??",
          "speaker_id": "1",
          "time": "20210824 17:22"
        },
        {
          "id": "MDRW2100011878.1.16",
          "form": "치밥먹을수있는고야?",
          "original_form": "치밥먹을수있는고야?",
          "speaker_id": "2",
          "time": "20210824 17:22"
        },
        {
          "id": "MDRW2100011878.1.17",
          "form": "조금있는데",
          "original_form": "조금있는데",
          "speaker_id": "1",
          "time": "20210824 17:22"
        },
        {
          "id": "MDRW2100011878.1.18",
          "form": "딱 먹을만큼",
          "original_form": "딱 먹을만큼",
          "speaker_id": "1",
          "time": "20210824 17:22"
        },
        {
          "id": "MDRW2100011878.1.19",
          "form": "치밥",
          "original_form": "치밥",
          "speaker_id": "1",
          "time": "20210824 17:22"
        },
        {
          "id": "MDRW2100011878.1.20",
          "form": "흠",
          "original_form": "흠",
          "speaker_id": "2",
          "time": "20210824 17:23"
        },
        {
          "id": "MDRW2100011878.1.21",
          "form": "초롱초롱",
          "original_form": "초롱초롱",
          "speaker_id": "1",
          "time": "20210824 17:23"
        },
        {
          "id": "MDRW2100011878.1.22",
          "form": "눈빛",
          "original_form": "눈빛",
          "speaker_id": "1",
          "time": "20210824 17:23"
        },
        {
          "id": "MDRW2100011878.1.23",
          "form": "치킨안땡기는데..",
          "original_form": "치킨안땡기는데..",
          "speaker_id": "2",
          "time": "20210824 17:23"
        },
        {
          "id": "MDRW2100011878.1.24",
          "form": "ㅜㅠ",
          "original_form": "ㅜㅠ",
          "speaker_id": "1",
          "time": "20210824 17:23"
        },
        {
          "id": "MDRW2100011878.1.25",
          "form": "나는 순대먹고싶엉",
          "original_form": "나는 순대먹고싶엉",
          "speaker_id": "2",
          "time": "20210824 17:23"
        },
        {
          "id": "MDRW2100011878.1.26",
          "form": "그럼 게살샌드위치..",
          "original_form": "그럼 게살샌드위치..",
          "speaker_id": "1",
          "time": "20210824 17:23"
        },
        {
          "id": "MDRW2100011878.1.27",
          "form": "게살도 영...",
          "original_form": "게살도 영...",
          "speaker_id": "2",
          "time": "20210824 17:23"
        },
        {
          "id": "MDRW2100011878.1.28",
