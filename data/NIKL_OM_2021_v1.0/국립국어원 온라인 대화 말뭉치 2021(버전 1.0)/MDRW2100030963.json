      "utterance": [
        {
          "id": "MDRW2100030963.1.1",
          "form": "만두 ㅎㅎ 전 고기 ㅎㅎㅎㅎ",
          "original_form": "만두 ㅎㅎ 전 고기 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 14:51"
        },
        {
          "id": "MDRW2100030963.1.2",
          "form": "왕만두 맛집있거든요ㅋㅋㅋ",
          "original_form": "왕만두 맛집있거든요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:55"
        },
        {
          "id": "MDRW2100030963.1.3",
          "form": "고기 사올때는 또 든든하겠네요",
          "original_form": "고기 사올때는 또 든든하겠네요",
          "speaker_id": "1",
          "time": "20210904 14:55"
        },
        {
          "id": "MDRW2100030963.1.4",
          "form": "ㅋㅋ 기분이가 많이 좋지요 ㅋㅋ",
          "original_form": "ㅋㅋ 기분이가 많이 좋지요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:59"
        },
        {
          "id": "MDRW2100030963.1.5",
          "form": "집에 야채가 있어서 오늘도 고기먹어요 ㅋㅋ",
          "original_form": "집에 야채가 있어서 오늘도 고기먹어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:59"
        },
        {
          "id": "MDRW2100030963.1.6",
          "form": "ㅋㅋㅋ전 백신 맞기전 준비로 삼겹살",
          "original_form": "ㅋㅋㅋ전 백신 맞기전 준비로 삼겹살",
          "speaker_id": "1",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100030963.1.7",
          "form": "아ㅜ1차세요?",
          "original_form": "아ㅜ1차세요?",
          "speaker_id": "2",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100030963.1.8",
          "form": "중요햡니다ㅠㅎㅎ 건강",
          "original_form": "중요햡니다ㅠㅎㅎ 건강",
          "speaker_id": "2",
          "time": "20210904 17:17"
        },
        {
          "id": "MDRW2100030963.1.9",
          "form": "아니용 2차 맞고왔어요 ㅋㅋㅋ",
          "original_form": "아니용 2차 맞고왔어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:29"
        },
        {
          "id": "MDRW2100030963.1.10",
          "form": "꺄악 쉬세요 쉬셔야해요 ㅎ",
          "original_form": "꺄악 쉬세요 쉬셔야해요 ㅎ",
          "speaker_id": "2",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100030963.1.11",
          "form": "넹 한 9시간부터면 증상이 나타난다는데",
          "original_form": "넹 한 9시간부터면 증상이 나타난다는데",
          "speaker_id": "1",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100030963.1.12",
          "form": "저 지금 한 8시간째거든오 ㅋㅋㅋ",
          "original_form": "저 지금 한 8시간째거든오 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100030963.1.13",
          "form": "이것저것 사와서 다 먹어치웟어요",
          "original_form": "이것저것 사와서 다 먹어치웟어요",
          "speaker_id": "1",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100030963.1.14",
          "form": "허우 ㅜ 어때요ㅠ괜찮으세요?",
          "original_form": "허우 ㅜ 어때요ㅠ괜찮으세요?",
          "speaker_id": "2",
          "time": "20210904 20:41"
        },
        {
          "id": "MDRW2100030963.1.15",
          "form": "아직도 아무렇지 않네요",
          "original_form": "아직도 아무렇지 않네요",
          "speaker_id": "1",
          "time": "20210904 20:59"
        },
        {
          "id": "MDRW2100030963.1.16",
          "form": "이제 9시간짼데",
          "original_form": "이제 9시간짼데",
          "speaker_id": "1",
          "time": "20210904 20:59"
        },
        {
          "id": "MDRW2100030963.1.17",
          "form": "그냥 1차 맞은 날처럼 팔아픈 정도",
          "original_form": "그냥 1차 맞은 날처럼 팔아픈 정도",
          "speaker_id": "1",
          "time": "20210904 20:59"
        },
        {
          "id": "MDRW2100030963.1.18",
          "form": "너무 다행이에요",
          "original_form": "너무 다행이에요",
          "speaker_id": "2",
          "time": "20210904 23:13"
        },
        {
          "id": "MDRW2100030963.1.19",
          "form": "타이레놀 먹었는데 약빨 잘 받네여ㅋㅋ 아플거같을때쯤 하나씩 먹었어요",
          "original_form": "타이레놀 먹었는데 약빨 잘 받네여ㅋㅋ 아플거같을때쯤 하나씩 먹었어요",
          "speaker_id": "1",
          "time": "20210905 07:10"
        },
        {
          "id": "MDRW2100030963.1.20",
          "form": "너무 다행입니다!!! 다들 2차가ㅊ더 아프다던데",
          "original_form": "너무 다행입니다!!! 다들 2차가ㅊ더 아프다던데",
          "speaker_id": "2",
          "time": "20210905 12:30"
        },
        {
          "id": "MDRW2100030963.1.21",
          "form": "그러게요 24시간 지났는데 아직 체온도 정상이구..ㅋㅋ",
          "original_form": "그러게요 24시간 지났는데 아직 체온도 정상이구..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:48"
        },
        {
          "id": "MDRW2100030963.1.22",
          "form": "근데 한 삼일은 안정을 취하라고는 해서",
          "original_form": "근데 한 삼일은 안정을 취하라고는 해서",
          "speaker_id": "1",
          "time": "20210905 12:49"
        },
        {
          "id": "MDRW2100030963.1.23",
          "form": "맞아요 중요합니다!",
          "original_form": "맞아요 중요합니다!",
          "speaker_id": "2",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100030963.1.24",
          "form": "이럴땐 인터넷쇼핑을 즐기셔요 ㅎ 필요한거 있으실때",
          "original_form": "이럴땐 인터넷쇼핑을 즐기셔요 ㅎ 필요한거 있으실때",
          "speaker_id": "2",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100030963.1.25",
          "form": "ㅋㅋㅋㅋ배달 음식 너무시켜서 쇼핑 자제해야될거같아요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ배달 음식 너무시켜서 쇼핑 자제해야될거같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:15"
        },
        {
          "id": "MDRW2100030963.1.26",
          "form": "앗 ㅎㅎㅎ 코로나엔 배달이쥬ㅜㅜㅋ",
          "original_form": "앗 ㅎㅎㅎ 코로나엔 배달이쥬ㅜㅜㅋ",
          "speaker_id": "2",
          "time": "20210905 14:33"
        },
        {
          "id": "MDRW2100030963.1.27",
          "form": "근데 얼마전에 식당 요리사가 확진됐는지 배달 포장해간 사람까지 다 검사받으러 오라고 했다고",
          "original_form": "근데 얼마전에 식당 요리사가 확진됐는지 배달 포장해간 사람까지 다 검사받으러 오라고 했다고",
          "speaker_id": "1",
          "time": "20210905 15:09"
        },
        {
          "id": "MDRW2100030963.1.28",
          "form": "그런 이야기 들으니 또 배달도 무섭네여",
          "original_form": "그런 이야기 들으니 또 배달도 무섭네여",
          "speaker_id": "1",
          "time": "20210905 15:09"
        },
        {
          "id": "MDRW2100030963.1.29",
          "form": "어....... 좀 많이 무섭네요ㅜ",
          "original_form": "어....... 좀 많이 무섭네요ㅜ",
          "speaker_id": "2",
          "time": "20210905 18:16"
        },
        {
          "id": "MDRW2100030963.1.30",
          "form": "쇼핑의 폐해군요ㅜ",
          "original_form": "쇼핑의 폐해군요ㅜ",
          "speaker_id": "2",
