      "utterance": [
        {
          "id": "MDRW2100045241.1.1",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045241.1.2",
          "form": "국내종은 아직 못봤구요",
          "original_form": "국내종은 아직 못봤구요",
          "speaker_id": "2",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045241.1.3",
          "form": "해외종은 흔하게 말려먹어요!",
          "original_form": "해외종은 흔하게 말려먹어요!",
          "speaker_id": "2",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045241.1.4",
          "form": "앗 뭐지 뭐지",
          "original_form": "앗 뭐지 뭐지",
          "speaker_id": "1",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045241.1.5",
          "form": "새콤한가요?",
          "original_form": "새콤한가요?",
          "speaker_id": "1",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045241.1.6",
          "form": "새콤달콤해요!",
          "original_form": "새콤달콤해요!",
          "speaker_id": "2",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045241.1.7",
          "form": "열대과일인가요?",
          "original_form": "열대과일인가요?",
          "speaker_id": "1",
          "time": "20210908 01:27"
        },
        {
          "id": "MDRW2100045241.1.8",
          "form": "아뇨 열대과일은 아니에요",
          "original_form": "아뇨 열대과일은 아니에요",
          "speaker_id": "2",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045241.1.9",
          "form": "아 무슨 과일인지 감도 안 잡혀요ㅋㅋㅋㅋ",
          "original_form": "아 무슨 과일인지 감도 안 잡혀요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045241.1.10",
          "form": "마트에서 흔하게 보실 수 있어요!",
          "original_form": "마트에서 흔하게 보실 수 있어요!",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045241.1.11",
          "form": "여름에 제철이고",
          "original_form": "여름에 제철이고",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045241.1.12",
          "form": "껍질채 먹어요!",
          "original_form": "껍질채 먹어요!",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100045241.1.13",
          "form": "껍질 째로 먹는 게 보편적인가요???",
          "original_form": "껍질 째로 먹는 게 보편적인가요???",
          "speaker_id": "1",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045241.1.14",
          "form": "넵 벗겨먹기 힘들어요!",
          "original_form": "넵 벗겨먹기 힘들어요!",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045241.1.15",
          "form": "으아아아 뭐지",
          "original_form": "으아아아 뭐지",
          "speaker_id": "1",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045241.1.16",
          "form": "두글자예요",
          "original_form": "두글자예요",
          "speaker_id": "2",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045241.1.17",
          "form": "안 익으면 초록색이고 익으면 무슨 색이 되나요",
          "original_form": "안 익으면 초록색이고 익으면 무슨 색이 되나요",
          "speaker_id": "1",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045241.1.18",
          "form": "붉은 색이요!",
          "original_form": "붉은 색이요!",
          "speaker_id": "2",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045241.1.19",
          "form": "붉은 계열?",
          "original_form": "붉은 계열?",
          "speaker_id": "2",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045241.1.20",
          "form": "아!",
          "original_form": "아!",
          "speaker_id": "1",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045241.1.21",
          "form": "정답! 자두!!!",
          "original_form": "정답! 자두!!!",
          "speaker_id": "1",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045241.1.22",
          "form": "넵 맞아요!",
          "original_form": "넵 맞아요!",
          "speaker_id": "2",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045241.1.23",
          "form": "아! 자두도 말려먹지요 참",
          "original_form": "아! 자두도 말려먹지요 참",
          "speaker_id": "1",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045241.1.24",
          "form": "벗겨먹기 힘들다는 게 힌트였어요 ㅋㅋㅋ",
          "original_form": "벗겨먹기 힘들다는 게 힌트였어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045241.1.25",
          "form": "넵 푸룬은 많이 팔더라구요",
          "original_form": "넵 푸룬은 많이 팔더라구요",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045241.1.26",
          "form": "이야 이거 은근 어렵네여",
          "original_form": "이야 이거 은근 어렵네여",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045241.1.27",
          "form": "그런거같아요 ㅋㅋㅋㅋ",
          "original_form": "그런거같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:39"
        }
      ]
    }
