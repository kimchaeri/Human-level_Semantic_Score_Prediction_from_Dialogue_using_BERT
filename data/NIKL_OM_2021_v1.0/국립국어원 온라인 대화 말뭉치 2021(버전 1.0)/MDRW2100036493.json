      "utterance": [
        {
          "id": "MDRW2100036493.1.1",
          "form": "너 무슨 약 먹어?",
          "original_form": "너 무슨 약 먹어?",
          "speaker_id": "2",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100036493.1.2",
          "form": "나는 지금 먹는 약은 없어",
          "original_form": "나는 지금 먹는 약은 없어",
          "speaker_id": "1",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100036493.1.3",
          "form": "아 내 말은 영양제 말하는거야!",
          "original_form": "아 내 말은 영양제 말하는거야!",
          "speaker_id": "2",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100036493.1.4",
          "form": "오해할까봐 덧붙여",
          "original_form": "오해할까봐 덧붙여",
          "speaker_id": "2",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100036493.1.5",
          "form": "아 그렇구나 나는 몸 아플때 먹는 약인줄!",
          "original_form": "아 그렇구나 나는 몸 아플때 먹는 약인줄!",
          "speaker_id": "1",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100036493.1.6",
          "form": "아 그래? 원래 안챙겨먹어?",
          "original_form": "아 그래? 원래 안챙겨먹어?",
          "speaker_id": "2",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100036493.1.7",
          "form": "나는 사실 홍삼을 먹었었어",
          "original_form": "나는 사실 홍삼을 먹었었어",
          "speaker_id": "1",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100036493.1.8",
          "form": "아냐! 건강해야지 ㅎㅎ",
          "original_form": "아냐! 건강해야지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100036493.1.9",
          "form": "효과가 있었어?",
          "original_form": "효과가 있었어?",
          "speaker_id": "2",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100036493.1.10",
          "form": "너는 홍삼이 몸에 잘 받니?",
          "original_form": "너는 홍삼이 몸에 잘 받니?",
          "speaker_id": "1",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100036493.1.11",
          "form": "나는 위약효과만 있더라",
          "original_form": "나는 위약효과만 있더라",
          "speaker_id": "2",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100036493.1.12",
          "form": "잘안받나봐",
          "original_form": "잘안받나봐",
          "speaker_id": "2",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100036493.1.13",
          "form": "그저그랬너",
          "original_form": "그저그랬너",
          "speaker_id": "2",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100036493.1.14",
          "form": "나두아니 사실\n드라마틱한효과는 모르겠더라",
          "original_form": "나두아니 사실\n드라마틱한효과는 모르겠더라",
          "speaker_id": "1",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100036493.1.15",
          "form": "하지만 먹고싶다",
          "original_form": "하지만 먹고싶다",
          "speaker_id": "2",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100036493.1.16",
          "form": "홍삼 좋은 거 같아",
          "original_form": "홍삼 좋은 거 같아",
          "speaker_id": "2",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100036493.1.17",
          "form": "홍삼을??",
          "original_form": "홍삼을??",
          "speaker_id": "1",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100036493.1.18",
          "form": "기분이 좋아. 건강해지는 향이야",
          "original_form": "기분이 좋아. 건강해지는 향이야",
          "speaker_id": "2",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100036493.1.19",
          "form": "그 홍삼맛 좋아해?",
          "original_form": "그 홍삼맛 좋아해?",
          "speaker_id": "1",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100036493.1.20",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100036493.1.21",
          "form": "근데 건강해지는 기분이 들어.",
          "original_form": "근데 건강해지는 기분이 들어.",
          "speaker_id": "2",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100036493.1.22",
          "form": "흠 그건 나도 공감해",
          "original_form": "흠 그건 나도 공감해",
          "speaker_id": "1",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100036493.1.23",
          "form": "그런의미에서 홍삼캔디는 싫어",
          "original_form": "그런의미에서 홍삼캔디는 싫어",
          "speaker_id": "1",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100036493.1.24",
          "form": "캔디면 맛있어야지",
          "original_form": "캔디면 맛있어야지",
          "speaker_id": "1",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100036493.1.25",
          "form": "나도 싫어ㅠ",
          "original_form": "나도 싫어ㅠ",
          "speaker_id": "2",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100036493.1.26",
          "form": "그런건 효과도 엾고",
          "original_form": "그런건 효과도 엾고",
          "speaker_id": "2",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100036493.1.27",
          "form": "왜있는지 모르겟어",
          "original_form": "왜있는지 모르겟어",
          "speaker_id": "2",
          "time": "20210830 23:50"
        }
      ]
    }
  ]
