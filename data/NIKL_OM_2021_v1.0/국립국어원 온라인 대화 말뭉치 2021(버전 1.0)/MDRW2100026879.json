      "utterance": [
        {
          "id": "MDRW2100026879.1.1",
          "form": "ㅁ",
          "original_form": "ㅁ",
          "speaker_id": "1",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100026879.1.2",
          "form": "책 자주 읽으시나요",
          "original_form": "책 자주 읽으시나요",
          "speaker_id": "2",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100026879.1.3",
          "form": "어제 읽은 책은 불편한 편의점입니다",
          "original_form": "어제 읽은 책은 불편한 편의점입니다",
          "speaker_id": "1",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100026879.1.4",
          "form": "매일 읽어요",
          "original_form": "매일 읽어요",
          "speaker_id": "1",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026879.1.5",
          "form": "요즘 베스트셀러맞죠",
          "original_form": "요즘 베스트셀러맞죠",
          "speaker_id": "2",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026879.1.6",
          "form": "밀리의서재에 광고하더라구요",
          "original_form": "밀리의서재에 광고하더라구요",
          "speaker_id": "2",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026879.1.7",
          "form": "상위권에 있던데",
          "original_form": "상위권에 있던데",
          "speaker_id": "2",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026879.1.8",
          "form": "콩가루수사단도 재밌어요 소설입니다",
          "original_form": "콩가루수사단도 재밌어요 소설입니다",
          "speaker_id": "1",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026879.1.9",
          "form": "전 스릴러소설 좋아합니다",
          "original_form": "전 스릴러소설 좋아합니다",
          "speaker_id": "2",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026879.1.10",
          "form": "베스트셀러인가요?",
          "original_form": "베스트셀러인가요?",
          "speaker_id": "1",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026879.1.11",
          "form": "그런거같아요",
          "original_form": "그런거같아요",
          "speaker_id": "2",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026879.1.12",
          "form": "그만한 자격은 있는 소설입니다",
          "original_form": "그만한 자격은 있는 소설입니다",
          "speaker_id": "1",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026879.1.13",
          "form": "읽어보세요",
          "original_form": "읽어보세요",
          "speaker_id": "1",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026879.1.14",
          "form": "많이읽은 목록 베스트 10에 있더라구요",
          "original_form": "많이읽은 목록 베스트 10에 있더라구요",
          "speaker_id": "2",
          "time": "20210902 00:53"
        },
        {
          "id": "MDRW2100026879.1.15",
          "form": "소설은 한국소설이 재밋어요",
          "original_form": "소설은 한국소설이 재밋어요",
          "speaker_id": "1",
          "time": "20210902 00:53"
        },
        {
          "id": "MDRW2100026879.1.16",
          "form": "꿈백화점 읽으셧나요",
          "original_form": "꿈백화점 읽으셧나요",
          "speaker_id": "1",
          "time": "20210902 00:53"
        },
        {
          "id": "MDRW2100026879.1.17",
          "form": "저도 한국작가꺼 재밌게봤머요",
          "original_form": "저도 한국작가꺼 재밌게봤머요",
          "speaker_id": "2",
          "time": "20210902 00:53"
        },
        {
          "id": "MDRW2100026879.1.18",
          "form": "비슷한 전개입니다",
          "original_form": "비슷한 전개입니다",
          "speaker_id": "1",
          "time": "20210902 00:53"
        },
        {
          "id": "MDRW2100026879.1.19",
          "form": "읽으려다 못읽었어요",
          "original_form": "읽으려다 못읽었어요",
          "speaker_id": "2",
          "time": "20210902 00:53"
        },
        {
          "id": "MDRW2100026879.1.20",
          "form": "마녀식당으로 오세요ㅡ 이것도 비슷해요",
          "original_form": "마녀식당으로 오세요ㅡ 이것도 비슷해요",
          "speaker_id": "1",
          "time": "20210902 00:53"
        },
        {
          "id": "MDRW2100026879.1.21",
          "form": "도입부분이 제스타일이 아니어서",
          "original_form": "도입부분이 제스타일이 아니어서",
          "speaker_id": "2",
          "time": "20210902 00:53"
        }
