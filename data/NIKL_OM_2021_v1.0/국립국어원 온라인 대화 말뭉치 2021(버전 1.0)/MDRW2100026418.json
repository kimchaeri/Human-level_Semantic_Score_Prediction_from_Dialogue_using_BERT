      "utterance": [
        {
          "id": "MDRW2100026418.1.1",
          "form": "종교 있으세요?",
          "original_form": "종교 있으세요?",
          "speaker_id": "2",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026418.1.2",
          "form": "저는 무교에요",
          "original_form": "저는 무교에요",
          "speaker_id": "1",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026418.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026418.1.4",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026418.1.5",
          "form": "무교예요",
          "original_form": "무교예요",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026418.1.6",
          "form": "교회가보신적은 있나요?",
          "original_form": "교회가보신적은 있나요?",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026418.1.7",
          "form": "어릴때 가봤어요",
          "original_form": "어릴때 가봤어요",
          "speaker_id": "1",
          "time": "20210902 00:05"
        },
        {
          "id": "MDRW2100026418.1.8",
          "form": "name2님도 어릴때 가보신적 있나요?",
          "original_form": "&name2&님도 어릴때 가보신적 있나요?",
          "speaker_id": "1",
          "time": "20210902 00:06"
        },
        {
          "id": "MDRW2100026418.1.9",
          "form": "저도 어릴때 과자준대서 가봤었어요",
          "original_form": "저도 어릴때 과자준대서 가봤었어요",
          "speaker_id": "2",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100026418.1.10",
          "form": "저도요ㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100026418.1.11",
          "form": "달란트시장? 같은거 열렸을때 떡볶이 준다고 해서 갔어요",
          "original_form": "달란트시장? 같은거 열렸을때 떡볶이 준다고 해서 갔어요",
          "speaker_id": "1",
          "time": "20210902 00:12"
        },
        {
          "id": "MDRW2100026418.1.12",
          "form": "어릴때 역시 한번쯤 먹을것으로 유혹당하네요",
          "original_form": "어릴때 역시 한번쯤 먹을것으로 유혹당하네요",
          "speaker_id": "2",
          "time": "20210902 10:22"
        },
        {
          "id": "MDRW2100026418.1.13",
          "form": "절은가보셨나요?",
          "original_form": "절은가보셨나요?",
          "speaker_id": "2",
          "time": "20210902 10:22"
        },
        {
          "id": "MDRW2100026418.1.14",
          "form": "절도 어릴때 몇번 가봤던거 같아요",
          "original_form": "절도 어릴때 몇번 가봤던거 같아요",
          "speaker_id": "1",
          "time": "20210902 11:54"
        },
        {
          "id": "MDRW2100026418.1.15",
          "form": "님은 가보신적 있으세요?",
          "original_form": "님은 가보신적 있으세요?",
          "speaker_id": "1",
          "time": "20210902 11:54"
        },
        {
          "id": "MDRW2100026418.1.16",
          "form": "저는 등산가다 잠깐씩 들린적 있어요",
          "original_form": "저는 등산가다 잠깐씩 들린적 있어요",
          "speaker_id": "2",
          "time": "20210902 11:59"
        },
        {
          "id": "MDRW2100026418.1.17",
          "form": "산속이다보니 풍경도 좋아서 한번씩 가봐요",
          "original_form": "산속이다보니 풍경도 좋아서 한번씩 가봐요",
          "speaker_id": "2",
          "time": "20210902 12:00"
        },
        {
          "id": "MDRW2100026418.1.18",
          "form": "그리고 산속에 있으니까 공기도 좋고 풍경도 예쁘더라고요!",
          "original_form": "그리고 산속에 있으니까 공기도 좋고 풍경도 예쁘더라고요!",
          "speaker_id": "1",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100026418.1.19",
          "form": "그죠! 가을에 낙엽질때가면 좋더라구요",
          "original_form": "그죠! 가을에 낙엽질때가면 좋더라구요",
          "speaker_id": "2",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100026418.1.20",
          "form": "조용하구 풍경좋구",
          "original_form": "조용하구 풍경좋구",
          "speaker_id": "2",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100026418.1.21",
          "form": "절에 가면 분위기가 평화로워서 좋아요",
          "original_form": "절에 가면 분위기가 평화로워서 좋아요",
          "speaker_id": "1",
          "time": "20210902 17:58"
        },
        {
          "id": "MDRW2100026418.1.22",
          "form": "그죠!",
          "original_form": "그죠!",
          "speaker_id": "2",
          "time": "20210902 17:59"
        }
      ]
