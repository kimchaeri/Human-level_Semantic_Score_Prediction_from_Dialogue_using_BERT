      "utterance": [
        {
          "id": "MDRW2100013458.1.1",
          "form": "안녕하세요~ 점심식사 하셨나요?",
          "original_form": "안녕하세요~ 점심식사 하셨나요?",
          "speaker_id": "1",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013458.1.2",
          "form": "네 방금 먹고왔어요~",
          "original_form": "네 방금 먹고왔어요~",
          "speaker_id": "2",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013458.1.3",
          "form": "라면ㅎㅎ",
          "original_form": "라면ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013458.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013458.1.5",
          "form": "저는 싸이버거세트 먹었어요 ㅎㅎ",
          "original_form": "저는 싸이버거세트 먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013458.1.6",
          "form": "둘다 인스턴트네요 ㅁㅁㅁㅁ",
          "original_form": "둘다 인스턴트네요 ㅁㅁㅁㅁ",
          "speaker_id": "1",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013458.1.7",
          "form": "ㅋㅋㅋㅋ그러게요",
          "original_form": "ㅋㅋㅋㅋ그러게요",
          "speaker_id": "2",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013458.1.8",
          "form": "무슨음식 좋아하세요",
          "original_form": "무슨음식 좋아하세요",
          "speaker_id": "2",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013458.1.9",
          "form": "저는 음,,, 저 회 진짜좋아해요 ㅎㅎ",
          "original_form": "저는 음,,, 저 회 진짜좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 14:36"
        },
        {
          "id": "MDRW2100013458.1.10",
          "form": "회 맛있죠!! 저도 해물 좋아해요",
          "original_form": "회 맛있죠!! 저도 해물 좋아해요",
          "speaker_id": "2",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100013458.1.11",
          "form": "무슨회 자주드세요",
          "original_form": "무슨회 자주드세요",
          "speaker_id": "2",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100013458.1.12",
          "form": "광어가 제일 접하기 쉬워서 ㅎㅎ 광어많이먹구요",
          "original_form": "광어가 제일 접하기 쉬워서 ㅎㅎ 광어많이먹구요",
          "speaker_id": "1",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100013458.1.13",
          "form": "도미도 자주먹는거같아요",
          "original_form": "도미도 자주먹는거같아요",
          "speaker_id": "1",
          "time": "20210825 14:37"
        },
        {
          "id": "MDRW2100013458.1.14",
          "form": "아항 저는 쫄깃쫄깃한게 좋더라구요",
          "original_form": "아항 저는 쫄깃쫄깃한게 좋더라구요",
          "speaker_id": "2",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100013458.1.15",
          "form": "얘기하니까 먹고싶네요",
          "original_form": "얘기하니까 먹고싶네요",
          "speaker_id": "2",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100013458.1.16",
          "form": "잘 기억안나는데 좀 비싼것중에서 아삭한식감나는 회가 있었던거같은데 ㅠㅠ 이름이기억이안나네요 그거진짜맛있었는데",
          "original_form": "잘 기억안나는데 좀 비싼것중에서 아삭한식감나는 회가 있었던거같은데 ㅠㅠ 이름이기억이안나네요 그거진짜맛있었는데",
          "speaker_id": "1",
          "time": "20210825 14:39"
        },
        {
          "id": "MDRW2100013458.1.17",
          "form": "비싼건 다 값을하나봐요",
          "original_form": "비싼건 다 값을하나봐요",
          "speaker_id": "2",
          "time": "20210825 14:41"
        },
        {
          "id": "MDRW2100013458.1.18",
          "form": "오랜만에 회먹고싶네요 2주전에 먹기도했는데 ㅋㅋㅋ",
          "original_form": "오랜만에 회먹고싶네요 2주전에 먹기도했는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:41"
        },
        {
          "id": "MDRW2100013458.1.19",
          "form": "2주면 오랜만ㅋㅋㅋㅋ",
          "original_form": "2주면 오랜만ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:42"
        },
        {
          "id": "MDRW2100013458.1.20",
          "form": "2주면 오랜만이에요 ㅋㅋㅋㅋ 주말에 회먹어야겠어요!",
          "original_form": "2주면 오랜만이에요 ㅋㅋㅋㅋ 주말에 회먹어야겠어요!",
