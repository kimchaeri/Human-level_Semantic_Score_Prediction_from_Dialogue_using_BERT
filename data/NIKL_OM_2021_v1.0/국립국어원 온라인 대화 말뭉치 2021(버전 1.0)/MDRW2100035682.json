      "utterance": [
        {
          "id": "MDRW2100035682.1.1",
          "form": "아 연습하면되지",
          "original_form": "아 연습하면되지",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100035682.1.2",
          "form": "첨부터잘하는사람이 어딧노",
          "original_form": "첨부터잘하는사람이 어딧노",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100035682.1.3",
          "form": "아니 지금은 여유도없고",
          "original_form": "아니 지금은 여유도없고",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100035682.1.4",
          "form": "엄마가 성질이급해서",
          "original_form": "엄마가 성질이급해서",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100035682.1.5",
          "form": "음 그런방향을 염두해두면서",
          "original_form": "음 그런방향을 염두해두면서",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100035682.1.6",
          "form": "걍 본인이 해야함",
          "original_form": "걍 본인이 해야함",
          "speaker_id": "1",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100035682.1.7",
          "form": "나이가면 ㅋㅋ 좀씩 되지않을까",
          "original_form": "나이가면 ㅋㅋ 좀씩 되지않을까",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100035682.1.8",
          "form": "난 사과깎기 좋아함 아빠가 손으로 하는거 잘해서 닮음",
          "original_form": "난 사과깎기 좋아함 아빠가 손으로 하는거 잘해서 닮음",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100035682.1.9",
          "form": "그리고 본인의 스타일이있음",
          "original_form": "그리고 본인의 스타일이있음",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100035682.1.10",
          "form": "다잘하는건아니고ㅋ",
          "original_form": "다잘하는건아니고ㅋ",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100035682.1.11",
          "form": "나를보면 복장터져죽으려함",
          "original_form": "나를보면 복장터져죽으려함",
          "speaker_id": "1",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100035682.1.12",
          "form": "아 그건 별로안좋은디",
          "original_form": "아 그건 별로안좋은디",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100035682.1.13",
          "form": "ㅋㅋㅋ 힘들군. 기회를 주세요 해봐",
          "original_form": "ㅋㅋㅋ 힘들군. 기회를 주세요 해봐",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100035682.1.14",
          "form": "근데 난 게을러",
          "original_form": "근데 난 게을러",
          "speaker_id": "1",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035682.1.15",
          "form": "그리고 미뤘다해 ㅋㅋ",
          "original_form": "그리고 미뤘다해 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035682.1.16",
          "form": "음ㅋ 나도..",
          "original_form": "음ㅋ 나도..",
          "speaker_id": "2",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100035682.1.17",
          "form": "그래서 결국엄마가함",
          "original_form": "그래서 결국엄마가함",
          "speaker_id": "1",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100035682.1.18",
          "form": "너와 나는 MBTI가 같아서.. 동질감을 느낀다",
          "original_form": "너와 나는 MBTI가 같아서.. 동질감을 느낀다",
          "speaker_id": "2",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100035682.1.19",
          "form": "불효녀다ㅜ",
          "original_form": "불효녀다ㅜ",
          "speaker_id": "1",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100035682.1.20",
          "form": "지금도 필사적으로 미루고있지..ㅋ",
          "original_form": "지금도 필사적으로 미루고있지..ㅋ",
          "speaker_id": "2",
          "time": "20210829 22:23"
        },
        {
          "id": "MDRW2100035682.1.21",
          "form": "하아...",
