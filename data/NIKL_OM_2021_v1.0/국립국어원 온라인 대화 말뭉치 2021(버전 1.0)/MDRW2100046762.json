      "utterance": [
        {
          "id": "MDRW2100046762.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211104 19:33"
        },
        {
          "id": "MDRW2100046762.1.2",
          "form": "안녕하세요! 뭐하고계세요?",
          "original_form": "안녕하세요! 뭐하고계세요?",
          "speaker_id": "2",
          "time": "20211104 20:57"
        },
        {
          "id": "MDRW2100046762.1.3",
          "form": "놀아요",
          "original_form": "놀아요",
          "speaker_id": "1",
          "time": "20211104 20:57"
        },
        {
          "id": "MDRW2100046762.1.4",
          "form": "저녁 먹었어요?",
          "original_form": "저녁 먹었어요?",
          "speaker_id": "1",
          "time": "20211104 20:58"
        },
        {
          "id": "MDRW2100046762.1.5",
          "form": "뭐하고요? ㅋㅋㅋ",
          "original_form": "뭐하고요? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 20:58"
        },
        {
          "id": "MDRW2100046762.1.6",
          "form": "인터넷 서치",
          "original_form": "인터넷 서치",
          "speaker_id": "1",
          "time": "20211104 20:58"
        },
        {
          "id": "MDRW2100046762.1.7",
          "form": "네! 전 샐러드 먹었어요!",
          "original_form": "네! 전 샐러드 먹었어요!",
          "speaker_id": "2",
          "time": "20211104 20:58"
        },
        {
          "id": "MDRW2100046762.1.8",
          "form": "오 어떤 분야 관심 있으세요?",
          "original_form": "오 어떤 분야 관심 있으세요?",
          "speaker_id": "2",
          "time": "20211104 20:58"
        },
        {
          "id": "MDRW2100046762.1.9",
          "form": "그게 밥이 되나요?",
          "original_form": "그게 밥이 되나요?",
          "speaker_id": "1",
          "time": "20211104 20:58"
        },
        {
          "id": "MDRW2100046762.1.10",
          "form": "지금 겨울옷 사려고요",
          "original_form": "지금 겨울옷 사려고요",
          "speaker_id": "1",
          "time": "20211104 20:58"
        },
        {
          "id": "MDRW2100046762.1.11",
          "form": "헉 부럽다",
          "original_form": "헉 부럽다",
          "speaker_id": "2",
          "time": "20211104 20:58"
        },
        {
          "id": "MDRW2100046762.1.12",
          "form": "코트요?!",
          "original_form": "코트요?!",
          "speaker_id": "2",
          "time": "20211104 20:58"
        },
        {
          "id": "MDRW2100046762.1.13",
          "form": "아뇨 니트요",
          "original_form": "아뇨 니트요",
          "speaker_id": "1",
          "time": "20211104 20:59"
        },
        {
          "id": "MDRW2100046762.1.14",
          "form": "오늘 바쁘죠?",
          "original_form": "오늘 바쁘죠?",
          "speaker_id": "1",
          "time": "20211104 21:03"
        },
        {
          "id": "MDRW2100046762.1.15",
          "form": "네...ㅠ",
          "original_form": "네...ㅠ",
          "speaker_id": "2",
          "time": "20211104 21:05"
        },
        {
          "id": "MDRW2100046762.1.16",
          "form": "옷은 어디서 사요 보통?",
          "original_form": "옷은 어디서 사요 보통?",
          "speaker_id": "2",
          "time": "20211104 21:05"
        },
        {
          "id": "MDRW2100046762.1.17",
          "form": "인쇼나 아울렛이요",
          "original_form": "인쇼나 아울렛이요",
          "speaker_id": "1",
          "time": "20211104 21:06"
        },
        {
          "id": "MDRW2100046762.1.18",
          "form": "무신사 이런데 이용하세요?!",
          "original_form": "무신사 이런데 이용하세요?!",
          "speaker_id": "2",
          "time": "20211104 21:07"
        },
        {
          "id": "MDRW2100046762.1.19",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20211104 21:07"
        },
        {
          "id": "MDRW2100046762.1.20",
          "form": "무신사 좋죠 ㅎㅎ 저도 애용해요",
          "original_form": "무신사 좋죠 ㅎㅎ 저도 애용해요",
