      "utterance": [
        {
          "id": "MDRW2100028508.1.1",
          "form": "야 손톱 좀 잘라라",
          "original_form": "야 손톱 좀 잘라라",
          "speaker_id": "2",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100028508.1.2",
          "form": "아 오늘 깎으려고 했는데 까먹었어ㅠ",
          "original_form": "아 오늘 깎으려고 했는데 까먹었어ㅠ",
          "speaker_id": "1",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028508.1.3",
          "form": "손톱이 귀신같다 아주",
          "original_form": "손톱이 귀신같다 아주",
          "speaker_id": "2",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028508.1.4",
          "form": "엄마 발톱도 귀신같음",
          "original_form": "엄마 발톱도 귀신같음",
          "speaker_id": "1",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028508.1.5",
          "form": "엄만 너무 까다로워서 힘든데",
          "original_form": "엄만 너무 까다로워서 힘든데",
          "speaker_id": "1",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100028508.1.6",
          "form": "니 손톱",
          "original_form": "니 손톱",
          "speaker_id": "2",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100028508.1.7",
          "form": "깎을 때 내 것도 좀",
          "original_form": "깎을 때 내 것도 좀",
          "speaker_id": "2",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100028508.1.8",
          "form": "깎아 줘",
          "original_form": "깎아 줘",
          "speaker_id": "2",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100028508.1.9",
          "form": "엄만 근데 막 소리 지르면서 대서",
          "original_form": "엄만 근데 막 소리 지르면서 대서",
          "speaker_id": "1",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100028508.1.10",
          "form": "넘 힘들다고",
          "original_form": "넘 힘들다고",
          "speaker_id": "1",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100028508.1.11",
          "form": "ㅋㅋㅋ무서우니까 그러지",
          "original_form": "ㅋㅋㅋ무서우니까 그러지",
          "speaker_id": "2",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100028508.1.12",
          "form": "어련히 알아서 잘 깎을까",
          "original_form": "어련히 알아서 잘 깎을까",
          "speaker_id": "1",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100028508.1.13",
          "form": "너무 짧게 자름 안돼",
          "original_form": "너무 짧게 자름 안돼",
          "speaker_id": "2",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100028508.1.14",
          "form": "아 알아",
          "original_form": "아 알아",
          "speaker_id": "1",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100028508.1.15",
          "form": "봐봐 벌써 부터 잔소리한다",
          "original_form": "봐봐 벌써 부터 잔소리한다",
          "speaker_id": "1",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100028508.1.16",
          "form": "어쩔수없다고",
          "original_form": "어쩔수없다고",
          "speaker_id": "2",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100028508.1.17",
          "form": "소리가",
          "original_form": "소리가",
          "speaker_id": "2",
          "time": "20210902 21:44"
        },
        {
          "id": "MDRW2100028508.1.18",
          "form": "맙",
          "original_form": "맙",
          "speaker_id": "2",
          "time": "20210902 21:44"
        },
        {
          "id": "MDRW2100028508.1.19",
          "form": "막",
          "original_form": "막",
          "speaker_id": "2",
          "time": "20210902 21:44"
        },
        {
          "id": "MDRW2100028508.1.20",
          "form": "나온다",
          "original_form": "나온다",
          "speaker_id": "2",
          "time": "20210902 21:44"
        },
        {
          "id": "MDRW2100028508.1.21",
          "form": "그러니까 힘들다곸ㅋㅋ",
          "original_form": "그러니까 힘들다곸ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:44"
        },
        {
          "id": "MDRW2100028508.1.22",
          "form": "알았어",
          "original_form": "알았어",
          "speaker_id": "2",
          "time": "20210902 21:44"
        },
        {
          "id": "MDRW2100028508.1.23",
          "form": "잘 깎아줘봐",
          "original_form": "잘 깎아줘봐",
          "speaker_id": "2",
          "time": "20210902 21:45"
        },
        {
          "id": "MDRW2100028508.1.24",
          "form": "ㅇㅋ잔소리만 안하면",
          "original_form": "ㅇㅋ잔소리만 안하면",
