      "utterance": [
        {
          "id": "MDRW2100045364.1.1",
          "form": "아 ㅠㅠㅠ 줄여서 치유물 미쳐버려",
          "original_form": "아 ㅠㅠㅠ 줄여서 치유물 미쳐버려",
          "speaker_id": "2",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045364.1.2",
          "form": "겉 표지는 멀쩡",
          "original_form": "겉 표지는 멀쩡",
          "speaker_id": "1",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045364.1.3",
          "form": "나루타루에",
          "original_form": "나루타루에",
          "speaker_id": "1",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045364.1.4",
          "form": "....이런 설명에 혹하는 나, 망한건가요? 진짜 한번 보고싶네",
          "original_form": "....이런 설명에 혹하는 나, 망한건가요? 진짜 한번 보고싶네",
          "speaker_id": "2",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045364.1.5",
          "form": "그 표지에 불가사리 있잖아",
          "original_form": "그 표지에 불가사리 있잖아",
          "speaker_id": "1",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045364.1.6",
          "form": "응응ㅇ응",
          "original_form": "응응ㅇ응",
          "speaker_id": "2",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045364.1.7",
          "form": "응ㅇ응응 뭔가 마스코트처럼 생긴....",
          "original_form": "응ㅇ응응 뭔가 마스코트처럼 생긴....",
          "speaker_id": "2",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045364.1.8",
          "form": "걔가 진짜 만악의 근원임",
          "original_form": "걔가 진짜 만악의 근원임",
          "speaker_id": "1",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045364.1.9",
          "form": "볼거면 대충 무슨 요소만 나오는지 말해줄까?",
          "original_form": "볼거면 대충 무슨 요소만 나오는지 말해줄까?",
          "speaker_id": "1",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045364.1.10",
          "form": "예.........ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "예.........ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045364.1.11",
          "form": "응응 말해줘 ㅎ ㅏ...ㅋ.ㅋ..ㅋ.ㅋ.ㅋㅋㅋㅋ",
          "original_form": "응응 말해줘 ㅎ ㅏ...ㅋ.ㅋ..ㅋ.ㅋ.ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045364.1.12",
          "form": "가족간의 문제 부터 시작해서",
          "original_form": "가족간의 문제 부터 시작해서",
          "speaker_id": "1",
          "time": "20210908 02:08"
        },
        {
          "id": "MDRW2100045364.1.13",
          "form": "수위높은 이지메 묘사에",
          "original_form": "수위높은 이지메 묘사에",
          "speaker_id": "1",
          "time": "20210908 02:08"
        },
        {
          "id": "MDRW2100045364.1.14",
          "form": "잠깐 뭐.....음...가족간의 사랑도 나오고",
          "original_form": "잠깐 뭐.....음...가족간의 사랑도 나오고",
