      "utterance": [
        {
          "id": "MDRW2100024177.1.1",
          "form": "name2님은 직장인이신가요~?",
          "original_form": "&name2&님은 직장인이신가요~?",
          "speaker_id": "1",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100024177.1.2",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100024177.1.3",
          "form": "나이스타이밍이시네요 주제변경타임",
          "original_form": "나이스타이밍이시네요 주제변경타임",
          "speaker_id": "2",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100024177.1.4",
          "form": "네~",
          "original_form": "네~",
          "speaker_id": "2",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100024177.1.5",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100024177.1.6",
          "form": "올해까지만 하구",
          "original_form": "올해까지만 하구",
          "speaker_id": "2",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100024177.1.7",
          "form": "오 회사는 어떠세요?",
          "original_form": "오 회사는 어떠세요?",
          "speaker_id": "1",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100024177.1.8",
          "form": "앗넵!",
          "original_form": "앗넵!",
          "speaker_id": "1",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100024177.1.9",
          "form": "새로 구해야하는데",
          "original_form": "새로 구해야하는데",
          "speaker_id": "2",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100024177.1.10",
          "form": "아직은 생각하지않으려구요 ㅋㅋ",
          "original_form": "아직은 생각하지않으려구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100024177.1.11",
          "form": "피곤.. ㅋㅋㅋㅋㅋ",
          "original_form": "피곤.. ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100024177.1.12",
          "form": "지금회사 그럭저럭 괜찮긴해요",
          "original_form": "지금회사 그럭저럭 괜찮긴해요",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100024177.1.13",
          "form": "name1님은 어떤직업 선호하세요~?",
          "original_form": "&name1&님은 어떤직업 선호하세요~?",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100024177.1.14",
          "form": "ㅜㅜ뭘하시든 더 좋은데 가셨으면 좋겠어요!!",
          "original_form": "ㅜㅜ뭘하시든 더 좋은데 가셨으면 좋겠어요!!",
          "speaker_id": "1",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100024177.1.15",
          "form": "우왕 감사합니다 name1님",
          "original_form": "우왕 감사합니다 &name1&님",
          "speaker_id": "2",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100024177.1.16",
          "form": "전 정말 뭘해먹고 살아야할지",
          "original_form": "전 정말 뭘해먹고 살아야할지",
          "speaker_id": "1",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100024177.1.17",
          "form": "너무 고민입니다ㅎㅎ...ㅠ",
          "original_form": "너무 고민입니다ㅎㅎ...ㅠ",
          "speaker_id": "1",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024177.1.18",
          "form": "ㅋㅋㅋㅋ하..  정말 공감입니다....",
          "original_form": "ㅋㅋㅋㅋ하..  정말 공감입니다....",
          "speaker_id": "2",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024177.1.19",
          "form": "저엉말...",
          "original_form": "저엉말...",
          "speaker_id": "2",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024177.1.20",
          "form": "원하시는 분야가잇을까요?",
          "original_form": "원하시는 분야가잇을까요?",
          "speaker_id": "2",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024177.1.21",
          "form": "음 출판쪽을 원해서 저번학기 인턴을 해봤는데",
          "original_form": "음 출판쪽을 원해서 저번학기 인턴을 해봤는데",
          "speaker_id": "1",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024177.1.22",
          "form": "생각보다 힘들더라구요ㅎㅎ",
          "original_form": "생각보다 힘들더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024177.1.23",
          "form": "그래서 다른걸 알아보고 있어요!",
          "original_form": "그래서 다른걸 알아보고 있어요!",
          "speaker_id": "1",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024177.1.24",
          "form": "아 ~~~",
          "original_form": "아 ~~~",
          "speaker_id": "2",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024177.1.25",
          "form": "글을 좋아하시는군요??",
          "original_form": "글을 좋아하시는군요??",
          "speaker_id": "2",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024177.1.26",
          "form": "제 친구도 대학때 출판사를 준비한적이 있었어요",
          "original_form": "제 친구도 대학때 출판사를 준비한적이 있었어요",
          "speaker_id": "2",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100024177.1.27",
          "form": "오!!!",
          "original_form": "오!!!",
          "speaker_id": "1",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100024177.1.28",
          "form": "그래도 그렇게 경험을 해보시고 가까이 가보신게 너무 대단해요",
          "original_form": "그래도 그렇게 경험을 해보시고 가까이 가보신게 너무 대단해요",
          "speaker_id": "2",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100024177.1.29",
          "form": "쉬운일이결코 아니셨을텐데",
          "original_form": "쉬운일이결코 아니셨을텐데",
          "speaker_id": "2",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100024177.1.30",
          "form": "감사해요ㅠㅜㅠ 근데 이제 다른 걸 찾아야 할 것 같아요ㅠ",
          "original_form": "감사해요ㅠㅜㅠ 근데 이제 다른 걸 찾아야 할 것 같아요ㅠ",
          "speaker_id": "1",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100024177.1.31",
          "form": "잘하고 좋아하는 일을 하면 좋을텐데",
          "original_form": "잘하고 좋아하는 일을 하면 좋을텐데",
          "speaker_id": "1",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100024177.1.32",
          "form": "생각보다 그런걸 찾는데 어렵네요ㅎㅎ",
          "original_form": "생각보다 그런걸 찾는데 어렵네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100024177.1.33",
          "form": "출판쪽 어룝다하는거 들었어요 ㅠㅠ",
          "original_form": "출판쪽 어룝다하는거 들었어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100024177.1.34",
          "form": "그러게말립니다.. 저는 .. 음악을 하고싶어했어요",
          "original_form": "그러게말립니다.. 저는 .. 음악을 하고싶어했어요",
          "speaker_id": "2",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100024177.1.35",
          "form": "예술쪽은 참   .. 왜이리 막연하고 까마득한지 모르겠습니당",
          "original_form": "예술쪽은 참   .. 왜이리 막연하고 까마득한지 모르겠습니당",
          "speaker_id": "2",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100024177.1.36",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100024177.1.37",
          "form": "이런건 우선 취미로 하고..",
          "original_form": "이런건 우선 취미로 하고..",
          "speaker_id": "2",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100024177.1.38",
          "form": "음악 너무 멋있어요...(전 리코더도 잘 못부는 사람입니다ㅋㅋㅋㅋ",
          "original_form": "음악 너무 멋있어요...(전 리코더도 잘 못부는 사람입니다ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:06"
        },
        {
          "id": "MDRW2100024177.1.39",
          "form": "생계는.. 흘러가는대로 .. 할수있는걸로 찾아가야하나~ 해요",
          "original_form": "생계는.. 흘러가는대로 .. 할수있는걸로 찾아가야하나~ 해요",
          "speaker_id": "2",
          "time": "20210830 23:06"
        },
        {
          "id": "MDRW2100024177.1.40",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:06"
        },
        {
          "id": "MDRW2100024177.1.41",
          "form": "name1님 혹시 글 좋아하시면 일기처럼 에세이 써보시는거 어떠세요?",
          "original_form": "&name1&님 혹시 글 좋아하시면 일기처럼 에세이 써보시는거 어떠세요?",
          "speaker_id": "2",
          "time": "20210830 23:06"
        },
        {
          "id": "MDRW2100024177.1.42",
          "form": "소설쪽이시라면.. 짧은 단편선들...?",
          "original_form": "소설쪽이시라면.. 짧은 단편선들...?",
          "speaker_id": "2",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100024177.1.43",
          "form": "앗 쓰고 있는게 있긴한데",
          "original_form": "앗 쓰고 있는게 있긴한데",
          "speaker_id": "1",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100024177.1.44",
          "form": "어디 보여주기엔.....많이 부족하고 부끄러운",
          "original_form": "어디 보여주기엔.....많이 부족하고 부끄러운",
          "speaker_id": "1",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100024177.1.45",
          "form": "글들입니다ㅎㅎ.ㅠㅠㅜㅜ",
          "original_form": "글들입니다ㅎㅎ.ㅠㅠㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100024177.1.46",
          "form": "좋아하는거랑 잘하는건 넘 다른 것 같아요",
          "original_form": "좋아하는거랑 잘하는건 넘 다른 것 같아요",
