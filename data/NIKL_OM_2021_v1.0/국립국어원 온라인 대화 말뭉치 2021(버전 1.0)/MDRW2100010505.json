        },
        {
          "id": "MDRW2100010505.1.2",
          "form": "짬짬이 스트레칭이요",
          "original_form": "짬짬이 스트레칭이요",
          "speaker_id": "1",
          "time": "20210823 19:31"
        },
        {
          "id": "MDRW2100010505.1.3",
          "form": "와 좋은 습관이셔요",
          "original_form": "와 좋은 습관이셔요",
