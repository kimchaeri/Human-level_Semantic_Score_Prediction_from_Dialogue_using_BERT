      "utterance": [
        {
          "id": "MDRW2100029584.1.1",
          "form": "요시아일랜드",
          "original_form": "요시아일랜드",
          "speaker_id": "2",
          "time": "20210903 10:39"
        },
        {
          "id": "MDRW2100029584.1.2",
          "form": "기억나네요",
          "original_form": "기억나네요",
          "speaker_id": "2",
          "time": "20210903 10:39"
        },
        {
          "id": "MDRW2100029584.1.3",
          "form": "요시가 알 주렁주렁 달고다니죠",
          "original_form": "요시가 알 주렁주렁 달고다니죠",
          "speaker_id": "1",
          "time": "20210903 10:39"
        },
        {
          "id": "MDRW2100029584.1.4",
          "form": "알들고 움직이기",
          "original_form": "알들고 움직이기",
          "speaker_id": "2",
          "time": "20210903 10:39"
        },
        {
          "id": "MDRW2100029584.1.5",
          "form": "아기자기한 게임 좋아해요",
          "original_form": "아기자기한 게임 좋아해요",
          "speaker_id": "1",
          "time": "20210903 10:39"
        },
        {
          "id": "MDRW2100029584.1.6",
          "form": "알던지기도 하죠ㄷㄷㄷ",
          "original_form": "알던지기도 하죠ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210903 10:39"
        },
        {
          "id": "MDRW2100029584.1.7",
          "form": "방울쏘아서",
          "original_form": "방울쏘아서",
          "speaker_id": "2",
          "time": "20210903 10:39"
        },
        {
          "id": "MDRW2100029584.1.8",
          "form": "터뜰기도",
          "original_form": "터뜰기도",
          "speaker_id": "2",
          "time": "20210903 10:40"
        },
        {
          "id": "MDRW2100029584.1.9",
          "form": "기억나네요",
          "original_form": "기억나네요",
          "speaker_id": "2",
          "time": "20210903 10:40"
        },
        {
          "id": "MDRW2100029584.1.10",
          "form": "마리오카트가 최고에요",
          "original_form": "마리오카트가 최고에요",
          "speaker_id": "1",
          "time": "20210903 10:40"
        },
        {
          "id": "MDRW2100029584.1.11",
          "form": "폰겜중에",
          "original_form": "폰겜중에",
          "speaker_id": "2",
          "time": "20210903 10:40"
        },
        {
          "id": "MDRW2100029584.1.12",
          "form": "절묘한타이밍",
          "original_form": "절묘한타이밍",
          "speaker_id": "2",
          "time": "20210903 10:40"
        },
        {
          "id": "MDRW2100029584.1.13",
          "form": "카트게임인가요",
          "original_form": "카트게임인가요",
          "speaker_id": "1",
          "time": "20210903 10:40"
        },
        {
          "id": "MDRW2100029584.1.14",
          "form": "이라는게 있었는데",
          "original_form": "이라는게 있었는데",
          "speaker_id": "2",
          "time": "20210903 10:40"
        },
        {
          "id": "MDRW2100029584.1.15",
          "form": "순간적으로 누르는겜이요",
          "original_form": "순간적으로 누르는겜이요",
          "speaker_id": "2",
          "time": "20210903 10:40"
        },
        {
          "id": "MDRW2100029584.1.16",
          "form": "타이밍맞춰서",
          "original_form": "타이밍맞춰서",
          "speaker_id": "2",
          "time": "20210903 10:40"
        },
        {
          "id": "MDRW2100029584.1.17",
          "form": "오 재밌겠네요",
          "original_form": "오 재밌겠네요",
          "speaker_id": "1",
          "time": "20210903 10:41"
        },
        {
          "id": "MDRW2100029584.1.18",
          "form": "요즘그런게 없네요",
          "original_form": "요즘그런게 없네요",
          "speaker_id": "2",
          "time": "20210903 10:41"
        },
        {
          "id": "MDRW2100029584.1.19",
          "form": "무한의 계단",
          "original_form": "무한의 계단",
          "speaker_id": "2",
          "time": "20210903 10:41"
        },
        {
          "id": "MDRW2100029584.1.20",
          "form": "저는 경영게임도 좋아해요",
          "original_form": "저는 경영게임도 좋아해요",
          "speaker_id": "1",
          "time": "20210903 10:41"
        },
        {
          "id": "MDRW2100029584.1.21",
          "form": "해보셨나요",
          "original_form": "해보셨나요",
          "speaker_id": "2",
          "time": "20210903 10:41"
        },
        {
          "id": "MDRW2100029584.1.22",
          "form": "경영겜뭐가있죠",
          "original_form": "경영겜뭐가있죠",
          "speaker_id": "2",
          "time": "20210903 10:41"
        },
        {
          "id": "MDRW2100029584.1.23",
          "form": "식당이나 요리게임",
          "original_form": "식당이나 요리게임",
          "speaker_id": "1",
          "time": "20210903 10:41"
        },
        {
          "id": "MDRW2100029584.1.24",
          "form": "아 요리",
          "original_form": "아 요리",
          "speaker_id": "2",
          "time": "20210903 10:41"
        },
        {
          "id": "MDRW2100029584.1.25",
          "form": "안해봤어요?",
          "original_form": "안해봤어요?",
