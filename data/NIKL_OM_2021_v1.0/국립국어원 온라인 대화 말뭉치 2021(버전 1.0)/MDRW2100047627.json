      "utterance": [
        {
          "id": "MDRW2100047627.1.1",
          "form": "전 12시에",
          "original_form": "전 12시에",
          "speaker_id": "2",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100047627.1.2",
          "form": "쇼핑 ㅋㅋ했렀어요",
          "original_form": "쇼핑 ㅋㅋ했렀어요",
          "speaker_id": "2",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100047627.1.3",
          "form": "막 세일해서",
          "original_form": "막 세일해서",
          "speaker_id": "2",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100047627.1.4",
          "form": "안자고",
          "original_form": "안자고",
          "speaker_id": "2",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100047627.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ뭐사셧어요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ뭐사셧어요?",
          "speaker_id": "1",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100047627.1.6",
          "form": "매번 다르죠 ㅋㅋ",
          "original_form": "매번 다르죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:39"
        },
        {
          "id": "MDRW2100047627.1.7",
          "form": "싼거 아무거나",
          "original_form": "싼거 아무거나",
          "speaker_id": "2",
          "time": "20210826 22:39"
        },
        {
          "id": "MDRW2100047627.1.8",
          "form": "좀 중독이였러요 ㅋㅋ",
          "original_form": "좀 중독이였러요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:39"
        },
        {
          "id": "MDRW2100047627.1.9",
          "form": "100원 990원 ㅋㅋ",
          "original_form": "100원 990원 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:39"
        },
        {
          "id": "MDRW2100047627.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아니 100원하면 사야죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아니 100원하면 사야죠",
          "speaker_id": "1",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100047627.1.11",
          "form": "그헣죠 ㅋㅋ",
          "original_form": "그헣죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100047627.1.12",
          "form": "밤12시 조용하고 쇼핑문이 열리죠",
          "original_form": "밤12시 조용하고 쇼핑문이 열리죠",
          "speaker_id": "2",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100047627.1.13",
          "form": "그때 샀렀는데",
          "original_form": "그때 샀렀는데",
          "speaker_id": "2",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100047627.1.14",
          "form": "실패도 많이해요",
          "original_form": "실패도 많이해요",
          "speaker_id": "2",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100047627.1.15",
          "form": "경쟁이 심해서",
          "original_form": "경쟁이 심해서",
          "speaker_id": "2",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100047627.1.16",
          "form": "지금 귀뚜라미 기르던데 탈출해서 ㅡㅡ",
          "original_form": "지금 귀뚜라미 기르던데 탈출해서 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100047627.1.17",
          "form": "약샀어요 ㅜㅜ",
          "original_form": "약샀어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100047627.1.18",
          "form": "집에 막 돌아다녀서",
          "original_form": "집에 막 돌아다녀서",
          "speaker_id": "2",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100047627.1.19",
          "form": "쇼핑 언제 어느 시간에 하세요?",
          "original_form": "쇼핑 언제 어느 시간에 하세요?",
          "speaker_id": "2",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100047627.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ저도 밤에 하는것같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ저도 밤에 하는것같아요",
          "speaker_id": "1",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100047627.1.21",
          "form": "여기저기 둘러보다가 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "여기저기 둘러보다가 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100047627.1.22",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100047627.1.23",
          "form": "애들도 자고 편해요",
          "original_form": "애들도 자고 편해요",
          "speaker_id": "2",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100047627.1.24",
          "form": "귀뚜라미를 기르세요??",
          "original_form": "귀뚜라미를 기르세요??",
          "speaker_id": "1",
          "time": "20210826 22:48"
        },
        {
          "id": "MDRW2100047627.1.25",
          "form": "예전에 도마뱀 먹이로요",
          "original_form": "예전에 도마뱀 먹이로요",
          "speaker_id": "2",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100047627.1.26",
          "form": "몇마리 탈출했나봥",
          "original_form": "몇마리 탈출했나봥",
          "speaker_id": "2",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100047627.1.27",
          "form": "봐요",
          "original_form": "봐요",
          "speaker_id": "2",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100047627.1.28",
          "form": "바퀴약 샀어요 ㅜㅜ",
          "original_form": "바퀴약 샀어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100047627.1.29",
          "form": "ㅠㅠ아이쿠",
          "original_form": "ㅠㅠ아이쿠",
          "speaker_id": "1",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100047627.1.30",
          "form": "그거야말로 꼭 필요한 쇼핑이네요 ㅠㅠ",
          "original_form": "그거야말로 꼭 필요한 쇼핑이네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100047627.1.31",
          "form": "네 ㅜㅜ",
          "original_form": "네 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100047627.1.32",
          "form": "바퀴도 없는데 귀뚤이라",
          "original_form": "바퀴도 없는데 귀뚤이라",
          "speaker_id": "2",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100047627.1.33",
          "form": "계속 귀뚤귀뚤 울어요?",
          "original_form": "계속 귀뚤귀뚤 울어요?",
          "speaker_id": "1",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100047627.1.34",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100047627.1.35",
          "form": "소심하게 울어서 눈치 못챘어요 ㅜㅜ",
          "original_form": "소심하게 울어서 눈치 못챘어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100047627.1.36",
          "form": "이제 커서 소리나뇨",
          "original_form": "이제 커서 소리나뇨",
          "speaker_id": "2",
          "time": "20210826 22:52"
        },
        {
          "id": "MDRW2100047627.1.37",
          "form": "으악ㄷㄷ 자라난거군요",
          "original_form": "으악ㄷㄷ 자라난거군요",
          "speaker_id": "1",
          "time": "20210826 22:52"
        },
        {
          "id": "MDRW2100047627.1.38",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:52"
        },
        {
          "id": "MDRW2100047627.1.39",
          "form": "뭘먹고 큰건지",
          "original_form": "뭘먹고 큰건지",
          "speaker_id": "2",
