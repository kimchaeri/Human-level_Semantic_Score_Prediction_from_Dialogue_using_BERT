      "utterance": [
        {
          "id": "MDRW2100010945.1.1",
          "form": "요즘 날씨가 이상하죠",
          "original_form": "요즘 날씨가 이상하죠",
          "speaker_id": "1",
          "time": "20210823 23:09"
        },
        {
          "id": "MDRW2100010945.1.2",
          "form": "날은 선선해졌는데 비가 엄청 많이 오더라구요",
          "original_form": "날은 선선해졌는데 비가 엄청 많이 오더라구요",
          "speaker_id": "2",
          "time": "20210823 23:12"
        },
        {
          "id": "MDRW2100010945.1.3",
          "form": "맞아요ㅎㅎ 전 서울인데 비가 수요일까지 올것같더라고요",
          "original_form": "맞아요ㅎㅎ 전 서울인데 비가 수요일까지 올것같더라고요",
          "speaker_id": "1",
          "time": "20210823 23:13"
        },
        {
          "id": "MDRW2100010945.1.4",
          "form": "동남아기후가 되는 것 같아요",
          "original_form": "동남아기후가 되는 것 같아요",
          "speaker_id": "1",
          "time": "20210823 23:13"
        },
        {
          "id": "MDRW2100010945.1.5",
          "form": "진짜 이번 여름 동남아 같더라구요 ...",
          "original_form": "진짜 이번 여름 동남아 같더라구요 ...",
          "speaker_id": "2",
          "time": "20210823 23:17"
        },
        {
          "id": "MDRW2100010945.1.6",
          "form": "비도 엄청 오고 습하고 ..ㅠㅠ 힘들었어요",
          "original_form": "비도 엄청 오고 습하고 ..ㅠㅠ 힘들었어요",
          "speaker_id": "2",
          "time": "20210823 23:17"
        },
        {
          "id": "MDRW2100010945.1.7",
          "form": "맞아요ㅜㅜ 바지 다 젖고..",
          "original_form": "맞아요ㅜㅜ 바지 다 젖고..",
          "speaker_id": "1",
          "time": "20210823 23:22"
        },
        {
          "id": "MDRW2100010945.1.8",
          "form": "근데 또 우산 사면 금방 비가 그치더라구요 ㅋㅋㅋㅋ",
          "original_form": "근데 또 우산 사면 금방 비가 그치더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:22"
        },
        {
          "id": "MDRW2100010945.1.9",
          "form": "우산 사고 비 금방 그칠때 정말 화나죠..",
          "original_form": "우산 사고 비 금방 그칠때 정말 화나죠..",
          "speaker_id": "2",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100010945.1.10",
          "form": "집에 우산만 늘어요 ..ㅜㅜ",
          "original_form": "집에 우산만 늘어요 ..ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100010945.1.11",
          "form": "진짜요 ㅠ",
          "original_form": "진짜요 ㅠ",
          "speaker_id": "1",
          "time": "20210823 23:27"
        },
        {
          "id": "MDRW2100010945.1.12",
          "form": "장화 있으세요?",
          "original_form": "장화 있으세요?",
          "speaker_id": "1",
          "time": "20210823 23:27"
        },
        {
          "id": "MDRW2100010945.1.13",
          "form": "아뇨 장화있으면 진짜 편하다는데 .. 장만은 아직 못했네요",
          "original_form": "아뇨 장화있으면 진짜 편하다는데 .. 장만은 아직 못했네요",
          "speaker_id": "2",
          "time": "20210823 23:36"
        },
        {
          "id": "MDRW2100010945.1.14",
          "form": "name1님은 장화 있으신가요?",
          "original_form": "&name1&님은 장화 있으신가요?",
          "speaker_id": "2",
          "time": "20210823 23:36"
        },
        {
          "id": "MDRW2100010945.1.15",
          "form": "저도 아직 장만 못했어요 ㅎㅎ",
          "original_form": "저도 아직 장만 못했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100010945.1.16",
          "form": "뭔가 하나 사려면 좋은걸로 사야할 것 같으면서..",
          "original_form": "뭔가 하나 사려면 좋은걸로 사야할 것 같으면서..",
          "speaker_id": "1",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100010945.1.17",
          "form": "그 가격은 너무 비싼가 싶기도하고 그렇더라구여 ㅋㅋ",
          "original_form": "그 가격은 너무 비싼가 싶기도하고 그렇더라구여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:39"
        },
        {
          "id": "MDRW2100010945.1.18",
          "form": "맞아요 .. 신을 수 있는 날이 적다보니까 고민하게 되더라구요ㅎㅎ",
          "original_form": "맞아요 .. 신을 수 있는 날이 적다보니까 고민하게 되더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 23:41"
        },
        {
          "id": "MDRW2100010945.1.19",
          "form": "이제는 그래도 비가 어느정도 그치면",
          "original_form": "이제는 그래도 비가 어느정도 그치면",
          "speaker_id": "1",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100010945.1.20",
          "form": "금방 가을이 되겠어요 ㅎㅎ",
          "original_form": "금방 가을이 되겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100010945.1.21",
          "form": "가을 너무 좋아요ㅎㅎ",
          "original_form": "가을 너무 좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 23:47"
        },
        {
          "id": "MDRW2100010945.1.22",
          "form": "제일 좋아하는 계절이 가을이에요!",
          "original_form": "제일 좋아하는 계절이 가을이에요!",
          "speaker_id": "2",
          "time": "20210823 23:47"
        },
        {
          "id": "MDRW2100010945.1.23",
          "form": "저도 가을 좋아요,,,",
          "original_form": "저도 가을 좋아요,,,",
          "speaker_id": "1",
          "time": "20210824 00:03"
        },
        {
          "id": "MDRW2100010945.1.24",
          "form": "가을에 고궁 걷는 거 낭만 그 자체,,",
          "original_form": "가을에 고궁 걷는 거 낭만 그 자체,,",
          "speaker_id": "1",
          "time": "20210824 00:03"
        },
        {
          "id": "MDRW2100010945.1.25",
          "form": "거기에 단풍까지 딱 .. 완벽하네요",
          "original_form": "거기에 단풍까지 딱 .. 완벽하네요",
          "speaker_id": "2",
          "time": "20210824 00:05"
        },
        {
          "id": "MDRW2100010945.1.26",
          "form": "가을 날씨가 선선해서 산책하는거 넘 좋아요 !",
          "original_form": "가을 날씨가 선선해서 산책하는거 넘 좋아요 !",
          "speaker_id": "2",
          "time": "20210824 00:05"
        }
      ]
    }
