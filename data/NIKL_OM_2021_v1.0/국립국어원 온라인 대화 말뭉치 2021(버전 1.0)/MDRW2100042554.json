          "form": "ㄹㅇ......",
          "original_form": "ㄹㅇ......",
          "speaker_id": "2",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100042554.1.5",
          "form": "내가 3년간",
          "original_form": "내가 3년간",
          "speaker_id": "2",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100042554.1.6",
          "form": "하루도 출석을",
          "original_form": "하루도 출석을",
          "speaker_id": "2",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100042554.1.7",
          "form": "5500원짜리는 질러줬는데",
          "original_form": "5500원짜리는 질러줬는데",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100042554.1.8",
          "form": "안빼먹었는데",
          "original_form": "안빼먹었는데",
          "speaker_id": "2",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100042554.1.9",
          "form": "그니까 ㅎ",
          "original_form": "그니까 ㅎ",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100042554.1.10",
          "form": "근데 정떨어지고나니까",
          "original_form": "근데 정떨어지고나니까",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100042554.1.11",
          "form": "걍 접속을 거의 안하게 됨..",
          "original_form": "걍 접속을 거의 안하게 됨..",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100042554.1.12",
          "form": "ㅇㅇ.....귀신같이",
          "original_form": "ㅇㅇ.....귀신같이",
          "speaker_id": "2",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100042554.1.13",
          "form": "3일에 한번",
          "original_form": "3일에 한번",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042554.1.14",
          "form": "5일에 한번",
          "original_form": "5일에 한번",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042554.1.15",
          "form": "그러고 있더라구.....",
          "original_form": "그러고 있더라구.....",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042554.1.16",
          "form": "내가 곧",
          "original_form": "내가 곧",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042554.1.17",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042554.1.18",
          "form": "해바라기락도",
          "original_form": "해바라기락도",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042554.1.19",
          "form": "받을수있다고",
          "original_form": "받을수있다고",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042554.1.20",
          "form": "을마나 좋아햇는데",
          "original_form": "을마나 좋아햇는데",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042554.1.21",
          "form": "납븐자식들......",
          "original_form": "납븐자식들......",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042554.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042554.1.23",
          "form": "ㅎ 그니까",
          "original_form": "ㅎ 그니까",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042554.1.24",
          "form": "근데 ㅎ 그걸 그때까지도 못받은게",
          "original_form": "근데 ㅎ 그걸 그때까지도 못받은게",
          "speaker_id": "2",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042554.1.25",
          "form": "과금텀이 길어지고있단",
          "original_form": "과금텀이 길어지고있단",
          "speaker_id": "2",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042554.1.26",
          "form": "얘기였지....",
          "original_form": "얘기였지....",
