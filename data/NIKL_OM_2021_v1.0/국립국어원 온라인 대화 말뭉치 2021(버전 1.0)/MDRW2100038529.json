        {
          "id": "MDRW2100038529.1.1",
          "form": "ㅋㅋㅋㅋ직접 만든 퀴즈인가요",
          "original_form": "ㅋㅋㅋㅋ직접 만든 퀴즈인가요",
          "speaker_id": "2",
          "time": "20210902 15:25"
        },
        {
          "id": "MDRW2100038529.1.2",
          "form": "사과 몸에 좋으니깐 먹는게 남는거니깐",
          "original_form": "사과 몸에 좋으니깐 먹는게 남는거니깐",
          "speaker_id": "1",
          "time": "20210902 15:25"
        },
        {
          "id": "MDRW2100038529.1.3",
          "form": "자주먹어줘야죠",
          "original_form": "자주먹어줘야죠",
          "speaker_id": "1",
          "time": "20210902 15:25"
        },
        {
          "id": "MDRW2100038529.1.4",
          "form": "ㅋㅋㅋㅋ넼ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ넼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100038529.1.5",
          "form": "저도 들은얘기라서 써봤어요ㅋㄱㅋㄱ",
          "original_form": "저도 들은얘기라서 써봤어요ㅋㄱㅋㄱ",
          "speaker_id": "1",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100038529.1.6",
          "form": "너무재미있길래요",
          "original_form": "너무재미있길래요",
          "speaker_id": "1",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100038529.1.7",
          "form": "신선해욬ㅋㅋㅋㅋ",
          "original_form": "신선해욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100038529.1.8",
          "form": "그쵸? 사과맛있네욕ㅋㅋㅋ",
          "original_form": "그쵸? 사과맛있네욕ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100038529.1.9",
          "form": "전 지금 미니약과 먹는데",
          "original_form": "전 지금 미니약과 먹는데",
          "speaker_id": "2",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100038529.1.10",
          "form": "은근 땡기네요 이거",
          "original_form": "은근 땡기네요 이거",
          "speaker_id": "2",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100038529.1.11",
          "form": "미니약과맛있어요",
          "original_form": "미니약과맛있어요",
          "speaker_id": "1",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100038529.1.12",
          "form": "그쵸?",
          "original_form": "그쵸?",
          "speaker_id": "1",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100038529.1.13",
          "form": "미니약과 편의점에 파는거 봤는데",
          "original_form": "미니약과 편의점에 파는거 봤는데",
          "speaker_id": "1",
          "time": "20210902 15:46"
        },
        {
          "id": "MDRW2100038529.1.14",
          "form": "나중에사먹어봐야겠다",
          "original_form": "나중에사먹어봐야겠다",
          "speaker_id": "1",
          "time": "20210902 15:46"
        },
        {
          "id": "MDRW2100038529.1.15",
          "form": "ㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210902 15:46"
        },
        {
          "id": "MDRW2100038529.1.16",
          "form": "넹 저두 어제 편의점에서 삿어요",
          "original_form": "넹 저두 어제 편의점에서 삿어요",
          "speaker_id": "2",
          "time": "20210902 15:48"
        },
        {
          "id": "MDRW2100038529.1.17",
          "form": "제가 생각한 미니약과가 맞겠네요",
          "original_form": "제가 생각한 미니약과가 맞겠네요",
          "speaker_id": "1",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100038529.1.18",
          "form": "넹넹 어제 첨 사먹어봣는데 앞으로 자주 먹을거 같아요 ㄴㅋㅋ",
          "original_form": "넹넹 어제 첨 사먹어봣는데 앞으로 자주 먹을거 같아요 ㄴㅋㅋ",
          "speaker_id": "2",
