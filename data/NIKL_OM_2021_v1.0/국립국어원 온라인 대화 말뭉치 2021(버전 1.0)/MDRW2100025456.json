      "utterance": [
        {
          "id": "MDRW2100025456.1.1",
          "form": "영화를 자주 보시나요?",
          "original_form": "영화를 자주 보시나요?",
          "speaker_id": "1",
          "time": "20210901 11:41"
        },
        {
          "id": "MDRW2100025456.1.2",
          "form": "영화값이 많이 올라서 잘안봐요",
          "original_form": "영화값이 많이 올라서 잘안봐요",
          "speaker_id": "2",
          "time": "20210901 11:41"
        },
        {
          "id": "MDRW2100025456.1.3",
          "form": "영화좋아하시나요",
          "original_form": "영화좋아하시나요",
          "speaker_id": "1",
          "time": "20210901 11:42"
        },
        {
          "id": "MDRW2100025456.1.4",
          "form": "자주 보세요?",
          "original_form": "자주 보세요?",
          "speaker_id": "2",
          "time": "20210901 11:42"
        },
        {
          "id": "MDRW2100025456.1.5",
          "form": "네네 할인 이런것들로 열씨미 보는편이예요",
          "original_form": "네네 할인 이런것들로 열씨미 보는편이예요",
          "speaker_id": "1",
          "time": "20210901 11:42"
        },
        {
          "id": "MDRW2100025456.1.6",
          "form": "최근에 싱크홀 봤는데",
          "original_form": "최근에 싱크홀 봤는데",
          "speaker_id": "1",
          "time": "20210901 11:42"
        },
        {
          "id": "MDRW2100025456.1.7",
          "form": "보셨나요??",
          "original_form": "보셨나요??",
          "speaker_id": "1",
          "time": "20210901 11:42"
        },
        {
          "id": "MDRW2100025456.1.8",
          "form": "아니요! 재난영화라고 듣기만했어요",
          "original_form": "아니요! 재난영화라고 듣기만했어요",
          "speaker_id": "2",
          "time": "20210901 11:44"
        },
        {
          "id": "MDRW2100025456.1.9",
          "form": "어떠셨어요?",
          "original_form": "어떠셨어요?",
          "speaker_id": "2",
          "time": "20210901 11:44"
        },
        {
          "id": "MDRW2100025456.1.10",
          "form": "전진짜재미있게봤어요",
          "original_form": "전진짜재미있게봤어요",
          "speaker_id": "1",
          "time": "20210901 11:45"
        },
        {
          "id": "MDRW2100025456.1.11",
          "form": "이광수랑 차승원이 다했어요 ㅋㅋㅋ",
          "original_form": "이광수랑 차승원이 다했어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:45"
        },
        {
          "id": "MDRW2100025456.1.12",
          "form": "킬링타임으로 좋나봐요ㅋㅋㅋㅋ",
          "original_form": "킬링타임으로 좋나봐요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:47"
        },
        {
          "id": "MDRW2100025456.1.13",
          "form": "엑시트랑 비교하면 어때요?",
          "original_form": "엑시트랑 비교하면 어때요?",
          "speaker_id": "2",
          "time": "20210901 11:48"
        },
        {
          "id": "MDRW2100025456.1.14",
          "form": "비교도 안되요 진짜 꿀잼이예요 ㅠㅠ",
          "original_form": "비교도 안되요 진짜 꿀잼이예요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 11:48"
        },
        {
          "id": "MDRW2100025456.1.15",
          "form": "차승원이 바보같은 캐릭으로 나와요!!",
          "original_form": "차승원이 바보같은 캐릭으로 나와요!!",
          "speaker_id": "1",
          "time": "20210901 11:48"
        },
        {
          "id": "MDRW2100025456.1.16",
          "form": "혼영 좋아하시나요?",
          "original_form": "혼영 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210901 11:48"
        },
        {
          "id": "MDRW2100025456.1.17",
          "form": "혼영 좋아해요 잔잔한영화는 혼영이 더 좋아요",
          "original_form": "혼영 좋아해요 잔잔한영화는 혼영이 더 좋아요",
          "speaker_id": "2",
          "time": "20210901 11:49"
        },
        {
          "id": "MDRW2100025456.1.18",
          "form": "저도 혼자 보는거좋아해요",
          "original_form": "저도 혼자 보는거좋아해요",
          "speaker_id": "1",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025456.1.19",
          "form": "어떤 영화 장르 좋아하세요?",
          "original_form": "어떤 영화 장르 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025456.1.20",
          "form": "저는 가리지않아요",
          "original_form": "저는 가리지않아요",
          "speaker_id": "1",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025456.1.21",
          "form": "근데 좀비나 무서운건 좋아하긴하는데 무서워해요 ㅋㅋ",
          "original_form": "근데 좀비나 무서운건 좋아하긴하는데 무서워해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025456.1.22",
          "form": "도라에몽도 혼자봤어요!",
          "original_form": "도라에몽도 혼자봤어요!",
          "speaker_id": "1",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025456.1.23",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100025456.1.24",
          "form": "영화 엄청 좋아하시나보다!",
          "original_form": "영화 엄청 좋아하시나보다!",
          "speaker_id": "2",
          "time": "20210901 12:03"
        },
        {
          "id": "MDRW2100025456.1.25",
          "form": "맞아요 도라에몽은 좀 민망하더라구요",
          "original_form": "맞아요 도라에몽은 좀 민망하더라구요",
