      "utterance": [
        {
          "id": "MDRW2100046087.1.1",
          "form": "배달 음식 자주 드세요?",
          "original_form": "배달 음식 자주 드세요?",
          "speaker_id": "2",
          "time": "20210908 10:19"
        },
        {
          "id": "MDRW2100046087.1.2",
          "form": "자주먹어여ㅜ",
          "original_form": "자주먹어여ㅜ",
          "speaker_id": "1",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100046087.1.3",
          "form": "전 포장이요",
          "original_form": "전 포장이요",
          "speaker_id": "2",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100046087.1.4",
          "form": "오 전 귀차니즘이라서",
          "original_form": "오 전 귀차니즘이라서",
          "speaker_id": "1",
          "time": "20210908 10:27"
        },
        {
          "id": "MDRW2100046087.1.5",
          "form": "배달비가 은근 아깝더라구요",
          "original_form": "배달비가 은근 아깝더라구요",
          "speaker_id": "2",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046087.1.6",
          "form": "맞아여ㅜㅜ",
          "original_form": "맞아여ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046087.1.7",
          "form": "근데 포장은 ㅜㅜ 멀고귀찮아여",
          "original_form": "근데 포장은 ㅜㅜ 멀고귀찮아여",
          "speaker_id": "1",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046087.1.8",
          "form": "날씨 안 좋을 땐 엄청 귀찮죠 전 운동 겸 다녀와요 ㅜㅋㅋ",
          "original_form": "날씨 안 좋을 땐 엄청 귀찮죠 전 운동 겸 다녀와요 ㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046087.1.9",
          "form": "부지런하세여ㅜㅜ",
          "original_form": "부지런하세여ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046087.1.10",
          "form": "점심엔 머드시나요",
          "original_form": "점심엔 머드시나요",
          "speaker_id": "1",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046087.1.11",
          "form": "빵 먹을 것 같아요",
          "original_form": "빵 먹을 것 같아요",
          "speaker_id": "2",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100046087.1.12",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100046087.1.13",
          "form": "저는 그냥 백반이욥",
          "original_form": "저는 그냥 백반이욥",
          "speaker_id": "1",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100046087.1.14",
          "form": "직원식당서ㅜㅜ",
          "original_form": "직원식당서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 10:33"
        },
        {
          "id": "MDRW2100046087.1.15",
          "form": "그래도 밥이 든든하죠 ㅎㅎ",
          "original_form": "그래도 밥이 든든하죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100046087.1.16",
          "form": "맛이 그닥이에여 ㅎㅎ",
          "original_form": "맛이 그닥이에여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100046087.1.17",
          "form": "그래도 잘먹슴다",
          "original_form": "그래도 잘먹슴다",
          "speaker_id": "1",
          "time": "20210908 10:34"
        },
        {
          "id": "MDRW2100046087.1.18",
          "form": "전 밥보다 빵 좋아해서 ㅜ 빵이 주식이면 좋겠어요",
          "original_form": "전 밥보다 빵 좋아해서 ㅜ 빵이 주식이면 좋겠어요",
          "speaker_id": "2",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100046087.1.19",
          "form": "빵순이시군욧!!!",
          "original_form": "빵순이시군욧!!!",
          "speaker_id": "1",
          "time": "20210908 10:37"
        }
      ]
