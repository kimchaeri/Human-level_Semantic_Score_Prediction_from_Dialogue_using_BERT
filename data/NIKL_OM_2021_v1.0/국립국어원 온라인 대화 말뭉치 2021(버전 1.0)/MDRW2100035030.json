          "speaker_id": "2",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100035030.1.8",
          "form": "인데요",
          "original_form": "인데요",
          "speaker_id": "2",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100035030.1.9",
          "form": "왜 저한테 집중을안하시지",
          "original_form": "왜 저한테 집중을안하시지",
          "speaker_id": "2",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100035030.1.10",
          "form": "집중랬어요",
          "original_form": "집중랬어요",
          "speaker_id": "1",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100035030.1.11",
          "form": "증거가 어딧는지",
          "original_form": "증거가 어딧는지",
          "speaker_id": "2",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100035030.1.12",
          "form": "믿으세요",
          "original_form": "믿으세요",
          "speaker_id": "1",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100035030.1.13",
          "form": "아멘",
          "original_form": "아멘",
          "speaker_id": "2",
          "time": "20210829 00:40"
        },
        {
          "id": "MDRW2100035030.1.14",
          "form": "전 요즘 빌리노래 좋아요",
          "original_form": "전 요즘 빌리노래 좋아요",
          "speaker_id": "1",
          "time": "20210829 01:16"
        },
        {
          "id": "MDRW2100035030.1.15",
          "form": "오 그러시구나",
          "original_form": "오 그러시구나",
          "speaker_id": "2",
          "time": "20210829 01:16"
        },
        {
          "id": "MDRW2100035030.1.16",
          "form": "그리거요?",
          "original_form": "그리거요?",
          "speaker_id": "2",
          "time": "20210829 01:16"
        },
        {
          "id": "MDRW2100035030.1.17",
          "form": "저는 블랙아이드피스 hit it",
          "original_form": "저는 블랙아이드피스 hit it",
          "speaker_id": "2",
          "time": "20210829 01:16"
        },
        {
          "id": "MDRW2100035030.1.18",
          "form": "티나셰노래",
