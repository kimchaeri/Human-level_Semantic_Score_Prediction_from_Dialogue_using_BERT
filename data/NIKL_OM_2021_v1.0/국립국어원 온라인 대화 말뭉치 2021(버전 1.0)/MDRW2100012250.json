      "utterance": [
        {
          "id": "MDRW2100012250.1.1",
          "form": "좋아하는 스포츠가 있으신가요?",
          "original_form": "좋아하는 스포츠가 있으신가요?",
          "speaker_id": "1",
          "time": "20210824 20:42"
        },
        {
          "id": "MDRW2100012250.1.2",
          "form": "야구좀 봤는데 요샌 안봐요 name1은요?",
          "original_form": "야구좀 봤는데 요샌 안봐요 &name1&은요?",
          "speaker_id": "2",
          "time": "20210824 21:19"
        },
        {
          "id": "MDRW2100012250.1.3",
          "form": "저는 지금 야구 보고 있어요ㅋㅋ",
          "original_form": "저는 지금 야구 보고 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:21"
        },
        {
          "id": "MDRW2100012250.1.4",
          "form": "누가이기고있나요",
          "original_form": "누가이기고있나요",
          "speaker_id": "2",
          "time": "20210824 21:23"
        },
        {
          "id": "MDRW2100012250.1.5",
          "form": "한화 대 두산 경기 보고 있는데 두산이 4대 11로 이기고 있어요",
          "original_form": "한화 대 두산 경기 보고 있는데 두산이 4대 11로 이기고 있어요",
          "speaker_id": "1",
          "time": "20210824 21:24"
        },
        {
          "id": "MDRW2100012250.1.6",
          "form": "오 역시두산이잘하는군요",
          "original_form": "오 역시두산이잘하는군요",
          "speaker_id": "2",
          "time": "20210824 21:26"
        },
        {
          "id": "MDRW2100012250.1.7",
          "form": "ㅋㅋ 요즘은 띄엄띄엄 이기는 느낌이에요",
          "original_form": "ㅋㅋ 요즘은 띄엄띄엄 이기는 느낌이에요",
          "speaker_id": "1",
          "time": "20210824 21:26"
        },
        {
          "id": "MDRW2100012250.1.8",
          "form": "혹시 어느 팀 팬이신가요?",
          "original_form": "혹시 어느 팀 팬이신가요?",
          "speaker_id": "1",
          "time": "20210824 21:27"
        },
        {
          "id": "MDRW2100012250.1.9",
          "form": "롯..데...ㅜㅜㅜㅜㅡ",
          "original_form": "롯..데...ㅜㅜㅜㅜㅡ",
          "speaker_id": "2",
          "time": "20210824 21:29"
        },
        {
          "id": "MDRW2100012250.1.10",
          "form": "꼴지에요",
          "original_form": "꼴지에요",
          "speaker_id": "2",
          "time": "20210824 21:29"
        },
        {
          "id": "MDRW2100012250.1.11",
          "form": "밑에서 일등이죠",
          "original_form": "밑에서 일등이죠",
          "speaker_id": "2",
          "time": "20210824 21:30"
        },
        {
          "id": "MDRW2100012250.1.12",
          "form": "요즘은 8위일 거에요",
          "original_form": "요즘은 8위일 거에요",
          "speaker_id": "1",
          "time": "20210824 21:30"
        },
        {
          "id": "MDRW2100012250.1.13",
          "form": "두산은 7위ㅋㅋ",
          "original_form": "두산은 7위ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:30"
        },
        {
          "id": "MDRW2100012250.1.14",
          "form": "ㅋㅋㅋㅋ음 .. 분발해야겠네요 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ음 .. 분발해야겠네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:32"
        },
        {
          "id": "MDRW2100012250.1.15",
          "form": "요즘은 잘 못해도 그렇구나 싶어요ㅋㅋ",
          "original_form": "요즘은 잘 못해도 그렇구나 싶어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100012250.1.16",
          "form": "그냥 나쁜일만 안터지면 좋겠어요ㅎㅎ",
          "original_form": "그냥 나쁜일만 안터지면 좋겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 21:36"
        },
        {
          "id": "MDRW2100012250.1.17",
          "form": "ㅎㅎ 그러니까요. 올해 유독 바람 잘 날 없는 느낌이에요",
          "original_form": "ㅎㅎ 그러니까요. 올해 유독 바람 잘 날 없는 느낌이에요",
          "speaker_id": "1",
          "time": "20210824 21:36"
        },
        {
          "id": "MDRW2100012250.1.18",
          "form": "코로나라 더욱 그러네요",
          "original_form": "코로나라 더욱 그러네요",
          "speaker_id": "2",
          "time": "20210824 21:38"
        },
        {
          "id": "MDRW2100012250.1.19",
          "form": "아무래도 무관중 경기가 추석 이후까지도 이어지지 않을까 싶어요",
          "original_form": "아무래도 무관중 경기가 추석 이후까지도 이어지지 않을까 싶어요",
          "speaker_id": "1",
          "time": "20210824 21:39"
        },
        {
          "id": "MDRW2100012250.1.20",
          "form": "크보가 힘들겠네요 관중석 표팔아야하는디 ㅋㅋ",
