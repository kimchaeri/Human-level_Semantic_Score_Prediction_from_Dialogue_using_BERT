      "utterance": [
        {
          "id": "MDRW2100019965.1.1",
          "form": "오오ㅎㅎ",
          "original_form": "오오ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100019965.1.2",
          "form": "식사는 한식양식 안가리고 다먹어요 ㅋㅋ",
          "original_form": "식사는 한식양식 안가리고 다먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100019965.1.3",
          "form": "그래도 특히 좋아하시는거있으실꺼아니에요ㅎ",
          "original_form": "그래도 특히 좋아하시는거있으실꺼아니에요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100019965.1.4",
          "form": "음.. 전 피자요! 아 요새는 브리또에 빠져살아요ㅋㅋㅋ",
          "original_form": "음.. 전 피자요! 아 요새는 브리또에 빠져살아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100019965.1.5",
          "form": "오오오ㅎㅎ",
          "original_form": "오오오ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019965.1.6",
          "form": "좋죠좋죠ㅎ 브리또ㅎㅎ",
          "original_form": "좋죠좋죠ㅎ 브리또ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019965.1.7",
          "form": "피자빵만들어드셔요ㅎ",
          "original_form": "피자빵만들어드셔요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019965.1.8",
          "form": "엄청 간단해요ㅎ 인터넷에 식빵으로 피자빵 만드는법이ㅛ을꺼에요ㅎㅎ",
          "original_form": "엄청 간단해요ㅎ 인터넷에 식빵으로 피자빵 만드는법이ㅛ을꺼에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019965.1.9",
          "form": "오 피자빵 왜 그생각을못했을까요!",
          "original_form": "오 피자빵 왜 그생각을못했을까요!",
          "speaker_id": "1",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019965.1.10",
          "form": "내일 해먹어봐야겠어요 ㅋㅋ",
          "original_form": "내일 해먹어봐야겠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019965.1.11",
          "form": "ㅎㅎ 해보셔요ㅎ",
          "original_form": "ㅎㅎ 해보셔요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019965.1.12",
          "form": "남자인저도 그냥 대충올려놓고 전자렌인지 돌리니깐 맜있더라구요ㅎ",
          "original_form": "남자인저도 그냥 대충올려놓고 전자렌인지 돌리니깐 맜있더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019965.1.13",
          "form": "아하! 집에오븐있는데 오랜만에 활용을해봐야겠어요 ㅎㅎ",
          "original_form": "아하! 집에오븐있는데 오랜만에 활용을해봐야겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019965.1.14",
          "form": "식빵에다가 토마토소스 바르시고 생양파 베이컨 고기도 좋구요ㅎㅎ 기호에따라 피클? 이정도ㅎㅎ",
          "original_form": "식빵에다가 토마토소스 바르시고 생양파 베이컨 고기도 좋구요ㅎㅎ 기호에따라 피클? 이정도ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019965.1.15",
          "form": "마지막으로 치즈를듬뿍넣으시면 엄청 맛있어요ㅎㅎㅎ",
          "original_form": "마지막으로 치즈를듬뿍넣으시면 엄청 맛있어요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019965.1.16",
          "form": "예전에 일할때 야식으로 많이먹었어요ㅎㅍ간단해서ㅎㅎ",
          "original_form": "예전에 일할때 야식으로 많이먹었어요ㅎㅍ간단해서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019965.1.17",
          "form": "오우~굳굳!! 토마토케첩으로 대체 가능하려나요?",
          "original_form": "오우~굳굳!! 토마토케첩으로 대체 가능하려나요?",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019965.1.18",
          "form": "할라피뇨도있는데 넣어봐야겠어요",
          "original_form": "할라피뇨도있는데 넣어봐야겠어요",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019965.1.19",
          "form": "되죠ㅎㅎ",
          "original_form": "되죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019965.1.20",
          "form": "근데 가장 피자맛에 가까운게 토마토스파게티 소스애요ㅎㅎ",
          "original_form": "근데 가장 피자맛에 가까운게 토마토스파게티 소스애요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019965.1.21",
          "form": "실제로 피자만들때 토마토 스파게티 소스베이스일껄요? 아닌가?ㅎㅎㅎ",
          "original_form": "실제로 피자만들때 토마토 스파게티 소스베이스일껄요? 아닌가?ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019965.1.22",
          "form": "맞아요.. 근데 확인해보니 아쉽게도 토마토소스가 없네요 ㅠ",
          "original_form": "맞아요.. 근데 확인해보니 아쉽게도 토마토소스가 없네요 ㅠ",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019965.1.23",
          "form": "맞아요맞아요!!",
          "original_form": "맞아요맞아요!!",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019965.1.24",
          "form": "ㅎㅎ 그럼 케챱으로 하셔도 맛있어요ㅎㅎ",
          "original_form": "ㅎㅎ 그럼 케챱으로 하셔도 맛있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019965.1.25",
          "form": "조리법이 간단해서 아마 맛있으실꺼에요ㅎㅎ",
          "original_form": "조리법이 간단해서 아마 맛있으실꺼에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019965.1.26",
          "form": "나만의 특별한 레시피 있으세요?",
          "original_form": "나만의 특별한 레시피 있으세요?",
          "speaker_id": "2",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019965.1.27",
          "form": "음 저만의 특별한레시피는 딱히 없는거같아요",
          "original_form": "음 저만의 특별한레시피는 딱히 없는거같아요",
          "speaker_id": "1",
          "time": "20210827 23:11"
        }
      ]
