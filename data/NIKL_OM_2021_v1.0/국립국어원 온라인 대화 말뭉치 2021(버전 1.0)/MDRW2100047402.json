        },
        {
          "id": "MDRW2100047402.1.2",
          "form": "영화 보는 것 좋아하시나요?",
          "original_form": "영화 보는 것 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100047402.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100047402.1.4",
          "form": "영화 좋아하는데 최근에는 잘못봤어요",
          "original_form": "영화 좋아하는데 최근에는 잘못봤어요",
          "speaker_id": "2",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100047402.1.5",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100047402.1.6",
          "form": "아무래도 코로나 때문에 영화관 가기가 쉽진 않죠ㅜ",
          "original_form": "아무래도 코로나 때문에 영화관 가기가 쉽진 않죠ㅜ",
          "speaker_id": "1",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100047402.1.7",
          "form": "네 영화 보는 것 좋아해요!",
          "original_form": "네 영화 보는 것 좋아해요!",
          "speaker_id": "1",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100047402.1.8",
          "form": "근데 저도 최근엔 영화관은 잘 못갔고, 집에서 결제해서 보고 그랬어요",
          "original_form": "근데 저도 최근엔 영화관은 잘 못갔고, 집에서 결제해서 보고 그랬어요",
          "speaker_id": "1",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100047402.1.9",
          "form": "아무래도 그렇죠ㅠㅠ",
          "original_form": "아무래도 그렇죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100047402.1.10",
          "form": "최근에는 뭐 보셨어요?",
          "original_form": "최근에는 뭐 보셨어요?",
          "speaker_id": "2",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100047402.1.11",
          "form": "최근엔 집에서 해리포터 시리즈 연속으로 봤어요ㅎㅎ",
          "original_form": "최근엔 집에서 해리포터 시리즈 연속으로 봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100047402.1.12",
          "form": "추억이네요 저는 작년 크리스마스에 재탕했는데",
          "original_form": "추억이네요 저는 작년 크리스마스에 재탕했는데",
          "speaker_id": "2",
          "time": "20210826 15:04"
        },
        {
          "id": "MDRW2100047402.1.13",
          "form": "해리포터는 주기적으로 생각나요",
          "original_form": "해리포터는 주기적으로 생각나요",
          "speaker_id": "2",
          "time": "20210826 15:04"
        },
        {
          "id": "MDRW2100047402.1.14",
          "form": "그쵸~ 휴가 때 한번씩 정주행하는 영화",
          "original_form": "그쵸~ 휴가 때 한번씩 정주행하는 영화",
          "speaker_id": "1",
          "time": "20210826 15:05"
        },
        {
          "id": "MDRW2100047402.1.15",
          "form": "공연 보는 것도 좋아하세요?",
          "original_form": "공연 보는 것도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 15:05"
        },
        {
          "id": "MDRW2100047402.1.16",
          "form": "경험이 많이 없어요",
          "original_form": "경험이 많이 없어요",
          "speaker_id": "2",
          "time": "20210826 15:06"
        },
        {
          "id": "MDRW2100047402.1.17",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 15:06"
        },
        {
          "id": "MDRW2100047402.1.18",
          "form": "저는 뮤지컬 보는 것 좋아해요",
          "original_form": "저는 뮤지컬 보는 것 좋아해요",
          "speaker_id": "1",
          "time": "20210826 15:06"
        },
        {
          "id": "MDRW2100047402.1.19",
          "form": "최근에 어떤극 보셨나요?",
          "original_form": "최근에 어떤극 보셨나요?",
          "speaker_id": "2",
          "time": "20210826 15:09"
        },
        {
          "id": "MDRW2100047402.1.20",
          "form": "최근에는 윤동주,달을쏘다 라는 가무극 봤어요",
          "original_form": "최근에는 윤동주,달을쏘다 라는 가무극 봤어요",
          "speaker_id": "1",
          "time": "20210826 15:09"
        },
        {
          "id": "MDRW2100047402.1.21",
          "form": "서울예술단에서 했던",
          "original_form": "서울예술단에서 했던",
          "speaker_id": "1",
          "time": "20210826 15:10"
        },
        {
          "id": "MDRW2100047402.1.22",
          "form": "어떠셨나요?",
          "original_form": "어떠셨나요?",
          "speaker_id": "2",
          "time": "20210826 15:10"
        },
        {
          "id": "MDRW2100047402.1.23",
          "form": "감동적이었어요",
          "original_form": "감동적이었어요",
          "speaker_id": "1",
          "time": "20210826 15:14"
        },
        {
          "id": "MDRW2100047402.1.24",
          "form": "특히 앙상블이 좋더라고요",
          "original_form": "특히 앙상블이 좋더라고요",
          "speaker_id": "1",
          "time": "20210826 15:14"
        }
      ]
