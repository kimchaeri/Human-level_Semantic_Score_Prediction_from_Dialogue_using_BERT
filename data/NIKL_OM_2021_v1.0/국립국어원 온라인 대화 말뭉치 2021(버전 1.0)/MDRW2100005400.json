      "utterance": [
        {
          "id": "MDRW2100005400.1.1",
          "form": "저 같은 사람은 혈관 드러울 걱정은 덜한데",
          "original_form": "저 같은 사람은 혈관 드러울 걱정은 덜한데",
          "speaker_id": "2",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005400.1.2",
          "form": "대신 탄수화물 중독이 되어버리죠",
          "original_form": "대신 탄수화물 중독이 되어버리죠",
          "speaker_id": "2",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005400.1.3",
          "form": "밥을 다른 사람들보다 많이 먹게 돼요",
          "original_form": "밥을 다른 사람들보다 많이 먹게 돼요",
          "speaker_id": "2",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005400.1.4",
          "form": "탄수화물은 진짜 못끊죠",
          "original_form": "탄수화물은 진짜 못끊죠",
          "speaker_id": "1",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005400.1.5",
          "form": "빵이나 떡 같은 것도 좋아하세요?",
          "original_form": "빵이나 떡 같은 것도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005400.1.6",
          "form": "없어서 못먹죠",
          "original_form": "없어서 못먹죠",
          "speaker_id": "2",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005400.1.7",
          "form": "떡은 어떤 떡을 좋아하시나요? 약간 백설기 같은거 좋아하시려나",
          "original_form": "떡은 어떤 떡을 좋아하시나요? 약간 백설기 같은거 좋아하시려나",
          "speaker_id": "1",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005400.1.8",
          "form": "진짜 지금 먹고싶은데 없어서 못먹어요",
          "original_form": "진짜 지금 먹고싶은데 없어서 못먹어요",
          "speaker_id": "2",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005400.1.9",
          "form": "백설기 인절미 시루떡",
          "original_form": "백설기 인절미 시루떡",
          "speaker_id": "2",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005400.1.10",
          "form": "개떡",
          "original_form": "개떡",
          "speaker_id": "2",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005400.1.11",
          "form": "절편",
          "original_form": "절편",
          "speaker_id": "2",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005400.1.12",
          "form": "완전좋아",
          "original_form": "완전좋아",
          "speaker_id": "1",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005400.1.13",
          "form": "설탕 들어간건 어때요",
          "original_form": "설탕 들어간건 어때요",
          "speaker_id": "1",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005400.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ 가래떡",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 가래떡",
          "speaker_id": "2",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005400.1.15",
          "form": "시루떡은 설탕이 들어가야 맛있더라고요",
          "original_form": "시루떡은 설탕이 들어가야 맛있더라고요",
          "speaker_id": "2",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005400.1.16",
          "form": "너무 단 건 잘 못먹었는데 요즘은 그래도 단걸 잘 찾아요",
          "original_form": "너무 단 건 잘 못먹었는데 요즘은 그래도 단걸 잘 찾아요",
          "speaker_id": "2",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005400.1.17",
          "form": "어렸을 땐 느끼한거 단거 먹다가 토했거든요;",
          "original_form": "어렸을 땐 느끼한거 단거 먹다가 토했거든요;",
          "speaker_id": "2",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005400.1.18",
          "form": "비위가 많이 약해서 밥도 잘 못먹었어요",
          "original_form": "비위가 많이 약해서 밥도 잘 못먹었어요",
          "speaker_id": "2",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005400.1.19",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005400.1.20",
          "form": "비위 약하다는게 참 힘들어요",
          "original_form": "비위 약하다는게 참 힘들어요",
          "speaker_id": "2",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005400.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005400.1.22",
          "form": "완전 애기때부터 그래서 영양실조로 손발톱도 뽑았다네요",
          "original_form": "완전 애기때부터 그래서 영양실조로 손발톱도 뽑았다네요",
          "speaker_id": "2",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005400.1.23",
          "form": "못먹는게 많아지니까",
          "original_form": "못먹는게 많아지니까",
          "speaker_id": "1",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005400.1.24",
          "form": "기억 못하니 참 다행이야",
          "original_form": "기억 못하니 참 다행이야",
          "speaker_id": "2",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005400.1.25",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "1",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005400.1.26",
          "form": "못먹겠는거 억지로 먹이면 토하고",
          "original_form": "못먹겠는거 억지로 먹이면 토하고",
          "speaker_id": "2",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005400.1.27",
          "form": "초딩땐 밥 못남기게 하고",
          "original_form": "초딩땐 밥 못남기게 하고",
          "speaker_id": "2",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005400.1.28",
          "form": "억지로 먹이는게 이상한거죠",
          "original_form": "억지로 먹이는게 이상한거죠",
          "speaker_id": "1",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005400.1.29",
          "form": "저는 그거 이해못해요",
          "original_form": "저는 그거 이해못해요",
          "speaker_id": "1",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005400.1.30",
          "form": "또 먹는 시간이 정해져있으니까... 지금도 그렇지만",
          "original_form": "또 먹는 시간이 정해져있으니까... 지금도 그렇지만",
          "speaker_id": "2",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005400.1.31",
          "form": "왜 억지로먹여",
          "original_form": "왜 억지로먹여",
          "speaker_id": "1",
          "time": "20210821 22:16"
        }
      ]
