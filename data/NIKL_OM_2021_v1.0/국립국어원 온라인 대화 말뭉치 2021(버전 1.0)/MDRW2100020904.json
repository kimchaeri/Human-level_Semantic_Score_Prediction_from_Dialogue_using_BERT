      "utterance": [
        {
          "id": "MDRW2100020904.1.1",
          "form": "봉사 자주해요?",
          "original_form": "봉사 자주해요?",
          "speaker_id": "1",
          "time": "20210828 14:01"
        },
        {
          "id": "MDRW2100020904.1.2",
          "form": "아니요 ㅠ",
          "original_form": "아니요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 14:02"
        },
        {
          "id": "MDRW2100020904.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210828 14:02"
        },
        {
          "id": "MDRW2100020904.1.4",
          "form": "저도요 ㅋㅋ 헌혈이 그나마 쉬운 봉사였는데 전 부적격이네요",
          "original_form": "저도요 ㅋㅋ 헌혈이 그나마 쉬운 봉사였는데 전 부적격이네요",
          "speaker_id": "1",
          "time": "20210828 14:06"
        },
        {
          "id": "MDRW2100020904.1.5",
          "form": "해보싱 거는",
          "original_form": "해보싱 거는",
          "speaker_id": "2",
          "time": "20210828 14:39"
        },
        {
          "id": "MDRW2100020904.1.6",
          "form": "뭐 있으세여?!",
          "original_form": "뭐 있으세여?!",
          "speaker_id": "2",
          "time": "20210828 14:40"
        },
        {
          "id": "MDRW2100020904.1.7",
          "form": "교육봉사도 하고 양로원 봉사도 하고~",
          "original_form": "교육봉사도 하고 양로원 봉사도 하고~",
          "speaker_id": "1",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100020904.1.8",
          "form": "그 외에는 대부분 시간채우려고 한거라 기억이 잘 안나네요",
          "original_form": "그 외에는 대부분 시간채우려고 한거라 기억이 잘 안나네요",
          "speaker_id": "1",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100020904.1.9",
          "form": "오 대박",
          "original_form": "오 대박",
          "speaker_id": "2",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100020904.1.10",
          "form": "많이 하셨ㄴ네여",
          "original_form": "많이 하셨ㄴ네여",
          "speaker_id": "2",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100020904.1.11",
          "form": "그거 다 학교다닐 적이에요 ㅜㅜ",
          "original_form": "그거 다 학교다닐 적이에요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 14:59"
        },
        {
          "id": "MDRW2100020904.1.12",
          "form": "성인 되고나서는 봉사활동 안해봤네요",
          "original_form": "성인 되고나서는 봉사활동 안해봤네요",
          "speaker_id": "1",
          "time": "20210828 14:59"
        },
        {
          "id": "MDRW2100020904.1.13",
          "form": "몸무게 미달로 헌혈 더이상 못해요",
          "original_form": "몸무게 미달로 헌혈 더이상 못해요",
          "speaker_id": "1",
          "time": "20210828 14:59"
        },
        {
          "id": "MDRW2100020904.1.14",
          "form": "전 성인 때는 학점채우려규 해봤어요ㅋㅋ",
          "original_form": "전 성인 때는 학점채우려규 해봤어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:20"
        },
        {
          "id": "MDRW2100020904.1.15",
          "form": "아 ㅋㅋ",
          "original_form": "아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:17"
        },
        {
          "id": "MDRW2100020904.1.16",
          "form": "봉사 해야되는 학점도 있어요?",
          "original_form": "봉사 해야되는 학점도 있어요?",
          "speaker_id": "1",
          "time": "20210828 20:17"
        },
        {
          "id": "MDRW2100020904.1.17",
          "form": "네 ㅠ 봉사하면",
          "original_form": "네 ㅠ 봉사하면",
          "speaker_id": "2",
          "time": "20210828 20:44"
        },
        {
          "id": "MDRW2100020904.1.18",
          "form": "1점 줬어요",
          "original_form": "1점 줬어요",
          "speaker_id": "2",
          "time": "20210828 20:44"
        },
        {
          "id": "MDRW2100020904.1.19",
          "form": "ㅋㅋㅋ 1점 채우려면 몇 시간 봉사 해야 되는 건가요..",
          "original_form": "ㅋㅋㅋ 1점 채우려면 몇 시간 봉사 해야 되는 건가요..",
          "speaker_id": "1",
          "time": "20210828 20:47"
        },
        {
          "id": "MDRW2100020904.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ잘 기억은 안나는데..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ잘 기억은 안나는데..",
          "speaker_id": "2",
          "time": "20210828 20:47"
        },
        {
          "id": "MDRW2100020904.1.21",
          "form": "쨋든 봉사하는 거였어요",
          "original_form": "쨋든 봉사하는 거였어요",
          "speaker_id": "2",
          "time": "20210828 20:47"
        },
        {
          "id": "MDRW2100020904.1.22",
          "form": "그래도 공부하는 것보다 착한 일 하는 것도 괜찮은 것 같네요",
          "original_form": "그래도 공부하는 것보다 착한 일 하는 것도 괜찮은 것 같네요",
          "speaker_id": "1",
          "time": "20210828 20:51"
        }
      ]
