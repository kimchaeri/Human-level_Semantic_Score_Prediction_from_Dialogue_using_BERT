      "utterance": [
        {
          "id": "MDRW2100030407.1.1",
          "form": "테이 좋아하셨나요?",
          "original_form": "테이 좋아하셨나요?",
          "speaker_id": "2",
          "time": "20210904 00:40"
        },
        {
          "id": "MDRW2100030407.1.2",
          "form": "테이 한창때는 잘 몰랐는데",
          "original_form": "테이 한창때는 잘 몰랐는데",
          "speaker_id": "1",
          "time": "20210904 00:40"
        },
        {
          "id": "MDRW2100030407.1.3",
          "form": "나중에 오페라스타 나온거 보고 좋아졌어요!",
          "original_form": "나중에 오페라스타 나온거 보고 좋아졌어요!",
          "speaker_id": "1",
          "time": "20210904 00:40"
        },
        {
          "id": "MDRW2100030407.1.4",
          "form": "님도 좋아하시나요?",
          "original_form": "님도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100030407.1.5",
          "form": "저도 좋아합니다",
          "original_form": "저도 좋아합니다",
          "speaker_id": "2",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100030407.1.6",
          "form": "저 대학교때",
          "original_form": "저 대학교때",
          "speaker_id": "2",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100030407.1.7",
          "form": "그노래가 나왓서요",
          "original_form": "그노래가 나왓서요",
          "speaker_id": "2",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100030407.1.8",
          "form": "사랑은 향기를 남기고",
          "original_form": "사랑은 향기를 남기고",
          "speaker_id": "2",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100030407.1.9",
          "form": "좋아하는 가수가 누구였어요?",
          "original_form": "좋아하는 가수가 누구였어요?",
          "speaker_id": "2",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100030407.1.10",
          "form": "저는...서태지요",
          "original_form": "저는...서태지요",
          "speaker_id": "1",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100030407.1.11",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "2",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100030407.1.12",
          "form": "완전 꼬꼬마시절에 보고 반함ㅎㅎ",
          "original_form": "완전 꼬꼬마시절에 보고 반함ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100030407.1.13",
          "form": "아 누님이시네요 진짜",
          "original_form": "아 누님이시네요 진짜",
          "speaker_id": "2",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100030407.1.14",
          "form": "아니 왜 제가 누님이쥬?",
          "original_form": "아니 왜 제가 누님이쥬?",
          "speaker_id": "1",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100030407.1.15",
          "form": "서태지 최고죠",
          "original_form": "서태지 최고죠",
          "speaker_id": "2",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100030407.1.16",
          "form": "하지만 누님이라 부르세요",
          "original_form": "하지만 누님이라 부르세요",
          "speaker_id": "1",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100030407.1.17",
          "form": "전 서태지 시기 아니니 ㅎㅎ",
          "original_form": "전 서태지 시기 아니니 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 00:44"
        },
        {
          "id": "MDRW2100030407.1.18",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210904 00:44"
        },
        {
          "id": "MDRW2100030407.1.19",
          "form": "대학교때 태이인데 왜 아니죵?",
          "original_form": "대학교때 태이인데 왜 아니죵?",
          "speaker_id": "1",
          "time": "20210904 00:44"
        },
        {
          "id": "MDRW2100030407.1.20",
          "form": "초등학교때 컴백홈 추는 애들 없었나요ㅜㅜ",
          "original_form": "초등학교때 컴백홈 추는 애들 없었나요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 00:44"
        },
        {
          "id": "MDRW2100030407.1.21",
          "form": "초등학교 1학년인데",
          "original_form": "초등학교 1학년인데",
          "speaker_id": "2",
          "time": "20210904 00:44"
        },
        {
          "id": "MDRW2100030407.1.22",
          "form": "그때는 시골에 살아서",
          "original_form": "그때는 시골에 살아서",
          "speaker_id": "2",
          "time": "20210904 00:45"
        },
        {
          "id": "MDRW2100030407.1.23",
          "form": "비석치기하고 놀았습니다",
          "original_form": "비석치기하고 놀았습니다",
          "speaker_id": "2",
          "time": "20210904 00:45"
        },
        {
          "id": "MDRW2100030407.1.24",
          "form": "92년이자나여 ㅋㅋ",
          "original_form": "92년이자나여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:45"
        },
        {
          "id": "MDRW2100030407.1.25",
          "form": "아니ㅋㅋㅋ",
          "original_form": "아니ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:45"
        },
        {
          "id": "MDRW2100030407.1.26",
          "form": "유머스트콤백홈",
          "original_form": "유머스트콤백홈",
          "speaker_id": "2",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100030407.1.27",
          "form": "티비와 안친하셨군요",
          "original_form": "티비와 안친하셨군요",
          "speaker_id": "1",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100030407.1.28",
          "form": "난 알아요가",
          "original_form": "난 알아요가",
          "speaker_id": "2",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100030407.1.29",
          "form": "네 티비 잘안보고 ㅋㅋ밖에서",
          "original_form": "네 티비 잘안보고 ㅋㅋ밖에서",
          "speaker_id": "2",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100030407.1.30",
          "form": "연날리고",
          "original_form": "연날리고",
          "speaker_id": "2",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100030407.1.31",
          "form": "놀았습니다",
          "original_form": "놀았습니다",
          "speaker_id": "2",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100030407.1.32",
          "form": "ㅋㅋ 저 컴백홈 보고",
          "original_form": "ㅋㅋ 저 컴백홈 보고",
          "speaker_id": "1",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030407.1.33",
          "form": "사실 가수잘몰랏서요 ㅋㅋ",
          "original_form": "사실 가수잘몰랏서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030407.1.34",
          "form": "컬쳐쇼크받았었어요",
          "original_form": "컬쳐쇼크받았었어요",
          "speaker_id": "1",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030407.1.35",
          "form": "저도 ㅋㅋㅋ",
          "original_form": "저도 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100030407.1.36",
          "form": "저게 노래야?",
          "original_form": "저게 노래야?",
          "speaker_id": "2",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100030407.1.37",
          "form": "이랬슴 ㅋㅋ",
          "original_form": "이랬슴 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100030407.1.38",
          "form": "ㅋㅋ 그랬는데 주변서 다 따라추고",
          "original_form": "ㅋㅋ 그랬는데 주변서 다 따라추고",
          "speaker_id": "1",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100030407.1.39",
          "form": "그렇게 입덕했쥬",
          "original_form": "그렇게 입덕했쥬",
          "speaker_id": "1",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100030407.1.40",
          "form": "따라추셧어요?",
          "original_form": "따라추셧어요?",
          "speaker_id": "2",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030407.1.41",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030407.1.42",
          "form": "누님 인싸셨군요",
          "original_form": "누님 인싸셨군요",
          "speaker_id": "2",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030407.1.43",
          "form": "아뇨 전...  태생이 몸치라 안됩니다",
          "original_form": "아뇨 전...  태생이 몸치라 안됩니다",
          "speaker_id": "1",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030407.1.44",
          "form": "저는 빼고 모든 애들ㅎㅎ",
          "original_form": "저는 빼고 모든 애들ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030407.1.45",
          "form": "아 그랬군요 ㅎㅎ",
          "original_form": "아 그랬군요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030407.1.46",
          "form": "원래 어릴때가 부끄러움이 젤 없잖아요ㅎ",
          "original_form": "원래 어릴때가 부끄러움이 젤 없잖아요ㅎ",
          "speaker_id": "1",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030407.1.47",
          "form": "저는 어릴때 부끄러워했서요 ㅋㅋㅋ",
          "original_form": "저는 어릴때 부끄러워했서요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030407.1.48",
          "form": "지금은 아니지만",
          "original_form": "지금은 아니지만",
          "speaker_id": "2",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030407.1.49",
          "form": "고학년만 됐어도 좀 몸사릴텐데",
          "original_form": "고학년만 됐어도 좀 몸사릴텐데",
          "speaker_id": "1",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030407.1.50",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030407.1.51",
          "form": "반대로 가기 쉽지 않은데요ㅎㅎ",
          "original_form": "반대로 가기 쉽지 않은데요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030407.1.52",
          "form": "궁금하네요 ㅋㅋ",
          "original_form": "궁금하네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100030407.1.53",
          "form": "그렇죠?",
          "original_form": "그렇죠?",
          "speaker_id": "2",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100030407.1.54",
          "form": "아 저는 초지일관 내향적이라 속으로만 좋아했습니다ㅎ",
          "original_form": "아 저는 초지일관 내향적이라 속으로만 좋아했습니다ㅎ",
          "speaker_id": "1",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100030407.1.55",
          "form": "부럽습니다",
          "original_form": "부럽습니다",
          "speaker_id": "2",
          "time": "20210904 00:52"
        }
      ]
    }
