      "utterance": [
        {
          "id": "MDRW2100016055.1.1",
          "form": "설거지하고 오셨구나",
          "original_form": "설거지하고 오셨구나",
          "speaker_id": "1",
          "time": "20210826 08:52"
        },
        {
          "id": "MDRW2100016055.1.2",
          "form": "집안일 자주 하는 편이세요",
          "original_form": "집안일 자주 하는 편이세요",
          "speaker_id": "1",
          "time": "20210826 08:52"
        },
        {
          "id": "MDRW2100016055.1.3",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "2",
          "time": "20210826 08:54"
        },
        {
          "id": "MDRW2100016055.1.4",
          "form": "어제 먹고 남은 고기먹고",
          "original_form": "어제 먹고 남은 고기먹고",
          "speaker_id": "2",
          "time": "20210826 08:54"
        },
        {
          "id": "MDRW2100016055.1.5",
          "form": "얼른 설거지 하고왔어요",
          "original_form": "얼른 설거지 하고왔어요",
          "speaker_id": "2",
          "time": "20210826 08:54"
        },
        {
          "id": "MDRW2100016055.1.6",
          "form": "잘하셨어요",
          "original_form": "잘하셨어요",
          "speaker_id": "1",
          "time": "20210826 08:54"
        },
        {
          "id": "MDRW2100016055.1.7",
          "form": "설거지 너무 귀찮지않나요",
          "original_form": "설거지 너무 귀찮지않나요",
          "speaker_id": "1",
          "time": "20210826 08:54"
        },
        {
          "id": "MDRW2100016055.1.8",
          "form": "제가 먹은 건 제가 해야돼서요",
          "original_form": "제가 먹은 건 제가 해야돼서요",
          "speaker_id": "2",
          "time": "20210826 08:54"
        },
        {
          "id": "MDRW2100016055.1.9",
          "form": "귀찮아도 별 수 없죠",
          "original_form": "귀찮아도 별 수 없죠",
          "speaker_id": "2",
          "time": "20210826 08:54"
        },
        {
          "id": "MDRW2100016055.1.10",
          "form": "으음 되게 책임감 있으시구나",
          "original_form": "으음 되게 책임감 있으시구나",
          "speaker_id": "1",
          "time": "20210826 08:55"
        },
        {
          "id": "MDRW2100016055.1.11",
          "form": "전 귀찮아서 방치했다 나중에 하거든요",
          "original_form": "전 귀찮아서 방치했다 나중에 하거든요",
          "speaker_id": "1",
          "time": "20210826 08:55"
        },
        {
          "id": "MDRW2100016055.1.12",
          "form": "저도 그렇게하고 싶은데",
          "original_form": "저도 그렇게하고 싶은데",
          "speaker_id": "2",
          "time": "20210826 08:56"
        },
        {
          "id": "MDRW2100016055.1.13",
          "form": "엄마한테 혼나요",
          "original_form": "엄마한테 혼나요",
          "speaker_id": "2",
          "time": "20210826 08:56"
        },
        {
          "id": "MDRW2100016055.1.14",
          "form": "잔소리를 듣기싫으면 몸을 움직여야돼요",
          "original_form": "잔소리를 듣기싫으면 몸을 움직여야돼요",
          "speaker_id": "2",
          "time": "20210826 08:56"
        },
        {
          "id": "MDRW2100016055.1.15",
          "form": "가족이랑 같이 사시는구나",
          "original_form": "가족이랑 같이 사시는구나",
          "speaker_id": "1",
          "time": "20210826 08:56"
        },
        {
          "id": "MDRW2100016055.1.16",
          "form": "형제는 있으세여",
          "original_form": "형제는 있으세여",
          "speaker_id": "1",
          "time": "20210826 08:56"
        },
        {
          "id": "MDRW2100016055.1.17",
          "form": "너",
          "original_form": "너",
          "speaker_id": "2",
          "time": "20210826 09:01"
        },
        {
          "id": "MDRW2100016055.1.18",
          "form": "여지동생 힌명이오",
          "original_form": "여지동생 힌명이오",
          "speaker_id": "2",
          "time": "20210826 09:01"
        },
        {
          "id": "MDRW2100016055.1.19",
          "form": "몇살차이나세요",
          "original_form": "몇살차이나세요",
          "speaker_id": "1",
          "time": "20210826 09:01"
        },
        {
          "id": "MDRW2100016055.1.20",
          "form": "4살요",
          "original_form": "4살요",
          "speaker_id": "2",
          "time": "20210826 09:12"
        },
        {
          "id": "MDRW2100016055.1.21",
          "form": "저도 4살차이나는 누나하나있어요",
          "original_form": "저도 4살차이나는 누나하나있어요",
          "speaker_id": "1",
          "time": "20210826 09:13"
        },
        {
          "id": "MDRW2100016055.1.22",
          "form": "휴우..",
          "original_form": "휴우..",
          "speaker_id": "2",
          "time": "20210826 09:14"
        },
        {
          "id": "MDRW2100016055.1.23",
          "form": "오?",
          "original_form": "오?",
          "speaker_id": "2",
          "time": "20210826 09:14"
        },
        {
          "id": "MDRW2100016055.1.24",
          "form": "남자분이세요?",
          "original_form": "남자분이세요?",
          "speaker_id": "2",
          "time": "20210826 09:14"
        },
        {
          "id": "MDRW2100016055.1.25",
          "form": "네..ㅎ",
          "original_form": "네..ㅎ",
          "speaker_id": "1",
          "time": "20210826 09:14"
        },
        {
          "id": "MDRW2100016055.1.26",
          "form": "모르셨구나",
          "original_form": "모르셨구나",
          "speaker_id": "1",
          "time": "20210826 09:14"
        },
        {
          "id": "MDRW2100016055.1.27",
          "form": "신기하네요 말투가 다정하셔서 여성분이실줄 알았어요",
          "original_form": "신기하네요 말투가 다정하셔서 여성분이실줄 알았어요",
          "speaker_id": "2",
          "time": "20210826 09:14"
        },
        {
          "id": "MDRW2100016055.1.28",
          "form": "전 집안일 다 정리하고 나왔어요",
          "original_form": "전 집안일 다 정리하고 나왔어요",
          "speaker_id": "2",
          "time": "20210826 09:14"
        }
      ]
    }
