          "speaker_id": "2",
          "time": "20210827 19:09"
        },
        {
          "id": "MDRW2100019359.1.3",
          "form": "신야근 로딩 오류때문에 고객센터에 문의한거",
          "original_form": "신야근 로딩 오류때문에 고객센터에 문의한거",
          "speaker_id": "1",
          "time": "20210827 19:09"
        },
        {
          "id": "MDRW2100019359.1.4",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210827 19:09"
        },
        {
          "id": "MDRW2100019359.1.5",
          "form": "답변으로 쿠폰 줬다고 했잖아",
          "original_form": "답변으로 쿠폰 줬다고 했잖아",
          "speaker_id": "1",
          "time": "20210827 19:09"
        },
        {
          "id": "MDRW2100019359.1.6",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210827 19:09"
        },
        {
          "id": "MDRW2100019359.1.7",
          "form": "그거 오늘 등록해봤는데",
          "original_form": "그거 오늘 등록해봤는데",
          "speaker_id": "1",
          "time": "20210827 19:09"
        },
        {
          "id": "MDRW2100019359.1.8",
          "form": "하트 10개 줌",
          "original_form": "하트 10개 줌",
          "speaker_id": "1",
          "time": "20210827 19:09"
        },
        {
          "id": "MDRW2100019359.1.9",
          "form": "별거 안주네",
          "original_form": "별거 안주네",
          "speaker_id": "2",
          "time": "20210827 19:09"
        },
        {
          "id": "MDRW2100019359.1.10",
          "form": "다음에 튕기면 캡쳐해서 문의해봐",
          "original_form": "다음에 튕기면 캡쳐해서 문의해봐",
          "speaker_id": "1",
          "time": "20210827 19:09"
        },
        {
          "id": "MDRW2100019359.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:09"
        },
        {
          "id": "MDRW2100019359.1.12",
          "form": "아녀 골드 아니면",
          "original_form": "아녀 골드 아니면",
          "speaker_id": "2",
          "time": "20210827 19:09"
        },
        {
          "id": "MDRW2100019359.1.13",
          "form": "ㄱㅊ",
          "original_form": "ㄱㅊ",
          "speaker_id": "2",
          "time": "20210827 19:09"
        },
        {
          "id": "MDRW2100019359.1.14",
          "form": "어차피 지금도",
          "original_form": "어차피 지금도",
          "speaker_id": "2",
          "time": "20210827 19:09"
        },
        {
          "id": "MDRW2100019359.1.15",
          "form": "난 하트 5개 주고 퉁칠줄 알앗는데",
          "original_form": "난 하트 5개 주고 퉁칠줄 알앗는데",
          "speaker_id": "1",
          "time": "20210827 19:10"
        },
        {
          "id": "MDRW2100019359.1.16",
          "form": "안까고잇어",
          "original_form": "안까고잇어",
          "speaker_id": "2",
          "time": "20210827 19:10"
        },
        {
          "id": "MDRW2100019359.1.17",
          "form": "10개 줘서 의외엿음ㅋㅋㅋㅋㅋ",
          "original_form": "10개 줘서 의외엿음ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:10"
        },
        {
          "id": "MDRW2100019359.1.18",
          "form": "진주를 10개 주면 모를까",
          "original_form": "진주를 10개 주면 모를까",
          "speaker_id": "2",
          "time": "20210827 19:10"
        },
        {
          "id": "MDRW2100019359.1.19",
          "form": "4챕에서",
          "original_form": "4챕에서",
          "speaker_id": "1",
          "time": "20210827 19:10"
        },
        {
          "id": "MDRW2100019359.1.20",
          "form": "게임오버 당한거",
          "original_form": "게임오버 당한거",
          "speaker_id": "1",
          "time": "20210827 19:10"
        },
        {
          "id": "MDRW2100019359.1.21",
          "form": "아 가볍게",
          "original_form": "아 가볍게",
          "speaker_id": "2",
          "time": "20210827 19:10"
        },
        {
          "id": "MDRW2100019359.1.22",
          "form": "다시 생각해도 어이없음",
          "original_form": "다시 생각해도 어이없음",
          "speaker_id": "1",
          "time": "20210827 19:10"
        },
        {
          "id": "MDRW2100019359.1.23",
          "form": "그거 리뷰나 좀 할까",
          "original_form": "그거 리뷰나 좀 할까",
          "speaker_id": "2",
          "time": "20210827 19:11"
        },
        {
          "id": "MDRW2100019359.1.24",
          "form": "ㅇㅇ그랭",
          "original_form": "ㅇㅇ그랭",
          "speaker_id": "1",
          "time": "20210827 19:11"
        },
        {
          "id": "MDRW2100019359.1.25",
          "form": "지금 세편ㅇ보고와서",
          "original_form": "지금 세편ㅇ보고와서",
          "speaker_id": "2",
          "time": "20210827 19:11"
        },
        {
          "id": "MDRW2100019359.1.26",
          "form": "근데 아직 엔딩 안봣지?",
          "original_form": "근데 아직 엔딩 안봣지?",
          "speaker_id": "1",
          "time": "20210827 19:11"
        },
        {
          "id": "MDRW2100019359.1.27",
          "form": "아직 멀엇지",
          "original_form": "아직 멀엇지",
          "speaker_id": "2",
          "time": "20210827 19:11"
        },
        {
          "id": "MDRW2100019359.1.28",
          "form": "나 아직 2챕이야",
          "original_form": "나 아직 2챕이야",
          "speaker_id": "2",
          "time": "20210827 19:11"
        },
        {
          "id": "MDRW2100019359.1.29",
          "form": "2챕 어디야?",
          "original_form": "2챕 어디야?",
          "speaker_id": "1",
          "time": "20210827 19:11"
        },
        {
          "id": "MDRW2100019359.1.30",
          "form": "이제 2-6",
          "original_form": "이제 2-6",
          "speaker_id": "2",
          "time": "20210827 19:12"
        },
        {
          "id": "MDRW2100019359.1.31",
          "form": "거기가 무슨내용이지...",
          "original_form": "거기가 무슨내용이지...",
          "speaker_id": "1",
          "time": "20210827 19:13"
        },
        {
          "id": "MDRW2100019359.1.32",
          "form": "내가 지금 계속 3~4챕 오갔더니",
          "original_form": "내가 지금 계속 3~4챕 오갔더니",
          "speaker_id": "1",
          "time": "20210827 19:13"
        },
        {
          "id": "MDRW2100019359.1.33",
          "form": "앞부분 전개 다 잊어버림",
