      "utterance": [
        {
          "id": "MDRW2100032712.1.1",
          "form": "ㅋㅋㅋㅋ오킼ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ오킼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:38"
        },
        {
          "id": "MDRW2100032712.1.2",
          "form": "하루에 몇번먹어??",
          "original_form": "하루에 몇번먹어??",
          "speaker_id": "1",
          "time": "20210905 22:39"
        },
        {
          "id": "MDRW2100032712.1.3",
          "form": "난 운동할때만 먹었는데",
          "original_form": "난 운동할때만 먹었는데",
          "speaker_id": "2",
          "time": "20210905 22:39"
        },
        {
          "id": "MDRW2100032712.1.4",
          "form": "오 보통 그렇게먹나보네",
          "original_form": "오 보통 그렇게먹나보네",
          "speaker_id": "1",
          "time": "20210905 22:39"
        },
        {
          "id": "MDRW2100032712.1.5",
          "form": "어떤 사람은 좀 단백질 섭취 부족한 날에도 먹는대",
          "original_form": "어떤 사람은 좀 단백질 섭취 부족한 날에도 먹는대",
          "speaker_id": "2",
          "time": "20210905 22:39"
        },
        {
          "id": "MDRW2100032712.1.6",
          "form": "탄수화물 몰빵 이런날",
          "original_form": "탄수화물 몰빵 이런날",
          "speaker_id": "2",
          "time": "20210905 22:39"
        },
        {
          "id": "MDRW2100032712.1.7",
          "form": "오...매일 단백질 양을 재는구나...",
          "original_form": "오...매일 단백질 양을 재는구나...",
          "speaker_id": "1",
          "time": "20210905 22:39"
        },
        {
          "id": "MDRW2100032712.1.8",
          "form": "그런가벼...난 아직 그정도는 모르겠어 ㅋㅋ",
          "original_form": "그런가벼...난 아직 그정도는 모르겠어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:40"
        },
        {
          "id": "MDRW2100032712.1.9",
          "form": "나도....하...진짜 열심히 살아야뇌나보더",
          "original_form": "나도....하...진짜 열심히 살아야뇌나보더",
          "speaker_id": "1",
          "time": "20210905 22:40"
        },
        {
          "id": "MDRW2100032712.1.10",
          "form": "살빼려면..",
          "original_form": "살빼려면..",
          "speaker_id": "1",
          "time": "20210905 22:40"
        },
        {
          "id": "MDRW2100032712.1.11",
          "form": "ㅜㅜ...근데 그렇게 며칠 살면",
          "original_form": "ㅜㅜ...근데 그렇게 며칠 살면",
          "speaker_id": "2",
          "time": "20210905 22:40"
        },
        {
          "id": "MDRW2100032712.1.12",
          "form": "현타오지 않나",
          "original_form": "현타오지 않나",
          "speaker_id": "2",
          "time": "20210905 22:40"
        },
        {
          "id": "MDRW2100032712.1.13",
          "form": "넘 서터레스야 ㅠ",
          "original_form": "넘 서터레스야 ㅠ",
          "speaker_id": "1",
          "time": "20210905 22:41"
        },
        {
          "id": "MDRW2100032712.1.14",
          "form": "음식 자제가 진짜 힘덜다",
          "original_form": "음식 자제가 진짜 힘덜다",
          "speaker_id": "1",
          "time": "20210905 22:41"
        },
        {
          "id": "MDRW2100032712.1.15",
          "form": "저녁마다 닭슴살에 두부 이딴거만 먹고 그러면",
          "original_form": "저녁마다 닭슴살에 두부 이딴거만 먹고 그러면",
          "speaker_id": "2",
          "time": "20210905 22:41"
        },
        {
          "id": "MDRW2100032712.1.16",
          "form": "화난다 막...",
          "original_form": "화난다 막...",
          "speaker_id": "2",
          "time": "20210905 22:41"
        },
        {
          "id": "MDRW2100032712.1.17",
          "form": "나는 평생 이렇게 못사는데...ㅋㅋ큐ㅠㅠ",
          "original_form": "나는 평생 이렇게 못사는데...ㅋㅋ큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 22:41"
        },
        {
          "id": "MDRW2100032712.1.18",
          "form": "내말이....맛잇는음식이 곧 기쁨인디",
          "original_form": "내말이....맛잇는음식이 곧 기쁨인디",
          "speaker_id": "1",
          "time": "20210905 22:41"
        },
        {
          "id": "MDRW2100032712.1.19",
          "form": "살 안찌는 체질이 젤부러워 진짜ㅠ",
          "original_form": "살 안찌는 체질이 젤부러워 진짜ㅠ",
          "speaker_id": "2",
          "time": "20210905 22:42"
        },
        {
          "id": "MDRW2100032712.1.20",
          "form": "내말이 체질도 타고나는거 너무 부럽다 ㅜㅝㅜㅜㅡ",
          "original_form": "내말이 체질도 타고나는거 너무 부럽다 ㅜㅝㅜㅜㅡ",
          "speaker_id": "1",
          "time": "20210905 22:42"
        },
        {
          "id": "MDRW2100032712.1.21",
          "form": "역시 인생은 불공평 ㅠㅠ",
          "original_form": "역시 인생은 불공평 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 22:42"
        },
        {
          "id": "MDRW2100032712.1.22",
          "form": "내 세포는 왤케 저장 잘하는건지...ㅋㅋㅋ큐ㅠㅠㅠ",
          "original_form": "내 세포는 왤케 저장 잘하는건지...ㅋㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 22:42"
        },
        {
          "id": "MDRW2100032712.1.23",
          "form": "아내말이 영양섭취고만하라고 ㅠㅠ",
          "original_form": "아내말이 영양섭취고만하라고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 22:43"
        }
      ]
    }
