      "utterance": [
        {
          "id": "MDRW2100002235.1.1",
          "form": "요새 스타벅스를 자주갔네요",
          "original_form": "요새 스타벅스를 자주갔네요",
          "speaker_id": "2",
          "time": "20210820 01:03"
        },
        {
          "id": "MDRW2100002235.1.2",
          "form": "예전에도 자주 갔는데 이제야 회원 가입했어요",
          "original_form": "예전에도 자주 갔는데 이제야 회원 가입했어요",
          "speaker_id": "2",
          "time": "20210820 01:04"
        },
        {
          "id": "MDRW2100002235.1.3",
          "form": "스벅 어떤 음료 잘드세요?",
          "original_form": "스벅 어떤 음료 잘드세요?",
          "speaker_id": "1",
          "time": "20210820 11:50"
        },
        {
          "id": "MDRW2100002235.1.4",
          "form": "저 요번에 새로나온 유자음료요",
          "original_form": "저 요번에 새로나온 유자음료요",
          "speaker_id": "2",
          "time": "20210820 12:04"
        },
        {
          "id": "MDRW2100002235.1.5",
          "form": "오 유자음료가 새로나왔나보네요 요즘은 잘안가서 몰랐는데",
          "original_form": "오 유자음료가 새로나왔나보네요 요즘은 잘안가서 몰랐는데",
          "speaker_id": "1",
          "time": "20210820 12:14"
        },
        {
          "id": "MDRW2100002235.1.6",
          "form": "저도 또 뜸해요 ㅋㅋ월초에 좀 갔었어요",
          "original_form": "저도 또 뜸해요 ㅋㅋ월초에 좀 갔었어요",
          "speaker_id": "2",
          "time": "20210820 12:15"
        },
        {
          "id": "MDRW2100002235.1.7",
          "form": "코로나로 자제중요",
          "original_form": "코로나로 자제중요",
          "speaker_id": "2",
          "time": "20210820 12:15"
        },
        {
          "id": "MDRW2100002235.1.8",
          "form": "커피는 아이가 바리스타2급이라 잘타줘요 ㅎㅎ",
          "original_form": "커피는 아이가 바리스타2급이라 잘타줘요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 12:15"
        },
        {
          "id": "MDRW2100002235.1.9",
          "form": "우와 바리스타자격증있다니 대단해여",
          "original_form": "우와 바리스타자격증있다니 대단해여",
          "speaker_id": "1",
          "time": "20210820 12:15"
        },
        {
          "id": "MDRW2100002235.1.10",
          "form": "그럼 커피집에서 내려주나요?",
          "original_form": "그럼 커피집에서 내려주나요?",
          "speaker_id": "1",
          "time": "20210820 12:16"
        },
        {
          "id": "MDRW2100002235.1.11",
          "form": "네 기계를 아이가 용돈으로 샀어요",
          "original_form": "네 기계를 아이가 용돈으로 샀어요",
          "speaker_id": "2",
          "time": "20210820 12:16"
        },
        {
          "id": "MDRW2100002235.1.12",
          "form": "원두를 제가사니 넘좋아요",
          "original_form": "원두를 제가사니 넘좋아요",
          "speaker_id": "2",
          "time": "20210820 12:16"
        },
        {
          "id": "MDRW2100002235.1.13",
          "form": "전 쓴커피는 싫어서요",
          "original_form": "전 쓴커피는 싫어서요",
          "speaker_id": "2",
          "time": "20210820 12:16"
        },
        {
          "id": "MDRW2100002235.1.14",
          "form": "저도 커피배워보고싶네요",
          "original_form": "저도 커피배워보고싶네요",
          "speaker_id": "1",
          "time": "20210820 12:21"
        },
        {
          "id": "MDRW2100002235.1.15",
          "form": "오 저도 쓴맛을싫어하는데 직접내리면 맛이 많이 차이가나나봐요",
          "original_form": "오 저도 쓴맛을싫어하는데 직접내리면 맛이 많이 차이가나나봐요",
          "speaker_id": "1",
          "time": "20210820 12:21"
        },
        {
          "id": "MDRW2100002235.1.16",
          "form": "네 원두를 고를수있어서 맛선택이 돼요",
          "original_form": "네 원두를 고를수있어서 맛선택이 돼요",
          "speaker_id": "2",
          "time": "20210820 12:22"
        },
        {
          "id": "MDRW2100002235.1.17",
          "form": "배우세요 따기 쉬워요",
          "original_form": "배우세요 따기 쉬워요",
          "speaker_id": "2",
          "time": "20210820 12:22"
        },
        {
          "id": "MDRW2100002235.1.18",
          "form": "커피 많이 먹어봐야하져?",
          "original_form": "커피 많이 먹어봐야하져?",
          "speaker_id": "1",
          "time": "20210820 12:23"
        },
        {
          "id": "MDRW2100002235.1.19",
          "form": "큽 카페인 잘안맞는데 딸수 있을지 그게 걱정이네요",
          "original_form": "큽 카페인 잘안맞는데 딸수 있을지 그게 걱정이네요",
          "speaker_id": "1",
          "time": "20210820 12:23"
        },
        {
          "id": "MDRW2100002235.1.20",
          "form": "자격증 딸때는 딸하는거보니 꼭 그렇지 않더라구요",
          "original_form": "자격증 딸때는 딸하는거보니 꼭 그렇지 않더라구요",
          "speaker_id": "2",
          "time": "20210820 12:24"
        },
        {
          "id": "MDRW2100002235.1.21",
          "form": "어려서 그냥 버렸어요",
          "original_form": "어려서 그냥 버렸어요",
          "speaker_id": "2",
          "time": "20210820 12:24"
        },
        {
          "id": "MDRW2100002235.1.22",
          "form": "오 그렇군요 한번 시간날때 배워봐야겠어요",
          "original_form": "오 그렇군요 한번 시간날때 배워봐야겠어요",
          "speaker_id": "1",
          "time": "20210820 12:24"
        },
        {
          "id": "MDRW2100002235.1.23",
          "form": "일반 프차 커피맛은 진짜 맛없겠어요",
          "original_form": "일반 프차 커피맛은 진짜 맛없겠어요",
          "speaker_id": "1",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002235.1.24",
          "form": "네 꼭 따보세요 필기도 쉬워요",
          "original_form": "네 꼭 따보세요 필기도 쉬워요",
          "speaker_id": "2",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002235.1.25",
          "form": "네 ㅋㅋ딸이 해준게 맛나요",
          "original_form": "네 ㅋㅋ딸이 해준게 맛나요",
          "speaker_id": "2",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002235.1.26",
          "form": "ㅋㅋㅋ부러워요 저도 언젠가 꼭 따서 맛있게먹어야지..",
          "original_form": "ㅋㅋㅋ부러워요 저도 언젠가 꼭 따서 맛있게먹어야지..",
          "speaker_id": "1",
          "time": "20210820 12:26"
        }
      ]
    }
