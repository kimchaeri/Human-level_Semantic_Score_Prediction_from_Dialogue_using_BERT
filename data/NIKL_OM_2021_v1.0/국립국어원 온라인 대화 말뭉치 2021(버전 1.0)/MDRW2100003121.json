      "utterance": [
        {
          "id": "MDRW2100003121.1.1",
          "form": "설것이완료하고 태마기행본다",
          "original_form": "설것이완료하고 태마기행본다",
          "speaker_id": "1",
          "time": "20210820 20:51"
        },
        {
          "id": "MDRW2100003121.1.2",
          "form": "수고하셨어요",
          "original_form": "수고하셨어요",
          "speaker_id": "2",
          "time": "20210820 20:52"
        },
        {
          "id": "MDRW2100003121.1.3",
          "form": "오늘은아프리카다",
          "original_form": "오늘은아프리카다",
          "speaker_id": "1",
          "time": "20210820 20:52"
        },
        {
          "id": "MDRW2100003121.1.4",
          "form": "아프리가좋죠 더울것같지만요",
          "original_form": "아프리가좋죠 더울것같지만요",
          "speaker_id": "2",
          "time": "20210820 20:52"
        },
        {
          "id": "MDRW2100003121.1.5",
          "form": "보기에는 시원해보이네",
          "original_form": "보기에는 시원해보이네",
          "speaker_id": "1",
          "time": "20210820 20:52"
        },
        {
          "id": "MDRW2100003121.1.6",
          "form": "태국가보셨잔아요 엄청덥죠",
          "original_form": "태국가보셨잔아요 엄청덥죠",
          "speaker_id": "2",
          "time": "20210820 20:53"
        },
        {
          "id": "MDRW2100003121.1.7",
          "form": "그렇긴해 저긴더하겠지",
          "original_form": "그렇긴해 저긴더하겠지",
          "speaker_id": "1",
          "time": "20210820 20:53"
        },
        {
          "id": "MDRW2100003121.1.8",
          "form": "물소리는시원하네요",
          "original_form": "물소리는시원하네요",
          "speaker_id": "2",
          "time": "20210820 20:53"
        },
        {
          "id": "MDRW2100003121.1.9",
          "form": "가을이빨리왔으면좋겠다",
          "original_form": "가을이빨리왔으면좋겠다",
          "speaker_id": "1",
          "time": "20210820 20:53"
        },
        {
          "id": "MDRW2100003121.1.10",
          "form": "처서자났으니 금방 가을오겠죠",
          "original_form": "처서자났으니 금방 가을오겠죠",
          "speaker_id": "2",
          "time": "20210820 20:54"
        },
        {
          "id": "MDRW2100003121.1.11",
          "form": "오늘환경포럼보니 그렇지않을것같다",
          "original_form": "오늘환경포럼보니 그렇지않을것같다",
          "speaker_id": "1",
          "time": "20210820 20:54"
        },
        {
          "id": "MDRW2100003121.1.12",
          "form": "열심히공부하시네요",
          "original_form": "열심히공부하시네요",
          "speaker_id": "2",
          "time": "20210820 20:54"
        },
        {
          "id": "MDRW2100003121.1.13",
          "form": "가족을위해서지",
          "original_form": "가족을위해서지",
          "speaker_id": "1",
          "time": "20210820 20:54"
        },
        {
          "id": "MDRW2100003121.1.14",
          "form": "알아요  고생많으세요",
          "original_form": "알아요  고생많으세요",
          "speaker_id": "2",
          "time": "20210820 20:55"
        },
        {
          "id": "MDRW2100003121.1.15",
          "form": "늦게다시공부하니 이거저거다하는거지",
          "original_form": "늦게다시공부하니 이거저거다하는거지",
          "speaker_id": "1",
          "time": "20210820 20:55"
        },
        {
          "id": "MDRW2100003121.1.16",
          "form": "올해지나면 상급학교 가시네요",
          "original_form": "올해지나면 상급학교 가시네요",
          "speaker_id": "2",
          "time": "20210820 20:55"
        },
        {
          "id": "MDRW2100003121.1.17",
          "form": "다 아들덕분이지",
          "original_form": "다 아들덕분이지",
          "speaker_id": "1",
          "time": "20210820 20:55"
        },
        {
          "id": "MDRW2100003121.1.18",
          "form": "멀요 나도 티비보안거죠",
          "original_form": "멀요 나도 티비보안거죠",
          "speaker_id": "2",
          "time": "20210820 20:56"
        },
        {
          "id": "MDRW2100003121.1.19",
          "form": "그래도 직접데리고가고 등록하고 고맙네",
          "original_form": "그래도 직접데리고가고 등록하고 고맙네",
          "speaker_id": "1",
          "time": "20210820 20:56"
        },
        {
          "id": "MDRW2100003121.1.20",
          "form": "엄마말고도 많은사람이 도전해요",
          "original_form": "엄마말고도 많은사람이 도전해요",
          "speaker_id": "2",
          "time": "20210820 20:56"
        },
        {
          "id": "MDRW2100003121.1.21",
          "form": "아프리카 트래킹좋네 900미터라네 해발",
          "original_form": "아프리카 트래킹좋네 900미터라네 해발",
          "speaker_id": "1",
          "time": "20210820 20:57"
        },
        {
          "id": "MDRW2100003121.1.22",
          "form": "엄마도 열심히하시면 높은곳에 올라갈꺼예요",
          "original_form": "엄마도 열심히하시면 높은곳에 올라갈꺼예요",
          "speaker_id": "2",
          "time": "20210820 20:57"
        },
        {
          "id": "MDRW2100003121.1.23",
          "form": "그래 그랬음 좋겠다",
          "original_form": "그래 그랬음 좋겠다",
          "speaker_id": "1",
          "time": "20210820 20:57"
        },
        {
          "id": "MDRW2100003121.1.24",
          "form": "먹는거나온다",
          "original_form": "먹는거나온다",
          "speaker_id": "1",
          "time": "20210820 20:58"
        },
        {
          "id": "MDRW2100003121.1.25",
          "form": "오 별로",
          "original_form": "오 별로",
          "speaker_id": "2",
          "time": "20210820 20:58"
        },
        {
          "id": "MDRW2100003121.1.26",
          "form": "하긴 밥먹었지",
          "original_form": "하긴 밥먹었지",
          "speaker_id": "1",
          "time": "20210820 20:58"
        },
        {
          "id": "MDRW2100003121.1.27",
          "form": "아뇨 제스탈아니예요",
          "original_form": "아뇨 제스탈아니예요",
          "speaker_id": "2",
          "time": "20210820 20:58"
        },
        {
          "id": "MDRW2100003121.1.28",
          "form": "군대음식같구나 담엔 군토킹해보자",
          "original_form": "군대음식같구나 담엔 군토킹해보자",
          "speaker_id": "1",
          "time": "20210820 20:59"
        },
        {
          "id": "MDRW2100003121.1.29",
          "form": "아 그건좀",
          "original_form": "아 그건좀",
          "speaker_id": "2",
          "time": "20210820 20:59"
        },
        {
          "id": "MDRW2100003121.1.30",
          "form": "강아지내놓았구나",
          "original_form": "강아지내놓았구나",
          "speaker_id": "1",
          "time": "20210820 21:05"
        },
        {
          "id": "MDRW2100003121.1.31",
          "form": "이친구유일한 락이죠 매일9시 잠시놀다가는시간",
          "original_form": "이친구유일한 락이죠 매일9시 잠시놀다가는시간",
          "speaker_id": "2",
          "time": "20210820 21:06"
        },
        {
          "id": "MDRW2100003121.1.32",
          "form": "하긴 안내주면안돼는시간이지",
          "original_form": "하긴 안내주면안돼는시간이지",
          "speaker_id": "1",
          "time": "20210820 21:06"
        },
        {
          "id": "MDRW2100003121.1.33",
          "form": "네 이친구도 다아나봐요",
          "original_form": "네 이친구도 다아나봐요",
          "speaker_id": "2",
          "time": "20210820 21:06"
        },
        {
          "id": "MDRW2100003121.1.34",
          "form": "개도 인간만큼은 아니지만  다생각이있지",
          "original_form": "개도 인간만큼은 아니지만  다생각이있지",
          "speaker_id": "1",
          "time": "20210820 21:07"
        },
        {
          "id": "MDRW2100003121.1.35",
          "form": "그렇긴해요",
          "original_form": "그렇긴해요",
          "speaker_id": "2",
          "time": "20210820 21:07"
        },
        {
          "id": "MDRW2100003121.1.36",
          "form": "좀사고치는쪽으로",
          "original_form": "좀사고치는쪽으로",
          "speaker_id": "1",
          "time": "20210820 21:07"
        },
        {
          "id": "MDRW2100003121.1.37",
          "form": "그게 노는거겠죠",
          "original_form": "그게 노는거겠죠",
          "speaker_id": "2",
          "time": "20210820 21:07"
        },
        {
          "id": "MDRW2100003121.1.38",
          "form": "오늘은조용하네",
          "original_form": "오늘은조용하네",
          "speaker_id": "1",
          "time": "20210820 21:08"
        },
        {
          "id": "MDRW2100003121.1.39",
          "form": "그때그때다르겄죠",
          "original_form": "그때그때다르겄죠",
          "speaker_id": "2",
          "time": "20210820 21:08"
        },
        {
          "id": "MDRW2100003121.1.40",
          "form": "오늘은  떨어진거없이 다치웠거든",
          "original_form": "오늘은  떨어진거없이 다치웠거든",
          "speaker_id": "1",
          "time": "20210820 21:08"
        },
        {
          "id": "MDRW2100003121.1.41",
          "form": "먹을게 없군요",
          "original_form": "먹을게 없군요",
          "speaker_id": "2",
          "time": "20210820 21:09"
        },
        {
          "id": "MDRW2100003121.1.42",
          "form": "사료는맛이없으니",
          "original_form": "사료는맛이없으니",
          "speaker_id": "1",
          "time": "20210820 21:09"
        },
        {
          "id": "MDRW2100003121.1.43",
          "form": "진짜로 아무맛도없죠",
          "original_form": "진짜로 아무맛도없죠",
          "speaker_id": "2",
          "time": "20210820 21:09"
        },
        {
          "id": "MDRW2100003121.1.44",
          "form": "사는게 재미없겠지",
          "original_form": "사는게 재미없겠지",
          "speaker_id": "1",
          "time": "20210820 21:09"
        },
        {
          "id": "MDRW2100003121.1.45",
          "form": "그렇기도하겠네요",
          "original_form": "그렇기도하겠네요",
          "speaker_id": "2",
          "time": "20210820 21:09"
        },
        {
          "id": "MDRW2100003121.1.46",
          "form": "인간보다 단순하니 우리보단나을게다",
          "original_form": "인간보다 단순하니 우리보단나을게다",
          "speaker_id": "1",
          "time": "20210820 21:10"
        },
        {
          "id": "MDRW2100003121.1.47",
          "form": "그렇긴해요",
          "original_form": "그렇긴해요",
          "speaker_id": "2",
          "time": "20210820 21:10"
        },
        {
          "id": "MDRW2100003121.1.48",
          "form": "내취향아님",
          "original_form": "내취향아님",
          "speaker_id": "2",
          "time": "20210820 21:10"
        },
        {
          "id": "MDRW2100003121.1.49",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "1",
          "time": "20210820 21:11"
        },
        {
          "id": "MDRW2100003121.1.50",
          "form": "나쁜단어썼다네요",
          "original_form": "나쁜단어썼다네요",
          "speaker_id": "2",
          "time": "20210820 21:11"
        },
        {
          "id": "MDRW2100003121.1.51",
          "form": "그렇네  조심해야지",
          "original_form": "그렇네  조심해야지",
          "speaker_id": "1",
          "time": "20210820 21:11"
        },
        {
          "id": "MDRW2100003121.1.52",
          "form": "우린 정말  소중한활동중이죠",
          "original_form": "우린 정말  소중한활동중이죠",
          "speaker_id": "2",
          "time": "20210820 21:12"
        },
        {
          "id": "MDRW2100003121.1.53",
          "form": "나도 좋고 너도좋고 좋은게조은거지",
          "original_form": "나도 좋고 너도좋고 좋은게조은거지",
          "speaker_id": "1",
          "time": "20210820 21:12"
        },
        {
          "id": "MDRW2100003121.1.54",
          "form": "살다보면 다그런거지 라고 그러네요",
          "original_form": "살다보면 다그런거지 라고 그러네요",
          "speaker_id": "2",
          "time": "20210820 21:12"
        },
        {
          "id": "MDRW2100003121.1.55",
          "form": "그게 진정한재미지",
          "original_form": "그게 진정한재미지",
          "speaker_id": "1",
          "time": "20210820 21:12"
        },
        {
          "id": "MDRW2100003121.1.56",
          "form": "그럴까요",
          "original_form": "그럴까요",
          "speaker_id": "2",
          "time": "20210820 21:12"
        },
        {
          "id": "MDRW2100003121.1.57",
          "form": "그럼",
          "original_form": "그럼",
          "speaker_id": "1",
          "time": "20210820 21:13"
        },
        {
          "id": "MDRW2100003121.1.58",
          "form": "커피나와요",
          "original_form": "커피나와요",
          "speaker_id": "2",
          "time": "20210820 21:13"
        },
        {
          "id": "MDRW2100003121.1.59",
          "form": "커피못먹어아쉽네",
          "original_form": "커피못먹어아쉽네",
          "speaker_id": "1",
          "time": "20210820 21:14"
        },
        {
          "id": "MDRW2100003121.1.60",
          "form": "잠이안오니그렇죠",
          "original_form": "잠이안오니그렇죠",
          "speaker_id": "2",
          "time": "20210820 21:14"
        },
        {
          "id": "MDRW2100003121.1.61",
          "form": "절을땐 엄청마셨지",
          "original_form": "절을땐 엄청마셨지",
          "speaker_id": "1",
          "time": "20210820 21:17"
        },
        {
          "id": "MDRW2100003121.1.62",
          "form": "커피없인 못사셨다며요",
          "original_form": "커피없인 못사셨다며요",
          "speaker_id": "2",
          "time": "20210820 21:18"
        },
        {
          "id": "MDRW2100003121.1.63",
          "form": "커피가 삶의 일부였지",
          "original_form": "커피가 삶의 일부였지",
          "speaker_id": "1",
          "time": "20210820 21:18"
        },
        {
          "id": "MDRW2100003121.1.64",
          "form": "어릴때보면 커피로 가득찼었죠 찬장이",
          "original_form": "어릴때보면 커피로 가득찼었죠 찬장이",
          "speaker_id": "2",
          "time": "20210820 21:18"
        },
        {
          "id": "MDRW2100003121.1.65",
          "form": "물론이지",
          "original_form": "물론이지",
          "speaker_id": "1",
          "time": "20210820 21:19"
        },
        {
          "id": "MDRW2100003121.1.66",
          "form": "단맛이 올라온데요",
          "original_form": "단맛이 올라온데요",
          "speaker_id": "2",
          "time": "20210820 21:19"
        },
        {
          "id": "MDRW2100003121.1.67",
          "form": "마시고는싶은데 먹으면못자",
          "original_form": "마시고는싶은데 먹으면못자",
          "speaker_id": "1",
          "time": "20210820 21:19"
        },
