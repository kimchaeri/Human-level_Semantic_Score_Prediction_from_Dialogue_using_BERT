      "utterance": [
        {
          "id": "MDRW2100034787.1.1",
          "form": "커피 좋아하세용?",
          "original_form": "커피 좋아하세용?",
          "speaker_id": "2",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100034787.1.2",
          "form": "네 아메리카노 자주 마셔요",
          "original_form": "네 아메리카노 자주 마셔요",
          "speaker_id": "1",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100034787.1.3",
          "form": "name2 님은요?",
          "original_form": "&name2& 님은요?",
          "speaker_id": "1",
          "time": "20210828 19:31"
        },
        {
          "id": "MDRW2100034787.1.4",
          "form": "저는 카페인때문에 커피는 못마시는데",
          "original_form": "저는 카페인때문에 커피는 못마시는데",
          "speaker_id": "2",
          "time": "20210828 19:31"
        },
        {
          "id": "MDRW2100034787.1.5",
          "form": "대신 프라푸치노나 스무디 자주 먹어용",
          "original_form": "대신 프라푸치노나 스무디 자주 먹어용",
          "speaker_id": "2",
          "time": "20210828 19:32"
        },
        {
          "id": "MDRW2100034787.1.6",
          "form": "카페인이 몸에 안 받으면 어쩔 수 없죠ㅠㅠ 저는 커피향을 좋아해서 자주 마셔요!",
          "original_form": "카페인이 몸에 안 받으면 어쩔 수 없죠ㅠㅠ 저는 커피향을 좋아해서 자주 마셔요!",
          "speaker_id": "1",
          "time": "20210828 19:33"
        },
        {
          "id": "MDRW2100034787.1.7",
          "form": "좋아하는 디저트 있으세요?",
          "original_form": "좋아하는 디저트 있으세요?",
          "speaker_id": "1",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100034787.1.8",
          "form": "아 맛아요 향은 진짜 너무 향긋하죠",
          "original_form": "아 맛아요 향은 진짜 너무 향긋하죠",
          "speaker_id": "2",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100034787.1.9",
          "form": "에그타르트랑 푸딩 좋아해요!",
          "original_form": "에그타르트랑 푸딩 좋아해요!",
          "speaker_id": "2",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100034787.1.10",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100034787.1.11",
          "form": "에그타르트 맛있죠! 저는 치즈케이크 제일 좋아해요~",
          "original_form": "에그타르트 맛있죠! 저는 치즈케이크 제일 좋아해요~",
          "speaker_id": "1",
          "time": "20210828 19:34"
        },
        {
          "id": "MDRW2100034787.1.12",
          "form": "우왕 달달하면서 쫀득한 그맛ㅋㅋㅋㅋ",
          "original_form": "우왕 달달하면서 쫀득한 그맛ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:35"
        },
        {
          "id": "MDRW2100034787.1.13",
          "form": "민트초코 좋아하세요?",
          "original_form": "민트초코 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 19:35"
        },
        {
          "id": "MDRW2100034787.1.14",
          "form": "민트맛을 잘 못 먹어서 민초는 안 좋아해요ㅎㅎ 민초 좋아하시나요?",
          "original_form": "민트맛을 잘 못 먹어서 민초는 안 좋아해요ㅎㅎ 민초 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210828 19:36"
        },
        {
          "id": "MDRW2100034787.1.15",
          "form": "넹ㅋㅋㅋ 저는 민초단이에용",
          "original_form": "넹ㅋㅋㅋ 저는 민초단이에용",
          "speaker_id": "2",
          "time": "20210828 19:37"
        },
        {
          "id": "MDRW2100034787.1.16",
          "form": "입안에서 화한 느낌이 넘 좋아요",
          "original_form": "입안에서 화한 느낌이 넘 좋아요",
          "speaker_id": "2",
          "time": "20210828 19:37"
        },
        {
          "id": "MDRW2100034787.1.17",
          "form": "저는 유독 민트의 화한 맛이 맵게 느껴져서ㅋㅋㅋ 박하사탕도 매워서 못 먹어요...ㅋㅋㅋㅋ",
          "original_form": "저는 유독 민트의 화한 맛이 맵게 느껴져서ㅋㅋㅋ 박하사탕도 매워서 못 먹어요...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:38"
        },
        {
          "id": "MDRW2100034787.1.18",
          "form": "아하ㅠㅠ 약간 화한거 안좋아하시는구나",
          "original_form": "아하ㅠㅠ 약간 화한거 안좋아하시는구나",
          "speaker_id": "2",
          "time": "20210828 19:38"
        },
        {
          "id": "MDRW2100034787.1.19",
          "form": "그럼 쌀국수에 고수는 드세요?",
          "original_form": "그럼 쌀국수에 고수는 드세요?",
          "speaker_id": "2",
          "time": "20210828 19:39"
        },
        {
          "id": "MDRW2100034787.1.20",
          "form": "고수 한번도 안 먹어 봐서 모르겠어요! 고수도 화한 맛인가요?",
          "original_form": "고수 한번도 안 먹어 봐서 모르겠어요! 고수도 화한 맛인가요?",
          "speaker_id": "1",
          "time": "20210828 19:40"
        },
        {
          "id": "MDRW2100034787.1.21",
          "form": "고수는 화하다기보다는",
          "original_form": "고수는 화하다기보다는",
          "speaker_id": "2",
          "time": "20210828 19:40"
        },
        {
          "id": "MDRW2100034787.1.22",
          "form": "안맞는다는 분들 말로는",
          "original_form": "안맞는다는 분들 말로는",
          "speaker_id": "2",
          "time": "20210828 19:40"
        },
        {
          "id": "MDRW2100034787.1.23",
          "form": "세제향 난다고 하더라구여ㅋㅋㅋ",
          "original_form": "세제향 난다고 하더라구여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:40"
        },
        {
          "id": "MDRW2100034787.1.24",
          "form": "앗ㅋㅋㅋ 그래서 호불호 갈리나봐요",
          "original_form": "앗ㅋㅋㅋ 그래서 호불호 갈리나봐요",
          "speaker_id": "1",
          "time": "20210828 19:41"
        }
      ]
    }
