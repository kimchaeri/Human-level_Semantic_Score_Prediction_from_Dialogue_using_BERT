      "utterance": [
        {
          "id": "MDRW2100023137.1.1",
          "form": "난 요즘 정신건강도 참 중요하다고 생각해",
          "original_form": "난 요즘 정신건강도 참 중요하다고 생각해",
          "speaker_id": "1",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100023137.1.2",
          "form": "너도 동의하니?",
          "original_form": "너도 동의하니?",
          "speaker_id": "1",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100023137.1.3",
          "form": "완전 중요해 정신건강",
          "original_form": "완전 중요해 정신건강",
          "speaker_id": "2",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100023137.1.4",
          "form": "몸도 중요하시만 마음이 제일 중요해",
          "original_form": "몸도 중요하시만 마음이 제일 중요해",
          "speaker_id": "2",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100023137.1.5",
          "form": "맞아 나도 그렇게 생각해",
          "original_form": "맞아 나도 그렇게 생각해",
          "speaker_id": "1",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100023137.1.6",
          "form": "난 요즘 평화롭게 명상하는 습관을 들이려고 노력해",
          "original_form": "난 요즘 평화롭게 명상하는 습관을 들이려고 노력해",
          "speaker_id": "1",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100023137.1.7",
          "form": "너도 명상해본 적 있니?",
          "original_form": "너도 명상해본 적 있니?",
          "speaker_id": "1",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100023137.1.8",
          "form": "우와 명상??",
          "original_form": "우와 명상??",
          "speaker_id": "2",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100023137.1.9",
          "form": "난 명상이 계속 실패하던데",
          "original_form": "난 명상이 계속 실패하던데",
          "speaker_id": "2",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100023137.1.10",
          "form": "아이고 왜 그럴까",
          "original_form": "아이고 왜 그럴까",
          "speaker_id": "1",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100023137.1.11",
          "form": "혼자 하니, 아니면 다른 사람들이랑 함께 하니?",
          "original_form": "혼자 하니, 아니면 다른 사람들이랑 함께 하니?",
          "speaker_id": "1",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100023137.1.12",
          "form": "명상하면 어때??",
          "original_form": "명상하면 어때??",
          "speaker_id": "2",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100023137.1.13",
          "form": "나는 혼자해",
          "original_form": "나는 혼자해",
          "speaker_id": "2",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100023137.1.14",
          "form": "하기 전보다 잡생각이 없어지고 좋아",
          "original_form": "하기 전보다 잡생각이 없어지고 좋아",
          "speaker_id": "1",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100023137.1.15",
          "form": "나도 혼자 해",
          "original_form": "나도 혼자 해",
          "speaker_id": "1",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100023137.1.16",
          "form": "노래틀면서 하니",
          "original_form": "노래틀면서 하니",
          "speaker_id": "2",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100023137.1.17",
          "form": "응 난 음악 들으면서 해",
          "original_form": "응 난 음악 들으면서 해",
          "speaker_id": "1",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100023137.1.18",
          "form": "너도 명상할때노래듣니?",
          "original_form": "너도 명상할때노래듣니?",
          "speaker_id": "1",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100023137.1.19",
          "form": "나는 유튜브에 명상 영상",
          "original_form": "나는 유튜브에 명상 영상",
          "speaker_id": "2",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100023137.1.20",
          "form": "틀고해",
          "original_form": "틀고해",
          "speaker_id": "2",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100023137.1.21",
          "form": "근데 뭔가 생각버리기가 힘들어",
          "original_form": "근데 뭔가 생각버리기가 힘들어",
          "speaker_id": "2",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100023137.1.22",
          "form": "나도 유튜브를 종종 봐",
          "original_form": "나도 유튜브를 종종 봐",
          "speaker_id": "1",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100023137.1.23",
          "form": "동감이야 나도 잘 안 될 때가 있더라",
          "original_form": "동감이야 나도 잘 안 될 때가 있더라",
          "speaker_id": "1",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100023137.1.24",
          "form": "그럴 때 노래를 바꿔 봐",
          "original_form": "그럴 때 노래를 바꿔 봐",
          "speaker_id": "1",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100023137.1.25",
          "form": "그렇구나 명상하면",
          "original_form": "그렇구나 명상하면",
          "speaker_id": "2",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100023137.1.26",
          "form": "많이 좋아진게 느껴져?",
          "original_form": "많이 좋아진게 느껴져?",
          "speaker_id": "2",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100023137.1.27",
          "form": "응 하기 전보다는 마음이 차분해져",
          "original_form": "응 하기 전보다는 마음이 차분해져",
          "speaker_id": "1",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100023137.1.28",
          "form": "생각은 사라지지 않지만 초조한 건 사라져",
          "original_form": "생각은 사라지지 않지만 초조한 건 사라져",
          "speaker_id": "1",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100023137.1.29",
          "form": "신기하다 니가 추천해준 방법으로도 한번 해볼게",
          "original_form": "신기하다 니가 추천해준 방법으로도 한번 해볼게",
          "speaker_id": "2",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100023137.1.30",
          "form": "응 좋은 생각이야ㅎㅎ",
          "original_form": "응 좋은 생각이야ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 22:12"
        }
      ]
    }
  ]
