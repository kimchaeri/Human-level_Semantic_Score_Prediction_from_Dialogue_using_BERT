      "utterance": [
        {
          "id": "MDRW2100046189.1.1",
          "form": "초반에몇편봤어 딘딘 겁나웃기던데",
          "original_form": "초반에몇편봤어 딘딘 겁나웃기던데",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046189.1.2",
          "form": "아니 거기 온유가 새로 합류하는데",
          "original_form": "아니 거기 온유가 새로 합류하는데",
          "speaker_id": "1",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046189.1.3",
          "form": "어때?",
          "original_form": "어때?",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046189.1.4",
          "form": "오늘 기사하나 떴거든",
          "original_form": "오늘 기사하나 떴거든",
          "speaker_id": "1",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046189.1.5",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046189.1.6",
          "form": "나도 방송은 안봤어",
          "original_form": "나도 방송은 안봤어",
          "speaker_id": "1",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046189.1.7",
          "form": "근데 500으로 10배 벌었때",
          "original_form": "근데 500으로 10배 벌었때",
          "speaker_id": "1",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046189.1.8",
          "form": "미쳤다 진짜? 나도알려줘",
          "original_form": "미쳤다 진짜? 나도알려줘",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046189.1.9",
          "form": "갑자기 찾아오는 상대적 박탈감..!",
          "original_form": "갑자기 찾아오는 상대적 박탈감..!",
          "speaker_id": "1",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046189.1.10",
          "form": "있는놈이더해",
          "original_form": "있는놈이더해",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046189.1.11",
          "form": "좋겠다 시드가 오백이라",
          "original_form": "좋겠다 시드가 오백이라",
          "speaker_id": "1",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046189.1.12",
          "form": "너도그정도있지않아?",
          "original_form": "너도그정도있지않아?",
          "speaker_id": "2",
          "time": "20210908 11:06"
        },
        {
          "id": "MDRW2100046189.1.13",
          "form": "그정도는 있는데 배포가 없네?",
          "original_form": "그정도는 있는데 배포가 없네?",
          "speaker_id": "1",
          "time": "20210908 11:07"
        },
        {
          "id": "MDRW2100046189.1.14",
          "form": "요즘 주식방송 많은데 좀 챙겨볼까",
          "original_form": "요즘 주식방송 많은데 좀 챙겨볼까",
          "speaker_id": "1",
          "time": "20210908 11:07"
