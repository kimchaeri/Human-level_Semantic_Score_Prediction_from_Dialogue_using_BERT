      "utterance": [
        {
          "id": "MDRW2100012565.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100012565.1.2",
          "form": "안녕하세요ㅎㅎ",
          "original_form": "안녕하세요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012565.1.3",
          "form": "저녁은 뭐드셨나요?",
          "original_form": "저녁은 뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012565.1.4",
          "form": "전 치킨이요",
          "original_form": "전 치킨이요",
          "speaker_id": "1",
          "time": "20210824 23:35"
        },
        {
          "id": "MDRW2100012565.1.5",
          "form": "치킨좋아하세요?",
          "original_form": "치킨좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 23:35"
        },
        {
          "id": "MDRW2100012565.1.6",
          "form": "치킨좋아하죠ㅎㅎ",
          "original_form": "치킨좋아하죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100012565.1.7",
          "form": "어디브랜드 좋아하세요?",
          "original_form": "어디브랜드 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100012565.1.8",
          "form": "전 페리카나요",
          "original_form": "전 페리카나요",
          "speaker_id": "1",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100012565.1.9",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100012565.1.10",
          "form": "오 전 비슷한 처갓집이요ㅎ",
          "original_form": "오 전 비슷한 처갓집이요ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100012565.1.11",
          "form": "왠지 옛날맛같고ㅎ 또 사라다주는게 너무좋아요ㅎㅎ",
          "original_form": "왠지 옛날맛같고ㅎ 또 사라다주는게 너무좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100012565.1.12",
          "form": "다른치킨은요?",
          "original_form": "다른치킨은요?",
          "speaker_id": "2",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100012565.1.13",
          "form": "처갓집도좋죠ㅋㅋㅋ 전다좋아요",
          "original_form": "처갓집도좋죠ㅋㅋㅋ 전다좋아요",
          "speaker_id": "1",
          "time": "20210824 23:58"
        },
        {
          "id": "MDRW2100012565.1.14",
          "form": "양념vs후라이드?",
          "original_form": "양념vs후라이드?",
          "speaker_id": "1",
          "time": "20210824 23:58"
        },
        {
          "id": "MDRW2100012565.1.15",
          "form": "치킨은 후라이드죠ㅎ",
          "original_form": "치킨은 후라이드죠ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:02"
        },
        {
          "id": "MDRW2100012565.1.16",
          "form": "양념은 더블로 시키고 기호에 맞게 머스타드나 다른소스먹어야죠ㅎㅎ",
          "original_form": "양념은 더블로 시키고 기호에 맞게 머스타드나 다른소스먹어야죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:02"
        },
        {
          "id": "MDRW2100012565.1.17",
          "form": "순살이좋으세요? 뼈가좋으세요?",
          "original_form": "순살이좋으세요? 뼈가좋으세요?",
          "speaker_id": "2",
          "time": "20210825 00:03"
        },
        {
          "id": "MDRW2100012565.1.18",
          "form": "전 양념파입니다ㅋㅋㅋㅋㅋ",
          "original_form": "전 양념파입니다ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:10"
        },
        {
          "id": "MDRW2100012565.1.19",
          "form": "거기에 뼈파요",
          "original_form": "거기에 뼈파요",
          "speaker_id": "1",
          "time": "20210825 00:10"
        },
        {
          "id": "MDRW2100012565.1.20",
          "form": "저는 후라이드가 싫어요ㅠㅠ",
          "original_form": "저는 후라이드가 싫어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 00:11"
        },
        {
          "id": "MDRW2100012565.1.21",
          "form": "그러시구나ㅎㅎ",
          "original_form": "그러시구나ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:14"
        },
        {
          "id": "MDRW2100012565.1.22",
          "form": "후라이드는 기름져서 싫으신거에요?ㅎ",
          "original_form": "후라이드는 기름져서 싫으신거에요?ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:14"
        },
        {
          "id": "MDRW2100012565.1.23",
          "form": "저도 뼈ㅎㅎ 발라먹는재미가있어서ㅎㅎ",
          "original_form": "저도 뼈ㅎㅎ 발라먹는재미가있어서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:14"
        },
        {
          "id": "MDRW2100012565.1.24",
          "form": "갑자기 배고파졌음요ㅎ",
          "original_form": "갑자기 배고파졌음요ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:14"
        },
        {
          "id": "MDRW2100012565.1.25",
          "form": "저는 뭔가 후라시드는",
          "original_form": "저는 뭔가 후라시드는",
          "speaker_id": "1",
          "time": "20210825 00:20"
        },
        {
          "id": "MDRW2100012565.1.26",
          "form": "밋밋하더라고요ㅋㅋㅋㅋ",
          "original_form": "밋밋하더라고요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:20"
        },
        {
          "id": "MDRW2100012565.1.27",
          "form": "아무래도 시간대가ㅠ",
          "original_form": "아무래도 시간대가ㅠ",
          "speaker_id": "1",
          "time": "20210825 00:20"
        },
        {
          "id": "MDRW2100012565.1.28",
          "form": "야식먹고 쓰러져 자기 딱좋은시간이죠ㅎㅎㅎ",
          "original_form": "야식먹고 쓰러져 자기 딱좋은시간이죠ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:22"
        },
        {
          "id": "MDRW2100012565.1.29",
          "form": "이래서 살을 못빼나봅니다ㅎㅎ",
          "original_form": "이래서 살을 못빼나봅니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:22"
        },
        {
          "id": "MDRW2100012565.1.30",
          "form": "요즘 근데 배달음식이 너무 좋아요ㅎ",
          "original_form": "요즘 근데 배달음식이 너무 좋아요ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:22"
        },
        {
          "id": "MDRW2100012565.1.31",
          "form": "치킨말고 뭐 시켜드세뇨?",
          "original_form": "치킨말고 뭐 시켜드세뇨?",
          "speaker_id": "2",
          "time": "20210825 00:22"
        },
        {
          "id": "MDRW2100012565.1.32",
          "form": "저도요ㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:30"
        },
        {
          "id": "MDRW2100012565.1.33",
          "form": "전 초밥이요",
          "original_form": "전 초밥이요",
          "speaker_id": "1",
          "time": "20210825 00:30"
        },
        {
          "id": "MDRW2100012565.1.34",
          "form": "전 배달음식",
          "original_form": "전 배달음식",
          "speaker_id": "1",
          "time": "20210825 00:30"
        },
        {
          "id": "MDRW2100012565.1.35",
          "form": "초밥을 많이시켜먹어요ㅋㅋㅋㅋ",
          "original_form": "초밥을 많이시켜먹어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:30"
        }
      ]
