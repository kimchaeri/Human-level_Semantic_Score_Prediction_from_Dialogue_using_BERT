      "utterance": [
        {
          "id": "MDRW2100024728.1.1",
          "form": "아깝다 비행기값만 좀 싸면 좋겠는데",
          "original_form": "아깝다 비행기값만 좀 싸면 좋겠는데",
          "speaker_id": "2",
          "time": "20210831 17:39"
        },
        {
          "id": "MDRW2100024728.1.2",
          "form": "내년 9월쯤에 갈까싶어",
          "original_form": "내년 9월쯤에 갈까싶어",
          "speaker_id": "2",
          "time": "20210831 17:40"
        },
        {
          "id": "MDRW2100024728.1.3",
          "form": "맞아 비용이 많이 들어서 놀러가는 건 보류잖니",
          "original_form": "맞아 비용이 많이 들어서 놀러가는 건 보류잖니",
          "speaker_id": "1",
          "time": "20210831 17:40"
        },
        {
          "id": "MDRW2100024728.1.4",
          "form": "가을쯤에 가네? 메이플시럽 맛있겠다",
          "original_form": "가을쯤에 가네? 메이플시럽 맛있겠다",
          "speaker_id": "1",
          "time": "20210831 17:40"
        },
        {
          "id": "MDRW2100024728.1.5",
          "form": "숙소는 어떻게 하기로 했어?",
          "original_form": "숙소는 어떻게 하기로 했어?",
          "speaker_id": "1",
          "time": "20210831 17:41"
        },
        {
          "id": "MDRW2100024728.1.6",
          "form": "니 선물로 시럽사와야겠다",
          "original_form": "니 선물로 시럽사와야겠다",
          "speaker_id": "2",
          "time": "20210831 17:41"
        },
        {
          "id": "MDRW2100024728.1.7",
          "form": "여행 최대문제 = 숙소",
          "original_form": "여행 최대문제 = 숙소",
          "speaker_id": "1",
          "time": "20210831 17:41"
        },
        {
          "id": "MDRW2100024728.1.8",
          "form": "숙소는 친구가 아마도 묵고있는 집에",
          "original_form": "숙소는 친구가 아마도 묵고있는 집에",
          "speaker_id": "2",
          "time": "20210831 17:41"
        },
        {
          "id": "MDRW2100024728.1.9",
          "form": "얹혀지내지않을까",
          "original_form": "얹혀지내지않을까",
          "speaker_id": "2",
          "time": "20210831 17:41"
        },
        {
          "id": "MDRW2100024728.1.10",
          "form": "한 달정도면 진짜 좋은 추억 많이 생기겠다",
          "original_form": "한 달정도면 진짜 좋은 추억 많이 생기겠다",
          "speaker_id": "1",
          "time": "20210831 17:42"
        },
        {
          "id": "MDRW2100024728.1.11",
          "form": "난 여행가기 전이면 늘 의사소통 어떻게 할까 궁리하는데",
          "original_form": "난 여행가기 전이면 늘 의사소통 어떻게 할까 궁리하는데",
          "speaker_id": "1",
          "time": "20210831 17:42"
        },
        {
          "id": "MDRW2100024728.1.12",
          "form": "여차하면 에어비앤비로",
          "original_form": "여차하면 에어비앤비로",
          "speaker_id": "2",
          "time": "20210831 17:42"
        },
        {
          "id": "MDRW2100024728.1.13",
          "form": "한달빌릴까 고민중",
          "original_form": "한달빌릴까 고민중",
          "speaker_id": "2",
          "time": "20210831 17:42"
        },
        {
          "id": "MDRW2100024728.1.14",
          "form": "ㅋㅋㅋㅋㅌㅋ통 크게 플렉스하네",
          "original_form": "ㅋㅋㅋㅋㅌㅋ통 크게 플렉스하네",
          "speaker_id": "1",
          "time": "20210831 17:43"
        },
        {
          "id": "MDRW2100024728.1.15",
          "form": "회화부지런히 공부해야겠다",
          "original_form": "회화부지런히 공부해야겠다",
          "speaker_id": "2",
          "time": "20210831 17:43"
        },
        {
          "id": "MDRW2100024728.1.16",
          "form": "인생에 이때아니면 갈",
          "original_form": "인생에 이때아니면 갈",
          "speaker_id": "2",
          "time": "20210831 17:43"
        },
        {
          "id": "MDRW2100024728.1.17",
          "form": "기회가 없을것같아",
          "original_form": "기회가 없을것같아",
          "speaker_id": "2",
          "time": "20210831 17:43"
        },
        {
          "id": "MDRW2100024728.1.18",
          "form": "갈수는 있겠지만 갈 엄두가 안난달까",
          "original_form": "갈수는 있겠지만 갈 엄두가 안난달까",
          "speaker_id": "2",
          "time": "20210831 17:43"
        },
        {
          "id": "MDRW2100024728.1.19",
          "form": "맞아 사람들이 입을 모아 말하는 게",
          "original_form": "맞아 사람들이 입을 모아 말하는 게",
          "speaker_id": "1",
          "time": "20210831 17:43"
        },
        {
          "id": "MDRW2100024728.1.20",
          "form": "젊을 때 시간 날 때 여행 좀 가볼걸 이 말이더라구",
          "original_form": "젊을 때 시간 날 때 여행 좀 가볼걸 이 말이더라구",
          "speaker_id": "1",
          "time": "20210831 17:44"
        },
        {
          "id": "MDRW2100024728.1.21",
          "form": "그래서  큰맘먹고 적금깨서 다녀오게",
          "original_form": "그래서  큰맘먹고 적금깨서 다녀오게",
          "speaker_id": "2",
          "time": "20210831 17:44"
        },
        {
          "id": "MDRW2100024728.1.22",
          "form": "가서 여행일지도 많이 써 봐봐",
          "original_form": "가서 여행일지도 많이 써 봐봐",
          "speaker_id": "1",
          "time": "20210831 17:45"
        },
        {
          "id": "MDRW2100024728.1.23",
          "form": "일기쓰고 자면 재밌겠다",
          "original_form": "일기쓰고 자면 재밌겠다",
          "speaker_id": "2",
          "time": "20210831 17:45"
        },
        {
          "id": "MDRW2100024728.1.24",
          "form": "보통 여행다녀온걸로 책쓰던데",
          "original_form": "보통 여행다녀온걸로 책쓰던데",
          "speaker_id": "2",
          "time": "20210831 17:45"
        },
        {
          "id": "MDRW2100024728.1.25",
          "form": "그러게 진짜 기록으로 남겨야 여행기억을 선명하게 남길 수 있겠더라고",
          "original_form": "그러게 진짜 기록으로 남겨야 여행기억을 선명하게 남길 수 있겠더라고",
          "speaker_id": "1",
          "time": "20210831 17:46"
        },
        {
          "id": "MDRW2100024728.1.26",
          "form": "인스타에 만화형식으로 올려서 책파는거 어때",
          "original_form": "인스타에 만화형식으로 올려서 책파는거 어때",
          "speaker_id": "2",
          "time": "20210831 17:46"
        }
      ]
    }
