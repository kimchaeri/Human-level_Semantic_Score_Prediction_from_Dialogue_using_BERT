      "utterance": [
        {
          "id": "MDRW2100023377.1.1",
          "form": "이제 투자이야기는 그만해야징ㅎㅎ",
          "original_form": "이제 투자이야기는 그만해야징ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:08"
        },
        {
          "id": "MDRW2100023377.1.2",
          "form": "ㅋㅋㅋㅋ 괜찮은데요",
          "original_form": "ㅋㅋㅋㅋ 괜찮은데요",
          "speaker_id": "1",
          "time": "20210830 01:08"
        },
        {
          "id": "MDRW2100023377.1.3",
          "form": "그래서 그걸로 따면 매꾸고 따면 매꾸고 하고잇어요ㅎ",
          "original_form": "그래서 그걸로 따면 매꾸고 따면 매꾸고 하고잇어요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:08"
        },
        {
          "id": "MDRW2100023377.1.4",
          "form": "그래서 지금까지 버틴거에요ㅎㅎ",
          "original_form": "그래서 지금까지 버틴거에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:08"
        },
        {
          "id": "MDRW2100023377.1.5",
          "form": "저도 한때는 공부하고 싶었는대 ㅎㅎ",
          "original_form": "저도 한때는 공부하고 싶었는대 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:08"
        },
        {
          "id": "MDRW2100023377.1.6",
          "form": "작년에 500튜자해서 천만원만들었구요",
          "original_form": "작년에 500튜자해서 천만원만들었구요",
          "speaker_id": "2",
          "time": "20210830 01:08"
        },
        {
          "id": "MDRW2100023377.1.7",
          "form": "공부는 별거없는것강애요ㅎ",
          "original_form": "공부는 별거없는것강애요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:09"
        },
        {
          "id": "MDRW2100023377.1.8",
          "form": "방해꾼이 계셔서요",
          "original_form": "방해꾼이 계셔서요",
          "speaker_id": "1",
          "time": "20210830 01:09"
        },
        {
          "id": "MDRW2100023377.1.9",
          "form": "저도 공부안하고해요ㅎㅎ 근데 그건봐요ㅎ 내가 투자하는 종목이 어떤거고 미래 성장가치는 얼마나 있을까만 판단래요ㅎ",
          "original_form": "저도 공부안하고해요ㅎㅎ 근데 그건봐요ㅎ 내가 투자하는 종목이 어떤거고 미래 성장가치는 얼마나 있을까만 판단래요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:09"
        },
        {
          "id": "MDRW2100023377.1.10",
          "form": "공부 전혀 안되요 ㅎㅎㅎ",
          "original_form": "공부 전혀 안되요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:09"
        },
        {
          "id": "MDRW2100023377.1.11",
          "form": "아아아ㅎ 바깥양반?",
          "original_form": "아아아ㅎ 바깥양반?",
          "speaker_id": "2",
          "time": "20210830 01:09"
        },
        {
          "id": "MDRW2100023377.1.12",
          "form": "ㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:09"
        },
        {
          "id": "MDRW2100023377.1.13",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210830 01:09"
        },
        {
          "id": "MDRW2100023377.1.14",
          "form": "그럼?",
          "original_form": "그럼?",
          "speaker_id": "2",
          "time": "20210830 01:10"
        },
        {
          "id": "MDRW2100023377.1.15",
          "form": "애기 있죠 ㅎㅎ",
          "original_form": "애기 있죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:10"
        },
        {
          "id": "MDRW2100023377.1.16",
          "form": "아아ㅎㅎㅎ",
          "original_form": "아아ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:10"
        },
        {
          "id": "MDRW2100023377.1.17",
          "form": "사랑스러운데 왜요 ㅡㅎㅎ",
          "original_form": "사랑스러운데 왜요 ㅡㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:10"
        },
        {
          "id": "MDRW2100023377.1.18",
          "form": "음 체력이",
          "original_form": "음 체력이",
          "speaker_id": "1",
          "time": "20210830 01:10"
        },
        {
          "id": "MDRW2100023377.1.19",
          "form": "근데 시간이",
          "original_form": "근데 시간이",
          "speaker_id": "2",
          "time": "20210830 01:10"
        },
        {
          "id": "MDRW2100023377.1.20",
          "form": "제가 못따라가요 허헙",
          "original_form": "제가 못따라가요 허헙",
          "speaker_id": "1",
          "time": "20210830 01:11"
        },
        {
          "id": "MDRW2100023377.1.21",
          "form": "육퇴할시간은아니시던데?",
          "original_form": "육퇴할시간은아니시던데?",
          "speaker_id": "2",
          "time": "20210830 01:11"
        },
        {
          "id": "MDRW2100023377.1.22",
          "form": "다 그렇죠ㅎ 애들 체력은 못따라가요ㅎ",
          "original_form": "다 그렇죠ㅎ 애들 체력은 못따라가요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:11"
        },
        {
          "id": "MDRW2100023377.1.23",
          "form": "아 전 수면패턴 잘잡아놔서",
          "original_form": "아 전 수면패턴 잘잡아놔서",
          "speaker_id": "1",
          "time": "20210830 01:11"
        },
        {
          "id": "MDRW2100023377.1.24",
          "form": "아아아ㅎ",
          "original_form": "아아아ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:11"
        },
        {
          "id": "MDRW2100023377.1.25",
          "form": "애기 규칙적이에요",
          "original_form": "애기 규칙적이에요",
          "speaker_id": "1",
          "time": "20210830 01:11"
        },
        {
          "id": "MDRW2100023377.1.26",
          "form": "우와 좋겟당ㅠ",
          "original_form": "우와 좋겟당ㅠ",
          "speaker_id": "2",
          "time": "20210830 01:11"
        },
        {
          "id": "MDRW2100023377.1.27",
          "form": "전 맨날 늦게자요",
          "original_form": "전 맨날 늦게자요",
          "speaker_id": "2",
          "time": "20210830 01:11"
        },
        {
          "id": "MDRW2100023377.1.28",
          "form": "헐 공주님 늦게자 ㅜㅜ",
          "original_form": "헐 공주님 늦게자 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 01:12"
        },
        {
          "id": "MDRW2100023377.1.29",
          "form": "아이가 11시넘어서잘때도있고 보통 10시반ㅠ",
          "original_form": "아이가 11시넘어서잘때도있고 보통 10시반ㅠ",
          "speaker_id": "2",
          "time": "20210830 01:12"
        },
        {
          "id": "MDRW2100023377.1.30",
          "form": "5살인데 시간되면 가서 자요",
          "original_form": "5살인데 시간되면 가서 자요",
          "speaker_id": "1",
          "time": "20210830 01:13"
        }
      ]
