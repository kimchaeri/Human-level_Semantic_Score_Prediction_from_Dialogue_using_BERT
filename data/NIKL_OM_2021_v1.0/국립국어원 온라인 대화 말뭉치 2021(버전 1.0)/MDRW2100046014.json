        },
        {
          "id": "MDRW2100046014.1.3",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210908 09:36"
        },
        {
          "id": "MDRW2100046014.1.4",
          "form": "ㅠ 전 살쩌서 ㅋㅋㅋㅋ",
          "original_form": "ㅠ 전 살쩌서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:37"
        },
        {
          "id": "MDRW2100046014.1.5",
          "form": "맞는게없어서 사야햇어요",
          "original_form": "맞는게없어서 사야햇어요",
          "speaker_id": "2",
          "time": "20210908 09:37"
        },
        {
          "id": "MDRW2100046014.1.6",
          "form": "ㅠ많이찌셨나용",
          "original_form": "ㅠ많이찌셨나용",
          "speaker_id": "1",
          "time": "20210908 09:38"
        },
        {
          "id": "MDRW2100046014.1.7",
          "form": "네네ㅠ",
          "original_form": "네네ㅠ",
          "speaker_id": "2",
          "time": "20210908 09:38"
        },
        {
          "id": "MDRW2100046014.1.8",
          "form": "재작년보다 작년 작년보다 올해네요ㅠㅠㅋㅋㅋ",
          "original_form": "재작년보다 작년 작년보다 올해네요ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:38"
        },
        {
          "id": "MDRW2100046014.1.9",
          "form": "옷만늫어요",
          "original_form": "옷만늫어요",
          "speaker_id": "2",
          "time": "20210908 09:38"
        },
        {
          "id": "MDRW2100046014.1.10",
          "form": "큐 돌아가면되죠",
