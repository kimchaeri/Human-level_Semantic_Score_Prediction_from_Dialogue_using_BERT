      "utterance": [
        {
          "id": "MDRW2100026411.1.1",
          "form": "짬뽕아니니??",
          "original_form": "짬뽕아니니??",
          "speaker_id": "2",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100026411.1.2",
          "form": "정답정답정답!!!!!!",
          "original_form": "정답정답정답!!!!!!",
          "speaker_id": "1",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100026411.1.3",
          "form": "뭐야 너도 잘 맞추잖아",
          "original_form": "뭐야 너도 잘 맞추잖아",
          "speaker_id": "1",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100026411.1.4",
          "form": "하하하하 우리 바운더리가 좁은걸지도",
          "original_form": "하하하하 우리 바운더리가 좁은걸지도",
          "speaker_id": "2",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100026411.1.5",
          "form": "식재료 뭐 들어가는지 별로 안 물어봤는데도",
          "original_form": "식재료 뭐 들어가는지 별로 안 물어봤는데도",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026411.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026411.1.7",
          "form": "이번엔 내가 맞춰보겠어",
          "original_form": "이번엔 내가 맞춰보겠어",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026411.1.8",
          "form": "정했어",
          "original_form": "정했어",
          "speaker_id": "2",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026411.1.9",
          "form": "좋아",
          "original_form": "좋아",
          "speaker_id": "1",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026411.1.10",
          "form": "한식이니?",
          "original_form": "한식이니?",
          "speaker_id": "1",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100026411.1.11",
          "form": "아니야 한식쟁이 물러났어",
          "original_form": "아니야 한식쟁이 물러났어",
          "speaker_id": "2",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026411.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026411.1.13",
          "form": "양식이니?",
          "original_form": "양식이니?",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026411.1.14",
          "form": "양식도 아니야",
          "original_form": "양식도 아니야",
          "speaker_id": "2",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100026411.1.15",
          "form": "재료를 물어보는건 어때",
          "original_form": "재료를 물어보는건 어때",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100026411.1.16",
          "form": "밀가루 들어가?",
          "original_form": "밀가루 들어가?",
          "speaker_id": "1",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100026411.1.17",
          "form": "밀가루도 좀 들어간다고 알고있긴해",
          "original_form": "밀가루도 좀 들어간다고 알고있긴해",
          "speaker_id": "2",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026411.1.18",
          "form": "그럼 밀가루가 주는 아니구만",
          "original_form": "그럼 밀가루가 주는 아니구만",
          "speaker_id": "1",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100026411.1.19",
          "form": "국물요리야?",
          "original_form": "국물요리야?",
          "speaker_id": "1",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100026411.1.20",
          "form": "국물요리긴한데 많이 물기가없어",
          "original_form": "국물요리긴한데 많이 물기가없어",
          "speaker_id": "2",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100026411.1.21",
          "form": "빛깔이 무슨 색이니",
          "original_form": "빛깔이 무슨 색이니",
          "speaker_id": "1",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100026411.1.22",
          "form": "아 이건거의 정답을 말해주는 정도인데",
          "original_form": "아 이건거의 정답을 말해주는 정도인데",
          "speaker_id": "2",
          "time": "20210901 22:18"
        },
        {
