      "utterance": [
        {
          "id": "MDRW2100033889.1.1",
          "form": "비가 꾸준히 오네요.",
          "original_form": "비가 꾸준히 오네요.",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100033889.1.2",
          "form": "그러게요 어제해나는거같더니",
          "original_form": "그러게요 어제해나는거같더니",
          "speaker_id": "1",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100033889.1.3",
          "form": "퇴근길에 비좀오고",
          "original_form": "퇴근길에 비좀오고",
          "speaker_id": "1",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100033889.1.4",
          "form": "비많이오나요?",
          "original_form": "비많이오나요?",
          "speaker_id": "1",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100033889.1.5",
          "form": "조금씩 계속 오고 있어요.",
          "original_form": "조금씩 계속 오고 있어요.",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100033889.1.6",
          "form": "여름에나 좀 올 것이지\r\n담 달 초까지 비 온다는 가 같아요",
          "original_form": "여름에나 좀 올 것이지\r\n담 달 초까지 비 온다는 가 같아요",
          "speaker_id": "2",
          "time": "20210825 15:12"
        },
        {
          "id": "MDRW2100033889.1.7",
          "form": "담달까지온대요?",
          "original_form": "담달까지온대요?",
          "speaker_id": "1",
          "time": "20210825 15:22"
        },
        {
          "id": "MDRW2100033889.1.8",
          "form": "진짜 가을장마인가보네요",
          "original_form": "진짜 가을장마인가보네요",
          "speaker_id": "1",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100033889.1.9",
          "form": "9초까지 온다는데.. 비 피해 없이 지나갔음 좋겠어요",
          "original_form": "9초까지 온다는데.. 비 피해 없이 지나갔음 좋겠어요",
          "speaker_id": "2",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100033889.1.10",
          "form": "많이 오는건아닌가봐요 태풍도 지나갔고",
          "original_form": "많이 오는건아닌가봐요 태풍도 지나갔고",
          "speaker_id": "1",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100033889.1.11",
          "form": "이러다 장마끝남 겨울오겠어요ㅋ",
          "original_form": "이러다 장마끝남 겨울오겠어요ㅋ",
          "speaker_id": "1",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100033889.1.12",
          "form": "가을이 비에 없어지는 건 아니겠죠..",
          "original_form": "가을이 비에 없어지는 건 아니겠죠..",
          "speaker_id": "2",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100033889.1.13",
          "form": "그니까요 오늘 좀덥고 습하긴해요",
          "original_form": "그니까요 오늘 좀덥고 습하긴해요",
          "speaker_id": "1",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100033889.1.14",
          "form": "여긴 어제 덥고 습했는데\r\n오늘은 계속 비와서 선선해요",
          "original_form": "여긴 어제 덥고 습했는데\r\n오늘은 계속 비와서 선선해요",
          "speaker_id": "2",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100033889.1.15",
          "form": "여긴 좀덥네요 올만에 바지를입어서그런가 ㅎㅎ",
          "original_form": "여긴 좀덥네요 올만에 바지를입어서그런가 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100033889.1.16",
          "form": "밤에는 확실히 춥더라구요\r\n이불 안 덮었는데 이불 덮어야 해옷",
          "original_form": "밤에는 확실히 춥더라구요\r\n이불 안 덮었는데 이불 덮어야 해옷",
          "speaker_id": "2",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100033889.1.17",
          "form": "아파트면 환기가 잘되니 그럴듯요",
          "original_form": "아파트면 환기가 잘되니 그럴듯요",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100033889.1.18",
          "form": "전 아직 에어컨을~~~ㅎ",
          "original_form": "전 아직 에어컨을~~~ㅎ",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100033889.1.19",
          "form": "시골 주택인데 산 밑 좀 더 춥게 느껴지나 보ㅏ요",
          "original_form": "시골 주택인데 산 밑 좀 더 춥게 느껴지나 보ㅏ요",
          "speaker_id": "2",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100033889.1.20",
          "form": "아 그럼 선선하겠네요",
          "original_form": "아 그럼 선선하겠네요",
          "speaker_id": "1",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100033889.1.21",
          "form": "공기는 좋겠어요^^",
          "original_form": "공기는 좋겠어요^^",
          "speaker_id": "1",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100033889.1.22",
          "form": "공기 좋지요~~",
          "original_form": "공기 좋지요~~",
          "speaker_id": "2",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100033889.1.23",
