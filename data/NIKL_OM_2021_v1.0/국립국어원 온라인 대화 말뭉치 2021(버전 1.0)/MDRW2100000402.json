      "utterance": [
        {
          "id": "MDRW2100000402.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210819 15:51"
        },
        {
          "id": "MDRW2100000402.1.2",
          "form": "안녕하세요! 요즘 하고 계신 게임 있으신가요??",
          "original_form": "안녕하세요! 요즘 하고 계신 게임 있으신가요??",
          "speaker_id": "2",
          "time": "20210819 15:53"
        },
        {
          "id": "MDRW2100000402.1.3",
          "form": "저 요즘 롤 시작했어요!!",
          "original_form": "저 요즘 롤 시작했어요!!",
          "speaker_id": "1",
          "time": "20210819 15:53"
        },
        {
          "id": "MDRW2100000402.1.4",
          "form": "저도 유투브 보니까 롤 재밌어보여서 해봤는데 욕만먹고 관뒀어요 ㅠ 재밌나요??",
          "original_form": "저도 유투브 보니까 롤 재밌어보여서 해봤는데 욕만먹고 관뒀어요 ㅠ 재밌나요??",
          "speaker_id": "2",
          "time": "20210819 15:59"
        },
        {
          "id": "MDRW2100000402.1.5",
          "form": "저도 욕만 먹어서..친구들이랑만 같이해요",
          "original_form": "저도 욕만 먹어서..친구들이랑만 같이해요",
          "speaker_id": "1",
          "time": "20210819 16:05"
        },
        {
          "id": "MDRW2100000402.1.6",
          "form": "ㅎㅎ 주로 어떤 챔피언으로 하시나요?",
          "original_form": "ㅎㅎ 주로 어떤 챔피언으로 하시나요?",
          "speaker_id": "2",
          "time": "20210819 16:06"
        },
        {
          "id": "MDRW2100000402.1.7",
          "form": "저는 럭스많이해요!",
          "original_form": "저는 럭스많이해요!",
          "speaker_id": "1",
          "time": "20210819 16:08"
        },
        {
          "id": "MDRW2100000402.1.8",
          "form": "님은 모스트가 무엇인가요?",
          "original_form": "님은 모스트가 무엇인가요?",
          "speaker_id": "1",
          "time": "20210819 16:08"
        },
        {
          "id": "MDRW2100000402.1.9",
          "form": "모스트가 무슨말인가요??",
          "original_form": "모스트가 무슨말인가요??",
          "speaker_id": "2",
          "time": "20210819 16:09"
        },
        {
          "id": "MDRW2100000402.1.10",
          "form": "저도 첨에 럭스로 배웠습니다 ㅎㅎ",
          "original_form": "저도 첨에 럭스로 배웠습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 16:09"
        },
        {
          "id": "MDRW2100000402.1.11",
          "form": "모스트는 주로 쓰는 캐릭터 말하는거에요!!",
          "original_form": "모스트는 주로 쓰는 캐릭터 말하는거에요!!",
          "speaker_id": "1",
          "time": "20210819 16:10"
        },
        {
          "id": "MDRW2100000402.1.12",
          "form": "저도 럭스요 지금은 다 까먹엇지만 ㅋㅋㅋ",
          "original_form": "저도 럭스요 지금은 다 까먹엇지만 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000402.1.13",
          "form": "포츈쿠키인가? 그것도 몇번했어요",
          "original_form": "포츈쿠키인가? 그것도 몇번했어요",
          "speaker_id": "2",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000402.1.14",
          "form": "그건 뭐에요??",
          "original_form": "그건 뭐에요??",
          "speaker_id": "1",
          "time": "20210819 16:13"
        },
        {
          "id": "MDRW2100000402.1.15",
          "form": "앗 네이버에 검색해보니까 미스포춘이래요 ㅎㅎ",
          "original_form": "앗 네이버에 검색해보니까 미스포춘이래요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 16:17"
        },
        {
          "id": "MDRW2100000402.1.16",
          "form": "아아 뭔지알아요!",
          "original_form": "아아 뭔지알아요!",
          "speaker_id": "1",
          "time": "20210819 16:17"
        },
        {
          "id": "MDRW2100000402.1.17",
          "form": "전 한번도 안해봤어요...!",
          "original_form": "전 한번도 안해봤어요...!",
          "speaker_id": "1",
          "time": "20210819 16:17"
        },
        {
          "id": "MDRW2100000402.1.18",
          "form": "럭스가 제일 하기 편한 것 같아요 속박하고 때리고 ㅋㅋㄱ",
          "original_form": "럭스가 제일 하기 편한 것 같아요 속박하고 때리고 ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000402.1.19",
          "form": "맞아요 ㅋㅋㅋ얼마안하신것 같은데 그래도 많이 하셨나봐요!!",
          "original_form": "맞아요 ㅋㅋㅋ얼마안하신것 같은데 그래도 많이 하셨나봐요!!",
          "speaker_id": "1",
          "time": "20210819 16:23"
        },
        {
          "id": "MDRW2100000402.1.20",
          "form": "ㅋㅋㅋㄱ한 3일 했던 것 같아요",
          "original_form": "ㅋㅋㅋㄱ한 3일 했던 것 같아요",
          "speaker_id": "2",
          "time": "20210819 16:24"
        },
        {
          "id": "MDRW2100000402.1.21",
          "form": "주말에 할 게 없어서 다른 게임 해볼까 생각중인데 추천 좀 해주세요",
          "original_form": "주말에 할 게 없어서 다른 게임 해볼까 생각중인데 추천 좀 해주세요",
          "speaker_id": "2",
          "time": "20210819 16:24"
        },
        {
          "id": "MDRW2100000402.1.22",
          "form": "음..혹시 동물의 숲 해보셨나요??",
          "original_form": "음..혹시 동물의 숲 해보셨나요??",
          "speaker_id": "1",
          "time": "20210819 16:25"
        },
        {
          "id": "MDRW2100000402.1.23",
          "form": "닌텐도 게임이요?",
          "original_form": "닌텐도 게임이요?",
          "speaker_id": "2",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000402.1.24",
          "form": "핸드폰에도 있어요!!",
          "original_form": "핸드폰에도 있어요!!",
          "speaker_id": "1",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000402.1.25",
          "form": "포켓 캠프라고있어요",
          "original_form": "포켓 캠프라고있어요",
          "speaker_id": "1",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000402.1.26",
          "form": "ㅋㅋㅋㅋ동물의 숲이랑 비슷한 게임인가요?",
          "original_form": "ㅋㅋㅋㅋ동물의 숲이랑 비슷한 게임인가요?",
          "speaker_id": "2",
          "time": "20210819 16:34"
        },
        {
          "id": "MDRW2100000402.1.27",
          "form": "동물의 숲은 노잼이던데",
          "original_form": "동물의 숲은 노잼이던데",
          "speaker_id": "2",
          "time": "20210819 16:34"
        },
        {
          "id": "MDRW2100000402.1.28",
          "form": "아예 똑같다고 볼 수 있는데..",
          "original_form": "아예 똑같다고 볼 수 있는데..",
          "speaker_id": "1",
          "time": "20210819 16:35"
        },
        {
          "id": "MDRW2100000402.1.29",
          "form": "그럼 안하시는게 좋을 것 같아요ㅎㅎ",
