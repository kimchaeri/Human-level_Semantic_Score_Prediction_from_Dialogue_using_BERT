      "utterance": [
        {
          "id": "MDRW2100017464.1.1",
          "form": "계란초밥 맛있죠",
          "original_form": "계란초밥 맛있죠",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017464.1.2",
          "form": "폭신달콤~",
          "original_form": "폭신달콤~",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017464.1.3",
          "form": "맞아요 제가 단거를 너무 좋아해서 그것도 맛나더라구요 ㅋㅋㅋ",
          "original_form": "맞아요 제가 단거를 너무 좋아해서 그것도 맛나더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017464.1.4",
          "form": "ㅎㅎ맛있죠",
          "original_form": "ㅎㅎ맛있죠",
          "speaker_id": "2",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100017464.1.5",
          "form": "가리는 음식도 있으세요?",
          "original_form": "가리는 음식도 있으세요?",
          "speaker_id": "1",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100017464.1.6",
          "form": "음 멍게 이런 건 잘 못먹어요",
          "original_form": "음 멍게 이런 건 잘 못먹어요",
          "speaker_id": "2",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100017464.1.7",
          "form": "삭힌 홍어도",
          "original_form": "삭힌 홍어도",
          "speaker_id": "2",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100017464.1.8",
          "form": "아저두요 ㅠㅠ",
          "original_form": "아저두요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017464.1.9",
          "form": "전 닭발 순대 간 이런거도 다 못 먹어요",
          "original_form": "전 닭발 순대 간 이런거도 다 못 먹어요",
          "speaker_id": "1",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017464.1.10",
          "form": "저는 닭발은 종종 먹는데",
          "original_form": "저는 닭발은 종종 먹는데",
          "speaker_id": "2",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017464.1.11",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017464.1.12",
          "form": "아 매운 음식은 요즘 좀 피하는 중입니다",
          "original_form": "아 매운 음식은 요즘 좀 피하는 중입니다",
          "speaker_id": "2",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017464.1.13",
          "form": "원랜 잘 드셨나봐요",
          "original_form": "원랜 잘 드셨나봐요",
          "speaker_id": "1",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017464.1.14",
          "form": "한동안 매운음식 유행이라 자주 먹었는데",
          "original_form": "한동안 매운음식 유행이라 자주 먹었는데",
          "speaker_id": "2",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017464.1.15",
          "form": "위에 안좋은 것 같아서",
          "original_form": "위에 안좋은 것 같아서",
          "speaker_id": "2",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017464.1.16",
          "form": "맞아요 ㅠ",
          "original_form": "맞아요 ㅠ",
          "speaker_id": "1",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017464.1.17",
          "form": "저도 좋아했는데 요샌 좀만 먹어도 속이 쓰려서 피해요",
          "original_form": "저도 좋아했는데 요샌 좀만 먹어도 속이 쓰려서 피해요",
          "speaker_id": "1",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017464.1.18",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017464.1.19",
          "form": "입도 얼얼하니 괴롭고요",
          "original_form": "입도 얼얼하니 괴롭고요",
