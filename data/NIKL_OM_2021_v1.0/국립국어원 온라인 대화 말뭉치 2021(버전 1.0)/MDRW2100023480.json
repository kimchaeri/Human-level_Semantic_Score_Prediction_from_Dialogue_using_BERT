      "utterance": [
        {
          "id": "MDRW2100023480.1.1",
          "form": "동아리활동 하시나요",
          "original_form": "동아리활동 하시나요",
          "speaker_id": "1",
          "time": "20210830 08:43"
        },
        {
          "id": "MDRW2100023480.1.2",
          "form": "안해요",
          "original_form": "안해요",
          "speaker_id": "2",
          "time": "20210830 10:22"
        },
        {
          "id": "MDRW2100023480.1.3",
          "form": "하시나요?",
          "original_form": "하시나요?",
          "speaker_id": "2",
          "time": "20210830 10:22"
        },
        {
          "id": "MDRW2100023480.1.4",
          "form": "해보려고요",
          "original_form": "해보려고요",
          "speaker_id": "1",
          "time": "20210830 10:26"
        },
        {
          "id": "MDRW2100023480.1.5",
          "form": "어떤동아리요?",
          "original_form": "어떤동아리요?",
          "speaker_id": "2",
          "time": "20210830 10:26"
        },
        {
          "id": "MDRW2100023480.1.6",
          "form": "아직 정한 건 없고 뭔가 활동을 하고 싶어서요",
          "original_form": "아직 정한 건 없고 뭔가 활동을 하고 싶어서요",
          "speaker_id": "1",
          "time": "20210830 10:27"
        },
        {
          "id": "MDRW2100023480.1.7",
          "form": "저는 집순이라서 퇴근하자마자 침대 직행입니다",
          "original_form": "저는 집순이라서 퇴근하자마자 침대 직행입니다",
          "speaker_id": "2",
          "time": "20210830 10:29"
        },
        {
          "id": "MDRW2100023480.1.8",
          "form": "부지런하시네욤",
          "original_form": "부지런하시네욤",
          "speaker_id": "2",
          "time": "20210830 10:29"
        },
        {
          "id": "MDRW2100023480.1.9",
          "form": "저도 집순인데 탈출해보려고요ㅋㅋ",
          "original_form": "저도 집순인데 탈출해보려고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 10:30"
        },
        {
          "id": "MDRW2100023480.1.10",
          "form": "전 파워집순이...ㅎㅎ",
          "original_form": "전 파워집순이...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 10:33"
        },
        {
          "id": "MDRW2100023480.1.11",
          "form": "숨쉬기운동하기도 바쁜삶...ㅎ",
          "original_form": "숨쉬기운동하기도 바쁜삶...ㅎ",
          "speaker_id": "2",
          "time": "20210830 10:33"
        },
        {
          "id": "MDRW2100023480.1.12",
          "form": "응원합니당",
          "original_form": "응원합니당",
          "speaker_id": "2",
          "time": "20210830 10:33"
        },
        {
          "id": "MDRW2100023480.1.13",
          "form": "아하 그렇군요 집에만 있으면 무기력해서 동아리 활동하고 싶거든요",
          "original_form": "아하 그렇군요 집에만 있으면 무기력해서 동아리 활동하고 싶거든요",
          "speaker_id": "1",
          "time": "20210830 10:37"
        },
        {
          "id": "MDRW2100023480.1.14",
          "form": "네 감사합니다",
          "original_form": "네 감사합니다",
          "speaker_id": "1",
          "time": "20210830 10:37"
        },
        {
          "id": "MDRW2100023480.1.15",
          "form": "주변친구들 독서모임 볼링모임",
          "original_form": "주변친구들 독서모임 볼링모임",
          "speaker_id": "2",
          "time": "20210830 10:59"
        },
        {
          "id": "MDRW2100023480.1.16",
          "form": "다양하게하더라구욤",
          "original_form": "다양하게하더라구욤",
          "speaker_id": "2",
          "time": "20210830 10:59"
        },
        {
          "id": "MDRW2100023480.1.17",
          "form": "저도 그렇게 뭔가 하고싶은데 의지만 한가득이네요",
          "original_form": "저도 그렇게 뭔가 하고싶은데 의지만 한가득이네요",
          "speaker_id": "1",
          "time": "20210830 10:59"
        },
        {
          "id": "MDRW2100023480.1.18",
          "form": "집순이가 의지가진것만해도 대단한거에요",
          "original_form": "집순이가 의지가진것만해도 대단한거에요",
          "speaker_id": "2",
          "time": "20210830 11:03"
        },
        {
          "id": "MDRW2100023480.1.19",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 11:03"
        },
        {
          "id": "MDRW2100023480.1.20",
          "form": "의지만 가득하고 동아리활동 시작은 못하고있어요",
          "original_form": "의지만 가득하고 동아리활동 시작은 못하고있어요",
