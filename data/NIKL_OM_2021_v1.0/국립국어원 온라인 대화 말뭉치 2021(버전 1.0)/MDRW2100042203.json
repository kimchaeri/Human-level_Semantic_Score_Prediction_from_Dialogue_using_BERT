      "utterance": [
        {
          "id": "MDRW2100042203.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210906 20:35"
        },
        {
          "id": "MDRW2100042203.1.2",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "2",
          "time": "20210906 20:36"
        },
        {
          "id": "MDRW2100042203.1.3",
          "form": "저녁 뭐드셨어요",
          "original_form": "저녁 뭐드셨어요",
          "speaker_id": "2",
          "time": "20210906 20:36"
        },
        {
          "id": "MDRW2100042203.1.4",
          "form": "부대찌개요^^name2님은요?",
          "original_form": "부대찌개요^^&name2&님은요?",
          "speaker_id": "1",
          "time": "20210906 20:36"
        },
        {
          "id": "MDRW2100042203.1.5",
          "form": "저는 김치찌개요",
          "original_form": "저는 김치찌개요",
          "speaker_id": "2",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042203.1.6",
          "form": "찌개 먹었네요 둘다 ㅋㅋ",
          "original_form": "찌개 먹었네요 둘다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042203.1.7",
          "form": "오 김치찌개도 맛있죠",
          "original_form": "오 김치찌개도 맛있죠",
          "speaker_id": "1",
          "time": "20210906 20:56"
        },
        {
          "id": "MDRW2100042203.1.8",
          "form": "고기넣고?",
          "original_form": "고기넣고?",
          "speaker_id": "1",
          "time": "20210906 20:57"
        },
        {
          "id": "MDRW2100042203.1.9",
          "form": "아 간장게장 시킨날 따라온 김치찌개에요",
          "original_form": "아 간장게장 시킨날 따라온 김치찌개에요",
          "speaker_id": "2",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100042203.1.10",
          "form": "와 ㅎㅎ 게장을 시키니 김치찌개가 ㅋ좋네요 1타2피",
          "original_form": "와 ㅎㅎ 게장을 시키니 김치찌개가 ㅋ좋네요 1타2피",
          "speaker_id": "1",
          "time": "20210906 21:14"
        },
        {
          "id": "MDRW2100042203.1.11",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100042203.1.12",
          "form": "맛있었어요",
          "original_form": "맛있었어요",
          "speaker_id": "2",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100042203.1.13",
          "form": "배달은 뭐든 맛있는거 같아요",
          "original_form": "배달은 뭐든 맛있는거 같아요",
          "speaker_id": "1",
          "time": "20210906 21:16"
        },
        {
          "id": "MDRW2100042203.1.14",
          "form": "남이해주는 음식",
          "original_form": "남이해주는 음식",
          "speaker_id": "1",
          "time": "20210906 21:16"
        },
        {
          "id": "MDRW2100042203.1.15",
          "form": "요즘은 좀 질리기도 해요 ㅋㅋㅋㅋ",
          "original_form": "요즘은 좀 질리기도 해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:16"
        },
        {
          "id": "MDRW2100042203.1.16",
          "form": "일회용기도 아깝구ㅠㅠ",
          "original_form": "일회용기도 아깝구ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100042203.1.17",
          "form": "자주 시켜드시나요?",
          "original_form": "자주 시켜드시나요?",
          "speaker_id": "1",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100042203.1.18",
          "form": "네네 너무 자주 시켜서 요즘은 질렸어요",
          "original_form": "네네 너무 자주 시켜서 요즘은 질렸어요",
          "speaker_id": "2",
          "time": "20210906 22:04"
        },
        {
          "id": "MDRW2100042203.1.19",
          "form": "자주시켜먹으면 그런것같긴하네용 저는주1회라",
          "original_form": "자주시켜먹으면 그런것같긴하네용 저는주1회라",
          "speaker_id": "1",
          "time": "20210906 23:24"
        },
        {
          "id": "MDRW2100042203.1.20",
          "form": "2회일때도있고",
          "original_form": "2회일때도있고",
          "speaker_id": "1",
          "time": "20210906 23:24"
        },
        {
          "id": "MDRW2100042203.1.21",
          "form": "저도 그렇습니다ㅠㅠ줄여야하는뎅",
          "original_form": "저도 그렇습니다ㅠㅠ줄여야하는뎅",
          "speaker_id": "2",
          "time": "20210907 07:05"
        },
        {
          "id": "MDRW2100042203.1.22",
          "form": "그정도면 양호한거 아닌가여ㅠㅠ",
          "original_form": "그정도면 양호한거 아닌가여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 08:14"
        },
        {
          "id": "MDRW2100042203.1.23",
          "form": "주말엔 저도 배달로ㅋㅋ",
          "original_form": "주말엔 저도 배달로ㅋㅋ",
          "speaker_id": "1",
