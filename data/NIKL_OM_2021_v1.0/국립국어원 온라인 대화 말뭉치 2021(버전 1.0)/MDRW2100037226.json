      "utterance": [
        {
          "id": "MDRW2100037226.1.1",
          "form": "그리고 제가 통탄스러운게",
          "original_form": "그리고 제가 통탄스러운게",
          "speaker_id": "2",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100037226.1.2",
          "form": "이 나라는",
          "original_form": "이 나라는",
          "speaker_id": "2",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100037226.1.3",
          "form": "남자가 조금만 잘난 구석이 있으면",
          "original_form": "남자가 조금만 잘난 구석이 있으면",
          "speaker_id": "2",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100037226.1.4",
          "form": "너무나 쉽게 여자랑 인연을 만들 수 있어요",
          "original_form": "너무나 쉽게 여자랑 인연을 만들 수 있어요",
          "speaker_id": "2",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100037226.1.5",
          "form": "근데 여자들은 점점 상향평준화 되죠",
          "original_form": "근데 여자들은 점점 상향평준화 되죠",
          "speaker_id": "2",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100037226.1.6",
          "form": "네 잘아시네요",
          "original_form": "네 잘아시네요",
          "speaker_id": "1",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100037226.1.7",
          "form": "그래서 그 버릇이 안고쳐져요",
          "original_form": "그래서 그 버릇이 안고쳐져요",
          "speaker_id": "1",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100037226.1.8",
          "form": "그쵸... 저 사실 비혼주의자거든요",
          "original_form": "그쵸... 저 사실 비혼주의자거든요",
          "speaker_id": "2",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100037226.1.9",
          "form": "네네 저도 그래요",
          "original_form": "네네 저도 그래요",
          "speaker_id": "1",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100037226.1.10",
          "form": "와 말이 통하겠네요!!!",
          "original_form": "와 말이 통하겠네요!!!",
          "speaker_id": "2",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100037226.1.11",
          "form": "ㅋㅋㅋ네네 젤 최근에 황당했던 남자는",
          "original_form": "ㅋㅋㅋ네네 젤 최근에 황당했던 남자는",
          "speaker_id": "1",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100037226.1.12",
          "form": "스벅에서요 돈든다고 바나나 한개 달랑 시켰어요",
          "original_form": "스벅에서요 돈든다고 바나나 한개 달랑 시켰어요",
          "speaker_id": "1",
          "time": "20210831 21:41"
        },
        {
          "id": "MDRW2100037226.1.13",
          "form": "네???? ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "네???? ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:41"
        },
        {
          "id": "MDRW2100037226.1.14",
          "form": "자기 혼자 먹는거죠?",
          "original_form": "자기 혼자 먹는거죠?",
          "speaker_id": "2",
          "time": "20210831 21:41"
        },
        {
          "id": "MDRW2100037226.1.15",
          "form": "그 바나나 저 주긴 했지만 이 나이에 고딩도 안하는 진상짓...",
          "original_form": "그 바나나 저 주긴 했지만 이 나이에 고딩도 안하는 진상짓...",
          "speaker_id": "1",
          "time": "20210831 21:41"
        },
        {
          "id": "MDRW2100037226.1.16",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210831 21:41"
        },
        {
          "id": "MDRW2100037226.1.17",
          "form": "name1님이 만났던 남자가요!",
          "original_form": "&name1&님이 만났던 남자가요!",
          "speaker_id": "2",
          "time": "20210831 21:41"
        },
        {
          "id": "MDRW2100037226.1.18",
          "form": "아니 스벅까지 가서 바나나요?",
          "original_form": "아니 스벅까지 가서 바나나요?",
          "speaker_id": "2",
          "time": "20210831 21:41"
        },
        {
          "id": "MDRW2100037226.1.19",
          "form": "아 진짜 구질구질하다",
          "original_form": "아 진짜 구질구질하다",
          "speaker_id": "2",
          "time": "20210831 21:41"
        },
        {
          "id": "MDRW2100037226.1.20",
          "form": "아~~~~~~~",
          "original_form": "아~~~~~~~",
          "speaker_id": "2",
          "time": "20210831 21:41"
        },
        {
          "id": "MDRW2100037226.1.21",
          "form": "너무 구질구질해서",
          "original_form": "너무 구질구질해서",
          "speaker_id": "2",
          "time": "20210831 21:41"
        },
        {
          "id": "MDRW2100037226.1.22",
          "form": "탄식이 절로 나와요",
          "original_form": "탄식이 절로 나와요",
          "speaker_id": "2",
          "time": "20210831 21:41"
        },
        {
          "id": "MDRW2100037226.1.23",
          "form": "그 바나나로 맞아야돼요",
          "original_form": "그 바나나로 맞아야돼요",
          "speaker_id": "2",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100037226.1.24",
          "form": "호텔서 자고 제가 돈 냈는데",
          "original_form": "호텔서 자고 제가 돈 냈는데",
          "speaker_id": "1",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100037226.1.25",
          "form": "아침은 자기가 산대요 그래놓고",
          "original_form": "아침은 자기가 산대요 그래놓고",
          "speaker_id": "1",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100037226.1.26",
          "form": "아 비쌌을텐데ㅠ",
          "original_form": "아 비쌌을텐데ㅠ",
          "speaker_id": "2",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100037226.1.27",
          "form": "내",
          "original_form": "내",
          "speaker_id": "2",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100037226.1.28",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100037226.1.29",
          "form": "아침 조식은 싸잖아요",
          "original_form": "아침 조식은 싸잖아요",
          "speaker_id": "1",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100037226.1.30",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100037226.1.31",
          "form": "그거 먹고 점심은 굶어야한다고",
          "original_form": "그거 먹고 점심은 굶어야한다고",
          "speaker_id": "1",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100037226.1.32",
          "form": "?????",
          "original_form": "?????",
          "speaker_id": "2",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100037226.1.33",
          "form": "사람 학대죠",
          "original_form": "사람 학대죠",
          "speaker_id": "1",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100037226.1.34",
          "form": "아니 아침도 얻어먹고",
          "original_form": "아니 아침도 얻어먹고",
          "speaker_id": "2",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100037226.1.35",
          "form": "점심은 안사겠대요?",
          "original_form": "점심은 안사겠대요?",
          "speaker_id": "2",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100037226.1.36",
          "form": "아니근데 진짜어이가없는게",
          "original_form": "아니근데 진짜어이가없는게",
          "speaker_id": "2",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100037226.1.37",
          "form": "아뇨 아침은 지가 샀는데",
          "original_form": "아뇨 아침은 지가 샀는데",
          "speaker_id": "1",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100037226.1.38",
          "form": "아 네네",
          "original_form": "아 네네",
          "speaker_id": "2",
          "time": "20210831 21:43"
        },
        {
          "id": "MDRW2100037226.1.39",
          "form": "아 이제 이해했어요!!! 네네",
          "original_form": "아 이제 이해했어요!!! 네네",
          "speaker_id": "2",
          "time": "20210831 21:43"
        },
        {
          "id": "MDRW2100037226.1.40",
          "form": "아니 별.....;;;;",
          "original_form": "아니 별.....;;;;",
          "speaker_id": "2",
          "time": "20210831 21:43"
        },
        {
          "id": "MDRW2100037226.1.41",
          "form": "점심음 굶자고...",
          "original_form": "점심음 굶자고...",
          "speaker_id": "1",
          "time": "20210831 21:43"
        },
