      "utterance": [
        {
          "id": "MDRW2100031686.1.1",
          "form": "안녕!",
          "original_form": "안녕!",
          "speaker_id": "1",
          "time": "20210904 23:33"
        },
        {
          "id": "MDRW2100031686.1.2",
          "form": "술 먹는 거 좋아해??",
          "original_form": "술 먹는 거 좋아해??",
          "speaker_id": "1",
          "time": "20210904 23:33"
        },
        {
          "id": "MDRW2100031686.1.3",
          "form": "응 좋아해",
          "original_form": "응 좋아해",
          "speaker_id": "2",
          "time": "20210905 00:37"
        },
        {
          "id": "MDRW2100031686.1.4",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210905 00:37"
        },
        {
          "id": "MDRW2100031686.1.5",
          "form": "나도 엄청 좋아해ㅋㅋ",
          "original_form": "나도 엄청 좋아해ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:39"
        },
        {
          "id": "MDRW2100031686.1.6",
          "form": "근데 아무래도 술은 몸에 안 좋으니까 점점 안 먹게 되더라",
          "original_form": "근데 아무래도 술은 몸에 안 좋으니까 점점 안 먹게 되더라",
          "speaker_id": "1",
          "time": "20210905 00:39"
        },
        {
          "id": "MDRW2100031686.1.7",
          "form": "몸에안좋아서 줄이다가 하루날잡으면 또 엄청먹게되는듯ㅠ",
          "original_form": "몸에안좋아서 줄이다가 하루날잡으면 또 엄청먹게되는듯ㅠ",
          "speaker_id": "2",
          "time": "20210905 00:40"
        },
        {
          "id": "MDRW2100031686.1.8",
          "form": "그러면 더 안 좋은 거 아냐?!ㅋㅋ",
          "original_form": "그러면 더 안 좋은 거 아냐?!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:41"
        },
        {
          "id": "MDRW2100031686.1.9",
          "form": "애초에 먹으라고 존재하는 게 아닌데 내장에 들이부으니 몸에 안 좋은 게 당연하겠지만",
          "original_form": "애초에 먹으라고 존재하는 게 아닌데 내장에 들이부으니 몸에 안 좋은 게 당연하겠지만",
          "speaker_id": "1",
          "time": "20210905 00:42"
        },
        {
          "id": "MDRW2100031686.1.10",
          "form": "맞아 ,,,, 진짜 안마셔야되는데,,",
          "original_form": "맞아 ,,,, 진짜 안마셔야되는데,,",
          "speaker_id": "2",
          "time": "20210905 00:42"
        },
        {
          "id": "MDRW2100031686.1.11",
          "form": "잘안되네",
          "original_form": "잘안되네",
          "speaker_id": "2",
          "time": "20210905 00:42"
        },
        {
          "id": "MDRW2100031686.1.12",
          "form": "술 줄이기 힘들 때마다 나는 그거 봐",
          "original_form": "술 줄이기 힘들 때마다 나는 그거 봐",
          "speaker_id": "1",
          "time": "20210905 00:43"
        },
        {
          "id": "MDRW2100031686.1.13",
          "form": "방송 나와서 주량 자랑하던 연예인들 지금 다 간암으로 사후에 있다는 글.",
          "original_form": "방송 나와서 주량 자랑하던 연예인들 지금 다 간암으로 사후에 있다는 글.",
          "speaker_id": "1",
          "time": "20210905 00:43"
        },
        {
          "id": "MDRW2100031686.1.14",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210905 00:43"
        },
        {
          "id": "MDRW2100031686.1.15",
          "form": "나도봐야겠다,,",
          "original_form": "나도봐야겠다,,",
          "speaker_id": "2",
          "time": "20210905 00:43"
        },
        {
          "id": "MDRW2100031686.1.16",
          "form": "그럼 넌이제술안마셔?",
          "original_form": "그럼 넌이제술안마셔?",
          "speaker_id": "2",
          "time": "20210905 00:43"
        },
        {
          "id": "MDRW2100031686.1.17",
          "form": "나도 사실 날 잡고 마셔...^^",
          "original_form": "나도 사실 날 잡고 마셔...^^",
          "speaker_id": "1",
          "time": "20210905 00:44"
        },
        {
          "id": "MDRW2100031686.1.18",
          "form": "몸이 또 술을 잘 받아서 경각심도 못 가지는...",
          "original_form": "몸이 또 술을 잘 받아서 경각심도 못 가지는...",
          "speaker_id": "1",
          "time": "20210905 00:44"
        },
        {
          "id": "MDRW2100031686.1.19",
          "form": "ㅋㄱㄱㄱㅋㅋㄱㅋㄱㅋㄱㅋㅋㅋㄱㅋㅋ",
          "original_form": "ㅋㄱㄱㄱㅋㅋㄱㅋㄱㅋㄱㅋㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:44"
        },
        {
          "id": "MDRW2100031686.1.20",
          "form": "아 주량이 쎄?",
          "original_form": "아 주량이 쎄?",
          "speaker_id": "2",
          "time": "20210905 00:44"
        },
        {
          "id": "MDRW2100031686.1.21",
          "form": "말술은 아닌데 숙취가 많이 없어~",
          "original_form": "말술은 아닌데 숙취가 많이 없어~",
          "speaker_id": "1",
          "time": "20210905 00:46"
        },
        {
          "id": "MDRW2100031686.1.22",
          "form": "좋겠다",
          "original_form": "좋겠다",
          "speaker_id": "2",
          "time": "20210905 00:48"
        },
        {
          "id": "MDRW2100031686.1.23",
          "form": "토 안해?",
          "original_form": "토 안해?",
          "speaker_id": "2",
          "time": "20210905 00:48"
        },
        {
          "id": "MDRW2100031686.1.24",
          "form": "몸이 저릴 때는 있는데 토는 웬만하면 안 하더라",
          "original_form": "몸이 저릴 때는 있는데 토는 웬만하면 안 하더라",
          "speaker_id": "1",
          "time": "20210905 00:51"
        },
        {
          "id": "MDRW2100031686.1.25",
          "form": "술 좋아하는 대신 평소 건강 챙기는 거 있어?",
          "original_form": "술 좋아하는 대신 평소 건강 챙기는 거 있어?",
          "speaker_id": "1",
          "time": "20210905 00:51"
        },
        {
          "id": "MDRW2100031686.1.26",
          "form": "안챙겨,,,",
          "original_form": "안챙겨,,,",
          "speaker_id": "2",
          "time": "20210905 00:52"
        },
        {
          "id": "MDRW2100031686.1.27",
          "form": "몸이망가졌어",
          "original_form": "몸이망가졌어",
          "speaker_id": "2",
          "time": "20210905 00:52"
        },
        {
          "id": "MDRW2100031686.1.28",
          "form": "근데 저번달부터 공원 뛰기 시작했어",
          "original_form": "근데 저번달부터 공원 뛰기 시작했어",
          "speaker_id": "2",
          "time": "20210905 00:52"
        },
        {
          "id": "MDRW2100031686.1.29",
          "form": "이봐욧",
          "original_form": "이봐욧",
          "speaker_id": "1",
          "time": "20210905 00:52"
        },
        {
          "id": "MDRW2100031686.1.30",
          "form": "런닝 좋지 런닝 짱",
          "original_form": "런닝 좋지 런닝 짱",
          "speaker_id": "1",
          "time": "20210905 00:53"
        },
        {
          "id": "MDRW2100031686.1.31",
          "form": "요즘 운동하기 너무 좋은 날씨잖아",
          "original_form": "요즘 운동하기 너무 좋은 날씨잖아",
          "speaker_id": "1",
          "time": "20210905 00:53"
        },
        {
          "id": "MDRW2100031686.1.32",
          "form": "아 근데 일주일했어",
          "original_form": "아 근데 일주일했어",
          "speaker_id": "2",
          "time": "20210905 00:53"
        },
        {
          "id": "MDRW2100031686.1.33",
          "form": "하고 바로 일주일내내 비오더라고,,,",
          "original_form": "하고 바로 일주일내내 비오더라고,,,",
          "speaker_id": "2",
          "time": "20210905 00:53"
        },
        {
          "id": "MDRW2100031686.1.34",
          "form": "어쩔수없이 못나갔지~~~",
          "original_form": "어쩔수없이 못나갔지~~~",
          "speaker_id": "2",
          "time": "20210905 00:53"
        },
        {
          "id": "MDRW2100031686.1.35",
          "form": "저 신나 보이는 물결들은 뭐지",
          "original_form": "저 신나 보이는 물결들은 뭐지",
