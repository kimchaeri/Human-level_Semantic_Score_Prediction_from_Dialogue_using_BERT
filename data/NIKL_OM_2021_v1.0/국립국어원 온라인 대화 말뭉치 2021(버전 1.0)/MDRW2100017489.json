      "utterance": [
        {
          "id": "MDRW2100017489.1.1",
          "form": "저녁드셧나욤",
          "original_form": "저녁드셧나욤",
          "speaker_id": "2",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017489.1.2",
          "form": "이제먹어야하는데 누워있어요",
          "original_form": "이제먹어야하는데 누워있어요",
          "speaker_id": "1",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017489.1.3",
          "form": "가족들 식사중인데 ㅋㅋ",
          "original_form": "가족들 식사중인데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017489.1.4",
          "form": "저는 진작먹고",
          "original_form": "저는 진작먹고",
          "speaker_id": "2",
          "time": "20210826 19:41"
        },
        {
          "id": "MDRW2100017489.1.5",
          "form": "침대누웟어요",
          "original_form": "침대누웟어요",
          "speaker_id": "2",
          "time": "20210826 19:41"
        },
        {
          "id": "MDRW2100017489.1.6",
          "form": "우와정말요",
          "original_form": "우와정말요",
          "speaker_id": "1",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100017489.1.7",
          "form": "저도이제먹고앉았네요",
          "original_form": "저도이제먹고앉았네요",
          "speaker_id": "1",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100017489.1.8",
          "form": "메뉴는 뭔가요?",
          "original_form": "메뉴는 뭔가요?",
          "speaker_id": "2",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100017489.1.9",
          "form": "낙지볶음입니다",
          "original_form": "낙지볶음입니다",
          "speaker_id": "1",
          "time": "20210826 20:05"
        },
        {
          "id": "MDRW2100017489.1.10",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210826 20:06"
        },
        {
          "id": "MDRW2100017489.1.11",
          "form": "저는 잔치국수 배민시켜서 먹엇어요",
          "original_form": "저는 잔치국수 배민시켜서 먹엇어요",
          "speaker_id": "2",
          "time": "20210826 20:07"
        },
        {
          "id": "MDRW2100017489.1.12",
          "form": "탄수화물 중독자에요ㅜ",
          "original_form": "탄수화물 중독자에요ㅜ",
          "speaker_id": "2",
          "time": "20210826 20:07"
        },
        {
          "id": "MDRW2100017489.1.13",
          "form": "오 잔치국수",
          "original_form": "오 잔치국수",
          "speaker_id": "1",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100017489.1.14",
          "form": "국수 맛나겠다",
          "original_form": "국수 맛나겠다",
          "speaker_id": "1",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100017489.1.15",
          "form": "좋아하는 음식 뭐에요 ? ㅎㅎ",
          "original_form": "좋아하는 음식 뭐에요 ? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100017489.1.16",
          "form": "저는 마라샹궈 좋아해요 name2님은요?",
          "original_form": "저는 마라샹궈 좋아해요 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017489.1.17",
          "form": "마라탕이욥!!",
          "original_form": "마라탕이욥!!",
          "speaker_id": "2",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017489.1.18",
          "form": "우아우아",
          "original_form": "우아우아",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017489.1.19",
          "form": "사실 다 좋아해요 가리는거없어요",
          "original_form": "사실 다 좋아해요 가리는거없어요",
          "speaker_id": "2",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017489.1.20",
          "form": "저 부산인데 마라탕 맛집 서면에있어요",
          "original_form": "저 부산인데 마라탕 맛집 서면에있어요",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017489.1.21",
          "form": "라라관이라구",
          "original_form": "라라관이라구",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017489.1.22",
          "form": "저는 생미역 다시마 멍게 못먹어요ㅠㅠ",
          "original_form": "저는 생미역 다시마 멍게 못먹어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017489.1.23",
          "form": "가봣죠....ㅎ",
          "original_form": "가봣죠....ㅎ",
          "speaker_id": "2",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017489.1.24",
          "form": "우와 대박",
