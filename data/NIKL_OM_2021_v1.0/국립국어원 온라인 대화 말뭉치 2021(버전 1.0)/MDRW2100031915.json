      "utterance": [
        {
          "id": "MDRW2100031915.1.1",
          "form": "아침드셨어요?",
          "original_form": "아침드셨어요?",
          "speaker_id": "2",
          "time": "20210905 08:36"
        },
        {
          "id": "MDRW2100031915.1.2",
          "form": "아침을 챙겨 드셔야지 건강할 수 있습니다",
          "original_form": "아침을 챙겨 드셔야지 건강할 수 있습니다",
          "speaker_id": "2",
          "time": "20210905 08:36"
        },
        {
          "id": "MDRW2100031915.1.3",
          "form": "조금 이따 먹으려고요",
          "original_form": "조금 이따 먹으려고요",
          "speaker_id": "1",
          "time": "20210905 08:36"
        },
        {
          "id": "MDRW2100031915.1.4",
          "form": "미용실 자주가세요?",
          "original_form": "미용실 자주가세요?",
          "speaker_id": "2",
          "time": "20210905 08:50"
        },
        {
          "id": "MDRW2100031915.1.5",
          "form": "아니요 미용실 가는 거 그렇게 안 좋아해요",
          "original_form": "아니요 미용실 가는 거 그렇게 안 좋아해요",
          "speaker_id": "1",
          "time": "20210905 08:51"
        },
        {
          "id": "MDRW2100031915.1.6",
          "form": "오 왜요?",
          "original_form": "오 왜요?",
          "speaker_id": "2",
          "time": "20210905 08:51"
        },
        {
          "id": "MDRW2100031915.1.7",
          "form": "케어받으면 좋잔아요",
          "original_form": "케어받으면 좋잔아요",
          "speaker_id": "2",
          "time": "20210905 08:51"
        },
        {
          "id": "MDRW2100031915.1.8",
          "form": "머리 기시져?",
          "original_form": "머리 기시져?",
          "speaker_id": "2",
          "time": "20210905 08:51"
        },
        {
          "id": "MDRW2100031915.1.9",
          "form": "가서 꾸벅꾸벅 졸아요",
          "original_form": "가서 꾸벅꾸벅 졸아요",
          "speaker_id": "1",
          "time": "20210905 08:51"
        },
        {
          "id": "MDRW2100031915.1.10",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210905 08:51"
        },
        {
          "id": "MDRW2100031915.1.11",
          "form": "짧으세요,",
          "original_form": "짧으세요,",
          "speaker_id": "2",
          "time": "20210905 08:51"
        },
        {
          "id": "MDRW2100031915.1.12",
          "form": "숏컷?",
          "original_form": "숏컷?",
          "speaker_id": "2",
          "time": "20210905 08:52"
        },
        {
          "id": "MDRW2100031915.1.13",
          "form": "그정돈 아니구요",
          "original_form": "그정돈 아니구요",
          "speaker_id": "1",
          "time": "20210905 08:52"
        },
        {
          "id": "MDRW2100031915.1.14",
          "form": "단발존 버티김 힘들지 않아요?",
          "original_form": "단발존 버티김 힘들지 않아요?",
          "speaker_id": "2",
          "time": "20210905 08:56"
        },
        {
          "id": "MDRW2100031915.1.15",
          "form": "무슨 뜻인지 잘 모르겠어요",
          "original_form": "무슨 뜻인지 잘 모르겠어요",
          "speaker_id": "1",
          "time": "20210905 08:56"
        },
        {
          "id": "MDRW2100031915.1.16",
          "form": "단발에서",
          "original_form": "단발에서",
          "speaker_id": "2",
          "time": "20210905 08:57"
        },
        {
          "id": "MDRW2100031915.1.17",
          "form": "어깨라인",
          "original_form": "어깨라인",
          "speaker_id": "2",
          "time": "20210905 08:57"
        },
        {
          "id": "MDRW2100031915.1.18",
          "form": "그 구간에서 자꾸 자르게된다더라구요",
          "original_form": "그 구간에서 자꾸 자르게된다더라구요",
          "speaker_id": "2",
          "time": "20210905 08:57"
        },
        {
          "id": "MDRW2100031915.1.19",
          "form": "여자들은",
          "original_form": "여자들은",
          "speaker_id": "2",
          "time": "20210905 08:57"
        },
        {
          "id": "MDRW2100031915.1.20",
          "form": "딱히 그런 거 같진 않아요",
          "original_form": "딱히 그런 거 같진 않아요",
          "speaker_id": "1",
          "time": "20210905 08:57"
        },
        {
          "id": "MDRW2100031915.1.21",
          "form": "전 여자가 아닌가봐요 ㅋㅋㅋ",
          "original_form": "전 여자가 아닌가봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:57"
        },
        {
          "id": "MDRW2100031915.1.22",
          "form": "들어맞는게 별로없어요 ㅋㅋㅋ",
          "original_form": "들어맞는게 별로없어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:57"
        },
        {
          "id": "MDRW2100031915.1.23",
          "form": "지가 그런걸 여자가 그런거라 일반화하는 여자들 진짜많아요 ㅋㅋㅋ",
          "original_form": "지가 그런걸 여자가 그런거라 일반화하는 여자들 진짜많아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:58"
        },
        {
          "id": "MDRW2100031915.1.24",
          "form": "아 단발존아이고",
          "original_form": "아 단발존아이고",
          "speaker_id": "2",
          "time": "20210905 09:00"
        },
        {
          "id": "MDRW2100031915.1.25",
          "form": "거지존 ㅋㅋ",
          "original_form": "거지존 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 09:00"
        },
        {
          "id": "MDRW2100031915.1.26",
          "form": "거지존은 들어보셔서요?",
          "original_form": "거지존은 들어보셔서요?",
          "speaker_id": "2",
          "time": "20210905 09:00"
        },
        {
          "id": "MDRW2100031915.1.27",
          "form": "ㅋㅋㅋ아뇨 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ아뇨 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 09:01"
        },
        {
          "id": "MDRW2100031915.1.28",
          "form": "그건뭐죠 ㅋㅋㅋ",
          "original_form": "그건뭐죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 09:01"
        },
        {
          "id": "MDRW2100031915.1.29",
          "form": "아 ㅋ제가 위에 설명드린거",
          "original_form": "아 ㅋ제가 위에 설명드린거",
          "speaker_id": "2",
          "time": "20210905 09:02"
        },
        {
          "id": "MDRW2100031915.1.30",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 09:02"
        },
        {
          "id": "MDRW2100031915.1.31",
          "form": "네일하시나요",
          "original_form": "네일하시나요",
          "speaker_id": "2",
