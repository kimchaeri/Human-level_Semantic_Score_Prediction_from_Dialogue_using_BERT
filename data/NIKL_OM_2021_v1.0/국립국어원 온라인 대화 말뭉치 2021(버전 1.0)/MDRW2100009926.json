      "utterance": [
        {
          "id": "MDRW2100009926.1.1",
          "form": "2학기 시작하셨나요",
          "original_form": "2학기 시작하셨나요",
          "speaker_id": "1",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100009926.1.2",
          "form": "저는 학생이아니예요",
          "original_form": "저는 학생이아니예요",
          "speaker_id": "2",
          "time": "20210901 17:38"
        },
        {
          "id": "MDRW2100009926.1.3",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "2",
          "time": "20210901 17:38"
        },
        {
          "id": "MDRW2100009926.1.4",
          "form": "아니용",
          "original_form": "아니용",
          "speaker_id": "1",
          "time": "20210901 17:39"
        },
        {
          "id": "MDRW2100009926.1.5",
          "form": "학교다니실때 동아리활동 하셨나요?",
          "original_form": "학교다니실때 동아리활동 하셨나요?",
          "speaker_id": "2",
          "time": "20210901 17:41"
        },
        {
          "id": "MDRW2100009926.1.6",
          "form": "네 전 일본어관련 동아리했어요",
          "original_form": "네 전 일본어관련 동아리했어요",
          "speaker_id": "1",
          "time": "20210901 17:51"
        },
        {
          "id": "MDRW2100009926.1.7",
          "form": "어떠셨나요?일본어 배우는데 많은 도움이 되었나요?",
          "original_form": "어떠셨나요?일본어 배우는데 많은 도움이 되었나요?",
          "speaker_id": "2",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100009926.1.8",
          "form": "사실 사람 사귀려고 간 곳이라 제가 열심히 안해서 큰 도움은 안되었던거같아요ㅋㅋ",
          "original_form": "사실 사람 사귀려고 간 곳이라 제가 열심히 안해서 큰 도움은 안되었던거같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100009926.1.9",
          "form": "동아리 뭐 하셨어요?",
          "original_form": "동아리 뭐 하셨어요?",
          "speaker_id": "1",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100009926.1.10",
          "form": "저는 배드민턴 잠깐했다가",
          "original_form": "저는 배드민턴 잠깐했다가",
          "speaker_id": "2",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100009926.1.11",
          "form": "나왔어요",
          "original_form": "나왔어요",
          "speaker_id": "2",
          "time": "20210901 18:44"
        },
        {
          "id": "MDRW2100009926.1.12",
          "form": "아 운동 좋아하시나봐요!",
          "original_form": "아 운동 좋아하시나봐요!",
          "speaker_id": "1",
          "time": "20210901 18:54"
        },
        {
          "id": "MDRW2100009926.1.13",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210901 19:55"
        },
        {
          "id": "MDRW2100009926.1.14",
          "form": "요즘도 꾸준히 운동하세요?",
          "original_form": "요즘도 꾸준히 운동하세요?",
          "speaker_id": "1",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100009926.1.15",
          "form": "주에 3번은 하려고해요",
          "original_form": "주에 3번은 하려고해요",
