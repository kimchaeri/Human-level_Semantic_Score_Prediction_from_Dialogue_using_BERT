      "utterance": [
        {
          "id": "MDRW2100045360.1.1",
          "form": "키우고 싶은 반려동물 있어요",
          "original_form": "키우고 싶은 반려동물 있어요",
          "speaker_id": "2",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045360.1.2",
          "form": "역시 고양이죠",
          "original_form": "역시 고양이죠",
          "speaker_id": "1",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045360.1.3",
          "form": "저는 셋째 고양이 키우고 싶어요",
          "original_form": "저는 셋째 고양이 키우고 싶어요",
          "speaker_id": "2",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045360.1.4",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045360.1.5",
          "form": "근데 애들이 경계해요",
          "original_form": "근데 애들이 경계해요",
          "speaker_id": "2",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045360.1.6",
          "form": "애기 고양이를 요",
          "original_form": "애기 고양이를 요",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045360.1.7",
          "form": "본 적 없어서 그런가 봐요",
          "original_form": "본 적 없어서 그런가 봐요",
          "speaker_id": "2",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045360.1.8",
          "form": "합사은 쉽지 않죠",
          "original_form": "합사은 쉽지 않죠",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045360.1.9",
          "form": "방송에서처럼 그렇네요",
          "original_form": "방송에서처럼 그렇네요",
          "speaker_id": "2",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045360.1.10",
          "form": "아니면 임보라도",
          "original_form": "아니면 임보라도",
          "speaker_id": "1",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045360.1.11",
          "form": "시간만 있으면 하고 싶네요",
          "original_form": "시간만 있으면 하고 싶네요",
          "speaker_id": "2",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045360.1.12",
          "form": "분유를 자주 줘야하더라구요",
          "original_form": "분유를 자주 줘야하더라구요",
          "speaker_id": "1",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045360.1.13",
          "form": "그러게 말이에요",
          "original_form": "그러게 말이에요",
          "speaker_id": "2",
          "time": "20210908 02:05"
        }
