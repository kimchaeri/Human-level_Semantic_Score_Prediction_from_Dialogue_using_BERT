      "utterance": [
        {
          "id": "MDRW2100006876.1.1",
          "form": "도넛도 좋아해요",
          "original_form": "도넛도 좋아해요",
          "speaker_id": "2",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100006876.1.2",
          "form": "크리스피크림이요",
          "original_form": "크리스피크림이요",
          "speaker_id": "1",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100006876.1.3",
          "form": "네 사르르 녹아요 폭신하게",
          "original_form": "네 사르르 녹아요 폭신하게",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100006876.1.4",
          "form": "네 제주도엔 없어요",
          "original_form": "네 제주도엔 없어요",
          "speaker_id": "1",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100006876.1.5",
          "form": "부산도 많이 있진않아요. 아쉽겠어요",
          "original_form": "부산도 많이 있진않아요. 아쉽겠어요",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100006876.1.6",
          "form": "부산가면 먹으러가요",
          "original_form": "부산가면 먹으러가요",
          "speaker_id": "1",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100006876.1.7",
          "form": "광복동 롯데백화점이랑 서면 롯데백화점 지하에 아마 있을거에요",
          "original_form": "광복동 롯데백화점이랑 서면 롯데백화점 지하에 아마 있을거에요",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100006876.1.8",
          "form": "진짜 오래되었죠",
          "original_form": "진짜 오래되었죠",
          "speaker_id": "1",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100006876.1.9",
          "form": "첨에 줄만 서면 나눠줄때 즐겁게 줄 선 기억이 있네요",
          "original_form": "첨에 줄만 서면 나눠줄때 즐겁게 줄 선 기억이 있네요",
          "speaker_id": "2",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100006876.1.10",
          "form": "그 서비스는 환상적이었죠",
          "original_form": "그 서비스는 환상적이었죠",
          "speaker_id": "1",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100006876.1.11",
          "form": "던킨도넛도 요즘 다양해요",
          "original_form": "던킨도넛도 요즘 다양해요",
          "speaker_id": "2",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100006876.1.12",
          "form": "아쉬운대로먹었는데 맛있어요",
          "original_form": "아쉬운대로먹었는데 맛있어요",
          "speaker_id": "1",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100006876.1.13",
          "form": "먼치킨이 한입쏙 들어가서 먹기 편해요",
          "original_form": "먼치킨이 한입쏙 들어가서 먹기 편해요",
          "speaker_id": "2",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100006876.1.14",
          "form": "튀김도 좋아하나요",
          "original_form": "튀김도 좋아하나요",
