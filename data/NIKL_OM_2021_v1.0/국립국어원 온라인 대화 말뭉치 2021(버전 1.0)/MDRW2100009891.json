      "utterance": [
        {
          "id": "MDRW2100009891.1.1",
          "form": "하시는 게임 있으세요,",
          "original_form": "하시는 게임 있으세요,",
          "speaker_id": "2",
          "time": "20210823 11:48"
        },
        {
          "id": "MDRW2100009891.1.2",
          "form": "아니요... 저 20대 초반때가\r\n마지막이었던거 같아요ㅋㅋ",
          "original_form": "아니요... 저 20대 초반때가\r\n마지막이었던거 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 11:52"
        },
        {
          "id": "MDRW2100009891.1.3",
          "form": "애니팡은 음 아시나요",
          "original_form": "애니팡은 음 아시나요",
          "speaker_id": "2",
          "time": "20210823 11:54"
        },
        {
          "id": "MDRW2100009891.1.4",
          "form": "오! 애니팡 알죠ㅋㅋ",
          "original_form": "오! 애니팡 알죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 11:54"
        },
        {
          "id": "MDRW2100009891.1.5",
          "form": "고수셨나요?",
          "original_form": "고수셨나요?",
          "speaker_id": "1",
          "time": "20210823 11:54"
        },
        {
          "id": "MDRW2100009891.1.6",
          "form": "한때 애니팡 하트로\r\n카톡 알림이 엄청 울리던때가 있었져ㅋㅋ",
          "original_form": "한때 애니팡 하트로\r\n카톡 알림이 엄청 울리던때가 있었져ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 11:55"
        },
        {
          "id": "MDRW2100009891.1.7",
          "form": "전 약간 그런 단순한 고전게임이 취향같아요",
          "original_form": "전 약간 그런 단순한 고전게임이 취향같아요",
          "speaker_id": "2",
          "time": "20210823 11:55"
        },
        {
          "id": "MDRW2100009891.1.8",
          "form": "요즘은 캐시퍼하고있어요",
          "original_form": "요즘은 캐시퍼하고있어요",
          "speaker_id": "2",
          "time": "20210823 11:55"
        },
        {
          "id": "MDRW2100009891.1.9",
          "form": "캐시퍼 검색하고 왔는데",
          "original_form": "캐시퍼 검색하고 왔는데",
          "speaker_id": "1",
          "time": "20210823 11:56"
        },
        {
          "id": "MDRW2100009891.1.10",
          "form": "돈버는 게임앱? 이거 맞나요?",
          "original_form": "돈버는 게임앱? 이거 맞나요?",
          "speaker_id": "1",
          "time": "20210823 11:56"
        },
        {
          "id": "MDRW2100009891.1.11",
          "form": "네 그거맞아요",
          "original_form": "네 그거맞아요",
          "speaker_id": "2",
          "time": "20210823 12:03"
        },
        {
          "id": "MDRW2100009891.1.12",
          "form": "재미있나요? :)",
          "original_form": "재미있나요? :)",
          "speaker_id": "1",
          "time": "20210823 12:05"
        },
        {
          "id": "MDRW2100009891.1.13",
          "form": "그냥 애니팡같은 중독성이있어요",
          "original_form": "그냥 애니팡같은 중독성이있어요",
          "speaker_id": "2",
          "time": "20210823 12:19"
        },
        {
          "id": "MDRW2100009891.1.14",
          "form": "또 포인트 1포인트 2포인트 모으면 현금으로 환급해준다고도하고",
          "original_form": "또 포인트 1포인트 2포인트 모으면 현금으로 환급해준다고도하고",
          "speaker_id": "2",
          "time": "20210823 12:19"
        },
        {
          "id": "MDRW2100009891.1.15",
          "form": "애니팡같은 중독성이 진짜 무서운거라\r\n저는 시작을 못하겠네요ㅋ",
          "original_form": "애니팡같은 중독성이 진짜 무서운거라\r\n저는 시작을 못하겠네요ㅋ",
          "speaker_id": "1",
          "time": "20210823 12:20"
        },
        {
          "id": "MDRW2100009891.1.16",
          "form": "하루에 몇 포 정도씩 모여요?",
          "original_form": "하루에 몇 포 정도씩 모여요?",
          "speaker_id": "1",
          "time": "20210823 12:20"
        },
        {
          "id": "MDRW2100009891.1.17",
          "form": "얼마나 하느냐에따라 다를거같아요",
          "original_form": "얼마나 하느냐에따라 다를거같아요",
          "speaker_id": "2",
          "time": "20210823 12:30"
        },
        {
          "id": "MDRW2100009891.1.18",
          "form": "겜도 하면서 포인트까지 모으고\r\n현금화까지 가능한건 꿀이네요ㅋ",
          "original_form": "겜도 하면서 포인트까지 모으고\r\n현금화까지 가능한건 꿀이네요ㅋ",
          "speaker_id": "1",
          "time": "20210823 12:38"
        },
        {
          "id": "MDRW2100009891.1.19",
          "form": "그죠 ㅋㅋ 어제 드라마보면서 아무생각없이 땅파고 있으니",
          "original_form": "그죠 ㅋㅋ 어제 드라마보면서 아무생각없이 땅파고 있으니",
          "speaker_id": "2",
          "time": "20210823 12:39"
        },
        {
          "id": "MDRW2100009891.1.20",
          "form": "세시간 순삭 ㅋㅋ",
          "original_form": "세시간 순삭 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 12:39"
        },
        {
          "id": "MDRW2100009891.1.21",
          "form": "오토 기능도 있는건가요? ㅋㅋ",
          "original_form": "오토 기능도 있는건가요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 12:40"
        },
        {
          "id": "MDRW2100009891.1.22",
          "form": "네네 방치모드있어요 세시간까지",
          "original_form": "네네 방치모드있어요 세시간까지",
          "speaker_id": "2",
          "time": "20210823 12:42"
        },
        {
          "id": "MDRW2100009891.1.23",
          "form": "근데 직접파거나",
          "original_form": "근데 직접파거나",
          "speaker_id": "2",
          "time": "20210823 12:43"
        },
        {
          "id": "MDRW2100009891.1.24",
          "form": "5분마다 방치모드푸는게 이득이래여",
          "original_form": "5분마다 방치모드푸는게 이득이래여",
          "speaker_id": "2",
          "time": "20210823 12:43"
        },
        {
          "id": "MDRW2100009891.1.25",
          "form": "꿀팁까지 있는 게임이네요:)",
          "original_form": "꿀팁까지 있는 게임이네요:)",
          "speaker_id": "1",
          "time": "20210823 13:46"
        },
        {
          "id": "MDRW2100009891.1.26",
          "form": "메모해 두었다가\r\n담에 설치해봐야겠어요",
          "original_form": "메모해 두었다가\r\n담에 설치해봐야겠어요",
