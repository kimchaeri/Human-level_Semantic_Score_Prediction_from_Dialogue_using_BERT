      "utterance": [
        {
          "id": "MDRW2100040186.1.1",
          "form": "앗 주제를",
          "original_form": "앗 주제를",
          "speaker_id": "1",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100040186.1.2",
          "form": "뭘로 해야할까요 ㅋㅋㅋㅋㅋ",
          "original_form": "뭘로 해야할까요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100040186.1.3",
          "form": "이것도 게임인가...",
          "original_form": "이것도 게임인가...",
          "speaker_id": "1",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100040186.1.4",
          "form": "관련주가 메타버스예요",
          "original_form": "관련주가 메타버스예요",
          "speaker_id": "1",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100040186.1.5",
          "form": "아 저는 아무거나 괜찮아요",
          "original_form": "아 저는 아무거나 괜찮아요",
          "speaker_id": "2",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100040186.1.6",
          "form": "예를 들면 가온미디어",
          "original_form": "예를 들면 가온미디어",
          "speaker_id": "1",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100040186.1.7",
          "form": "이것도 게임 관련인가...",
          "original_form": "이것도 게임 관련인가...",
          "speaker_id": "1",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100040186.1.8",
          "form": "아 메타버스ㅋㅋ따라잡기 힘드네요",
          "original_form": "아 메타버스ㅋㅋ따라잡기 힘드네요",
          "speaker_id": "2",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100040186.1.9",
          "form": "인공지능 플랫폼 사업하는 곳이래요",
          "original_form": "인공지능 플랫폼 사업하는 곳이래요",
          "speaker_id": "1",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100040186.1.10",
          "form": "그러니까요 저도 사실 아직 잘...",
          "original_form": "그러니까요 저도 사실 아직 잘...",
          "speaker_id": "1",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100040186.1.11",
          "form": "알체라 라는 곳도",
          "original_form": "알체라 라는 곳도",
          "speaker_id": "1",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100040186.1.12",
          "form": "제페토에서 구찌랑 샤넬판다길래 뭔소린가싶어서요ㅋㅋ",
          "original_form": "제페토에서 구찌랑 샤넬판다길래 뭔소린가싶어서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100040186.1.13",
          "form": "인공지능",
          "original_form": "인공지능",
          "speaker_id": "1",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100040186.1.14",
          "form": "아 지짜요?",
          "original_form": "아 지짜요?",
          "speaker_id": "1",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100040186.1.15",
          "form": "구찌랑 샤넬을 제페토를 통해서 판다구요?!",
          "original_form": "구찌랑 샤넬을 제페토를 통해서 판다구요?!",
          "speaker_id": "1",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100040186.1.16",
          "form": "넵 게임캐릭터 아이템같은건데",
          "original_form": "넵 게임캐릭터 아이템같은건데",
          "speaker_id": "2",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100040186.1.17",
          "form": "에스파 같은 개념인가?",
          "original_form": "에스파 같은 개념인가?",
          "speaker_id": "1",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100040186.1.18",
          "form": "아~~~",
          "original_form": "아~~~",
          "speaker_id": "1",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100040186.1.19",
          "form": "현질로 산다네요",
          "original_form": "현질로 산다네요",
          "speaker_id": "2",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100040186.1.20",
          "form": "구찌 샤넬이랑 협약?같은거 맺어서 한대요",
          "original_form": "구찌 샤넬이랑 협약?같은거 맺어서 한대요",
          "speaker_id": "2",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100040186.1.21",
          "form": "와...?",
          "original_form": "와...?",
          "speaker_id": "1",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100040186.1.22",
          "form": "아니 세상에",
          "original_form": "아니 세상에",
          "speaker_id": "1",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100040186.1.23",
          "form": "저 처음 들었어요",
          "original_form": "저 처음 들었어요",
          "speaker_id": "1",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100040186.1.24",
          "form": "증강 현실에서 무슨 일이 벌어지고 있는거죠 ㅋㅋㅋㅋ",
          "original_form": "증강 현실에서 무슨 일이 벌어지고 있는거죠 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100040186.1.25",
          "form": "그래서 잘나가는게임인가 싶어서 ㅋㅋ 아시나 물어봤어요",
          "original_form": "그래서 잘나가는게임인가 싶어서 ㅋㅋ 아시나 물어봤어요",
          "speaker_id": "2",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100040186.1.26",
          "form": "혹시 블랙미러 보셨어요?",
          "original_form": "혹시 블랙미러 보셨어요?",
          "speaker_id": "1",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100040186.1.27",
          "form": "넷플릭스에서 하는 과학공상 드라마인데",
          "original_form": "넷플릭스에서 하는 과학공상 드라마인데",
          "speaker_id": "1",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100040186.1.28",
          "form": "그리고 거기 겜상에 씨유편의점 세워서",
          "original_form": "그리고 거기 겜상에 씨유편의점 세워서",
          "speaker_id": "2",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100040186.1.29",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100040186.1.30",
          "form": "게임에서 그런 일이 벌어진다구요?",
          "original_form": "게임에서 그런 일이 벌어진다구요?",
          "speaker_id": "1",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100040186.1.31",
          "form": "세상에 !!!",
          "original_form": "세상에 !!!",
          "speaker_id": "1",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100040186.1.32",
          "form": "거기서 뭐사면 씨유 상품권이벤트인가?한대요",
          "original_form": "거기서 뭐사면 씨유 상품권이벤트인가?한대요",
          "speaker_id": "2",
          "time": "20210904 15:07"
        }
      ]
