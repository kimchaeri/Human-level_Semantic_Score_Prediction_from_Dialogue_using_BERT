      "utterance": [
        {
          "id": "MDRW2100017785.1.1",
          "form": "저는 요즘 화장품쇼핑을 즐겨합니다",
          "original_form": "저는 요즘 화장품쇼핑을 즐겨합니다",
          "speaker_id": "1",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100017785.1.2",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100017785.1.3",
          "form": "저는 약간 왕창 사서 왕창버리는 느낌이예욬ㅋㅋㅋㅋ",
          "original_form": "저는 약간 왕창 사서 왕창버리는 느낌이예욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:07"
        },
        {
          "id": "MDRW2100017785.1.4",
          "form": "요즘꽂혀있는거는 모공화장품이라 왕창 결재하고있습니다ㅎ",
          "original_form": "요즘꽂혀있는거는 모공화장품이라 왕창 결재하고있습니다ㅎ",
          "speaker_id": "1",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017785.1.5",
          "form": "좋은 제품있나요?",
          "original_form": "좋은 제품있나요?",
          "speaker_id": "2",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017785.1.6",
          "form": "넘버즈인 3번세럼 추천합니다",
          "original_form": "넘버즈인 3번세럼 추천합니다",
          "speaker_id": "1",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100017785.1.7",
          "form": "세럼!",
          "original_form": "세럼!",
          "speaker_id": "2",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017785.1.8",
          "form": "촉촉해요",
          "original_form": "촉촉해요",
          "speaker_id": "1",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100017785.1.9",
          "form": "오옹...",
          "original_form": "오옹...",
          "speaker_id": "2",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100017785.1.10",
          "form": "다른거 보다",
          "original_form": "다른거 보다",
          "speaker_id": "2",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100017785.1.11",
          "form": "모공 줄이는게 ... 필요해요 ㅠ",
          "original_form": "모공 줄이는게 ... 필요해요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017785.1.12",
          "form": "그치?",
          "original_form": "그치?",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017785.1.13",
          "form": "그래서 모공제품",
          "original_form": "그래서 모공제품",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017785.1.14",
          "form": "다섭렵중인데",
          "original_form": "다섭렵중인데",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017785.1.15",
          "form": "하나좋은거있으면 추천또하겠습니당",
          "original_form": "하나좋은거있으면 추천또하겠습니당",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017785.1.16",
          "form": "감사합미다",
          "original_form": "감사합미다",
          "speaker_id": "2",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017785.1.17",
          "form": "뭘요ㅎㅎ",
          "original_form": "뭘요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017785.1.18",
          "form": "수영장애플?",
          "original_form": "수영장애플?",
          "speaker_id": "1",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017785.1.19",
          "form": "그것도 다음달온다는데 써보고 추천해볼께요",
          "original_form": "그것도 다음달온다는데 써보고 추천해볼께요",
          "speaker_id": "1",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017785.1.20",
          "form": "알겠슴미다",
          "original_form": "알겠슴미다",
          "speaker_id": "2",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017785.1.21",
          "form": "모공삭제되는 그날까지 쇼핑할껍니다ㅎ",
          "original_form": "모공삭제되는 그날까지 쇼핑할껍니다ㅎ",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017785.1.22",
          "form": "종종 추천 부탁드립니다",
          "original_form": "종종 추천 부탁드립니다",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100017785.1.23",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210826 21:16"
        }
      ]
