        {
          "id": "MDRW2100016294.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 11:43"
        },
        {
          "id": "MDRW2100016294.1.3",
          "form": "종교가 있으신가요?",
          "original_form": "종교가 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 11:44"
        },
        {
          "id": "MDRW2100016294.1.4",
          "form": "무교입니다",
          "original_form": "무교입니다",
          "speaker_id": "1",
          "time": "20210826 11:44"
        },
        {
          "id": "MDRW2100016294.1.5",
          "form": "저도 무교예요 ㅎㅎ",
          "original_form": "저도 무교예요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 11:45"
        },
        {
          "id": "MDRW2100016294.1.6",
          "form": "근데 불교는 좋아하는편이에요",
          "original_form": "근데 불교는 좋아하는편이에요",
          "speaker_id": "2",
          "time": "20210826 11:45"
        },
        {
          "id": "MDRW2100016294.1.7",
          "form": "이유가 있으신가요?",
          "original_form": "이유가 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 11:45"
        },
        {
          "id": "MDRW2100016294.1.8",
          "form": "무교요?",
          "original_form": "무교요?",
          "speaker_id": "2",
          "time": "20210826 11:45"
        },
        {
          "id": "MDRW2100016294.1.9",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210826 11:45"
        },
        {
          "id": "MDRW2100016294.1.10",
          "form": "아 종교도 부지런해야 하겠더라구요",
          "original_form": "아 종교도 부지런해야 하겠더라구요",
          "speaker_id": "2",
          "time": "20210826 11:46"
        },
        {
          "id": "MDRW2100016294.1.11",
          "form": "공부도 해야되고",
          "original_form": "공부도 해야되고",
          "speaker_id": "2",
          "time": "20210826 11:46"
        },
        {
          "id": "MDRW2100016294.1.12",
          "form": "절 가본적 있으세요?",
          "original_form": "절 가본적 있으세요?",
          "speaker_id": "1",
          "time": "20210826 11:46"
        },
        {
          "id": "MDRW2100016294.1.13",
          "form": "네네 여행가면 근처 절은 가본거 같아요",
          "original_form": "네네 여행가면 근처 절은 가본거 같아요",
          "speaker_id": "2",
          "time": "20210826 11:47"
        },
        {
          "id": "MDRW2100016294.1.14",
          "form": "막 유명한절은 아니어서 이름은 기억이 안나네요",
          "original_form": "막 유명한절은 아니어서 이름은 기억이 안나네요",
          "speaker_id": "2",
          "time": "20210826 11:47"
        },
        {
          "id": "MDRW2100016294.1.15",
          "form": "저는 봉은사",
          "original_form": "저는 봉은사",
          "speaker_id": "1",
          "time": "20210826 11:49"
        },
        {
          "id": "MDRW2100016294.1.16",
          "form": "가봤어요",
          "original_form": "가봤어요",
          "speaker_id": "1",
          "time": "20210826 11:49"
        },
        {
          "id": "MDRW2100016294.1.17",
          "form": "오 봉은사 서울에 있는 절이네요",
          "original_form": "오 봉은사 서울에 있는 절이네요",
          "speaker_id": "2",
          "time": "20210826 11:50"
        },
        {
          "id": "MDRW2100016294.1.18",
          "form": "서울에 있는 절은 한번도 안 가봤어요",
          "original_form": "서울에 있는 절은 한번도 안 가봤어요",
          "speaker_id": "2",
          "time": "20210826 11:50"
        },
        {
          "id": "MDRW2100016294.1.19",
          "form": "도심에 있어서 특이하더라고요",
          "original_form": "도심에 있어서 특이하더라고요",
          "speaker_id": "1",
          "time": "20210826 11:52"
        }
      ]
