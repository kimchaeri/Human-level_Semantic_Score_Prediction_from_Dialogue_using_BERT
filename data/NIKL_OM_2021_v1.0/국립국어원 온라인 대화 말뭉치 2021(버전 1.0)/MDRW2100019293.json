      "utterance": [
        {
          "id": "MDRW2100019293.1.1",
          "form": "리브메이트 뽑기 하셨나요?",
          "original_form": "리브메이트 뽑기 하셨나요?",
          "speaker_id": "1",
          "time": "20210827 17:00"
        },
        {
          "id": "MDRW2100019293.1.2",
          "form": "네당근 꽝이죠",
          "original_form": "네당근 꽝이죠",
          "speaker_id": "2",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100019293.1.3",
          "form": "쳇봇도꽝",
          "original_form": "쳇봇도꽝",
          "speaker_id": "2",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100019293.1.4",
          "form": "꽝파티!!",
          "original_form": "꽝파티!!",
          "speaker_id": "1",
          "time": "20210827 17:01"
        },
        {
          "id": "MDRW2100019293.1.5",
          "form": "해피볼도..100포..",
          "original_form": "해피볼도..100포..",
          "speaker_id": "2",
          "time": "20210827 17:03"
        },
        {
          "id": "MDRW2100019293.1.6",
          "form": "31사전예약하시나요?",
          "original_form": "31사전예약하시나요?",
          "speaker_id": "2",
          "time": "20210827 17:03"
        },
        {
          "id": "MDRW2100019293.1.7",
          "form": "해피볼 안했다 우왓",
          "original_form": "해피볼 안했다 우왓",
          "speaker_id": "1",
          "time": "20210827 17:03"
        },
        {
          "id": "MDRW2100019293.1.8",
          "form": "해야겠..죠? ㅋㅋㅋ 귀찮...ㅠ",
          "original_form": "해야겠..죠? ㅋㅋㅋ 귀찮...ㅠ",
          "speaker_id": "1",
          "time": "20210827 17:03"
        },
        {
          "id": "MDRW2100019293.1.9",
          "form": "그치만 동생에게 약속했죠 8월달도 사줄게라고",
          "original_form": "그치만 동생에게 약속했죠 8월달도 사줄게라고",
          "speaker_id": "1",
          "time": "20210827 17:03"
        },
        {
          "id": "MDRW2100019293.1.10",
          "form": "그거안빼먹으면 5천포아니에요?",
          "original_form": "그거안빼먹으면 5천포아니에요?",
          "speaker_id": "2",
          "time": "20210827 17:03"
        },
        {
          "id": "MDRW2100019293.1.11",
          "form": "헐! 전부다?",
          "original_form": "헐! 전부다?",
          "speaker_id": "1",
          "time": "20210827 17:04"
        },
        {
          "id": "MDRW2100019293.1.12",
          "form": "와 대박인디",
          "original_form": "와 대박인디",
          "speaker_id": "1",
          "time": "20210827 17:04"
        },
        {
          "id": "MDRW2100019293.1.13",
          "form": "현명한 소비 카페 보면서 따라하긴하는데 세세하게 기억은 안남",
          "original_form": "현명한 소비 카페 보면서 따라하긴하는데 세세하게 기억은 안남",
          "speaker_id": "1",
          "time": "20210827 17:04"
        },
        {
          "id": "MDRW2100019293.1.14",
          "form": "아닌가 ㅋㅋㅋ",
          "original_form": "아닌가 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:07"
        },
        {
          "id": "MDRW2100019293.1.15",
          "form": "다는오바지?",
          "original_form": "다는오바지?",
          "speaker_id": "2",
          "time": "20210827 17:07"
        },
        {
          "id": "MDRW2100019293.1.16",
          "form": "다는 아냐 ㅋㅋㅋ",
          "original_form": "다는 아냐 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:07"
        },
        {
          "id": "MDRW2100019293.1.17",
          "form": "ㅋㅋㅋ좀 쉬워보이는것만?",
          "original_form": "ㅋㅋㅋ좀 쉬워보이는것만?",
          "speaker_id": "1",
          "time": "20210827 17:07"
        },
        {
          "id": "MDRW2100019293.1.18",
          "form": "매일하면맞나봐...",
          "original_form": "매일하면맞나봐...",
          "speaker_id": "2",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100019293.1.19",
          "form": "23잏부터",
          "original_form": "23잏부터",
          "speaker_id": "2",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100019293.1.20",
          "form": "29일",
          "original_form": "29일",
          "speaker_id": "2",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100019293.1.21",
          "form": "와 대박 혜자이벤트다!!!",
          "original_form": "와 대박 혜자이벤트다!!!",
          "speaker_id": "1",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100019293.1.22",
          "form": "열심히하자 까먹지말고",
          "original_form": "열심히하자 까먹지말고",
          "speaker_id": "1",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100019293.1.23",
          "form": "지금 5번이라떳어?",
          "original_form": "지금 5번이라떳어?",
          "speaker_id": "2",
          "time": "20210827 17:10"
        },
        {
          "id": "MDRW2100019293.1.24",
          "form": "그럼 매일한게맞는듯",
          "original_form": "그럼 매일한게맞는듯",
          "speaker_id": "2",
          "time": "20210827 17:10"
        },
        {
          "id": "MDRW2100019293.1.25",
          "form": "23일브터!!",
          "original_form": "23일브터!!",
          "speaker_id": "2",
          "time": "20210827 17:10"
        },
        {
          "id": "MDRW2100019293.1.26",
          "form": "응응!! 다행이다 와 ㅠㅠ",
          "original_form": "응응!! 다행이다 와 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 17:11"
        },
        {
          "id": "MDRW2100019293.1.27",
          "form": "대박 ㅋㅋㅋ !!!!!",
          "original_form": "대박 ㅋㅋㅋ !!!!!",
          "speaker_id": "2",
          "time": "20210827 17:11"
        },
        {
          "id": "MDRW2100019293.1.28",
          "form": "31일 스템프모으시나요?",
          "original_form": "31일 스템프모으시나요?",
          "speaker_id": "2",
          "time": "20210827 17:11"
        },
        {
          "id": "MDRW2100019293.1.29",
          "form": "이벤트는 역시 현명한 소비 카페야",
          "original_form": "이벤트는 역시 현명한 소비 카페야",
          "speaker_id": "1",
          "time": "20210827 17:11"
        },
        {
          "id": "MDRW2100019293.1.30",
          "form": "광고같네요",
          "original_form": "광고같네요",
          "speaker_id": "2",
          "time": "20210827 17:11"
        },
        {
          "id": "MDRW2100019293.1.31",
          "form": "ㅋㅋ스템프는 많이 빼먹었더라..",
          "original_form": "ㅋㅋ스템프는 많이 빼먹었더라..",
          "speaker_id": "1",
          "time": "20210827 17:11"
        },
        {
          "id": "MDRW2100019293.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ타이틀인가요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ타이틀인가요",
          "speaker_id": "2",
          "time": "20210827 17:11"
        },
        {
          "id": "MDRW2100019293.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아웃기",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아웃기",
          "speaker_id": "1",
          "time": "20210827 17:11"
        },
        {
          "id": "MDRW2100019293.1.34",
