      "utterance": [
        {
          "id": "MDRW2100033555.1.1",
          "form": "밖에 비많이오나봐요",
          "original_form": "밖에 비많이오나봐요",
          "speaker_id": "1",
          "time": "20210824 13:58"
        },
        {
          "id": "MDRW2100033555.1.2",
          "form": "그쳤다가 조금씩와요",
          "original_form": "그쳤다가 조금씩와요",
          "speaker_id": "2",
          "time": "20210824 13:58"
        },
        {
          "id": "MDRW2100033555.1.3",
          "form": "출근할땐 조금오더니 제법오네요",
          "original_form": "출근할땐 조금오더니 제법오네요",
          "speaker_id": "1",
          "time": "20210824 13:59"
        },
        {
          "id": "MDRW2100033555.1.4",
          "form": "습도때매 그만좀 왔음 좋겠어요",
          "original_form": "습도때매 그만좀 왔음 좋겠어요",
          "speaker_id": "2",
          "time": "20210824 14:00"
        },
        {
          "id": "MDRW2100033555.1.5",
          "form": "끈적거려요",
          "original_form": "끈적거려요",
          "speaker_id": "2",
          "time": "20210824 14:00"
        },
        {
          "id": "MDRW2100033555.1.6",
          "form": "그니까요 비오니 좀습하네요",
          "original_form": "그니까요 비오니 좀습하네요",
          "speaker_id": "1",
          "time": "20210824 14:00"
        },
        {
          "id": "MDRW2100033555.1.7",
          "form": "태풍은 지나간건가요",
          "original_form": "태풍은 지나간건가요",
          "speaker_id": "1",
          "time": "20210824 14:00"
        },
        {
          "id": "MDRW2100033555.1.8",
          "form": "지나간거같아요.",
          "original_form": "지나간거같아요.",
          "speaker_id": "2",
          "time": "20210824 14:01"
        },
        {
          "id": "MDRW2100033555.1.9",
          "form": "더 동쪽으로가서 그나마 피해덜했다고 한거같아요",
          "original_form": "더 동쪽으로가서 그나마 피해덜했다고 한거같아요",
          "speaker_id": "2",
          "time": "20210824 14:01"
        },
        {
          "id": "MDRW2100033555.1.10",
          "form": "온다한거같은데 언제또 지나갔대요ㅎㅎ",
          "original_form": "온다한거같은데 언제또 지나갔대요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 14:01"
        },
        {
          "id": "MDRW2100033555.1.11",
          "form": "ㅎㅎ 금방지나갔더라구요",
          "original_form": "ㅎㅎ 금방지나갔더라구요",
          "speaker_id": "2",
          "time": "20210824 14:02"
        },
        {
          "id": "MDRW2100033555.1.12",
          "form": "제주도 400미리온다더니",
          "original_form": "제주도 400미리온다더니",
          "speaker_id": "1",
          "time": "20210824 14:03"
        },
        {
          "id": "MDRW2100033555.1.13",
          "form": "비피해없이 지나갔는지 몰겠어요",
          "original_form": "비피해없이 지나갔는지 몰겠어요",
          "speaker_id": "1",
          "time": "20210824 14:03"
        },
        {
          "id": "MDRW2100033555.1.14",
          "form": "제가사는데가 피해가 별로없어서 별로 와닫지않아요.",
          "original_form": "제가사는데가 피해가 별로없어서 별로 와닫지않아요.",
          "speaker_id": "2",
          "time": "20210824 14:03"
        },
        {
          "id": "MDRW2100033555.1.15",
          "form": "피해나는곳만 계속 나는거같아요",
          "original_form": "피해나는곳만 계속 나는거같아요",
          "speaker_id": "2",
          "time": "20210824 14:04"
        },
        {
          "id": "MDRW2100033555.1.16",
          "form": "저도요 비온다더니",
          "original_form": "저도요 비온다더니",
          "speaker_id": "1",
          "time": "20210824 14:04"
        },
        {
          "id": "MDRW2100033555.1.17",
          "form": "어제도 많이 안오더라구요",
          "original_form": "어제도 많이 안오더라구요",
          "speaker_id": "1",
          "time": "20210824 14:04"
        },
        {
          "id": "MDRW2100033555.1.18",
          "form": "비올때마다 엄청 불안할듯해요",
          "original_form": "비올때마다 엄청 불안할듯해요",
          "speaker_id": "2",
          "time": "20210824 14:04"
        },
        {
          "id": "MDRW2100033555.1.19",
          "form": "매번 잠기는곳은 그럴거같아요",
          "original_form": "매번 잠기는곳은 그럴거같아요",
          "speaker_id": "1",
          "time": "20210824 14:04"
        }
