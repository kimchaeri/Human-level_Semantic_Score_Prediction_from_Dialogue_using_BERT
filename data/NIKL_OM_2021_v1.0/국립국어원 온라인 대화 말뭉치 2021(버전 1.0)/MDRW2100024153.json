      "utterance": [
        {
          "id": "MDRW2100024153.1.1",
          "form": "스포츠도 좋아하세요?",
          "original_form": "스포츠도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024153.1.2",
          "form": "네!ㅋㅋ 체력이안좋아서",
          "original_form": "네!ㅋㅋ 체력이안좋아서",
          "speaker_id": "2",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100024153.1.3",
          "form": "보는거 좋아해요",
          "original_form": "보는거 좋아해요",
          "speaker_id": "2",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100024153.1.4",
          "form": "아 주로 어떤거 보시나요ㅎㅎ",
          "original_form": "아 주로 어떤거 보시나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100024153.1.5",
          "form": "챙겨보는건 따로없고..",
          "original_form": "챙겨보는건 따로없고..",
          "speaker_id": "2",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100024153.1.6",
          "form": "배구 농구 축구",
          "original_form": "배구 농구 축구",
          "speaker_id": "2",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100024153.1.7",
          "form": "오 ㅎㅎ 전 야구를 봅니다",
          "original_form": "오 ㅎㅎ 전 야구를 봅니다",
          "speaker_id": "1",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100024153.1.8",
          "form": "ㅎㅎㅎ직관보다는 티비로 설명들으면서",
          "original_form": "ㅎㅎㅎ직관보다는 티비로 설명들으면서",
          "speaker_id": "2",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100024153.1.9",
          "form": "보는게",
          "original_form": "보는게",
          "speaker_id": "2",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100024153.1.10",
          "form": "앜.ㅋㅋㅋ 야구는 너무어려워요",
          "original_form": "앜.ㅋㅋㅋ 야구는 너무어려워요",
          "speaker_id": "2",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100024153.1.11",
          "form": "맞죠 ㅋㅋㅋ",
          "original_form": "맞죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100024153.1.12",
          "form": "그래서 제 친구들도 보통",
          "original_form": "그래서 제 친구들도 보통",
          "speaker_id": "1",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100024153.1.13",
          "form": "축구배구봐요 ㅋㅋ",
          "original_form": "축구배구봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100024153.1.14",
          "form": "야구는 공짜표있어서",
          "original_form": "야구는 공짜표있어서",
          "speaker_id": "2",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100024153.1.15",
          "form": "야구장은가봤는데",
          "original_form": "야구장은가봤는데",
          "speaker_id": "2",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100024153.1.16",
          "form": "치맥먹는재미",
          "original_form": "치맥먹는재미",
          "speaker_id": "2",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100024153.1.17",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100024153.1.18",
          "form": "요즘은 못먹지만ㅜㅜ",
          "original_form": "요즘은 못먹지만ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100024153.1.19",
          "form": "아쉬워요ㅠㅗ",
          "original_form": "아쉬워요ㅠㅗ",
          "speaker_id": "2",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100024153.1.20",
          "form": "헉 오타입니다",
          "original_form": "헉 오타입니다",
          "speaker_id": "2",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100024153.1.21",
          "form": "괜찮아오 !",
          "original_form": "괜찮아오 !",
          "speaker_id": "1",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100024153.1.22",
          "form": "올림픽도 보셨어요?",
          "original_form": "올림픽도 보셨어요?",
          "speaker_id": "1",
          "time": "20210830 22:50"
        },
        {
          "id": "MDRW2100024153.1.23",
          "form": "네네.ㅋㅋ",
          "original_form": "네네.ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:51"
        },
        {
          "id": "MDRW2100024153.1.24",
          "form": "야구도 봤는데",
          "original_form": "야구도 봤는데",
          "speaker_id": "2",
          "time": "20210830 22:51"
        },
        {
          "id": "MDRW2100024153.1.25",
          "form": "그 껌씹는장면이 기억에 남아요",
          "original_form": "그 껌씹는장면이 기억에 남아요",
          "speaker_id": "2",
          "time": "20210830 22:51"
        },
        {
          "id": "MDRW2100024153.1.26",
          "form": "아 ㅋㅋ 그장면을 보셨군요...!",
          "original_form": "아 ㅋㅋ 그장면을 보셨군요...!",
          "speaker_id": "1",
          "time": "20210830 22:52"
        }
      ]
    }
  ]
