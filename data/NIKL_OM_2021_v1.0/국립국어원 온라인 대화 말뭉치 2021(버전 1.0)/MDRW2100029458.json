      "utterance": [
        {
          "id": "MDRW2100029458.1.1",
          "form": "넷플리스 이용하시너용?",
          "original_form": "넷플리스 이용하시너용?",
          "speaker_id": "2",
          "time": "20210903 11:49"
        },
        {
          "id": "MDRW2100029458.1.2",
          "form": "네네 저 넷플릭스 써요",
          "original_form": "네네 저 넷플릭스 써요",
          "speaker_id": "1",
          "time": "20210903 12:23"
        },
        {
          "id": "MDRW2100029458.1.3",
          "form": "넷플릭스 보시나요?",
          "original_form": "넷플릭스 보시나요?",
          "speaker_id": "1",
          "time": "20210903 12:23"
        },
        {
          "id": "MDRW2100029458.1.4",
          "form": "저두요! 최근엔 디피봣여용 ㅎㅎ",
          "original_form": "저두요! 최근엔 디피봣여용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 12:43"
        },
        {
          "id": "MDRW2100029458.1.5",
          "form": "오 디피 무슨 내용인가요? 1위에있더라구요",
          "original_form": "오 디피 무슨 내용인가요? 1위에있더라구요",
          "speaker_id": "1",
          "time": "20210903 13:06"
        },
        {
          "id": "MDRW2100029458.1.6",
          "form": "군대내용이라 안보려고 햇는데",
          "original_form": "군대내용이라 안보려고 햇는데",
          "speaker_id": "2",
          "time": "20210903 15:21"
        },
        {
          "id": "MDRW2100029458.1.7",
          "form": "탈영병잡는 이야기라 훈훈하고 슬프고 그래용 ㅠㅍ",
          "original_form": "탈영병잡는 이야기라 훈훈하고 슬프고 그래용 ㅠㅍ",
          "speaker_id": "2",
          "time": "20210903 15:22"
        },
        {
          "id": "MDRW2100029458.1.8",
          "form": "허얼 ㅜㅜ 그런얘기였군요!!",
          "original_form": "허얼 ㅜㅜ 그런얘기였군요!!",
          "speaker_id": "1",
          "time": "20210903 15:22"
        },
        {
          "id": "MDRW2100029458.1.9",
          "form": "저도 군대얘기같아서 안끌렸었거든요",
          "original_form": "저도 군대얘기같아서 안끌렸었거든요",
          "speaker_id": "1",
          "time": "20210903 15:22"
        },
        {
          "id": "MDRW2100029458.1.10",
          "form": "저도 남편이 봐서 봤는ㄷㅔ 재밌더아구요",
          "original_form": "저도 남편이 봐서 봤는ㄷㅔ 재밌더아구요",
          "speaker_id": "2",
          "time": "20210903 15:26"
        },
        {
          "id": "MDRW2100029458.1.11",
          "form": "짧기도 하고 맘먹고보면 몰아볼수있는....ㅎㅎ",
          "original_form": "짧기도 하고 맘먹고보면 몰아볼수있는....ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 15:26"
        },
        {
          "id": "MDRW2100029458.1.12",
          "form": "다음 주말에 볼것으로 고려해봐야겠네요 ㅋㅋ",
          "original_form": "다음 주말에 볼것으로 고려해봐야겠네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:27"
        },
        {
          "id": "MDRW2100029458.1.13",
          "form": "저는 맨날 뭐볼지 고민만하다가 지쳐서 넷플 꺼요 ㅋㅋㅋㅋ",
          "original_form": "저는 맨날 뭐볼지 고민만하다가 지쳐서 넷플 꺼요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:28"
        },
        {
          "id": "MDRW2100029458.1.14",
          "form": "맞아요 ㅠㅠ져도 볼거 다 보다가 이제 지칠.... 요새는 라이프? 드것도 봐요",
          "original_form": "맞아요 ㅠㅠ져도 볼거 다 보다가 이제 지칠.... 요새는 라이프? 드것도 봐요",
          "speaker_id": "2",
          "time": "20210903 15:30"
        },
        {
          "id": "MDRW2100029458.1.15",
          "form": "오 그건 무슨 내용이에요? 드라마에요?",
          "original_form": "오 그건 무슨 내용이에요? 드라마에요?",
          "speaker_id": "1",
          "time": "20210903 15:30"
        },
        {
          "id": "MDRW2100029458.1.16",
          "form": "조승우 나오고 이동욱 나오단데 병원이야기더라구요 드라마용",
          "original_form": "조승우 나오고 이동욱 나오단데 병원이야기더라구요 드라마용",
          "speaker_id": "2",
          "time": "20210903 15:30"
        },
        {
          "id": "MDRW2100029458.1.17",
          "form": "비밀의 숲아세용? 그작가예용ㅋㅋ",
          "original_form": "비밀의 숲아세용? 그작가예용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:30"
        },
        {
          "id": "MDRW2100029458.1.18",
          "form": "오 대박 그거는 지금 어느 방송국에서 하고있나요?",
          "original_form": "오 대박 그거는 지금 어느 방송국에서 하고있나요?",
          "speaker_id": "1",
          "time": "20210903 15:32"
        },
        {
          "id": "MDRW2100029458.1.19",
          "form": "아 그거 예전꺼라서 넷플에서 보실수이ㅆ어용 ㅎㅎ",
          "original_form": "아 그거 예전꺼라서 넷플에서 보실수이ㅆ어용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 17:52"
        },
        {
          "id": "MDRW2100029458.1.20",
          "form": "그러니까요ㅠㅠㅠ 여행 프로그램같은것도 보시나요?",
          "original_form": "그러니까요ㅠㅠㅠ 여행 프로그램같은것도 보시나요?",
