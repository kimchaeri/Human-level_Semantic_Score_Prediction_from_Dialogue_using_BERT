      "utterance": [
        {
          "id": "MDRW2100030464.1.1",
          "form": "누군가(무언가)로 부터 탄생한 불멸이",
          "original_form": "누군가(무언가)로 부터 탄생한 불멸이",
          "speaker_id": "2",
          "time": "20210904 01:30"
        },
        {
          "id": "MDRW2100030464.1.2",
          "form": "상대로 부터 받은 자극을 통해 배워나는 이야기..",
          "original_form": "상대로 부터 받은 자극을 통해 배워나는 이야기..",
          "speaker_id": "2",
          "time": "20210904 01:30"
        },
        {
          "id": "MDRW2100030464.1.3",
          "form": "이야기 진행은 그런데",
          "original_form": "이야기 진행은 그런데",
          "speaker_id": "2",
          "time": "20210904 01:31"
        },
        {
          "id": "MDRW2100030464.1.4",
          "form": "최종적으론 뭘 말하고 싶은지는 더 봐야지",
          "original_form": "최종적으론 뭘 말하고 싶은지는 더 봐야지",
          "speaker_id": "2",
          "time": "20210904 01:31"
        },
        {
          "id": "MDRW2100030464.1.5",
          "form": "불멸의 상태인거야? 아니면 이름이 불멸이야?",
          "original_form": "불멸의 상태인거야? 아니면 이름이 불멸이야?",
          "speaker_id": "1",
          "time": "20210904 01:33"
        },
        {
          "id": "MDRW2100030464.1.6",
          "form": "그 두개가 일단은 다른데 옵션이 두개가 같은거라면 전개가 다르니까",
          "original_form": "그 두개가 일단은 다른데 옵션이 두개가 같은거라면 전개가 다르니까",
          "speaker_id": "1",
          "time": "20210904 01:34"
        },
        {
          "id": "MDRW2100030464.1.7",
          "form": "불멸의 상태인거고 처음엔 사람도 아니엇어 ㅋㅋ",
          "original_form": "불멸의 상태인거고 처음엔 사람도 아니엇어 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:37"
        },
        {
          "id": "MDRW2100030464.1.8",
          "form": "다른 사람만나서 불멸이라서 후시?라고 이름 붙여줌",
          "original_form": "다른 사람만나서 불멸이라서 후시?라고 이름 붙여줌",
          "speaker_id": "2",
          "time": "20210904 01:37"
        },
        {
          "id": "MDRW2100030464.1.9",
          "form": "후시 라는게 불멸이란 뜻이야?",
          "original_form": "후시 라는게 불멸이란 뜻이야?",
          "speaker_id": "1",
          "time": "20210904 01:38"
        },
        {
          "id": "MDRW2100030464.1.10",
          "form": "새로운 애니메이션이구만",
          "original_form": "새로운 애니메이션이구만",
          "speaker_id": "1",
          "time": "20210904 01:38"
        },
        {
          "id": "MDRW2100030464.1.11",
          "form": "음 내가 말한 거만 읽으면 아주 밝아보이지만",
          "original_form": "음 내가 말한 거만 읽으면 아주 밝아보이지만",
          "speaker_id": "2",
          "time": "20210904 01:39"
        },
        {
          "id": "MDRW2100030464.1.12",
          "form": "정반대에요",
          "original_form": "정반대에요",
          "speaker_id": "2",
          "time": "20210904 01:40"
        },
        {
          "id": "MDRW2100030464.1.13",
          "form": "캐릭터에 애정을 주지마세요..다이다이하니까..",
          "original_form": "캐릭터에 애정을 주지마세요..다이다이하니까..",
          "speaker_id": "2",
          "time": "20210904 01:40"
        },
        {
          "id": "MDRW2100030464.1.14",
          "form": "아하 치유물이구나",
          "original_form": "아하 치유물이구나",
          "speaker_id": "1",
          "time": "20210904 01:40"
        },
        {
          "id": "MDRW2100030464.1.15",
          "form": "그렇담 걸러야지",
          "original_form": "그렇담 걸러야지",
          "speaker_id": "1",
          "time": "20210904 01:40"
        },
        {
          "id": "MDRW2100030464.1.16",
          "form": "나랑 드래곤길들이기나 보자",
          "original_form": "나랑 드래곤길들이기나 보자",
          "speaker_id": "1",
          "time": "20210904 01:40"
        },
        {
          "id": "MDRW2100030464.1.17",
          "form": "그러니까 추천 안하고 잇자나요 ㅋㅋ",
          "original_form": "그러니까 추천 안하고 잇자나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:41"
        },
        {
          "id": "MDRW2100030464.1.18",
          "form": "드길 천년만년 해줘 ㅠ",
          "original_form": "드길 천년만년 해줘 ㅠ",
          "speaker_id": "2",
          "time": "20210904 01:42"
        },
        {
          "id": "MDRW2100030464.1.19",
          "form": "내 장르가 완결이라니",
          "original_form": "내 장르가 완결이라니",
          "speaker_id": "1",
          "time": "20210904 01:42"
        },
        {
          "id": "MDRW2100030464.1.20",
          "form": "믿을 수가 없어",
          "original_form": "믿을 수가 없어",
          "speaker_id": "1",
          "time": "20210904 01:42"
        },
        {
          "id": "MDRW2100030464.1.21",
          "form": "아냐 그럴리없어!",
          "original_form": "아냐 그럴리없어!",
          "speaker_id": "2",
          "time": "20210904 01:43"
        },
        {
          "id": "MDRW2100030464.1.22",
          "form": "3편 이후 애니메이션이 TV판으로 더 나온댔는데",
          "original_form": "3편 이후 애니메이션이 TV판으로 더 나온댔는데",
          "speaker_id": "1",
          "time": "20210904 01:43"
        },
        {
          "id": "MDRW2100030464.1.23",
          "form": "넷플릭스 나 기다리고 있는데",
          "original_form": "넷플릭스 나 기다리고 있는데",
          "speaker_id": "1",
          "time": "20210904 01:43"
        },
        {
          "id": "MDRW2100030464.1.24",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:44"
        },
        {
          "id": "MDRW2100030464.1.25",
          "form": "판권가져오자",
          "original_form": "판권가져오자",
          "speaker_id": "2",
          "time": "20210904 01:44"
        },
        {
          "id": "MDRW2100030464.1.26",
          "form": "일하자 넷플렉스",
          "original_form": "일하자 넷플렉스",
          "speaker_id": "2",
          "time": "20210904 01:44"
        },
        {
          "id": "MDRW2100030464.1.27",
          "form": "판권은 넷플릭스거야 ㅠ",
          "original_form": "판권은 넷플릭스거야 ㅠ",
          "speaker_id": "1",
          "time": "20210904 01:47"
        },
        {
          "id": "MDRW2100030464.1.28",
          "form": "그러니까 좀만 더 만들어줘",
          "original_form": "그러니까 좀만 더 만들어줘",
          "speaker_id": "1",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100030464.1.29",
          "form": "더 판을 굴리렴 넷플 라프텔이 따라온당",
          "original_form": "더 판을 굴리렴 넷플 라프텔이 따라온당",
          "speaker_id": "2",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100030464.1.30",
          "form": "물론 장르가 좀 다르지만 ㅋㅋ",
          "original_form": "물론 장르가 좀 다르지만 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100030464.1.31",
          "form": "갸아아ㅏㄱ",
          "original_form": "갸아아ㅏㄱ",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100030464.1.32",
          "form": "넷플도 좋은데 은근 볼게없어",
          "original_form": "넷플도 좋은데 은근 볼게없어",
          "speaker_id": "1",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100030464.1.33",
          "form": "나랑 강연금 덕질이나 해줘",
          "original_form": "나랑 강연금 덕질이나 해줘",
          "speaker_id": "1",
