      "utterance": [
        {
          "id": "MDRW2100034220.1.1",
          "form": "미국에서 간 곳 중 어디가 제일 기억나?",
          "original_form": "미국에서 간 곳 중 어디가 제일 기억나?",
          "speaker_id": "2",
          "time": "20210827 21:23"
        },
        {
          "id": "MDRW2100034220.1.2",
          "form": "음..",
          "original_form": "음..",
          "speaker_id": "1",
          "time": "20210827 21:23"
        },
        {
          "id": "MDRW2100034220.1.3",
          "form": "나는 바닷가 ㅋㅋ",
          "original_form": "나는 바닷가 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:23"
        },
        {
          "id": "MDRW2100034220.1.4",
          "form": "나도 ㅋㅋ 바다 다 좋지",
          "original_form": "나도 ㅋㅋ 바다 다 좋지",
          "speaker_id": "1",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100034220.1.5",
          "form": "ㅠㅠ 파도도 높게 치고",
          "original_form": "ㅠㅠ 파도도 높게 치고",
          "speaker_id": "2",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100034220.1.6",
          "form": "물도 파래서 좋았지",
          "original_form": "물도 파래서 좋았지",
          "speaker_id": "1",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100034220.1.7",
          "form": "그니깡~ 물놀이는 많이 못했지만 ㅋㅋㅋ 좋았엉",
          "original_form": "그니깡~ 물놀이는 많이 못했지만 ㅋㅋㅋ 좋았엉",
          "speaker_id": "2",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100034220.1.8",
          "form": "담에가면 서핑 해봐라",
          "original_form": "담에가면 서핑 해봐라",
          "speaker_id": "1",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100034220.1.9",
          "form": "그럴까 ㅎㅎ",
          "original_form": "그럴까 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100034220.1.10",
          "form": "난 무서워서 못한다",
          "original_form": "난 무서워서 못한다",
          "speaker_id": "1",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100034220.1.11",
          "form": "나도 무섭긴한데 ㅋㅋ",
          "original_form": "나도 무섭긴한데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100034220.1.12",
          "form": "해봐라",
          "original_form": "해봐라",
          "speaker_id": "1",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100034220.1.13",
          "form": "그래야겠다!",
          "original_form": "그래야겠다!",
