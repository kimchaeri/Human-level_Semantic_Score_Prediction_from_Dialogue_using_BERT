      "utterance": [
        {
          "id": "MDRW2100012120.1.1",
          "form": "동호회 하시는거있으세요?",
          "original_form": "동호회 하시는거있으세요?",
          "speaker_id": "2",
          "time": "20210825 01:16"
        },
        {
          "id": "MDRW2100012120.1.2",
          "form": "동호회 하는거 없어요 여름님은요?",
          "original_form": "동호회 하는거 없어요 여름님은요?",
          "speaker_id": "1",
          "time": "20210825 01:16"
        },
        {
          "id": "MDRW2100012120.1.3",
          "form": "오 넘 궁금하지만",
          "original_form": "오 넘 궁금하지만",
          "speaker_id": "2",
          "time": "20210825 01:17"
        },
        {
          "id": "MDRW2100012120.1.4",
          "form": "저는 지금은 없구요 코로나끝나면",
          "original_form": "저는 지금은 없구요 코로나끝나면",
          "speaker_id": "2",
          "time": "20210825 01:18"
        },
        {
          "id": "MDRW2100012120.1.5",
          "form": "러닝동호회 들어보고싶어요",
          "original_form": "러닝동호회 들어보고싶어요",
          "speaker_id": "2",
          "time": "20210825 01:18"
        },
        {
          "id": "MDRW2100012120.1.6",
          "form": "수영이나",
          "original_form": "수영이나",
          "speaker_id": "2",
          "time": "20210825 01:18"
        },
        {
          "id": "MDRW2100012120.1.7",
          "form": "옥돔님은여",
          "original_form": "옥돔님은여",
          "speaker_id": "2",
          "time": "20210825 01:18"
        },
        {
          "id": "MDRW2100012120.1.8",
          "form": "배구 동호회 해보고싶은데",
          "original_form": "배구 동호회 해보고싶은데",
          "speaker_id": "1",
          "time": "20210825 01:18"
        },
        {
          "id": "MDRW2100012120.1.9",
          "form": "지역에 없어요",
          "original_form": "지역에 없어요",
          "speaker_id": "1",
          "time": "20210825 01:19"
        },
        {
          "id": "MDRW2100012120.1.10",
          "form": "저도 올림픽보고 배구 넘 보고싶고 해보고싶었는데 그쳐 저희지역에도 없더라구요ㅜ",
          "original_form": "저도 올림픽보고 배구 넘 보고싶고 해보고싶었는데 그쳐 저희지역에도 없더라구요ㅜ",
          "speaker_id": "2",
          "time": "20210825 01:20"
        },
        {
          "id": "MDRW2100012120.1.11",
          "form": "경기도..버스타고 가야되고",
          "original_form": "경기도..버스타고 가야되고",
          "speaker_id": "2",
          "time": "20210825 01:20"
        },
        {
          "id": "MDRW2100012120.1.12",
          "form": "배구는 정말 없더라고요",
          "original_form": "배구는 정말 없더라고요",
          "speaker_id": "1",
          "time": "20210825 01:26"
        },
        {
          "id": "MDRW2100012120.1.13",
          "form": "축구는많던데..",
          "original_form": "축구는많던데..",
          "speaker_id": "1",
          "time": "20210825 01:26"
        },
        {
          "id": "MDRW2100012120.1.14",
          "form": "그쳐..ㅜ저희동넨 애들 축구밖에...",
          "original_form": "그쳐..ㅜ저희동넨 애들 축구밖에...",
          "speaker_id": "2",
          "time": "20210825 01:27"
        },
        {
          "id": "MDRW2100012120.1.15",
          "form": "일반사람들도 같이즐길려면 뭐가좀 있어야 즐길텐데말이져ㅜㅜ",
          "original_form": "일반사람들도 같이즐길려면 뭐가좀 있어야 즐길텐데말이져ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 01:27"
        },
        {
          "id": "MDRW2100012120.1.16",
          "form": "혼자 토스연습이라도 하려고용ㅋㅋ",
          "original_form": "혼자 토스연습이라도 하려고용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:28"
        },
        {
          "id": "MDRW2100012120.1.17",
          "form": "봉사활동 해보신적 있으신가요?",
          "original_form": "봉사활동 해보신적 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 02:15"
        },
        {
          "id": "MDRW2100012120.1.18",
          "form": "예전에 해본적있어요 ㅎㅎ 동물보호소에서",
          "original_form": "예전에 해본적있어요 ㅎㅎ 동물보호소에서",
          "speaker_id": "2",
          "time": "20210825 13:11"
        },
        {
          "id": "MDRW2100012120.1.19",
          "form": "옥돔님은요",
          "original_form": "옥돔님은요",
          "speaker_id": "2",
          "time": "20210825 13:11"
        },
        {
          "id": "MDRW2100012120.1.20",
          "form": "청소년센터에서 봉사활동 해봤습니다",
          "original_form": "청소년센터에서 봉사활동 해봤습니다",
          "speaker_id": "1",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100012120.1.21",
          "form": "어떤활동을 하나요 거기에선?",
          "original_form": "어떤활동을 하나요 거기에선?",
          "speaker_id": "2",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100012120.1.22",
          "form": "검정고시 가르쳤어요",
          "original_form": "검정고시 가르쳤어요",
          "speaker_id": "1",
          "time": "20210825 15:48"
        }
      ]
