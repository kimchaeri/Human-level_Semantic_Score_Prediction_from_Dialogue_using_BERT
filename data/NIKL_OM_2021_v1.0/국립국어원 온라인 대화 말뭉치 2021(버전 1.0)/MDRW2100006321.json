      "utterance": [
        {
          "id": "MDRW2100006321.1.1",
          "form": "해좀 쨍쨍했음 좋겠어요",
          "original_form": "해좀 쨍쨍했음 좋겠어요",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100006321.1.2",
          "form": "저도요 이러다 가을하늘도못보고 겨울올듯",
          "original_form": "저도요 이러다 가을하늘도못보고 겨울올듯",
          "speaker_id": "1",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100006321.1.3",
          "form": "헐~~ 가을장마때매 비만왔는데,..",
          "original_form": "헐~~ 가을장마때매 비만왔는데,..",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100006321.1.4",
          "form": "그러게요.  가을을 느끼고싶네요",
          "original_form": "그러게요.  가을을 느끼고싶네요",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100006321.1.5",
          "form": "날씨가 이상해져요 갈수록",
          "original_form": "날씨가 이상해져요 갈수록",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100006321.1.6",
          "form": "맞아요,",
          "original_form": "맞아요,",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100006321.1.7",
          "form": "단풍구경가시나요?",
          "original_form": "단풍구경가시나요?",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100006321.1.8",
          "form": "긴팔도 꺼내야되겠는데 옷도없고ㅎ",
          "original_form": "긴팔도 꺼내야되겠는데 옷도없고ㅎ",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100006321.1.9",
          "form": "저도 옷정리가 필요해요~",
          "original_form": "저도 옷정리가 필요해요~",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100006321.1.10",
          "form": "단풍은 그냥 출퇴근길에 있는거나 봐야될거같아요",
          "original_form": "단풍은 그냥 출퇴근길에 있는거나 봐야될거같아요",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100006321.1.11",
          "form": "코로나도 심각하고",
          "original_form": "코로나도 심각하고",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100006321.1.12",
          "form": "저도 올해도 가로수로 대신해야겠어요.",
          "original_form": "저도 올해도 가로수로 대신해야겠어요.",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100006321.1.13",
          "form": "어디가기 무섭네요",
          "original_form": "어디가기 무섭네요",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100006321.1.14",
          "form": "그니까요 점점 심해져서",
          "original_form": "그니까요 점점 심해져서",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006321.1.15",
          "form": "단풍구경은 가고싶긴하네요ㅎ",
          "original_form": "단풍구경은 가고싶긴하네요ㅎ",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006321.1.16",
          "form": "마스크벗고 일광욕이라도 했음 좋겠어요",
          "original_form": "마스크벗고 일광욕이라도 했음 좋겠어요",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006321.1.17",
          "form": "돗자리피고 놀고싶네요",
          "original_form": "돗자리피고 놀고싶네요",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006321.1.18",
          "form": "비오기전에 날씨 완전좋았는데",
          "original_form": "비오기전에 날씨 완전좋았는데",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006321.1.19",
          "form": "비가 넘 계속오네요",
          "original_form": "비가 넘 계속오네요",
          "speaker_id": "1",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100006321.1.20",
          "form": "맞아요. 그만와도 괜찮은데",
          "original_form": "맞아요. 그만와도 괜찮은데",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100006321.1.21",
          "form": "태풍오믄 더심할텐데 걱정이예요",
          "original_form": "태풍오믄 더심할텐데 걱정이예요",
