      "utterance": [
        {
          "id": "MDRW2100043761.1.1",
          "form": "혹시 몰라서 보내놔요",
          "original_form": "혹시 몰라서 보내놔요",
          "speaker_id": "1",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043761.1.2",
          "form": "센스",
          "original_form": "센스",
          "speaker_id": "2",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043761.1.3",
          "form": "ㅎㅎ 저도 까먹을때가 있어서요",
          "original_form": "ㅎㅎ 저도 까먹을때가 있어서요",
          "speaker_id": "1",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043761.1.4",
          "form": "산미구엘이라는 맥주 드셔보셨나요?",
          "original_form": "산미구엘이라는 맥주 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043761.1.5",
          "form": "아니요 처음들어봤어요 ㅎㅎ",
          "original_form": "아니요 처음들어봤어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043761.1.6",
          "form": "맛있어용??",
          "original_form": "맛있어용??",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043761.1.7",
          "form": "이것도 깔끔한 목넘김이 좋은 맛있는 맥주예요",
          "original_form": "이것도 깔끔한 목넘김이 좋은 맛있는 맥주예요",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043761.1.8",
          "form": "네네 맛있어욤",
          "original_form": "네네 맛있어욤",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043761.1.9",
          "form": "오 맛있구나 꼭 먹어볼게요",
          "original_form": "오 맛있구나 꼭 먹어볼게요",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043761.1.10",
          "form": "느끼한 음식이랑 먹으면 시원해서 좋아요",
          "original_form": "느끼한 음식이랑 먹으면 시원해서 좋아요",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043761.1.11",
          "form": "튀김같은거랑 먹으면 딱이겠네요",
          "original_form": "튀김같은거랑 먹으면 딱이겠네요",
          "speaker_id": "1",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043761.1.12",
          "form": "저 지금 딱 먹으면 좋을거 같아요",
          "original_form": "저 지금 딱 먹으면 좋을거 같아요",
          "speaker_id": "1",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043761.1.13",
          "form": "약간 느끼해서 ㅋㅋㅋㅋ",
          "original_form": "약간 느끼해서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100043761.1.14",
          "form": "그럴때있죠.. ㅜㅜ",
          "original_form": "그럴때있죠.. ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043761.1.15",
          "form": "넹 지금이에요ㅋㅋㅋㅋ",
          "original_form": "넹 지금이에요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043761.1.16",
          "form": "여름에 맥주 한잔 하고 그래야되는데",
          "original_form": "여름에 맥주 한잔 하고 그래야되는데",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043761.1.17",
          "form": "올해는 맥주를 잘 안마셨네요",
          "original_form": "올해는 맥주를 잘 안마셨네요",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043761.1.18",
          "form": "어째서요? 많이 더웠는데요..?",
          "original_form": "어째서요? 많이 더웠는데요..?",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100043761.1.19",
          "form": "그러게요 ㅋㅋㅋㅋ 밖에 잘 안나가서 그랬던거 같아요!!",
          "original_form": "그러게요 ㅋㅋㅋㅋ 밖에 잘 안나가서 그랬던거 같아요!!",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100043761.1.20",
          "form": "밖에 안나가니까 덜 먹긴해요ㅋㅋㅋㅋ",
          "original_form": "밖에 안나가니까 덜 먹긴해요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100043761.1.21",
          "form": "맞아요 확실히 전 줄었어요",
          "original_form": "맞아요 확실히 전 줄었어요",
