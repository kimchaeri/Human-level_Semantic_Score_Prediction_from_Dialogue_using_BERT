        {
          "id": "MDRW2100029789.1.2",
          "form": "쌀국수",
          "original_form": "쌀국수",
          "speaker_id": "2",
          "time": "20210903 13:36"
        },
        {
          "id": "MDRW2100029789.1.3",
          "form": "너는??",
          "original_form": "너는??",
          "speaker_id": "2",
          "time": "20210903 13:36"
        },
        {
          "id": "MDRW2100029789.1.4",
          "form": "뭐야 맛있겠다",
          "original_form": "뭐야 맛있겠다",
          "speaker_id": "1",
          "time": "20210903 13:37"
        },
        {
          "id": "MDRW2100029789.1.5",
          "form": "난 주먹밥먹음",
          "original_form": "난 주먹밥먹음",
          "speaker_id": "1",
          "time": "20210903 13:37"
        },
        {
          "id": "MDRW2100029789.1.6",
          "form": "사실 아직 안 먹었어 시킨거 기다리는 중이야",
          "original_form": "사실 아직 안 먹었어 시킨거 기다리는 중이야",
          "speaker_id": "2",
          "time": "20210903 13:43"
        },
        {
          "id": "MDRW2100029789.1.7",
          "form": "주먹밥 맛있겠다~!! 배고파",
          "original_form": "주먹밥 맛있겠다~!! 배고파",
          "speaker_id": "2",
          "time": "20210903 13:43"
        },
        {
          "id": "MDRW2100029789.1.8",
          "form": "쌀국수도 배달이 돼??",
          "original_form": "쌀국수도 배달이 돼??",
          "speaker_id": "1",
          "time": "20210903 13:44"
        },
        {
          "id": "MDRW2100029789.1.9",
          "form": "신기하다",
          "original_form": "신기하다",
          "speaker_id": "1",
          "time": "20210903 13:44"
        },
        {
          "id": "MDRW2100029789.1.10",
          "form": "난 먹긴했는데 배고픔",
          "original_form": "난 먹긴했는데 배고픔",
          "speaker_id": "1",
          "time": "20210903 13:44"
        },
        {
          "id": "MDRW2100029789.1.11",
          "form": "아니 밖에서 시키고 기다리는중 가져가야지",
          "original_form": "아니 밖에서 시키고 기다리는중 가져가야지",
          "speaker_id": "2",
          "time": "20210903 13:44"
        },
        {
          "id": "MDRW2100029789.1.12",
          "form": "사람이 많아서 오래걸려",
          "original_form": "사람이 많아서 오래걸려",
          "speaker_id": "2",
          "time": "20210903 13:44"
        },
        {
          "id": "MDRW2100029789.1.13",
          "form": "아ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 13:47"
        },
        {
          "id": "MDRW2100029789.1.14",
          "form": "쌀국수좋아해?",
          "original_form": "쌀국수좋아해?",
