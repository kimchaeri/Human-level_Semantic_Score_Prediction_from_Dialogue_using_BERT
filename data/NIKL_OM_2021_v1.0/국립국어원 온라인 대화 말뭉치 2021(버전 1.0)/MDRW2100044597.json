      "utterance": [
        {
          "id": "MDRW2100044597.1.1",
          "form": "그리고 난 계속 일하고 싶은데",
          "original_form": "그리고 난 계속 일하고 싶은데",
          "speaker_id": "1",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044597.1.2",
          "form": "주변에 결혼 한 사람들 거의 다 중도에 퇴사함",
          "original_form": "주변에 결혼 한 사람들 거의 다 중도에 퇴사함",
          "speaker_id": "1",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044597.1.3",
          "form": "결혼하려면 기준이 너무 높아...",
          "original_form": "결혼하려면 기준이 너무 높아...",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044597.1.4",
          "form": "ㅜ",
          "original_form": "ㅜ",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044597.1.5",
          "form": "맞아 공무원이나 좀 덜하지",
          "original_form": "맞아 공무원이나 좀 덜하지",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044597.1.6",
          "form": "사기업은 다 퇴사하는걸로 보더라",
          "original_form": "사기업은 다 퇴사하는걸로 보더라",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044597.1.7",
          "form": "ㅇㅇ 우리는 공기업인데도",
          "original_form": "ㅇㅇ 우리는 공기업인데도",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044597.1.8",
          "form": "애 초등학교 들어갈 때 쯤 되면",
          "original_form": "애 초등학교 들어갈 때 쯤 되면",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044597.1.9",
          "form": "육아휴직 쓰고",
          "original_form": "육아휴직 쓰고",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044597.1.10",
          "form": "(절래",
          "original_form": "(절래",
          "speaker_id": "2",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044597.1.11",
          "form": "승진과 영영 이별함...",
          "original_form": "승진과 영영 이별함...",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044597.1.12",
          "form": "ㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044597.1.13",
          "form": "아니 애가 학ㄱ교간다고 엄마가 필요하딘 않아",
          "original_form": "아니 애가 학ㄱ교간다고 엄마가 필요하딘 않아",
          "speaker_id": "2",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044597.1.14",
          "form": "오히려 덜 필요해지는거 아닌가",
          "original_form": "오히려 덜 필요해지는거 아닌가",
          "speaker_id": "2",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044597.1.15",
          "form": "요즘은 또 다른가?",
          "original_form": "요즘은 또 다른가?",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044597.1.16",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044597.1.17",
          "form": "왜냐면 애가 학교 막 들어가면",
          "original_form": "왜냐면 애가 학교 막 들어가면",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044597.1.18",
          "form": "하교 시간이 12시임....",
          "original_form": "하교 시간이 12시임....",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044597.1.19",
          "form": "근데 초등학생은 유치원이나 어린이집처럼 늦게까지 봐주는 데도 없어",
          "original_form": "근데 초등학생은 유치원이나 어린이집처럼 늦게까지 봐주는 데도 없어",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044597.1.20",
          "form": "ㅋ큐ㅠ",
          "original_form": "ㅋ큐ㅠ",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044597.1.21",
          "form": "나도 12시 하교했었는데 1학년...",
          "original_form": "나도 12시 하교했었는데 1학년...",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044597.1.22",
          "form": "흠",
          "original_form": "흠",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044597.1.23",
          "form": "얘들이랑 놀았나? 집에서 티비도 보고 그랬던거 같은데",
          "original_form": "얘들이랑 놀았나? 집에서 티비도 보고 그랬던거 같은데",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044597.1.24",
          "form": "학원가 칭구들(급기야다",
          "original_form": "학원가 칭구들(급기야다",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044597.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ ㅇㅇ 그래서 요즘 일하는 직원들 보면",
          "original_form": "ㅋㅋㅋㅋㅋㅋ ㅇㅇ 그래서 요즘 일하는 직원들 보면",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044597.1.26",
          "form": "애들 학원으로 돌리더라구",
          "original_form": "애들 학원으로 돌리더라구",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044597.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ 머 그거 아니더라도 요즘 칭구들 학원 아니면 애들 만날 곳이라도 있는지",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 머 그거 아니더라도 요즘 칭구들 학원 아니면 애들 만날 곳이라도 있는지",
          "speaker_id": "2",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044597.1.28",
          "form": "태권도가 그렇게 핫하다던데",
          "original_form": "태권도가 그렇게 핫하다던데",
          "speaker_id": "2",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044597.1.29",
          "form": "틔터에서 봤어",
          "original_form": "틔터에서 봤어",
          "speaker_id": "2",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044597.1.30",
          "form": "그치 학교가도 어차피 친구랑 못만나서...",
          "original_form": "그치 학교가도 어차피 친구랑 못만나서...",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044597.1.31",
          "form": "ㅇㅇ... 학원도 굳이 못 나가게 하면 못 만나지만 그래도 좀 느슨할테니까",
          "original_form": "ㅇㅇ... 학원도 굳이 못 나가게 하면 못 만나지만 그래도 좀 느슨할테니까",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044597.1.32",
          "form": "ㅇㅇ 학원강사도 먹고살아야지..",
          "original_form": "ㅇㅇ 학원강사도 먹고살아야지..",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044597.1.33",
          "form": "요즘 안그래도 줄어든 직업 많은데",
          "original_form": "요즘 안그래도 줄어든 직업 많은데",
          "speaker_id": "1",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044597.1.34",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
