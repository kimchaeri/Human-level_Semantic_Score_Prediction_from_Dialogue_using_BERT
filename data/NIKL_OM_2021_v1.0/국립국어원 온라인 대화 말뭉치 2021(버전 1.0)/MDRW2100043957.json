      "utterance": [
        {
          "id": "MDRW2100043957.1.1",
          "form": "헉 뭐라고?",
          "original_form": "헉 뭐라고?",
          "speaker_id": "2",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100043957.1.2",
          "form": "마지막거 휙 넘어가서 못 봤어",
          "original_form": "마지막거 휙 넘어가서 못 봤어",
          "speaker_id": "2",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100043957.1.3",
          "form": "설마 오징어땅콩인가",
          "original_form": "설마 오징어땅콩인가",
          "speaker_id": "1",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100043957.1.4",
          "form": "아!!!!!!우깡새니?!!!!",
          "original_form": "아!!!!!!우깡새니?!!!!",
          "speaker_id": "1",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100043957.1.5",
          "form": "아!!!!! 새우깡 정답정답정답",
          "original_form": "아!!!!! 새우깡 정답정답정답",
          "speaker_id": "2",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100043957.1.6",
          "form": "그것도 매운 새우깡 먹고싶다",
          "original_form": "그것도 매운 새우깡 먹고싶다",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043957.1.7",
          "form": "아싸아싸",
          "original_form": "아싸아싸",
          "speaker_id": "1",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043957.1.8",
          "form": "니가 나 새우깡 먹는 그림 그려줬던 거 기억나?",
          "original_form": "니가 나 새우깡 먹는 그림 그려줬던 거 기억나?",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043957.1.9",
          "form": "매운것도 맛있고",
          "original_form": "매운것도 맛있고",
          "speaker_id": "1",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043957.1.10",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043957.1.11",
          "form": "진짜? 하나도 기억아나",
          "original_form": "진짜? 하나도 기억아나",
          "speaker_id": "1",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043957.1.12",
          "form": "쌀맛도 맛있는데",
          "original_form": "쌀맛도 맛있는데",
          "speaker_id": "1",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043957.1.13",
          "form": "맞아 쌀새우깡도 있었어",
          "original_form": "맞아 쌀새우깡도 있었어",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043957.1.14",
          "form": "그거 두 개를 붙여팔지 않았냐",
          "original_form": "그거 두 개를 붙여팔지 않았냐",
          "speaker_id": "2",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100043957.1.15",
          "form": "그랬구나 인기가없었나",
          "original_form": "그랬구나 인기가없었나",
          "speaker_id": "1",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100043957.1.16",
          "form": "그러게말여 사라지고 이젠 안 파나봐",
          "original_form": "그러게말여 사라지고 이젠 안 파나봐",
          "speaker_id": "2",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100043957.1.17",
          "form": "맛있었는데 쩝",
          "original_form": "맛있었는데 쩝",
          "speaker_id": "2",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100043957.1.18",
          "form": "그림그린거 들고있니?",
          "original_form": "그림그린거 들고있니?",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100043957.1.19",
          "form": "궁금하다",
          "original_form": "궁금하다",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100043957.1.20",
          "form": "아마도..? 니 그림은 다 소중하게 가지고았는데",
          "original_form": "아마도..? 니 그림은 다 소중하게 가지고았는데",
          "speaker_id": "2",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100043957.1.21",
          "form": "쌀새우깡 역사속으로 사라졌나벼",
          "original_form": "쌀새우깡 역사속으로 사라졌나벼",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100043957.1.22",
          "form": "무수한 파일 중에 찾을 수 있으려나",
          "original_form": "무수한 파일 중에 찾을 수 있으려나",
          "speaker_id": "2",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100043957.1.23",
          "form": "ㅋㅋㅋㅋ내일 보여줄게",
          "original_form": "ㅋㅋㅋㅋ내일 보여줄게",
          "speaker_id": "2",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100043957.1.24",
          "form": "넌 잔치국수 좋아하니",
          "original_form": "넌 잔치국수 좋아하니",
          "speaker_id": "1",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100043957.1.25",
          "form": "응 잔치국수 좋아해",
          "original_form": "응 잔치국수 좋아해",
          "speaker_id": "2",
          "time": "20210907 21:07"
        }
      ]
