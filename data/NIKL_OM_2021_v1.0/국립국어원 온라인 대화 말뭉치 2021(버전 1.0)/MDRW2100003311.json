      "utterance": [
        {
          "id": "MDRW2100003311.1.1",
          "form": "10마넌 ㅋㅋㅋ 이이상 당근에선 비싸게주고사면안돼 ㅋㅋ",
          "original_form": "10마넌 ㅋㅋㅋ 이이상 당근에선 비싸게주고사면안돼 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 21:44"
        },
        {
          "id": "MDRW2100003311.1.2",
          "form": "넌 얼마까지 사고팔아봄",
          "original_form": "넌 얼마까지 사고팔아봄",
          "speaker_id": "2",
          "time": "20210820 21:44"
        },
        {
          "id": "MDRW2100003311.1.3",
          "form": "아 나는 내가 산 가격 그대로 12만에 팔아봄 ㅋㅋㅋ",
          "original_form": "아 나는 내가 산 가격 그대로 12만에 팔아봄 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 21:45"
        },
        {
          "id": "MDRW2100003311.1.4",
          "form": "쌔거",
          "original_form": "쌔거",
          "speaker_id": "1",
          "time": "20210820 21:45"
        },
        {
          "id": "MDRW2100003311.1.5",
          "form": "산거는 죄다 5만 밑이야",
          "original_form": "산거는 죄다 5만 밑이야",
          "speaker_id": "1",
          "time": "20210820 21:45"
        },
        {
          "id": "MDRW2100003311.1.6",
          "form": "ㅋㅋㅋ당근은 싸게사는맛이지",
          "original_form": "ㅋㅋㅋ당근은 싸게사는맛이지",
          "speaker_id": "2",
          "time": "20210820 21:46"
        },
        {
          "id": "MDRW2100003311.1.7",
          "form": "아 범퍼침대 새거 25마넌짜리 15마넌에 사봤다 ㅋㅋ",
          "original_form": "아 범퍼침대 새거 25마넌짜리 15마넌에 사봤다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 21:47"
        },
        {
          "id": "MDRW2100003311.1.8",
          "form": "당근이 꿀이야",
          "original_form": "당근이 꿀이야",
          "speaker_id": "2",
          "time": "20210820 21:47"
        },
        {
          "id": "MDRW2100003311.1.9",
          "form": "왜 일찍안했을까",
          "original_form": "왜 일찍안했을까",
          "speaker_id": "2",
          "time": "20210820 21:47"
        },
        {
          "id": "MDRW2100003311.1.10",
          "form": "잘 샀네 완전",
          "original_form": "잘 샀네 완전",
          "speaker_id": "1",
          "time": "20210820 21:48"
        },
        {
          "id": "MDRW2100003311.1.11",
          "form": "택배 보내고 뭐하고 안해서 좋은듯",
          "original_form": "택배 보내고 뭐하고 안해서 좋은듯",
          "speaker_id": "1",
          "time": "20210820 21:48"
        },
        {
          "id": "MDRW2100003311.1.12",
          "form": "바로 동네에서 거래 끝이잖아",
          "original_form": "바로 동네에서 거래 끝이잖아",
          "speaker_id": "1",
          "time": "20210820 21:49"
        },
        {
          "id": "MDRW2100003311.1.13",
          "form": "근데 난 보통 차타고다녀옴 ㅋㅋ 걸어서 갈 거리엔 원하는게 없더라",
          "original_form": "근데 난 보통 차타고다녀옴 ㅋㅋ 걸어서 갈 거리엔 원하는게 없더라",
          "speaker_id": "2",
          "time": "20210820 21:49"
        },
        {
          "id": "MDRW2100003311.1.14",
          "form": "요즘은 비대면 문고리거래 많이해서 더 편하기도함 ㅋㅋ",
          "original_form": "요즘은 비대면 문고리거래 많이해서 더 편하기도함 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 21:49"
        },
        {
          "id": "MDRW2100003311.1.15",
          "form": "비대면거래 신선하다 ㅋㅋㅋㅋ",
          "original_form": "비대면거래 신선하다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 21:50"
        },
        {
          "id": "MDRW2100003311.1.16",
          "form": "아 나 근데 당근 하다가.. 중학교 동창 만난적 있다",
          "original_form": "아 나 근데 당근 하다가.. 중학교 동창 만난적 있다",
          "speaker_id": "1",
          "time": "20210820 21:50"
        },
        {
          "id": "MDRW2100003311.1.17",
          "form": "아마 이체하면서 내 이름보고 알았을듯;; 나도 걔 얼굴하고 이름 보고 알았어",
          "original_form": "아마 이체하면서 내 이름보고 알았을듯;; 나도 걔 얼굴하고 이름 보고 알았어",
          "speaker_id": "1",
          "time": "20210820 21:51"
        },
        {
          "id": "MDRW2100003311.1.18",
          "form": "ㅋㅋㅋㅋ만나서 서로 알아봄?",
          "original_form": "ㅋㅋㅋㅋ만나서 서로 알아봄?",
          "speaker_id": "2",
          "time": "20210820 22:05"
        },
        {
          "id": "MDRW2100003311.1.19",
          "form": "인사했겠네 ㅋㅋ",
          "original_form": "인사했겠네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:05"
        },
        {
          "id": "MDRW2100003311.1.20",
          "form": "알아는 봤는데 인사는 안함",
          "original_form": "알아는 봤는데 인사는 안함",
          "speaker_id": "1",
          "time": "20210820 22:08"
        },
        {
          "id": "MDRW2100003311.1.21",
          "form": "어색하지...한 14년만인데",
          "original_form": "어색하지...한 14년만인데",
          "speaker_id": "1",
          "time": "20210820 22:08"
        },
        {
          "id": "MDRW2100003311.1.22",
          "form": "그냥 판매자와 구매자로 남음 ㅋㅋ",
          "original_form": "그냥 판매자와 구매자로 남음 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:08"
        },
        {
          "id": "MDRW2100003311.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ14년길지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ14년길지",
          "speaker_id": "2",
          "time": "20210820 22:17"
        },
        {
          "id": "MDRW2100003311.1.24",
          "form": "요즘 티몬 핫딜도 자주 애용중 ㅋㅋ",
          "original_form": "요즘 티몬 핫딜도 자주 애용중 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:18"
        },
        {
          "id": "MDRW2100003311.1.25",
          "form": "ㅋㅋㅋㅋ 식품?? 주로 뭐 사 ㅌㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 식품?? 주로 뭐 사 ㅌㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:19"
        },
        {
          "id": "MDRW2100003311.1.26",
          "form": "티몬은 아주 가끔 산다 ㅋㅋㅋ",
          "original_form": "티몬은 아주 가끔 산다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:20"
        },
        {
          "id": "MDRW2100003311.1.27",
          "form": "마라샹궈 마라탕 소스 티몬에서만 샀었는데 ㅋㅋㅋ",
          "original_form": "마라샹궈 마라탕 소스 티몬에서만 샀었는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:20"
        },
        {
          "id": "MDRW2100003311.1.28",
          "form": "분유병이랑 너트 ㅋㅋ",
          "original_form": "분유병이랑 너트 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:27"
        },
        {
          "id": "MDRW2100003311.1.29",
          "form": "걍 전부 애기용품이구나 ㅋㅋㅋㅋ",
          "original_form": "걍 전부 애기용품이구나 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:27"
        },
        {
          "id": "MDRW2100003311.1.30",
          "form": "그렇지 ㅋㅋㅋ 나한테 쓸건없다이제",
          "original_form": "그렇지 ㅋㅋㅋ 나한테 쓸건없다이제",
          "speaker_id": "2",
          "time": "20210820 22:28"
        },
        {
          "id": "MDRW2100003311.1.31",
          "form": "티몬 무배 금요일이던가",
          "original_form": "티몬 무배 금요일이던가",
          "speaker_id": "1",
          "time": "20210820 22:28"
        },
        {
          "id": "MDRW2100003311.1.32",
          "form": "아 그런게있음?",
          "original_form": "아 그런게있음?",
          "speaker_id": "2",
          "time": "20210820 22:28"
        },
        {
          "id": "MDRW2100003311.1.33",
