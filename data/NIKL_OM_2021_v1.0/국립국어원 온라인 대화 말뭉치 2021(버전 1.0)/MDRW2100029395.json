      "utterance": [
        {
          "id": "MDRW2100029395.1.1",
          "form": "좋아하는 계절 있나요?",
          "original_form": "좋아하는 계절 있나요?",
          "speaker_id": "1",
          "time": "20210903 06:21"
        },
        {
          "id": "MDRW2100029395.1.2",
