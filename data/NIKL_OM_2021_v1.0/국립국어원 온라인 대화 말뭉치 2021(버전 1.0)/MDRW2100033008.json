        },
        {
          "id": "MDRW2100033008.1.2",
          "form": "모가디슈 봤어요!!",
          "original_form": "모가디슈 봤어요!!",
          "speaker_id": "2",
          "time": "20210906 14:19"
        },
        {
          "id": "MDRW2100033008.1.3",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210906 18:11"
        },
        {
          "id": "MDRW2100033008.1.4",
          "form": "코로나 이후로 공연장 극장 하나도 안가게 되더라구요",
          "original_form": "코로나 이후로 공연장 극장 하나도 안가게 되더라구요",
          "speaker_id": "1",
          "time": "20210906 18:12"
        },
        {
          "id": "MDRW2100033008.1.5",
          "form": "한번도요?ㅋㅋ",
          "original_form": "한번도요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100033008.1.6",
          "form": "영화 재미있나요",
          "original_form": "영화 재미있나요",
          "speaker_id": "1",
          "time": "20210906 18:24"
        },
        {
          "id": "MDRW2100033008.1.7",
          "form": "모가디슈요? 재밌었어요",
          "original_form": "모가디슈요? 재밌었어요",
          "speaker_id": "2",
          "time": "20210906 20:35"
        },
        {
          "id": "MDRW2100033008.1.8",
          "form": "주인공이 누구인가요 ?",
          "original_form": "주인공이 누구인가요 ?",
          "speaker_id": "1",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100033008.1.9",
          "form": "김윤식 조인성 허준호?",
          "original_form": "김윤식 조인성 허준호?",
          "speaker_id": "2",
          "time": "20210906 20:59"
        },
        {
          "id": "MDRW2100033008.1.10",
          "form": "아 ! 조인성 나온 영화 예고는 봣어요",
          "original_form": "아 ! 조인성 나온 영화 예고는 봣어요",
          "speaker_id": "1",
          "time": "20210906 21:08"
        },
        {
          "id": "MDRW2100033008.1.11",
          "form": "네 ㅎㅎ 연기 굿",
          "original_form": "네 ㅎㅎ 연기 굿",
          "speaker_id": "2",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100033008.1.12",
          "form": "조연에 여자분 김재화씨도 연기 잘하죠",
          "original_form": "조연에 여자분 김재화씨도 연기 잘하죠",
          "speaker_id": "1",
          "time": "20210907 01:24"
        },
        {
          "id": "MDRW2100033008.1.13",
          "form": "네 맞아요 ㅎㅎ 잘하심!",
          "original_form": "네 맞아요 ㅎㅎ 잘하심!",
          "speaker_id": "2",
