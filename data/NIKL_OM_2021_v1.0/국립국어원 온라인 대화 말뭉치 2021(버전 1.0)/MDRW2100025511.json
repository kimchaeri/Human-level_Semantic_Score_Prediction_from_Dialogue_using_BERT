      "utterance": [
        {
          "id": "MDRW2100025511.1.1",
          "form": "안녕하세요~ 인터넷쇼핑 많이하시나요?",
          "original_form": "안녕하세요~ 인터넷쇼핑 많이하시나요?",
          "speaker_id": "1",
          "time": "20210901 12:24"
        },
        {
          "id": "MDRW2100025511.1.2",
          "form": "네 많이 하는편이에요 :)",
          "original_form": "네 많이 하는편이에요 :)",
          "speaker_id": "2",
          "time": "20210901 12:25"
        },
        {
          "id": "MDRW2100025511.1.3",
          "form": "저도 매일 뭘 사는거같아요ㅎㅎ 최근엔 뭘 사셨어요?",
          "original_form": "저도 매일 뭘 사는거같아요ㅎㅎ 최근엔 뭘 사셨어요?",
          "speaker_id": "1",
          "time": "20210901 12:26"
        },
        {
          "id": "MDRW2100025511.1.4",
          "form": "최근에는 화장품 많이산것같아요",
          "original_form": "최근에는 화장품 많이산것같아요",
          "speaker_id": "2",
          "time": "20210901 12:26"
        },
        {
          "id": "MDRW2100025511.1.5",
          "form": "그리고 날씨가 추워져서 가을 옷도 사구요",
          "original_form": "그리고 날씨가 추워져서 가을 옷도 사구요",
          "speaker_id": "2",
          "time": "20210901 12:26"
        },
        {
          "id": "MDRW2100025511.1.6",
          "form": "저도 화장품 사는거 좋아해요ㅎㅎ",
          "original_form": "저도 화장품 사는거 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 12:27"
        },
        {
          "id": "MDRW2100025511.1.7",
          "form": "벌써 가을옷도 사셨구나! 어떤거사셨어요?",
          "original_form": "벌써 가을옷도 사셨구나! 어떤거사셨어요?",
          "speaker_id": "1",
          "time": "20210901 12:27"
        },
        {
          "id": "MDRW2100025511.1.8",
          "form": "너무 추워서 얇은 가디건 샀어요",
          "original_form": "너무 추워서 얇은 가디건 샀어요",
          "speaker_id": "2",
          "time": "20210901 12:27"
        },
        {
          "id": "MDRW2100025511.1.9",
          "form": "갑자기 기온이 뚝떨어졌더라구요",
          "original_form": "갑자기 기온이 뚝떨어졌더라구요",
          "speaker_id": "2",
          "time": "20210901 12:27"
        },
        {
          "id": "MDRW2100025511.1.10",
          "form": "이제 금방 겨울옷도 사야할거 같아요",
          "original_form": "이제 금방 겨울옷도 사야할거 같아요",
          "speaker_id": "2",
          "time": "20210901 12:28"
        },
        {
          "id": "MDRW2100025511.1.11",
          "form": "그러게요ㅎㅎ 중고거래는 하는편이세요?",
          "original_form": "그러게요ㅎㅎ 중고거래는 하는편이세요?",
          "speaker_id": "1",
          "time": "20210901 12:30"
        },
        {
          "id": "MDRW2100025511.1.12",
          "form": "앗 저는 당근이나 중고는 자주 안하는것 같아요",
          "original_form": "앗 저는 당근이나 중고는 자주 안하는것 같아요",
          "speaker_id": "2",
          "time": "20210901 12:31"
        },
        {
          "id": "MDRW2100025511.1.13",
          "form": "ㅋㄱㄱ첫시도가 어려운것 같아요 한번도 못해봤어요",
          "original_form": "ㅋㄱㄱ첫시도가 어려운것 같아요 한번도 못해봤어요",
          "speaker_id": "2",
          "time": "20210901 12:31"
        },
        {
          "id": "MDRW2100025511.1.14",
          "form": "저도 그랬는데 당근생기고나서는 꽤 자주하는거같아요ㅎㅎ",
          "original_form": "저도 그랬는데 당근생기고나서는 꽤 자주하는거같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 12:32"
        },
        {
          "id": "MDRW2100025511.1.15",
          "form": "혹시 집에 뭔가 있으면 한번 해보세요ㅎㅎ",
          "original_form": "혹시 집에 뭔가 있으면 한번 해보세요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 12:32"
        },
        {
          "id": "MDRW2100025511.1.16",
          "form": "그럴까봐요",
          "original_form": "그럴까봐요",
          "speaker_id": "2",
          "time": "20210901 12:32"
        },
        {
          "id": "MDRW2100025511.1.17",
          "form": "저희언니는 자주하는거 같은데",
          "original_form": "저희언니는 자주하는거 같은데",
          "speaker_id": "2",
          "time": "20210901 12:32"
        },
        {
          "id": "MDRW2100025511.1.18",
          "form": "전 익숙하지가 않아서 그냥 사는거같아요",
          "original_form": "전 익숙하지가 않아서 그냥 사는거같아요",
          "speaker_id": "2",
          "time": "20210901 12:32"
        },
        {
          "id": "MDRW2100025511.1.19",
          "form": "아하ㅋㅋ 이게 한번 하면 재미가 들리더라구요ㅋㅋ",
          "original_form": "아하ㅋㅋ 이게 한번 하면 재미가 들리더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:33"
        },
        {
          "id": "MDRW2100025511.1.20",
          "form": "다들 그말씀하시더라구요 ㅋㅋㅋ",
          "original_form": "다들 그말씀하시더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:33"
        },
        {
          "id": "MDRW2100025511.1.21",
          "form": "저희사무실 사람들도 당근알람 자주 울려요 ㅎㅎ",
          "original_form": "저희사무실 사람들도 당근알람 자주 울려요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 12:33"
        },
        {
          "id": "MDRW2100025511.1.22",
          "form": "아 맞아요ㅎㅎ 길가다보면 길거리에서도 들리더라구요",
          "original_form": "아 맞아요ㅎㅎ 길가다보면 길거리에서도 들리더라구요",
          "speaker_id": "1",
          "time": "20210901 12:33"
        }
      ]
