      "utterance": [
        {
          "id": "MDRW2100027710.1.1",
          "form": "안녕하세요ㅎ",
          "original_form": "안녕하세요ㅎ",
          "speaker_id": "1",
          "time": "20210903 12:17"
        },
        {
          "id": "MDRW2100027710.1.2",
          "form": "게임많이 하시나요?ㅎ",
          "original_form": "게임많이 하시나요?ㅎ",
          "speaker_id": "1",
          "time": "20210903 12:17"
        },
        {
          "id": "MDRW2100027710.1.3",
          "form": "안녕하세요~ 많이 하진 않는데 좋아하는 편이에요ㅋㅋ",
          "original_form": "안녕하세요~ 많이 하진 않는데 좋아하는 편이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:23"
        },
        {
          "id": "MDRW2100027710.1.4",
          "form": "어떤게임하세요?",
          "original_form": "어떤게임하세요?",
          "speaker_id": "1",
          "time": "20210903 12:39"
        },
        {
          "id": "MDRW2100027710.1.5",
          "form": "요즘은 쿠키런킹덤 하고있어요!",
          "original_form": "요즘은 쿠키런킹덤 하고있어요!",
          "speaker_id": "2",
          "time": "20210903 14:18"
        },
        {
          "id": "MDRW2100027710.1.6",
          "form": "아ㅋㅋㅋ게임관련된 주식은 안하시나요?ㅋ",
          "original_form": "아ㅋㅋㅋ게임관련된 주식은 안하시나요?ㅋ",
          "speaker_id": "1",
          "time": "20210903 15:02"
        },
        {
          "id": "MDRW2100027710.1.7",
          "form": "ㄴㅋㅋㅋㅋㅋㅋ이번에 뎁스 많이 올랐죠..",
          "original_form": "ㄴㅋㅋㅋㅋㅋㅋ이번에 뎁스 많이 올랐죠..",
          "speaker_id": "2",
          "time": "20210903 15:02"
        },
        {
          "id": "MDRW2100027710.1.8",
          "form": "주식을 안해요ㅠㅠㅠ 아숩",
          "original_form": "주식을 안해요ㅠㅠㅠ 아숩",
          "speaker_id": "2",
          "time": "20210903 15:03"
        },
        {
          "id": "MDRW2100027710.1.9",
          "form": "관심가져보셔요ㅋ",
          "original_form": "관심가져보셔요ㅋ",
          "speaker_id": "1",
          "time": "20210903 15:05"
        },
        {
          "id": "MDRW2100027710.1.10",
          "form": "게임 주식으로 좀 버셨군요!",
          "original_form": "게임 주식으로 좀 버셨군요!",
          "speaker_id": "2",
          "time": "20210903 15:23"
        },
        {
          "id": "MDRW2100027710.1.11",
          "form": "아뇨..ㅈ",
          "original_form": "아뇨..ㅈ",
          "speaker_id": "1",
          "time": "20210903 16:08"
        },
        {
          "id": "MDRW2100027710.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㄱ이제막 관싯ㅇ가져볼까행ᆢ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱ이제막 관싯ㅇ가져볼까행ᆢ",
          "speaker_id": "1",
          "time": "20210903 16:08"
        },
        {
          "id": "MDRW2100027710.1.13",
          "form": "해요",
          "original_form": "해요",
          "speaker_id": "1",
          "time": "20210903 16:08"
        },
        {
          "id": "MDRW2100027710.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ뭐에요 안하시면서 추천하시고",
          "original_form": "ㅋㅋㅋㅋㅋㅋ뭐에요 안하시면서 추천하시고",
          "speaker_id": "2",
          "time": "20210903 16:12"
        },
        {
          "id": "MDRW2100027710.1.15",
          "form": "개미는 개미답게 살거에요..",
          "original_form": "개미는 개미답게 살거에요..",
          "speaker_id": "2",
          "time": "20210903 16:12"
        },
        {
          "id": "MDRW2100027710.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ하고는 있는데...파란불이라ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ하고는 있는데...파란불이라ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:22"
        },
        {
          "id": "MDRW2100027710.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ안돼요 그런 슬픈 일",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ안돼요 그런 슬픈 일",
          "speaker_id": "2",
          "time": "20210903 16:31"
        },
        {
          "id": "MDRW2100027710.1.18",
          "form": "주식하시는 분들 말 들어보니 다음주에 한번 폭락할거라구",
          "original_form": "주식하시는 분들 말 들어보니 다음주에 한번 폭락할거라구",
          "speaker_id": "2",
          "time": "20210903 16:31"
        },
        {
          "id": "MDRW2100027710.1.19",
          "form": "그러던데요 뭐래더라.. 선물옵션 뭐 끝나는 날이랬나",
          "original_form": "그러던데요 뭐래더라.. 선물옵션 뭐 끝나는 날이랬나",
          "speaker_id": "2",
          "time": "20210903 16:31"
        },
        {
          "id": "MDRW2100027710.1.20",
          "form": "ㅋㅋㅋㅋ더는ㅠ",
          "original_form": "ㅋㅋㅋㅋ더는ㅠ",
          "speaker_id": "1",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100027710.1.21",
          "form": "ㅋㅋㅋㅋ큐ㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100027710.1.22",
          "form": "오..오를거에요.. 빨개지는 그날까지 존버..!",
