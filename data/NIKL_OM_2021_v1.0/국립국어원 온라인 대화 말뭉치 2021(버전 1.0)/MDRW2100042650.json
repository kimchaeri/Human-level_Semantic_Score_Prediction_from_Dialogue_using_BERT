      "utterance": [
        {
          "id": "MDRW2100042650.1.1",
          "form": "그런건 제대로 하잖아",
          "original_form": "그런건 제대로 하잖아",
          "speaker_id": "2",
          "time": "20210907 00:24"
        },
        {
          "id": "MDRW2100042650.1.2",
          "form": "선까진 감는데 방향까지..ㅠㅠ",
          "original_form": "선까진 감는데 방향까지..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 00:24"
        },
        {
          "id": "MDRW2100042650.1.3",
          "form": "근데.. 니도 변기솔 일회용 써라 ㅋㅋㅋ\r\n딴거 몬쓰게",
          "original_form": "근데.. 니도 변기솔 일회용 써라 ㅋㅋㅋ\r\n딴거 몬쓰게",
          "speaker_id": "2",
          "time": "20210907 00:24"
        },
        {
          "id": "MDRW2100042650.1.4",
          "form": "아 몰라 진짜 짜증나 택배도 오늘 4갠가 와있었는데",
          "original_form": "아 몰라 진짜 짜증나 택배도 오늘 4갠가 와있었는데",
          "speaker_id": "1",
          "time": "20210907 00:24"
        },
        {
          "id": "MDRW2100042650.1.5",
          "form": "오자마자 짜증내면서",
          "original_form": "오자마자 짜증내면서",
          "speaker_id": "1",
          "time": "20210907 00:24"
        },
        {
          "id": "MDRW2100042650.1.6",
          "form": "욕하더라",
          "original_form": "욕하더라",
          "speaker_id": "1",
          "time": "20210907 00:25"
        },
        {
          "id": "MDRW2100042650.1.7",
          "form": "정신이 나갔다했나??",
          "original_form": "정신이 나갔다했나??",
          "speaker_id": "1",
          "time": "20210907 00:25"
        },
        {
          "id": "MDRW2100042650.1.8",
          "form": ".여튼 암말 안하고 다 버려주니깐 카면서",
          "original_form": ".여튼 암말 안하고 다 버려주니깐 카면서",
          "speaker_id": "1",
          "time": "20210907 00:26"
        },
        {
          "id": "MDRW2100042650.1.9",
          "form": "아니 일회용쓰는데",
          "original_form": "아니 일회용쓰는데",
          "speaker_id": "1",
          "time": "20210907 00:26"
        },
        {
          "id": "MDRW2100042650.1.10",
          "form": "그 사이가 잘안되서ㅠ 그거만 거기쓰거든 물내려오는 거기",
          "original_form": "그 사이가 잘안되서ㅠ 그거만 거기쓰거든 물내려오는 거기",
          "speaker_id": "1",
          "time": "20210907 00:26"
        },
        {
          "id": "MDRW2100042650.1.11",
          "form": "드라이기 때문에??",
          "original_form": "드라이기 때문에??",
          "speaker_id": "2",
          "time": "20210907 00:26"
        },
        {
          "id": "MDRW2100042650.1.12",
          "form": "곡선",
          "original_form": "곡선",
          "speaker_id": "1",
          "time": "20210907 00:27"
        },
        {
          "id": "MDRW2100042650.1.13",
          "form": "크린스틱인가 3엠꺼 써ㅠ",
          "original_form": "크린스틱인가 3엠꺼 써ㅠ",
          "speaker_id": "1",
          "time": "20210907 00:27"
        },
        {
          "id": "MDRW2100042650.1.14",
          "form": "집에 발 디디는순간부터 욕하고 들어옴",
          "original_form": "집에 발 디디는순간부터 욕하고 들어옴",
          "speaker_id": "1",
          "time": "20210907 00:27"
        },
        {
          "id": "MDRW2100042650.1.15",
          "form": "새거면 상관없자낭",
          "original_form": "새거면 상관없자낭",
          "speaker_id": "2",
          "time": "20210907 00:28"
        },
        {
          "id": "MDRW2100042650.1.16",
          "form": "아니. 크린스틱은 버리고...",
          "original_form": "아니. 크린스틱은 버리고...",
          "speaker_id": "1",
          "time": "20210907 00:28"
        },
        {
          "id": "MDRW2100042650.1.17",
          "form": "그 틈새솔은... 다회용이지",
          "original_form": "그 틈새솔은... 다회용이지",
          "speaker_id": "1",
          "time": "20210907 00:28"
        },
        {
          "id": "MDRW2100042650.1.18",
          "form": "뭐 말하는지 몰긋당",
          "original_form": "뭐 말하는지 몰긋당",
          "speaker_id": "2",
          "time": "20210907 00:31"
        },
        {
          "id": "MDRW2100042650.1.19",
          "form": "음 틈새는 어딘지 알겠지>",
          "original_form": "음 틈새는 어딘지 알겠지>",
          "speaker_id": "1",
          "time": "20210907 00:31"
        },
        {
          "id": "MDRW2100042650.1.20",
          "form": "아래에도 있는데 위에도 또 솔이있어서 그 틈새를 밀수있어",
          "original_form": "아래에도 있는데 위에도 또 솔이있어서 그 틈새를 밀수있어",
          "speaker_id": "1",
          "time": "20210907 00:32"
        },
        {
          "id": "MDRW2100042650.1.21",
          "form": "아.. 타일 사이사이??",
          "original_form": "아.. 타일 사이사이??",
          "speaker_id": "2",
          "time": "20210907 00:32"
        },
        {
          "id": "MDRW2100042650.1.22",
          "form": "변기솔...",
          "original_form": "변기솔...",
          "speaker_id": "1",
          "time": "20210907 00:33"
        },
        {
          "id": "MDRW2100042650.1.23",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 00:33"
        },
        {
          "id": "MDRW2100042650.1.24",
          "form": "물내려오는 틈새..ㅋㅋㅋ",
          "original_form": "물내려오는 틈새..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 00:33"
        },
        {
          "id": "MDRW2100042650.1.25",
          "form": "아.. 물나오는곳??",
          "original_form": "아.. 물나오는곳??",
          "speaker_id": "2",
          "time": "20210907 00:33"
        },
        {
          "id": "MDRW2100042650.1.26",
          "form": "응 거기 일회용은 거기가 잘 안되ㅠ",
          "original_form": "응 거기 일회용은 거기가 잘 안되ㅠ",
          "speaker_id": "1",
          "time": "20210907 00:34"
        },
        {
          "id": "MDRW2100042650.1.27",
          "form": "그래서 솔로 문대주는데ㅠ",
          "original_form": "그래서 솔로 문대주는데ㅠ",
          "speaker_id": "1",
          "time": "20210907 00:34"
        },
        {
          "id": "MDRW2100042650.1.28",
          "form": "더모아 휴대폰요금 냄ㅋ",
          "original_form": "더모아 휴대폰요금 냄ㅋ",
          "speaker_id": "1",
          "time": "20210907 00:34"
        },
        {
          "id": "MDRW2100042650.1.29",
          "form": "난 접때 말한 홈쇼핑 그거 쓰는데",
          "original_form": "난 접때 말한 홈쇼핑 그거 쓰는데",
          "speaker_id": "2",
          "time": "20210907 00:36"
        },
        {
          "id": "MDRW2100042650.1.30",
          "form": "근깐 그런것도 그 구멍은 잘 안 딱이자나ㅠㅠ",
          "original_form": "근깐 그런것도 그 구멍은 잘 안 딱이자나ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 00:37"
        },
        {
          "id": "MDRW2100042650.1.31",
          "form": "물 나오는 그 구멍을 솔로 문질러줘야 깨끗하지",
          "original_form": "물 나오는 그 구멍을 솔로 문질러줘야 깨끗하지",
          "speaker_id": "1",
          "time": "20210907 00:37"
        },
        {
          "id": "MDRW2100042650.1.32",
          "form": "그래서 변기에 넣고 요래요래 움직여서 반 딱 나누면 기역자 되자나 ㅋㅋ\r\n막대기 힘줘서 막 문지르면,, 시꺼먼것도 나오고 쉬 묻어있던것도 노란거소 닦이고 하던데 \r\n한번씩 수세미로 손넣어서 닦구",
          "original_form": "그래서 변기에 넣고 요래요래 움직여서 반 딱 나누면 기역자 되자나 ㅋㅋ\r\n막대기 힘줘서 막 문지르면,, 시꺼먼것도 나오고 쉬 묻어있던것도 노란거소 닦이고 하던데 \r\n한번씩 수세미로 손넣어서 닦구",
          "speaker_id": "2",
          "time": "20210907 00:38"
        }
      ]
