      "utterance": [
        {
          "id": "MDRW2100031581.1.1",
          "form": "백신 예약은 하셨나요",
          "original_form": "백신 예약은 하셨나요",
          "speaker_id": "1",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100031581.1.2",
          "form": "저 백신 이미 맞아서 ㅎㅎ",
          "original_form": "저 백신 이미 맞아서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 00:14"
        },
        {
          "id": "MDRW2100031581.1.3",
          "form": "얀센  맞았어요",
          "original_form": "얀센  맞았어요",
          "speaker_id": "2",
          "time": "20210905 00:14"
        },
        {
          "id": "MDRW2100031581.1.4",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "1",
          "time": "20210905 00:24"
        },
        {
          "id": "MDRW2100031581.1.5",
          "form": "아프진 않았나요",
          "original_form": "아프진 않았나요",
          "speaker_id": "1",
          "time": "20210905 00:25"
        },
        {
          "id": "MDRW2100031581.1.6",
          "form": "네 살짝 몸살?",
          "original_form": "네 살짝 몸살?",
          "speaker_id": "2",
          "time": "20210905 00:27"
        },
        {
          "id": "MDRW2100031581.1.7",
          "form": "이제 백신 맞으시나ᆢ",
          "original_form": "이제 백신 맞으시나ᆢ",
          "speaker_id": "2",
          "time": "20210905 00:27"
        },
        {
          "id": "MDRW2100031581.1.8",
          "form": "요",
          "original_form": "요",
          "speaker_id": "2",
          "time": "20210905 00:27"
        },
        {
          "id": "MDRW2100031581.1.9",
          "form": "네 화요일날 맞아요",
          "original_form": "네 화요일날 맞아요",
          "speaker_id": "1",
          "time": "20210905 08:24"
        },
        {
          "id": "MDRW2100031581.1.10",
          "form": "좀 걱정이되네요",
          "original_form": "좀 걱정이되네요",
          "speaker_id": "1",
          "time": "20210905 08:24"
        },
        {
          "id": "MDRW2100031581.1.11",
          "form": "괜찮으실거에요",
          "original_form": "괜찮으실거에요",
          "speaker_id": "2",
          "time": "20210905 08:27"
        },
        {
          "id": "MDRW2100031581.1.12",
          "form": "타이레놀 준비해두시고요",
          "original_form": "타이레놀 준비해두시고요",
          "speaker_id": "2",
          "time": "20210905 08:27"
        },
        {
          "id": "MDRW2100031581.1.13",
          "form": "네 사놓긴 했죠",
          "original_form": "네 사놓긴 했죠",
          "speaker_id": "1",
          "time": "20210905 08:27"
        },
        {
          "id": "MDRW2100031581.1.14",
          "form": "괜찮겠죠",
          "original_form": "괜찮겠죠",
          "speaker_id": "1",
          "time": "20210905 08:28"
        },
        {
          "id": "MDRW2100031581.1.15",
          "form": "네 문제 없으실걸요",
          "original_form": "네 문제 없으실걸요",
          "speaker_id": "2",
          "time": "20210905 08:28"
        },
        {
          "id": "MDRW2100031581.1.16",
          "form": "독감주사도 부작용이 늘 있다고 하죠 \n코로나도 아주 적게 있는 부작용 이겠죠",
          "original_form": "독감주사도 부작용이 늘 있다고 하죠 \n코로나도 아주 적게 있는 부작용 이겠죠",
          "speaker_id": "1",
          "time": "20210905 08:29"
        },
        {
          "id": "MDRW2100031581.1.17",
          "form": "네 일부인데 뉴스 나오니까",
          "original_form": "네 일부인데 뉴스 나오니까",
          "speaker_id": "2",
          "time": "20210905 09:21"
        },
        {
          "id": "MDRW2100031581.1.18",
          "form": "무섭게 느껴지는 거일거에요",
          "original_form": "무섭게 느껴지는 거일거에요",
          "speaker_id": "2",
          "time": "20210905 09:21"
        },
        {
          "id": "MDRW2100031581.1.19",
          "form": "뉴스에서 많이 나오니깐 더 그럴수도 있죠",
          "original_form": "뉴스에서 많이 나오니깐 더 그럴수도 있죠",
          "speaker_id": "1",
          "time": "20210905 09:56"
        },
        {
          "id": "MDRW2100031581.1.20",
          "form": "2차는 언제 맞으시는 건가요",
          "original_form": "2차는 언제 맞으시는 건가요",
          "speaker_id": "1",
          "time": "20210905 09:56"
        },
        {
          "id": "MDRW2100031581.1.21",
          "form": "아 얀센은 한번에 끝입니다 ㅎㅎ",
          "original_form": "아 얀센은 한번에 끝입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 10:05"
        },
        {
