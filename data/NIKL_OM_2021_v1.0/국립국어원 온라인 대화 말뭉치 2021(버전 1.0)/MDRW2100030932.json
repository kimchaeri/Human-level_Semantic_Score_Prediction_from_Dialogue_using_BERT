      "utterance": [
        {
          "id": "MDRW2100030932.1.1",
          "form": "집안일 하시나요",
          "original_form": "집안일 하시나요",
          "speaker_id": "1",
          "time": "20210904 14:25"
        },
        {
          "id": "MDRW2100030932.1.2",
          "form": "네 집안일해요ㅋㅋ하세요?",
          "original_form": "네 집안일해요ㅋㅋ하세요?",
          "speaker_id": "2",
          "time": "20210904 14:26"
        },
        {
          "id": "MDRW2100030932.1.3",
          "form": "네 하지요 ~!",
          "original_form": "네 하지요 ~!",
          "speaker_id": "1",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100030932.1.4",
          "form": "네 저두요",
          "original_form": "네 저두요",
          "speaker_id": "2",
          "time": "20210904 15:00"
        },
        {
          "id": "MDRW2100030932.1.5",
          "form": "일상입니다ㅋㅋ",
          "original_form": "일상입니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:00"
        },
        {
          "id": "MDRW2100030932.1.6",
          "form": "어떤게 제일 싫으신가요",
          "original_form": "어떤게 제일 싫으신가요",
          "speaker_id": "1",
          "time": "20210904 15:37"
        },
        {
          "id": "MDRW2100030932.1.7",
          "form": "저는 다 싫은데ㅋㅋ",
          "original_form": "저는 다 싫은데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:06"
        },
        {
          "id": "MDRW2100030932.1.8",
          "form": "쓰레기분리수거랑 화장실청소요",
          "original_form": "쓰레기분리수거랑 화장실청소요",
          "speaker_id": "2",
          "time": "20210904 16:06"
        },
        {
          "id": "MDRW2100030932.1.9",
          "form": "오 화장실 청소는 제가 전담하는거네요",
          "original_form": "오 화장실 청소는 제가 전담하는거네요",
          "speaker_id": "1",
          "time": "20210904 16:15"
        },
        {
          "id": "MDRW2100030932.1.10",
          "form": "네ㅋㅋ어떤게 싫으세요?",
          "original_form": "네ㅋㅋ어떤게 싫으세요?",
          "speaker_id": "2",
          "time": "20210905 10:28"
        },
        {
          "id": "MDRW2100030932.1.11",
          "form": "화장실 청소 하기싫죠.. 해야하는거니 어쩔수없이...",
          "original_form": "화장실 청소 하기싫죠.. 해야하는거니 어쩔수없이...",
          "speaker_id": "1",
          "time": "20210905 13:34"
        },
        {
          "id": "MDRW2100030932.1.12",
          "form": "맞아요. 해야되니 하는데 너무 하기싫어요ㅋㅋㅋ",
          "original_form": "맞아요. 해야되니 하는데 너무 하기싫어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:56"
        },
        {
          "id": "MDRW2100030932.1.13",
          "form": "그렇죠 가사분담은 어떻게 하시나요?",
          "original_form": "그렇죠 가사분담은 어떻게 하시나요?",
          "speaker_id": "1",
          "time": "20210905 14:03"
        },
        {
          "id": "MDRW2100030932.1.14",
          "form": "거의다 제가 하구요ㅋㅋ",
          "original_form": "거의다 제가 하구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 17:20"
        },
        {
          "id": "MDRW2100030932.1.15",
          "form": "신랑이",
          "original_form": "신랑이",
          "speaker_id": "2",
          "time": "20210905 17:20"
        },
        {
          "id": "MDRW2100030932.1.16",
          "form": "화장실청소해요ㅋㅋ",
          "original_form": "화장실청소해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 17:20"
        },
        {
          "id": "MDRW2100030932.1.17",
          "form": "비슷하네요..",
          "original_form": "비슷하네요..",
