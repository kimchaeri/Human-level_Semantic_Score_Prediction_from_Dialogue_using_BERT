      "utterance": [
        {
          "id": "MDRW2100030244.1.1",
          "form": "멍게쟁이라뇨! 그럼 님은 소쟁이인가욧!",
          "original_form": "멍게쟁이라뇨! 그럼 님은 소쟁이인가욧!",
          "speaker_id": "2",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100030244.1.2",
          "form": "멍게 닮으셨나요",
          "original_form": "멍게 닮으셨나요",
          "speaker_id": "1",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100030244.1.3",
          "form": "닮고 싶네요",
          "original_form": "닮고 싶네요",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030244.1.4",
          "form": "그 오묘한 매력!!",
          "original_form": "그 오묘한 매력!!",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030244.1.5",
          "form": "님은 소 닮으셨나요",
          "original_form": "님은 소 닮으셨나요",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100030244.1.6",
          "form": "멍게가",
          "original_form": "멍게가",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030244.1.7",
          "form": "좀 매력 있는듯요",
          "original_form": "좀 매력 있는듯요",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030244.1.8",
          "form": "차도녀니까",
          "original_form": "차도녀니까",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030244.1.9",
          "form": "비단멍게",
          "original_form": "비단멍게",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030244.1.10",
          "form": "해드릴게요",
          "original_form": "해드릴게요",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030244.1.11",
          "form": "노노",
          "original_form": "노노",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030244.1.12",
          "form": "저는 고오오급이니 돌멍게로 해주세요",
          "original_form": "저는 고오오급이니 돌멍게로 해주세요",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030244.1.13",
          "form": "내실이 중요하쥬",
          "original_form": "내실이 중요하쥬",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030244.1.14",
          "form": "네 돌멍게역장님!",
          "original_form": "네 돌멍게역장님!",
          "speaker_id": "1",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100030244.1.15",
          "form": "라이언 멍게버전은 안 나올까요",
          "original_form": "라이언 멍게버전은 안 나올까요",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030244.1.16",
          "form": "님이 멍게 든 꼴을 봐야 하는데",
          "original_form": "님이 멍게 든 꼴을 봐야 하는데",
          "speaker_id": "2",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100030244.1.17",
          "form": "저는 멍게가 아닌데요 선생님",
          "original_form": "저는 멍게가 아닌데요 선생님",
          "speaker_id": "1",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030244.1.18",
          "form": "~-~",
          "original_form": "~-~",
          "speaker_id": "1",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030244.1.19",
          "form": "멍게만 멍게 들란 법 있남요",
          "original_form": "멍게만 멍게 들란 법 있남요",
          "speaker_id": "2",
          "time": "20210903 23:23"
        },
        {
          "id": "MDRW2100030244.1.20",
          "form": "님도 언젠가 멍게의 매력에서 헤어나오지 못하게 될거임",
          "original_form": "님도 언젠가 멍게의 매력에서 헤어나오지 못하게 될거임",
          "speaker_id": "2",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030244.1.21",
          "form": "아 제가 돌멍게님을 드는건가요",
          "original_form": "아 제가 돌멍게님을 드는건가요",
          "speaker_id": "1",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030244.1.22",
          "form": "상당히 힘들겠군요",
          "original_form": "상당히 힘들겠군요",
          "speaker_id": "1",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030244.1.23",
          "form": "멍게소주의 매력을 모르는 name1님이 불쌍해ㅜㅇ ㅜ",
          "original_form": "멍게소주의 매력을 모르는 &name1&님이 불쌍해ㅜㅇ ㅜ",
          "speaker_id": "2",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030244.1.24",
          "form": "어 알아요",
          "original_form": "어 알아요",
          "speaker_id": "1",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030244.1.25",
          "form": "그거 파내고서",
          "original_form": "그거 파내고서",
          "speaker_id": "1",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100030244.1.26",
          "form": "아앗",
          "original_form": "아앗",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030244.1.27",
          "form": "제가 그정도는 아니구",
          "original_form": "제가 그정도는 아니구",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030244.1.28",
          "form": "그냥  멍게랑 소주 먹는 겁니다ㅎ",
          "original_form": "그냥  멍게랑 소주 먹는 겁니다ㅎ",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100030244.1.29",
          "form": "초보시네",
          "original_form": "초보시네",
          "speaker_id": "1",
