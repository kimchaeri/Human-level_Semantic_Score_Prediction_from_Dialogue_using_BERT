      "utterance": [
        {
          "id": "MDRW2100031631.1.1",
          "form": "푸르지오 근처에는 닭집이 굉장히 많습니다",
          "original_form": "푸르지오 근처에는 닭집이 굉장히 많습니다",
          "speaker_id": "2",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100031631.1.2",
          "form": "처갓집 또 있구요",
          "original_form": "처갓집 또 있구요",
          "speaker_id": "2",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100031631.1.3",
          "form": "제라진통닭 같이 좀 오래된 통닭 집이에요",
          "original_form": "제라진통닭 같이 좀 오래된 통닭 집이에요",
          "speaker_id": "1",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100031631.1.4",
          "form": "옛날통닭이라고 보시면 됩니다",
          "original_form": "옛날통닭이라고 보시면 됩니다",
          "speaker_id": "1",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100031631.1.5",
          "form": "동키치킨도 있고요",
          "original_form": "동키치킨도 있고요",
          "speaker_id": "2",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100031631.1.6",
          "form": "동키치킨 오랜만이네요",
          "original_form": "동키치킨 오랜만이네요",
          "speaker_id": "1",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100031631.1.7",
          "form": "이름이 특이하네요 제라진통닭",
          "original_form": "이름이 특이하네요 제라진통닭",
          "speaker_id": "2",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100031631.1.8",
          "form": "참 좋아했었는데",
          "original_form": "참 좋아했었는데",
          "speaker_id": "1",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100031631.1.9",
          "form": "제주 어가 들어간 가게가 무척 많습니다",
          "original_form": "제주 어가 들어간 가게가 무척 많습니다",
          "speaker_id": "1",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100031631.1.10",
          "form": "동키치킨에서 목만 튀겨서 파는 넥슬라이스라는 메뉴가 있는데요",
          "original_form": "동키치킨에서 목만 튀겨서 파는 넥슬라이스라는 메뉴가 있는데요",
          "speaker_id": "2",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100031631.1.11",
          "form": "슬라이스 잔인 한대요",
          "original_form": "슬라이스 잔인 한대요",
          "speaker_id": "1",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100031631.1.12",
          "form": "가격도 저렴하고 맥주랑 먹으면 딱이에요 정말 딱 술 안주입니다",
          "original_form": "가격도 저렴하고 맥주랑 먹으면 딱이에요 정말 딱 술 안주입니다",
          "speaker_id": "2",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100031631.1.13",
          "form": "처갓집은 슈프림이 맛있는 것 같아요",
          "original_form": "처갓집은 슈프림이 맛있는 것 같아요",
          "speaker_id": "2",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100031631.1.14",
          "form": "넥슬라이스",
          "original_form": "넥슬라이스",
          "speaker_id": "2",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100031631.1.15",
          "form": "묵 튀김 궁금하긴 하네요",
          "original_form": "묵 튀김 궁금하긴 하네요",
          "speaker_id": "1",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100031631.1.16",
          "form": "슈프림 무슨 맛이죠",
          "original_form": "슈프림 무슨 맛이죠",
          "speaker_id": "1",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100031631.1.17",
          "form": "하지만 페리카나에서는 목살로 만든 목살로 튀긴 메뉴가 새로 나왔는데 곧을 먹이라고 하는데 정말 더 맛있고 인기 많은 것 같아요",
          "original_form": "하지만 페리카나에서는 목살로 만든 목살로 튀긴 메뉴가 새로 나왔는데 곧을 먹이라고 하는데 정말 더 맛있고 인기 많은 것 같아요",
          "speaker_id": "2",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100031631.1.18",
          "form": "양념과 비슷한데 양념 보다 조금 더 뭔가가 들어가서 맛있는 맛이예요",
          "original_form": "양념과 비슷한데 양념 보다 조금 더 뭔가가 들어가서 맛있는 맛이예요",
          "speaker_id": "2",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100031631.1.19",
          "form": "목살은 먹을게 없는 줄 알았어요",
          "original_form": "목살은 먹을게 없는 줄 알았어요",
          "speaker_id": "1",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100031631.1.20",
          "form": "처갓집에서는 슈프림이 제일 맛있는 것 같아요",
          "original_form": "처갓집에서는 슈프림이 제일 맛있는 것 같아요",
          "speaker_id": "2",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100031631.1.21",
          "form": "돼지 목살은 먹을게 많지만",
          "original_form": "돼지 목살은 먹을게 많지만",
          "speaker_id": "1",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100031631.1.22",
          "form": "자담치킨도 있고 쌀통닭 또 있어요",
          "original_form": "자담치킨도 있고 쌀통닭 또 있어요",
          "speaker_id": "2",
          "time": "20210904 23:17"
        }
      ]
    }
