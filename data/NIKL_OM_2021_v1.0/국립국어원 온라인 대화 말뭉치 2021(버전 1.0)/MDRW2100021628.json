      "utterance": [
        {
          "id": "MDRW2100021628.1.1",
          "form": "목요닐부터",
          "original_form": "목요닐부터",
          "speaker_id": "2",
          "time": "20210828 21:05"
        },
        {
          "id": "MDRW2100021628.1.2",
          "form": "주말인가요?",
          "original_form": "주말인가요?",
          "speaker_id": "2",
          "time": "20210828 21:05"
        },
        {
          "id": "MDRW2100021628.1.3",
          "form": "ㅋㅋㅋ목욜부터 안했나유 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ목욜부터 안했나유 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:05"
        },
        {
          "id": "MDRW2100021628.1.4",
          "form": "그츄 목요일이 막창먹은걸유",
          "original_form": "그츄 목요일이 막창먹은걸유",
          "speaker_id": "2",
          "time": "20210828 21:06"
        },
        {
          "id": "MDRW2100021628.1.5",
          "form": "오늘 놀면뭐하니 봤어?ㅋㅋ",
          "original_form": "오늘 놀면뭐하니 봤어?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:07"
        },
        {
          "id": "MDRW2100021628.1.6",
          "form": "노노!!! 왜요!?",
          "original_form": "노노!!! 왜요!?",
          "speaker_id": "2",
          "time": "20210828 21:13"
        },
        {
          "id": "MDRW2100021628.1.7",
          "form": "나도 뒷부분만 봤는데 유재석 갑자기 데려가서 뉴스 생방이라면서 시키는데 엄청 잘하더라 ㅋㅋㅋ",
          "original_form": "나도 뒷부분만 봤는데 유재석 갑자기 데려가서 뉴스 생방이라면서 시키는데 엄청 잘하더라 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:15"
        },
        {
          "id": "MDRW2100021628.1.8",
          "form": "오 역시 진짜 못하는ㄱㅔ 없는사람인가봐",
          "original_form": "오 역시 진짜 못하는ㄱㅔ 없는사람인가봐",
          "speaker_id": "2",
          "time": "20210828 21:17"
        },
        {
          "id": "MDRW2100021628.1.9",
          "form": "그니깐 ㅋㅋㅋ진짜 잘하더라 ㅋㅋㅋ 담주에 하하나오는데 하하 울기일보직전ㅋㅋㅋ",
          "original_form": "그니깐 ㅋㅋㅋ진짜 잘하더라 ㅋㅋㅋ 담주에 하하나오는데 하하 울기일보직전ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:22"
        },
        {
          "id": "MDRW2100021628.1.10",
          "form": "아 저번주에 정준하도나오고 광희랑 무한도전어쩌고 하더만",
          "original_form": "아 저번주에 정준하도나오고 광희랑 무한도전어쩌고 하더만",
          "speaker_id": "2",
          "time": "20210828 21:22"
        },
        {
          "id": "MDRW2100021628.1.11",
          "form": "ㅋㅋㅋ응 완전 무한도전같음ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ응 완전 무한도전같음ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100021628.1.12",
          "form": "원래무항고전봣나요?",
          "original_form": "원래무항고전봣나요?",
          "speaker_id": "2",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100021628.1.13",
          "form": "매번 본 것은 아니지만 보긴했쥬 ㅋㅋ",
          "original_form": "매번 본 것은 아니지만 보긴했쥬 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100021628.1.14",
          "form": "갯마을차차차하넹",
          "original_form": "갯마을차차차하넹",
          "speaker_id": "1",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100021628.1.15",
          "form": "오늘첫방이야?",
          "original_form": "오늘첫방이야?",
          "speaker_id": "2",
          "time": "20210828 21:53"
        },
        {
          "id": "MDRW2100021628.1.16",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210828 21:53"
        },
        {
          "id": "MDRW2100021628.1.17",
          "form": "응ㅋㅋㅋ재밌당",
          "original_form": "응ㅋㅋㅋ재밌당",
          "speaker_id": "1",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100021628.1.18",
          "form": "흠 내일오빠랑봐야지 ㅋㅋㅋ",
          "original_form": "흠 내일오빠랑봐야지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100021628.1.19",
          "form": "신민아 넘 예쁘다 ㅋㅋ",
          "original_form": "신민아 넘 예쁘다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:08"
        },
        {
          "id": "MDRW2100021628.1.20",
          "form": "ㅠㅡㅠ ㅋㅋㅋㅋ맞아 김선호도 얼마나 능글능글연기잘할꼬",
          "original_form": "ㅠㅡㅠ ㅋㅋㅋㅋ맞아 김선호도 얼마나 능글능글연기잘할꼬",
          "speaker_id": "2",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100021628.1.21",
          "form": "진짜 잘하네 ㅋㅋㅋ 잘생기기더 참 잘생겼어",
          "original_form": "진짜 잘하네 ㅋㅋㅋ 잘생기기더 참 잘생겼어",
          "speaker_id": "1",
