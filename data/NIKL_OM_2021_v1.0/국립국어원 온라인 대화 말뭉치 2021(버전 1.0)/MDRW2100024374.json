      "utterance": [
        {
          "id": "MDRW2100024374.1.1",
          "form": "폴드종이 그",
          "original_form": "폴드종이 그",
          "speaker_id": "1",
          "time": "20210831 02:51"
        },
        {
          "id": "MDRW2100024374.1.2",
          "form": "장화신은 고양이 맞죠?",
          "original_form": "장화신은 고양이 맞죠?",
          "speaker_id": "1",
          "time": "20210831 02:51"
        },
        {
          "id": "MDRW2100024374.1.3",
          "form": "네..맞네요",
          "original_form": "네..맞네요",
          "speaker_id": "2",
          "time": "20210831 02:52"
        },
        {
          "id": "MDRW2100024374.1.4",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "2",
          "time": "20210831 02:52"
        },
        {
          "id": "MDRW2100024374.1.5",
          "form": "처음알고가네요 ㅠㅠ",
          "original_form": "처음알고가네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 02:52"
        },
        {
          "id": "MDRW2100024374.1.6",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 02:53"
        },
        {
          "id": "MDRW2100024374.1.7",
          "form": "정말 한쪽은",
          "original_form": "정말 한쪽은",
          "speaker_id": "1",
          "time": "20210831 02:53"
        },
        {
          "id": "MDRW2100024374.1.8",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210831 02:53"
        },
        {
          "id": "MDRW2100024374.1.9",
          "form": "유기견 유기묘들 데려다 키우고",
          "original_form": "유기견 유기묘들 데려다 키우고",
          "speaker_id": "1",
          "time": "20210831 02:53"
        },
        {
          "id": "MDRW2100024374.1.10",
          "form": "ㅠㅜ",
          "original_form": "ㅠㅜ",
          "speaker_id": "2",
          "time": "20210831 02:53"
        },
        {
          "id": "MDRW2100024374.1.11",
          "form": "한쪽은 펫샵에서 사다 키우고..",
          "original_form": "한쪽은 펫샵에서 사다 키우고..",
          "speaker_id": "1",
          "time": "20210831 02:53"
        },
        {
          "id": "MDRW2100024374.1.12",
          "form": "ㅠㅠ 그쵸..",
          "original_form": "ㅠㅠ 그쵸..",
          "speaker_id": "2",
          "time": "20210831 02:53"
        },
        {
          "id": "MDRW2100024374.1.13",
          "form": "ㅠㅠㅠㅠ 펫샵 정말 사라져야할것같아요",
          "original_form": "ㅠㅠㅠㅠ 펫샵 정말 사라져야할것같아요",
          "speaker_id": "1",
          "time": "20210831 02:53"
        },
        {
          "id": "MDRW2100024374.1.14",
          "form": "동물 입양할 때 시험도 보고!!",
          "original_form": "동물 입양할 때 시험도 보고!!",
          "speaker_id": "1",
          "time": "20210831 02:53"
        },
        {
          "id": "MDRW2100024374.1.15",
          "form": "ㅜㅜ 그러개요..",
          "original_form": "ㅜㅜ 그러개요..",
          "speaker_id": "2",
          "time": "20210831 02:54"
        },
        {
          "id": "MDRW2100024374.1.16",
          "form": "저는 작년인가 어느 고양이카페에서 고양ㅇ이로 열심히 장사하다가 \r\n나중에 문닫고 강가에 풀어줫다는거",
          "original_form": "저는 작년인가 어느 고양이카페에서 고양ㅇ이로 열심히 장사하다가 \r\n나중에 문닫고 강가에 풀어줫다는거",
          "speaker_id": "2",
          "time": "20210831 02:54"
        },
        {
          "id": "MDRW2100024374.1.17",
          "form": "너무 마음이안좋앗어요.",
          "original_form": "너무 마음이안좋앗어요.",
          "speaker_id": "2",
          "time": "20210831 02:54"
        },
        {
          "id": "MDRW2100024374.1.18",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210831 02:54"
        },
        {
          "id": "MDRW2100024374.1.19",
          "form": "고양이카페 그 사람오는거 맞아요",
          "original_form": "고양이카페 그 사람오는거 맞아요",
          "speaker_id": "2",
          "time": "20210831 02:54"
        },
        {
          "id": "MDRW2100024374.1.20",
          "form": "정상이 아닌 거 아닌가요...",
          "original_form": "정상이 아닌 거 아닌가요...",
          "speaker_id": "1",
          "time": "20210831 02:55"
        },
        {
          "id": "MDRW2100024374.1.21",
          "form": "그것이알고싶다인가 동물몽장에 나왓엇어요..\r\n품종ㅇ묘 많더라고요",
          "original_form": "그것이알고싶다인가 동물몽장에 나왓엇어요..\r\n품종ㅇ묘 많더라고요",
          "speaker_id": "2",
          "time": "20210831 02:55"
        },
        {
          "id": "MDRW2100024374.1.22",
          "form": "다 커서 입양도안되갖고..그렇됴..",
          "original_form": "다 커서 입양도안되갖고..그렇됴..",
          "speaker_id": "2",
          "time": "20210831 02:55"
        },
        {
          "id": "MDRW2100024374.1.23",
          "form": "고양이카페 강아지카페 이런 거 증말..",
          "original_form": "고양이카페 강아지카페 이런 거 증말..",
          "speaker_id": "1",
          "time": "20210831 02:55"
        },
        {
          "id": "MDRW2100024374.1.24",
          "form": "인간들은 다 애기만좋이힌대서 ㅜ",
          "original_form": "인간들은 다 애기만좋이힌대서 ㅜ",
          "speaker_id": "2",
          "time": "20210831 02:55"
        }
      ]
    }
