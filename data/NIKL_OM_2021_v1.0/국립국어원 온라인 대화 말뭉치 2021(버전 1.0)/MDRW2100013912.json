      "utterance": [
        {
          "id": "MDRW2100013912.1.1",
          "form": "가족 많아요?",
          "original_form": "가족 많아요?",
          "speaker_id": "1",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100013912.1.2",
          "form": "네 설거지 했어요",
          "original_form": "네 설거지 했어요",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100013912.1.3",
          "form": "이제 밥해야죠 ㅎㅎ",
          "original_form": "이제 밥해야죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100013912.1.4",
          "form": "아뇨 4이요",
          "original_form": "아뇨 4이요",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100013912.1.5",
          "form": "어떻게 되세요 가족?",
          "original_form": "어떻게 되세요 가족?",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100013912.1.6",
          "form": "4명이요",
          "original_form": "4명이요",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100013912.1.7",
          "form": "아 똑같네요",
          "original_form": "아 똑같네요",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100013912.1.8",
          "form": "아이가 둘이군요",
          "original_form": "아이가 둘이군요",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100013912.1.9",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100013912.1.10",
          "form": "초딩이요",
          "original_form": "초딩이요",
          "speaker_id": "1",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100013912.1.11",
          "form": "전 고3 초딩요",
          "original_form": "전 고3 초딩요",
          "speaker_id": "2",
          "time": "20210825 23:04"
        },
        {
          "id": "MDRW2100013912.1.12",
          "form": "선배시네요",
          "original_form": "선배시네요",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100013912.1.13",
          "form": "ㅎㅎ애들 금방커요 ㅎㅎ금방 선배돼요",
          "original_form": "ㅎㅎ애들 금방커요 ㅎㅎ금방 선배돼요",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100013912.1.14",
          "form": "하긴그렇죠",
          "original_form": "하긴그렇죠",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100013912.1.15",
          "form": "애들이 이제 자서 좀 편하시죠",
          "original_form": "애들이 이제 자서 좀 편하시죠",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100013912.1.16",
          "form": "저희애들은 안자고 있네요 ㅋㅋ",
          "original_form": "저희애들은 안자고 있네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100013912.1.17",
          "form": "내일 개학이라 일찍 재우요",
          "original_form": "내일 개학이라 일찍 재우요",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100013912.1.18",
          "form": "아직 안했군요",
          "original_form": "아직 안했군요",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100013912.1.19",
          "form": "저희는 저번주 등교요",
          "original_form": "저희는 저번주 등교요",
          "speaker_id": "2",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100013912.1.20",
          "form": "애들도 학교 가니 안심심하겠어요",
          "original_form": "애들도 학교 가니 안심심하겠어요",
          "speaker_id": "2",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100013912.1.21",
          "form": "없으니 시우ㅏ",
          "original_form": "없으니 시우ㅏ",
          "speaker_id": "1",
          "time": "20210825 23:13"
        }
      ]
