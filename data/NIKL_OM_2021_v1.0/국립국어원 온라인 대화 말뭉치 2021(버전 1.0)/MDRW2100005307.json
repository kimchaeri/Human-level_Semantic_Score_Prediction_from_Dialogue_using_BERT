        },
        {
          "id": "MDRW2100005307.1.2",
          "form": "혹시 메타버스 들어보셨어요?",
          "original_form": "혹시 메타버스 들어보셨어요?",
          "speaker_id": "1",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005307.1.3",
          "form": "아니요 처음 들어봐요",
          "original_form": "아니요 처음 들어봐요",
          "speaker_id": "2",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005307.1.4",
          "form": "메타버스라는게 ar 써서 거기서 대화하는걸로 알고있어요",
          "original_form": "메타버스라는게 ar 써서 거기서 대화하는걸로 알고있어요",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005307.1.5",
          "form": "요즘은 그게 흥한다네요",
          "original_form": "요즘은 그게 흥한다네요",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005307.1.6",
          "form": "신기한것 같아요",
          "original_form": "신기한것 같아요",
          "speaker_id": "2",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005307.1.7",
          "form": "ar같은거에 관심 있으세요?",
          "original_form": "ar같은거에 관심 있으세요?",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005307.1.8",
          "form": "아니요 관심이 없어요",
          "original_form": "아니요 관심이 없어요",
          "speaker_id": "2",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005307.1.9",
          "form": "코로나때문에 이런게 더 유행하나봐요",
          "original_form": "코로나때문에 이런게 더 유행하나봐요",
          "speaker_id": "1",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005307.1.10",
          "form": "그렇군요 제가 잘몰라서요",
          "original_form": "그렇군요 제가 잘몰라서요",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005307.1.11",
          "form": "ar말고 vr도 발전하면 현실이랑 정말 비슷할거같아요",
          "original_form": "ar말고 vr도 발전하면 현실이랑 정말 비슷할거같아요",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005307.1.12",
          "form": "vr기기 써보셨어요?",
          "original_form": "vr기기 써보셨어요?",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005307.1.13",
          "form": "사용하진 않았지만 어떤건지 알아요",
          "original_form": "사용하진 않았지만 어떤건지 알아요",
          "speaker_id": "2",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005307.1.14",
          "form": "ar은 포켓몬고같은건데",
          "original_form": "ar은 포켓몬고같은건데",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005307.1.15",
          "form": "증강현실이랑 가상현실 차이더라구요",
          "original_form": "증강현실이랑 가상현실 차이더라구요",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005307.1.16",
          "form": "ar이랑 vr은",
          "original_form": "ar이랑 vr은",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005307.1.17",
          "form": "지금 알았네요",
          "original_form": "지금 알았네요",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005307.1.18",
          "form": "ar도 안써보셨어요?",
