      "utterance": [
        {
          "id": "MDRW2100026627.1.1",
          "form": "팀탐 드셔보심?",
          "original_form": "팀탐 드셔보심?",
          "speaker_id": "2",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100026627.1.2",
          "form": "ㅎㅎ 이건 안부러움!",
          "original_form": "ㅎㅎ 이건 안부러움!",
          "speaker_id": "1",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100026627.1.3",
          "form": "저 단거 쥐약이라 세입이 한계예요",
          "original_form": "저 단거 쥐약이라 세입이 한계예요",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026627.1.4",
          "form": "근데 우유랑은 환상이라 세입만 먹고 싶네요ㅜ",
          "original_form": "근데 우유랑은 환상이라 세입만 먹고 싶네요ㅜ",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026627.1.5",
          "form": "저 첨알았습니다",
          "original_form": "저 첨알았습니다",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026627.1.6",
          "form": "우유랑 먹어라네요",
          "original_form": "우유랑 먹어라네요",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026627.1.7",
          "form": "호주식 섭취방법인가요",
          "original_form": "호주식 섭취방법인가요",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026627.1.8",
          "form": "그럼 원래 뭐랑 드시나요?",
          "original_form": "그럼 원래 뭐랑 드시나요?",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026627.1.9",
          "form": "맥주랑 먹었죠 ㅎㅎ",
          "original_form": "맥주랑 먹었죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026627.1.10",
          "form": "팀탐을요...?ㅎㅎ",
          "original_form": "팀탐을요...?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100026627.1.11",
          "form": "네ㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026627.1.12",
          "form": "먹을줄 몰라서요",
          "original_form": "먹을줄 몰라서요",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026627.1.13",
          "form": "제가 아는 팀탐이 그거 맞쥬?ㅎㅎㅎ",
          "original_form": "제가 아는 팀탐이 그거 맞쥬?ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026627.1.14",
          "form": "네 영어로 팀탐",
          "original_form": "네 영어로 팀탐",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026627.1.15",
          "form": "그건 먹을줄 모르는게 아니라..",
          "original_form": "그건 먹을줄 모르는게 아니라..",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026627.1.16",
          "form": "호주 워킹간 친구들ㅇㅇ",
          "original_form": "호주 워킹간 친구들ㅇㅇ",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026627.1.17",
          "form": "사오는 팀탐",
          "original_form": "사오는 팀탐",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026627.1.18",
          "form": "술만 아는 거 아닌가욪ㅎ ㅎ",
          "original_form": "술만 아는 거 아닌가욪ㅎ ㅎ",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026627.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ배우신분이네여",
          "original_form": "ㅋㅋㅋㅋㅋㅋ배우신분이네여",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026627.1.20",
          "form": "디저트 뭐 자주 드십니까?",
          "original_form": "디저트 뭐 자주 드십니까?",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026627.1.21",
          "form": "회! 고기!",
          "original_form": "회! 고기!",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026627.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026627.1.23",
          "form": "아 그리고 집에 오는길에 편의점서",
          "original_form": "아 그리고 집에 오는길에 편의점서",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026627.1.24",
          "form": "그건 메인디시자나요",
          "original_form": "그건 메인디시자나요",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026627.1.25",
          "form": "쏜살치킨 사왔어요",
          "original_form": "쏜살치킨 사왔어요",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026627.1.26",
          "form": "쏜살칙킨?",
          "original_form": "쏜살칙킨?",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026627.1.27",
          "form": "디저트로 치킨 먹을 거임ㅎㅎ",
          "original_form": "디저트로 치킨 먹을 거임ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026627.1.28",
          "form": "얼만가요?",
          "original_form": "얼만가요?",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026627.1.29",
          "form": "사실 전 밥이 없음 디저트라 생각해서ㅎ",
          "original_form": "사실 전 밥이 없음 디저트라 생각해서ㅎ",
          "speaker_id": "1",
          "time": "20210901 23:23"
        }
      ]
