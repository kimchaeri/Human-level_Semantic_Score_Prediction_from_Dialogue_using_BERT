      "utterance": [
        {
          "id": "MDRW2100009604.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210823 01:32"
        },
        {
          "id": "MDRW2100009604.1.2",
          "form": "안녕하세요, 어떤 계절을 가장 좋아하시나요?",
          "original_form": "안녕하세요, 어떤 계절을 가장 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210823 11:15"
        },
        {
          "id": "MDRW2100009604.1.3",
          "form": "저는 여름 제일 좋아요",
          "original_form": "저는 여름 제일 좋아요",
          "speaker_id": "1",
          "time": "20210823 12:33"
        },
        {
          "id": "MDRW2100009604.1.4",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210823 12:33"
        },
        {
          "id": "MDRW2100009604.1.5",
          "form": "저는 가을을 가장 좋아해요! 시원하고 식욕의 계절이니까요ㅌㅌ",
          "original_form": "저는 가을을 가장 좋아해요! 시원하고 식욕의 계절이니까요ㅌㅌ",
          "speaker_id": "2",
          "time": "20210823 13:20"
        },
        {
          "id": "MDRW2100009604.1.6",
          "form": "아 근데 가을이 정말 사람 살기에는",
          "original_form": "아 근데 가을이 정말 사람 살기에는",
          "speaker_id": "1",
          "time": "20210823 22:11"
        },
        {
          "id": "MDRW2100009604.1.7",
          "form": "최고인 거 같아요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "최고인 거 같아요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:11"
        },
        {
          "id": "MDRW2100009604.1.8",
          "form": "약간 삶의 여유를 되찾아주는 시원함이랄까요ㅋㅋㅋ 미세먼지도 없고요!",
          "original_form": "약간 삶의 여유를 되찾아주는 시원함이랄까요ㅋㅋㅋ 미세먼지도 없고요!",
          "speaker_id": "2",
          "time": "20210823 22:12"
        },
        {
          "id": "MDRW2100009604.1.9",
          "form": "아 하늘 높고 맑은 게",
          "original_form": "아 하늘 높고 맑은 게",
          "speaker_id": "1",
          "time": "20210823 22:23"
        },
        {
          "id": "MDRW2100009604.1.10",
          "form": "정말 정신적으로도 좋더라고요",
          "original_form": "정말 정신적으로도 좋더라고요",
          "speaker_id": "1",
          "time": "20210823 22:23"
        },
        {
          "id": "MDRW2100009604.1.11",
          "form": "약간 잠시 사운드 오브 뮤직의 주인공이 된 기분이랄까요ㅋㅋ",
          "original_form": "약간 잠시 사운드 오브 뮤직의 주인공이 된 기분이랄까요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100009604.1.12",
          "form": "그쵸 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100009604.1.13",
          "form": "사계절 정말 좋아요",
          "original_form": "사계절 정말 좋아요",
          "speaker_id": "1",
          "time": "20210823 22:29"
        },
        {
          "id": "MDRW2100009604.1.14",
          "form": "사계절이 뚜렷해야 좋은데 요즘은 기상이변으로 여름과 겨울이 너무 존재감이 크네요..",
          "original_form": "사계절이 뚜렷해야 좋은데 요즘은 기상이변으로 여름과 겨울이 너무 존재감이 크네요..",
          "speaker_id": "2",
          "time": "20210823 22:32"
        },
        {
          "id": "MDRW2100009604.1.15",
          "form": "아 공감학니다 ㅜㅠ",
          "original_form": "아 공감학니다 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100009604.1.16",
          "form": "너모ㅜ 덥고 너무 추워요",
          "original_form": "너모ㅜ 덥고 너무 추워요",
          "speaker_id": "1",
          "time": "20210823 22:33"
        },
        {
          "id": "MDRW2100009604.1.17",
          "form": "가을과 봄이 길었으면 좋겠어요ㅠ",
          "original_form": "가을과 봄이 길었으면 좋겠어요ㅠ",
          "speaker_id": "2",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100009604.1.18",
          "form": "저도 간절히 바랍니다 정말 ㅠㅠ",
          "original_form": "저도 간절히 바랍니다 정말 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 22:36"
        },
        {
          "id": "MDRW2100009604.1.19",
          "form": "가을 봄은 짧은데",
          "original_form": "가을 봄은 짧은데",
          "speaker_id": "1",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100009604.1.20",
          "form": "옷 사기도 애매하고요 ㅠㅠ",
          "original_form": "옷 사기도 애매하고요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 22:37"
        },
        {
          "id": "MDRW2100009604.1.21",
          "form": "샀다가 1달도 안 돼서 다음 계절로 넘어가면 솜사탕 씻은 너구리가 돼요...",
          "original_form": "샀다가 1달도 안 돼서 다음 계절로 넘어가면 솜사탕 씻은 너구리가 돼요...",
          "speaker_id": "2",
          "time": "20210823 22:42"
        },
        {
          "id": "MDRW2100009604.1.22",
          "form": "아 정말요....",
          "original_form": "아 정말요....",
          "speaker_id": "1",
          "time": "20210823 22:44"
        }
      ]
    }
