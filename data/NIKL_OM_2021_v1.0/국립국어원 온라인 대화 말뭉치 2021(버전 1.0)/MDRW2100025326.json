      "utterance": [
        {
          "id": "MDRW2100025326.1.1",
          "form": "오늘 비가 온다던데 살고 있는곳도 비가 오나요?",
          "original_form": "오늘 비가 온다던데 살고 있는곳도 비가 오나요?",
          "speaker_id": "2",
          "time": "20210901 09:32"
        },
        {
          "id": "MDRW2100025326.1.2",
          "form": "새벽에 비가왔는데 지금은 그쳤어요",
          "original_form": "새벽에 비가왔는데 지금은 그쳤어요",
          "speaker_id": "1",
          "time": "20210901 09:33"
        },
        {
          "id": "MDRW2100025326.1.3",
          "form": "거기 비오나요?",
          "original_form": "거기 비오나요?",
          "speaker_id": "1",
          "time": "20210901 09:33"
        },
        {
          "id": "MDRW2100025326.1.4",
          "form": "여긴 잠깐 왔다가 그쳤어요!",
          "original_form": "여긴 잠깐 왔다가 그쳤어요!",
          "speaker_id": "2",
          "time": "20210901 09:33"
        },
        {
          "id": "MDRW2100025326.1.5",
          "form": "날은 꾸물꾸물하네요 ㅎㅎ",
          "original_form": "날은 꾸물꾸물하네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 09:33"
        },
        {
          "id": "MDRW2100025326.1.6",
          "form": "흐린게 언제 비와도 이상하지않네요",
          "original_form": "흐린게 언제 비와도 이상하지않네요",
          "speaker_id": "1",
          "time": "20210901 09:58"
        },
        {
          "id": "MDRW2100025326.1.7",
          "form": "우산챙겨다니세요",
          "original_form": "우산챙겨다니세요",
          "speaker_id": "1",
          "time": "20210901 09:58"
        },
        {
          "id": "MDRW2100025326.1.8",
          "form": "감사합니다^^",
          "original_form": "감사합니다^^",
          "speaker_id": "2",
          "time": "20210901 15:44"
        },
        {
          "id": "MDRW2100025326.1.9",
          "form": "부디 이동시간에는 비가 안와야 할텐데요",
          "original_form": "부디 이동시간에는 비가 안와야 할텐데요",
          "speaker_id": "2",
          "time": "20210901 15:44"
        },
        {
          "id": "MDRW2100025326.1.10",
          "form": "우산 챙기셨나요? 여기는 비가 안오고 있어요 ㅎㅎ",
          "original_form": "우산 챙기셨나요? 여기는 비가 안오고 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 15:44"
        },
        {
          "id": "MDRW2100025326.1.11",
          "form": "네 여기도 비가 안오네요",
          "original_form": "네 여기도 비가 안오네요",
          "speaker_id": "1",
          "time": "20210901 15:45"
        },
        {
          "id": "MDRW2100025326.1.12",
          "form": "요즘 날씨가 덥지도 않고 참 좋네요",
          "original_form": "요즘 날씨가 덥지도 않고 참 좋네요",
          "speaker_id": "1",
          "time": "20210901 15:45"
        },
        {
          "id": "MDRW2100025326.1.13",
          "form": "저녁에 산책하기 좋겠어요",
          "original_form": "저녁에 산책하기 좋겠어요",
          "speaker_id": "1",
          "time": "20210901 15:45"
        },
        {
          "id": "MDRW2100025326.1.14",
          "form": "다행이네요! 맞아요 바람도 선선하고 좋아요",
          "original_form": "다행이네요! 맞아요 바람도 선선하고 좋아요",
          "speaker_id": "2",
          "time": "20210901 15:46"
        },
        {
          "id": "MDRW2100025326.1.15",
          "form": "가을은 언제 올까요? 거의 온 것 같네요!",
          "original_form": "가을은 언제 올까요? 거의 온 것 같네요!",
          "speaker_id": "2",
          "time": "20210901 15:46"
        },
        {
          "id": "MDRW2100025326.1.16",
          "form": "이제 곧인가봐요 설레네요",
          "original_form": "이제 곧인가봐요 설레네요",
          "speaker_id": "1",
          "time": "20210901 15:46"
        },
        {
          "id": "MDRW2100025326.1.17",
          "form": "가을 좋아하세요?",
          "original_form": "가을 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 15:47"
        },
        {
          "id": "MDRW2100025326.1.18",
          "form": "가을 생각안해봤는데 좋은 것 같아요 요즘 좀 좋아졌어요 ㅎㅎ",
          "original_form": "가을 생각안해봤는데 좋은 것 같아요 요즘 좀 좋아졌어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 15:52"
        },
        {
          "id": "MDRW2100025326.1.19",
          "form": "저는 가을에 여행가는걸 너무 좋아했어요",
          "original_form": "저는 가을에 여행가는걸 너무 좋아했어요",
          "speaker_id": "1",
          "time": "20210901 16:17"
        },
        {
          "id": "MDRW2100025326.1.20",
          "form": "최근에 코로나때문에 가질못하지만요",
          "original_form": "최근에 코로나때문에 가질못하지만요",
          "speaker_id": "1",
          "time": "20210901 16:17"
        },
        {
          "id": "MDRW2100025326.1.21",
          "form": "가을 여행지중 어디가 가장기억에 남으셨나요?",
          "original_form": "가을 여행지중 어디가 가장기억에 남으셨나요?",
          "speaker_id": "2",
          "time": "20210901 20:34"
        },
        {
          "id": "MDRW2100025326.1.22",
          "form": "저는 경주 자주갔어요",
          "original_form": "저는 경주 자주갔어요",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100025326.1.23",
          "form": "요근래 3년은 안갔지만 가을마다 갔었어요",
          "original_form": "요근래 3년은 안갔지만 가을마다 갔었어요",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100025326.1.24",
          "form": "경주 너무 좋죠 가을마다 갔다니 너무 좋으셨겠어요!",
          "original_form": "경주 너무 좋죠 가을마다 갔다니 너무 좋으셨겠어요!",
          "speaker_id": "2",
          "time": "20210901 21:12"
        }
      ]
    }
