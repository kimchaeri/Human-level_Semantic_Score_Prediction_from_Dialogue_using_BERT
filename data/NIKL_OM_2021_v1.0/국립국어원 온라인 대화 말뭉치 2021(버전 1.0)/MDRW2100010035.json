      "utterance": [
        {
          "id": "MDRW2100010035.1.1",
          "form": "뮤지컬보러가고싶은데",
          "original_form": "뮤지컬보러가고싶은데",
          "speaker_id": "2",
          "time": "20210823 13:25"
        },
        {
          "id": "MDRW2100010035.1.2",
          "form": "너무 비싸....",
          "original_form": "너무 비싸....",
          "speaker_id": "2",
          "time": "20210823 13:25"
        },
        {
          "id": "MDRW2100010035.1.3",
          "form": "뭐보고싶은디?",
          "original_form": "뭐보고싶은디?",
          "speaker_id": "1",
          "time": "20210823 13:25"
        },
        {
          "id": "MDRW2100010035.1.4",
          "form": "헤드윅이라고...",
          "original_form": "헤드윅이라고...",
          "speaker_id": "2",
          "time": "20210823 13:25"
        },
        {
          "id": "MDRW2100010035.1.5",
          "form": "조승우 이규형 오만석나옴..",
          "original_form": "조승우 이규형 오만석나옴..",
          "speaker_id": "2",
          "time": "20210823 13:25"
        },
        {
          "id": "MDRW2100010035.1.6",
          "form": "와 캐스팅 빵빵하네",
          "original_form": "와 캐스팅 빵빵하네",
          "speaker_id": "1",
          "time": "20210823 13:26"
        },
        {
          "id": "MDRW2100010035.1.7",
          "form": "그래서 가격도 가격인데 티팅하기도 힘들고",
          "original_form": "그래서 가격도 가격인데 티팅하기도 힘들고",
          "speaker_id": "2",
          "time": "20210823 13:26"
        },
        {
          "id": "MDRW2100010035.1.8",
          "form": "티팅이머야",
          "original_form": "티팅이머야",
          "speaker_id": "1",
          "time": "20210823 13:26"
        },
        {
          "id": "MDRW2100010035.1.9",
          "form": "티켓팅이요",
          "original_form": "티켓팅이요",
          "speaker_id": "2",
          "time": "20210823 13:26"
        },
        {
          "id": "MDRW2100010035.1.10",
          "form": "헤드윅뮤지컬보고싶어서 미리 영화로도 봐뒀단말야ㅜㅜ",
          "original_form": "헤드윅뮤지컬보고싶어서 미리 영화로도 봐뒀단말야ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 13:27"
        },
        {
          "id": "MDRW2100010035.1.11",
          "form": "에고",
          "original_form": "에고",
          "speaker_id": "1",
          "time": "20210823 13:27"
        },
        {
          "id": "MDRW2100010035.1.12",
          "form": "무지 보고싶겠구나",
          "original_form": "무지 보고싶겠구나",
          "speaker_id": "1",
          "time": "20210823 13:27"
        },
        {
          "id": "MDRW2100010035.1.13",
          "form": "가격은?",
          "original_form": "가격은?",
          "speaker_id": "1",
          "time": "20210823 13:28"
        },
        {
          "id": "MDRW2100010035.1.14",
          "form": "젤비싼 자리가 15마넌 이정도하고",
          "original_form": "젤비싼 자리가 15마넌 이정도하고",
          "speaker_id": "2",
          "time": "20210823 13:28"
        },
        {
          "id": "MDRW2100010035.1.15",
          "form": "싼 자리는 5마넌?",
          "original_form": "싼 자리는 5마넌?",
          "speaker_id": "2",
          "time": "20210823 13:28"
        },
        {
          "id": "MDRW2100010035.1.16",
          "form": "오 괜찮네",
          "original_form": "오 괜찮네",
          "speaker_id": "1",
          "time": "20210823 13:28"
        },
        {
          "id": "MDRW2100010035.1.17",
          "form": "근데 비싼자리가 잡기 더힘들어요",
          "original_form": "근데 비싼자리가 잡기 더힘들어요",
          "speaker_id": "2",
          "time": "20210823 13:28"
        },
        {
          "id": "MDRW2100010035.1.18",
          "form": "ㅋㅋㄱ",
          "original_form": "ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210823 13:28"
        },
        {
          "id": "MDRW2100010035.1.19",
          "form": "인기가 많은 뮤지컬인가보다",
          "original_form": "인기가 많은 뮤지컬인가보다",
          "speaker_id": "1",
          "time": "20210823 13:29"
        },
        {
          "id": "MDRW2100010035.1.20",
          "form": "하긴 캐스팅이 정말 화려하긴해",
          "original_form": "하긴 캐스팅이 정말 화려하긴해",
          "speaker_id": "1",
          "time": "20210823 13:29"
        },
        {
          "id": "MDRW2100010035.1.21",
          "form": "맞아요....",
          "original_form": "맞아요....",
          "speaker_id": "2",
          "time": "20210823 13:29"
        }
      ]
    }
