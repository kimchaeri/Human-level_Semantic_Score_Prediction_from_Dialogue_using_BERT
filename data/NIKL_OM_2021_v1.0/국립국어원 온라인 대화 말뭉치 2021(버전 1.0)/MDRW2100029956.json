      "utterance": [
        {
          "id": "MDRW2100029956.1.1",
          "form": "반려동물 용품 요즘에 신기한거 많던데",
          "original_form": "반려동물 용품 요즘에 신기한거 많던데",
          "speaker_id": "1",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100029956.1.2",
          "form": "오 어떤걸 보셨나요!",
          "original_form": "오 어떤걸 보셨나요!",
          "speaker_id": "2",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100029956.1.3",
          "form": "고양이 방석인데 타르트 모양에 과일 쿠션 있는거였어요",
          "original_form": "고양이 방석인데 타르트 모양에 과일 쿠션 있는거였어요",
          "speaker_id": "1",
          "time": "20210903 17:30"
        },
        {
          "id": "MDRW2100029956.1.4",
          "form": "너무 귀여워서ㅋㅋㅋ 한동안 눈꽂고 봤어요..",
          "original_form": "너무 귀여워서ㅋㅋㅋ 한동안 눈꽂고 봤어요..",
          "speaker_id": "1",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100029956.1.5",
          "form": "아 저 본 것 같아요ㅋㅋㅋㅋ",
          "original_form": "아 저 본 것 같아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100029956.1.6",
          "form": "고양이가 들어가면",
          "original_form": "고양이가 들어가면",
          "speaker_id": "2",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100029956.1.7",
          "form": "타르트와 일체가되서 귀엽더라구요",
          "original_form": "타르트와 일체가되서 귀엽더라구요",
          "speaker_id": "2",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100029956.1.8",
          "form": "고양이타르트 되잖아요 완전 귀여운...ㅜㅜ..",
          "original_form": "고양이타르트 되잖아요 완전 귀여운...ㅜㅜ..",
          "speaker_id": "1",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100029956.1.9",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100029956.1.10",
          "form": "그거랑 호빵 숨숨집?도 너무 귀여워요",
          "original_form": "그거랑 호빵 숨숨집?도 너무 귀여워요",
          "speaker_id": "2",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100029956.1.11",
          "form": "아 그거 진짜 고양이 체형이랑 안맞으면",
          "original_form": "아 그거 진짜 고양이 체형이랑 안맞으면",
          "speaker_id": "1",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100029956.1.12",
          "form": "머리만 들어가잖아요ㅋㅋㅋㅋ",
          "original_form": "머리만 들어가잖아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100029956.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그런 경우도 있군요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그런 경우도 있군요",
          "speaker_id": "2",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100029956.1.14",
          "form": "그리고 기껏 사줬는데",
          "original_form": "그리고 기껏 사줬는데",
          "speaker_id": "2",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100029956.1.15",
          "form": "고양이가 숨숨집엔 안들어가고",
          "original_form": "고양이가 숨숨집엔 안들어가고",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100029956.1.16",
          "form": "박스에만ㅋㅋㅋ들어가죠 비닐이나ㅜ",
          "original_form": "박스에만ㅋㅋㅋ들어가죠 비닐이나ㅜ",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100029956.1.17",
          "form": "숨숨집을 포장했던 골판지 상자에만 들어간다는 리뷰글",
          "original_form": "숨숨집을 포장했던 골판지 상자에만 들어간다는 리뷰글",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100029956.1.18",
          "form": "진짜 많이 본 것 같아요ㅋㅋㅋㅋㅋ",
          "original_form": "진짜 많이 본 것 같아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100029956.1.19",
          "form": "맞아요ㅜ 귀엽고 이해못하겠어요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅜ 귀엽고 이해못하겠어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100029956.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㄱㅋㅋ종이가 좋은걸까요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱㅋㅋ종이가 좋은걸까요",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100029956.1.21",
          "form": "너무 엉뚱한데 귀여워요",
          "original_form": "너무 엉뚱한데 귀여워요",
          "speaker_id": "2",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100029956.1.22",
          "form": "반려묘 집사신가요??",
          "original_form": "반려묘 집사신가요??",
          "speaker_id": "1",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100029956.1.23",
          "form": "저는 아직... 예비집사입니다ㅜ",
          "original_form": "저는 아직... 예비집사입니다ㅜ",
          "speaker_id": "1",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100029956.1.24",
          "form": "저는 동물 책임질 자신이 없어서ㅋㄱㅋㅠ 남의 집 강아지나 고양이를 많이 귀여워하려구요!",
          "original_form": "저는 동물 책임질 자신이 없어서ㅋㄱㅋㅠ 남의 집 강아지나 고양이를 많이 귀여워하려구요!",
          "speaker_id": "2",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100029956.1.25",
          "form": "이런걸 랜선집사?라고 하죠ㅋㅋㅋ?",
          "original_form": "이런걸 랜선집사?라고 하죠ㅋㅋㅋ?",
          "speaker_id": "2",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100029956.1.26",
          "form": "아 맞아요 랜선집사ㅋㅋㅋㄱ저도 아직은ㅠㅠㅜ...",
          "original_form": "아 맞아요 랜선집사ㅋㅋㅋㄱ저도 아직은ㅠㅠㅜ...",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100029956.1.27",
          "form": "데려오려고 열심히 공부중이에요 돈도 모으고!",
          "original_form": "데려오려고 열심히 공부중이에요 돈도 모으고!",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100029956.1.28",
          "form": "오 멋있어요!",
          "original_form": "오 멋있어요!",
          "speaker_id": "2",
          "time": "20210903 17:37"
        },
        {
          "id": "MDRW2100029956.1.29",
          "form": "고양이 키우려면 많이 준비하고 공부해야하는 것 같지만",
          "original_form": "고양이 키우려면 많이 준비하고 공부해야하는 것 같지만",
          "speaker_id": "2",
          "time": "20210903 17:37"
        },
        {
          "id": "MDRW2100029956.1.30",
          "form": "미리 열심히 준비하시고 계시고 관심도 많으신 것 같으니 잘 해내실 것 같아요!!",
          "original_form": "미리 열심히 준비하시고 계시고 관심도 많으신 것 같으니 잘 해내실 것 같아요!!",
          "speaker_id": "2",
          "time": "20210903 17:38"
        }
      ]
    }
