      "utterance": [
        {
          "id": "MDRW2100009204.1.1",
          "form": "이제 집안일은 끝나셨나요??",
          "original_form": "이제 집안일은 끝나셨나요??",
          "speaker_id": "1",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009204.1.2",
          "form": "네 설거지 밥 끝이요",
          "original_form": "네 설거지 밥 끝이요",
          "speaker_id": "2",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100009204.1.3",
          "form": "다하셨어요?",
          "original_form": "다하셨어요?",
          "speaker_id": "2",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100009204.1.4",
          "form": "네 저도 이제 끝내고 애들도 재웠어요\r\n요몇일 너무 못자긴 했는지 몸이 너무 피곤하네요 ㅠ",
          "original_form": "네 저도 이제 끝내고 애들도 재웠어요\r\n요몇일 너무 못자긴 했는지 몸이 너무 피곤하네요 ㅠ",
          "speaker_id": "1",
          "time": "20210822 22:14"
        },
        {
          "id": "MDRW2100009204.1.5",
          "form": "오늘은 조금 일찍 자봐야 겠어요 ㅠㅠ",
          "original_form": "오늘은 조금 일찍 자봐야 겠어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 22:15"
        },
        {
          "id": "MDRW2100009204.1.6",
          "form": "어머 애들 일찍자서 좋겠어요 ㅎㅎ",
          "original_form": "어머 애들 일찍자서 좋겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009204.1.7",
          "form": "네 어서 주무세요 ㅎㅎ",
          "original_form": "네 어서 주무세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009204.1.8",
          "form": "진짜 애들케어에 주부들은 주말도 힘들죠",
          "original_form": "진짜 애들케어에 주부들은 주말도 힘들죠",
          "speaker_id": "2",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009204.1.9",
          "form": "오늘낮잠 못주무셔서 더 그런가보네요",
          "original_form": "오늘낮잠 못주무셔서 더 그런가보네요",
          "speaker_id": "2",
          "time": "20210822 22:17"
        },
        {
          "id": "MDRW2100009204.1.10",
          "form": "다음날이 쉬는 날이면 늦게 자는데 평일 학교가는 날은 일찍자야된다고 약속해놨었거든요 ㅋㅋㅋ",
          "original_form": "다음날이 쉬는 날이면 늦게 자는데 평일 학교가는 날은 일찍자야된다고 약속해놨었거든요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:17"
        },
        {
          "id": "MDRW2100009204.1.11",
          "form": "오 좋네요 애들도착하고",
          "original_form": "오 좋네요 애들도착하고",
          "speaker_id": "2",
          "time": "20210822 22:17"
        },
        {
          "id": "MDRW2100009204.1.12",
          "form": "어깨도 아프고 온몸이 아픈게 ㅠㅠㅋㅋㅋ",
          "original_form": "어깨도 아프고 온몸이 아픈게 ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:17"
        },
        {
          "id": "MDRW2100009204.1.13",
          "form": "스마트 워치 끼고 자면 확실이 저는 깊은 수면은 20분정도 밖에 안되고 자면서 1시간이상은 깨더라구요..",
          "original_form": "스마트 워치 끼고 자면 확실이 저는 깊은 수면은 20분정도 밖에 안되고 자면서 1시간이상은 깨더라구요..",
          "speaker_id": "1",
          "time": "20210822 22:18"
        },
        {
          "id": "MDRW2100009204.1.14",
          "form": "수면시간도 짧은데 숙면까지 못취하니 확실히 피곤해요 ㅠㅠ",
          "original_form": "수면시간도 짧은데 숙면까지 못취하니 확실히 피곤해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 22:18"
        },
        {
          "id": "MDRW2100009204.1.15",
          "form": "맞아요 저도 그래요",
          "original_form": "맞아요 저도 그래요",
          "speaker_id": "2",
          "time": "20210822 22:18"
        },
        {
          "id": "MDRW2100009204.1.16",
          "form": "언제 쭉잤는지 모르겠어요",
          "original_form": "언제 쭉잤는지 모르겠어요",
          "speaker_id": "2",
          "time": "20210822 22:18"
        },
        {
          "id": "MDRW2100009204.1.17",
          "form": "어렸을때는 푹 잘잤었는데 ㅠㅠ",
          "original_form": "어렸을때는 푹 잘잤었는데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100009204.1.18",
          "form": "애들도 다커서 독립하면 잘잘까요 ㅋㅋ",
          "original_form": "애들도 다커서 독립하면 잘잘까요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100009204.1.19",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100009204.1.20",
          "form": "결혼후 이래요",
          "original_form": "결혼후 이래요",
          "speaker_id": "2",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100009204.1.21",
          "form": "애들키우면서 자꾸 깨는 버르하다보니 ㅠㅠㅠㅠ",
          "original_form": "애들키우면서 자꾸 깨는 버르하다보니 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100009204.1.22",
          "form": "습관이란게 무섭죠 ㅠㅠ",
          "original_form": "습관이란게 무섭죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100009204.1.23",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 22:21"
        },
        {
          "id": "MDRW2100009204.1.24",
          "form": "애기들 자주깨니",
          "original_form": "애기들 자주깨니",
          "speaker_id": "2",
          "time": "20210822 22:21"
        },
        {
          "id": "MDRW2100009204.1.25",
          "form": "분유 먹이고 그러다보니",
          "original_form": "분유 먹이고 그러다보니",
          "speaker_id": "2",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009204.1.26",
          "form": "자주깨서 그때부터 버릇이 된건지",
          "original_form": "자주깨서 그때부터 버릇이 된건지",
          "speaker_id": "2",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009204.1.27",
          "form": "네 자다가 깨서 울고..\r\n지금도 저흰 아직 같이 자거든요...\r\n온몸을 비틀고 차고",
          "original_form": "네 자다가 깨서 울고..\r\n지금도 저흰 아직 같이 자거든요...\r\n온몸을 비틀고 차고",
          "speaker_id": "1",
          "time": "20210822 22:23"
        },
        {
          "id": "MDRW2100009204.1.28",
          "form": "몸무림도 얼마나치는지",
          "original_form": "몸무림도 얼마나치는지",
          "speaker_id": "1",
          "time": "20210822 22:23"
        }
      ]
    }
