      "utterance": [
        {
          "id": "MDRW2100046411.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211021 11:42"
        },
        {
          "id": "MDRW2100046411.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211021 11:42"
        },
        {
          "id": "MDRW2100046411.1.3",
          "form": "공연 보는거 좋아하시나요?",
          "original_form": "공연 보는거 좋아하시나요?",
          "speaker_id": "2",
          "time": "20211021 11:42"
        },
        {
          "id": "MDRW2100046411.1.4",
          "form": "아이돌 보는거  좋아해요",
          "original_form": "아이돌 보는거  좋아해요",
          "speaker_id": "1",
          "time": "20211021 11:42"
        },
        {
          "id": "MDRW2100046411.1.5",
          "form": "좋아하는 아이돌이 있으세요?",
          "original_form": "좋아하는 아이돌이 있으세요?",
          "speaker_id": "2",
          "time": "20211021 11:43"
        },
        {
          "id": "MDRW2100046411.1.6",
          "form": "트와이스 러블리즈요",
          "original_form": "트와이스 러블리즈요",
          "speaker_id": "1",
          "time": "20211021 11:44"
        },
        {
          "id": "MDRW2100046411.1.7",
          "form": "사나 진짜 존예에요",
          "original_form": "사나 진짜 존예에요",
          "speaker_id": "2",
          "time": "20211021 11:45"
        },
        {
          "id": "MDRW2100046411.1.8",
          "form": "다시 태어나면..",
          "original_form": "다시 태어나면..",
          "speaker_id": "2",
          "time": "20211021 11:45"
        },
        {
          "id": "MDRW2100046411.1.9",
          "form": "사나로..",
          "original_form": "사나로..",
          "speaker_id": "2",
          "time": "20211021 11:45"
        },
        {
          "id": "MDRW2100046411.1.10",
          "form": "태어나고싶어요",
          "original_form": "태어나고싶어요",
          "speaker_id": "2",
          "time": "20211021 11:45"
        },
        {
          "id": "MDRW2100046411.1.11",
          "form": "저도 사나가 최애에요",
          "original_form": "저도 사나가 최애에요",
          "speaker_id": "1",
          "time": "20211021 11:45"
        },
        {
          "id": "MDRW2100046411.1.12",
          "form": "저는 딱히",
          "original_form": "저는 딱히",
          "speaker_id": "2",
          "time": "20211021 11:45"
        },
        {
          "id": "MDRW2100046411.1.13",
          "form": "좋아하는 아이돌이",
          "original_form": "좋아하는 아이돌이",
          "speaker_id": "2",
          "time": "20211021 11:45"
        },
        {
          "id": "MDRW2100046411.1.14",
          "form": "없지만",
          "original_form": "없지만",
          "speaker_id": "2",
          "time": "20211021 11:45"
        },
        {
          "id": "MDRW2100046411.1.15",
          "form": "사나는 사나다..",
          "original_form": "사나는 사나다..",
          "speaker_id": "2",
          "time": "20211021 11:45"
        },
        {
          "id": "MDRW2100046411.1.16",
          "form": "약간 차은우는 차은우다",
          "original_form": "약간 차은우는 차은우다",
          "speaker_id": "2",
          "time": "20211021 11:45"
        },
        {
          "id": "MDRW2100046411.1.17",
          "form": "같은",
          "original_form": "같은",
          "speaker_id": "2",
          "time": "20211021 11:45"
        },
        {
          "id": "MDRW2100046411.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211021 11:45"
        },
        {
          "id": "MDRW2100046411.1.19",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211021 11:45"
        },
        {
          "id": "MDRW2100046411.1.20",
          "form": "콘서트도 가보셨어요?",
          "original_form": "콘서트도 가보셨어요?",
          "speaker_id": "2",
          "time": "20211021 11:46"
        },
        {
          "id": "MDRW2100046411.1.21",
          "form": "트와이스나 러블리즈",
          "original_form": "트와이스나 러블리즈",
          "speaker_id": "2",
          "time": "20211021 11:46"
        },
        {
          "id": "MDRW2100046411.1.22",
          "form": "트와이스 한번요",
          "original_form": "트와이스 한번요",
          "speaker_id": "1",
          "time": "20211021 11:46"
        },
        {
          "id": "MDRW2100046411.1.23",
          "form": "어떠셨나요??",
          "original_form": "어떠셨나요??",
          "speaker_id": "2",
          "time": "20211021 11:46"
        },
        {
          "id": "MDRW2100046411.1.24",
          "form": "실물 너무 이뻣어요",
          "original_form": "실물 너무 이뻣어요",
          "speaker_id": "1",
          "time": "20211021 11:47"
        },
        {
          "id": "MDRW2100046411.1.25",
          "form": "ㅠㅠ 저도 영접하고싶습니다..\r\n",
          "original_form": "ㅠㅠ 저도 영접하고싶습니다..\r\n",
