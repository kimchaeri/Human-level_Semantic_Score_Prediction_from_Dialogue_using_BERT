      "utterance": [
        {
          "id": "MDRW2100042716.1.1",
          "form": "어제 비가 많이 와서 물이 고였어요",
          "original_form": "어제 비가 많이 와서 물이 고였어요",
          "speaker_id": "2",
          "time": "20210907 07:45"
        },
        {
          "id": "MDRW2100042716.1.2",
          "form": "에고 저도 이제 출근하는데 땅이 젖었네요ㅜ",
          "original_form": "에고 저도 이제 출근하는데 땅이 젖었네요ㅜ",
          "speaker_id": "1",
          "time": "20210907 07:45"
        },
        {
          "id": "MDRW2100042716.1.3",
          "form": "오늘도 비 내리나요? 여긴 잠시 멈췄어요",
          "original_form": "오늘도 비 내리나요? 여긴 잠시 멈췄어요",
          "speaker_id": "2",
          "time": "20210907 07:47"
        },
        {
          "id": "MDRW2100042716.1.4",
          "form": "저희도 잠시 소강상태인데",
          "original_form": "저희도 잠시 소강상태인데",
          "speaker_id": "1",
          "time": "20210907 07:47"
        },
        {
          "id": "MDRW2100042716.1.5",
          "form": "밤새 비가 와서 땅은 젖어있네요",
          "original_form": "밤새 비가 와서 땅은 젖어있네요",
          "speaker_id": "1",
          "time": "20210907 07:47"
        },
        {
          "id": "MDRW2100042716.1.6",
          "form": "참 오늘 절기상 백로래요  본격적으로 가을이네요",
          "original_form": "참 오늘 절기상 백로래요  본격적으로 가을이네요",
          "speaker_id": "2",
          "time": "20210907 07:50"
        },
        {
          "id": "MDRW2100042716.1.7",
          "form": "아 그랬군요",
          "original_form": "아 그랬군요",
          "speaker_id": "1",
          "time": "20210907 07:50"
        },
        {
          "id": "MDRW2100042716.1.8",
          "form": "이젠 정말 가을 느낌이 나요",
          "original_form": "이젠 정말 가을 느낌이 나요",
          "speaker_id": "1",
          "time": "20210907 07:50"
        },
        {
          "id": "MDRW2100042716.1.9",
          "form": "오늘 긴팔 입었어요",
          "original_form": "오늘 긴팔 입었어요",
          "speaker_id": "1",
          "time": "20210907 07:50"
        },
        {
          "id": "MDRW2100042716.1.10",
          "form": "이제 짧은 옷은 안녕이죠",
          "original_form": "이제 짧은 옷은 안녕이죠",
          "speaker_id": "2",
          "time": "20210907 07:51"
        },
        {
          "id": "MDRW2100042716.1.11",
          "form": "네 쉬폰 소재도 안녕",
          "original_form": "네 쉬폰 소재도 안녕",
          "speaker_id": "1",
          "time": "20210907 07:52"
        },
        {
          "id": "MDRW2100042716.1.12",
          "form": "트렌치코트나 야상은 아직 입기에 조금 그렇죠",
          "original_form": "트렌치코트나 야상은 아직 입기에 조금 그렇죠",
          "speaker_id": "2",
          "time": "20210907 07:54"
        },
        {
          "id": "MDRW2100042716.1.13",
          "form": "네 얇은 긴 셔츠 정도",
          "original_form": "네 얇은 긴 셔츠 정도",
          "speaker_id": "1",
          "time": "20210907 07:54"
        },
        {
          "id": "MDRW2100042716.1.14",
          "form": "아님 민소매+가디건",
          "original_form": "아님 민소매+가디건",
          "speaker_id": "1",
          "time": "20210907 07:54"
        },
        {
          "id": "MDRW2100042716.1.15",
          "form": "환절기 옷차림은 늘 어렵네요ㅠ",
          "original_form": "환절기 옷차림은 늘 어렵네요ㅠ",
          "speaker_id": "2",
          "time": "20210907 07:55"
        },
        {
          "id": "MDRW2100042716.1.16",
          "form": "네 얇은 것 여러겹 입는 게 좋은 것 같아요",
          "original_form": "네 얇은 것 여러겹 입는 게 좋은 것 같아요",
          "speaker_id": "1",
          "time": "20210907 07:55"
        },
        {
          "id": "MDRW2100042716.1.17",
          "form": "더우면 벗도록",
          "original_form": "더우면 벗도록",
          "speaker_id": "1",
          "time": "20210907 07:55"
        },
        {
          "id": "MDRW2100042716.1.18",
          "form": "일단 가디건이나 긴셔츠 챙겨야 겠어요",
          "original_form": "일단 가디건이나 긴셔츠 챙겨야 겠어요",
          "speaker_id": "2",
          "time": "20210907 07:56"
        },
        {
          "id": "MDRW2100042716.1.19",
          "form": "네네 오늘은 햇볕도 강하지 않을 것 같아요",
          "original_form": "네네 오늘은 햇볕도 강하지 않을 것 같아요",
          "speaker_id": "1",
          "time": "20210907 07:56"
        }
