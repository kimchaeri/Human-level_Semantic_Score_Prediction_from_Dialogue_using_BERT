      "utterance": [
        {
          "id": "MDRW2100032072.1.1",
          "form": "교회 다닐 때 봉사활동 가셨나요",
          "original_form": "교회 다닐 때 봉사활동 가셨나요",
          "speaker_id": "1",
          "time": "20210905 13:50"
        },
        {
          "id": "MDRW2100032072.1.2",
          "form": "저는 기독교 동아리도 했는데 봉사활동 1박 2일로 갔습니다",
          "original_form": "저는 기독교 동아리도 했는데 봉사활동 1박 2일로 갔습니다",
          "speaker_id": "2",
          "time": "20210905 13:50"
        },
        {
          "id": "MDRW2100032072.1.3",
          "form": "우와 봉사활동 어디로 가셨나요",
          "original_form": "우와 봉사활동 어디로 가셨나요",
          "speaker_id": "1",
          "time": "20210905 13:51"
        },
        {
          "id": "MDRW2100032072.1.4",
          "form": "소록도라고 아시나요",
          "original_form": "소록도라고 아시나요",
          "speaker_id": "2",
          "time": "20210905 13:51"
        },
        {
          "id": "MDRW2100032072.1.5",
          "form": "아니요 처음 들어봅니다",
          "original_form": "아니요 처음 들어봅니다",
          "speaker_id": "1",
          "time": "20210905 13:51"
        },
        {
          "id": "MDRW2100032072.1.6",
          "form": "나환자들이 사는 섬입니다",
          "original_form": "나환자들이 사는 섬입니다",
          "speaker_id": "2",
          "time": "20210905 13:51"
        },
        {
          "id": "MDRW2100032072.1.7",
          "form": "아 그렇군요 거기에서 어떤 걸 하셨죠",
          "original_form": "아 그렇군요 거기에서 어떤 걸 하셨죠",
          "speaker_id": "1",
          "time": "20210905 13:51"
        },
        {
          "id": "MDRW2100032072.1.8",
          "form": "거기에 교회가 있고 봉사를 갔지요 대학교 1학년 때요",
          "original_form": "거기에 교회가 있고 봉사를 갔지요 대학교 1학년 때요",
          "speaker_id": "2",
          "time": "20210905 13:51"
        },
        {
          "id": "MDRW2100032072.1.9",
          "form": "청소를 했고요",
          "original_form": "청소를 했고요",
          "speaker_id": "2",
          "time": "20210905 13:52"
        },
        {
          "id": "MDRW2100032072.1.10",
          "form": "청소 정말 하면 깨끗해 찌긴 하지만 안 하면 티가 나는 힘든 일이지요",
          "original_form": "청소 정말 하면 깨끗해 찌긴 하지만 안 하면 티가 나는 힘든 일이지요",
          "speaker_id": "1",
          "time": "20210905 13:52"
        },
        {
          "id": "MDRW2100032072.1.11",
          "form": "음식도 만들어서 대접 했던 거 같아요",
          "original_form": "음식도 만들어서 대접 했던 거 같아요",
          "speaker_id": "2",
          "time": "20210905 13:52"
        },
        {
          "id": "MDRW2100032072.1.12",
          "form": "요리도 잘 하시나 봐요",
          "original_form": "요리도 잘 하시나 봐요",
          "speaker_id": "1",
          "time": "20210905 13:52"
        },
        {
          "id": "MDRW2100032072.1.13",
          "form": "그리고 무슨 찬양을 준비했지요",
          "original_form": "그리고 무슨 찬양을 준비했지요",
          "speaker_id": "2",
          "time": "20210905 13:52"
        },
        {
          "id": "MDRW2100032072.1.14",
          "form": "어떤 찬양 이었나요",
          "original_form": "어떤 찬양 이었나요",
          "speaker_id": "1",
          "time": "20210905 13:52"
        },
        {
          "id": "MDRW2100032072.1.15",
          "form": "저는 그때 어려서 청소나 했던 거 같아요",
          "original_form": "저는 그때 어려서 청소나 했던 거 같아요",
          "speaker_id": "2",
          "time": "20210905 13:52"
        },
        {
          "id": "MDRW2100032072.1.16",
          "form": "20살 이었으니까요",
          "original_form": "20살 이었으니까요",
