      "utterance": [
        {
          "id": "MDRW2100012416.1.1",
          "form": "응. 재밌더라.",
          "original_form": "응. 재밌더라.",
          "speaker_id": "2",
          "time": "20210824 21:55"
        },
        {
          "id": "MDRW2100012416.1.2",
          "form": "신하균 연기 최고야.",
          "original_form": "신하균 연기 최고야.",
          "speaker_id": "2",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012416.1.3",
          "form": "그치? 여진구도 그렇고 둘다 연기 진짜 잘해",
          "original_form": "그치? 여진구도 그렇고 둘다 연기 진짜 잘해",
          "speaker_id": "1",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012416.1.4",
          "form": "괴물은 연출도 좋은 듯",
          "original_form": "괴물은 연출도 좋은 듯",
          "speaker_id": "1",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012416.1.5",
          "form": "응. 보면서 몰입하게 돼.",
          "original_form": "응. 보면서 몰입하게 돼.",
          "speaker_id": "2",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012416.1.6",
          "form": "몇 화까지 봤어??",
          "original_form": "몇 화까지 봤어??",
          "speaker_id": "1",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100012416.1.7",
          "form": "10화까지 봤어.",
          "original_form": "10화까지 봤어.",
          "speaker_id": "2",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100012416.1.8",
          "form": "아까워서 아껴 보는 중.",
          "original_form": "아까워서 아껴 보는 중.",
          "speaker_id": "2",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100012416.1.9",
          "form": "아 그럼 대충 범인은 알겠네??",
          "original_form": "아 그럼 대충 범인은 알겠네??",
          "speaker_id": "1",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100012416.1.10",
          "form": "10화 전에 나오잖아",
          "original_form": "10화 전에 나오잖아",
          "speaker_id": "1",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100012416.1.11",
          "form": "6화인가에서 강진묵은 잡혔는데 아직 예전 범인은 안 나왔어.",
          "original_form": "6화인가에서 강진묵은 잡혔는데 아직 예전 범인은 안 나왔어.",
          "speaker_id": "2",
          "time": "20210824 21:58"
        },
        {
          "id": "MDRW2100012416.1.12",
          "form": "스포하지 마.",
          "original_form": "스포하지 마.",
          "speaker_id": "2",
          "time": "20210824 21:58"
        },
        {
          "id": "MDRW2100012416.1.13",
          "form": "ㅋㅋㅋㅋ알겠어 엄마가 다 볼 때까지 얘기 안 할게~~",
          "original_form": "ㅋㅋㅋㅋ알겠어 엄마가 다 볼 때까지 얘기 안 할게~~",
          "speaker_id": "1",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100012416.1.14",
          "form": "16화까지밖에 없어서 아껴 보는 중이야.",
          "original_form": "16화까지밖에 없어서 아껴 보는 중이야.",
          "speaker_id": "2",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100012416.1.15",
          "form": "이거 다 보면 넷플릭스에서 볼 거 없어.",
          "original_form": "이거 다 보면 넷플릭스에서 볼 거 없어.",
          "speaker_id": "2",
          "time": "20210824 22:00"
        },
        {
          "id": "MDRW2100012416.1.16",
          "form": "내가 또 추천해주면 되지~ 볼 건 많아ㅋㅋ",
          "original_form": "내가 또 추천해주면 되지~ 볼 건 많아ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:00"
        },
        {
          "id": "MDRW2100012416.1.17",
          "form": "그럼 다행이고. 요즘 티비에선 재밌는 거 안해서 지겨워.",
          "original_form": "그럼 다행이고. 요즘 티비에선 재밌는 거 안해서 지겨워.",
          "speaker_id": "2",
          "time": "20210824 22:01"
        },
        {
          "id": "MDRW2100012416.1.18",
          "form": "드라마가 많이 없기는 한 것 같아",
          "original_form": "드라마가 많이 없기는 한 것 같아",
          "speaker_id": "1",
          "time": "20210824 22:02"
        },
        {
          "id": "MDRW2100012416.1.19",
          "form": "예능 중에서 대탈출 재밌대",
          "original_form": "예능 중에서 대탈출 재밌대",
          "speaker_id": "1",
          "time": "20210824 22:02"
        },
        {
          "id": "MDRW2100012416.1.20",
          "form": "신동 나오는 거",
          "original_form": "신동 나오는 거",
          "speaker_id": "1",
          "time": "20210824 22:02"
        },
        {
          "id": "MDRW2100012416.1.21",
          "form": "무슨 프로그램인데?",
          "original_form": "무슨 프로그램인데?",
          "speaker_id": "2",
          "time": "20210824 22:03"
        },
        {
          "id": "MDRW2100012416.1.22",
          "form": "방탈출 같은 거! 추리 프로그램이랑 비슷해",
          "original_form": "방탈출 같은 거! 추리 프로그램이랑 비슷해",
          "speaker_id": "1",
          "time": "20210824 22:03"
        },
        {
          "id": "MDRW2100012416.1.23",
          "form": "스케일이 엄청 크대",
          "original_form": "스케일이 엄청 크대",
          "speaker_id": "1",
          "time": "20210824 22:04"
        },
        {
          "id": "MDRW2100012416.1.24",
          "form": "나중에 집에 와서 티비에서 틀어줘 봐",
          "original_form": "나중에 집에 와서 티비에서 틀어줘 봐",
          "speaker_id": "2",
          "time": "20210824 22:05"
        },
        {
          "id": "MDRW2100012416.1.25",
          "form": "엄마는 요즘 놀면 뭐하니만 봐.",
          "original_form": "엄마는 요즘 놀면 뭐하니만 봐.",
          "speaker_id": "2",
          "time": "20210824 22:05"
        }
      ]
