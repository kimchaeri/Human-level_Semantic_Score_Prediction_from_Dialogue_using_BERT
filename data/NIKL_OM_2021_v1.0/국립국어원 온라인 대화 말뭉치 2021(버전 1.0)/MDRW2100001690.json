      "utterance": [
        {
          "id": "MDRW2100001690.1.1",
          "form": "있잖아 너는 반려동물 뭐키우구싶음??",
          "original_form": "있잖아 너는 반려동물 뭐키우구싶음??",
          "speaker_id": "1",
          "time": "20210819 18:06"
        },
        {
          "id": "MDRW2100001690.1.2",
          "form": "나는 그래도 고양이~~",
          "original_form": "나는 그래도 고양이~~",
          "speaker_id": "1",
          "time": "20210819 18:06"
        },
        {
          "id": "MDRW2100001690.1.3",
          "form": "반려동물 머키우고 싶냑!",
          "original_form": "반려동물 머키우고 싶냑!",
          "speaker_id": "1",
          "time": "20210819 18:07"
        },
        {
          "id": "MDRW2100001690.1.4",
          "form": "나도 고양이!!",
          "original_form": "나도 고양이!!",
          "speaker_id": "2",
          "time": "20210819 18:07"
        },
        {
          "id": "MDRW2100001690.1.5",
          "form": "히히 진짜 키우고싶다",
          "original_form": "히히 진짜 키우고싶다",
          "speaker_id": "1",
          "time": "20210819 18:07"
        },
        {
          "id": "MDRW2100001690.1.6",
          "form": "우리 맨날 구석이랑 구출이 얘기하잖아",
          "original_form": "우리 맨날 구석이랑 구출이 얘기하잖아",
          "speaker_id": "1",
          "time": "20210819 18:07"
        },
        {
          "id": "MDRW2100001690.1.7",
          "form": "우리가 만났던 길고양이들",
          "original_form": "우리가 만났던 길고양이들",
          "speaker_id": "1",
          "time": "20210819 18:08"
        },
        {
          "id": "MDRW2100001690.1.8",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210819 18:08"
        },
        {
          "id": "MDRW2100001690.1.9",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "2",
          "time": "20210819 18:08"
        },
        {
          "id": "MDRW2100001690.1.10",
          "form": "길고양이는 이제 잘 못만지겟어",
          "original_form": "길고양이는 이제 잘 못만지겟어",
          "speaker_id": "2",
          "time": "20210819 18:08"
        },
        {
          "id": "MDRW2100001690.1.11",
          "form": "왜???",
          "original_form": "왜???",
          "speaker_id": "1",
          "time": "20210819 18:08"
        },
        {
          "id": "MDRW2100001690.1.12",
          "form": "아니 어릴때 길냥이 쓰다듬으면서 다니다가",
          "original_form": "아니 어릴때 길냥이 쓰다듬으면서 다니다가",
          "speaker_id": "2",
          "time": "20210819 18:09"
        },
        {
          "id": "MDRW2100001690.1.13",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210819 18:09"
        },
        {
          "id": "MDRW2100001690.1.14",
          "form": "인수공통질병?걸려서",
          "original_form": "인수공통질병?걸려서",
          "speaker_id": "2",
          "time": "20210819 18:09"
        },
        {
          "id": "MDRW2100001690.1.15",
          "form": "피부과갔엇잔아",
          "original_form": "피부과갔엇잔아",
          "speaker_id": "2",
          "time": "20210819 18:10"
        },
        {
          "id": "MDRW2100001690.1.16",
          "form": "아그래??",
          "original_form": "아그래??",
          "speaker_id": "1",
          "time": "20210819 18:10"
        },
        {
          "id": "MDRW2100001690.1.17",
          "form": "위험한 질병이야?",
          "original_form": "위험한 질병이야?",
          "speaker_id": "1",
          "time": "20210819 18:10"
        },
        {
          "id": "MDRW2100001690.1.18",
          "form": "몰라 막 몸에",
          "original_form": "몰라 막 몸에",
          "speaker_id": "2",
          "time": "20210819 18:10"
        },
        {
          "id": "MDRW2100001690.1.19",
          "form": "동그라미 알러지같은거",
          "original_form": "동그라미 알러지같은거",
          "speaker_id": "2",
          "time": "20210819 18:10"
        },
        {
          "id": "MDRW2100001690.1.20",
          "form": "막 올라옴",
          "original_form": "막 올라옴",
          "speaker_id": "2",
          "time": "20210819 18:10"
        },
        {
          "id": "MDRW2100001690.1.21",
          "form": "으악.....",
          "original_form": "으악.....",
          "speaker_id": "1",
          "time": "20210819 18:10"
        },
        {
          "id": "MDRW2100001690.1.22",
          "form": "아팠겠다",
          "original_form": "아팠겠다",
          "speaker_id": "1",
          "time": "20210819 18:10"
        },
        {
          "id": "MDRW2100001690.1.23",
          "form": "그래도 피부과가서 약바르니까",
          "original_form": "그래도 피부과가서 약바르니까",
          "speaker_id": "2",
          "time": "20210819 18:10"
        },
        {
          "id": "MDRW2100001690.1.24",
          "form": "금방낫더라",
          "original_form": "금방낫더라",
          "speaker_id": "2",
          "time": "20210819 18:10"
        },
        {
          "id": "MDRW2100001690.1.25",
          "form": "오 다행이다",
          "original_form": "오 다행이다",
          "speaker_id": "1",
          "time": "20210819 18:10"
        },
        {
          "id": "MDRW2100001690.1.26",
          "form": "그 병이름은 링웜이래",
          "original_form": "그 병이름은 링웜이래",
          "speaker_id": "2",
          "time": "20210819 18:10"
        },
        {
          "id": "MDRW2100001690.1.27",
          "form": "아하 링웜??",
          "original_form": "아하 링웜??",
          "speaker_id": "1",
