      "utterance": [
        {
          "id": "MDRW2100006719.1.1",
          "form": "책 많이 보세요?",
          "original_form": "책 많이 보세요?",
          "speaker_id": "1",
          "time": "20210908 07:29"
        },
        {
          "id": "MDRW2100006719.1.2",
          "form": "요즘은 잘 안보네요",
          "original_form": "요즘은 잘 안보네요",
          "speaker_id": "2",
          "time": "20210908 07:34"
        },
        {
          "id": "MDRW2100006719.1.3",
          "form": "책 좀봐야하슨데",
          "original_form": "책 좀봐야하슨데",
          "speaker_id": "2",
          "time": "20210908 07:34"
        },
        {
          "id": "MDRW2100006719.1.4",
          "form": "name1는 어떠세요",
          "original_form": "&name1&는 어떠세요",
          "speaker_id": "2",
          "time": "20210908 07:34"
        },
        {
          "id": "MDRW2100006719.1.5",
          "form": "저는 꽤 읽어요",
          "original_form": "저는 꽤 읽어요",
          "speaker_id": "1",
          "time": "20210908 07:35"
        },
        {
          "id": "MDRW2100006719.1.6",
          "form": "근데 만화책도 많이 봐요ㅋ",
          "original_form": "근데 만화책도 많이 봐요ㅋ",
          "speaker_id": "1",
          "time": "20210908 07:35"
        },
        {
          "id": "MDRW2100006719.1.7",
          "form": "최근에 뭐 읽으셨어요?",
          "original_form": "최근에 뭐 읽으셨어요?",
          "speaker_id": "2",
          "time": "20210908 07:36"
        },
        {
          "id": "MDRW2100006719.1.8",
          "form": "전 아몬드요ㅋ",
          "original_form": "전 아몬드요ㅋ",
          "speaker_id": "1",
          "time": "20210908 07:37"
        },
        {
          "id": "MDRW2100006719.1.9",
          "form": "소설이죠 그거",
          "original_form": "소설이죠 그거",
          "speaker_id": "2",
          "time": "20210908 07:37"
        },
        {
          "id": "MDRW2100006719.1.10",
          "form": "네 무표정한 표지에요",
          "original_form": "네 무표정한 표지에요",
          "speaker_id": "1",
          "time": "20210908 07:38"
        },
        {
          "id": "MDRW2100006719.1.11",
          "form": "내용이 뭐에요?",
          "original_form": "내용이 뭐에요?",
          "speaker_id": "2",
          "time": "20210908 07:38"
        },
        {
          "id": "MDRW2100006719.1.12",
          "form": "뇌를 다쳐 감정을 잃어버린 소년의 내용이에요",
          "original_form": "뇌를 다쳐 감정을 잃어버린 소년의 내용이에요",
          "speaker_id": "1",
          "time": "20210908 07:40"
        },
        {
          "id": "MDRW2100006719.1.13",
          "form": "청소년 작품이라는데 어렵지 않나 싶어요",
          "original_form": "청소년 작품이라는데 어렵지 않나 싶어요",
          "speaker_id": "1",
          "time": "20210908 07:41"
        },
        {
          "id": "MDRW2100006719.1.14",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210908 07:49"
        },
        {
          "id": "MDRW2100006719.1.15",
          "form": "소설 읽어본게 언제인지",
          "original_form": "소설 읽어본게 언제인지",
          "speaker_id": "2",
          "time": "20210908 07:49"
        },
        {
          "id": "MDRW2100006719.1.16",
          "form": "저는 부의 인문학을 가장 마지막으로 읽었는데",
          "original_form": "저는 부의 인문학을 가장 마지막으로 읽었는데",
          "speaker_id": "2",
          "time": "20210908 07:49"
        },
        {
          "id": "MDRW2100006719.1.17",
          "form": "요즘 항상 재태크나 경제관련 책만 읽어요",
          "original_form": "요즘 항상 재태크나 경제관련 책만 읽어요",
          "speaker_id": "2",
          "time": "20210908 07:50"
        },
        {
          "id": "MDRW2100006719.1.18",
          "form": "그나마도 잘 안읽어서",
          "original_form": "그나마도 잘 안읽어서",
          "speaker_id": "2",
          "time": "20210908 07:50"
        },
        {
          "id": "MDRW2100006719.1.19",
          "form": "저도 주식책 읽어요ㅎ",
          "original_form": "저도 주식책 읽어요ㅎ",
          "speaker_id": "1",
          "time": "20210908 07:51"
        },
        {
          "id": "MDRW2100006719.1.20",
          "form": "경제를 알아야겠더라고요",
          "original_form": "경제를 알아야겠더라고요",
          "speaker_id": "1",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100006719.1.21",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210908 07:52"
        },
        {
          "id": "MDRW2100006719.1.22",
          "form": "진짜 이제 필수죠 경제상식",
          "original_form": "진짜 이제 필수죠 경제상식",
          "speaker_id": "2",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100006719.1.23",
          "form": "근데 어렵더라고요",
          "original_form": "근데 어렵더라고요",
          "speaker_id": "1",
          "time": "20210908 07:53"
        },
        {
          "id": "MDRW2100006719.1.24",
          "form": "그래야 부자되겠지만ㅋ",
          "original_form": "그래야 부자되겠지만ㅋ",
