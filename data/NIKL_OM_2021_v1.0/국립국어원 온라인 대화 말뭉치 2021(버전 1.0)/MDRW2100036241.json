      "utterance": [
        {
          "id": "MDRW2100036241.1.1",
          "form": "주먹밥.. 아 대박이에요 침고이네요오ㅋㅋ",
          "original_form": "주먹밥.. 아 대박이에요 침고이네요오ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:48"
        },
        {
          "id": "MDRW2100036241.1.2",
          "form": "지금 톡 한번 스윽 했슴닷",
          "original_form": "지금 톡 한번 스윽 했슴닷",
          "speaker_id": "2",
          "time": "20210830 19:49"
        },
        {
          "id": "MDRW2100036241.1.3",
          "form": "저 보고계시는줄 알았어요",
          "original_form": "저 보고계시는줄 알았어요",
          "speaker_id": "2",
          "time": "20210830 19:49"
        },
        {
          "id": "MDRW2100036241.1.4",
          "form": "허엇!!! 대박 야식으로 먹으면",
          "original_form": "허엇!!! 대박 야식으로 먹으면",
          "speaker_id": "1",
          "time": "20210830 19:50"
        },
        {
          "id": "MDRW2100036241.1.5",
          "form": "다섯 배 맛있잖아요",
          "original_form": "다섯 배 맛있잖아요",
          "speaker_id": "1",
          "time": "20210830 19:50"
        },
        {
          "id": "MDRW2100036241.1.6",
          "form": "부..럽..다",
          "original_form": "부..럽..다",
          "speaker_id": "1",
          "time": "20210830 19:50"
        },
        {
          "id": "MDRW2100036241.1.7",
          "form": "진짜요",
          "original_form": "진짜요",
          "speaker_id": "2",
          "time": "20210830 20:01"
        },
        {
          "id": "MDRW2100036241.1.8",
          "form": "근데 답이 없어요..",
          "original_form": "근데 답이 없어요..",
          "speaker_id": "2",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100036241.1.9",
          "form": "밥먹나봐여...",
          "original_form": "밥먹나봐여...",
          "speaker_id": "2",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100036241.1.10",
          "form": "오마이갓 그럴 순 없어요...",
          "original_form": "오마이갓 그럴 순 없어요...",
          "speaker_id": "1",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100036241.1.11",
          "form": "그럼 내일이라도 ...",
          "original_form": "그럼 내일이라도 ...",
          "speaker_id": "1",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100036241.1.12",
          "form": "그ㄴ래야겠어요..",
          "original_form": "그ㄴ래야겠어요..",
          "speaker_id": "2",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100036241.1.13",
          "form": "그 친구 제 떡볶이 프렌드거든요ㅋㅋㅋ",
          "original_form": "그 친구 제 떡볶이 프렌드거든요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100036241.1.14",
          "form": "같이 일주일간 먹었었어요ㅋㅋㅋ",
          "original_form": "같이 일주일간 먹었었어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100036241.1.15",
          "form": "아니 세상에 그럼 떡볶이 쉴만한 걸요.........",
          "original_form": "아니 세상에 그럼 떡볶이 쉴만한 걸요.........",
          "speaker_id": "1",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100036241.1.16",
          "form": "어떤 떡볶이 드셨어요!?",
          "original_form": "어떤 떡볶이 드셨어요!?",
          "speaker_id": "1",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100036241.1.17",
          "form": "시장떡볶이랑 마트 밑에 3군데 돌아가서 먹고 신전이랑 매떡",
          "original_form": "시장떡볶이랑 마트 밑에 3군데 돌아가서 먹고 신전이랑 매떡",
          "speaker_id": "2",
          "time": "20210830 20:23"
        },
        {
          "id": "MDRW2100036241.1.18",
          "form": "삼첩분식 이렇게 먹었어요!",
          "original_form": "삼첩분식 이렇게 먹었어요!",
          "speaker_id": "2",
          "time": "20210830 20:23"
        },
        {
          "id": "MDRW2100036241.1.19",
          "form": "헐... 진짜 넘 맛있었겠다 ㅜ.^",
          "original_form": "헐... 진짜 넘 맛있었겠다 ㅜ.^",
          "speaker_id": "1",
          "time": "20210830 20:25"
        },
        {
          "id": "MDRW2100036241.1.20",
          "form": "삼첩 안 먹어봤는데 맛나나요?!",
          "original_form": "삼첩 안 먹어봤는데 맛나나요?!",
          "speaker_id": "1",
          "time": "20210830 20:25"
        },
        {
          "id": "MDRW2100036241.1.21",
          "form": "네! 거기 감자튀김이 존맷이었어요",
          "original_form": "네! 거기 감자튀김이 존맷이었어요",
          "speaker_id": "2",
          "time": "20210830 20:27"
        },
        {
          "id": "MDRW2100036241.1.22",
          "form": "근데 컨디션 좀 타더라구요",
          "original_form": "근데 컨디션 좀 타더라구요",
          "speaker_id": "2",
          "time": "20210830 20:28"
        },
        {
          "id": "MDRW2100036241.1.23",
          "form": "눅눅과 바삭 그 사이..",
          "original_form": "눅눅과 바삭 그 사이..",
          "speaker_id": "2",
          "time": "20210830 20:28"
        },
        {
          "id": "MDRW2100036241.1.24",
          "form": "헐 눅눅하면 완전 꽝이잖아요 -.- !!!!!!!!!",
          "original_form": "헐 눅눅하면 완전 꽝이잖아요 -.- !!!!!!!!!",
          "speaker_id": "1",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100036241.1.25",
          "form": "자주 눅눅한가요?",
          "original_form": "자주 눅눅한가요?",
          "speaker_id": "1",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100036241.1.26",
          "form": "두번중에 1대1이였어요..",
          "original_form": "두번중에 1대1이였어요..",
          "speaker_id": "2",
          "time": "20210830 20:53"
        },
        {
          "id": "MDRW2100036241.1.27",
          "form": "다음에 판가름이 날 예정임덧",
          "original_form": "다음에 판가름이 날 예정임덧",
          "speaker_id": "2",
          "time": "20210830 20:53"
        },
        {
          "id": "MDRW2100036241.1.28",
          "form": "으악 -.- 담에도 그렇다면 다른 곳으로 넘어가버리세요",
          "original_form": "으악 -.- 담에도 그렇다면 다른 곳으로 넘어가버리세요",
          "speaker_id": "1",
          "time": "20210830 20:54"
        },
        {
          "id": "MDRW2100036241.1.29",
          "form": "눅눅한 감자 절대 용서 못해욥",
          "original_form": "눅눅한 감자 절대 용서 못해욥",
          "speaker_id": "1",
          "time": "20210830 20:54"
        },
        {
          "id": "MDRW2100036241.1.30",
          "form": "전 햄버거 감자튀김도 그래서 잘 안 먹어요",
          "original_form": "전 햄버거 감자튀김도 그래서 잘 안 먹어요",
          "speaker_id": "1",
          "time": "20210830 20:54"
        },
        {
          "id": "MDRW2100036241.1.31",
          "form": "저두요!",
          "original_form": "저두요!",
          "speaker_id": "2",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100036241.1.32",
          "form": "그래서 감튀 안시키기도해요",
          "original_form": "그래서 감튀 안시키기도해요",
          "speaker_id": "2",
          "time": "20210830 21:01"
        }
      ]
    }
