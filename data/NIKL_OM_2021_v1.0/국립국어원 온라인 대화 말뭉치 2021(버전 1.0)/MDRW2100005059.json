      "utterance": [
        {
          "id": "MDRW2100005059.1.1",
          "form": "저녁식사 하셨어요?",
          "original_form": "저녁식사 하셨어요?",
          "speaker_id": "2",
          "time": "20210821 21:27"
        },
        {
          "id": "MDRW2100005059.1.2",
          "form": "네 식사하셨어요?",
          "original_form": "네 식사하셨어요?",
          "speaker_id": "1",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100005059.1.3",
          "form": "배가 안고파서 토마토 썰어놓은거 먹었어요ㅎ",
          "original_form": "배가 안고파서 토마토 썰어놓은거 먹었어요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005059.1.4",
          "form": "제육이 아직도 안녹아요",
          "original_form": "제육이 아직도 안녹아요",
          "speaker_id": "2",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005059.1.5",
          "form": "헉 아직도요?",
          "original_form": "헉 아직도요?",
          "speaker_id": "1",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100005059.1.6",
          "form": "엄청 꽝꽝 얼어서 왔나봐요ㅋ",
          "original_form": "엄청 꽝꽝 얼어서 왔나봐요ㅋ",
          "speaker_id": "2",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100005059.1.7",
          "form": "렌지에 해동으로라도 돌리셔야하는거아니예요?",
          "original_form": "렌지에 해동으로라도 돌리셔야하는거아니예요?",
          "speaker_id": "1",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005059.1.8",
          "form": "걍 냅뒀다 낼점심으로 먹어야겠어요",
          "original_form": "걍 냅뒀다 낼점심으로 먹어야겠어요",
          "speaker_id": "2",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100005059.1.9",
          "form": "뭐드셨어요?",
          "original_form": "뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100005059.1.10",
          "form": "오늘 토마토드신게 다인거예요?",
          "original_form": "오늘 토마토드신게 다인거예요?",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005059.1.11",
          "form": "저는 친정에서 호박잎이랑 된장찌개해주셔서 그걸로 저녁해결했어요",
          "original_form": "저는 친정에서 호박잎이랑 된장찌개해주셔서 그걸로 저녁해결했어요",
          "speaker_id": "1",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005059.1.12",
          "form": "오 호박잎 좋아하는데 쌈싸서 먹음 맛있죠",
          "original_form": "오 호박잎 좋아하는데 쌈싸서 먹음 맛있죠",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005059.1.13",
          "form": "전점심에 감자고로케랑 곱창ㅋ",
          "original_form": "전점심에 감자고로케랑 곱창ㅋ",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005059.1.14",
          "form": "요즘 철인가봐요 호박잎",
          "original_form": "요즘 철인가봐요 호박잎",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005059.1.15",
          "form": "신랑 호박잎 먹고싶었다며 밥을 두공기먹더라구요",
          "original_form": "신랑 호박잎 먹고싶었다며 밥을 두공기먹더라구요",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005059.1.16",
          "form": "저도 먹고싶네요 먹어본지 오래됐어요",
          "original_form": "저도 먹고싶네요 먹어본지 오래됐어요",
          "speaker_id": "2",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005059.1.17",
          "form": "이런건 어머니손맛이 필요한거라 ㅎ",
          "original_form": "이런건 어머니손맛이 필요한거라 ㅎ",
          "speaker_id": "1",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005059.1.18",
          "form": "저도 엄마가 해줘서 먹었네요 ㅎ",
          "original_form": "저도 엄마가 해줘서 먹었네요 ㅎ",
          "speaker_id": "1",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005059.1.19",
          "form": "안그럼 찌고하는거 귀찮아서 못할듯요",
          "original_form": "안그럼 찌고하는거 귀찮아서 못할듯요",
          "speaker_id": "1",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005059.1.20",
          "form": "ㅎㅎ 다듬고 세척하고 찌고 번거롭긴해요",
          "original_form": "ㅎㅎ 다듬고 세척하고 찌고 번거롭긴해요",
          "speaker_id": "2",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005059.1.21",
          "form": "맞아요 담주말 어머니께 예약해보세요 호박잎 ㅎㅎ",
          "original_form": "맞아요 담주말 어머니께 예약해보세요 호박잎 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005059.1.22",
          "form": "그래야겠어요ㅎ",
          "original_form": "그래야겠어요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:47"
        }
      ]
    }
