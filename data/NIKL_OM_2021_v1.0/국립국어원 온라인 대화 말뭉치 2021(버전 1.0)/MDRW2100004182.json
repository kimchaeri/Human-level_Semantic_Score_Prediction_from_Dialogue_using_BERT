      "utterance": [
        {
          "id": "MDRW2100004182.1.1",
          "form": "오늘 아침부터 폭우가......",
          "original_form": "오늘 아침부터 폭우가......",
          "speaker_id": "1",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004182.1.2",
          "form": "오전에 잠시 병원다녀왔는데",
          "original_form": "오전에 잠시 병원다녀왔는데",
          "speaker_id": "1",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004182.1.3",
          "form": "바람에 우산 손잡이가 부러졌어요 ㅋㅋ",
          "original_form": "바람에 우산 손잡이가 부러졌어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:22"
        },
        {
          "id": "MDRW2100004182.1.4",
          "form": "헐....",
          "original_form": "헐....",
          "speaker_id": "2",
          "time": "20210821 16:52"
        },
        {
          "id": "MDRW2100004182.1.5",
          "form": "태풍도 온다던데요?",
          "original_form": "태풍도 온다던데요?",
          "speaker_id": "2",
          "time": "20210821 16:52"
        },
        {
          "id": "MDRW2100004182.1.6",
          "form": "그러게요ㅠ\r\n태풍이 오고 있데요",
          "original_form": "그러게요ㅠ\r\n태풍이 오고 있데요",
          "speaker_id": "1",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004182.1.7",
          "form": "name2이님 비오는 날 좋아하세요?",
          "original_form": "&name2&이님 비오는 날 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004182.1.8",
          "form": "아뇨...  저 뭐 내리는거 시러해요",
          "original_form": "아뇨...  저 뭐 내리는거 시러해요",
          "speaker_id": "2",
          "time": "20210821 17:42"
        },
        {
          "id": "MDRW2100004182.1.9",
          "form": "비 눈 둘다....ㅋㅋㅋ",
          "original_form": "비 눈 둘다....ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:42"
        },
        {
          "id": "MDRW2100004182.1.10",
          "form": "비는요 제가 예전에 어렸을때 자취할때 반지하살때 폭우땜에 침수된적이 있어가지고...",
          "original_form": "비는요 제가 예전에 어렸을때 자취할때 반지하살때 폭우땜에 침수된적이 있어가지고...",
          "speaker_id": "2",
          "time": "20210821 17:42"
        },
        {
          "id": "MDRW2100004182.1.11",
          "form": "아주 싫어하고",
          "original_form": "아주 싫어하고",
          "speaker_id": "2",
          "time": "20210821 17:43"
        },
        {
          "id": "MDRW2100004182.1.12",
          "form": "눈은...  아시죠?",
          "original_form": "눈은...  아시죠?",
          "speaker_id": "2",
          "time": "20210821 17:43"
        },
        {
          "id": "MDRW2100004182.1.13",
          "form": "남자들 눈 싫어하는거",
          "original_form": "남자들 눈 싫어하는거",
          "speaker_id": "2",
          "time": "20210821 17:43"
        },
        {
          "id": "MDRW2100004182.1.14",
          "form": "ㅋㅋㅋ 군대 이후로 싫어지신건가요?",
          "original_form": "ㅋㅋㅋ 군대 이후로 싫어지신건가요?",
          "speaker_id": "1",
          "time": "20210821 17:46"
        },
        {
          "id": "MDRW2100004182.1.15",
          "form": "네 ㅋㅋㅋ 눈은 그래요 ㅋ",
          "original_form": "네 ㅋㅋㅋ 눈은 그래요 ㅋ",
          "speaker_id": "2",
          "time": "20210821 20:02"
        },
        {
          "id": "MDRW2100004182.1.16",
          "form": "그리고 미끄럽자나요",
          "original_form": "그리고 미끄럽자나요",
          "speaker_id": "2",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100004182.1.17",
          "form": "이제 넘어지면 큰일날 나이라",
          "original_form": "이제 넘어지면 큰일날 나이라",
          "speaker_id": "2",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100004182.1.18",
          "form": "ㅋㅋㅋㅇㅋ",
          "original_form": "ㅋㅋㅋㅇㅋ",
          "speaker_id": "2",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100004182.1.19",
          "form": "ㅋㅋ아 갑자기 슬프네여ㅜㅜ",
          "original_form": "ㅋㅋ아 갑자기 슬프네여ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100004182.1.20",
          "form": "이제 뼈도 잘 안붙을 나인가여?ㅋㅋ",
          "original_form": "이제 뼈도 잘 안붙을 나인가여?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100004182.1.21",
          "form": "그정도까진 아니지만 고생하자나요 ㅋㅋㅋ",
          "original_form": "그정도까진 아니지만 고생하자나요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100004182.1.22",
          "form": "태풍땜에 비 많이온대요ㅜㅜ",
          "original_form": "태풍땜에 비 많이온대요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100004182.1.23",
          "form": "한 동안은 우산을 챙겨야겠어요ㅠ",
          "original_form": "한 동안은 우산을 챙겨야겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100004182.1.24",
          "form": "아직 안자고 계셨군여",
          "original_form": "아직 안자고 계셨군여",
          "speaker_id": "2",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100004182.1.25",
          "form": "갑자기 비가 이렇게 오네요",
          "original_form": "갑자기 비가 이렇게 오네요",
          "speaker_id": "2",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100004182.1.26",
          "form": "태풍이 빨리 지나가면 좋겠어요",
          "original_form": "태풍이 빨리 지나가면 좋겠어요",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100004182.1.27",
          "form": "태풍도 갑자기 오고 ㅋ",
          "original_form": "태풍도 갑자기 오고 ㅋ",
          "speaker_id": "2",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100004182.1.28",
          "form": "일기예보보니 담주도 비소식 있던데",
          "original_form": "일기예보보니 담주도 비소식 있던데",
          "speaker_id": "1",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100004182.1.29",
          "form": "빗길운전 조심하세요:)",
          "original_form": "빗길운전 조심하세요:)",
          "speaker_id": "1",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100004182.1.30",
          "form": "담주 내내 비오나봐요",
          "original_form": "담주 내내 비오나봐요",
