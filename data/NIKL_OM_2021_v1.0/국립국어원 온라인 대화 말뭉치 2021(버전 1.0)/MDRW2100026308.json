      "utterance": [
        {
          "id": "MDRW2100026308.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026308.1.2",
          "form": "요즘 계속 비오지 않아요?",
          "original_form": "요즘 계속 비오지 않아요?",
          "speaker_id": "1",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026308.1.3",
          "form": "그러게요 지긋지긋 하네요ㅋㅋ",
          "original_form": "그러게요 지긋지긋 하네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:35"
        },
        {
          "id": "MDRW2100026308.1.4",
          "form": "이번 여름에는 너무 안오더니",
          "original_form": "이번 여름에는 너무 안오더니",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100026308.1.5",
          "form": "갑자기 요즘에 퍼붓네요ㅠ",
          "original_form": "갑자기 요즘에 퍼붓네요ㅠ",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100026308.1.6",
          "form": "가을장마라고 그러더라구요 ㅋㅋ",
          "original_form": "가을장마라고 그러더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100026308.1.7",
          "form": "늦장마가 너무 길게온거같아요",
          "original_form": "늦장마가 너무 길게온거같아요",
          "speaker_id": "1",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100026308.1.8",
          "form": "일단 저희동네는 이번주는 비 예보 없네요",
          "original_form": "일단 저희동네는 이번주는 비 예보 없네요",
          "speaker_id": "2",
          "time": "20210901 21:38"
        },
        {
          "id": "MDRW2100026308.1.9",
          "form": "저는 최근에 연속으로 온건진 잘 기억 안나는데",
          "original_form": "저는 최근에 연속으로 온건진 잘 기억 안나는데",
          "speaker_id": "1",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026308.1.10",
          "form": "일단 어제 비가 너무많이와서 더 길게 온거같은 기분이에요",
          "original_form": "일단 어제 비가 너무많이와서 더 길게 온거같은 기분이에요",
          "speaker_id": "1",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026308.1.11",
          "form": "주말에 잠깐 맑다가 다시 또 비 오니까 더 싫더라구요ㅠ",
          "original_form": "주말에 잠깐 맑다가 다시 또 비 오니까 더 싫더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026308.1.12",
          "form": "우산 드는거 너무 힘들지 않아요?",
          "original_form": "우산 드는거 너무 힘들지 않아요?",
          "speaker_id": "1",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026308.1.13",
          "form": "ㅋㅋ 귀찮아서 어깨에 얹어보기도 하고..",
          "original_form": "ㅋㅋ 귀찮아서 어깨에 얹어보기도 하고..",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100026308.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100026308.1.15",
          "form": "근데 또 비 많이 오면 어깨에 얹으면 비 다젖지 않아요?ㅠㅠ",
          "original_form": "근데 또 비 많이 오면 어깨에 얹으면 비 다젖지 않아요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100026308.1.16",
          "form": "그쵸. 그래서 잠깐 하다 다시 제대로 잡죠ㅋㅋ",
          "original_form": "그쵸. 그래서 잠깐 하다 다시 제대로 잡죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100026308.1.17",
          "form": "저는 손목 힘이 약해서 그런지",
          "original_form": "저는 손목 힘이 약해서 그런지",
          "speaker_id": "1",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026308.1.18",
          "form": "우산들면 우산이 엄청 흔들리더라고요",
          "original_form": "우산들면 우산이 엄청 흔들리더라고요",
          "speaker_id": "1",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026308.1.19",
          "form": "우산을 쓰고 다니고 싶다는 생각도 가끔 해요ㅋㅋ 실제로 그러면 관종 되겠지만",
          "original_form": "우산을 쓰고 다니고 싶다는 생각도 가끔 해요ㅋㅋ 실제로 그러면 관종 되겠지만",
          "speaker_id": "2",
          "time": "20210901 23:01"
        }
      ]
