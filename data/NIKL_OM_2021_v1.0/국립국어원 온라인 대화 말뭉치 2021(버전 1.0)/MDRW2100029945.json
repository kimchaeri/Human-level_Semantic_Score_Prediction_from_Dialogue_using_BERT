      "utterance": [
        {
          "id": "MDRW2100029945.1.1",
          "form": "동물 키우시나요?",
          "original_form": "동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100029945.1.2",
          "form": "네 강아지 한 마리를 키우고 있어요",
          "original_form": "네 강아지 한 마리를 키우고 있어요",
          "speaker_id": "1",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100029945.1.3",
          "form": "헉 무슨 종인가요",
          "original_form": "헉 무슨 종인가요",
          "speaker_id": "2",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100029945.1.4",
          "form": "요크셔테리어요!",
          "original_form": "요크셔테리어요!",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100029945.1.5",
          "form": "혹시 묭님도 반려동물을 키우시나요?",
          "original_form": "혹시 묭님도 반려동물을 키우시나요?",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100029945.1.6",
          "form": "너무 귀여울 거 같아요ㅠㅜ",
          "original_form": "너무 귀여울 거 같아요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100029945.1.7",
          "form": "저는 고양이요!",
          "original_form": "저는 고양이요!",
          "speaker_id": "2",
          "time": "20210903 16:55"
        },
        {
          "id": "MDRW2100029945.1.8",
          "form": "진짜 귀여울 것 같아요ㅠㅠㅠ무슨 종인가요?",
          "original_form": "진짜 귀여울 것 같아요ㅠㅠㅠ무슨 종인가요?",
          "speaker_id": "1",
          "time": "20210903 16:55"
        },
        {
          "id": "MDRW2100029945.1.9",
          "form": "코숏이에요! 검정하양,,!",
          "original_form": "코숏이에요! 검정하양,,!",
          "speaker_id": "2",
          "time": "20210903 17:00"
        },
        {
          "id": "MDRW2100029945.1.10",
          "form": "고양이가 제일 좋아하는 장난감은 뭔가요?",
          "original_form": "고양이가 제일 좋아하는 장난감은 뭔가요?",
          "speaker_id": "1",
          "time": "20210903 17:01"
        },
        {
          "id": "MDRW2100029945.1.11",
          "form": "나이가 들어서 장난감을 잘 안 가지고 놀더라구요ㅠㅠ",
          "original_form": "나이가 들어서 장난감을 잘 안 가지고 놀더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 17:03"
        },
        {
          "id": "MDRW2100029945.1.12",
          "form": "강아지는 뭐로 노나요?",
          "original_form": "강아지는 뭐로 노나요?",
          "speaker_id": "2",
          "time": "20210903 17:03"
        },
        {
          "id": "MDRW2100029945.1.13",
          "form": "굴리면 간식 나오는 공을 제일 좋아하는 것 같아요",
          "original_form": "굴리면 간식 나오는 공을 제일 좋아하는 것 같아요",
          "speaker_id": "1",
          "time": "20210903 17:03"
        },
        {
          "id": "MDRW2100029945.1.14",
          "form": "헉 똑똑한가봐요 그 장난감도 잘 가지고 노는 걸 보니!",
          "original_form": "헉 똑똑한가봐요 그 장난감도 잘 가지고 노는 걸 보니!",
          "speaker_id": "2",
          "time": "20210903 17:04"
        },
        {
          "id": "MDRW2100029945.1.15",
          "form": "앗 감사합니다",
          "original_form": "앗 감사합니다",
          "speaker_id": "1",
          "time": "20210903 17:07"
        },
        {
          "id": "MDRW2100029945.1.16",
          "form": "고양이는 몇살인가요?",
          "original_form": "고양이는 몇살인가요?",
