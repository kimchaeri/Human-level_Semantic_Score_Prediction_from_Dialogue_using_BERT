      "utterance": [
        {
          "id": "MDRW2100003326.1.1",
          "form": "이제 곧 가을이네요",
          "original_form": "이제 곧 가을이네요",
          "speaker_id": "1",
          "time": "20210820 21:48"
        },
        {
          "id": "MDRW2100003326.1.2",
          "form": "그러게요 주말 비오고 나면 가을올거같아요",
          "original_form": "그러게요 주말 비오고 나면 가을올거같아요",
          "speaker_id": "2",
          "time": "20210820 21:49"
        },
        {
          "id": "MDRW2100003326.1.3",
          "form": "올해는 태풍이 없었네요",
          "original_form": "올해는 태풍이 없었네요",
          "speaker_id": "1",
          "time": "20210820 21:51"
        },
        {
          "id": "MDRW2100003326.1.4",
          "form": "그러네요  비도 많이안오고 ~",
          "original_form": "그러네요  비도 많이안오고 ~",
          "speaker_id": "2",
          "time": "20210820 21:53"
        },
        {
          "id": "MDRW2100003326.1.5",
          "form": "방금 올림픽 끝났는데",
          "original_form": "방금 올림픽 끝났는데",
          "speaker_id": "1",
          "time": "20210820 21:54"
        },
        {
          "id": "MDRW2100003326.1.6",
          "form": "이번 겨울에 또 열리네요",
          "original_form": "이번 겨울에 또 열리네요",
          "speaker_id": "1",
          "time": "20210820 21:54"
        },
        {
          "id": "MDRW2100003326.1.7",
          "form": "올림픽이요 ? 내년 아닌가요? 동계올림픽",
          "original_form": "올림픽이요 ? 내년 아닌가요? 동계올림픽",
          "speaker_id": "2",
          "time": "20210820 21:55"
        },
        {
          "id": "MDRW2100003326.1.8",
          "form": "네 내년 2월인가?",
          "original_form": "네 내년 2월인가?",
          "speaker_id": "1",
          "time": "20210820 21:57"
        },
        {
          "id": "MDRW2100003326.1.9",
          "form": "되게 금방이죠",
          "original_form": "되게 금방이죠",
          "speaker_id": "1",
          "time": "20210820 21:57"
        },
        {
          "id": "MDRW2100003326.1.10",
          "form": "아 그렇군요 그때가 겨울이니",
          "original_form": "아 그렇군요 그때가 겨울이니",
          "speaker_id": "2",
          "time": "20210820 21:58"
        },
        {
          "id": "MDRW2100003326.1.11",
          "form": "금방 겨울올거같아요",
          "original_form": "금방 겨울올거같아요",
          "speaker_id": "2",
          "time": "20210820 21:58"
        },
        {
          "id": "MDRW2100003326.1.12",
          "form": "요즘엔 시간이 빨리가는거같아요",
          "original_form": "요즘엔 시간이 빨리가는거같아요",
          "speaker_id": "1",
          "time": "20210820 21:59"
        },
        {
          "id": "MDRW2100003326.1.13",
          "form": "나이 먹으니 더그렇네요ㅎ 가을이나 좀 길어졌음 좋겠네요",
          "original_form": "나이 먹으니 더그렇네요ㅎ 가을이나 좀 길어졌음 좋겠네요",
          "speaker_id": "2",
          "time": "20210820 22:00"
        },
        {
          "id": "MDRW2100003326.1.14",
          "form": "어느계절 좋아하세요?",
          "original_form": "어느계절 좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 22:00"
        },
        {
          "id": "MDRW2100003326.1.15",
          "form": "원래는 겨울좋아했는데",
          "original_form": "원래는 겨울좋아했는데",
          "speaker_id": "1",
          "time": "20210820 22:01"
        },
        {
          "id": "MDRW2100003326.1.16",
          "form": "요즘에는 봄이나 여름될때 냄새가 좋더라구요",
          "original_form": "요즘에는 봄이나 여름될때 냄새가 좋더라구요",
          "speaker_id": "1",
          "time": "20210820 22:01"
        },
        {
          "id": "MDRW2100003326.1.17",
          "form": "전 더위타서 여름을 싫어해요 가을이 젤 좋더라구요",
          "original_form": "전 더위타서 여름을 싫어해요 가을이 젤 좋더라구요",
          "speaker_id": "2",
          "time": "20210820 22:09"
        },
        {
          "id": "MDRW2100003326.1.18",
          "form": "바람선선하고 파란하늘 보면 기분이 이상해요 가을타는지 ㅎㅎ",
          "original_form": "바람선선하고 파란하늘 보면 기분이 이상해요 가을타는지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 22:09"
        },
        {
          "id": "MDRW2100003326.1.19",
          "form": "계절이 바뀔때마다 냄새가 바껴서 그런가 분위기 타더라구요",
          "original_form": "계절이 바뀔때마다 냄새가 바껴서 그런가 분위기 타더라구요",
          "speaker_id": "1",
          "time": "20210820 22:10"
        },
        {
          "id": "MDRW2100003326.1.20",
          "form": "그런가봐요ㅎ 봄도 타고",
          "original_form": "그런가봐요ㅎ 봄도 타고",
          "speaker_id": "2",
          "time": "20210820 22:11"
        }
      ]
