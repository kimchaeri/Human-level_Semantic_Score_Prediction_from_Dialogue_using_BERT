      "utterance": [
        {
          "id": "MDRW2100043805.1.1",
          "form": "딱딱한 복숭아였던 것 같은데",
          "original_form": "딱딱한 복숭아였던 것 같은데",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043805.1.2",
          "form": "딱딱이 복숭아가 최고여",
          "original_form": "딱딱이 복숭아가 최고여",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043805.1.3",
          "form": "방치해뒀더니 물렁하 복숭아가 됐어",
          "original_form": "방치해뒀더니 물렁하 복숭아가 됐어",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043805.1.4",
          "form": "아 그것도 은근 맛있어",
          "original_form": "아 그것도 은근 맛있어",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100043805.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ넌 무조건 딱복파인줄 알았는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋ넌 무조건 딱복파인줄 알았는데",
          "speaker_id": "2",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043805.1.6",
          "form": "의외구나!",
          "original_form": "의외구나!",
          "speaker_id": "2",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043805.1.7",
          "form": "응 나이들면서 좋아졌어",
          "original_form": "응 나이들면서 좋아졌어",
          "speaker_id": "1",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043805.1.8",
          "form": "역시 세월 따라 식성도 변하는구나",
          "original_form": "역시 세월 따라 식성도 변하는구나",
          "speaker_id": "2",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100043805.1.9",
          "form": "지금 내가 생각하는 음식 맞춰볼래?",
          "original_form": "지금 내가 생각하는 음식 맞춰볼래?",
          "speaker_id": "2",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100043805.1.10",
          "form": "좋아",
          "original_form": "좋아",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043805.1.11",
          "form": "김치가 들어가니?",
          "original_form": "김치가 들어가니?",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043805.1.12",
          "form": "아니!",
          "original_form": "아니!",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043805.1.13",
          "form": "김치 안 들어가는 요리야",
          "original_form": "김치 안 들어가는 요리야",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043805.1.14",
          "form": "간장베이스 요리니!",
          "original_form": "간장베이스 요리니!",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043805.1.15",
          "form": "아니!!! 간장도 아니야",
          "original_form": "아니!!! 간장도 아니야",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043805.1.16",
          "form": "짭짤하긴 해",
          "original_form": "짭짤하긴 해",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043805.1.17",
          "form": "빨간베이스니?",
          "original_form": "빨간베이스니?",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043805.1.18",
          "form": "아니! 빨간베이스 요리도 아니지롱",
          "original_form": "아니! 빨간베이스 요리도 아니지롱",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043805.1.19",
          "form": "야채가 주 요리니",
          "original_form": "야채가 주 요리니",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043805.1.20",
          "form": "밀가루가 주니까 그렇다고 봐야 할까..?",
          "original_form": "밀가루가 주니까 그렇다고 봐야 할까..?",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043805.1.21",
          "form": "수제비니?",
          "original_form": "수제비니?",
          "speaker_id": "1",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043805.1.22",
          "form": "뛩~ 수제비 아닙니다",
          "original_form": "뛩~ 수제비 아닙니다",
          "speaker_id": "2",
          "time": "20210907 20:32"
        }
      ]
