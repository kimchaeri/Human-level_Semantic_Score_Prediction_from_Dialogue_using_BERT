      "utterance": [
        {
          "id": "MDRW2100006940.1.1",
          "form": "야식 드시는 거 좋아하세요?",
          "original_form": "야식 드시는 거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 00:00"
        },
        {
          "id": "MDRW2100006940.1.2",
          "form": "야식 좋아하죠!",
          "original_form": "야식 좋아하죠!",
          "speaker_id": "2",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100006940.1.3",
          "form": "근데 즐겨먹지는 않아요",
          "original_form": "근데 즐겨먹지는 않아요",
          "speaker_id": "2",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100006940.1.4",
          "form": "제일 좋아하는 메뉴는 뭔가요?",
          "original_form": "제일 좋아하는 메뉴는 뭔가요?",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006940.1.5",
          "form": "저는 지코바가 너무 먹고 싶네요 ㅎㅎㅎㅎ",
          "original_form": "저는 지코바가 너무 먹고 싶네요 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006940.1.6",
          "form": "오븐구이 치킨이에요!",
          "original_form": "오븐구이 치킨이에요!",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006940.1.7",
          "form": "오 저도 지코바 좋아해요!",
          "original_form": "오 저도 지코바 좋아해요!",
          "speaker_id": "2",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006940.1.8",
          "form": "양념순살 먹고 남은 양념에 밥 비벼 먹는 걸 좋아해요",
          "original_form": "양념순살 먹고 남은 양념에 밥 비벼 먹는 걸 좋아해요",
          "speaker_id": "2",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006940.1.9",
          "form": "그래도 아무래도 제일 자주 찾는 야식은 역시 라면 아닐까요",
          "original_form": "그래도 아무래도 제일 자주 찾는 야식은 역시 라면 아닐까요",
          "speaker_id": "2",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006940.1.10",
          "form": "헐 맛있겠다",
          "original_form": "헐 맛있겠다",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006940.1.11",
          "form": "저는 라면을 야식으로 잘 안먹어서",
          "original_form": "저는 라면을 야식으로 잘 안먹어서",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006940.1.12",
          "form": "라면은 점심으로 먹는게 제일 좋은 것 같아요 ㅠㅠ",
          "original_form": "라면은 점심으로 먹는게 제일 좋은 것 같아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006940.1.13",
          "form": "라면 먹고 자면 다음날 얼굴에서부터 티가 나니까요 ㅋㅋㅋㅋ",
          "original_form": "라면 먹고 자면 다음날 얼굴에서부터 티가 나니까요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006940.1.14",
          "form": "저는 라면은 아침으로 자주 먹는 편이에요",
          "original_form": "저는 라면은 아침으로 자주 먹는 편이에요",
          "speaker_id": "2",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006940.1.15",
          "form": "헐 아침으로는 또 처음이네요",
          "original_form": "헐 아침으로는 또 처음이네요",
          "speaker_id": "1",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006940.1.16",
          "form": "간편하니까요",
          "original_form": "간편하니까요",
          "speaker_id": "2",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006940.1.17",
          "form": "무슨 라면 제일 좋아하세요?",
          "original_form": "무슨 라면 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006940.1.18",
          "form": "저는 진라면 순한맛! 아니면 신라면 자주 먹어요",
          "original_form": "저는 진라면 순한맛! 아니면 신라면 자주 먹어요",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006940.1.19",
          "form": "진라면 매운 맛은 맵더라구요 너무...",
          "original_form": "진라면 매운 맛은 맵더라구요 너무...",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006940.1.20",
          "form": "앜ㅋㅋㅋ 그렇구나 매운맛 저는 좋아하는데",
          "original_form": "앜ㅋㅋㅋ 그렇구나 매운맛 저는 좋아하는데",
          "speaker_id": "1",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006940.1.21",
          "form": "불닭은 절대 못드시나용?",
          "original_form": "불닭은 절대 못드시나용?",
          "speaker_id": "1",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006940.1.22",
          "form": "오리지널은 절대 못 먹어요! 까르보불닭 정도만 종종 먹습니다...",
          "original_form": "오리지널은 절대 못 먹어요! 까르보불닭 정도만 종종 먹습니다...",
          "speaker_id": "2",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006940.1.23",
          "form": "까르보 불닭은 안매워서 좋아요 ㅎㅎㅎ",
          "original_form": "까르보 불닭은 안매워서 좋아요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006940.1.24",
          "form": "참치마요랑 먹으면 맛있는데!",
          "original_form": "참치마요랑 먹으면 맛있는데!",
          "speaker_id": "1",
          "time": "20210822 00:14"
        }
      ]
    }
