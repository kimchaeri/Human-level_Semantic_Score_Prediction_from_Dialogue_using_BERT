        {
          "id": "MDRW2100034692.1.1",
          "form": "며칠됨?만난지",
          "original_form": "며칠됨?만난지",
          "speaker_id": "2",
          "time": "20210828 17:36"
        },
        {
          "id": "MDRW2100034692.1.2",
          "form": "사실은 ㅎㅎ",
          "original_form": "사실은 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 17:37"
        },
        {
          "id": "MDRW2100034692.1.3",
          "form": "사귄지는 일주일됐고",
          "original_form": "사귄지는 일주일됐고",
          "speaker_id": "1",
          "time": "20210828 17:37"
        },
        {
          "id": "MDRW2100034692.1.4",
          "form": "굿",
          "original_form": "굿",
          "speaker_id": "2",
          "time": "20210828 17:37"
        },
        {
          "id": "MDRW2100034692.1.5",
          "form": "음 만난지 한달됐댔나? 2주됐댔나",
          "original_form": "음 만난지 한달됐댔나? 2주됐댔나",
          "speaker_id": "1",
          "time": "20210828 17:37"
        },
        {
          "id": "MDRW2100034692.1.6",
          "form": "지금참좋은때네",
          "original_form": "지금참좋은때네",
          "speaker_id": "2",
          "time": "20210828 17:37"
        },
        {
          "id": "MDRW2100034692.1.7",
          "form": "여하간 7번 실제로 만나고 걔가 사귀자 했대여 ㅋ",
          "original_form": "여하간 7번 실제로 만나고 걔가 사귀자 했대여 ㅋ",
          "speaker_id": "1",
          "time": "20210828 17:37"
        },
        {
          "id": "MDRW2100034692.1.8",
          "form": "7번 일주일만 강하게 기억에남음 ㅋㅋ",
          "original_form": "7번 일주일만 강하게 기억에남음 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:38"
        },
        {
          "id": "MDRW2100034692.1.9",
          "form": "일주일내내본거네",
          "original_form": "일주일내내본거네",
          "speaker_id": "2",
          "time": "20210828 17:38"
        },
        {
          "id": "MDRW2100034692.1.10",
          "form": "아닌가 ㅋㅋ",
          "original_form": "아닌가 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:38"
        },
        {
          "id": "MDRW2100034692.1.11",
          "form": "아니 사귀기전까지 7번 실제로 만났고",
          "original_form": "아니 사귀기전까지 7번 실제로 만났고",
          "speaker_id": "1",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100034692.1.12",
          "form": "사귄지는 일주일됐고",
          "original_form": "사귄지는 일주일됐고",
          "speaker_id": "1",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100034692.1.13",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100034692.1.14",
          "form": "그런거구만",
          "original_form": "그런거구만",
          "speaker_id": "2",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100034692.1.15",
          "form": "근데 7번만나는게 그 한달인가 2주인가",
          "original_form": "근데 7번만나는게 그 한달인가 2주인가",
          "speaker_id": "1",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100034692.1.16",
          "form": "사진봤어?",
          "original_form": "사진봤어?",
          "speaker_id": "2",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100034692.1.17",
          "form": "코로나라서 ㅋㅋ",
          "original_form": "코로나라서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100034692.1.18",
          "form": "아직 사귀기전엔 좀 드문드문 만났다고함",
          "original_form": "아직 사귀기전엔 좀 드문드문 만났다고함",
