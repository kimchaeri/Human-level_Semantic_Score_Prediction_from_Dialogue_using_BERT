      "utterance": [
        {
          "id": "MDRW2100032618.1.1",
          "form": "누님 ㅎㅎ",
          "original_form": "누님 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 22:23"
        },
        {
          "id": "MDRW2100032618.1.2",
          "form": "홍석천아니구요 ㅋㅋ",
          "original_form": "홍석천아니구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:23"
        },
        {
          "id": "MDRW2100032618.1.3",
          "form": "홍라면이라구 풀무원에서 나와용",
          "original_form": "홍라면이라구 풀무원에서 나와용",
          "speaker_id": "2",
          "time": "20210905 22:23"
        },
        {
          "id": "MDRW2100032618.1.4",
          "form": "음 라면!라면!라면!",
          "original_form": "음 라면!라면!라면!",
          "speaker_id": "2",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100032618.1.5",
          "form": "이런맛입니다 ㅋㅋ",
          "original_form": "이런맛입니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100032618.1.6",
          "form": "이제 먹엇서요",
          "original_form": "이제 먹엇서요",
          "speaker_id": "2",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100032618.1.7",
          "form": "정리한다고",
          "original_form": "정리한다고",
          "speaker_id": "2",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100032618.1.8",
          "form": "식사하셧냐고 제가 묻고 나서 답변못듣고 제출되어서",
          "original_form": "식사하셧냐고 제가 묻고 나서 답변못듣고 제출되어서",
          "speaker_id": "2",
          "time": "20210905 22:24"
        },
        {
          "id": "MDRW2100032618.1.9",
          "form": "뭐 드신지 알려줘용",
          "original_form": "뭐 드신지 알려줘용",
          "speaker_id": "2",
          "time": "20210905 22:25"
        },
        {
          "id": "MDRW2100032618.1.10",
          "form": "아ㅋㅋ 편의점 홍라면인줄ㅎㅎ",
          "original_form": "아ㅋㅋ 편의점 홍라면인줄ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100032618.1.11",
          "form": "일하면서 에너지바로 대충 떼웠구ㅜ",
          "original_form": "일하면서 에너지바로 대충 떼웠구ㅜ",
          "speaker_id": "1",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100032618.1.12",
          "form": "이제 귀가해서 살치살 꺼냈습니다ㅎㅎ",
          "original_form": "이제 귀가해서 살치살 꺼냈습니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100032618.1.13",
          "form": "살?",
          "original_form": "살?",
          "speaker_id": "2",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032618.1.14",
          "form": "치?",
          "original_form": "치?",
          "speaker_id": "2",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032618.1.15",
          "form": "살?!",
          "original_form": "살?!",
          "speaker_id": "2",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032618.1.16",
          "form": "너무 맛있겠네요",
          "original_form": "너무 맛있겠네요",
          "speaker_id": "2",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032618.1.17",
          "form": "몇그람?",
          "original_form": "몇그람?",
          "speaker_id": "2",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032618.1.18",
          "form": "살치살 300그램 꽃등심 300그램 있네요ㅎ",
          "original_form": "살치살 300그램 꽃등심 300그램 있네요ㅎ",
          "speaker_id": "1",
          "time": "20210905 22:36"
        },
        {
          "id": "MDRW2100032618.1.19",
          "form": "우선 살치살부터 조사버릴 예정이에유ㅎㅎ",
          "original_form": "우선 살치살부터 조사버릴 예정이에유ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 22:36"
        },
        {
          "id": "MDRW2100032618.1.20",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210905 22:41"
        },
        {
          "id": "MDRW2100032618.1.21",
          "form": "재료 좋은면",
          "original_form": "재료 좋은면",
          "speaker_id": "2",
          "time": "20210905 22:41"
        },
        {
          "id": "MDRW2100032618.1.22",
          "form": "재료가 극강입니다",
          "original_form": "재료가 극강입니다",
          "speaker_id": "2",
          "time": "20210905 22:41"
        },
        {
          "id": "MDRW2100032618.1.23",
          "form": "살치살 저 너무 좋아하는뎅 ㅋㅋ",
          "original_form": "살치살 저 너무 좋아하는뎅 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:41"
        },
        {
          "id": "MDRW2100032618.1.24",
          "form": "소금만 톡 쳐서",
          "original_form": "소금만 톡 쳐서",
          "speaker_id": "2",
          "time": "20210905 22:42"
        },
        {
          "id": "MDRW2100032618.1.25",
          "form": "소금간만으로도",
          "original_form": "소금간만으로도",
          "speaker_id": "2",
          "time": "20210905 22:42"
        },
        {
          "id": "MDRW2100032618.1.26",
          "form": "캬",
          "original_form": "캬",
          "speaker_id": "2",
          "time": "20210905 22:42"
        },
        {
          "id": "MDRW2100032618.1.27",
          "form": "소주 1병각이네여",
          "original_form": "소주 1병각이네여",
          "speaker_id": "2",
          "time": "20210905 22:43"
        },
        {
          "id": "MDRW2100032618.1.28",
          "form": "맞아요1소금만!!",
          "original_form": "맞아요1소금만!!",
          "speaker_id": "1",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100032618.1.29",
          "form": "다드심?",
          "original_form": "다드심?",
          "speaker_id": "2",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100032618.1.30",
          "form": "소주는?",
          "original_form": "소주는?",
          "speaker_id": "2",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100032618.1.31",
          "form": "술 자제해야 하는데 너무 맛있어서 큰일ㅜ",
          "original_form": "술 자제해야 하는데 너무 맛있어서 큰일ㅜ",
          "speaker_id": "1",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100032618.1.32",
          "form": "술은 뭘로?",
          "original_form": "술은 뭘로?",
          "speaker_id": "2",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100032618.1.33",
          "form": "아직 술 안 땄어요!",
          "original_form": "아직 술 안 땄어요!",
          "speaker_id": "1",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100032618.1.34",
          "form": "빨리 따여!",
          "original_form": "빨리 따여!",
          "speaker_id": "2",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100032618.1.35",
          "form": "이제 한입 먹음",
          "original_form": "이제 한입 먹음",
          "speaker_id": "1",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100032618.1.36",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100032618.1.37",
          "form": "영통하고싶다 레알",
          "original_form": "영통하고싶다 레알",
          "speaker_id": "2",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100032618.1.38",
          "form": "아닠ㅋ 음주를 조장하는 사회라니!!",
          "original_form": "아닠ㅋ 음주를 조장하는 사회라니!!",
