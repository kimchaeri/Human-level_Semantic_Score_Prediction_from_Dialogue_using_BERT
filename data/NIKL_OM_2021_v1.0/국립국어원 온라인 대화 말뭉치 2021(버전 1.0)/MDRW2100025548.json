        },
        {
          "id": "MDRW2100025548.1.2",
          "form": "그런 커플들이 너무 많아요 이미.... 흑",
          "original_form": "그런 커플들이 너무 많아요 이미.... 흑",
          "speaker_id": "1",
          "time": "20210901 13:08"
        },
        {
          "id": "MDRW2100025548.1.3",
          "form": "비극이죠",
          "original_form": "비극이죠",
          "speaker_id": "2",
          "time": "20210901 13:08"
        },
        {
          "id": "MDRW2100025548.1.4",
          "form": "그러니까요 여자들은 상향평준화 되고",
          "original_form": "그러니까요 여자들은 상향평준화 되고",
          "speaker_id": "1",
          "time": "20210901 13:08"
        },
        {
          "id": "MDRW2100025548.1.5",
          "form": "남자들은 노오오오오력을 안해요",
          "original_form": "남자들은 노오오오오력을 안해요",
          "speaker_id": "1",
          "time": "20210901 13:08"
        },
        {
          "id": "MDRW2100025548.1.6",
          "form": "안그래도 어제 대화하셨던 분도 30대 여성이셨는데",
          "original_form": "안그래도 어제 대화하셨던 분도 30대 여성이셨는데",
          "speaker_id": "1",
          "time": "20210901 13:09"
        },
        {
          "id": "MDRW2100025548.1.7",
          "form": "만나던 남자랑 데이트할 때",
          "original_form": "만나던 남자랑 데이트할 때",
          "speaker_id": "1",
          "time": "20210901 13:09"
        },
        {
          "id": "MDRW2100025548.1.8",
          "form": "호텔비는 여자분이 내셨대요",
          "original_form": "호텔비는 여자분이 내셨대요",
          "speaker_id": "1",
          "time": "20210901 13:09"
        },
        {
          "id": "MDRW2100025548.1.9",
          "form": "hotel비를요?",
          "original_form": "hotel비를요?",
          "speaker_id": "2",
          "time": "20210901 13:10"
        },
        {
          "id": "MDRW2100025548.1.10",
          "form": "네네. 그래서 대신  남자가 밥값을 내기로 했는데",
          "original_form": "네네. 그래서 대신  남자가 밥값을 내기로 했는데",
          "speaker_id": "1",
          "time": "20210901 13:10"
        },
        {
          "id": "MDRW2100025548.1.11",
          "form": "너무하다 진짜",
          "original_form": "너무하다 진짜",
          "speaker_id": "2",
          "time": "20210901 13:10"
        },
        {
          "id": "MDRW2100025548.1.12",
          "form": "다음날 조식을 남자가 샀는데",
          "original_form": "다음날 조식을 남자가 샀는데",
          "speaker_id": "1",
          "time": "20210901 13:10"
        },
        {
          "id": "MDRW2100025548.1.13",
          "form": "호텔 조식은 상대적으로 저렴하잖아요?",
          "original_form": "호텔 조식은 상대적으로 저렴하잖아요?",
          "speaker_id": "1",
          "time": "20210901 13:10"
        },
        {
          "id": "MDRW2100025548.1.14",
          "form": "근데 조식 한 번 샀다고",
          "original_form": "근데 조식 한 번 샀다고",
          "speaker_id": "1",
          "time": "20210901 13:10"
        },
        {
          "id": "MDRW2100025548.1.15",
          "form": "점심은 굶자고 했대요",
          "original_form": "점심은 굶자고 했대요",
          "speaker_id": "1",
          "time": "20210901 13:10"
        },
        {
          "id": "MDRW2100025548.1.16",
          "form": "그게 상대가 되나?",
          "original_form": "그게 상대가 되나?",
          "speaker_id": "2",
          "time": "20210901 13:10"
        },
        {
          "id": "MDRW2100025548.1.17",
          "form": "헐?",
          "original_form": "헐?",
          "speaker_id": "2",
          "time": "20210901 13:10"
        },
        {
          "id": "MDRW2100025548.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 어이없죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 어이없죠",
          "speaker_id": "1",
          "time": "20210901 13:11"
        },
        {
          "id": "MDRW2100025548.1.19",
          "form": "와, 진짜 끔찍하다",
          "original_form": "와, 진짜 끔찍하다",
          "speaker_id": "2",
          "time": "20210901 13:11"
        },
        {
          "id": "MDRW2100025548.1.20",
          "form": "그리고 또 다른 남자는",
          "original_form": "그리고 또 다른 남자는",
          "speaker_id": "1",
          "time": "20210901 13:11"
        },
        {
          "id": "MDRW2100025548.1.21",
          "form": "100일 선물이라고",
          "original_form": "100일 선물이라고",
          "speaker_id": "1",
          "time": "20210901 13:11"
        },
        {
          "id": "MDRW2100025548.1.22",
          "form": "마트에서 할인하는 옷을 사서",
          "original_form": "마트에서 할인하는 옷을 사서",
          "speaker_id": "1",
          "time": "20210901 13:11"
        },
        {
          "id": "MDRW2100025548.1.23",
          "form": "마트에서 팔아봤자",
          "original_form": "마트에서 팔아봤자",
          "speaker_id": "2",
          "time": "20210901 13:11"
        },
        {
          "id": "MDRW2100025548.1.24",
          "form": "선물해줬는데 옷에 얼룩이 묻어있다고 그걸",
          "original_form": "선물해줬는데 옷에 얼룩이 묻어있다고 그걸",
          "speaker_id": "1",
          "time": "20210901 13:11"
        },
        {
          "id": "MDRW2100025548.1.25",
          "form": "만원대아닌가..?",
          "original_form": "만원대아닌가..?",
          "speaker_id": "2",
          "time": "20210901 13:11"
        },
        {
          "id": "MDRW2100025548.1.26",
          "form": "자기 어머니 시켜서",
          "original_form": "자기 어머니 시켜서",
          "speaker_id": "1",
          "time": "20210901 13:12"
        },
        {
          "id": "MDRW2100025548.1.27",
          "form": "세탁하게 했대요",
          "original_form": "세탁하게 했대요",
          "speaker_id": "1",
          "time": "20210901 13:12"
        },
        {
          "id": "MDRW2100025548.1.28",
          "form": "근데 그것마저도 얼룩이 다 안지워져서",
          "original_form": "근데 그것마저도 얼룩이 다 안지워져서",
          "speaker_id": "1",
          "time": "20210901 13:12"
        },
        {
          "id": "MDRW2100025548.1.29",
          "form": "마트에서도 매대에 널린거 샀구나",
          "original_form": "마트에서도 매대에 널린거 샀구나",
          "speaker_id": "2",
          "time": "20210901 13:13"
        },
        {
          "id": "MDRW2100025548.1.30",
          "form": "여자분이 알아채신거래요",
          "original_form": "여자분이 알아채신거래요",
          "speaker_id": "1",
          "time": "20210901 13:14"
        },
        {
          "id": "MDRW2100025548.1.31",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210901 13:14"
        },
        {
          "id": "MDRW2100025548.1.32",
          "form": "진짜 극한의 가성비",
          "original_form": "진짜 극한의 가성비",
          "speaker_id": "2",
          "time": "20210901 13:14"
        },
        {
          "id": "MDRW2100025548.1.33",
          "form": "왜 그러고 산당ㅅ",
          "original_form": "왜 그러고 산당ㅅ",
          "speaker_id": "2",
          "time": "20210901 13:14"
        },
        {
          "id": "MDRW2100025548.1.34",
          "form": "아니 연애동아리냐고",
          "original_form": "아니 연애동아리냐고",
          "speaker_id": "1",
          "time": "20210901 13:14"
        },
        {
          "id": "MDRW2100025548.1.35",
          "form": "휴..",
          "original_form": "휴..",
          "speaker_id": "1",
          "time": "20210901 13:15"
        }
      ]
