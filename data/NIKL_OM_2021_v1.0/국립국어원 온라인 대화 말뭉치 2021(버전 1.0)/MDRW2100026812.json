          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100026812.1.19",
          "form": "대단은요 ㅋ",
          "original_form": "대단은요 ㅋ",
          "speaker_id": "1",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100026812.1.20",
          "form": "제가 봤을땬 그래요ㅎㅎ",
