      "utterance": [
        {
          "id": "MDRW2100026812.1.1",
          "form": "ㅎㅎ 아근데 안피곤하세요?",
          "original_form": "ㅎㅎ 아근데 안피곤하세요?",
          "speaker_id": "2",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100026812.1.2",
          "form": "아 아까 ㅋㅋ 잠깜 졸앗다 깻어여 ㅋㅋㅋ",
          "original_form": "아 아까 ㅋㅋ 잠깜 졸앗다 깻어여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100026812.1.3",
          "form": "아고아고 피곤하시나보네요ㅎㅎ",
          "original_form": "아고아고 피곤하시나보네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100026812.1.4",
          "form": "그럼지금 교구만드시는중이에요?ㅎㅎ",
          "original_form": "그럼지금 교구만드시는중이에요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026812.1.5",
          "form": "애기키우는게 너무어려운것같애요ㅠ",
          "original_form": "애기키우는게 너무어려운것같애요ㅠ",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026812.1.6",
          "form": "아뇨 교구는 오늘 안해도 되욮ㅎㅎ",
          "original_form": "아뇨 교구는 오늘 안해도 되욮ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026812.1.7",
          "form": "오오ㅎㅎ",
          "original_form": "오오ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026812.1.8",
          "form": "암튼 너무어려워요ㅎ",
          "original_form": "암튼 너무어려워요ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026812.1.9",
          "form": "ㅇ어렵죠 ㅜㅁ ㅜ",
          "original_form": "ㅇ어렵죠 ㅜㅁ ㅜ",
          "speaker_id": "1",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026812.1.10",
          "form": "많이 놀아주소 싶은데ㅎ",
          "original_form": "많이 놀아주소 싶은데ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026812.1.11",
          "form": "맘대로안되네요ㅎ",
          "original_form": "맘대로안되네요ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026812.1.12",
          "form": "예스 24사에서  0원구매한것들이 와서 ㅋ",
          "original_form": "예스 24사에서  0원구매한것들이 와서 ㅋ",
          "speaker_id": "1",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026812.1.13",
          "form": "조립하는거 와서 자기전에 엄청 만들구 자네요 ㅎㅎ",
          "original_form": "조립하는거 와서 자기전에 엄청 만들구 자네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026812.1.14",
          "form": "ㅎㅎ 잼겟네요ㅎㅎㅎ",
          "original_form": "ㅎㅎ 잼겟네요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026812.1.15",
