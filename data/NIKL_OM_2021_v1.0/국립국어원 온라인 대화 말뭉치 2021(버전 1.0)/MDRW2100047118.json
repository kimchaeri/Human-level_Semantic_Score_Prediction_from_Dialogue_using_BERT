      "utterance": [
        {
          "id": "MDRW2100047118.1.1",
          "form": "과자 좋아하시나요",
          "original_form": "과자 좋아하시나요",
          "speaker_id": "1",
          "time": "20210822 16:23"
        },
        {
          "id": "MDRW2100047118.1.2",
          "form": "넹 ㅋㅋㅋㅋ 저 좋아해요",
          "original_form": "넹 ㅋㅋㅋㅋ 저 좋아해요",
          "speaker_id": "2",
          "time": "20210822 16:24"
        },
        {
          "id": "MDRW2100047118.1.3",
          "form": "어떤 과자좋아해요 나는 달콤파다 짭짤파다",
          "original_form": "어떤 과자좋아해요 나는 달콤파다 짭짤파다",
          "speaker_id": "1",
          "time": "20210822 16:24"
        },
        {
          "id": "MDRW2100047118.1.4",
          "form": "전 짭짤파입니다",
          "original_form": "전 짭짤파입니다",
          "speaker_id": "1",
          "time": "20210822 16:24"
        },
        {
          "id": "MDRW2100047118.1.5",
          "form": "앗...저는 단짠단짠파",
          "original_form": "앗...저는 단짠단짠파",
          "speaker_id": "2",
          "time": "20210822 16:24"
        },
        {
          "id": "MDRW2100047118.1.6",
          "form": "역시 맛잘알일거같더라니",
          "original_form": "역시 맛잘알일거같더라니",
          "speaker_id": "1",
          "time": "20210822 16:24"
        },
        {
          "id": "MDRW2100047118.1.7",
          "form": "어떤 과자 좋아해요",
          "original_form": "어떤 과자 좋아해요",
          "speaker_id": "1",
          "time": "20210822 16:24"
        },
        {
          "id": "MDRW2100047118.1.8",
          "form": "음 저는 짭짤류는 감자칩쪽 좋아하고...",
          "original_form": "음 저는 짭짤류는 감자칩쪽 좋아하고...",
          "speaker_id": "2",
          "time": "20210822 16:25"
        },
        {
          "id": "MDRW2100047118.1.9",
          "form": "뭘 좀 아시네요",
          "original_form": "뭘 좀 아시네요",
          "speaker_id": "1",
          "time": "20210822 16:25"
        },
        {
          "id": "MDRW2100047118.1.10",
          "form": "감자칩은 짭짤이지",
          "original_form": "감자칩은 짭짤이지",
          "speaker_id": "1",
          "time": "20210822 16:25"
        },
        {
          "id": "MDRW2100047118.1.11",
          "form": "달달은요",
          "original_form": "달달은요",
          "speaker_id": "1",
          "time": "20210822 16:25"
        },
        {
          "id": "MDRW2100047118.1.12",
          "form": "그쵸그쵸 ㅋㅋㅋㅋㅋ",
          "original_form": "그쵸그쵸 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:25"
        },
        {
          "id": "MDRW2100047118.1.13",
          "form": "달달쪽은 종류가 다양해서 꼽기 애매한데",
          "original_form": "달달쪽은 종류가 다양해서 꼽기 애매한데",
          "speaker_id": "2",
          "time": "20210822 16:25"
        },
        {
          "id": "MDRW2100047118.1.14",
          "form": "혹시 꼬북칩",
          "original_form": "혹시 꼬북칩",
          "speaker_id": "1",
          "time": "20210822 16:25"
        },
        {
          "id": "MDRW2100047118.1.15",
          "form": "초코가 들어가면 달달이죠",
          "original_form": "초코가 들어가면 달달이죠",
          "speaker_id": "2",
          "time": "20210822 16:25"
        },
        {
          "id": "MDRW2100047118.1.16",
          "form": "어때요",
          "original_form": "어때요",
          "speaker_id": "1",
          "time": "20210822 16:25"
        },
        {
          "id": "MDRW2100047118.1.17",
          "form": "앗 꼬북칩",
          "original_form": "앗 꼬북칩",
          "speaker_id": "2",
          "time": "20210822 16:26"
        },
        {
          "id": "MDRW2100047118.1.18",
          "form": "인절미맛밖에 안먹어봤어요",
          "original_form": "인절미맛밖에 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210822 16:26"
        },
        {
          "id": "MDRW2100047118.1.19",
          "form": "초코츄러스맛 맛있다던데 ㅋㅋㅋㅋ",
          "original_form": "초코츄러스맛 맛있다던데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:26"
        },
        {
          "id": "MDRW2100047118.1.20",
          "form": "아 안타깝다",
          "original_form": "아 안타깝다",
          "speaker_id": "1",
          "time": "20210822 16:26"
        },
        {
          "id": "MDRW2100047118.1.21",
          "form": "초코맛을 아직 안드셨다니",
          "original_form": "초코맛을 아직 안드셨다니",
          "speaker_id": "1",
          "time": "20210822 16:26"
        }
      ]
