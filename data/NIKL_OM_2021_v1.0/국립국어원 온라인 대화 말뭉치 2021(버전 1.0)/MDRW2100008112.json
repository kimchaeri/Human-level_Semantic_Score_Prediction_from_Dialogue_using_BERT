          "speaker_id": "2",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100008112.1.3",
          "form": "맑아요",
          "original_form": "맑아요",
          "speaker_id": "1",
          "time": "20210822 13:24"
        },
        {
