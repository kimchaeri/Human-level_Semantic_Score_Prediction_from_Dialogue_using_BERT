      "utterance": [
        {
          "id": "MDRW2100040358.1.1",
          "form": "아 그 춤이 좀 어려운 춤인가요?",
          "original_form": "아 그 춤이 좀 어려운 춤인가요?",
          "speaker_id": "1",
          "time": "20210904 17:00"
        },
        {
          "id": "MDRW2100040358.1.2",
          "form": "최종 목표라고 하시는 거면..?!",
          "original_form": "최종 목표라고 하시는 거면..?!",
          "speaker_id": "1",
          "time": "20210904 17:00"
        },
        {
          "id": "MDRW2100040358.1.3",
          "form": "네네 댄스브레이크가 좀 어려워 보였어요",
          "original_form": "네네 댄스브레이크가 좀 어려워 보였어요",
          "speaker_id": "2",
          "time": "20210904 17:00"
        },
        {
          "id": "MDRW2100040358.1.4",
          "form": "와 그래도 댄스 레슨이라니 정말 멋져요 ㅜㅜ 운동도 되지요?",
          "original_form": "와 그래도 댄스 레슨이라니 정말 멋져요 ㅜㅜ 운동도 되지요?",
          "speaker_id": "1",
          "time": "20210904 17:02"
        },
        {
          "id": "MDRW2100040358.1.5",
          "form": "네네 운동 많이되고",
          "original_form": "네네 운동 많이되고",
          "speaker_id": "2",
          "time": "20210904 17:02"
        },
        {
          "id": "MDRW2100040358.1.6",
          "form": "저에게 취하는 운동이에요 ㅋㅋㅋㅋ",
          "original_form": "저에게 취하는 운동이에요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:02"
        },
        {
          "id": "MDRW2100040358.1.7",
          "form": "나 멋져 쩔어 !! 이런 느낌",
          "original_form": "나 멋져 쩔어 !! 이런 느낌",
          "speaker_id": "2",
          "time": "20210904 17:02"
        },
        {
          "id": "MDRW2100040358.1.8",
          "form": "ㅋㅋㅋㅋㅋ아 그럴 것 같아요... 전 완전 몸치인데ㅠㅜ 어떻게 시작하셨나요?",
          "original_form": "ㅋㅋㅋㅋㅋ아 그럴 것 같아요... 전 완전 몸치인데ㅠㅜ 어떻게 시작하셨나요?",
          "speaker_id": "1",
          "time": "20210906 15:03"
        },
        {
          "id": "MDRW2100040358.1.9",
          "form": "하고 싶었는데",
          "original_form": "하고 싶었는데",
          "speaker_id": "2",
          "time": "20210906 15:04"
        },
        {
          "id": "MDRW2100040358.1.10",
          "form": "근처에 마침 학원이 있어서",
          "original_form": "근처에 마침 학원이 있어서",
          "speaker_id": "2",
          "time": "20210906 15:04"
        },
        {
          "id": "MDRW2100040358.1.11",
          "form": "용기내어 상담받고 시작했어요",
          "original_form": "용기내어 상담받고 시작했어요",
          "speaker_id": "2",
          "time": "20210906 15:04"
        },
        {
          "id": "MDRW2100040358.1.12",
          "form": "퍙소부터 춤에 관심이 있으셨던거죠?",
          "original_form": "퍙소부터 춤에 관심이 있으셨던거죠?",
          "speaker_id": "1",
          "time": "20210906 15:13"
        },
        {
          "id": "MDRW2100040358.1.13",
          "form": "방송댄스 이런거 배우시는건가요?",
          "original_form": "방송댄스 이런거 배우시는건가요?",
          "speaker_id": "1",
          "time": "20210906 15:13"
        },
        {
          "id": "MDRW2100040358.1.14",
          "form": "네내 맞아요",
          "original_form": "네내 맞아요",
          "speaker_id": "2",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100040358.1.15",
          "form": "아이돌 댄스해요",
          "original_form": "아이돌 댄스해요",
          "speaker_id": "2",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100040358.1.16",
          "form": "댄스 따라 추는거 좋아해서",
          "original_form": "댄스 따라 추는거 좋아해서",
          "speaker_id": "2",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100040358.1.17",
          "form": "가게 되었어요",
          "original_form": "가게 되었어요",
          "speaker_id": "2",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100040358.1.18",
          "form": "우와...앙...",
          "original_form": "우와...앙...",
          "speaker_id": "1",
          "time": "20210906 15:19"
        },
        {
          "id": "MDRW2100040358.1.19",
          "form": "그럼 아이돌 가수도 좋아하시나요?",
          "original_form": "그럼 아이돌 가수도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210906 15:19"
        },
        {
          "id": "MDRW2100040358.1.20",
          "form": "네네 아스트로 투피엠 조아해요",
          "original_form": "네네 아스트로 투피엠 조아해요",
          "speaker_id": "2",
          "time": "20210906 15:27"
        },
        {
          "id": "MDRW2100040358.1.21",
          "form": "아 저번에 문빈이랑 차은우.. 말씀하셨었죠! ㅎㅎㅎ",
          "original_form": "아 저번에 문빈이랑 차은우.. 말씀하셨었죠! ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 15:34"
        },
        {
          "id": "MDRW2100040358.1.22",
          "form": "오오 맞아요 ㅋㅋㅋ",
          "original_form": "오오 맞아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:34"
        },
        {
          "id": "MDRW2100040358.1.23",
          "form": "유투브도 맨날 그것만 보고",
          "original_form": "유투브도 맨날 그것만 보고",
          "speaker_id": "2",
          "time": "20210906 15:34"
        },
        {
          "id": "MDRW2100040358.1.24",
          "form": "누구좋아하세여",
          "original_form": "누구좋아하세여",
          "speaker_id": "2",
          "time": "20210906 15:34"
        },
        {
          "id": "MDRW2100040358.1.25",
          "form": "저는 요새는 오마이걸이랑 몬스타엑스 자주 봐요 ㅋㅋㅋ",
          "original_form": "저는 요새는 오마이걸이랑 몬스타엑스 자주 봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:36"
        },
        {
          "id": "MDRW2100040358.1.26",
          "form": "와아 오마이걸 저도조아해요",
