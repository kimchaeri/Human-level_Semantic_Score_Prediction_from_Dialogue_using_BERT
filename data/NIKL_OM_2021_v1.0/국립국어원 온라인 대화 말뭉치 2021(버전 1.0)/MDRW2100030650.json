      "utterance": [
        {
          "id": "MDRW2100030650.1.1",
          "form": "아 운동하려고 나간거야??",
          "original_form": "아 운동하려고 나간거야??",
          "speaker_id": "2",
          "time": "20210904 11:03"
        },
        {
          "id": "MDRW2100030650.1.2",
          "form": "좋지 요즘 걷기 딱 좋아",
          "original_form": "좋지 요즘 걷기 딱 좋아",
          "speaker_id": "2",
          "time": "20210904 11:03"
        },
        {
          "id": "MDRW2100030650.1.3",
          "form": "응 추워서 딱 운동하기 좋다했지",
          "original_form": "응 추워서 딱 운동하기 좋다했지",
          "speaker_id": "1",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100030650.1.4",
          "form": "근데 안춥지만ㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 안춥지만ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100030650.1.5",
          "form": "그냥 나왔으",
          "original_form": "그냥 나왔으",
          "speaker_id": "1",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100030650.1.6",
          "form": "좋아좋아",
          "original_form": "좋아좋아",
          "speaker_id": "2",
          "time": "20210904 11:06"
        },
        {
          "id": "MDRW2100030650.1.7",
          "form": "나도 요즘 스트레스 받을 때마다 뛰어",
          "original_form": "나도 요즘 스트레스 받을 때마다 뛰어",
          "speaker_id": "2",
          "time": "20210904 11:06"
        },
        {
          "id": "MDRW2100030650.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 11:06"
        },
        {
          "id": "MDRW2100030650.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ엥 난그런건안해",
          "original_form": "ㅋㅋㅋㅋㅋㅋ엥 난그런건안해",
          "speaker_id": "1",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100030650.1.10",
          "form": "그럼 더 나가기 싫던데",
          "original_form": "그럼 더 나가기 싫던데",
          "speaker_id": "1",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100030650.1.11",
          "form": "하여튼 이제 밥먹었는데 또 걸을라고",
          "original_form": "하여튼 이제 밥먹었는데 또 걸을라고",
          "speaker_id": "1",
          "time": "20210904 12:19"
        },
        {
          "id": "MDRW2100030650.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ안 뛰는구나?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ안 뛰는구나?",
          "speaker_id": "2",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100030650.1.13",
          "form": "그래 날씨도 좋은데 산책해 좋지",
          "original_form": "그래 날씨도 좋은데 산책해 좋지",
          "speaker_id": "2",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100030650.1.14",
          "form": "응 절대 어디늦는거 아니면",
          "original_form": "응 절대 어디늦는거 아니면",
          "speaker_id": "1",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100030650.1.15",
          "form": "시장이야 걷기 참 좋네",
          "original_form": "시장이야 걷기 참 좋네",
          "speaker_id": "1",
          "time": "20210904 13:21"
        },
        {
          "id": "MDRW2100030650.1.16",
          "form": "시장이 또 걷기 좋지",
          "original_form": "시장이 또 걷기 좋지",
          "speaker_id": "2",
          "time": "20210904 13:23"
        },
        {
          "id": "MDRW2100030650.1.17",
          "form": "맛있는거 많잖아 두근거리게",
          "original_form": "맛있는거 많잖아 두근거리게",
          "speaker_id": "2",
          "time": "20210904 13:23"
        },
        {
          "id": "MDRW2100030650.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ근데 덥다 걷기 너무",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ근데 덥다 걷기 너무",
          "speaker_id": "1",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100030650.1.19",
          "form": "운동 힘드네",
          "original_form": "운동 힘드네",
          "speaker_id": "1",
          "time": "20210904 13:29"
        },
        {
          "id": "MDRW2100030650.1.20",
          "form": "그럼 힘들지 그리고 많이 걷기도 했어",
          "original_form": "그럼 힘들지 그리고 많이 걷기도 했어",
          "speaker_id": "2",
          "time": "20210904 13:29"
        },
        {
          "id": "MDRW2100030650.1.21",
          "form": "이렇게 오래 할 줄은 몰랐는데 대단해",
          "original_form": "이렇게 오래 할 줄은 몰랐는데 대단해",
          "speaker_id": "2",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100030650.1.22",
          "form": "이제 그만할라고",
          "original_form": "이제 그만할라고",
          "speaker_id": "1",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100030650.1.23",
          "form": "집은 버스타고 가야겠어 ^_^",
          "original_form": "집은 버스타고 가야겠어 ^_^",
          "speaker_id": "1",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100030650.1.24",
          "form": "아주 제대로 했구나 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아주 제대로 했구나 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:31"
        },
        {
          "id": "MDRW2100030650.1.25",
          "form": "그래그래 버스타고 가",
          "original_form": "그래그래 버스타고 가",
          "speaker_id": "2",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100030650.1.26",
          "form": "그치? 첫날부터 무리한듯 역시 운동은 꾸준해야하는데",
          "original_form": "그치? 첫날부터 무리한듯 역시 운동은 꾸준해야하는데",
          "speaker_id": "1",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100030650.1.27",
          "form": "낼 할수있을지모르게써",
          "original_form": "낼 할수있을지모르게써",
          "speaker_id": "1",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100030650.1.28",
          "form": "할 수 있지",
          "original_form": "할 수 있지",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030650.1.29",
          "form": "응원해 응원!!ㅋㅋㅋㅋㅋ",
          "original_form": "응원해 응원!!ㅋㅋㅋㅋㅋ",
