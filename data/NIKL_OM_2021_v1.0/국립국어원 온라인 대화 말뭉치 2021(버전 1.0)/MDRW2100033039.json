      "utterance": [
        {
          "id": "MDRW2100033039.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100033039.1.2",
          "form": "재테크하시나요?",
          "original_form": "재테크하시나요?",
          "speaker_id": "1",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100033039.1.3",
          "form": "네 주린이예요ㅋㅋ 재테크 하시는거 있으세요?",
          "original_form": "네 주린이예요ㅋㅋ 재테크 하시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100033039.1.4",
          "form": "저는 코인이랑 주식이요",
          "original_form": "저는 코인이랑 주식이요",
          "speaker_id": "1",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100033039.1.5",
          "form": "지분 조각 투자도 해보셨어요?",
          "original_form": "지분 조각 투자도 해보셨어요?",
          "speaker_id": "1",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100033039.1.6",
          "form": "요즘 많던데",
          "original_form": "요즘 많던데",
          "speaker_id": "1",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100033039.1.7",
          "form": "지분 조각 투자 처음 들어봤어요 아직 주린이라서ㅠㅠ",
          "original_form": "지분 조각 투자 처음 들어봤어요 아직 주린이라서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100033039.1.8",
          "form": "예를 들어서 곡의 저작권에 일부 투자하는거에요",
          "original_form": "예를 들어서 곡의 저작권에 일부 투자하는거에요",
          "speaker_id": "1",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100033039.1.9",
          "form": "그래서 저작권료가 나오면 자기 지분율만큼 저작권료를 받고",
          "original_form": "그래서 저작권료가 나오면 자기 지분율만큼 저작권료를 받고",
          "speaker_id": "1",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100033039.1.10",
          "form": "또는 부동산에 일정 권리만큼만 투자해서 임대 소득이 나오면 또 자기 지분율만큼 받고",
          "original_form": "또는 부동산에 일정 권리만큼만 투자해서 임대 소득이 나오면 또 자기 지분율만큼 받고",
          "speaker_id": "1",
          "time": "20210906 14:35"
        },
        {
          "id": "MDRW2100033039.1.11",
          "form": "주식이랑 같아요 결국",
          "original_form": "주식이랑 같아요 결국",
          "speaker_id": "1",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100033039.1.12",
          "form": "주식 수익률은 어때요?",
          "original_form": "주식 수익률은 어때요?",
          "speaker_id": "1",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100033039.1.13",
          "form": "아 저 저작권료 얘기는 들어봤어요! 요새 티비 광고도 해가지고ㅋㅋ",
          "original_form": "아 저 저작권료 얘기는 들어봤어요! 요새 티비 광고도 해가지고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:36"
        },
        {
          "id": "MDRW2100033039.1.14",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100033039.1.15",
          "form": "주식은 창과 방패의 싸움중이요...ㅋㅋㅋ어디서 수익이 나면 어디서 잃고 있어서",
          "original_form": "주식은 창과 방패의 싸움중이요...ㅋㅋㅋ어디서 수익이 나면 어디서 잃고 있어서",
          "speaker_id": "2",
          "time": "20210906 15:10"
        },
        {
          "id": "MDRW2100033039.1.16",
          "form": "코인하신다던데 코인 어렵지 않나요?",
          "original_form": "코인하신다던데 코인 어렵지 않나요?",
          "speaker_id": "2",
          "time": "20210906 15:10"
        },
        {
          "id": "MDRW2100033039.1.17",
          "form": "코인은 잘 모르겠어서 기계적으로 샀는데",
          "original_form": "코인은 잘 모르겠어서 기계적으로 샀는데",
          "speaker_id": "1",
          "time": "20210906 15:19"
        },
        {
          "id": "MDRW2100033039.1.18",
          "form": "지금 상승장이라 그런지 수익 조금 나고 있어요",
          "original_form": "지금 상승장이라 그런지 수익 조금 나고 있어요",
          "speaker_id": "1",
          "time": "20210906 15:20"
        },
        {
          "id": "MDRW2100033039.1.19",
          "form": "제 대학친구는 회사 때려치고 전업으로 코인 투자하더니 돈 엄청벌었어요",
          "original_form": "제 대학친구는 회사 때려치고 전업으로 코인 투자하더니 돈 엄청벌었어요",
          "speaker_id": "1",
          "time": "20210906 15:20"
        },
        {
          "id": "MDRW2100033039.1.20",
          "form": "와 인터넷에서 말로만 듣던 분이 주변에 계셨네요ㅋㅋ 전 코인은 아직 못하겠더라구요",
          "original_form": "와 인터넷에서 말로만 듣던 분이 주변에 계셨네요ㅋㅋ 전 코인은 아직 못하겠더라구요",
          "speaker_id": "2",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100033039.1.21",
          "form": "혹시 해외주식도 하고 계세요?",
          "original_form": "혹시 해외주식도 하고 계세요?",
          "speaker_id": "2",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100033039.1.22",
          "form": "네 해외주식도 하고 있어요",
          "original_form": "네 해외주식도 하고 있어요",
          "speaker_id": "1",
          "time": "20210906 15:36"
        },
        {
          "id": "MDRW2100033039.1.23",
          "form": "그냥 사고 묵묵히 기다리고 있어요",
          "original_form": "그냥 사고 묵묵히 기다리고 있어요",
          "speaker_id": "1",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100033039.1.24",
          "form": "해외주식 하세요?",
          "original_form": "해외주식 하세요?",
          "speaker_id": "1",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100033039.1.25",
          "form": "해외주식 발만 담갔어요...ㅋㅋ 진짜 조금 사놨는데 수익률이 좋더라구요..맨날 조금 투자한데는 많이 오르고 많이 산 데는 조금 오르고",
          "original_form": "해외주식 발만 담갔어요...ㅋㅋ 진짜 조금 사놨는데 수익률이 좋더라구요..맨날 조금 투자한데는 많이 오르고 많이 산 데는 조금 오르고",
          "speaker_id": "2",
          "time": "20210906 15:47"
        },
        {
          "id": "MDRW2100033039.1.26",
          "form": "그래도 오르는게 어디에요",
          "original_form": "그래도 오르는게 어디에요",
          "speaker_id": "1",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100033039.1.27",
          "form": "수익만나도 대단한거지",
          "original_form": "수익만나도 대단한거지",
          "speaker_id": "1",
          "time": "20210906 15:50"
        }
      ]
