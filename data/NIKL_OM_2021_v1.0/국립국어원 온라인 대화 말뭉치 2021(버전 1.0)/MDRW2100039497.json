      "utterance": [
        {
          "id": "MDRW2100039497.1.1",
          "form": "스팸계란볶음밥도 맛있져",
          "original_form": "스팸계란볶음밥도 맛있져",
          "speaker_id": "1",
          "time": "20210903 18:19"
        },
        {
          "id": "MDRW2100039497.1.2",
          "form": "맞아요 스팸들어가면 머든 맛나지죠",
          "original_form": "맞아요 스팸들어가면 머든 맛나지죠",
          "speaker_id": "2",
          "time": "20210903 18:20"
        },
        {
          "id": "MDRW2100039497.1.3",
          "form": "부대찌개도요ㅋ",
          "original_form": "부대찌개도요ㅋ",
          "speaker_id": "1",
          "time": "20210903 18:20"
        },
        {
          "id": "MDRW2100039497.1.4",
          "form": "계란국에 스팸김치볶음밥 나름 잘조합이 되네요",
          "original_form": "계란국에 스팸김치볶음밥 나름 잘조합이 되네요",
          "speaker_id": "2",
          "time": "20210903 18:20"
        },
        {
          "id": "MDRW2100039497.1.5",
          "form": "부대엔 몽창 집어넣어야쥬 스팸",
          "original_form": "부대엔 몽창 집어넣어야쥬 스팸",
          "speaker_id": "2",
          "time": "20210903 18:21"
        },
        {
          "id": "MDRW2100039497.1.6",
          "form": "계란국 좋네요",
          "original_form": "계란국 좋네요",
          "speaker_id": "1",
          "time": "20210903 18:21"
        },
        {
          "id": "MDRW2100039497.1.7",
          "form": "국물어떻게 내세요",
          "original_form": "국물어떻게 내세요",
          "speaker_id": "1",
          "time": "20210903 18:21"
        },
        {
          "id": "MDRW2100039497.1.8",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210903 18:21"
        },
        {
          "id": "MDRW2100039497.1.9",
          "form": "저는 멸치넣고 이렇게 안하고",
          "original_form": "저는 멸치넣고 이렇게 안하고",
          "speaker_id": "2",
          "time": "20210903 18:22"
        },
        {
          "id": "MDRW2100039497.1.10",
          "form": "간편하게 씨제이에서 나오는 산들애",
          "original_form": "간편하게 씨제이에서 나오는 산들애",
          "speaker_id": "2",
          "time": "20210903 18:22"
        },
        {
          "id": "MDRW2100039497.1.11",
          "form": "멸치육수 넣어서 끓여요",
          "original_form": "멸치육수 넣어서 끓여요",
          "speaker_id": "2",
          "time": "20210903 18:22"
        },
        {
          "id": "MDRW2100039497.1.12",
          "form": "오 그런거 좋아요ㅋㅋ",
          "original_form": "오 그런거 좋아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:22"
        },
        {
          "id": "MDRW2100039497.1.13",
          "form": "육수내기 힘들어요",
          "original_form": "육수내기 힘들어요",
          "speaker_id": "1",
          "time": "20210903 18:23"
        },
        {
          "id": "MDRW2100039497.1.14",
          "form": "파송송넣고",
          "original_form": "파송송넣고",
          "speaker_id": "2",
          "time": "20210903 18:23"
        },
        {
          "id": "MDRW2100039497.1.15",
          "form": "끓으면 계란풀고 ㅋ",
          "original_form": "끓으면 계란풀고 ㅋ",
          "speaker_id": "2",
          "time": "20210903 18:23"
        },
        {
          "id": "MDRW2100039497.1.16",
          "form": "국간장 한스푼",
          "original_form": "국간장 한스푼",
          "speaker_id": "2",
          "time": "20210903 18:23"
        },
        {
          "id": "MDRW2100039497.1.17",
          "form": "전지금 계란국 계란찜 고민중이요ㅋ",
          "original_form": "전지금 계란국 계란찜 고민중이요ㅋ",
          "speaker_id": "1",
          "time": "20210903 18:24"
        },
        {
          "id": "MDRW2100039497.1.18",
          "form": "쭈꾸미에 어울리는거요?",
          "original_form": "쭈꾸미에 어울리는거요?",
          "speaker_id": "2",
          "time": "20210903 18:24"
        },
        {
          "id": "MDRW2100039497.1.19",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210903 18:24"
        },
        {
          "id": "MDRW2100039497.1.20",
          "form": "역시 찜일까요",
          "original_form": "역시 찜일까요",
          "speaker_id": "1",
          "time": "20210903 18:25"
        },
        {
          "id": "MDRW2100039497.1.21",
          "form": "찜일것같은데요 ㅎ",
          "original_form": "찜일것같은데요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 18:25"
        },
        {
          "id": "MDRW2100039497.1.22",
