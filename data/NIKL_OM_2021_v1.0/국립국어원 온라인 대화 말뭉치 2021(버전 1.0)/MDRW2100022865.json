      "utterance": [
        {
          "id": "MDRW2100022865.1.1",
          "form": "동유렂은 풍경이 영화 그자체죠?",
          "original_form": "동유렂은 풍경이 영화 그자체죠?",
          "speaker_id": "1",
          "time": "20210829 18:42"
        },
        {
          "id": "MDRW2100022865.1.2",
          "form": "네ㅎ 영화배경같앗어요",
          "original_form": "네ㅎ 영화배경같앗어요",
          "speaker_id": "2",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100022865.1.3",
          "form": "영화는 어떤 장르자주보세요?",
          "original_form": "영화는 어떤 장르자주보세요?",
          "speaker_id": "1",
          "time": "20210829 18:54"
        },
        {
          "id": "MDRW2100022865.1.4",
          "form": "액션이요ㅎㅎ",
          "original_form": "액션이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 18:54"
        },
        {
          "id": "MDRW2100022865.1.5",
          "form": "분노의질주나 본시리즈같은거",
          "original_form": "분노의질주나 본시리즈같은거",
          "speaker_id": "2",
          "time": "20210829 18:54"
        },
        {
          "id": "MDRW2100022865.1.6",
          "form": "최근 나온거 보셧어여? 분노질주",
          "original_form": "최근 나온거 보셧어여? 분노질주",
          "speaker_id": "1",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022865.1.7",
          "form": "넵 봣어요",
          "original_form": "넵 봣어요",
          "speaker_id": "2",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022865.1.8",
          "form": "스케일이 진짜... 이젠 우주까지 진출",
          "original_form": "스케일이 진짜... 이젠 우주까지 진출",
          "speaker_id": "1",
          "time": "20210829 18:59"
        },
        {
          "id": "MDRW2100022865.1.9",
          "form": "ㅎㅎ그니까요",
          "original_form": "ㅎㅎ그니까요",
          "speaker_id": "2",
          "time": "20210829 19:00"
        },
        {
          "id": "MDRW2100022865.1.10",
          "form": "다음편은 더 이상 갈곳도 엄는데..",
          "original_form": "다음편은 더 이상 갈곳도 엄는데..",
          "speaker_id": "1",
          "time": "20210829 19:01"
        },
        {
          "id": "MDRW2100022865.1.11",
          "form": "ㅋㅋ이제갈때까지 갓죠",
          "original_form": "ㅋㅋ이제갈때까지 갓죠",
          "speaker_id": "2",
          "time": "20210829 19:09"
        },
        {
          "id": "MDRW2100022865.1.12",
          "form": "본시리즈는 다시 안나올려나요?",
          "original_form": "본시리즈는 다시 안나올려나요?",
          "speaker_id": "1",
          "time": "20210829 19:11"
        },
        {
          "id": "MDRW2100022865.1.13",
          "form": "옛날만큼 잼잇지도 않은거같아요ㅎ",
          "original_form": "옛날만큼 잼잇지도 않은거같아요ㅎ",
          "speaker_id": "2",
          "time": "20210829 19:19"
        },
        {
          "id": "MDRW2100022865.1.14",
          "form": "분노시리즈요?",
          "original_form": "분노시리즈요?",
          "speaker_id": "1",
          "time": "20210829 19:21"
        },
        {
          "id": "MDRW2100022865.1.15",
          "form": "본이요ㅎ",
          "original_form": "본이요ㅎ",
          "speaker_id": "2",
          "time": "20210829 19:30"
        }
      ]
