      "utterance": [
        {
          "id": "MDRW2100038508.1.1",
          "form": "이학년 때부터 영어학원 다닐 거야??",
          "original_form": "이학년 때부터 영어학원 다닐 거야??",
          "speaker_id": "2",
          "time": "20210902 14:51"
        },
        {
          "id": "MDRW2100038508.1.2",
          "form": "경미언니도 가기로 했거든",
          "original_form": "경미언니도 가기로 했거든",
          "speaker_id": "2",
          "time": "20210902 14:51"
        },
        {
          "id": "MDRW2100038508.1.3",
          "form": "얘기 들어보고 싶으면 같이 가도 괜찮아",
          "original_form": "얘기 들어보고 싶으면 같이 가도 괜찮아",
          "speaker_id": "2",
          "time": "20210902 14:51"
        },
        {
          "id": "MDRW2100038508.1.4",
          "form": "언제요??",
          "original_form": "언제요??",
          "speaker_id": "1",
          "time": "20210902 14:51"
        },
        {
          "id": "MDRW2100038508.1.5",
          "form": "영어학원??",
          "original_form": "영어학원??",
          "speaker_id": "1",
          "time": "20210902 14:52"
        },
        {
          "id": "MDRW2100038508.1.6",
          "form": "2학년부턴 다녀야 안되긋어요~",
          "original_form": "2학년부턴 다녀야 안되긋어요~",
          "speaker_id": "1",
          "time": "20210902 14:52"
        },
        {
          "id": "MDRW2100038508.1.7",
          "form": "어 영어학원",
          "original_form": "어 영어학원",
          "speaker_id": "2",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100038508.1.8",
          "form": "이학기간담회를 이번 주 토요일 2시에 하거든",
          "original_form": "이학기간담회를 이번 주 토요일 2시에 하거든",
          "speaker_id": "2",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100038508.1.9",
          "form": "니 시간 되면 같이 가자",
          "original_form": "니 시간 되면 같이 가자",
          "speaker_id": "2",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100038508.1.10",
          "form": "수 엄마랑 온유 언니랑 내 이렇게 셋이 간다",
          "original_form": "수 엄마랑 온유 언니랑 내 이렇게 셋이 간다",
          "speaker_id": "2",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100038508.1.11",
          "form": "언니..나 요번주 토욜에 일해요ㅠ",
          "original_form": "언니..나 요번주 토욜에 일해요ㅠ",
          "speaker_id": "1",
          "time": "20210902 15:27"
        },
        {
          "id": "MDRW2100038508.1.12",
          "form": "35000원 벌러가는데..간담회 안가면",
          "original_form": "35000원 벌러가는데..간담회 안가면",
          "speaker_id": "1",
          "time": "20210902 15:27"
        },
        {
          "id": "MDRW2100038508.1.13",
          "form": "수업 못들어요??",
          "original_form": "수업 못들어요??",
          "speaker_id": "1",
          "time": "20210902 15:27"
        },
        {
          "id": "MDRW2100038508.1.14",
          "form": "아니 수업 들을 수 있지ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니 수업 들을 수 있지ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100038508.1.15",
          "form": "간담회 가서 선생님 어떤지 한번 보라고",
          "original_form": "간담회 가서 선생님 어떤지 한번 보라고",
          "speaker_id": "2",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100038508.1.16",
          "form": "그래서 같이 가자 한 거지",
          "original_form": "그래서 같이 가자 한 거지",
          "speaker_id": "2",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100038508.1.17",
          "form": "내년에 진짜 거기 갈 거면",
          "original_form": "내년에 진짜 거기 갈 거면",
          "speaker_id": "2",
          "time": "20210902 15:40"
        },
        {
          "id": "MDRW2100038508.1.18",
          "form": "선생님한테 미리 얘기해 놓는 게 나을 것 같아서",
          "original_form": "선생님한테 미리 얘기해 놓는 게 나을 것 같아서",
          "speaker_id": "2",
          "time": "20210902 15:40"
        },
        {
          "id": "MDRW2100038508.1.19",
          "form": "글쵸 쌤 한번 보믄 좋긴한데~",
          "original_form": "글쵸 쌤 한번 보믄 좋긴한데~",
          "speaker_id": "1",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100038508.1.20",
          "form": "난 따로 한번 가봐야 겠어요 ㅋㅋㅋㅋ",
          "original_form": "난 따로 한번 가봐야 겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100038508.1.21",
          "form": "하필...요번 토요일 ㅠㅠ",
          "original_form": "하필...요번 토요일 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100038508.1.22",
          "form": "그래 쌤 간담회 아니더라도",
          "original_form": "그래 쌤 간담회 아니더라도",
          "speaker_id": "2",
          "time": "20210902 15:43"
        },
        {
          "id": "MDRW2100038508.1.23",
          "form": "시간예약하면 만날수 있으니",
          "original_form": "시간예약하면 만날수 있으니",
          "speaker_id": "2",
          "time": "20210902 15:43"
        },
        {
          "id": "MDRW2100038508.1.24",
          "form": "니 일 몇시에 끝나는데??",
          "original_form": "니 일 몇시에 끝나는데??",
          "speaker_id": "2",
          "time": "20210902 15:43"
        },
        {
          "id": "MDRW2100038508.1.25",
          "form": "난 힐데스, 반도안에서 다 해결해야됨 ㅋㅋㅋㅋ",
          "original_form": "난 힐데스, 반도안에서 다 해결해야됨 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:43"
        },
        {
          "id": "MDRW2100038508.1.26",
          "form": "토욜은 세시요ㅠ",
          "original_form": "토욜은 세시요ㅠ",
          "speaker_id": "1",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100038508.1.27",
          "form": "늘 두신데 ㅋㅋㅋㅋ",
          "original_form": "늘 두신데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100038508.1.28",
          "form": "아 아쉽네이 ㅠ",
          "original_form": "아 아쉽네이 ㅠ",
          "speaker_id": "2",
          "time": "20210902 15:54"
        },
        {
          "id": "MDRW2100038508.1.29",
          "form": "그래 거기 힐데스라 좋다 ㅎㅎ",
          "original_form": "그래 거기 힐데스라 좋다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 15:55"
        },
        {
          "id": "MDRW2100038508.1.30",
          "form": "언니 나 피곤해 죽을 거 같은데",
          "original_form": "언니 나 피곤해 죽을 거 같은데",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100038508.1.31",
          "form": "잠이 안 와요ㅠ",
          "original_form": "잠이 안 와요ㅠ",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100038508.1.32",
          "form": "이거 진짜 화 이자 때문 아니에요ㅋㅋㅋ",
          "original_form": "이거 진짜 화 이자 때문 아니에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100038508.1.33",
          "form": "웁스 우짜노",
          "original_form": "웁스 우짜노",
          "speaker_id": "2",
          "time": "20210903 07:34"
        },
        {
          "id": "MDRW2100038508.1.34",
          "form": "난 어제 애들이랑 뻗었다",
          "original_form": "난 어제 애들이랑 뻗었다",
          "speaker_id": "2",
          "time": "20210903 07:34"
        },
        {
          "id": "MDRW2100038508.1.35",
          "form": "비록 화장실 간다고 서너번 깼지만 그래도 일찍 자니 좀 낫네",
          "original_form": "비록 화장실 간다고 서너번 깼지만 그래도 일찍 자니 좀 낫네",
          "speaker_id": "2",
          "time": "20210903 07:34"
        },
        {
          "id": "MDRW2100038508.1.36",
          "form": "좀 잤나??",
          "original_form": "좀 잤나??",
          "speaker_id": "2",
          "time": "20210903 07:34"
        },
        {
          "id": "MDRW2100038508.1.37",
          "form": "그러고 잤어요 언니ㅋㅋㅋㅋㅋ",
          "original_form": "그러고 잤어요 언니ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 07:50"
        },
        {
          "id": "MDRW2100038508.1.38",
          "form": "열한시쯤에는 잠드네요ㅋㅋㅋ",
          "original_form": "열한시쯤에는 잠드네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 07:50"
        },
        {
          "id": "MDRW2100038508.1.39",
          "form": "화이자 덕에 밤 시간이 생겼어요",
          "original_form": "화이자 덕에 밤 시간이 생겼어요",
          "speaker_id": "1",
          "time": "20210903 07:50"
        },
        {
          "id": "MDRW2100038508.1.40",
          "form": "ㅋㅋㅋㅋㅋㄱㅋㄱ 다행인거가 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㄱㅋㄱ 다행인거가 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 08:30"
        },
        {
          "id": "MDRW2100038508.1.41",
          "form": "난 이제 출근한다",
          "original_form": "난 이제 출근한다",
          "speaker_id": "2",
          "time": "20210903 08:30"
        },
        {
          "id": "MDRW2100038508.1.42",
          "form": "그렇네여 ㅋㅋㅋㅋㅋ밤 애들자고 티비도 보고",
          "original_form": "그렇네여 ㅋㅋㅋㅋㅋ밤 애들자고 티비도 보고",
          "speaker_id": "1",
          "time": "20210903 08:46"
        },
        {
          "id": "MDRW2100038508.1.43",
          "form": "맛있눈것도 먹고 좋구만요 ㅋㅋㅋㅋ",
          "original_form": "맛있눈것도 먹고 좋구만요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 08:46"
        },
        {
          "id": "MDRW2100038508.1.44",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱ 또 뭐먹었노??",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱ 또 뭐먹었노??",
          "speaker_id": "2",
          "time": "20210903 09:34"
        },
        {
          "id": "MDRW2100038508.1.45",
          "form": "다이어트한다했찮아",
          "original_form": "다이어트한다했찮아",
          "speaker_id": "2",
          "time": "20210903 09:34"
        },
        {
          "id": "MDRW2100038508.1.46",
          "form": "과자 몇개 먹었어요ㅋㅋㅋㅋㅋㅋㅋ 애들몰래 ㅋㅋㅋㅋ",
          "original_form": "과자 몇개 먹었어요ㅋㅋㅋㅋㅋㅋㅋ 애들몰래 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 09:35"
        },
        {
          "id": "MDRW2100038508.1.47",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱ 잘했다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱ 잘했다",
          "speaker_id": "2",
          "time": "20210903 09:44"
        },
        {
          "id": "MDRW2100038508.1.48",
          "form": "몰래먹는 맛이 있지\r\nㅋㅋㅋㅋㅋㄱ",
          "original_form": "몰래먹는 맛이 있지\r\nㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210903 09:44"
        },
        {
          "id": "MDRW2100038508.1.49",
          "form": "아침에 코코볼 먹고 나옴",
          "original_form": "아침에 코코볼 먹고 나옴",
          "speaker_id": "2",
          "time": "20210903 09:44"
        },
        {
          "id": "MDRW2100038508.1.50",
          "form": "투가 \r\n엄마 뭐라도 먹고 가 라고 ㅋㅋㅋㄱ",
          "original_form": "투가 \r\n엄마 뭐라도 먹고 가 라고 ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210903 09:44"
        },
        {
          "id": "MDRW2100038508.1.51",
          "form": "맨날 할머니가 하는소리 ㅋㅋㅋ 따라함 ㅋㅋㅋㅋ",
          "original_form": "맨날 할머니가 하는소리 ㅋㅋㅋ 따라함 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:44"
        },
        {
          "id": "MDRW2100038508.1.52",
          "form": "ㅋㅋㅋㅋㅋ뭐라도 먹어야 든든함 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ뭐라도 먹어야 든든함 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100038508.1.53",
          "form": "나이들수록 뭐라도 묵어야함 ㅋㅋㅋ",
          "original_form": "나이들수록 뭐라도 묵어야함 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100038508.1.54",
          "form": "둘째 왔나??",
          "original_form": "둘째 왔나??",
          "speaker_id": "2",
          "time": "20210903 16:39"
        },
        {
          "id": "MDRW2100038508.1.55",
          "form": "난 우리 투 언어 센터 왔음",
          "original_form": "난 우리 투 언어 센터 왔음",
          "speaker_id": "2",
          "time": "20210903 16:39"
        },
        {
          "id": "MDRW2100038508.1.56",
          "form": "일주일에 두 번 다니는데",
          "original_form": "일주일에 두 번 다니는데",
          "speaker_id": "2",
          "time": "20210903 16:39"
