      "utterance": [
        {
          "id": "MDRW2100022469.1.1",
          "form": "책 좋아하시나요",
          "original_form": "책 좋아하시나요",
          "speaker_id": "1",
          "time": "20210829 14:31"
        },
        {
          "id": "MDRW2100022469.1.2",
          "form": "좋아는 하는데 많이 읽는것 같지는 않아요 ㅋㅋ 책많이 읽으세요?",
          "original_form": "좋아는 하는데 많이 읽는것 같지는 않아요 ㅋㅋ 책많이 읽으세요?",
          "speaker_id": "2",
          "time": "20210829 14:32"
        },
        {
          "id": "MDRW2100022469.1.3",
          "form": "저도 비슷해요\" 사는거 빌리는건 좋은데...ㅋ",
          "original_form": "저도 비슷해요\" 사는거 빌리는건 좋은데...ㅋ",
          "speaker_id": "1",
          "time": "20210829 14:34"
        },
        {
          "id": "MDRW2100022469.1.4",
          "form": "최근에 뭐 읽으셨어요?",
          "original_form": "최근에 뭐 읽으셨어요?",
          "speaker_id": "2",
          "time": "20210829 14:35"
        },
        {
          "id": "MDRW2100022469.1.5",
          "form": "아 이번에 양희은 책을 빌려왔어요 인기많더라구요",
          "original_form": "아 이번에 양희은 책을 빌려왔어요 인기많더라구요",
          "speaker_id": "1",
          "time": "20210829 14:35"
        },
        {
          "id": "MDRW2100022469.1.6",
          "form": "주로 어떤 장르 읽으세요?",
          "original_form": "주로 어떤 장르 읽으세요?",
          "speaker_id": "2",
          "time": "20210829 14:36"
        },
        {
          "id": "MDRW2100022469.1.7",
          "form": "저는 에세이나 육아서..자기계발서..이런거 읽어요",
          "original_form": "저는 에세이나 육아서..자기계발서..이런거 읽어요",
          "speaker_id": "1",
          "time": "20210829 15:43"
        },
        {
          "id": "MDRW2100022469.1.8",
          "form": "아 저는 소설 주로 읽어요! ㅋㅋ",
          "original_form": "아 저는 소설 주로 읽어요! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:43"
        },
        {
          "id": "MDRW2100022469.1.9",
          "form": "좋아하는 작가 있나요?",
          "original_form": "좋아하는 작가 있나요?",
          "speaker_id": "1",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100022469.1.10",
          "form": "특별히 좋아하는 작가가 있는건 아닌거 같아요 ㅋㅋ 정유정작가거 재밌게 읽었어요!",
          "original_form": "특별히 좋아하는 작가가 있는건 아닌거 같아요 ㅋㅋ 정유정작가거 재밌게 읽었어요!",
          "speaker_id": "2",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100022469.1.11",
          "form": "아 정유정 작가는 엄청 유명하죠 안읽었지만 알아요 ㅎ",
          "original_form": "아 정유정 작가는 엄청 유명하죠 안읽었지만 알아요 ㅎ",
          "speaker_id": "1",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100022469.1.12",
          "form": "최근에 읽은 책은 천개의 파랑이라는 책이에요!",
          "original_form": "최근에 읽은 책은 천개의 파랑이라는 책이에요!",
          "speaker_id": "2",
          "time": "20210829 15:53"
        },
        {
          "id": "MDRW2100022469.1.13",
          "form": "처음 듣네요~ 소설인거죠?",
          "original_form": "처음 듣네요~ 소설인거죠?",
          "speaker_id": "1",
          "time": "20210829 16:02"
        },
        {
          "id": "MDRW2100022469.1.14",
          "form": "네네 소설이에요 제가 아이가 있다면 읽히고 싶어요 ㅋㅋ",
          "original_form": "네네 소설이에요 제가 아이가 있다면 읽히고 싶어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:05"
        },
        {
          "id": "MDRW2100022469.1.15",
          "form": "어머 어떤 이유에서요?",
          "original_form": "어머 어떤 이유에서요?",
          "speaker_id": "1",
          "time": "20210829 16:12"
        },
        {
          "id": "MDRW2100022469.1.16",
          "form": "천개의 파랑 내용이 따듯해요!",
          "original_form": "천개의 파랑 내용이 따듯해요!",
          "speaker_id": "2",
          "time": "20210829 16:19"
        }
      ]
