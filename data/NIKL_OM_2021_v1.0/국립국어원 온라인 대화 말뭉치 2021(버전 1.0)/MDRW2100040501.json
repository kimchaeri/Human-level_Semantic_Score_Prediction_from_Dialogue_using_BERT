      "utterance": [
        {
          "id": "MDRW2100040501.1.1",
          "form": "갔다 왔지",
          "original_form": "갔다 왔지",
          "speaker_id": "2",
          "time": "20210904 22:03"
        },
        {
          "id": "MDRW2100040501.1.2",
          "form": "어디로 갔어?",
          "original_form": "어디로 갔어?",
          "speaker_id": "1",
          "time": "20210904 22:03"
        },
        {
          "id": "MDRW2100040501.1.3",
          "form": "앞 산에 갔다 왔지",
          "original_form": "앞 산에 갔다 왔지",
          "speaker_id": "2",
          "time": "20210904 22:03"
        },
        {
          "id": "MDRW2100040501.1.4",
          "form": "그렇군~ 거기 딱 좋더라",
          "original_form": "그렇군~ 거기 딱 좋더라",
          "speaker_id": "1",
          "time": "20210904 22:03"
        },
        {
          "id": "MDRW2100040501.1.5",
          "form": "정상까지 가야 된다",
          "original_form": "정상까지 가야 된다",
          "speaker_id": "2",
          "time": "20210904 22:03"
        },
        {
          "id": "MDRW2100040501.1.6",
          "form": "웅웅 딱 운동되는 높이 ㅋ",
          "original_form": "웅웅 딱 운동되는 높이 ㅋ",
          "speaker_id": "1",
          "time": "20210904 22:03"
        },
        {
          "id": "MDRW2100040501.1.7",
          "form": "강아지는 조금 힘드려나",
          "original_form": "강아지는 조금 힘드려나",
          "speaker_id": "2",
          "time": "20210904 22:03"
        },
        {
          "id": "MDRW2100040501.1.8",
          "form": "그럴수도있을듯..",
          "original_form": "그럴수도있을듯..",
          "speaker_id": "1",
          "time": "20210904 22:03"
        },
        {
          "id": "MDRW2100040501.1.9",
          "form": "좀 힘들어 하긴 한다",
          "original_form": "좀 힘들어 하긴 한다",
          "speaker_id": "2",
          "time": "20210904 22:04"
        },
        {
          "id": "MDRW2100040501.1.10",
          "form": "그럼 좀만 가야지",
          "original_form": "그럼 좀만 가야지",
          "speaker_id": "1",
          "time": "20210904 22:04"
        },
        {
          "id": "MDRW2100040501.1.11",
          "form": "가다 보면 욕심이 난다",
          "original_form": "가다 보면 욕심이 난다",
          "speaker_id": "2",
          "time": "20210904 22:04"
        },
        {
          "id": "MDRW2100040501.1.12",
          "form": "안된당~",
          "original_form": "안된당~",
          "speaker_id": "1",
          "time": "20210904 22:04"
        },
        {
          "id": "MDRW2100040501.1.13",
          "form": "그래 무리하면 안 되지",
          "original_form": "그래 무리하면 안 되지",
          "speaker_id": "2",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100040501.1.14",
          "form": "웅웅 조금만 다녀유 ㅎㅎ",
          "original_form": "웅웅 조금만 다녀유 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100040501.1.15",
          "form": "알았다",
          "original_form": "알았다",
          "speaker_id": "2",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100040501.1.16",
          "form": "강아지 새로 산 칫솔 좋은듯",
          "original_form": "강아지 새로 산 칫솔 좋은듯",
          "speaker_id": "1",
