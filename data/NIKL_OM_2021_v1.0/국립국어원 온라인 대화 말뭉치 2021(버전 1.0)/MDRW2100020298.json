        {
          "id": "MDRW2100020298.1.2",
          "form": "요즘은 이비에스가 재밌어",
          "original_form": "요즘은 이비에스가 재밌어",
          "speaker_id": "2",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020298.1.3",
          "form": "자이언트 펭티비 보니",
          "original_form": "자이언트 펭티비 보니",
          "speaker_id": "1",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020298.1.4",
          "form": "너가 전에 소개시켜준 이후로 계속해서 보고 있어",
          "original_form": "너가 전에 소개시켜준 이후로 계속해서 보고 있어",
          "speaker_id": "2",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020298.1.5",
          "form": "우리 펭 수 너무 귀엽지",
          "original_form": "우리 펭 수 너무 귀엽지",
          "speaker_id": "1",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020298.1.6",
          "form": "펭 수는 왕 커서 왕 귀여워",
          "original_form": "펭 수는 왕 커서 왕 귀여워",
          "speaker_id": "2",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020298.1.7",
          "form": "펭수 거 프로그램 되게 유익하기도 해",
          "original_form": "펭수 거 프로그램 되게 유익하기도 해",
          "speaker_id": "1",
          "time": "20210828 02:23"
        },
        {
          "id": "MDRW2100020298.1.8",
          "form": "행수는 왜 못하는 게 없지",
          "original_form": "행수는 왜 못하는 게 없지",
          "speaker_id": "2",
          "time": "20210828 02:23"
        },
        {
          "id": "MDRW2100020298.1.9",
          "form": "우리 펭 수는 다재다능하기도 하지",
          "original_form": "우리 펭 수는 다재다능하기도 하지",
          "speaker_id": "1",
          "time": "20210828 02:24"
        },
        {
          "id": "MDRW2100020298.1.10",
          "form": "펭귄 치고 귀여워",
          "original_form": "펭귄 치고 귀여워",
          "speaker_id": "2",
          "time": "20210828 02:24"
        },
        {
          "id": "MDRW2100020298.1.11",
          "form": "요즘에 제일 귀여울걸 조류 중에",
          "original_form": "요즘에 제일 귀여울걸 조류 중에",
