      "utterance": [
        {
          "id": "MDRW2100024517.1.1",
          "form": "공모주가",
          "original_form": "공모주가",
          "speaker_id": "1",
          "time": "20210831 12:49"
        },
        {
          "id": "MDRW2100024517.1.2",
          "form": "나도 자세히는 잘 모르는데",
          "original_form": "나도 자세히는 잘 모르는데",
          "speaker_id": "1",
          "time": "20210831 12:49"
        },
        {
          "id": "MDRW2100024517.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ 드디어 메타로 왔구만",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ 드디어 메타로 왔구만",
          "speaker_id": "2",
          "time": "20210831 12:49"
        },
        {
          "id": "MDRW2100024517.1.4",
          "form": "그 주식의 값을 정하는",
          "original_form": "그 주식의 값을 정하는",
          "speaker_id": "1",
          "time": "20210831 12:49"
        },
        {
          "id": "MDRW2100024517.1.5",
          "form": "그런건것같더라고",
          "original_form": "그런건것같더라고",
          "speaker_id": "1",
          "time": "20210831 12:49"
        },
        {
          "id": "MDRW2100024517.1.6",
          "form": "공모가 측정하고 사람들이 막 사들이거나 팔거나 하면",
          "original_form": "공모가 측정하고 사람들이 막 사들이거나 팔거나 하면",
          "speaker_id": "1",
          "time": "20210831 12:50"
        },
        {
          "id": "MDRW2100024517.1.7",
          "form": "아....난 어디\r\n아파트 공모주인줄 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아....난 어디\r\n아파트 공모주인줄 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:50"
        },
        {
          "id": "MDRW2100024517.1.8",
          "form": "청약넣는 그런거 ㅋㅋㅋㅋㄱ",
          "original_form": "청약넣는 그런거 ㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210831 12:50"
        },
        {
          "id": "MDRW2100024517.1.9",
          "form": "평균이 나오잖아 ㅋㅋ",
          "original_form": "평균이 나오잖아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:50"
        },
        {
          "id": "MDRW2100024517.1.10",
          "form": "그런거랑 비슷한 거지뭐 ㅋㅋ",
          "original_form": "그런거랑 비슷한 거지뭐 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:50"
        },
        {
          "id": "MDRW2100024517.1.11",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210831 12:50"
        },
        {
          "id": "MDRW2100024517.1.12",
          "form": "어렵네",
          "original_form": "어렵네",
          "speaker_id": "2",
          "time": "20210831 12:50"
        },
        {
          "id": "MDRW2100024517.1.13",
          "form": "요즘에는 그래서 공모가를 높게 측정해서",
          "original_form": "요즘에는 그래서 공모가를 높게 측정해서",
          "speaker_id": "1",
          "time": "20210831 12:50"
        },
        {
          "id": "MDRW2100024517.1.14",
          "form": "무슨말인지 모르겠다",
          "original_form": "무슨말인지 모르겠다",
          "speaker_id": "2",
          "time": "20210831 12:50"
        },
        {
          "id": "MDRW2100024517.1.15",
          "form": "주식값을 올리려고 하는건가??",
          "original_form": "주식값을 올리려고 하는건가??",
          "speaker_id": "2",
          "time": "20210831 12:50"
        },
        {
          "id": "MDRW2100024517.1.16",
          "form": "내는 것도 있거든 그럼 거기 넣으면 꽝되는기다 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "내는 것도 있거든 그럼 거기 넣으면 꽝되는기다 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:51"
        },
        {
          "id": "MDRW2100024517.1.17",
          "form": "평균값을 높게??",
          "original_form": "평균값을 높게??",
          "speaker_id": "2",
          "time": "20210831 12:51"
        },
        {
          "id": "MDRW2100024517.1.18",
          "form": "아 사람들이 많이\r\n안사면 꽝이가??",
          "original_form": "아 사람들이 많이\r\n안사면 꽝이가??",
          "speaker_id": "2",
          "time": "20210831 12:51"
        },
        {
          "id": "MDRW2100024517.1.19",
          "form": "얍얍",
          "original_form": "얍얍",
          "speaker_id": "1",
          "time": "20210831 12:51"
        },
        {
          "id": "MDRW2100024517.1.20",
          "form": "적당한 가격에 사람들이 많이 사야되네",
          "original_form": "적당한 가격에 사람들이 많이 사야되네",
          "speaker_id": "2",
          "time": "20210831 12:51"
        },
        {
          "id": "MDRW2100024517.1.21",
          "form": "그럼 이거는 일단 없는 주식을",
          "original_form": "그럼 이거는 일단 없는 주식을",
          "speaker_id": "2",
          "time": "20210831 12:51"
        },
        {
          "id": "MDRW2100024517.1.22",
          "form": "나도 솔직히 잘모르는데",
          "original_form": "나도 솔직히 잘모르는데",
          "speaker_id": "1",
          "time": "20210831 12:51"
        },
        {
          "id": "MDRW2100024517.1.23",
          "form": "새로 만들면서 평균치를 내는거야??",
          "original_form": "새로 만들면서 평균치를 내는거야??",
          "speaker_id": "2",
          "time": "20210831 12:51"
        },
        {
          "id": "MDRW2100024517.1.24",
          "form": "아니아니 그런건 아니고",
          "original_form": "아니아니 그런건 아니고",
          "speaker_id": "1",
          "time": "20210831 12:52"
        },
        {
          "id": "MDRW2100024517.1.25",
          "form": "주식에는 평균치가 없는데 ㅋㅋㅋ",
          "original_form": "주식에는 평균치가 없는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:52"
        },
        {
          "id": "MDRW2100024517.1.26",
          "form": "아 그래 그래",
          "original_form": "아 그래 그래",
          "speaker_id": "2",
          "time": "20210831 12:52"
        },
        {
          "id": "MDRW2100024517.1.27",
          "form": "올랐다 내렸다 하니",
          "original_form": "올랐다 내렸다 하니",
          "speaker_id": "2",
          "time": "20210831 12:52"
        },
        {
          "id": "MDRW2100024517.1.28",
          "form": "쉽게 말을 하자면 ㅋㅋ 그래 얘기하더라고 ㅋㅋ",
          "original_form": "쉽게 말을 하자면 ㅋㅋ 그래 얘기하더라고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:52"
        },
        {
          "id": "MDRW2100024517.1.29",
          "form": "그렇구만",
          "original_form": "그렇구만",
          "speaker_id": "2",
          "time": "20210831 12:52"
        },
        {
          "id": "MDRW2100024517.1.30",
          "form": "어렵다 이",
          "original_form": "어렵다 이",
          "speaker_id": "2",
          "time": "20210831 12:52"
        },
        {
          "id": "MDRW2100024517.1.31",
          "form": "이런 거 전혀 해본 적이 없어서",
          "original_form": "이런 거 전혀 해본 적이 없어서",
          "speaker_id": "2",
          "time": "20210831 12:52"
        }
      ]
    }
