        {
          "id": "MDRW2100046006.1.1",
          "form": "나 남친 선물 골라줘",
          "original_form": "나 남친 선물 골라줘",
          "speaker_id": "2",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100046006.1.2",
          "form": "뭐말해봐",
          "original_form": "뭐말해봐",
          "speaker_id": "1",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100046006.1.3",
          "form": "신발을 살까 고민 중",
          "original_form": "신발을 살까 고민 중",
          "speaker_id": "2",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100046006.1.4",
          "form": "나는 신발은 비추천",
          "original_form": "나는 신발은 비추천",
          "speaker_id": "1",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100046006.1.5",
          "form": "이유가 있니",
          "original_form": "이유가 있니",
          "speaker_id": "2",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100046006.1.6",
          "form": "신발은 신어보고 사야 해",
          "original_form": "신발은 신어보고 사야 해",
          "speaker_id": "1",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100046006.1.7",
          "form": "사이즈는 알고 있어",
          "original_form": "사이즈는 알고 있어",
          "speaker_id": "2",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100046006.1.8",
          "form": "그래도 직접 거랑 달라",
          "original_form": "그래도 직접 거랑 달라",
          "speaker_id": "1",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100046006.1.9",
          "form": "반대하는 이유가 또 있니",
          "original_form": "반대하는 이유가 또 있니",
          "speaker_id": "2",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100046006.1.10",
          "form": "신발은 조금만 쓰면 어두워지잖아",
          "original_form": "신발은 조금만 쓰면 어두워지잖아",
          "speaker_id": "1",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100046006.1.11",
          "form": "닮긴 하지",
          "original_form": "닮긴 하지",
          "speaker_id": "2",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100046006.1.12",
          "form": "그래서 나는 신발은  편하지 않아",
          "original_form": "그래서 나는 신발은  편하지 않아",
          "speaker_id": "1",
          "time": "20210908 09:32"
        },
        {
          "id": "MDRW2100046006.1.13",
          "form": "그럼 지갑을 살까",
          "original_form": "그럼 지갑을 살까",
          "speaker_id": "2",
          "time": "20210908 09:32"
        },
        {
          "id": "MDRW2100046006.1.14",
          "form": "그게 낫겠다",
          "original_form": "그게 낫겠다",
          "speaker_id": "1",
          "time": "20210908 09:32"
        }
      ]
