      "utterance": [
        {
          "id": "MDRW2100034239.1.1",
          "form": "맞고나면 안무서워요",
          "original_form": "맞고나면 안무서워요",
          "speaker_id": "2",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100034239.1.2",
          "form": "아 그러시군요",
          "original_form": "아 그러시군요",
          "speaker_id": "2",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100034239.1.3",
          "form": "그래서요.. 맞아야 차라리 나을 거 같아요",
          "original_form": "그래서요.. 맞아야 차라리 나을 거 같아요",
          "speaker_id": "1",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100034239.1.4",
          "form": "예약이 늦은 순서군요",
          "original_form": "예약이 늦은 순서군요",
          "speaker_id": "2",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100034239.1.5",
          "form": "전 나이가 이번에  십부제 신청하는 거여서 신청했어요",
          "original_form": "전 나이가 이번에  십부제 신청하는 거여서 신청했어요",
          "speaker_id": "1",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100034239.1.6",
          "form": "그럼요 괜히 걱정만 가득이고",
          "original_form": "그럼요 괜히 걱정만 가득이고",
          "speaker_id": "2",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100034239.1.7",
          "form": "맞음 속편해요",
          "original_form": "맞음 속편해요",
          "speaker_id": "2",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100034239.1.8",
          "form": "저희는 허ㅏ이자",
          "original_form": "저희는 허ㅏ이자",
          "speaker_id": "2",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100034239.1.9",
          "form": "화이자",
          "original_form": "화이자",
          "speaker_id": "2",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100034239.1.10",
          "form": "그니까요..",
          "original_form": "그니까요..",
          "speaker_id": "1",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100034239.1.11",
          "form": "아스트라",
          "original_form": "아스트라",
          "speaker_id": "2",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100034239.1.12",
          "form": "얀센",
          "original_form": "얀센",
          "speaker_id": "2",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100034239.1.13",
          "form": "부작용은 없으셨어요?",
          "original_form": "부작용은 없으셨어요?",
          "speaker_id": "1",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100034239.1.14",
          "form": "가족이 다 다르게 맞았는데",
          "original_form": "가족이 다 다르게 맞았는데",
          "speaker_id": "2",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100034239.1.15",
          "form": "가족마다 달랐구나",
          "original_form": "가족마다 달랐구나",
          "speaker_id": "1",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100034239.1.16",
          "form": "아무도 부작용 없어요",
          "original_form": "아무도 부작용 없어요",
          "speaker_id": "2",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100034239.1.17",
          "form": "와우 다행이에요",
          "original_form": "와우 다행이에요",
          "speaker_id": "1",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100034239.1.18",
          "form": "사실 극히 일부예요",
          "original_form": "사실 극히 일부예요",
          "speaker_id": "2",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100034239.1.19",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100034239.1.20",
          "form": "저희 부모님은 아제 맞고 동생은 화이자 맞았는데 괜찮았어요",
          "original_form": "저희 부모님은 아제 맞고 동생은 화이자 맞았는데 괜찮았어요",
          "speaker_id": "1",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100034239.1.21",
          "form": "정말 골고루 다 경험했네요",
          "original_form": "정말 골고루 다 경험했네요",
          "speaker_id": "2",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100034239.1.22",
          "form": "저만 맞으면 돼요",
          "original_form": "저만 맞으면 돼요",
          "speaker_id": "1",
          "time": "20210827 21:46"
        },
        {
          "id": "MDRW2100034239.1.23",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210827 21:47"
        },
        {
          "id": "MDRW2100034239.1.24",
          "form": "맘편히 하시고 맞으세요",
          "original_form": "맘편히 하시고 맞으세요",
          "speaker_id": "2",
          "time": "20210827 21:47"
        },
        {
          "id": "MDRW2100034239.1.25",
          "form": "사실 맞고나면 진짜 후련해요",
          "original_form": "사실 맞고나면 진짜 후련해요",
          "speaker_id": "2",
          "time": "20210827 21:47"
        },
        {
          "id": "MDRW2100034239.1.26",
          "form": "네~ 전 이만 자러 가야할거 같아서요.. 내일 뵐게요..ㅎㅎ",
          "original_form": "네~ 전 이만 자러 가야할거 같아서요.. 내일 뵐게요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 21:47"
        },
        {
          "id": "MDRW2100034239.1.27",
          "form": "친정도 다 맞았어요",
          "original_form": "친정도 다 맞았어요",
          "speaker_id": "2",
          "time": "20210827 21:47"
        },
        {
          "id": "MDRW2100034239.1.28",
          "form": "저희는 아들도 2차 완료요",
          "original_form": "저희는 아들도 2차 완료요",
          "speaker_id": "2",
          "time": "20210827 21:47"
        },
        {
          "id": "MDRW2100034239.1.29",
          "form": "고3이라",
          "original_form": "고3이라",
          "speaker_id": "2",
          "time": "20210827 21:47"
        },
        {
          "id": "MDRW2100034239.1.30",
          "form": "부작용 없이 잘 맞으실 거예요",
          "original_form": "부작용 없이 잘 맞으실 거예요",
          "speaker_id": "2",
          "time": "20210827 21:49"
        },
        {
          "id": "MDRW2100034239.1.31",
          "form": "홧팅요",
          "original_form": "홧팅요",
          "speaker_id": "2",
          "time": "20210827 21:49"
        }
      ]
    }
