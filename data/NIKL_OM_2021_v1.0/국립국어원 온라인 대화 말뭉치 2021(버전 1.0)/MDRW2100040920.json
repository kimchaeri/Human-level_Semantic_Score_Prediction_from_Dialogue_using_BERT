      "utterance": [
        {
          "id": "MDRW2100040920.1.1",
          "form": "배가 고프다",
          "original_form": "배가 고프다",
          "speaker_id": "2",
          "time": "20210905 12:41"
        },
        {
          "id": "MDRW2100040920.1.2",
          "form": "아직 주꾸미 배달예정시간이 40분 남았는데",
          "original_form": "아직 주꾸미 배달예정시간이 40분 남았는데",
          "speaker_id": "2",
          "time": "20210905 12:41"
        },
        {
          "id": "MDRW2100040920.1.3",
          "form": "잠시 책을 읽으며 허기를 달래보아",
          "original_form": "잠시 책을 읽으며 허기를 달래보아",
          "speaker_id": "1",
          "time": "20210905 12:42"
        },
        {
          "id": "MDRW2100040920.1.4",
          "form": "응응 마음의 양식을 채운다...",
          "original_form": "응응 마음의 양식을 채운다...",
          "speaker_id": "2",
          "time": "20210905 12:43"
        },
        {
          "id": "MDRW2100040920.1.5",
          "form": "몸의 양식을 기다리며",
          "original_form": "몸의 양식을 기다리며",
          "speaker_id": "2",
          "time": "20210905 12:43"
        },
        {
          "id": "MDRW2100040920.1.6",
          "form": "배고파..",
          "original_form": "배고파..",
          "speaker_id": "1",
          "time": "20210905 12:45"
        },
        {
          "id": "MDRW2100040920.1.7",
          "form": "응 배고파",
          "original_form": "응 배고파",
          "speaker_id": "2",
          "time": "20210905 12:48"
        },
        {
          "id": "MDRW2100040920.1.8",
          "form": "배달현황 봤는데",
          "original_form": "배달현황 봤는데",
          "speaker_id": "2",
          "time": "20210905 12:49"
        },
        {
          "id": "MDRW2100040920.1.9",
          "form": "아직 조리중이래",
          "original_form": "아직 조리중이래",
          "speaker_id": "2",
          "time": "20210905 12:49"
        },
        {
          "id": "MDRW2100040920.1.10",
          "form": "ㅋㅋㅋㅋ해서 오겠지",
          "original_form": "ㅋㅋㅋㅋ해서 오겠지",
          "speaker_id": "1",
          "time": "20210905 12:49"
        },
        {
          "id": "MDRW2100040920.1.11",
          "form": "한시는 넘어야 배달출발할득",
          "original_form": "한시는 넘어야 배달출발할득",
          "speaker_id": "1",
          "time": "20210905 12:49"
        },
        {
          "id": "MDRW2100040920.1.12",
          "form": "혹시나 출발했나 봤지",
          "original_form": "혹시나 출발했나 봤지",
          "speaker_id": "2",
          "time": "20210905 12:50"
        },
        {
          "id": "MDRW2100040920.1.13",
          "form": "굶주림에 지쳐",
          "original_form": "굶주림에 지쳐",
          "speaker_id": "1",
          "time": "20210905 12:52"
        },
        {
          "id": "MDRW2100040920.1.14",
          "form": "허상을 좇는 자들",
          "original_form": "허상을 좇는 자들",
          "speaker_id": "1",
          "time": "20210905 12:52"
        },
        {
          "id": "MDRW2100040920.1.15",
          "form": "ㅎㅎㅎ배고픕니다",
          "original_form": "ㅎㅎㅎ배고픕니다",
          "speaker_id": "2",
          "time": "20210905 13:00"
        },
        {
          "id": "MDRW2100040920.1.16",
          "form": "출발했나 다시 한번 체크",
          "original_form": "출발했나 다시 한번 체크",
          "speaker_id": "2",
          "time": "20210905 13:00"
        },
        {
          "id": "MDRW2100040920.1.17",
          "form": "출발했나유",
          "original_form": "출발했나유",
          "speaker_id": "1",
          "time": "20210905 13:02"
        },
        {
          "id": "MDRW2100040920.1.18",
          "form": "아직 조리중으로 뜹니다ㅠ",
          "original_form": "아직 조리중으로 뜹니다ㅠ",
          "speaker_id": "2",
          "time": "20210905 13:05"
        },
        {
          "id": "MDRW2100040920.1.19",
          "form": "배달도착예정시간 10분 전인디",
          "original_form": "배달도착예정시간 10분 전인디",
          "speaker_id": "2",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100040920.1.20",
          "form": "오겠지",
          "original_form": "오겠지",
          "speaker_id": "1",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100040920.1.21",
          "form": "업뎃안했을지도?",
          "original_form": "업뎃안했을지도?",
          "speaker_id": "1",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100040920.1.22",
          "form": "아직도 배달 현황이 변하지 않아",
          "original_form": "아직도 배달 현황이 변하지 않아",
          "speaker_id": "2",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100040920.1.23",
          "form": "얼른 와라~",
          "original_form": "얼른 와라~",
          "speaker_id": "2",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100040920.1.24",
          "form": "시간 지나서도 안오면 가게 전화해보자",
          "original_form": "시간 지나서도 안오면 가게 전화해보자",
          "speaker_id": "1",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100040920.1.25",
          "form": "주꾸미 맛있었다",
          "original_form": "주꾸미 맛있었다",
          "speaker_id": "2",
          "time": "20210905 14:30"
        },
        {
          "id": "MDRW2100040920.1.26",
          "form": "그치?",
          "original_form": "그치?",
          "speaker_id": "2",
          "time": "20210905 14:30"
        },
        {
          "id": "MDRW2100040920.1.27",
          "form": "아 왕맛있었네",
          "original_form": "아 왕맛있었네",
