      "utterance": [
        {
          "id": "MDRW2100027777.1.1",
          "form": "주제만 봐도 눈물 나는 나",
          "original_form": "주제만 봐도 눈물 나는 나",
          "speaker_id": "2",
          "time": "20210902 14:59"
        },
        {
          "id": "MDRW2100027777.1.2",
          "form": "수강신청 대실패했잖아",
          "original_form": "수강신청 대실패했잖아",
          "speaker_id": "2",
          "time": "20210902 15:00"
        },
        {
          "id": "MDRW2100027777.1.3",
          "form": "그러니까.. 이번에 교수님란테 보내는 메일 내용도 걍 짠해",
          "original_form": "그러니까.. 이번에 교수님란테 보내는 메일 내용도 걍 짠해",
          "speaker_id": "1",
          "time": "20210902 15:01"
        },
        {
          "id": "MDRW2100027777.1.4",
          "form": "나 편입생이라 졸업시켜주세요...",
          "original_form": "나 편입생이라 졸업시켜주세요...",
          "speaker_id": "2",
          "time": "20210902 15:02"
        },
        {
          "id": "MDRW2100027777.1.5",
          "form": "대신 그래도 학교 다니니까 무료로 심리검사도 받고 하는거지",
          "original_form": "대신 그래도 학교 다니니까 무료로 심리검사도 받고 하는거지",
          "speaker_id": "2",
          "time": "20210902 15:02"
        },
        {
          "id": "MDRW2100027777.1.6",
          "form": "이제 졸업반인데 인생이 꼬인다 진짜",
          "original_form": "이제 졸업반인데 인생이 꼬인다 진짜",
          "speaker_id": "1",
          "time": "20210902 15:02"
        },
        {
          "id": "MDRW2100027777.1.7",
          "form": "맞아 즐길거 다 즐겨 학생할인도 즐겨",
          "original_form": "맞아 즐길거 다 즐겨 학생할인도 즐겨",
          "speaker_id": "1",
          "time": "20210902 15:02"
        },
        {
          "id": "MDRW2100027777.1.8",
          "form": "학생스토어도 다 털고 졸업하겠어",
          "original_form": "학생스토어도 다 털고 졸업하겠어",
          "speaker_id": "2",
          "time": "20210902 15:02"
        },
        {
          "id": "MDRW2100027777.1.9",
          "form": "이때 아님 없다",
          "original_form": "이때 아님 없다",
          "speaker_id": "1",
          "time": "20210902 15:02"
        },
        {
          "id": "MDRW2100027777.1.10",
          "form": "서포터즈도 스트레스받고",
          "original_form": "서포터즈도 스트레스받고",
          "speaker_id": "2",
          "time": "20210902 15:02"
        },
        {
          "id": "MDRW2100027777.1.11",
          "form": "그리고 나 대신 갤워치4도 대리구매 해주셈",
          "original_form": "그리고 나 대신 갤워치4도 대리구매 해주셈",
          "speaker_id": "1",
          "time": "20210902 15:02"
        },
        {
          "id": "MDRW2100027777.1.12",
          "form": "학생할인 써먹어야한다고",
          "original_form": "학생할인 써먹어야한다고",
          "speaker_id": "1",
          "time": "20210902 15:02"
        },
        {
          "id": "MDRW2100027777.1.13",
          "form": "이사람...내 할인도 써먹다니",
          "original_form": "이사람...내 할인도 써먹다니",
          "speaker_id": "2",
          "time": "20210902 15:03"
        },
        {
          "id": "MDRW2100027777.1.14",
          "form": "마구 이용해버릴테다",
          "original_form": "마구 이용해버릴테다",
          "speaker_id": "1",
          "time": "20210902 15:03"
        },
        {
          "id": "MDRW2100027777.1.15",
          "form": "하지만 마음에 들었습니다",
          "original_form": "하지만 마음에 들었습니다",
          "speaker_id": "2",
          "time": "20210902 15:03"
        },
        {
          "id": "MDRW2100027777.1.16",
          "form": "휴우... 대신 그림그려줄게",
          "original_form": "휴우... 대신 그림그려줄게",
          "speaker_id": "1",
          "time": "20210902 15:03"
        },
        {
          "id": "MDRW2100027777.1.17",
          "form": "좋았어",
          "original_form": "좋았어",
          "speaker_id": "2",
          "time": "20210902 15:03"
        },
        {
          "id": "MDRW2100027777.1.18",
          "form": "학생 할인 정말 좋은 제도야...",
          "original_form": "학생 할인 정말 좋은 제도야...",
          "speaker_id": "1",
          "time": "20210902 15:03"
        },
        {
          "id": "MDRW2100027777.1.19",
          "form": "대학교도 좋아야 그런 기능 있는거라고",
          "original_form": "대학교도 좋아야 그런 기능 있는거라고",
          "speaker_id": "1",
          "time": "20210902 15:03"
        },
        {
          "id": "MDRW2100027777.1.20",
          "form": "사실 오늘도 1시 수업이었는데",
          "original_form": "사실 오늘도 1시 수업이었는데",
          "speaker_id": "2",
          "time": "20210902 15:03"
        },
        {
          "id": "MDRW2100027777.1.21",
          "form": "1시 수업 취소하고 다른 수업 잡았어",
          "original_form": "1시 수업 취소하고 다른 수업 잡았어",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027777.1.22",
          "form": "잘했어 나이스",
          "original_form": "잘했어 나이스",
          "speaker_id": "1",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027777.1.23",
          "form": "이 수업 때문에 시간이 겹쳐서 빌어서 넣지도 못하겠더라고",
          "original_form": "이 수업 때문에 시간이 겹쳐서 빌어서 넣지도 못하겠더라고",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027777.1.24",
          "form": "이대로 진행된다면 화금 공강이야",
          "original_form": "이대로 진행된다면 화금 공강이야",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027777.1.25",
          "form": "아이고야 다른 수업은 잘잡음?",
          "original_form": "아이고야 다른 수업은 잘잡음?",
          "speaker_id": "1",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027777.1.26",
          "form": "화금 공강 좋다",
          "original_form": "화금 공강 좋다",
          "speaker_id": "1",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027777.1.27",
          "form": "그냥저냥?",
          "original_form": "그냥저냥?",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027777.1.28",
          "form": "그게 그거야",
          "original_form": "그게 그거야",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027777.1.29",
          "form": "실습아니라면야",
          "original_form": "실습아니라면야",
          "speaker_id": "1",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027777.1.30",
          "form": "수업은 그냥 교수님 자랑이라 편한데",
          "original_form": "수업은 그냥 교수님 자랑이라 편한데",
          "speaker_id": "2",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100027777.1.31",
          "form": "교수가 좀 싫은 거랑",
          "original_form": "교수가 좀 싫은 거랑",
          "speaker_id": "2",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100027777.1.32",
          "form": "변경한거는 이제 교수는 좋은데",
          "original_form": "변경한거는 이제 교수는 좋은데",
          "speaker_id": "2",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100027777.1.33",
          "form": "수업이 좀 손이 많이 가는 거",
          "original_form": "수업이 좀 손이 많이 가는 거",
          "speaker_id": "2",
          "time": "20210902 15:05"
        }
      ]
    }
