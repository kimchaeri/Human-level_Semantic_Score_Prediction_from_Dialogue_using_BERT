      "utterance": [
        {
          "id": "MDRW2100013327.1.1",
          "form": "좋아하는 아이돌 있어요?",
          "original_form": "좋아하는 아이돌 있어요?",
          "speaker_id": "1",
          "time": "20210825 14:04"
        },
        {
          "id": "MDRW2100013327.1.2",
          "form": "딱히 최애는 없는데 아이돌 알기는 많이 알아요",
          "original_form": "딱히 최애는 없는데 아이돌 알기는 많이 알아요",
          "speaker_id": "2",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013327.1.3",
          "form": "요즘은 츄가 넘 귀여워요",
          "original_form": "요즘은 츄가 넘 귀여워요",
          "speaker_id": "2",
          "time": "20210825 14:05"
        },
        {
          "id": "MDRW2100013327.1.4",
          "form": "아 츄 이달소 맞나요?",
          "original_form": "아 츄 이달소 맞나요?",
          "speaker_id": "1",
          "time": "20210825 14:06"
        },
        {
          "id": "MDRW2100013327.1.5",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "2",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100013327.1.6",
          "form": "그 강철부대? 거기서 봤어요 귀엽더라고요ㅎㅎ",
          "original_form": "그 강철부대? 거기서 봤어요 귀엽더라고요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 14:07"
        },
        {
          "id": "MDRW2100013327.1.7",
          "form": "말티즈 닮았어요...",
          "original_form": "말티즈 닮았어요...",
          "speaker_id": "2",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013327.1.8",
          "form": "name1은 좋아하는 아이돌 있으세요?",
          "original_form": "&name1&은 좋아하는 아이돌 있으세요?",
          "speaker_id": "2",
          "time": "20210825 14:08"
        },
        {
          "id": "MDRW2100013327.1.9",
          "form": "말티즈! 그러게요",
          "original_form": "말티즈! 그러게요",
          "speaker_id": "1",
          "time": "20210825 14:09"
        },
        {
          "id": "MDRW2100013327.1.10",
          "form": "저는 워너원 좋아했었는데",
          "original_form": "저는 워너원 좋아했었는데",
          "speaker_id": "1",
          "time": "20210825 14:09"
        },
        {
          "id": "MDRW2100013327.1.11",
          "form": "누굴 제일 좋아하셨나요",
          "original_form": "누굴 제일 좋아하셨나요",
          "speaker_id": "2",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013327.1.12",
          "form": "윤지성 하성운 황민현 좋아했어요",
          "original_form": "윤지성 하성운 황민현 좋아했어요",
          "speaker_id": "1",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100013327.1.13",
          "form": "영원+1 이라는 노래 들어보셨나요",
          "original_form": "영원+1 이라는 노래 들어보셨나요",
          "speaker_id": "1",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100013327.1.14",
          "form": "아 그 세명이서 부른 노래 맞지요?",
          "original_form": "아 그 세명이서 부른 노래 맞지요?",
          "speaker_id": "2",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100013327.1.15",
          "form": "친구가 황민현을 좋아해서 들어본 적은 있어요",
          "original_form": "친구가 황민현을 좋아해서 들어본 적은 있어요",
          "speaker_id": "2",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100013327.1.16",
          "form": "네 맞아요ㅎㅎ 그노래 제일 좋아했어요",
          "original_form": "네 맞아요ㅎㅎ 그노래 제일 좋아했어요",
          "speaker_id": "1",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013327.1.17",
          "form": "요즘 즐겨듣는 노래있으면 추천해주세요",
          "original_form": "요즘 즐겨듣는 노래있으면 추천해주세요",
          "speaker_id": "1",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013327.1.18",
          "form": "요즘 아이유랑 악뮤 신곡이 넘 좋더라고요",
          "original_form": "요즘 아이유랑 악뮤 신곡이 넘 좋더라고요",
          "speaker_id": "2",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013327.1.19",
          "form": "낙화!",
          "original_form": "낙화!",
          "speaker_id": "1",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013327.1.20",
