      "utterance": [
        {
          "id": "MDRW2100026593.1.1",
          "form": "오오 알아요 알아요!!!!!!",
          "original_form": "오오 알아요 알아요!!!!!!",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026593.1.2",
          "form": "이거로 공룡이름 외웠죠",
          "original_form": "이거로 공룡이름 외웠죠",
          "speaker_id": "2",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100026593.1.3",
          "form": "ㅋㅋㅋㅋ 맞아요맞아요 !! 공룡나오는것중 사우르스 팡팡 제일 좋아했어요",
          "original_form": "ㅋㅋㅋㅋ 맞아요맞아요 !! 공룡나오는것중 사우르스 팡팡 제일 좋아했어요",
          "speaker_id": "1",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100026593.1.4",
          "form": "그리고 몬타나 존스 아시나요? ㅋㅋㅋ",
          "original_form": "그리고 몬타나 존스 아시나요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100026593.1.5",
          "form": "ㅋㅋㅋㅋㅋ재밌는거 많았네요",
          "original_form": "ㅋㅋㅋㅋㅋ재밌는거 많았네요",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026593.1.6",
          "form": "네 알죠",
          "original_form": "네 알죠",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026593.1.7",
          "form": "엠비씨에서 했죠ㅋㅋㅋ",
          "original_form": "엠비씨에서 했죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100026593.1.8",
          "form": "그거 커서 다시 봐도 재밌더라구요 ㅋㅋㅋㅋ",
          "original_form": "그거 커서 다시 봐도 재밌더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026593.1.9",
          "form": "그것도 재밌었는데ㅋㅋㅋㅋㅋ",
          "original_form": "그것도 재밌었는데ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026593.1.10",
          "form": "보거스도 재밌고",
          "original_form": "보거스도 재밌고",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026593.1.11",
          "form": "헐 보거스 알죠알죠",
          "original_form": "헐 보거스 알죠알죠",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026593.1.12",
          "form": "은비까비도 재밌었는데요",
          "original_form": "은비까비도 재밌었는데요",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026593.1.13",
          "form": "그거 찰흙 장면만 얼마나 기다렸구요",
          "original_form": "그거 찰흙 장면만 얼마나 기다렸구요",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026593.1.14",
          "form": "그부분을 특히나 너무 좋아했어요 ㅋㅋㅋㅋㅋ",
          "original_form": "그부분을 특히나 너무 좋아했어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026593.1.15",
          "form": "은비까비 당연히 봤습니다 배추도사 무도사도 봤다구요 ㅋㅋㅋ  예전에는 6시에 집에가서 만화를 보는게 너무 당연한 일상이었어요 ㅋㅋ 무조건 해야하는 일들 중 하나였죠",
          "original_form": "은비까비 당연히 봤습니다 배추도사 무도사도 봤다구요 ㅋㅋㅋ  예전에는 6시에 집에가서 만화를 보는게 너무 당연한 일상이었어요 ㅋㅋ 무조건 해야하는 일들 중 하나였죠",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026593.1.16",
          "form": "악ㅋㅋㅋ찰흙 맞아요ㅋㅋㅋㅋㅋㄱㅋ",
          "original_form": "악ㅋㅋㅋ찰흙 맞아요ㅋㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026593.1.17",
          "form": "맞아요 ㅎ일요일 8시엔 디즈니도 봐야죠",
          "original_form": "맞아요 ㅎ일요일 8시엔 디즈니도 봐야죠",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026593.1.18",
          "form": "진짜 일찍일어나기 힘든데 그거땜에 일어나고",
          "original_form": "진짜 일찍일어나기 힘든데 그거땜에 일어나고",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026593.1.19",
          "form": "맞아요맞아요 ㅋㅋㅋㅋ 거기서 푸도 나오고 가고일? 이랑 티몬과품바도 봤어욫ㅋㅋ",
          "original_form": "맞아요맞아요 ㅋㅋㅋㅋ 거기서 푸도 나오고 가고일? 이랑 티몬과품바도 봤어욫ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026593.1.20",
          "form": "맞아요ㅋㅋㅋㄱㅋㅋㅋ 티몬과품바ㅋㅋ진짜 재밌게 본",
          "original_form": "맞아요ㅋㅋㅋㄱㅋㅋㅋ 티몬과품바ㅋㅋ진짜 재밌게 본",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026593.1.21",
          "form": "또....음 돈대기돈대기..",
          "original_form": "또....음 돈대기돈대기..",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026593.1.22",
          "form": "맞나?",
          "original_form": "맞나?",
          "speaker_id": "2",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026593.1.23",
          "form": "주전자",
          "original_form": "주전자",
          "speaker_id": "2",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026593.1.24",
          "form": "타임머신? 그거아세요?",
          "original_form": "타임머신? 그거아세요?",
          "speaker_id": "2",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026593.1.25",
          "form": "아아아 그 뭐더라 시간탐험대??",
          "original_form": "아아아 그 뭐더라 시간탐험대??",
          "speaker_id": "1",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026593.1.26",
          "form": "맞아요 그거같아요! ㅋㅋ거인나오고",
          "original_form": "맞아요 그거같아요! ㅋㅋ거인나오고",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026593.1.27",
          "form": "그거도 진짜 재밌게봤는데",
          "original_form": "그거도 진짜 재밌게봤는데",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100026593.1.28",
          "form": "이비에스에서 했던거같아요",
          "original_form": "이비에스에서 했던거같아요",
