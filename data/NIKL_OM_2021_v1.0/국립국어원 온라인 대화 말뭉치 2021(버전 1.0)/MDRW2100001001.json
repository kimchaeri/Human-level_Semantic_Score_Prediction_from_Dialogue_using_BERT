      "utterance": [
        {
          "id": "MDRW2100001001.1.1",
          "form": "아르바이트를 해본적이 있나요?",
          "original_form": "아르바이트를 해본적이 있나요?",
          "speaker_id": "1",
          "time": "20210820 11:37"
        },
        {
          "id": "MDRW2100001001.1.2",
          "form": "넵. 카페랑 서빙 샌드위치 가게 해봤어요",
          "original_form": "넵. 카페랑 서빙 샌드위치 가게 해봤어요",
          "speaker_id": "2",
          "time": "20210820 11:54"
        },
        {
          "id": "MDRW2100001001.1.3",
          "form": "카페알바는 어떤가요?",
          "original_form": "카페알바는 어떤가요?",
          "speaker_id": "1",
          "time": "20210820 11:54"
        },
        {
          "id": "MDRW2100001001.1.4",
          "form": "음료 제조 하는 거 멋있어보이는데 어려운 점이 있었나요?",
          "original_form": "음료 제조 하는 거 멋있어보이는데 어려운 점이 있었나요?",
          "speaker_id": "1",
          "time": "20210820 11:55"
        },
        {
          "id": "MDRW2100001001.1.5",
          "form": "저는 해본 것 중에는 카페가 제일 재밌었어요.",
          "original_form": "저는 해본 것 중에는 카페가 제일 재밌었어요.",
          "speaker_id": "2",
          "time": "20210820 12:14"
        },
        {
          "id": "MDRW2100001001.1.6",
          "form": "음료는 레시피를 잊지만 않으면 괜찮은 것 같아요",
          "original_form": "음료는 레시피를 잊지만 않으면 괜찮은 것 같아요",
          "speaker_id": "2",
          "time": "20210820 12:15"
        },
        {
          "id": "MDRW2100001001.1.7",
          "form": "가장 좋아하는 음료레시피는 뭐였나요?",
          "original_form": "가장 좋아하는 음료레시피는 뭐였나요?",
          "speaker_id": "1",
          "time": "20210820 12:15"
        },
        {
          "id": "MDRW2100001001.1.8",
          "form": "그냥 커피류는 다 좋았어요",
          "original_form": "그냥 커피류는 다 좋았어요",
          "speaker_id": "2",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100001001.1.9",
          "form": "샷 뽑는게 너무 재밌더라구요 ㅋㅋ",
          "original_form": "샷 뽑는게 너무 재밌더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100001001.1.10",
          "form": "해본적은 없는데 재밌을거같아요",
          "original_form": "해본적은 없는데 재밌을거같아요",
          "speaker_id": "1",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100001001.1.11",
          "form": "기계로 뽑는거져?",
          "original_form": "기계로 뽑는거져?",
          "speaker_id": "1",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100001001.1.12",
          "form": "넹! 근데 원두가루 템핑하고 이런게 너무 재밌었어요 ㅋㅋ",
          "original_form": "넹! 근데 원두가루 템핑하고 이런게 너무 재밌었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100001001.1.13",
          "form": "기회가된다면 해보고싶어요ㅎㅎ",
          "original_form": "기회가된다면 해보고싶어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100001001.1.14",
          "form": "커피말고 다음으로 잘나가는 메뉴는 뭐였나요?",
          "original_form": "커피말고 다음으로 잘나가는 메뉴는 뭐였나요?",
          "speaker_id": "1",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100001001.1.15",
          "form": "재밌을 거에요 ㅎㅎㅎ",
          "original_form": "재밌을 거에요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100001001.1.16",
          "form": "쉐이크나 프라푸치가 많이 나가요",
          "original_form": "쉐이크나 프라푸치가 많이 나가요",
          "speaker_id": "2",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100001001.1.17",
          "form": "근데 너무 귀찮은.. ㅠㅠ",
          "original_form": "근데 너무 귀찮은.. ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100001001.1.18",
          "form": "손이 많이가긴할거같아요",
          "original_form": "손이 많이가긴할거같아요",
          "speaker_id": "1",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100001001.1.19",
          "form": "그 휘핑 하는거",
          "original_form": "그 휘핑 하는거",
          "speaker_id": "1",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100001001.1.20",
          "form": "그거 어려워보이던데 어때요?",
          "original_form": "그거 어려워보이던데 어때요?",
          "speaker_id": "1",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100001001.1.21",
          "form": "휘핑은 짜는 거 익숙해지면 쉬워요",
          "original_form": "휘핑은 짜는 거 익숙해지면 쉬워요",
          "speaker_id": "2",
          "time": "20210820 14:01"
        },
        {
          "id": "MDRW2100001001.1.22",
          "form": "컵 둘레로 먼저 깔고 하면 쉬워요 ㅎㅎㅎ",
          "original_form": "컵 둘레로 먼저 깔고 하면 쉬워요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 14:01"
        },
        {
          "id": "MDRW2100001001.1.23",
          "form": "컵둘레 기억할게요!",
          "original_form": "컵둘레 기억할게요!",
          "speaker_id": "1",
          "time": "20210820 14:02"
        }
      ]
