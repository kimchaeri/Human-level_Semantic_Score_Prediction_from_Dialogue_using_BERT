      "utterance": [
        {
          "id": "MDRW2100032426.1.1",
          "form": "자외선은 피 해야죠",
          "original_form": "자외선은 피 해야죠",
          "speaker_id": "2",
          "time": "20210905 17:19"
        },
        {
          "id": "MDRW2100032426.1.2",
          "form": "햇살이 따가우면 피부가 벗겨지죠",
          "original_form": "햇살이 따가우면 피부가 벗겨지죠",
          "speaker_id": "1",
          "time": "20210905 17:20"
        },
        {
          "id": "MDRW2100032426.1.3",
          "form": "저는 꼭 전 그라스를 씁니다",
          "original_form": "저는 꼭 전 그라스를 씁니다",
          "speaker_id": "2",
          "time": "20210905 17:20"
        },
        {
          "id": "MDRW2100032426.1.4",
          "form": "저도 모자를 쓰고 나가는 편이에요",
          "original_form": "저도 모자를 쓰고 나가는 편이에요",
          "speaker_id": "1",
          "time": "20210905 17:20"
        },
        {
          "id": "MDRW2100032426.1.5",
          "form": "햇살에 눈이 부신일까요",
          "original_form": "햇살에 눈이 부신일까요",
          "speaker_id": "2",
          "time": "20210905 17:20"
        },
        {
          "id": "MDRW2100032426.1.6",
          "form": "햇볕을 쬐긴 해야합니다",
          "original_form": "햇볕을 쬐긴 해야합니다",
          "speaker_id": "1",
          "time": "20210905 17:20"
        },
        {
          "id": "MDRW2100032426.1.7",
          "form": "햇볕 둘째어야 잠 인디가 생겨요",
          "original_form": "햇볕 둘째어야 잠 인디가 생겨요",
          "speaker_id": "2",
          "time": "20210905 17:20"
        },
        {
          "id": "MDRW2100032426.1.8",
          "form": "비타민은 햇볕이 보충해줘요",
          "original_form": "비타민은 햇볕이 보충해줘요",
          "speaker_id": "1",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100032426.1.9",
          "form": "덥지만 밖으로 한 번씩 나가야 해요",
          "original_form": "덥지만 밖으로 한 번씩 나가야 해요",
          "speaker_id": "2",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100032426.1.10",
          "form": "여름에도 산책은 필수에요",
          "original_form": "여름에도 산책은 필수에요",
          "speaker_id": "1",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100032426.1.11",
          "form": "겨울 햇살은 따뜻하고 좋아요",
          "original_form": "겨울 햇살은 따뜻하고 좋아요",
          "speaker_id": "2",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100032426.1.12",
          "form": "햇볕은 사실 좋은거죠 활력을 주잖아요",
          "original_form": "햇볕은 사실 좋은거죠 활력을 주잖아요",
          "speaker_id": "1",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100032426.1.13",
          "form": "구름이 낀 날은 좀 우울하죠",
          "original_form": "구름이 낀 날은 좀 우울하죠",
          "speaker_id": "2",
