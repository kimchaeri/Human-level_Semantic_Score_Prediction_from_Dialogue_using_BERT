      "utterance": [
        {
          "id": "MDRW2100000881.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210819 20:18"
        },
        {
          "id": "MDRW2100000881.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000881.1.3",
          "form": "오늘 날씨 좋았는데 갑자기 비와서 엄청 뛰어왔네요ㅜㅜ",
          "original_form": "오늘 날씨 좋았는데 갑자기 비와서 엄청 뛰어왔네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000881.1.4",
          "form": "여긴 비가 안왔어요",
          "original_form": "여긴 비가 안왔어요",
          "speaker_id": "1",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000881.1.5",
          "form": "거긴 왔나요",
          "original_form": "거긴 왔나요",
          "speaker_id": "1",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000881.1.6",
          "form": "네 방금 갑자기요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "네 방금 갑자기요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:36"
        },
        {
          "id": "MDRW2100000881.1.7",
          "form": "아직 더운거같아요 저만 그렇게 느끼나요",
          "original_form": "아직 더운거같아요 저만 그렇게 느끼나요",
          "speaker_id": "1",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000881.1.8",
          "form": "맞아요 진짜 나가기가 싫더라구요ㅋㅋ",
          "original_form": "맞아요 진짜 나가기가 싫더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000881.1.9",
          "form": "특히 낮에는 나가기 무서워요",
          "original_form": "특히 낮에는 나가기 무서워요",
          "speaker_id": "2",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000881.1.10",
          "form": "26도 던대 다음주",
          "original_form": "26도 던대 다음주",
          "speaker_id": "1",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000881.1.11",
          "form": "이온도 더운거맞죠?",
          "original_form": "이온도 더운거맞죠?",
          "speaker_id": "1",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000881.1.12",
          "form": "오 그정도면 그래도 시원한거 같은데요??",
          "original_form": "오 그정도면 그래도 시원한거 같은데요??",
          "speaker_id": "2",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000881.1.13",
          "form": "다음주부터 좀 풀리나봐요",
          "original_form": "다음주부터 좀 풀리나봐요",
          "speaker_id": "2",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000881.1.14",
          "form": "그런가요?",
          "original_form": "그런가요?",
          "speaker_id": "1",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000881.1.15",
          "form": "애기가 더위를 못견디는대",
          "original_form": "애기가 더위를 못견디는대",
          "speaker_id": "1",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000881.1.16",
          "form": "그정도면 에어컨을 안틀어도될 날씨일지 고민이네요",
          "original_form": "그정도면 에어컨을 안틀어도될 날씨일지 고민이네요",
          "speaker_id": "1",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000881.1.17",
          "form": "26도면 괜찮을거같아요!!",
          "original_form": "26도면 괜찮을거같아요!!",
          "speaker_id": "2",
          "time": "20210819 20:44"
        },
        {
          "id": "MDRW2100000881.1.18",
          "form": "특히 요즘엔 바람이 많이 불어서",
          "original_form": "특히 요즘엔 바람이 많이 불어서",
          "speaker_id": "2",
          "time": "20210819 20:44"
        },
        {
          "id": "MDRW2100000881.1.19",
          "form": "창문만 열어도 시원하더라구요",
          "original_form": "창문만 열어도 시원하더라구요",
          "speaker_id": "2",
          "time": "20210819 20:44"
        },
        {
          "id": "MDRW2100000881.1.20",
          "form": "이쪽만 그런가ㅎㅎㅎ",
          "original_form": "이쪽만 그런가ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 20:44"
        },
        {
          "id": "MDRW2100000881.1.21",
          "form": "저흰 아직 더워요 서쪽이세요?",
          "original_form": "저흰 아직 더워요 서쪽이세요?",
          "speaker_id": "1",
          "time": "20210819 20:44"
        },
        {
          "id": "MDRW2100000881.1.22",
          "form": "네네 서쪽이요!!",
          "original_form": "네네 서쪽이요!!",
          "speaker_id": "2",
          "time": "20210819 20:46"
        },
        {
          "id": "MDRW2100000881.1.23",
          "form": "그래서 그런가봐요",
          "original_form": "그래서 그런가봐요",
          "speaker_id": "1",
          "time": "20210819 20:48"
        },
        {
          "id": "MDRW2100000881.1.24",
          "form": "옷차림은 요즘 가디건 가지고 다녀야할거같은거 같아요",
          "original_form": "옷차림은 요즘 가디건 가지고 다녀야할거같은거 같아요",
          "speaker_id": "1",
          "time": "20210819 20:48"
        },
        {
          "id": "MDRW2100000881.1.25",
          "form": "맞아요 얇은 겉옷 꼭 챙겨요 실내는 아직도 에어컨 너무 빵빵해서ㅋㅋ",
          "original_form": "맞아요 얇은 겉옷 꼭 챙겨요 실내는 아직도 에어컨 너무 빵빵해서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:50"
        }
      ]
