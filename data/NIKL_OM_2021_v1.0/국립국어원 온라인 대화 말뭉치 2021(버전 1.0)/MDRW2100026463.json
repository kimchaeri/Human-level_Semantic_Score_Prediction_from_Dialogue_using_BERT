      "utterance": [
        {
          "id": "MDRW2100026463.1.1",
          "form": "안녕하세요 책읽는거 좋아하시나요?!",
          "original_form": "안녕하세요 책읽는거 좋아하시나요?!",
          "speaker_id": "1",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026463.1.2",
          "form": "예전에는 읽으려고 노력했는데 요즘엔 좀 덜 읽긴 하네요ㅎㅎ 그래도 가끔은 읽고 있어요",
          "original_form": "예전에는 읽으려고 노력했는데 요즘엔 좀 덜 읽긴 하네요ㅎㅎ 그래도 가끔은 읽고 있어요",
          "speaker_id": "2",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100026463.1.3",
          "form": "name1님은 독서 좋아하세요?",
          "original_form": "&name1&님은 독서 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100026463.1.4",
          "form": "열심히 읽으려고 노력중입니다ㅎㅎ",
          "original_form": "열심히 읽으려고 노력중입니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100026463.1.5",
          "form": "가장최근에 읽으신 책이 어떤건가요?!",
          "original_form": "가장최근에 읽으신 책이 어떤건가요?!",
          "speaker_id": "1",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026463.1.6",
          "form": "제일 최근에는 어디에서 살것인가 읽었던거 같아요",
          "original_form": "제일 최근에는 어디에서 살것인가 읽었던거 같아요",
          "speaker_id": "2",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100026463.1.7",
          "form": "그 건축가분이 썼던 책",
          "original_form": "그 건축가분이 썼던 책",
          "speaker_id": "2",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100026463.1.8",
          "form": "우와 재밌을것같아요",
          "original_form": "우와 재밌을것같아요",
          "speaker_id": "1",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100026463.1.9",
          "form": "저는 최근에 비혼수업읽었어요",
          "original_form": "저는 최근에 비혼수업읽었어요",
          "speaker_id": "1",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100026463.1.10",
          "form": "쉽게 읽혀서 좋았어요",
          "original_form": "쉽게 읽혀서 좋았어요",
          "speaker_id": "2",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100026463.1.11",
          "form": "비혼수업 내용 어떤가요?",
          "original_form": "비혼수업 내용 어떤가요?",
          "speaker_id": "2",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100026463.1.12",
          "form": "다음에 읽어볼게요!",
          "original_form": "다음에 읽어볼게요!",
          "speaker_id": "1",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100026463.1.13",
          "form": "제가 비혼주의자라서",
          "original_form": "제가 비혼주의자라서",
          "speaker_id": "1",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100026463.1.14",
          "form": "챙겨야할것들 그런내용도있고",
          "original_form": "챙겨야할것들 그런내용도있고",
          "speaker_id": "1",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026463.1.15",
          "form": "무엇보다 공감가서 좋더라고요",
          "original_form": "무엇보다 공감가서 좋더라고요",
          "speaker_id": "1",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026463.1.16",
          "form": "궁금해서 여쭤봤어요",
          "original_form": "궁금해서 여쭤봤어요",
          "speaker_id": "2",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026463.1.17",
          "form": "한번읽어보기에 괜찮은것같아요ㅎㅎ",
          "original_form": "한번읽어보기에 괜찮은것같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026463.1.18",
          "form": "저는 그 나쁜페미니스트 읽었었는데",
          "original_form": "저는 그 나쁜페미니스트 읽었었는데",
          "speaker_id": "2",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026463.1.19",
          "form": "그거 내용이 좋더라고요",
          "original_form": "그거 내용이 좋더라고요",
          "speaker_id": "2",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026463.1.20",
          "form": "여자고 남자고 상관없이 필독서 만들고 싶은 책이었어요",
          "original_form": "여자고 남자고 상관없이 필독서 만들고 싶은 책이었어요",
          "speaker_id": "2",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026463.1.21",
          "form": "저도봤어요!",
          "original_form": "저도봤어요!",
          "speaker_id": "1",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100026463.1.22",
          "form": "역시 유명한책이군요",
          "original_form": "역시 유명한책이군요",
          "speaker_id": "1",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100026463.1.23",
          "form": "유명한 책아니었음 저까지 읽을리는 없었을거 같기도 해요 ㅋㅋㅋㅋ",
          "original_form": "유명한 책아니었음 저까지 읽을리는 없었을거 같기도 해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:37"
        }
      ]
    }
