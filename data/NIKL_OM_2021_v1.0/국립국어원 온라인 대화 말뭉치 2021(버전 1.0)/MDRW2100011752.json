      "utterance": [
        {
          "id": "MDRW2100011752.1.1",
          "form": "요새 티켓두장 씩 줘요",
          "original_form": "요새 티켓두장 씩 줘요",
          "speaker_id": "1",
          "time": "20210824 15:45"
        },
        {
          "id": "MDRW2100011752.1.2",
          "form": "헐 진짜요?",
          "original_form": "헐 진짜요?",
          "speaker_id": "2",
          "time": "20210824 15:45"
        },
        {
          "id": "MDRW2100011752.1.3",
          "form": "헌혈자가없대요ㅜ",
          "original_form": "헌혈자가없대요ㅜ",
          "speaker_id": "1",
          "time": "20210824 15:46"
        },
        {
          "id": "MDRW2100011752.1.4",
          "form": "코로나때문에 아무래도 안가나봐요ㅠㅠ",
          "original_form": "코로나때문에 아무래도 안가나봐요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 15:46"
        },
        {
          "id": "MDRW2100011752.1.5",
          "form": "코로나끝나면 빨리 콘서트도 가고싶어요",
          "original_form": "코로나끝나면 빨리 콘서트도 가고싶어요",
          "speaker_id": "2",
          "time": "20210824 15:46"
        },
        {
          "id": "MDRW2100011752.1.6",
          "form": "마스크벗고 영화관에서 팝콘도 먹고싶구ㅠㅠ",
          "original_form": "마스크벗고 영화관에서 팝콘도 먹고싶구ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 15:47"
        },
        {
          "id": "MDRW2100011752.1.7",
          "form": "팝콘도 팔지만 못먹잖아요 ㅋㅋㅋ",
          "original_form": "팝콘도 팔지만 못먹잖아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:56"
        },
        {
          "id": "MDRW2100011752.1.8",
          "form": "포장해가야한대요ㅜ",
          "original_form": "포장해가야한대요ㅜ",
          "speaker_id": "1",
          "time": "20210824 15:56"
        },
        {
          "id": "MDRW2100011752.1.9",
          "form": "아 그것도 포장이라니ㅋㅋㅠ",
          "original_form": "아 그것도 포장이라니ㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210824 16:00"
        },
        {
          "id": "MDRW2100011752.1.10",
          "form": "웃프네요",
          "original_form": "웃프네요",
          "speaker_id": "2",
          "time": "20210824 16:00"
        },
        {
          "id": "MDRW2100011752.1.11",
          "form": "네 ㅋㅋ요새 극장사정이 말이아닌거 같아요",
          "original_form": "네 ㅋㅋ요새 극장사정이 말이아닌거 같아요",
          "speaker_id": "1",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011752.1.12",
          "form": "그래서그런가 티켓 금액은 계속 오르더라구요",
          "original_form": "그래서그런가 티켓 금액은 계속 오르더라구요",
          "speaker_id": "2",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011752.1.13",
          "form": "깜짝 놀랐어요",
          "original_form": "깜짝 놀랐어요",
          "speaker_id": "2",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011752.1.14",
          "form": "너무 비싸서",
          "original_form": "너무 비싸서",
          "speaker_id": "2",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011752.1.15",
          "form": "사람들이 더안가는거같아요",
          "original_form": "사람들이 더안가는거같아요",
          "speaker_id": "1",
          "time": "20210824 16:06"
        },
        {
          "id": "MDRW2100011752.1.16",
          "form": "모가디슈보러갔는데 전세낸거같더라구요",
          "original_form": "모가디슈보러갔는데 전세낸거같더라구요",
          "speaker_id": "1",
          "time": "20210824 16:06"
        },
        {
          "id": "MDRW2100011752.1.17",
          "form": "랑종 같은 공포영화도 보세요?",
          "original_form": "랑종 같은 공포영화도 보세요?",
          "speaker_id": "2",
          "time": "20210824 16:09"
        },
        {
          "id": "MDRW2100011752.1.18",
          "form": "오우 노웅ㅜㅜㅜ",
          "original_form": "오우 노웅ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 16:09"
        },
        {
          "id": "MDRW2100011752.1.19",
          "form": "ㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210824 16:09"
        },
        {
          "id": "MDRW2100011752.1.20",
          "form": "넘 무서워요ㅜㅜ띠꾸님은요",
          "original_form": "넘 무서워요ㅜㅜ띠꾸님은요",
          "speaker_id": "1",
          "time": "20210824 16:09"
        },
        {
          "id": "MDRW2100011752.1.21",
          "form": "저두 절대",
          "original_form": "저두 절대",
          "speaker_id": "2",
          "time": "20210824 16:10"
        },
        {
          "id": "MDRW2100011752.1.22",
          "form": "돈준다고해도 안볼거같아요",
          "original_form": "돈준다고해도 안볼거같아요",
          "speaker_id": "2",
          "time": "20210824 16:10"
        },
        {
          "id": "MDRW2100011752.1.23",
          "form": "특히 만약 영화관에서?",
          "original_form": "특히 만약 영화관에서?",
          "speaker_id": "2",
          "time": "20210824 16:10"
        },
        {
          "id": "MDRW2100011752.1.24",
          "form": "어휴",
          "original_form": "어휴",
          "speaker_id": "2",
          "time": "20210824 16:10"
        },
        {
          "id": "MDRW2100011752.1.25",
          "form": "생각만해도 아찔해여",
          "original_form": "생각만해도 아찔해여",
          "speaker_id": "2",
          "time": "20210824 16:10"
        },
        {
          "id": "MDRW2100011752.1.26",
          "form": "노우..저는 친구가 만약 속이고 데려간다면",
          "original_form": "노우..저는 친구가 만약 속이고 데려간다면",
          "speaker_id": "1",
          "time": "20210824 16:10"
        },
        {
          "id": "MDRW2100011752.1.27",
          "form": "한동안 짱무섭다고하도그래서 너무 궁금햇는데",
          "original_form": "한동안 짱무섭다고하도그래서 너무 궁금햇는데",
          "speaker_id": "2",
          "time": "20210824 16:10"
        },
        {
          "id": "MDRW2100011752.1.28",
          "form": "극장탈출합니다",
          "original_form": "극장탈출합니다",
          "speaker_id": "1",
          "time": "20210824 16:10"
        },
        {
          "id": "MDRW2100011752.1.29",
          "form": "그래도 안봤어요",
          "original_form": "그래도 안봤어요",
          "speaker_id": "2",
          "time": "20210824 16:10"
        },
        {
          "id": "MDRW2100011752.1.30",
          "form": "ㅋㄱㅋㅋㅋ친구 때릴거같아요",
          "original_form": "ㅋㄱㅋㅋㅋ친구 때릴거같아요",
          "speaker_id": "2",
          "time": "20210824 16:10"
        },
        {
          "id": "MDRW2100011752.1.31",
          "form": "정권지르기로 후기줘야함",
          "original_form": "정권지르기로 후기줘야함",
          "speaker_id": "1",
          "time": "20210824 16:11"
        },
        {
          "id": "MDRW2100011752.1.32",
          "form": "명치에",
          "original_form": "명치에",
          "speaker_id": "1",
          "time": "20210824 16:11"
        },
        {
          "id": "MDRW2100011752.1.33",
          "form": "ㅋㅋㅋㄱ맞아요",
          "original_form": "ㅋㅋㅋㄱ맞아요",
          "speaker_id": "2",
          "time": "20210824 16:11"
        },
        {
          "id": "MDRW2100011752.1.34",
          "form": "속인 값이다...",
          "original_form": "속인 값이다...",
          "speaker_id": "2",
          "time": "20210824 16:11"
        }
      ]
