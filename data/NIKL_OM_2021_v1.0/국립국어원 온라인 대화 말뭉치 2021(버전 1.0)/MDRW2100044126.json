      "utterance": [
        {
          "id": "MDRW2100044126.1.1",
          "form": "졸립당",
          "original_form": "졸립당",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100044126.1.2",
          "form": "건강을 위해 좀 이따 자야지",
          "original_form": "건강을 위해 좀 이따 자야지",
          "speaker_id": "1",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100044126.1.3",
          "form": "얼른 자자",
          "original_form": "얼른 자자",
          "speaker_id": "2",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100044126.1.4",
          "form": "응 숙면이 건강에 최고지",
          "original_form": "응 숙면이 건강에 최고지",
          "speaker_id": "1",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044126.1.5",
          "form": "오늘은 일찍 자려고 했는디",
          "original_form": "오늘은 일찍 자려고 했는디",
          "speaker_id": "1",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044126.1.6",
          "form": "건강을 위해서도 얼른 자야지",
          "original_form": "건강을 위해서도 얼른 자야지",
          "speaker_id": "2",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044126.1.7",
          "form": "눈도 피곤할텐데 액정 오래봐서",
          "original_form": "눈도 피곤할텐데 액정 오래봐서",
          "speaker_id": "2",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044126.1.8",
          "form": "응 곧 잘겨",
          "original_form": "응 곧 잘겨",
          "speaker_id": "1",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100044126.1.9",
          "form": "고지가 눈앞이라 포기하긴 아쉽네",
          "original_form": "고지가 눈앞이라 포기하긴 아쉽네",
          "speaker_id": "1",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100044126.1.10",
          "form": "얼른 끝내고 자자",
          "original_form": "얼른 끝내고 자자",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044126.1.11",
          "form": "그러게 힘내자! 목표 달성하면 정신건강에도 좋지",
          "original_form": "그러게 힘내자! 목표 달성하면 정신건강에도 좋지",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044126.1.12",
          "form": "응 그걸로 맛있는 것도 먹고!",
          "original_form": "응 그걸로 맛있는 것도 먹고!",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044126.1.13",
          "form": "응 덕분에 요거트 먹어서 피부도 좋아질듯",
          "original_form": "응 덕분에 요거트 먹어서 피부도 좋아질듯",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044126.1.14",
          "form": "장건강이랑",
          "original_form": "장건강이랑",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044126.1.15",
          "form": "ㅋㅋㅋ응응",
          "original_form": "ㅋㅋㅋ응응",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044126.1.16",
          "form": "손가락 관절운동도 됐으려나?",
          "original_form": "손가락 관절운동도 됐으려나?",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044126.1.17",
          "form": "머리도 굴리고 ㅋㅋ",
          "original_form": "머리도 굴리고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044126.1.18",
          "form": "차치키로 유산균 충전",
          "original_form": "차치키로 유산균 충전",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044126.1.19",
          "form": "눈건강은 해친듯",
          "original_form": "눈건강은 해친듯",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044126.1.20",
          "form": "건조하지?",
          "original_form": "건조하지?",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044126.1.21",
          "form": "인공눈물좀 넣어",
          "original_form": "인공눈물좀 넣어",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
