      "utterance": [
        {
          "id": "MDRW2100016151.1.1",
          "form": "자녀들의 음악도 좋아하시나요?",
          "original_form": "자녀들의 음악도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 10:22"
        },
        {
          "id": "MDRW2100016151.1.2",
          "form": "아이가 맨날 음악 듣던데요",
          "original_form": "아이가 맨날 음악 듣던데요",
          "speaker_id": "1",
          "time": "20210826 10:22"
        },
        {
          "id": "MDRW2100016151.1.3",
          "form": "이어폰 달고 살아요 ㅎㅎ",
          "original_form": "이어폰 달고 살아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 10:22"
        },
        {
          "id": "MDRW2100016151.1.4",
          "form": "어떤 음악요?",
          "original_form": "어떤 음악요?",
          "speaker_id": "1",
          "time": "20210826 10:23"
        },
        {
          "id": "MDRW2100016151.1.5",
          "form": "요즘엔 모녀 아미도 많다더라고요",
          "original_form": "요즘엔 모녀 아미도 많다더라고요",
          "speaker_id": "2",
          "time": "20210826 10:24"
        },
        {
          "id": "MDRW2100016151.1.6",
          "form": "아 ㅎㅎ",
          "original_form": "아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 10:24"
        },
        {
          "id": "MDRW2100016151.1.7",
          "form": "오히려 제가 방탄좋아하고",
          "original_form": "오히려 제가 방탄좋아하고",
          "speaker_id": "1",
          "time": "20210826 10:24"
        },
        {
          "id": "MDRW2100016151.1.8",
          "form": "딸은 안좋아해요",
          "original_form": "딸은 안좋아해요",
          "speaker_id": "1",
          "time": "20210826 10:24"
        },
        {
          "id": "MDRW2100016151.1.9",
          "form": "취향이 틀려요",
          "original_form": "취향이 틀려요",
          "speaker_id": "1",
          "time": "20210826 10:24"
        },
        {
          "id": "MDRW2100016151.1.10",
          "form": "전 요새는 발라드",
          "original_form": "전 요새는 발라드",
          "speaker_id": "1",
          "time": "20210826 10:24"
        },
        {
          "id": "MDRW2100016151.1.11",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:24"
        },
        {
          "id": "MDRW2100016151.1.12",
          "form": "들어요",
          "original_form": "들어요",
          "speaker_id": "1",
          "time": "20210826 10:24"
        },
        {
          "id": "MDRW2100016151.1.13",
          "form": "놀면뭐하니 이후로",
          "original_form": "놀면뭐하니 이후로",
          "speaker_id": "2",
          "time": "20210826 10:24"
        },
        {
          "id": "MDRW2100016151.1.14",
          "form": "msg sg 워너비",
          "original_form": "msg sg 워너비",
          "speaker_id": "1",
          "time": "20210826 10:25"
        },
        {
          "id": "MDRW2100016151.1.15",
          "form": "그때감성돋아요",
          "original_form": "그때감성돋아요",
          "speaker_id": "2",
          "time": "20210826 10:25"
        },
        {
          "id": "MDRW2100016151.1.16",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 10:25"
        },
        {
          "id": "MDRW2100016151.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 10:25"
        },
        {
          "id": "MDRW2100016151.1.18",
          "form": "경연노래도 좋고 ㅎㅎ",
          "original_form": "경연노래도 좋고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 10:25"
        },
        {
          "id": "MDRW2100016151.1.19",
          "form": "2ne1 도요",
          "original_form": "2ne1 도요",
          "speaker_id": "1",
          "time": "20210826 10:25"
        },
        {
          "id": "MDRW2100016151.1.20",
          "form": "우리나라 노래 잘 하는 사람 왜케 많은지",
          "original_form": "우리나라 노래 잘 하는 사람 왜케 많은지",
          "speaker_id": "2",
          "time": "20210826 10:25"
        },
        {
          "id": "MDRW2100016151.1.21",
          "form": "와 저도 투애니원ㅠㅠ",
          "original_form": "와 저도 투애니원ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 10:25"
        },
        {
          "id": "MDRW2100016151.1.22",
          "form": "정말 깜놀요",
          "original_form": "정말 깜놀요",
          "speaker_id": "1",
          "time": "20210826 10:25"
        },
        {
          "id": "MDRW2100016151.1.23",
          "form": "다붙여줘서 좋았어요",
          "original_form": "다붙여줘서 좋았어요",
          "speaker_id": "1",
          "time": "20210826 10:26"
        },
        {
          "id": "MDRW2100016151.1.24",
          "form": "누굴 떨어뜨리겠어요",
          "original_form": "누굴 떨어뜨리겠어요",
          "speaker_id": "1",
          "time": "20210826 10:26"
        },
        {
          "id": "MDRW2100016151.1.25",
          "form": "간절함이 ㅜㅜ보였어요",
          "original_form": "간절함이 ㅜㅜ보였어요",
          "speaker_id": "1",
          "time": "20210826 10:26"
        },
        {
          "id": "MDRW2100016151.1.26",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 10:26"
        },
        {
          "id": "MDRW2100016151.1.27",
          "form": "그러면서 옛노래듣고",
          "original_form": "그러면서 옛노래듣고",
          "speaker_id": "1",
          "time": "20210826 10:26"
        },
        {
          "id": "MDRW2100016151.1.28",
          "form": "결과물도 스타일이 달라서",
          "original_form": "결과물도 스타일이 달라서",
          "speaker_id": "2",
          "time": "20210826 10:26"
        },
        {
          "id": "MDRW2100016151.1.29",
          "form": "오히려 만족",
          "original_form": "오히려 만족",
