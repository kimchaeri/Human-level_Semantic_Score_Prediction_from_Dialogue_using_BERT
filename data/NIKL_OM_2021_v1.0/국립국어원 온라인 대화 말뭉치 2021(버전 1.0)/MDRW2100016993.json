      "utterance": [
        {
          "id": "MDRW2100016993.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 16:43"
        },
        {
          "id": "MDRW2100016993.1.2",
          "form": "안녕하세요 오늘 날씨는 어때요",
          "original_form": "안녕하세요 오늘 날씨는 어때요",
          "speaker_id": "2",
          "time": "20210826 16:44"
        },
        {
          "id": "MDRW2100016993.1.3",
          "form": "오늘 간만에 비가 안오고 맑아요",
          "original_form": "오늘 간만에 비가 안오고 맑아요",
          "speaker_id": "1",
          "time": "20210826 16:44"
        },
        {
          "id": "MDRW2100016993.1.4",
          "form": "여기도 엄청 쨍쨍합니다",
          "original_form": "여기도 엄청 쨍쨍합니다",
          "speaker_id": "2",
          "time": "20210826 16:46"
        },
        {
          "id": "MDRW2100016993.1.5",
          "form": "그래도 기상청 못믿고 긴옷 입었는데 덥네요",
          "original_form": "그래도 기상청 못믿고 긴옷 입었는데 덥네요",
          "speaker_id": "2",
          "time": "20210826 16:46"
        },
        {
          "id": "MDRW2100016993.1.6",
          "form": "긴옷을 입기엔 아직 더위가 가시지 않은것같아요",
          "original_form": "긴옷을 입기엔 아직 더위가 가시지 않은것같아요",
          "speaker_id": "1",
          "time": "20210826 16:47"
        },
        {
          "id": "MDRW2100016993.1.7",
          "form": "저도 아직 짧은옷 ㅋㅋ",
          "original_form": "저도 아직 짧은옷 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:47"
        },
        {
          "id": "MDRW2100016993.1.8",
          "form": "가을을 빨리 부르고싶은 마음에 오버했어요",
          "original_form": "가을을 빨리 부르고싶은 마음에 오버했어요",
          "speaker_id": "2",
          "time": "20210826 16:47"
        },
        {
          "id": "MDRW2100016993.1.9",
          "form": "가을을 향한 염원이 담긴 의상이엇군요 ㅋㅋㅋ",
          "original_form": "가을을 향한 염원이 담긴 의상이엇군요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:49"
        },
        {
          "id": "MDRW2100016993.1.10",
          "form": "네 아직 한여름이네여",
          "original_form": "네 아직 한여름이네여",
          "speaker_id": "2",
          "time": "20210826 16:50"
        },
        {
          "id": "MDRW2100016993.1.11",
          "form": "그래도 밤에는 이제 좀 덜더운것같아요",
          "original_form": "그래도 밤에는 이제 좀 덜더운것같아요",
          "speaker_id": "1",
          "time": "20210826 16:50"
        },
        {
          "id": "MDRW2100016993.1.12",
          "form": "맞아요 밤산책 나가면 기분좋죠",
          "original_form": "맞아요 밤산책 나가면 기분좋죠",
          "speaker_id": "2",
          "time": "20210826 16:51"
        },
        {
          "id": "MDRW2100016993.1.13",
          "form": "맞아요 딱 적당히 선선한것같아요",
          "original_form": "맞아요 딱 적당히 선선한것같아요",
          "speaker_id": "1",
          "time": "20210826 16:52"
        },
        {
          "id": "MDRW2100016993.1.14",
          "form": "8월도 다 끝났네요ㅎㅎ",
          "original_form": "8월도 다 끝났네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 16:52"
        },
        {
          "id": "MDRW2100016993.1.15",
          "form": "가을이 오는건 좋은데 올해가 가는건 너무 싫네요 ㅋㅋㅋㅋㅋ",
          "original_form": "가을이 오는건 좋은데 올해가 가는건 너무 싫네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:53"
        },
        {
          "id": "MDRW2100016993.1.16",
          "form": "진짜 작년이랑 올해는 뭘 안했는데 시간만 갔어요",
          "original_form": "진짜 작년이랑 올해는 뭘 안했는데 시간만 갔어요",
          "speaker_id": "2",
          "time": "20210826 16:53"
        },
        {
          "id": "MDRW2100016993.1.17",
          "form": "여름도 언제 지나가나 햇는데 벌써 지나가는 느낌이고 ㅋㅋㅋㅋ",
          "original_form": "여름도 언제 지나가나 햇는데 벌써 지나가는 느낌이고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
