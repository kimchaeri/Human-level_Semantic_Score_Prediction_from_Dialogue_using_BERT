      "utterance": [
        {
          "id": "MDRW2100032962.1.1",
          "form": "저는bts 콘서트 가고싶어요",
          "original_form": "저는bts 콘서트 가고싶어요",
          "speaker_id": "1",
          "time": "20210906 11:47"
        },
        {
          "id": "MDRW2100032962.1.2",
          "form": "저도요 너무 인기많아서 티켓을 구하기 힘들껄요",
          "original_form": "저도요 너무 인기많아서 티켓을 구하기 힘들껄요",
          "speaker_id": "2",
          "time": "20210906 11:47"
        },
        {
          "id": "MDRW2100032962.1.3",
          "form": "그쪽도 팬인가요",
          "original_form": "그쪽도 팬인가요",
          "speaker_id": "1",
          "time": "20210906 11:47"
        },
        {
          "id": "MDRW2100032962.1.4",
          "form": "네 노래가 희망적이고 건전해서요",
          "original_form": "네 노래가 희망적이고 건전해서요",
          "speaker_id": "2",
          "time": "20210906 11:47"
        },
        {
          "id": "MDRW2100032962.1.5",
          "form": "맞아요 메시지가 좋고 건강해요",
          "original_form": "맞아요 메시지가 좋고 건강해요",
          "speaker_id": "1",
          "time": "20210906 11:47"
        },
        {
          "id": "MDRW2100032962.1.6",
          "form": "세계적으로 인기라 이제 콘서트를 못가겠지요",
          "original_form": "세계적으로 인기라 이제 콘서트를 못가겠지요",
          "speaker_id": "2",
          "time": "20210906 11:48"
        },
        {
          "id": "MDRW2100032962.1.7",
          "form": "군대가기전 콘서트보고싶어요",
          "original_form": "군대가기전 콘서트보고싶어요",
          "speaker_id": "1",
          "time": "20210906 11:48"
        },
        {
          "id": "MDRW2100032962.1.8",
          "form": "bts정도면 군을 면제해주어야 할텐데됴",
          "original_form": "bts정도면 군을 면제해주어야 할텐데됴",
          "speaker_id": "2",
          "time": "20210906 11:48"
        },
        {
          "id": "MDRW2100032962.1.9",
          "form": "맞아요 업적이대단해요",
          "original_form": "맞아요 업적이대단해요",
          "speaker_id": "1",
          "time": "20210906 11:48"
        },
        {
          "id": "MDRW2100032962.1.10",
          "form": "코로나 아니었다면 얼마나 많은 공연을 했을까요",
          "original_form": "코로나 아니었다면 얼마나 많은 공연을 했을까요",
          "speaker_id": "2",
          "time": "20210906 11:48"
        },
        {
          "id": "MDRW2100032962.1.11",
          "form": "많은 아티스트가 힘들죠",
          "original_form": "많은 아티스트가 힘들죠",
          "speaker_id": "1",
          "time": "20210906 11:48"
        },
        {
          "id": "MDRW2100032962.1.12",
          "form": "공연도 오페라도 다 중단되었지요",
          "original_form": "공연도 오페라도 다 중단되었지요",
          "speaker_id": "2",
          "time": "20210906 11:49"
        },
        {
          "id": "MDRW2100032962.1.13",
          "form": "그나마 전시회는 봐요",
          "original_form": "그나마 전시회는 봐요",
