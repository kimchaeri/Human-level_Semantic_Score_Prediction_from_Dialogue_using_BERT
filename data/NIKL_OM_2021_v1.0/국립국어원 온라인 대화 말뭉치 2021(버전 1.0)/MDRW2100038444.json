      "utterance": [
        {
          "id": "MDRW2100038444.1.1",
          "form": "어이구 어째서요 ㅜㅜ",
          "original_form": "어이구 어째서요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100038444.1.2",
          "form": "은행일 있어서 왔는데 아마 점심시간이 지날거같아요 ㅋㅋㅋㅋ",
          "original_form": "은행일 있어서 왔는데 아마 점심시간이 지날거같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100038444.1.3",
          "form": "은행 끝나면 바로 운동가야할듯....",
          "original_form": "은행 끝나면 바로 운동가야할듯....",
          "speaker_id": "1",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100038444.1.4",
          "form": "배고플때 운동하면 잘 안되지않아요? ㅠㅠ",
          "original_form": "배고플때 운동하면 잘 안되지않아요? ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100038444.1.5",
          "form": "저는 운동을 빠지는 한이 잇어도 ㅋㅋ 밥먹는 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 운동을 빠지는 한이 잇어도 ㅋㅋ 밥먹는 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100038444.1.6",
          "form": "그건 그래요 ㅠ 하지만 너무 먹어도 ㅋㅋㅋㅋ 배가아파요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그건 그래요 ㅠ 하지만 너무 먹어도 ㅋㅋㅋㅋ 배가아파요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100038444.1.7",
          "form": "그 갈비뼈 부근 아픈거 ㅋㅋㅋㅋㅋ",
          "original_form": "그 갈비뼈 부근 아픈거 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100038444.1.8",
          "form": "진짜 너무 아프죠",
          "original_form": "진짜 너무 아프죠",
          "speaker_id": "2",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100038444.1.9",
          "form": "하.. 우짤수가업ㄱ내요",
          "original_form": "하.. 우짤수가업ㄱ내요",
          "speaker_id": "2",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100038444.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ 그래서 차라리 운동 딱 끝내고 깔끔하게 씻고 밥먹을 예정입니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그래서 차라리 운동 딱 끝내고 깔끔하게 씻고 밥먹을 예정입니다",
          "speaker_id": "1",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100038444.1.11",
          "form": "근데 뭐먹죠...",
          "original_form": "근데 뭐먹죠...",
          "speaker_id": "1",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100038444.1.12",
          "form": "저는.. 닭가슴살에 단호박 먹었어요 ㅎ..",
          "original_form": "저는.. 닭가슴살에 단호박 먹었어요 ㅎ..",
          "speaker_id": "2",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100038444.1.13",
          "form": "오쿡꺼 맛있더라고요",
          "original_form": "오쿡꺼 맛있더라고요",
          "speaker_id": "2",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100038444.1.14",
          "form": "헉... 다이어트중이신가요..?",
          "original_form": "헉... 다이어트중이신가요..?",
          "speaker_id": "1",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100038444.1.15",
          "form": "넹 ㅎ...",
          "original_form": "넹 ㅎ...",
          "speaker_id": "2",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100038444.1.16",
          "form": "체지방량좀 줄이려고",
          "original_form": "체지방량좀 줄이려고",
          "speaker_id": "2",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100038444.1.17",
          "form": "애쓰고잇어요 ㅠ",
          "original_form": "애쓰고잇어요 ㅠ",
          "speaker_id": "2",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100038444.1.18",
          "form": "아하 글쿤요",
          "original_form": "아하 글쿤요",
          "speaker_id": "1",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100038444.1.19",
          "form": "전 힘들거같애욬ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 힘들거같애욬ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:31"
        },
        {
          "id": "MDRW2100038444.1.20",
          "form": "운동하는데 밥이라도 잘먹어야",
          "original_form": "운동하는데 밥이라도 잘먹어야",
          "speaker_id": "2",
          "time": "20210902 13:31"
        },
        {
          "id": "MDRW2100038444.1.21",
          "form": "행복하죠 ㅠ",
          "original_form": "행복하죠 ㅠ",
          "speaker_id": "2",
          "time": "20210902 13:31"
        },
        {
          "id": "MDRW2100038444.1.22",
          "form": "저는 안행복해요 ㅋㅋㅋㅋㅋ",
