      "utterance": [
        {
          "id": "MDRW2100038333.1.1",
          "form": "저는 카페일하는데 카페 좋아하시나요??",
          "original_form": "저는 카페일하는데 카페 좋아하시나요??",
          "speaker_id": "2",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100038333.1.2",
          "form": "카페 좋아해요",
          "original_form": "카페 좋아해요",
          "speaker_id": "1",
          "time": "20210902 10:27"
        },
        {
          "id": "MDRW2100038333.1.3",
          "form": "가끔 오래 앉아있기도 해요",
          "original_form": "가끔 오래 앉아있기도 해요",
          "speaker_id": "1",
          "time": "20210902 10:27"
        },
        {
          "id": "MDRW2100038333.1.4",
          "form": "저는 조용한 카페만 찾는편이에요",
          "original_form": "저는 조용한 카페만 찾는편이에요",
          "speaker_id": "2",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100038333.1.5",
          "form": "조용한 카페가 아무래도 좋죠",
          "original_form": "조용한 카페가 아무래도 좋죠",
          "speaker_id": "1",
          "time": "20210902 10:34"
        },
        {
          "id": "MDRW2100038333.1.6",
          "form": "신경쓰이는것도 없구요",
          "original_form": "신경쓰이는것도 없구요",
          "speaker_id": "1",
          "time": "20210902 10:34"
        },
        {
          "id": "MDRW2100038333.1.7",
          "form": "맞아요 가만히 하늘쳐다보고있어욬ㅋㅋㅋ",
          "original_form": "맞아요 가만히 하늘쳐다보고있어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:42"
        },
        {
          "id": "MDRW2100038333.1.8",
          "form": "저는 커피향으로 가득찬 카페 좋아해요",
          "original_form": "저는 커피향으로 가득찬 카페 좋아해요",
          "speaker_id": "1",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100038333.1.9",
          "form": "저는 저번에 산속에있는 카페갔는데",
          "original_form": "저는 저번에 산속에있는 카페갔는데",
          "speaker_id": "2",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100038333.1.10",
          "form": "벽난로쓰는집인데너무 조터라구요 ㅠㅠ",
          "original_form": "벽난로쓰는집인데너무 조터라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100038333.1.11",
          "form": "와 벽난로 쓰는곳도 있군요",
          "original_form": "와 벽난로 쓰는곳도 있군요",
          "speaker_id": "1",
          "time": "20210902 10:52"
        },
        {
          "id": "MDRW2100038333.1.12",
          "form": "진짜 아기자기해요",
          "original_form": "진짜 아기자기해요",
          "speaker_id": "2",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100038333.1.13",
          "form": "한번 가보고 싶어요",
          "original_form": "한번 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210902 11:13"
        },
        {
          "id": "MDRW2100038333.1.14",
          "form": "뭐라고 찾아야 나올까요",
          "original_form": "뭐라고 찾아야 나올까요",
          "speaker_id": "1",
          "time": "20210902 11:13"
        },
        {
          "id": "MDRW2100038333.1.15",
          "form": "어음... 감성있는카페??",
          "original_form": "어음... 감성있는카페??",
          "speaker_id": "2",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100038333.1.16",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100038333.1.17",
          "form": "ㅋㅋ 찾아보겠습니다",
          "original_form": "ㅋㅋ 찾아보겠습니다",
          "speaker_id": "1",
          "time": "20210902 11:40"
        },
        {
          "id": "MDRW2100038333.1.18",
          "form": "다른지역으로도 찾아보세요",
          "original_form": "다른지역으로도 찾아보세요",
          "speaker_id": "2",
          "time": "20210902 11:45"
        },
        {
          "id": "MDRW2100038333.1.19",
          "form": "저는 먼허따면",
          "original_form": "저는 먼허따면",
          "speaker_id": "2",
          "time": "20210902 11:46"
        },
        {
          "id": "MDRW2100038333.1.20",
          "form": "다른데가고싶어서",
          "original_form": "다른데가고싶어서",
          "speaker_id": "2",
          "time": "20210902 11:46"
        },
        {
          "id": "MDRW2100038333.1.21",
          "form": "캡쳐해두거나 그래요",
          "original_form": "캡쳐해두거나 그래요",
          "speaker_id": "2",
          "time": "20210902 11:46"
        },
        {
          "id": "MDRW2100038333.1.22",
          "form": "와 준비도 하시는군요",
          "original_form": "와 준비도 하시는군요",
          "speaker_id": "1",
          "time": "20210902 11:47"
        },
        {
          "id": "MDRW2100038333.1.23",
          "form": "당연하죠 진심인편",
          "original_form": "당연하죠 진심인편",
          "speaker_id": "2",
          "time": "20210902 11:48"
        },
        {
          "id": "MDRW2100038333.1.24",
          "form": "저도 많이 찾아놔야겠네요",
          "original_form": "저도 많이 찾아놔야겠네요",
          "speaker_id": "1",
          "time": "20210902 11:49"
        },
        {
          "id": "MDRW2100038333.1.25",
          "form": "운전면허 따고싶네요 ㅠ",
          "original_form": "운전면허 따고싶네요 ㅠ",
          "speaker_id": "2",
          "time": "20210902 12:03"
        },
        {
          "id": "MDRW2100038333.1.26",
          "form": "저두요... 친구들 면허딴거 보면 부러워요",
          "original_form": "저두요... 친구들 면허딴거 보면 부러워요",
          "speaker_id": "1",
          "time": "20210902 12:04"
        },
        {
          "id": "MDRW2100038333.1.27",
          "form": "맞아요 어째 저래 잘따는걸까요 ㅋㅋ",
          "original_form": "맞아요 어째 저래 잘따는걸까요 ㅋㅋ",
