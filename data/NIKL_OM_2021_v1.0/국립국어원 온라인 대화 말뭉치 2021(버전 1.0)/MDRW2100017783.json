      "utterance": [
        {
          "id": "MDRW2100017783.1.1",
          "form": "오늘 날씨 참 덥더라죠 ㅋㅋ",
          "original_form": "오늘 날씨 참 덥더라죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:06"
        },
        {
          "id": "MDRW2100017783.1.2",
          "form": "맞아요 선선해졌다 싶엇는데",
          "original_form": "맞아요 선선해졌다 싶엇는데",
          "speaker_id": "1",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017783.1.3",
          "form": "다시더워졌어요",
          "original_form": "다시더워졌어요",
          "speaker_id": "1",
          "time": "20210826 21:08"
        },
        {
          "id": "MDRW2100017783.1.4",
          "form": "맞아요 ㅜ 오늘 에어컨틀었더라죠",
          "original_form": "맞아요 ㅜ 오늘 에어컨틀었더라죠",
          "speaker_id": "2",
          "time": "20210826 21:09"
        },
        {
          "id": "MDRW2100017783.1.5",
          "form": "비가오려는지 습하기도하더라구요",
          "original_form": "비가오려는지 습하기도하더라구요",
          "speaker_id": "1",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100017783.1.6",
          "form": "장마가 아직 완전히 간게 아닌거같아요",
          "original_form": "장마가 아직 완전히 간게 아닌거같아요",
          "speaker_id": "2",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017783.1.7",
          "form": "그니까요 ㅠ 요며칠 창문열고잤는데",
          "original_form": "그니까요 ㅠ 요며칠 창문열고잤는데",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017783.1.8",
          "form": "지금너무습해서 에어컨 틀었어요",
          "original_form": "지금너무습해서 에어컨 틀었어요",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017783.1.9",
          "form": "아 얼른 가을옷 장만하고싶네요",
          "original_form": "아 얼른 가을옷 장만하고싶네요",
          "speaker_id": "2",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100017783.1.10",
          "form": "저는 이미 가을 니트를 샀습니다 ㅋㅋㅋㅋ",
          "original_form": "저는 이미 가을 니트를 샀습니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017783.1.11",
          "form": "오 ㅋㅋㅋ 얼리버드시네요 ㅋㅋㅋ",
          "original_form": "오 ㅋㅋㅋ 얼리버드시네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017783.1.12",
          "form": "저두 쇼핑가고싶어요 근데 날씨가 오락가락해서 나갈 기분이안나요",
          "original_form": "저두 쇼핑가고싶어요 근데 날씨가 오락가락해서 나갈 기분이안나요",
          "speaker_id": "2",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017783.1.13",
          "form": "맞아요 아우터 사고싶은데",
          "original_form": "맞아요 아우터 사고싶은데",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017783.1.14",
          "form": "쇼핑하러가기에는 귀찮네요ㅠ",
          "original_form": "쇼핑하러가기에는 귀찮네요ㅠ",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017783.1.15",
          "form": "맞아요 이래서 인쇼에 눈을돌리게되더라고요..",
          "original_form": "맞아요 이래서 인쇼에 눈을돌리게되더라고요..",
