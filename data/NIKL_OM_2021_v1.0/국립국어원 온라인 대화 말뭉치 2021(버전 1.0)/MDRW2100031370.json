      "utterance": [
        {
          "id": "MDRW2100031370.1.1",
          "form": "저는 주로 해변 쪽 계속 걸었어요 그리고 중앙시장 가고",
          "original_form": "저는 주로 해변 쪽 계속 걸었어요 그리고 중앙시장 가고",
          "speaker_id": "2",
          "time": "20210904 20:53"
        },
        {
          "id": "MDRW2100031370.1.2",
          "form": "아 힐링여행이네요",
          "original_form": "아 힐링여행이네요",
          "speaker_id": "1",
          "time": "20210904 20:53"
        },
        {
          "id": "MDRW2100031370.1.3",
          "form": "맞아요 맛집투어하고ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 맛집투어하고ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 20:53"
        },
        {
          "id": "MDRW2100031370.1.4",
          "form": "커피콩빵이 되게 맛있어요",
          "original_form": "커피콩빵이 되게 맛있어요",
          "speaker_id": "2",
          "time": "20210904 20:53"
        },
        {
          "id": "MDRW2100031370.1.5",
          "form": "맛집은 어디가 가장 좋으셨어요??",
          "original_form": "맛집은 어디가 가장 좋으셨어요??",
          "speaker_id": "1",
          "time": "20210904 20:53"
        },
        {
          "id": "MDRW2100031370.1.6",
          "form": "아 커피콩빵 진짜요!!!!",
          "original_form": "아 커피콩빵 진짜요!!!!",
          "speaker_id": "1",
          "time": "20210904 20:54"
        },
        {
          "id": "MDRW2100031370.1.7",
          "form": "별 거 안들 거 간 거 같은데 맛있더라구요 ㅋㅋㅋㅋ",
          "original_form": "별 거 안들 거 간 거 같은데 맛있더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:54"
        },
        {
          "id": "MDRW2100031370.1.8",
          "form": "그쵸ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그쵸ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 20:54"
        },
        {
          "id": "MDRW2100031370.1.9",
          "form": "저는 맛집이라고 하기 좀 민망한데",
          "original_form": "저는 맛집이라고 하기 좀 민망한데",
          "speaker_id": "2",
          "time": "20210904 20:55"
        },
        {
          "id": "MDRW2100031370.1.10",
          "form": "그 중앙시장에",
          "original_form": "그 중앙시장에",
          "speaker_id": "2",
          "time": "20210904 20:55"
        },
        {
          "id": "MDRW2100031370.1.11",
          "form": "꽃게집이 있어요",
          "original_form": "꽃게집이 있어요",
          "speaker_id": "2",
          "time": "20210904 20:55"
        },
        {
          "id": "MDRW2100031370.1.12",
          "form": "진짜 맛있었어요",
          "original_form": "진짜 맛있었어요",
          "speaker_id": "2",
          "time": "20210904 20:55"
        },
        {
          "id": "MDRW2100031370.1.13",
          "form": "우와 꽃게찜을 파는 건가요??",
          "original_form": "우와 꽃게찜을 파는 건가요??",
          "speaker_id": "1",
          "time": "20210904 20:55"
        },
        {
          "id": "MDRW2100031370.1.14",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210904 20:55"
        },
        {
          "id": "MDRW2100031370.1.15",
          "form": "꽃게 쪄주고 볶음밥 해주고",
          "original_form": "꽃게 쪄주고 볶음밥 해주고",
          "speaker_id": "2",
          "time": "20210904 20:55"
        },
        {
          "id": "MDRW2100031370.1.16",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210904 20:55"
        },
        {
          "id": "MDRW2100031370.1.17",
          "form": "꽃게 맛있겠어요....!!!!",
          "original_form": "꽃게 맛있겠어요....!!!!",
          "speaker_id": "1",
          "time": "20210904 20:56"
        },
        {
          "id": "MDRW2100031370.1.18",
          "form": "진짜 맛있더라구요",
          "original_form": "진짜 맛있더라구요",
          "speaker_id": "2",
          "time": "20210904 20:56"
        },
        {
          "id": "MDRW2100031370.1.19",
          "form": "볶음밥까지 진짜 가도싶네요",
          "original_form": "볶음밥까지 진짜 가도싶네요",
          "speaker_id": "1",
          "time": "20210904 20:56"
        },
        {
          "id": "MDRW2100031370.1.20",
          "form": "꽃게라면도 파니까 한 번 가보세요!!",
          "original_form": "꽃게라면도 파니까 한 번 가보세요!!",
          "speaker_id": "2",
          "time": "20210904 20:56"
        },
        {
          "id": "MDRW2100031370.1.21",
          "form": "와 해장하기 딱이네요",
          "original_form": "와 해장하기 딱이네요",
          "speaker_id": "1",
          "time": "20210904 20:57"
        },
        {
          "id": "MDRW2100031370.1.22",
          "form": "네 진짜 나중에 숙박 잡아서 가보고싶네요",
          "original_form": "네 진짜 나중에 숙박 잡아서 가보고싶네요",
          "speaker_id": "1",
          "time": "20210904 20:57"
        },
        {
          "id": "MDRW2100031370.1.23",
          "form": "ㅋㅋㅋㅋㅋ저도 숙박 잡았는데",
          "original_form": "ㅋㅋㅋㅋㅋ저도 숙박 잡았는데",
          "speaker_id": "2",
          "time": "20210904 20:57"
        },
        {
          "id": "MDRW2100031370.1.24",
          "form": "진짜 먹고 해장하기 딱 좋았어요",
          "original_form": "진짜 먹고 해장하기 딱 좋았어요",
          "speaker_id": "2",
          "time": "20210904 20:57"
        },
        {
          "id": "MDRW2100031370.1.25",
          "form": "아침으로 딱이네요 진짜",
          "original_form": "아침으로 딱이네요 진짜",
          "speaker_id": "1",
          "time": "20210904 20:57"
        },
        {
          "id": "MDRW2100031370.1.26",
          "form": "대게집은 많아도 꽃게집은 은근 찾기 힘든 거 같은데 잘 찾아가셨네요 ㅋㅋㅋ",
          "original_form": "대게집은 많아도 꽃게집은 은근 찾기 힘든 거 같은데 잘 찾아가셨네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:58"
        },
        {
          "id": "MDRW2100031370.1.27",
          "form": "맞아요 거기가 킹크랩도 하고 다양하더라고요!",
          "original_form": "맞아요 거기가 킹크랩도 하고 다양하더라고요!",
