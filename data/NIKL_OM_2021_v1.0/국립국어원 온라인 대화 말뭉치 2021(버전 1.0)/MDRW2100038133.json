      "utterance": [
        {
          "id": "MDRW2100038133.1.1",
          "form": "필라테스같은거 받아볼까했는데 또 넘비싸구",
          "original_form": "필라테스같은거 받아볼까했는데 또 넘비싸구",
          "speaker_id": "2",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100038133.1.2",
          "form": "강사도 이쪽지역에선 괜찮으신분도 별로없더라구여",
          "original_form": "강사도 이쪽지역에선 괜찮으신분도 별로없더라구여",
          "speaker_id": "2",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100038133.1.3",
          "form": "name2님 어느쪽 사시는데요?",
          "original_form": "&name2&님 어느쪽 사시는데요?",
          "speaker_id": "1",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100038133.1.4",
          "form": "저 필라테스 얼마전에 시작했어요!",
          "original_form": "저 필라테스 얼마전에 시작했어요!",
          "speaker_id": "1",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100038133.1.5",
          "form": "저 경남이에여",
          "original_form": "저 경남이에여",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100038133.1.6",
          "form": "1회에 얼마로 끊으셨어요?",
          "original_form": "1회에 얼마로 끊으셨어요?",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100038133.1.7",
          "form": "25회에 35만원이에요!",
          "original_form": "25회에 35만원이에요!",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100038133.1.8",
          "form": "근데 이게",
          "original_form": "근데 이게",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100038133.1.9",
          "form": "친구가 사장님이랑 친해서",
          "original_form": "친구가 사장님이랑 친해서",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100038133.1.10",
          "form": "와아",
          "original_form": "와아",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100038133.1.11",
          "form": "깎아준거예요ㅠ",
          "original_form": "깎아준거예요ㅠ",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100038133.1.12",
          "form": "아 경남이면 여기랑 멀구나..",
          "original_form": "아 경남이면 여기랑 멀구나..",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100038133.1.13",
          "form": "아 할인해준거였군여 역시ㅜ",
          "original_form": "아 할인해준거였군여 역시ㅜ",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100038133.1.14",
          "form": "저는 경기 살아요!",
          "original_form": "저는 경기 살아요!",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100038133.1.15",
          "form": "근데 8대 1이긴 해요,,",
          "original_form": "근데 8대 1이긴 해요,,",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100038133.1.16",
          "form": "돈만 많으면 2대 1이라도 할텐데...",
          "original_form": "돈만 많으면 2대 1이라도 할텐데...",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100038133.1.17",
          "form": "아하 8대1이면 선생님 봐주는거 시간짤ㅈ지않아요?",
          "original_form": "아하 8대1이면 선생님 봐주는거 시간짤ㅈ지않아요?",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100038133.1.18",
          "form": "그쳐ㅜ저도 돈만많으면...",
          "original_form": "그쳐ㅜ저도 돈만많으면...",
          "speaker_id": "2",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100038133.1.19",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100038133.1.20",
          "form": "근데 진짜 너무 아파서",
          "original_form": "근데 진짜 너무 아파서",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100038133.1.21",
          "form": "선생님이 절 안봐줬으면 하게 되더라구욬ㅋㅋㅋ",
          "original_form": "선생님이 절 안봐줬으면 하게 되더라구욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100038133.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아 뭔지알져",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아 뭔지알져",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100038133.1.23",
          "form": "이게 양가감정이 ㅋㅋㅋㅋㅋ",
          "original_form": "이게 양가감정이 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:41"
        },
        {
          "id": "MDRW2100038133.1.24",
          "form": "맞아요...진짜 너무 힘들었어요...",
          "original_form": "맞아요...진짜 너무 힘들었어요...",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100038133.1.25",
          "form": "돈 아까워서라도 또 가야하는데",
          "original_form": "돈 아까워서라도 또 가야하는데",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100038133.1.26",
          "form": "고생할거 아니까",
          "original_form": "고생할거 아니까",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100038133.1.27",
          "form": "발걸음이 안떨어지네요ㅠ",
          "original_form": "발걸음이 안떨어지네요ㅠ",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100038133.1.28",
          "form": "그쳐ㅜㅜㅜ 운동꾸준히가기 넘힘들어요",
          "original_form": "그쳐ㅜㅜㅜ 운동꾸준히가기 넘힘들어요",
          "speaker_id": "2",
          "time": "20210901 23:45"
        }
      ]
    }
