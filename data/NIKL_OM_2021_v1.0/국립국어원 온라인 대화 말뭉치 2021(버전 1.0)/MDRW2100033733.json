      "utterance": [
        {
          "id": "MDRW2100033733.1.1",
          "form": "친절하게좀 해주지",
          "original_form": "친절하게좀 해주지",
          "speaker_id": "2",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100033733.1.2",
          "form": "이거해봤어",
          "original_form": "이거해봤어",
          "speaker_id": "1",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100033733.1.3",
          "form": "아닐건데",
          "original_form": "아닐건데",
          "speaker_id": "1",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100033733.1.4",
          "form": "이거이거 잖아",
          "original_form": "이거이거 잖아",
          "speaker_id": "1",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100033733.1.5",
          "form": "아니 나만 그렇게 느끼는건가",
          "original_form": "아니 나만 그렇게 느끼는건가",
          "speaker_id": "1",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100033733.1.6",
          "form": "ㅋㅋ 군대식이네",
          "original_form": "ㅋㅋ 군대식이네",
          "speaker_id": "2",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100033733.1.7",
          "form": "아님 내가 나이가 많아서",
          "original_form": "아님 내가 나이가 많아서",
          "speaker_id": "1",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100033733.1.8",
          "form": "그녀의 반말이 더 거슬리는건가",
          "original_form": "그녀의 반말이 더 거슬리는건가",
          "speaker_id": "1",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100033733.1.9",
          "form": "다같은학생이지",
          "original_form": "다같은학생이지",
          "speaker_id": "2",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100033733.1.10",
          "form": "나이차이가 별로 안 나서인가",
          "original_form": "나이차이가 별로 안 나서인가",
          "speaker_id": "1",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100033733.1.11",
          "form": "30대 후반이신것같더라고요",
          "original_form": "30대 후반이신것같더라고요",
          "speaker_id": "1",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100033733.1.12",
          "form": "아 여선생님이시구나",
          "original_form": "아 여선생님이시구나",
          "speaker_id": "2",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100033733.1.13",
          "form": "미혼의",
          "original_form": "미혼의",
          "speaker_id": "1",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100033733.1.14",
          "form": "나쁜의미가 아니라",
          "original_form": "나쁜의미가 아니라",
          "speaker_id": "1",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100033733.1.15",
          "form": "오 그렇군",
          "original_form": "오 그렇군",
          "speaker_id": "2",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100033733.1.16",
          "form": "좀 결혼하신분과",
          "original_form": "좀 결혼하신분과",
          "speaker_id": "1",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100033733.1.17",
          "form": "학생몇명?",
          "original_form": "학생몇명?",
          "speaker_id": "2",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100033733.1.18",
          "form": "안하사분은 느낌이 다름",
          "original_form": "안하사분은 느낌이 다름",
          "speaker_id": "1",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100033733.1.19",
          "form": "8명인데",
          "original_form": "8명인데",
          "speaker_id": "1",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100033733.1.20",
          "form": "소수정예네",
          "original_form": "소수정예네",
          "speaker_id": "2",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100033733.1.21",
          "form": "다 나빼고 90년대생 20대인듯염",
          "original_form": "다 나빼고 90년대생 20대인듯염",
          "speaker_id": "1",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100033733.1.22",
          "form": "늙어서 슬픔",
          "original_form": "늙어서 슬픔",
          "speaker_id": "1",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100033733.1.23",
          "form": "그럼  name1님좀 봐주지ㅜㅜㅜ",
          "original_form": "그럼  &name1&님좀 봐주지ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100033733.1.24",
          "form": "어머님들 수업도 하시고",
          "original_form": "어머님들 수업도 하시고",
          "speaker_id": "1",
          "time": "20210824 23:56"
        },
        {
          "id": "MDRW2100033733.1.25",
          "form": "54년생도 수업하셨다는데",
          "original_form": "54년생도 수업하셨다는데",
          "speaker_id": "1",
          "time": "20210824 23:56"
        },
        {
          "id": "MDRW2100033733.1.26",
          "form": "그분들한테도 그리말하시나 싶음",
          "original_form": "그분들한테도 그리말하시나 싶음",
          "speaker_id": "1",
          "time": "20210824 23:56"
        },
        {
          "id": "MDRW2100033733.1.27",
          "form": "오 근데왜 그럼 강의평가가 두렵지 않나봐",
          "original_form": "오 근데왜 그럼 강의평가가 두렵지 않나봐",
          "speaker_id": "2",
          "time": "20210824 23:56"
        },
        {
          "id": "MDRW2100033733.1.28",
          "form": "복습예습을 잘해가면 어때",
          "original_form": "복습예습을 잘해가면 어때",
          "speaker_id": "2",
          "time": "20210824 23:57"
        },
        {
          "id": "MDRW2100033733.1.29",
          "form": "근데 진짜 일부러는 아닌거 같음 그냥 자기보다 나이 적으니 편하게 말하고 반말하는게 심기에 거슬릴뿐",
          "original_form": "근데 진짜 일부러는 아닌거 같음 그냥 자기보다 나이 적으니 편하게 말하고 반말하는게 심기에 거슬릴뿐",
          "speaker_id": "1",
