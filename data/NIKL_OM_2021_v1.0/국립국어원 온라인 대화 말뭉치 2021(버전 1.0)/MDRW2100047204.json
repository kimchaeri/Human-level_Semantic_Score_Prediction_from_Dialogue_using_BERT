      "utterance": [
        {
          "id": "MDRW2100047204.1.1",
          "form": "우딜?ㅋㅋㅋ",
          "original_form": "우딜?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100047204.1.2",
          "form": "그 편키트요",
          "original_form": "그 편키트요",
          "speaker_id": "2",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100047204.1.3",
          "form": "저 그거못함요",
          "original_form": "저 그거못함요",
          "speaker_id": "1",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100047204.1.4",
          "form": "우딜하셨어요?",
          "original_form": "우딜하셨어요?",
          "speaker_id": "1",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100047204.1.5",
          "form": "네~  1번만ㅠㅠ",
          "original_form": "네~  1번만ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100047204.1.6",
          "form": "전 피곤해서 집에와서 뒹굴다 깁콘하날 날렸어요ㅠ",
          "original_form": "전 피곤해서 집에와서 뒹굴다 깁콘하날 날렸어요ㅠ",
          "speaker_id": "1",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100047204.1.7",
          "form": "동네 없어요ㅠㅠ",
          "original_form": "동네 없어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100047204.1.8",
          "form": "이런!!! 아깝네요ㅠ",
          "original_form": "이런!!! 아깝네요ㅠ",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100047204.1.9",
          "form": "폴더 날짜를 잘못써놔서ㅠㅠ",
          "original_form": "폴더 날짜를 잘못써놔서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100047204.1.10",
          "form": "그럴때 제일 아까워요",
          "original_form": "그럴때 제일 아까워요",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100047204.1.11",
          "form": "이디야 토피넛이였는데",
          "original_form": "이디야 토피넛이였는데",
          "speaker_id": "1",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100047204.1.12",
          "form": "아메도아니고 ㅠㅠ",
          "original_form": "아메도아니고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100047204.1.13",
          "form": "비싼그",
          "original_form": "비싼그",
          "speaker_id": "2",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100047204.1.14",
          "form": "이벤트로 받은건 환불안되겠죠?",
          "original_form": "이벤트로 받은건 환불안되겠죠?",
          "speaker_id": "1",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100047204.1.15",
          "form": "근데 그거 기프티콘에",
          "original_form": "근데 그거 기프티콘에",
          "speaker_id": "2",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100047204.1.16",
          "form": "뜨는거에요?",
          "original_form": "뜨는거에요?",
          "speaker_id": "2",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100047204.1.17",
          "form": "아뇨 아이넘버꺼라",
          "original_form": "아뇨 아이넘버꺼라",
          "speaker_id": "1",
          "time": "20210825 23:47"
        },
        {
          "id": "MDRW2100047204.1.18",
          "form": "이벤트로받은건 조회도안되더라구요",
          "original_form": "이벤트로받은건 조회도안되더라구요",
          "speaker_id": "1",
          "time": "20210825 23:48"
        },
        {
          "id": "MDRW2100047204.1.19",
          "form": "그럼 안되요ㅠㅠ흑",
          "original_form": "그럼 안되요ㅠㅠ흑",
          "speaker_id": "2",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100047204.1.20",
          "form": "ㅠㅠ 아까비 비싼건데",
          "original_form": "ㅠㅠ 아까비 비싼건데",
          "speaker_id": "1",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100047204.1.21",
          "form": "기프티콘어플 뜨는건 되던데ㅜㅠ",
          "original_form": "기프티콘어플 뜨는건 되던데ㅜㅠ",
          "speaker_id": "2",
          "time": "20210825 23:49"
        },
        {
          "id": "MDRW2100047204.1.22",
          "form": "당첨되기도 쉽지않은걸 날리다니",
          "original_form": "당첨되기도 쉽지않은걸 날리다니",
          "speaker_id": "1",
          "time": "20210825 23:50"
        }
      ]
