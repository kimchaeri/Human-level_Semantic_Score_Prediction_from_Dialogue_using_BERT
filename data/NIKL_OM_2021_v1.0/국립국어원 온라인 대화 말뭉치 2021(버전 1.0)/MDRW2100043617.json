        {
          "id": "MDRW2100043617.1.1",
          "form": "성인되면 치맥하자",
          "original_form": "성인되면 치맥하자",
          "speaker_id": "2",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043617.1.2",
          "form": "좋다!!!",
          "original_form": "좋다!!!",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043617.1.3",
          "form": "나도 아직 안해봣어",
          "original_form": "나도 아직 안해봣어",
          "speaker_id": "2",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043617.1.4",
          "form": "언닌 치콜이니깐",
          "original_form": "언닌 치콜이니깐",
          "speaker_id": "1",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043617.1.5",
          "form": "그렇지!!!",
          "original_form": "그렇지!!!",
          "speaker_id": "2",
          "time": "20210907 19:44"
        },
        {
          "id": "MDRW2100043617.1.6",
          "form": "콜라가 아직 좋아??",
          "original_form": "콜라가 아직 좋아??",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043617.1.7",
          "form": "맥주는 별로야ㅠㅜ",
          "original_form": "맥주는 별로야ㅠㅜ",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043617.1.8",
          "form": "맥주 맛없어?",
          "original_form": "맥주 맛없어?",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043617.1.9",
          "form": "응...좀 그래",
          "original_form": "응...좀 그래",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043617.1.10",
          "form": "무슨맛이길래..",
          "original_form": "무슨맛이길래..",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043617.1.11",
          "form": "걍 너무 쓴맛이야",
          "original_form": "걍 너무 쓴맛이야",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100043617.1.12",
          "form": "술이 다그렇자나",
          "original_form": "술이 다그렇자나",
