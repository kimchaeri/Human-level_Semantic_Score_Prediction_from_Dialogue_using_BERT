      "utterance": [
        {
          "id": "MDRW2100038348.1.1",
          "form": "아하하 1.5키로 줄었습니다",
          "original_form": "아하하 1.5키로 줄었습니다",
          "speaker_id": "1",
          "time": "20210902 10:39"
        },
        {
          "id": "MDRW2100038348.1.2",
          "form": "여섯시에 재니까 몸무게를",
          "original_form": "여섯시에 재니까 몸무게를",
          "speaker_id": "1",
          "time": "20210902 10:39"
        },
        {
          "id": "MDRW2100038348.1.3",
          "form": "오오오오.!!.!!!!.",
          "original_form": "오오오오.!!.!!!!.",
          "speaker_id": "2",
          "time": "20210902 10:39"
        },
        {
          "id": "MDRW2100038348.1.4",
          "form": "다시 화장실갔다가",
          "original_form": "다시 화장실갔다가",
          "speaker_id": "1",
          "time": "20210902 10:39"
        },
        {
          "id": "MDRW2100038348.1.5",
          "form": "일곱시에또재고",
          "original_form": "일곱시에또재고",
          "speaker_id": "1",
          "time": "20210902 10:39"
        },
        {
          "id": "MDRW2100038348.1.6",
          "form": "자몽 자식 해냈군요!!!",
          "original_form": "자몽 자식 해냈군요!!!",
          "speaker_id": "2",
          "time": "20210902 10:39"
        },
        {
          "id": "MDRW2100038348.1.7",
          "form": "또 화장실가서 어떻게든 수분을 내보내고 또재니까",
          "original_form": "또 화장실가서 어떻게든 수분을 내보내고 또재니까",
          "speaker_id": "1",
          "time": "20210902 10:39"
        },
        {
          "id": "MDRW2100038348.1.8",
          "form": "최대 1.9까지 나왔는데",
          "original_form": "최대 1.9까지 나왔는데",
          "speaker_id": "1",
          "time": "20210902 10:40"
        },
        {
          "id": "MDRW2100038348.1.9",
          "form": "앗ㅎㅎ 그건 너무 허수이니 빼고ㅎ",
          "original_form": "앗ㅎㅎ 그건 너무 허수이니 빼고ㅎ",
          "speaker_id": "2",
          "time": "20210902 10:40"
        },
        {
          "id": "MDRW2100038348.1.10",
          "form": "자몽자식 ㅋㅋ 왜이렇게 웃기죠",
          "original_form": "자몽자식 ㅋㅋ 왜이렇게 웃기죠",
          "speaker_id": "1",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100038348.1.11",
          "form": "근데 사실 생각해봄 효과가 아주 드라마틱한 아닌데",
          "original_form": "근데 사실 생각해봄 효과가 아주 드라마틱한 아닌데",
          "speaker_id": "2",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100038348.1.12",
          "form": "근데 이거 솔직히 수분일거같달까 ㅋㅋㄲ",
          "original_form": "근데 이거 솔직히 수분일거같달까 ㅋㅋㄲ",
          "speaker_id": "1",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100038348.1.13",
          "form": "어제 충격적인 숫자를 들어서인지 대단해 보여요ㅎㅎ",
          "original_form": "어제 충격적인 숫자를 들어서인지 대단해 보여요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100038348.1.14",
          "form": "여덟시에 물마시면서 든생각 ㅋㅋ",
          "original_form": "여덟시에 물마시면서 든생각 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100038348.1.15",
          "form": "일단 보양식부터 드시고ㅎ  그후 소송할지말지 보시죠ㅎ",
          "original_form": "일단 보양식부터 드시고ㅎ  그후 소송할지말지 보시죠ㅎ",
          "speaker_id": "2",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100038348.1.16",
          "form": "겉으로 보기엔 아~~무런변화없어요 ㅋㅋ",
          "original_form": "겉으로 보기엔 아~~무런변화없어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:42"
        },
        {
          "id": "MDRW2100038348.1.17",
          "form": "전그냥",
          "original_form": "전그냥",
          "speaker_id": "1",
          "time": "20210902 10:42"
        },
        {
          "id": "MDRW2100038348.1.18",
          "form": "거기에 만족하려고요ㅜ",
          "original_form": "거기에 만족하려고요ㅜ",
          "speaker_id": "1",
          "time": "20210902 10:42"
        },
        {
          "id": "MDRW2100038348.1.19",
          "form": "ㅎ  이제 다이어트 초기니까요!",
          "original_form": "ㅎ  이제 다이어트 초기니까요!",
          "speaker_id": "2",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100038348.1.20",
          "form": "제가 그동안 진짜 대식가였는데 이걸해냈다는거? ㅋㅋㅋㅋ",
