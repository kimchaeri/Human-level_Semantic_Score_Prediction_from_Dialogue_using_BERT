      "utterance": [
        {
          "id": "MDRW2100025432.1.1",
          "form": "이따가 코엑스에서 6시까지 보는거맞져ㅋㅋㅋㅋㅋㅋ",
          "original_form": "이따가 코엑스에서 6시까지 보는거맞져ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:14"
        },
        {
          "id": "MDRW2100025432.1.2",
          "form": "딤섬",
          "original_form": "딤섬",
          "speaker_id": "1",
          "time": "20210901 11:14"
        },
        {
          "id": "MDRW2100025432.1.3",
          "form": "ㅇㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210901 11:14"
        },
        {
          "id": "MDRW2100025432.1.4",
          "form": "딤섬먹어야디",
          "original_form": "딤섬먹어야디",
          "speaker_id": "2",
          "time": "20210901 11:14"
        },
        {
          "id": "MDRW2100025432.1.5",
          "form": "딤삼딤섬먹어야징",
          "original_form": "딤삼딤섬먹어야징",
          "speaker_id": "1",
          "time": "20210901 11:14"
        },
        {
          "id": "MDRW2100025432.1.6",
          "form": "아 맞아 제가 또",
          "original_form": "아 맞아 제가 또",
          "speaker_id": "2",
          "time": "20210901 11:14"
        },
        {
          "id": "MDRW2100025432.1.7",
          "form": "거기 솥밥도 맛있다고했아",
          "original_form": "거기 솥밥도 맛있다고했아",
          "speaker_id": "1",
          "time": "20210901 11:15"
        },
        {
          "id": "MDRW2100025432.1.8",
          "form": "엄청난고깃집을알아냈어",
          "original_form": "엄청난고깃집을알아냈어",
          "speaker_id": "2",
          "time": "20210901 11:15"
        },
        {
          "id": "MDRW2100025432.1.9",
          "form": "신당쪽 금돼지식당",
          "original_form": "신당쪽 금돼지식당",
          "speaker_id": "2",
          "time": "20210901 11:15"
        },
        {
          "id": "MDRW2100025432.1.10",
          "form": "그 고깃집이 어디져",
          "original_form": "그 고깃집이 어디져",
          "speaker_id": "1",
          "time": "20210901 11:15"
        },
        {
          "id": "MDRW2100025432.1.11",
          "form": "찾아보고올겤ㅋ",
          "original_form": "찾아보고올겤ㅋ",
          "speaker_id": "1",
          "time": "20210901 11:15"
        },
        {
          "id": "MDRW2100025432.1.12",
          "form": "서울 3대 돼지고기집이래",
          "original_form": "서울 3대 돼지고기집이래",
          "speaker_id": "2",
          "time": "20210901 11:15"
        },
        {
          "id": "MDRW2100025432.1.13",
          "form": "서울삼대 돼지고기집이 숙대 남영돈",
          "original_form": "서울삼대 돼지고기집이 숙대 남영돈",
          "speaker_id": "2",
          "time": "20210901 11:15"
        },
        {
          "id": "MDRW2100025432.1.14",
          "form": "헐 김치찌개뭐야",
          "original_form": "헐 김치찌개뭐야",
          "speaker_id": "1",
          "time": "20210901 11:16"
        },
        {
          "id": "MDRW2100025432.1.15",
          "form": "신당 금돼지식당",
          "original_form": "신당 금돼지식당",
          "speaker_id": "2",
          "time": "20210901 11:16"
        },
        {
          "id": "MDRW2100025432.1.16",
          "form": "몽탄",
          "original_form": "몽탄",
          "speaker_id": "2",
          "time": "20210901 11:16"
        },
        {
          "id": "MDRW2100025432.1.17",
          "form": "용산쪽",
          "original_form": "용산쪽",
          "speaker_id": "2",
          "time": "20210901 11:16"
        },
        {
          "id": "MDRW2100025432.1.18",
          "form": "몽탄 여기 가보고싶었던 곳이었어",
          "original_form": "몽탄 여기 가보고싶었던 곳이었어",
          "speaker_id": "1",
          "time": "20210901 11:17"
        },
        {
          "id": "MDRW2100025432.1.19",
          "form": "나중에 가봐야지",
          "original_form": "나중에 가봐야지",
          "speaker_id": "1",
          "time": "20210901 11:17"
        },
        {
          "id": "MDRW2100025432.1.20",
          "form": "아 나도 ㄹㅇ가보고싶어",
          "original_form": "아 나도 ㄹㅇ가보고싶어",
          "speaker_id": "2",
          "time": "20210901 11:17"
        },
        {
          "id": "MDRW2100025432.1.21",
          "form": "존맛일거같아",
          "original_form": "존맛일거같아",
          "speaker_id": "2",
          "time": "20210901 11:17"
        },
        {
          "id": "MDRW2100025432.1.22",
          "form": "남영돈도존맛이더라",
          "original_form": "남영돈도존맛이더라",
          "speaker_id": "2",
          "time": "20210901 11:17"
        },
        {
          "id": "MDRW2100025432.1.23",
          "form": "엄청대기해야하는군",
          "original_form": "엄청대기해야하는군",
          "speaker_id": "1",
          "time": "20210901 11:17"
        },
        {
          "id": "MDRW2100025432.1.24",
          "form": "완전미친맛",
          "original_form": "완전미친맛",
          "speaker_id": "2",
          "time": "20210901 11:17"
        },
        {
          "id": "MDRW2100025432.1.25",
          "form": "먹고싶다",
          "original_form": "먹고싶다",
          "speaker_id": "1",
          "time": "20210901 11:18"
        },
        {
          "id": "MDRW2100025432.1.26",
          "form": "목살",
          "original_form": "목살",
          "speaker_id": "1",
          "time": "20210901 11:18"
        },
        {
          "id": "MDRW2100025432.1.27",
          "form": "근데올려놓고카페가면돼",
          "original_form": "근데올려놓고카페가면돼",
          "speaker_id": "2",
          "time": "20210901 11:18"
        },
        {
          "id": "MDRW2100025432.1.28",
          "form": "ㅇㅇㅇㅇ 남영돈은 가브리살이 존엄",
          "original_form": "ㅇㅇㅇㅇ 남영돈은 가브리살이 존엄",
