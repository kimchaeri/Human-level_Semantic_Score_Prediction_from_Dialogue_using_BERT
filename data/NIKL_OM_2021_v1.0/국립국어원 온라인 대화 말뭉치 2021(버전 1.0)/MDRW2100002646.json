      "utterance": [
        {
          "id": "MDRW2100002646.1.1",
          "form": "name1님은 반려동물 키우세요?",
          "original_form": "&name1&님은 반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210820 14:19"
        },
        {
          "id": "MDRW2100002646.1.2",
          "form": "네 강아지 두마리 키워요",
          "original_form": "네 강아지 두마리 키워요",
          "speaker_id": "1",
          "time": "20210820 14:32"
        },
        {
          "id": "MDRW2100002646.1.3",
          "form": "포메라니안인데 털이 넘 많이 빠지네요 ㅎㅎ",
          "original_form": "포메라니안인데 털이 넘 많이 빠지네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 14:32"
        },
        {
          "id": "MDRW2100002646.1.4",
          "form": "와 두마리~ 넘 귀여울것같아요",
          "original_form": "와 두마리~ 넘 귀여울것같아요",
          "speaker_id": "2",
          "time": "20210820 14:32"
        },
        {
          "id": "MDRW2100002646.1.5",
          "form": "네 ㅎㅎ 앙칼지긴한데 넘 귀여워요 ㅠㅋㅋ",
          "original_form": "네 ㅎㅎ 앙칼지긴한데 넘 귀여워요 ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:33"
        },
        {
          "id": "MDRW2100002646.1.6",
          "form": "강아지 좋아하세요?",
          "original_form": "강아지 좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 14:34"
        },
        {
          "id": "MDRW2100002646.1.7",
          "form": "네 강아지 좋아해요ㅎㅎ",
          "original_form": "네 강아지 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 14:34"
        },
        {
          "id": "MDRW2100002646.1.8",
          "form": "제가 키우진 못해도",
          "original_form": "제가 키우진 못해도",
          "speaker_id": "2",
          "time": "20210820 14:35"
        },
        {
          "id": "MDRW2100002646.1.9",
          "form": "산책할때 마주치면 넘 귀엽고 좋더라구요",
          "original_form": "산책할때 마주치면 넘 귀엽고 좋더라구요",
          "speaker_id": "2",
          "time": "20210820 14:35"
        },
        {
          "id": "MDRW2100002646.1.10",
          "form": "ㅎㅎ산책할때 강아지 마주치면 넘 기분좋은것같아요",
          "original_form": "ㅎㅎ산책할때 강아지 마주치면 넘 기분좋은것같아요",
          "speaker_id": "1",
          "time": "20210820 14:36"
        },
        {
          "id": "MDRW2100002646.1.11",
          "form": "저희집 강아지 한마리는 산책을 좋아하는데 한마리는 싫어해서 문제에요 ㅠㅋ",
          "original_form": "저희집 강아지 한마리는 산책을 좋아하는데 한마리는 싫어해서 문제에요 ㅠㅋ",
          "speaker_id": "1",
          "time": "20210820 14:36"
        },
        {
          "id": "MDRW2100002646.1.12",
          "form": "헉 왜 산책을 싫어할까요",
          "original_form": "헉 왜 산책을 싫어할까요",
          "speaker_id": "2",
          "time": "20210820 14:38"
        },
        {
          "id": "MDRW2100002646.1.13",
          "form": "예전에 세나개보면 산책 싫어하는 강아지들 간식으로 보상하면서 산책하더라구요",
          "original_form": "예전에 세나개보면 산책 싫어하는 강아지들 간식으로 보상하면서 산책하더라구요",
          "speaker_id": "2",
          "time": "20210820 14:39"
        },
        {
          "id": "MDRW2100002646.1.14",
          "form": "물론 저보다 주인분이 잘 아시겠지만요",
          "original_form": "물론 저보다 주인분이 잘 아시겠지만요",
          "speaker_id": "2",
          "time": "20210820 14:40"
        },
        {
          "id": "MDRW2100002646.1.15",
          "form": "오 산책가서 걷기 싫어할때마다 간식으로 유인하긴하는데",
          "original_form": "오 산책가서 걷기 싫어할때마다 간식으로 유인하긴하는데",
          "speaker_id": "1",
          "time": "20210820 14:41"
        },
        {
          "id": "MDRW2100002646.1.16",
          "form": "없으면 얄짤없네요 ㅠㅎㅋㅋ",
          "original_form": "없으면 얄짤없네요 ㅠㅎㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:41"
        },
        {
          "id": "MDRW2100002646.1.17",
          "form": "넘 똑똑한 강아지들..",
          "original_form": "넘 똑똑한 강아지들..",
          "speaker_id": "1",
          "time": "20210820 14:41"
        },
        {
          "id": "MDRW2100002646.1.18",
          "form": "아ㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:42"
        },
        {
          "id": "MDRW2100002646.1.19",
          "form": "진짜 너무 똑똑하네여ㅋㅋㅋ",
          "original_form": "진짜 너무 똑똑하네여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:42"
        },
        {
          "id": "MDRW2100002646.1.20",
          "form": "한번 걸을때마다 줄수도 없고 난감하시겠어요",
          "original_form": "한번 걸을때마다 줄수도 없고 난감하시겠어요",
          "speaker_id": "2",
          "time": "20210820 14:42"
        },
        {
          "id": "MDRW2100002646.1.21",
          "form": "네 ㅠㅋㅋ 가족 다같이 가면 좋아하는데 누가 집에 있거나하면 넘넘 가기싫어해요 ㅋ",
          "original_form": "네 ㅠㅋㅋ 가족 다같이 가면 좋아하는데 누가 집에 있거나하면 넘넘 가기싫어해요 ㅋ",
          "speaker_id": "1",
          "time": "20210820 14:44"
        },
        {
          "id": "MDRW2100002646.1.22",
          "form": "산책이 싫은것보다 가족들이랑 집에있고싶은건가봐요ㅋㅋ",
          "original_form": "산책이 싫은것보다 가족들이랑 집에있고싶은건가봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:48"
        },
        {
          "id": "MDRW2100002646.1.23",
          "form": "그런가봐요~ 먹는거 제일 좋아하기도하고 ㅎ",
          "original_form": "그런가봐요~ 먹는거 제일 좋아하기도하고 ㅎ",
          "speaker_id": "1",
          "time": "20210820 14:51"
        }
      ]
    }
  ]
