      "utterance": [
        {
          "id": "MDRW2100020512.1.1",
          "form": "피클은 드시나요",
          "original_form": "피클은 드시나요",
          "speaker_id": "1",
          "time": "20210828 08:43"
        },
        {
          "id": "MDRW2100020512.1.2",
          "form": "어휴 피클먹어야 많이먹죠",
          "original_form": "어휴 피클먹어야 많이먹죠",
          "speaker_id": "2",
          "time": "20210828 08:43"
        },
        {
          "id": "MDRW2100020512.1.3",
          "form": "맘졸였다 오이 못드실까봐",
          "original_form": "맘졸였다 오이 못드실까봐",
          "speaker_id": "1",
          "time": "20210828 08:43"
        },
        {
          "id": "MDRW2100020512.1.4",
          "form": "저도 금방 마음이 싸했어요",
          "original_form": "저도 금방 마음이 싸했어요",
          "speaker_id": "2",
          "time": "20210828 08:43"
        },
        {
          "id": "MDRW2100020512.1.5",
          "form": "질문을 잘못한줄알고 순간 땀이 삐질",
          "original_form": "질문을 잘못한줄알고 순간 땀이 삐질",
          "speaker_id": "1",
          "time": "20210828 08:43"
        },
        {
          "id": "MDRW2100020512.1.6",
          "form": "못드시는 걸까봐",
          "original_form": "못드시는 걸까봐",
          "speaker_id": "2",
          "time": "20210828 08:44"
        },
        {
          "id": "MDRW2100020512.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 오이 좋아합니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 오이 좋아합니다",
          "speaker_id": "1",
          "time": "20210828 08:44"
        },
        {
          "id": "MDRW2100020512.1.8",
          "form": "오이 청량하죠",
          "original_form": "오이 청량하죠",
          "speaker_id": "2",
          "time": "20210828 08:44"
        },
        {
          "id": "MDRW2100020512.1.9",
          "form": "생오이도 잘먹습니다",
          "original_form": "생오이도 잘먹습니다",
          "speaker_id": "2",
          "time": "20210828 08:44"
        },
        {
          "id": "MDRW2100020512.1.10",
          "form": "올해초에 오이 나올 때 많이 사먹었거든요",
          "original_form": "올해초에 오이 나올 때 많이 사먹었거든요",
          "speaker_id": "1",
          "time": "20210828 08:44"
        },
        {
          "id": "MDRW2100020512.1.11",
          "form": "그걸보고 저희 아부지가 오이를 심으셨는데",
          "original_form": "그걸보고 저희 아부지가 오이를 심으셨는데",
          "speaker_id": "1",
          "time": "20210828 08:44"
        },
        {
          "id": "MDRW2100020512.1.12",
          "form": "저희는 밭에서 따왔어요",
          "original_form": "저희는 밭에서 따왔어요",
          "speaker_id": "2",
          "time": "20210828 08:44"
        },
        {
          "id": "MDRW2100020512.1.13",
          "form": "못생긴 오이던가요?",
          "original_form": "못생긴 오이던가요?",
          "speaker_id": "2",
          "time": "20210828 08:44"
        },
        {
          "id": "MDRW2100020512.1.14",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210828 08:44"
        },
        {
          "id": "MDRW2100020512.1.15",
          "form": "너무 커져서 밍숭맹숭",
          "original_form": "너무 커져서 밍숭맹숭",
          "speaker_id": "1",
          "time": "20210828 08:45"
        },
        {
          "id": "MDRW2100020512.1.16",
          "form": "와 성공하셨군요",
          "original_form": "와 성공하셨군요",
