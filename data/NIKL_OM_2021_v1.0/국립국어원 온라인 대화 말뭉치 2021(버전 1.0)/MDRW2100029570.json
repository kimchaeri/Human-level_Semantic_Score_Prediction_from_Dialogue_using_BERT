      "utterance": [
        {
          "id": "MDRW2100029570.1.1",
          "form": "저두요..  덕질 투어 가보고 싶어요",
          "original_form": "저두요..  덕질 투어 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210903 10:29"
        },
        {
          "id": "MDRW2100029570.1.2",
          "form": "ㅋㅋ무슨덕이신가요",
          "original_form": "ㅋㅋ무슨덕이신가요",
          "speaker_id": "1",
          "time": "20210903 10:34"
        },
        {
          "id": "MDRW2100029570.1.3",
          "form": "방탄 좋아합니다ㅎㅎ",
          "original_form": "방탄 좋아합니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 10:37"
        },
        {
          "id": "MDRW2100029570.1.4",
          "form": "아미셨군요",
          "original_form": "아미셨군요",
          "speaker_id": "1",
          "time": "20210903 10:40"
        },
        {
          "id": "MDRW2100029570.1.5",
          "form": "네ㅎㅎ",
          "original_form": "네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 10:43"
        },
        {
          "id": "MDRW2100029570.1.6",
          "form": "그래서 코로나 끝나고 동해가려구요",
          "original_form": "그래서 코로나 끝나고 동해가려구요",
          "speaker_id": "2",
          "time": "20210903 10:43"
        },
        {
          "id": "MDRW2100029570.1.7",
          "form": "음 동해?가 왜",
          "original_form": "음 동해?가 왜",
          "speaker_id": "1",
          "time": "20210903 11:38"
        },
        {
          "id": "MDRW2100029570.1.8",
          "form": "동해쪽에 앨범 촬영지가 많아요",
          "original_form": "동해쪽에 앨범 촬영지가 많아요",
          "speaker_id": "2",
          "time": "20210903 12:00"
        },
        {
          "id": "MDRW2100029570.1.9",
          "form": "아 사진 찍으시러",
          "original_form": "아 사진 찍으시러",
          "speaker_id": "1",
          "time": "20210903 12:43"
        },
        {
          "id": "MDRW2100029570.1.10",
          "form": "한번 가봤는데 그때 인생사진 건지기도 했어요ㅎㅎ",
          "original_form": "한번 가봤는데 그때 인생사진 건지기도 했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 12:48"
        },
        {
          "id": "MDRW2100029570.1.11",
          "form": "ㅋㅋ그럼 뭐 지금 가셔도?",
          "original_form": "ㅋㅋ그럼 뭐 지금 가셔도?",
          "speaker_id": "1",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100029570.1.12",
          "form": "오히려 한산할지도요",
          "original_form": "오히려 한산할지도요",
          "speaker_id": "1",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100029570.1.13",
          "form": "시간날때 빨리 갔다와야겠네요ㅎㅎ",
          "original_form": "시간날때 빨리 갔다와야겠네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 15:00"
        },
        {
          "id": "MDRW2100029570.1.14",
          "form": "네 시기 잘 맞추시면 텅텅!!",
          "original_form": "네 시기 잘 맞추시면 텅텅!!",
          "speaker_id": "1",
          "time": "20210903 16:04"
        },
        {
          "id": "MDRW2100029570.1.15",
          "form": "당장 주말에 갈까 고민하고 있어요",
