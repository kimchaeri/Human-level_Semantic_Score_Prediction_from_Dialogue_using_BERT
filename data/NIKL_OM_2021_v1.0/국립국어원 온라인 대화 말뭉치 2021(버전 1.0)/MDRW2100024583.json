      "utterance": [
        {
          "id": "MDRW2100024583.1.1",
          "form": "혹시 그 박신혜랑 전종서 나온",
          "original_form": "혹시 그 박신혜랑 전종서 나온",
          "speaker_id": "1",
          "time": "20210831 14:47"
        },
        {
          "id": "MDRW2100024583.1.2",
          "form": "오리지널 영화도 보셨나요?",
          "original_form": "오리지널 영화도 보셨나요?",
          "speaker_id": "1",
          "time": "20210831 14:47"
        },
        {
          "id": "MDRW2100024583.1.3",
          "form": "폰인가? 콜인가?",
          "original_form": "폰인가? 콜인가?",
          "speaker_id": "1",
          "time": "20210831 14:47"
        },
        {
          "id": "MDRW2100024583.1.4",
          "form": "아 그건 무서울까봐 못봤어요",
          "original_form": "아 그건 무서울까봐 못봤어요",
          "speaker_id": "2",
          "time": "20210831 14:48"
        },
        {
          "id": "MDRW2100024583.1.5",
          "form": "무서운영화 좋아하세요?",
          "original_form": "무서운영화 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 14:48"
        },
        {
          "id": "MDRW2100024583.1.6",
          "form": "네! 전 피만 안 나오면",
          "original_form": "네! 전 피만 안 나오면",
          "speaker_id": "1",
          "time": "20210831 14:48"
        },
        {
          "id": "MDRW2100024583.1.7",
          "form": "다 잘 보는 편이에요!",
          "original_form": "다 잘 보는 편이에요!",
          "speaker_id": "1",
          "time": "20210831 14:48"
        },
        {
          "id": "MDRW2100024583.1.8",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210831 14:48"
        },
        {
          "id": "MDRW2100024583.1.9",
          "form": "아 전 아예 못봐서 ㅠㅠ",
          "original_form": "아 전 아예 못봐서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 14:48"
        },
        {
          "id": "MDRW2100024583.1.10",
          "form": "좋아하시는 배우는 혹시 있으신가요??",
          "original_form": "좋아하시는 배우는 혹시 있으신가요??",
          "speaker_id": "2",
          "time": "20210831 14:49"
        },
        {
          "id": "MDRW2100024583.1.11",
          "form": "저 김태리 엄청 좋아해요!",
          "original_form": "저 김태리 엄청 좋아해요!",
          "speaker_id": "1",
          "time": "20210831 14:52"
        },
        {
          "id": "MDRW2100024583.1.12",
          "form": "name2님은요?!",
          "original_form": "&name2&님은요?!",
          "speaker_id": "1",
          "time": "20210831 14:52"
        },
        {
          "id": "MDRW2100024583.1.13",
          "form": "저는 송중기요!",
          "original_form": "저는 송중기요!",
          "speaker_id": "2",
          "time": "20210831 14:53"
        },
        {
          "id": "MDRW2100024583.1.14",
          "form": "승리호 보셨나요???",
          "original_form": "승리호 보셨나요???",
          "speaker_id": "2",
          "time": "20210831 14:53"
        },
        {
          "id": "MDRW2100024583.1.15",
          "form": "헛 저 그거 반쯤 보다가 잤는데",
          "original_form": "헛 저 그거 반쯤 보다가 잤는데",
          "speaker_id": "1",
          "time": "20210831 14:53"
        },
        {
          "id": "MDRW2100024583.1.16",
          "form": "비빠서 다시 못 보고 있어요",
          "original_form": "비빠서 다시 못 보고 있어요",
          "speaker_id": "1",
          "time": "20210831 14:53"
        },
        {
          "id": "MDRW2100024583.1.17",
          "form": "헛 송중기면 빈센조!!!",
          "original_form": "헛 송중기면 빈센조!!!",
          "speaker_id": "1",
          "time": "20210831 14:53"
        },
        {
          "id": "MDRW2100024583.1.18",
          "form": "보셨너요!?",
          "original_form": "보셨너요!?",
          "speaker_id": "1",
          "time": "20210831 14:53"
        },
        {
          "id": "MDRW2100024583.1.19",
          "form": "아 네네",
          "original_form": "아 네네",
          "speaker_id": "2",
          "time": "20210831 14:54"
        },
        {
          "id": "MDRW2100024583.1.20",
          "form": "승리호에 김태리 넘 좋아서",
          "original_form": "승리호에 김태리 넘 좋아서",
          "speaker_id": "2",
          "time": "20210831 14:54"
        },
        {
          "id": "MDRW2100024583.1.21",
          "form": "미스터선샤인도 요즘 조금씩 보고있어요",
          "original_form": "미스터선샤인도 요즘 조금씩 보고있어요",
          "speaker_id": "2",
          "time": "20210831 14:54"
        },
        {
          "id": "MDRW2100024583.1.22",
          "form": "혹시 그거 보셨나요?",
          "original_form": "혹시 그거 보셨나요?",
          "speaker_id": "2",
          "time": "20210831 14:54"
        },
        {
          "id": "MDRW2100024583.1.23",
          "form": "헛 네네!!!! 저도 어제 시작했어요!!!",
          "original_form": "헛 네네!!!! 저도 어제 시작했어요!!!",
          "speaker_id": "1",
          "time": "20210831 14:55"
        },
        {
          "id": "MDRW2100024583.1.24",
          "form": "결말이 좀 슬프다고 들어서",
          "original_form": "결말이 좀 슬프다고 들어서",
          "speaker_id": "1",
          "time": "20210831 14:56"
        },
        {
          "id": "MDRW2100024583.1.25",
          "form": "용기 못내고 있었거든요 ㅠㅠ",
          "original_form": "용기 못내고 있었거든요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 14:56"
        },
        {
          "id": "MDRW2100024583.1.26",
          "form": "아 저도 슬플까봐 걱정되네요 ㅠㅠ",
          "original_form": "아 저도 슬플까봐 걱정되네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 14:57"
        },
        {
          "id": "MDRW2100024583.1.27",
          "form": "김태리 작품 중에 뭐 보고 팬되신거에요?",
          "original_form": "김태리 작품 중에 뭐 보고 팬되신거에요?",
          "speaker_id": "2",
          "time": "20210831 14:58"
        },
        {
          "id": "MDRW2100024583.1.28",
          "form": "저 리틀 포레스트요!!",
          "original_form": "저 리틀 포레스트요!!",
          "speaker_id": "1",
          "time": "20210831 15:00"
        },
        {
          "id": "MDRW2100024583.1.29",
          "form": "혹시 보셨나요?",
          "original_form": "혹시 보셨나요?",
          "speaker_id": "1",
          "time": "20210831 15:01"
        },
        {
          "id": "MDRW2100024583.1.30",
          "form": "네네 거기서 진짜 예쁘더라구요ㅠㅠ",
          "original_form": "네네 거기서 진짜 예쁘더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 15:01"
        },
        {
          "id": "MDRW2100024583.1.31",
          "form": "그거 보고 아가씨까지 봤는데...",
          "original_form": "그거 보고 아가씨까지 봤는데...",
          "speaker_id": "1",
          "time": "20210831 15:02"
        },
        {
          "id": "MDRW2100024583.1.32",
          "form": "그거랑 아가씨 봤는데",
          "original_form": "그거랑 아가씨 봤는데",
          "speaker_id": "1",
          "time": "20210831 15:02"
        },
        {
          "id": "MDRW2100024583.1.33",
          "form": "헤어나올 수 없더라고욥",
          "original_form": "헤어나올 수 없더라고욥",
