      "utterance": [
        {
          "id": "MDRW2100003331.1.1",
          "form": "오늘날씨어땠어요?",
          "original_form": "오늘날씨어땠어요?",
          "speaker_id": "1",
          "time": "20210820 21:51"
        },
        {
          "id": "MDRW2100003331.1.2",
          "form": "오늘날씨 가을같아요",
          "original_form": "오늘날씨 가을같아요",
          "speaker_id": "2",
          "time": "20210820 21:56"
        },
        {
          "id": "MDRW2100003331.1.3",
          "form": "맞아요 선선해요\r\n내일비온데요ㅜㅜ",
          "original_form": "맞아요 선선해요\r\n내일비온데요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 21:58"
        },
        {
          "id": "MDRW2100003331.1.4",
          "form": "이제장마인데",
          "original_form": "이제장마인데",
          "speaker_id": "2",
          "time": "20210820 22:02"
        },
        {
          "id": "MDRW2100003331.1.5",
          "form": "비조심해요",
          "original_form": "비조심해요",
          "speaker_id": "2",
          "time": "20210820 22:02"
        },
        {
          "id": "MDRW2100003331.1.6",
          "form": "비올때 나가는거 싫어하죠?ㅋ",
          "original_form": "비올때 나가는거 싫어하죠?ㅋ",
          "speaker_id": "1",
          "time": "20210820 22:03"
        },
        {
          "id": "MDRW2100003331.1.7",
          "form": "비오는날 집이최고죠ㅋㅋㅋ",
          "original_form": "비오는날 집이최고죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:07"
        },
        {
          "id": "MDRW2100003331.1.8",
          "form": "닌가면안좋아요",
          "original_form": "닌가면안좋아요",
          "speaker_id": "2",
          "time": "20210820 22:07"
        },
        {
          "id": "MDRW2100003331.1.9",
          "form": "좋아하는 계절은요?",
          "original_form": "좋아하는 계절은요?",
          "speaker_id": "1",
          "time": "20210820 22:11"
        },
        {
          "id": "MDRW2100003331.1.10",
          "form": "가을이요",
          "original_form": "가을이요",
          "speaker_id": "2",
          "time": "20210820 22:11"
        },
        {
          "id": "MDRW2100003331.1.11",
          "form": "가을이 점점더짧아지는 느낌이에요ㅜ",
          "original_form": "가을이 점점더짧아지는 느낌이에요ㅜ",
          "speaker_id": "1",
          "time": "20210820 22:22"
        },
        {
          "id": "MDRW2100003331.1.12",
          "form": "봄여~~~릉 갈 겨~~~~울",
          "original_form": "봄여~~~릉 갈 겨~~~~울",
          "speaker_id": "2",
          "time": "20210820 22:27"
        },
        {
          "id": "MDRW2100003331.1.13",
          "form": "ㅋㅋ 갈 정답\r\n그래도 요즘 하늘 넘 이뻐요",
          "original_form": "ㅋㅋ 갈 정답\r\n그래도 요즘 하늘 넘 이뻐요",
          "speaker_id": "1",
          "time": "20210820 22:31"
        },
        {
          "id": "MDRW2100003331.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210820 22:40"
        },
        {
          "id": "MDRW2100003331.1.15",
          "form": "공원좋은데",
          "original_form": "공원좋은데",
          "speaker_id": "2",
          "time": "20210820 22:40"
        },
        {
          "id": "MDRW2100003331.1.16",
          "form": "공원자주 가서 산책하고해야겟어요 비안올때ㅎㅎ",
          "original_form": "공원자주 가서 산책하고해야겟어요 비안올때ㅎㅎ",
