      "utterance": [
        {
          "id": "MDRW2100013695.1.1",
          "form": "전시회나 공연 보는거 좋아해?",
          "original_form": "전시회나 공연 보는거 좋아해?",
          "speaker_id": "1",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013695.1.2",
          "form": "너무 좋아하지 너는?",
          "original_form": "너무 좋아하지 너는?",
          "speaker_id": "2",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013695.1.3",
          "form": "나도 좋아해  최근에 공연 뭐봤어?",
          "original_form": "나도 좋아해  최근에 공연 뭐봤어?",
          "speaker_id": "1",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013695.1.4",
          "form": "라포엠이라는 성악가 공연 봤어",
          "original_form": "라포엠이라는 성악가 공연 봤어",
          "speaker_id": "2",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013695.1.5",
          "form": "어땠어? 노래좋아?",
          "original_form": "어땠어? 노래좋아?",
          "speaker_id": "1",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013695.1.6",
          "form": "응 되게 감동적이였어",
          "original_form": "응 되게 감동적이였어",
          "speaker_id": "2",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013695.1.7",
          "form": "나는 공연보단 전시가 좋아",
          "original_form": "나는 공연보단 전시가 좋아",
          "speaker_id": "1",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100013695.1.8",
          "form": "오 어떤 전시 가봤어?",
          "original_form": "오 어떤 전시 가봤어?",
          "speaker_id": "2",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013695.1.9",
          "form": "최근에 국립중앙박물관에서 이건희 전시 보고왔어",
          "original_form": "최근에 국립중앙박물관에서 이건희 전시 보고왔어",
          "speaker_id": "1",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013695.1.10",
          "form": "와 인왕제색도 보고옴?",
          "original_form": "와 인왕제색도 보고옴?",
          "speaker_id": "2",
          "time": "20210825 15:43"
        },
        {
          "id": "MDRW2100013695.1.11",
          "form": "맞아! 사실 그거 보려고 간게 컸는데 진짜 좋더라",
          "original_form": "맞아! 사실 그거 보려고 간게 컸는데 진짜 좋더라",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013695.1.12",
          "form": "좋았겠다",
          "original_form": "좋았겠다",
          "speaker_id": "2",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013695.1.13",
          "form": "다음에 같이 갈래?",
          "original_form": "다음에 같이 갈래?",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013695.1.14",
          "form": "좋아 나랑 공연도 갔이 가자",
          "original_form": "좋아 나랑 공연도 갔이 가자",
          "speaker_id": "2",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100013695.1.15",
          "form": "난 아무때나 좋아",
          "original_form": "난 아무때나 좋아",
          "speaker_id": "1",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100013695.1.16",
          "form": "안녕",
          "original_form": "안녕",
