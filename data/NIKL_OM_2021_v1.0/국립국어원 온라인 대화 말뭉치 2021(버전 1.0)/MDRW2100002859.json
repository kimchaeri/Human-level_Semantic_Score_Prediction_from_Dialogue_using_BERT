          "speaker_id": "2",
          "time": "20210820 16:02"
        },
        {
          "id": "MDRW2100002859.1.3",
          "form": "인생영화 있나요",
          "original_form": "인생영화 있나요",
          "speaker_id": "1",
          "time": "20210820 16:03"
        },
        {
          "id": "MDRW2100002859.1.4",
          "form": "첫키스만 50번째요",
          "original_form": "첫키스만 50번째요",
          "speaker_id": "2",
          "time": "20210820 16:04"
        },
        {
          "id": "MDRW2100002859.1.5",
          "form": "오 잼나나요",
          "original_form": "오 잼나나요",
          "speaker_id": "1",
          "time": "20210820 16:04"
        },
        {
          "id": "MDRW2100002859.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210820 16:05"
        },
        {
          "id": "MDRW2100002859.1.7",
          "form": "넷플 좋아해줘도 가볍게 보기좋아여",
          "original_form": "넷플 좋아해줘도 가볍게 보기좋아여",
          "speaker_id": "1",
          "time": "20210820 16:05"
        },
        {
          "id": "MDRW2100002859.1.8",
          "form": "최근에 봤어서여",
          "original_form": "최근에 봤어서여",
          "speaker_id": "1",
          "time": "20210820 16:05"
        },
        {
          "id": "MDRW2100002859.1.9",
          "form": "그래여?",
          "original_form": "그래여?",
          "speaker_id": "2",
          "time": "20210820 16:05"
        },
        {
          "id": "MDRW2100002859.1.10",
          "form": "넵 뮤지컬도 보고싶어영",
          "original_form": "넵 뮤지컬도 보고싶어영",
          "speaker_id": "1",
          "time": "20210820 16:07"
        },
        {
          "id": "MDRW2100002859.1.11",
          "form": "저는 아직 못봤어요",
          "original_form": "저는 아직 못봤어요",
          "speaker_id": "2",
          "time": "20210820 16:10"
        },
        {
          "id": "MDRW2100002859.1.12",
          "form": "꼭 보세요 ㅎ 전 일년에 한번씩은 꼭보러가요 ㅋㅋ 저만의 규칙이랄까",
          "original_form": "꼭 보세요 ㅎ 전 일년에 한번씩은 꼭보러가요 ㅋㅋ 저만의 규칙이랄까",
          "speaker_id": "1",
          "time": "20210820 16:10"
        },
        {
          "id": "MDRW2100002859.1.13",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "2",
          "time": "20210820 16:11"
        },
        {
          "id": "MDRW2100002859.1.14",
