      "utterance": [
        {
          "id": "MDRW2100037991.1.1",
          "form": "둘째도 합기도 보내야 되나ㅋㅋ",
          "original_form": "둘째도 합기도 보내야 되나ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:05"
        },
        {
          "id": "MDRW2100037991.1.2",
          "form": "우린 바이올린 끊었다ㅋㅋ",
          "original_form": "우린 바이올린 끊었다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:05"
        },
        {
          "id": "MDRW2100037991.1.3",
          "form": "아 진짜?? 잘했다.",
          "original_form": "아 진짜?? 잘했다.",
          "speaker_id": "1",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100037991.1.4",
          "form": "고민 많이 했었잖아",
          "original_form": "고민 많이 했었잖아",
          "speaker_id": "1",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100037991.1.5",
          "form": "진전이 없네ㅋㅋㅋㅋ",
          "original_form": "진전이 없네ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100037991.1.6",
          "form": "근데 선생님이 자꾸 시키니까",
          "original_form": "근데 선생님이 자꾸 시키니까",
          "speaker_id": "2",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100037991.1.7",
          "form": "본인도 재미가 없고",
          "original_form": "본인도 재미가 없고",
          "speaker_id": "2",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100037991.1.8",
          "form": "맞어 맞어 ㅋㅋ 잘했다. ㅋㅋㅋ",
          "original_form": "맞어 맞어 ㅋㅋ 잘했다. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100037991.1.9",
          "form": "그냥 배운거에 의미를 두자 ㅋㅋ",
          "original_form": "그냥 배운거에 의미를 두자 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100037991.1.10",
          "form": "피아노 잘 치잖아 ㅋㅋ",
          "original_form": "피아노 잘 치잖아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100037991.1.11",
          "form": "피아노도 그냥 정체돼 있다ㅋㅋㅋㅋ",
          "original_form": "피아노도 그냥 정체돼 있다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100037991.1.12",
          "form": "경미언니도 얘기하는데",
          "original_form": "경미언니도 얘기하는데",
          "speaker_id": "2",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100037991.1.13",
          "form": "진짜 뭘 얼마만큼 배운 건지를 모르겠다",
          "original_form": "진짜 뭘 얼마만큼 배운 건지를 모르겠다",
          "speaker_id": "2",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100037991.1.14",
          "form": "아하 ㅋㅋㅋ 재건이도 그렇다나?",
          "original_form": "아하 ㅋㅋㅋ 재건이도 그렇다나?",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037991.1.15",
          "form": "응 알 수가 없다면서 재건 이는 안 들려준다 했잖아",
          "original_form": "응 알 수가 없다면서 재건 이는 안 들려준다 했잖아",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037991.1.16",
          "form": "나도 솔직히 얘가 뭘 배우는지를 모르겠다",
          "original_form": "나도 솔직히 얘가 뭘 배우는지를 모르겠다",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037991.1.17",
          "form": "아 그렇기는 했지",
          "original_form": "아 그렇기는 했지",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037991.1.18",
          "form": "들려주면은 배우는줄 알텐데",
          "original_form": "들려주면은 배우는줄 알텐데",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037991.1.19",
          "form": "연희 도 집에 와서 치면",
          "original_form": "연희 도 집에 와서 치면",
          "speaker_id": "2",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100037991.1.20",
          "form": "솔직히 그때 한 달 배웠을 때랑",
          "original_form": "솔직히 그때 한 달 배웠을 때랑",
