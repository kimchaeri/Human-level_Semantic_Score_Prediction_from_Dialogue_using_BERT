      "utterance": [
        {
          "id": "MDRW2100009401.1.1",
          "form": "게임하시나요?",
          "original_form": "게임하시나요?",
          "speaker_id": "1",
          "time": "20210822 23:13"
        },
        {
          "id": "MDRW2100009401.1.2",
          "form": "게임 어제 폰겜만 세시간했어요 ㅋㅋ",
          "original_form": "게임 어제 폰겜만 세시간했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009401.1.3",
          "form": "엇 뭔 겜 하시나요?",
          "original_form": "엇 뭔 겜 하시나요?",
          "speaker_id": "1",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100009401.1.4",
          "form": "저도 엊그제부터 이상한 겜하고잇어요ㅜ",
          "original_form": "저도 엊그제부터 이상한 겜하고잇어요ㅜ",
          "speaker_id": "1",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100009401.1.5",
          "form": "전 어려운건 못하겠더라고요 ㅋㅋ",
          "original_form": "전 어려운건 못하겠더라고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:24"
        },
        {
          "id": "MDRW2100009401.1.6",
          "form": "캐시퍼라고",
          "original_form": "캐시퍼라고",
          "speaker_id": "2",
          "time": "20210822 23:24"
        },
        {
          "id": "MDRW2100009401.1.7",
          "form": "단순한 고전게임이요",
          "original_form": "단순한 고전게임이요",
          "speaker_id": "2",
          "time": "20210822 23:24"
        },
        {
          "id": "MDRW2100009401.1.8",
          "form": "저는 로얄 뭐더라ㅜㅜ",
          "original_form": "저는 로얄 뭐더라ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 23:24"
        },
        {
          "id": "MDRW2100009401.1.9",
          "form": "이상한 퍼즐겜이에요ㅋ",
          "original_form": "이상한 퍼즐겜이에요ㅋ",
          "speaker_id": "1",
          "time": "20210822 23:24"
        },
        {
          "id": "MDRW2100009401.1.10",
          "form": "캐시퍼는 광물캐서 팔면 포인트줘요",
          "original_form": "캐시퍼는 광물캐서 팔면 포인트줘요",
          "speaker_id": "2",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100009401.1.11",
          "form": "1포인트 2포인트",
          "original_form": "1포인트 2포인트",
          "speaker_id": "2",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100009401.1.12",
          "form": "현금으로준대요 환급 ㅋㅋ",
          "original_form": "현금으로준대요 환급 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100009401.1.13",
          "form": "오잉 mmo rpg인가요?",
          "original_form": "오잉 mmo rpg인가요?",
          "speaker_id": "1",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100009401.1.14",
          "form": "ㅇㅅㅇ그런복잡한 게임 아니에요",
          "original_form": "ㅇㅅㅇ그런복잡한 게임 아니에요",
          "speaker_id": "2",
          "time": "20210822 23:32"
        },
        {
          "id": "MDRW2100009401.1.15",
          "form": "진심 단순한 고전 폰게임",
          "original_form": "진심 단순한 고전 폰게임",
          "speaker_id": "2",
          "time": "20210822 23:32"
        },
        {
          "id": "MDRW2100009401.1.16",
          "form": "막 인터넷 연결 없이하는거요?",
          "original_form": "막 인터넷 연결 없이하는거요?",
          "speaker_id": "1",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009401.1.17",
          "form": "그건 아니에요 ㅋㅋ단지 약간 게임 +리워드앱의 개념이죠",
          "original_form": "그건 아니에요 ㅋㅋ단지 약간 게임 +리워드앱의 개념이죠",
          "speaker_id": "2",
          "time": "20210822 23:38"
        },
        {
          "id": "MDRW2100009401.1.18",
          "form": "제가 또 앱테크를 좀 해서요",
          "original_form": "제가 또 앱테크를 좀 해서요",
          "speaker_id": "2",
          "time": "20210822 23:38"
        },
        {
          "id": "MDRW2100009401.1.19",
          "form": "저는 스타크래프트 했었어요",
          "original_form": "저는 스타크래프트 했었어요",
          "speaker_id": "1",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100009401.1.20",
          "form": "리워드앱 개념 게임도 있나봐요",
          "original_form": "리워드앱 개념 게임도 있나봐요",
          "speaker_id": "1",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100009401.1.21",
          "form": "캐시퍼랑 치킨각이 그래요",
          "original_form": "캐시퍼랑 치킨각이 그래요",
          "speaker_id": "2",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100009401.1.22",
          "form": "막많이주는건 아니지만 ㅋ",
          "original_form": "막많이주는건 아니지만 ㅋ",
          "speaker_id": "2",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100009401.1.23",
          "form": "왠지 주말에도 게임하는 제 모습이 좀 짠하네용ㅋㅋ",
          "original_form": "왠지 주말에도 게임하는 제 모습이 좀 짠하네용ㅋㅋ",
