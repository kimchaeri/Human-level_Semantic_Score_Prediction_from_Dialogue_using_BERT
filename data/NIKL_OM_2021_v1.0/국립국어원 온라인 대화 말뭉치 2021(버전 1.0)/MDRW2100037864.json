      "utterance": [
        {
          "id": "MDRW2100037864.1.1",
          "form": "ㅇㅇ........무리야",
          "original_form": "ㅇㅇ........무리야",
          "speaker_id": "2",
          "time": "20210901 20:07"
        },
        {
          "id": "MDRW2100037864.1.2",
          "form": "ㅇ ㅏ.. 라이덴....",
          "original_form": "ㅇ ㅏ.. 라이덴....",
          "speaker_id": "1",
          "time": "20210901 20:11"
        },
        {
          "id": "MDRW2100037864.1.3",
          "form": "신이니까 명함은 따주고싶은데",
          "original_form": "신이니까 명함은 따주고싶은데",
          "speaker_id": "1",
          "time": "20210901 20:11"
        },
        {
          "id": "MDRW2100037864.1.4",
          "form": "160×90이면",
          "original_form": "160×90이면",
          "speaker_id": "1",
          "time": "20210901 20:11"
        },
        {
          "id": "MDRW2100037864.1.5",
          "form": "14400인가?",
          "original_form": "14400인가?",
          "speaker_id": "1",
          "time": "20210901 20:11"
        },
        {
          "id": "MDRW2100037864.1.6",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210901 20:11"
        },
        {
          "id": "MDRW2100037864.1.7",
          "form": "이거 한 번은 할 수 있는대",
          "original_form": "이거 한 번은 할 수 있는대",
          "speaker_id": "1",
          "time": "20210901 20:11"
        },
        {
          "id": "MDRW2100037864.1.8",
          "form": "그러면 이제",
          "original_form": "그러면 이제",
          "speaker_id": "2",
          "time": "20210901 20:11"
        },
        {
          "id": "MDRW2100037864.1.9",
          "form": "복각을 못하지",
          "original_form": "복각을 못하지",
          "speaker_id": "2",
          "time": "20210901 20:12"
        },
        {
          "id": "MDRW2100037864.1.10",
          "form": "이 다음에 픽업캐 뽑으려고 했을때",
          "original_form": "이 다음에 픽업캐 뽑으려고 했을때",
          "speaker_id": "1",
          "time": "20210901 20:12"
        },
        {
          "id": "MDRW2100037864.1.11",
          "form": "확뚫당하면",
          "original_form": "확뚫당하면",
          "speaker_id": "1",
          "time": "20210901 20:12"
        },
        {
          "id": "MDRW2100037864.1.12",
          "form": "때려치는거임...",
          "original_form": "때려치는거임...",
          "speaker_id": "1",
          "time": "20210901 20:12"
        },
        {
          "id": "MDRW2100037864.1.13",
          "form": "내가 감우는 이해할수잇어 복각할 때가 됐대",
          "original_form": "내가 감우는 이해할수잇어 복각할 때가 됐대",
          "speaker_id": "2",
          "time": "20210901 20:12"
        },
        {
          "id": "MDRW2100037864.1.14",
          "form": "근데 왜 알베도가 언급이 되냐고",
          "original_form": "근데 왜 알베도가 언급이 되냐고",
          "speaker_id": "2",
          "time": "20210901 20:12"
        },
        {
          "id": "MDRW2100037864.1.15",
          "form": "소 내놔....",
          "original_form": "소 내놔....",
          "speaker_id": "2",
          "time": "20210901 20:12"
        },
        {
          "id": "MDRW2100037864.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:13"
        },
        {
          "id": "MDRW2100037864.1.17",
          "form": "알베도도 복각할 때가 됐어",
          "original_form": "알베도도 복각할 때가 됐어",
          "speaker_id": "1",
          "time": "20210901 20:13"
        },
        {
          "id": "MDRW2100037864.1.18",
          "form": "싫어......",
          "original_form": "싫어......",
          "speaker_id": "2",
          "time": "20210901 20:13"
        },
        {
          "id": "MDRW2100037864.1.19",
          "form": "근데 알베도가 얼음이 아닌건",
          "original_form": "근데 알베도가 얼음이 아닌건",
          "speaker_id": "2",
          "time": "20210901 20:13"
        },
        {
          "id": "MDRW2100037864.1.20",
          "form": "좀 신기하긵하다",
          "original_form": "좀 신기하긵하다",
          "speaker_id": "2",
          "time": "20210901 20:13"
        },
        {
          "id": "MDRW2100037864.1.21",
          "form": "걔 약간 호문쿨루스같은거잔아",
          "original_form": "걔 약간 호문쿨루스같은거잔아",
          "speaker_id": "2",
          "time": "20210901 20:13"
        },
        {
          "id": "MDRW2100037864.1.22",
          "form": "인간과 비인간사이에 끼어잇단 생각 안하나봐",
          "original_form": "인간과 비인간사이에 끼어잇단 생각 안하나봐",
          "speaker_id": "2",
          "time": "20210901 20:14"
        },
        {
          "id": "MDRW2100037864.1.23",
          "form": "ㅇㅇ 본인이 정체성의 고민을 딱히 겪는거같진 않던...",
          "original_form": "ㅇㅇ 본인이 정체성의 고민을 딱히 겪는거같진 않던...",
          "speaker_id": "1",
          "time": "20210901 20:15"
        },
        {
          "id": "MDRW2100037864.1.24",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋ직업만족도 좀 높아보엿어....",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋ직업만족도 좀 높아보엿어....",
          "speaker_id": "2",
          "time": "20210901 20:15"
        },
        {
          "id": "MDRW2100037864.1.25",
          "form": "ㅇㅇ 설탕이랑 짝짜꿍 잘맞더라 ㅋㅋㅋㅋ",
          "original_form": "ㅇㅇ 설탕이랑 짝짜꿍 잘맞더라 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:16"
        },
        {
          "id": "MDRW2100037864.1.26",
          "form": "ㅇㅓ근데 그럼",
          "original_form": "ㅇㅓ근데 그럼",
          "speaker_id": "2",
          "time": "20210901 20:17"
        },
        {
          "id": "MDRW2100037864.1.27",
          "form": "얼음신도 그런 고민 할까?",
          "original_form": "얼음신도 그런 고민 할까?",
          "speaker_id": "2",
          "time": "20210901 20:17"
        },
        {
          "id": "MDRW2100037864.1.28",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100037864.1.29",
          "form": "얼음신은",
          "original_form": "얼음신은",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100037864.1.30",
          "form": "인간을 사랑했다 어쩌구 얘기 나온거 보면",
          "original_form": "인간을 사랑했다 어쩌구 얘기 나온거 보면",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100037864.1.31",
          "form": "사람편에 서서 가짜라는걸 밝힐지",
          "original_form": "사람편에 서서 가짜라는걸 밝힐지",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100037864.1.32",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100037864.1.33",
          "form": "아님 천리가 준 집정관의 정체성을 가질지",
          "original_form": "아님 천리가 준 집정관의 정체성을 가질지",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100037864.1.34",
          "form": "고민한듯",
          "original_form": "고민한듯",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100037864.1.35",
          "form": "아 맞아 궁금한게",
          "original_form": "아 맞아 궁금한게",
          "speaker_id": "2",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100037864.1.36",
          "form": "다이루크도 사안 쓴적있어??",
          "original_form": "다이루크도 사안 쓴적있어??",
          "speaker_id": "2",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100037864.1.37",
          "form": "아닐걸?",
          "original_form": "아닐걸?",
          "speaker_id": "1",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100037864.1.38",
          "form": "근데 잘은 모름",
          "original_form": "근데 잘은 모름",
          "speaker_id": "1",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100037864.1.39",
          "form": "얘가 중간에 반항기 들어서",
          "original_form": "얘가 중간에 반항기 들어서",
          "speaker_id": "1",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100037864.1.40",
          "form": "신의 눈 때려치고 딴짓한 적 잇거든",
          "original_form": "신의 눈 때려치고 딴짓한 적 잇거든",
          "speaker_id": "1",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100037864.1.41",
          "form": "장작위키 사안 설명에 다이루크 아빠랑 다이루크가",
          "original_form": "장작위키 사안 설명에 다이루크 아빠랑 다이루크가",
          "speaker_id": "2",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100037864.1.42",
          "form": "사안쓴적있다고",
          "original_form": "사안쓴적있다고",
          "speaker_id": "2",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100037864.1.43",
          "form": "나와서",
          "original_form": "나와서",
          "speaker_id": "2",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100037864.1.44",
          "form": "다이루크 아빠는 확실한데",
          "original_form": "다이루크 아빠는 확실한데",
          "speaker_id": "1",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100037864.1.45",
          "form": "다이루크도 썼엇나??",
          "original_form": "다이루크도 썼엇나??",
          "speaker_id": "1",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100037864.1.46",
          "form": "모르겠어......",
          "original_form": "모르겠어......",
          "speaker_id": "2",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100037864.1.47",
          "form": "얘네도 스토리작가 탈주했나",
          "original_form": "얘네도 스토리작가 탈주했나",
          "speaker_id": "1",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100037864.1.48",
          "form": "몬드나 리월에 비해 영....",
          "original_form": "몬드나 리월에 비해 영....",
          "speaker_id": "1",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100037864.1.49",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100037864.1.50",
