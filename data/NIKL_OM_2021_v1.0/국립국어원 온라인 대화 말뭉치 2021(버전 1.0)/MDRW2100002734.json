      "utterance": [
        {
          "id": "MDRW2100002734.1.1",
          "form": "이건희 전시회 가보고싶다",
          "original_form": "이건희 전시회 가보고싶다",
          "speaker_id": "2",
          "time": "20210820 15:08"
        },
        {
          "id": "MDRW2100002734.1.2",
          "form": "그러게 그거 경쟁률이 심하대",
          "original_form": "그러게 그거 경쟁률이 심하대",
          "speaker_id": "1",
          "time": "20210820 15:08"
        },
        {
          "id": "MDRW2100002734.1.3",
          "form": "아 내가 말한 건 이건희 미술품 전시회라는거야.",
          "original_form": "아 내가 말한 건 이건희 미술품 전시회라는거야.",
          "speaker_id": "2",
          "time": "20210820 15:08"
        },
        {
          "id": "MDRW2100002734.1.4",
          "form": "맞아.",
          "original_form": "맞아.",
          "speaker_id": "2",
          "time": "20210820 15:08"
        },
        {
          "id": "MDRW2100002734.1.5",
          "form": "티켓 품절이 빠르다고 봤어",
          "original_form": "티켓 품절이 빠르다고 봤어",
          "speaker_id": "2",
          "time": "20210820 15:08"
        },
        {
          "id": "MDRW2100002734.1.6",
          "form": "그렇지 ㅠㅠ 이건희를 전시할 수는 없으니까",
          "original_form": "그렇지 ㅠㅠ 이건희를 전시할 수는 없으니까",
          "speaker_id": "1",
          "time": "20210820 15:08"
        },
        {
          "id": "MDRW2100002734.1.7",
          "form": "너는 대구미술관에 가 본 적 있니?",
          "original_form": "너는 대구미술관에 가 본 적 있니?",
          "speaker_id": "2",
          "time": "20210820 15:08"
        },
        {
          "id": "MDRW2100002734.1.8",
          "form": "응응 너랑 갔었잖아",
          "original_form": "응응 너랑 갔었잖아",
          "speaker_id": "1",
          "time": "20210820 15:08"
        },
        {
          "id": "MDRW2100002734.1.9",
          "form": "말이 참 애매하다. 그렇지?",
          "original_form": "말이 참 애매하다. 그렇지?",
          "speaker_id": "2",
          "time": "20210820 15:08"
        },
        {
          "id": "MDRW2100002734.1.10",
          "form": "흠? 우리가 같이 갔었던가?",
          "original_form": "흠? 우리가 같이 갔었던가?",
          "speaker_id": "2",
          "time": "20210820 15:09"
        },
        {
          "id": "MDRW2100002734.1.11",
          "form": "어쨌든 너 전시회 다니는 거 좋아하니?",
          "original_form": "어쨌든 너 전시회 다니는 거 좋아하니?",
          "speaker_id": "2",
          "time": "20210820 15:09"
        },
        {
          "id": "MDRW2100002734.1.12",
          "form": "나는 엠비씨 전시회도 가보고싶었어",
          "original_form": "나는 엠비씨 전시회도 가보고싶었어",
          "speaker_id": "2",
          "time": "20210820 15:09"
        },
        {
          "id": "MDRW2100002734.1.13",
          "form": "응 전시회에 가면 문화생활하는 기분이야",
          "original_form": "응 전시회에 가면 문화생활하는 기분이야",
          "speaker_id": "1",
          "time": "20210820 15:09"
        },
        {
          "id": "MDRW2100002734.1.14",
          "form": "당시 코로나가 너무 심해져서 갈 수가 없었어.",
          "original_form": "당시 코로나가 너무 심해져서 갈 수가 없었어.",
          "speaker_id": "2",
          "time": "20210820 15:09"
        },
        {
          "id": "MDRW2100002734.1.15",
          "form": "맞아.",
          "original_form": "맞아.",
          "speaker_id": "2",
          "time": "20210820 15:09"
        },
        {
          "id": "MDRW2100002734.1.16",
          "form": "지금은 안하는 전시호ㅣ니?",
          "original_form": "지금은 안하는 전시호ㅣ니?",
          "speaker_id": "1",
          "time": "20210820 15:09"
        },
        {
          "id": "MDRW2100002734.1.17",
          "form": "현대인이 된 듯한 기분이야.",
          "original_form": "현대인이 된 듯한 기분이야.",
          "speaker_id": "2",
          "time": "20210820 15:09"
        },
        {
          "id": "MDRW2100002734.1.18",
          "form": "응 ㅠ 프랑스 관련 전시였던 거 같아",
          "original_form": "응 ㅠ 프랑스 관련 전시였던 거 같아",
          "speaker_id": "2",
          "time": "20210820 15:10"
        },
        {
          "id": "MDRW2100002734.1.19",
          "form": "맞아 일상에 활력이돼",
          "original_form": "맞아 일상에 활력이돼",
          "speaker_id": "1",
          "time": "20210820 15:10"
        },
        {
          "id": "MDRW2100002734.1.20",
          "form": "근데 너무 멀어",
          "original_form": "근데 너무 멀어",
          "speaker_id": "2",
          "time": "20210820 15:10"
        },
        {
          "id": "MDRW2100002734.1.21",
          "form": "우와 나도 궁금하다",
          "original_form": "우와 나도 궁금하다",
          "speaker_id": "1",
          "time": "20210820 15:10"
        }
      ]
