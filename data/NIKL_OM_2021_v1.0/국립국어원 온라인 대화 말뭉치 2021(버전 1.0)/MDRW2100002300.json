      "utterance": [
        {
          "id": "MDRW2100002300.1.1",
          "form": "스포츠 좋아하시나요?",
          "original_form": "스포츠 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210820 08:51"
        },
        {
          "id": "MDRW2100002300.1.2",
          "form": "음 하는 것보다 관람이 좋아요",
          "original_form": "음 하는 것보다 관람이 좋아요",
          "speaker_id": "2",
          "time": "20210820 08:52"
        },
        {
          "id": "MDRW2100002300.1.3",
          "form": "최근에 올림픽의 여운도 아직남아있구요",
          "original_form": "최근에 올림픽의 여운도 아직남아있구요",
          "speaker_id": "2",
          "time": "20210820 08:52"
        },
        {
          "id": "MDRW2100002300.1.4",
          "form": "맞아요 ㅋㅋ하는건 넘 숨차고 힘든데",
          "original_form": "맞아요 ㅋㅋ하는건 넘 숨차고 힘든데",
          "speaker_id": "1",
          "time": "20210820 09:27"
        },
        {
          "id": "MDRW2100002300.1.5",
          "form": "보는건 왤케 재밌는지 ㅋㅋ",
          "original_form": "보는건 왤케 재밌는지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 09:28"
        },
        {
          "id": "MDRW2100002300.1.6",
          "form": "이번에 가장 인상깊은 경기가 있었나요?",
          "original_form": "이번에 가장 인상깊은 경기가 있었나요?",
          "speaker_id": "2",
          "time": "20210820 09:29"
        },
        {
          "id": "MDRW2100002300.1.7",
          "form": "흠 저는 뭐니뭐니해도 양궁이요 !",
          "original_form": "흠 저는 뭐니뭐니해도 양궁이요 !",
          "speaker_id": "1",
          "time": "20210820 10:53"
        },
        {
          "id": "MDRW2100002300.1.8",
          "form": "아무래도 양궁이 핫했죠",
          "original_form": "아무래도 양궁이 핫했죠",
          "speaker_id": "2",
          "time": "20210820 10:54"
        },
        {
          "id": "MDRW2100002300.1.9",
          "form": "김제덕 선수의 코리아파이팅!!!이 인상적이었어요",
          "original_form": "김제덕 선수의 코리아파이팅!!!이 인상적이었어요",
          "speaker_id": "2",
          "time": "20210820 10:54"
        },
        {
          "id": "MDRW2100002300.1.10",
          "form": "맞아여ㅋㅋ 김제덕선수 흥해라 !!",
          "original_form": "맞아여ㅋㅋ 김제덕선수 흥해라 !!",
          "speaker_id": "1",
          "time": "20210820 13:06"
        },
        {
          "id": "MDRW2100002300.1.11",
          "form": "저는 이번에 수영도 괜찮았어요",
          "original_form": "저는 이번에 수영도 괜찮았어요",
          "speaker_id": "2",
          "time": "20210820 13:08"
        },
        {
          "id": "MDRW2100002300.1.12",
          "form": "뭔가 더 성장할 가능성이 보였다랄까",
          "original_form": "뭔가 더 성장할 가능성이 보였다랄까",
          "speaker_id": "2",
          "time": "20210820 13:08"
        },
        {
          "id": "MDRW2100002300.1.13",
          "form": "맞아요 아쉽지만 그래도 유망주발견이요 ㅎㅎ",
          "original_form": "맞아요 아쉽지만 그래도 유망주발견이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 13:10"
        },
        {
          "id": "MDRW2100002300.1.14",
          "form": "내년에도 국제대회 많대요",
          "original_form": "내년에도 국제대회 많대요",
          "speaker_id": "1",
          "time": "20210820 13:10"
        },
        {
          "id": "MDRW2100002300.1.15",
          "form": "맞아요 계속 성장해서 파리 때는 좋은 결과 보고싶어요",
          "original_form": "맞아요 계속 성장해서 파리 때는 좋은 결과 보고싶어요",
          "speaker_id": "2",
          "time": "20210820 13:10"
        },
        {
          "id": "MDRW2100002300.1.16",
          "form": "이번 올림픽은 뭔가 건강한 느낌이 들어서 좋았어요",
          "original_form": "이번 올림픽은 뭔가 건강한 느낌이 들어서 좋았어요",
          "speaker_id": "2",
          "time": "20210820 13:12"
        },
        {
          "id": "MDRW2100002300.1.17",
          "form": "기록과 상관없이 참여했다는 것에 의의를 둔 그게 좋았다랄까",
          "original_form": "기록과 상관없이 참여했다는 것에 의의를 둔 그게 좋았다랄까",
          "speaker_id": "2",
          "time": "20210820 13:13"
        },
        {
          "id": "MDRW2100002300.1.18",
          "form": "아하 다음 올림픽이 파리인가용 ?!",
          "original_form": "아하 다음 올림픽이 파리인가용 ?!",
          "speaker_id": "1",
          "time": "20210820 13:13"
        },
        {
          "id": "MDRW2100002300.1.19",
          "form": "네 3년후에요ㅎㅎ",
          "original_form": "네 3년후에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 13:14"
        },
        {
          "id": "MDRW2100002300.1.20",
          "form": "2024년!",
          "original_form": "2024년!",
