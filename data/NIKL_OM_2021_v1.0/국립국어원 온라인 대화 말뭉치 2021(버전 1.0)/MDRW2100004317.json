        },
        {
          "id": "MDRW2100004317.1.2",
          "form": "딤섬도 맛있어",
          "original_form": "딤섬도 맛있어",
          "speaker_id": "2",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004317.1.3",
          "form": "중식 맛있지",
          "original_form": "중식 맛있지",
          "speaker_id": "1",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004317.1.4",
          "form": "고추잡채랑 꽃빵 먹고 싶다",
          "original_form": "고추잡채랑 꽃빵 먹고 싶다",
          "speaker_id": "1",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004317.1.5",
          "form": "꽃빵 튀긴 거에 연유 찍어먹으면 맛있어",
          "original_form": "꽃빵 튀긴 거에 연유 찍어먹으면 맛있어",
          "speaker_id": "2",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004317.1.6",
          "form": "동파육 유산슬 또 뭐 있더라",
          "original_form": "동파육 유산슬 또 뭐 있더라",
          "speaker_id": "1",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004317.1.7",
          "form": "고량주 ㅋㅋㅋㅋㅋ",
          "original_form": "고량주 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004317.1.8",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004317.1.9",
          "form": "요리 되게 많은데 생각이 안난다",
          "original_form": "요리 되게 많은데 생각이 안난다",
          "speaker_id": "1",
          "time": "20210821 16:03"
        },
        {
          "id": "MDRW2100004317.1.10",
          "form": "전가복 샥스핀 ㅋㅋㅋ",
          "original_form": "전가복 샥스핀 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:05"
        },
        {
          "id": "MDRW2100004317.1.11",
          "form": "고급 음식으로 가겠다고? ㅋㅋㅋ",
          "original_form": "고급 음식으로 가겠다고? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 16:05"
        },
        {
          "id": "MDRW2100004317.1.12",
          "form": "그래도 기본인 짬뽕이랑 탕수육이 제일이지 ㅋㅋ",
          "original_form": "그래도 기본인 짬뽕이랑 탕수육이 제일이지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:06"
        },
        {
          "id": "MDRW2100004317.1.13",
          "form": "와 얘기하니까 너무 먹고 싶어졌어",
          "original_form": "와 얘기하니까 너무 먹고 싶어졌어",
          "speaker_id": "1",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004317.1.14",
          "form": "점심 먹은 지 얼마 안된 거 같은데",
          "original_form": "점심 먹은 지 얼마 안된 거 같은데",
          "speaker_id": "2",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004317.1.15",
          "form": "배고프다 ㅋㅋ",
          "original_form": "배고프다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004317.1.16",
          "form": "맛있는 건 정말 참을 수 없어",
          "original_form": "맛있는 건 정말 참을 수 없어",
          "speaker_id": "2",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004317.1.17",
          "form": "누구든 맛을 보면 이렇게!",
          "original_form": "누구든 맛을 보면 이렇게!",
          "speaker_id": "1",
          "time": "20210821 16:08"
        }
      ]
