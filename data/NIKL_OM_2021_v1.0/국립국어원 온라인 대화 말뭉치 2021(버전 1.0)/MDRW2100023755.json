      "utterance": [
        {
          "id": "MDRW2100023755.1.1",
          "form": "그러니까 갑자기 해가 쨍해",
          "original_form": "그러니까 갑자기 해가 쨍해",
          "speaker_id": "2",
          "time": "20210830 16:39"
        },
        {
          "id": "MDRW2100023755.1.2",
          "form": "이따가 나가야 하는데",
          "original_form": "이따가 나가야 하는데",
          "speaker_id": "1",
          "time": "20210830 16:39"
        },
        {
          "id": "MDRW2100023755.1.3",
          "form": "장보러 간다고 했지?",
          "original_form": "장보러 간다고 했지?",
          "speaker_id": "2",
          "time": "20210830 16:40"
        },
        {
          "id": "MDRW2100023755.1.4",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210830 16:40"
        },
        {
          "id": "MDRW2100023755.1.5",
          "form": "더워서 걸어갈 수 있겠나?",
          "original_form": "더워서 걸어갈 수 있겠나?",
          "speaker_id": "2",
          "time": "20210830 16:40"
        },
        {
          "id": "MDRW2100023755.1.6",
          "form": "그니까 말이야",
          "original_form": "그니까 말이야",
          "speaker_id": "1",
          "time": "20210830 16:40"
        },
        {
          "id": "MDRW2100023755.1.7",
          "form": "한동안 좀 가을 날씨가 되는 것 같더니",
          "original_form": "한동안 좀 가을 날씨가 되는 것 같더니",
          "speaker_id": "2",
          "time": "20210830 16:40"
        },
        {
          "id": "MDRW2100023755.1.8",
          "form": "하 그러게",
          "original_form": "하 그러게",
          "speaker_id": "1",
          "time": "20210830 16:41"
        },
        {
          "id": "MDRW2100023755.1.9",
          "form": "며칠 날씨가 참 좋았었는데",
          "original_form": "며칠 날씨가 참 좋았었는데",
          "speaker_id": "2",
          "time": "20210830 16:41"
        },
        {
          "id": "MDRW2100023755.1.10",
          "form": "날씨가 시원해져야 맘편히 나갈텐데",
          "original_form": "날씨가 시원해져야 맘편히 나갈텐데",
          "speaker_id": "1",
          "time": "20210830 16:41"
        },
        {
          "id": "MDRW2100023755.1.11",
          "form": "맞아 날씨가 선선하면 다니기가 좋지",
          "original_form": "맞아 날씨가 선선하면 다니기가 좋지",
          "speaker_id": "2",
          "time": "20210830 16:42"
        },
        {
          "id": "MDRW2100023755.1.12",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210830 16:42"
        },
        {
          "id": "MDRW2100023755.1.13",
          "form": "갑자기 왜 다시 더워졌을까",
          "original_form": "갑자기 왜 다시 더워졌을까",
          "speaker_id": "2",
          "time": "20210830 16:43"
        },
        {
          "id": "MDRW2100023755.1.14",
          "form": "날씨가 오락가락 하는듯",
          "original_form": "날씨가 오락가락 하는듯",
          "speaker_id": "1",
          "time": "20210830 16:43"
        },
        {
          "id": "MDRW2100023755.1.15",
          "form": "오늘 날이 덥다",
          "original_form": "오늘 날이 덥다",
