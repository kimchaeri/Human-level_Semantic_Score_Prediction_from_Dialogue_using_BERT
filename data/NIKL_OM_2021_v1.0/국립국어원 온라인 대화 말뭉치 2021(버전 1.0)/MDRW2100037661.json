      "utterance": [
        {
          "id": "MDRW2100037661.1.1",
          "form": "오늘 맛있는 점심 드셨나요??",
          "original_form": "오늘 맛있는 점심 드셨나요??",
          "speaker_id": "1",
          "time": "20210901 14:51"
        },
        {
          "id": "MDRW2100037661.1.2",
          "form": "네 맛점했습니다!",
          "original_form": "네 맛점했습니다!",
          "speaker_id": "2",
          "time": "20210901 14:52"
        },
        {
          "id": "MDRW2100037661.1.3",
          "form": "name1님은 뭐 드셨나요?",
          "original_form": "&name1&님은 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210901 14:52"
        },
        {
          "id": "MDRW2100037661.1.4",
          "form": "저는 들깨칼국수 먹었어요",
          "original_form": "저는 들깨칼국수 먹었어요",
          "speaker_id": "1",
          "time": "20210901 15:12"
        },
        {
          "id": "MDRW2100037661.1.5",
          "form": "고소하고 국물도 걸쭉해서 든든하고 좋았어요 ㅎㅎ",
          "original_form": "고소하고 국물도 걸쭉해서 든든하고 좋았어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 15:13"
        },
        {
          "id": "MDRW2100037661.1.6",
          "form": "뭐드셨어요?",
          "original_form": "뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210901 15:13"
        },
        {
          "id": "MDRW2100037661.1.7",
          "form": "전  밥에다 김치전이요",
          "original_form": "전  밥에다 김치전이요",
          "speaker_id": "2",
          "time": "20210901 15:21"
        },
        {
          "id": "MDRW2100037661.1.8",
          "form": "김치 많이넣고 간 세게 해서 반찬처럼 먹었어요",
          "original_form": "김치 많이넣고 간 세게 해서 반찬처럼 먹었어요",
          "speaker_id": "2",
          "time": "20210901 15:21"
        },
        {
          "id": "MDRW2100037661.1.9",
          "form": "오~ 김치전 맛있죠ㅠ",
          "original_form": "오~ 김치전 맛있죠ㅠ",
          "speaker_id": "1",
          "time": "20210901 15:23"
        },
        {
          "id": "MDRW2100037661.1.10",
          "form": "요즘 비와서 부침개땡기더라구요",
          "original_form": "요즘 비와서 부침개땡기더라구요",
          "speaker_id": "1",
          "time": "20210901 15:23"
        },
        {
          "id": "MDRW2100037661.1.11",
          "form": "저는 국물요리도 너무 땡겻어요",
          "original_form": "저는 국물요리도 너무 땡겻어요",
          "speaker_id": "1",
          "time": "20210901 15:23"
        },
        {
          "id": "MDRW2100037661.1.12",
          "form": "네.ㅈ 저도 비와서 후닥 부쳤어요ㅋㅋ",
          "original_form": "네.ㅈ 저도 비와서 후닥 부쳤어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:41"
        },
        {
          "id": "MDRW2100037661.1.13",
          "form": "들깨 칼국수... 속이 뜨끈했겠네요",
          "original_form": "들깨 칼국수... 속이 뜨끈했겠네요",
          "speaker_id": "2",
          "time": "20210901 15:41"
        },
        {
          "id": "MDRW2100037661.1.14",
          "form": "네 국물이 약간 꾸덕한 쪽이었어서 좋더라구요",
          "original_form": "네 국물이 약간 꾸덕한 쪽이었어서 좋더라구요",
          "speaker_id": "1",
          "time": "20210901 15:45"
        },
        {
          "id": "MDRW2100037661.1.15",
          "form": "든든하니 ㅎㅎ",
          "original_form": "든든하니 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 15:45"
        },
        {
          "id": "MDRW2100037661.1.16",
          "form": "좋아하는 음식 있으세요?",
          "original_form": "좋아하는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210901 15:45"
        },
        {
          "id": "MDRW2100037661.1.17",
          "form": "음 지금은 좀 맑은 국물같은게 땡기네요",
          "original_form": "음 지금은 좀 맑은 국물같은게 땡기네요",
          "speaker_id": "2",
          "time": "20210901 15:49"
        },
        {
          "id": "MDRW2100037661.1.18",
          "form": "샤브샤브같은거요",
          "original_form": "샤브샤브같은거요",
          "speaker_id": "2",
          "time": "20210901 15:49"
        },
        {
          "id": "MDRW2100037661.1.19",
          "form": "버섯이랑 배추 많이 해서!",
          "original_form": "버섯이랑 배추 많이 해서!",
          "speaker_id": "2",
          "time": "20210901 15:49"
        },
        {
          "id": "MDRW2100037661.1.20",
          "form": "오! 저도 샤브샤브 좋아해요! 멸치육수에다가 먹는!",
          "original_form": "오! 저도 샤브샤브 좋아해요! 멸치육수에다가 먹는!",
          "speaker_id": "1",
          "time": "20210901 15:52"
        },
        {
          "id": "MDRW2100037661.1.21",
          "form": "샤브에 들어간 배추 청경채ㅠㅠ 이런것들 담백하고 맛있잖아요 ㅎㅎ",
          "original_form": "샤브에 들어간 배추 청경채ㅠㅠ 이런것들 담백하고 맛있잖아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 15:52"
        },
        {
          "id": "MDRW2100037661.1.22",
          "form": "선호하는 체인점 있으세요?",
          "original_form": "선호하는 체인점 있으세요?",
          "speaker_id": "1",
          "time": "20210901 15:52"
        },
        {
          "id": "MDRW2100037661.1.23",
          "form": "근방에 어바웃 샤브 있어서 종종 가요",
          "original_form": "근방에 어바웃 샤브 있어서 종종 가요",
          "speaker_id": "2",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100037661.1.24",
          "form": "1인용도 팔구요",
          "original_form": "1인용도 팔구요",
          "speaker_id": "2",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100037661.1.25",
          "form": "오 처음들어보는 샤브집이에요!!",
          "original_form": "오 처음들어보는 샤브집이에요!!",
          "speaker_id": "1",
          "time": "20210901 15:56"
        },
        {
          "id": "MDRW2100037661.1.26",
          "form": "저도 1인샤브 자주 가거든요~~ 오 검색해서 주변에 있는지 봐야겠어요!!!",
          "original_form": "저도 1인샤브 자주 가거든요~~ 오 검색해서 주변에 있는지 봐야겠어요!!!",
          "speaker_id": "1",
          "time": "20210901 15:56"
        },
        {
          "id": "MDRW2100037661.1.27",
          "form": "체인점이니까 있을수도 있을거에요",
          "original_form": "체인점이니까 있을수도 있을거에요",
          "speaker_id": "2",
          "time": "20210901 15:57"
        },
        {
          "id": "MDRW2100037661.1.28",
          "form": "와 검색해봤는데 가성비좋네요!",
          "original_form": "와 검색해봤는데 가성비좋네요!",
          "speaker_id": "1",
          "time": "20210901 16:00"
        },
        {
          "id": "MDRW2100037661.1.29",
          "form": "저희동네에는 없지만 기회되면 먹어봐야겠어요 ㅎㅎ",
          "original_form": "저희동네에는 없지만 기회되면 먹어봐야겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 16:00"
        },
        {
          "id": "MDRW2100037661.1.30",
          "form": "검색하다가 숙명여대점 눌러봤는데 칼국수가 무제한이네요",
          "original_form": "검색하다가 숙명여대점 눌러봤는데 칼국수가 무제한이네요",
          "speaker_id": "1",
          "time": "20210901 16:00"
        },
        {
          "id": "MDRW2100037661.1.31",
          "form": "오, 여기는 칼국수 무제한은 아닌데",
          "original_form": "오, 여기는 칼국수 무제한은 아닌데",
          "speaker_id": "2",
          "time": "20210901 16:01"
        },
        {
          "id": "MDRW2100037661.1.32",
          "form": "그래도 1인세트 다먹으면 배부르니까요 뭐 ㅋㅋ",
          "original_form": "그래도 1인세트 다먹으면 배부르니까요 뭐 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:03"
        }
      ]
