        },
        {
          "id": "MDRW2100033969.1.2",
          "form": "하이",
          "original_form": "하이",
          "speaker_id": "2",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100033969.1.3",
          "form": "마라탕 토론하자",
          "original_form": "마라탕 토론하자",
          "speaker_id": "1",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100033969.1.4",
          "form": "그래ㅋㅋㅋ",
          "original_form": "그래ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100033969.1.5",
          "form": "어디에서 시켜 먹어?",
          "original_form": "어디에서 시켜 먹어?",
          "speaker_id": "2",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100033969.1.6",
          "form": "나 라와나 신미방",
          "original_form": "나 라와나 신미방",
          "speaker_id": "1",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100033969.1.7",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100033969.1.8",
          "form": "나는 상관없는데 셀프로 담을 수 있는 곳",
          "original_form": "나는 상관없는데 셀프로 담을 수 있는 곳",
          "speaker_id": "2",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100033969.1.9",
          "form": "셀프가 더 좋은가?",
          "original_form": "셀프가 더 좋은가?",
          "speaker_id": "1",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100033969.1.10",
          "form": "나는 안 먹는 재료가 꽤 있어서 셀프가 편해",
          "original_form": "나는 안 먹는 재료가 꽤 있어서 셀프가 편해",
          "speaker_id": "2",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100033969.1.11",
          "form": "그럼 그렇겠다",
          "original_form": "그럼 그렇겠다",
          "speaker_id": "1",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100033969.1.12",
          "form": "나는 호불호 갈리는 버섯들도 먹고 푸주나 건두부도 좋아해서",
          "original_form": "나는 호불호 갈리는 버섯들도 먹고 푸주나 건두부도 좋아해서",
          "speaker_id": "1",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100033969.1.13",
          "form": "너는 다 잘 먹지",
          "original_form": "너는 다 잘 먹지",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100033969.1.14",
          "form": "맞지 맞지ㅋㅋㅋㅋ",
          "original_form": "맞지 맞지ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100033969.1.15",
          "form": "소스는?",
          "original_form": "소스는?",
          "speaker_id": "1",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100033969.1.16",
          "form": "땅콩 소스가 제일 좋았어",
          "original_form": "땅콩 소스가 제일 좋았어",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100033969.1.17",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100033969.1.18",
          "form": "나는 따로 안 먹어",
          "original_form": "나는 따로 안 먹어",
          "speaker_id": "1",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100033969.1.19",
          "form": "무슨 맛이야?",
          "original_form": "무슨 맛이야?",
          "speaker_id": "1",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100033969.1.20",
          "form": "고소하고 찍어먹으면 덜 매워서 좋아",
          "original_form": "고소하고 찍어먹으면 덜 매워서 좋아",
          "speaker_id": "2",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100033969.1.21",
          "form": "오 괜찮네",
          "original_form": "오 괜찮네",
          "speaker_id": "1",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100033969.1.22",
          "form": "나도 다음에 한번 찍어 먹어봐야 할 듯",
          "original_form": "나도 다음에 한번 찍어 먹어봐야 할 듯",
          "speaker_id": "1",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100033969.1.23",
          "form": "좋아 다음에 같이 먹어",
          "original_form": "좋아 다음에 같이 먹어",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100033969.1.24",
          "form": "좋아좋아~",
          "original_form": "좋아좋아~",
          "speaker_id": "1",
          "time": "20210825 17:22"
        }
      ]
