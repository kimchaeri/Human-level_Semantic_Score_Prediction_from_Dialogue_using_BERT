      "utterance": [
        {
          "id": "MDRW2100018921.1.1",
          "form": "저녁 뭐드세요?",
          "original_form": "저녁 뭐드세요?",
          "speaker_id": "2",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100018921.1.2",
          "form": "저는 후라이드 치킨 먹어요~",
          "original_form": "저는 후라이드 치킨 먹어요~",
          "speaker_id": "1",
          "time": "20210827 19:14"
        },
        {
          "id": "MDRW2100018921.1.3",
          "form": "양념소스 콕콕 찍어서요!",
          "original_form": "양념소스 콕콕 찍어서요!",
          "speaker_id": "1",
          "time": "20210827 19:14"
        },
        {
          "id": "MDRW2100018921.1.4",
          "form": "오~ 치킨 맛있죠",
          "original_form": "오~ 치킨 맛있죠",
          "speaker_id": "2",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100018921.1.5",
          "form": "전 배불러서 기절할 것 같아요 ㅠ",
          "original_form": "전 배불러서 기절할 것 같아요 ㅠ",
          "speaker_id": "1",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100018921.1.6",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "1",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100018921.1.7",
          "form": "네ㅋㅋ 저는 닭갈비 먹었어요",
          "original_form": "네ㅋㅋ 저는 닭갈비 먹었어요",
          "speaker_id": "2",
          "time": "20210827 19:55"
        },
        {
          "id": "MDRW2100018921.1.8",
          "form": "춘천닭갈비요?",
          "original_form": "춘천닭갈비요?",
          "speaker_id": "1",
          "time": "20210827 19:57"
        },
        {
          "id": "MDRW2100018921.1.9",
          "form": "넹!!",
          "original_form": "넹!!",
          "speaker_id": "2",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100018921.1.10",
          "form": "역시나 쫄면사리가 맛있더라구요",
          "original_form": "역시나 쫄면사리가 맛있더라구요",
          "speaker_id": "2",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100018921.1.11",
          "form": "이야 쫄면사리~",
          "original_form": "이야 쫄면사리~",
          "speaker_id": "1",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100018921.1.12",
          "form": "반찬은 어떤 것들이 나왔어요?",
          "original_form": "반찬은 어떤 것들이 나왔어요?",
          "speaker_id": "1",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100018921.1.13",
          "form": "이번에 이사하면서 계란후라이가 생겼더라구요!",
          "original_form": "이번에 이사하면서 계란후라이가 생겼더라구요!",
          "speaker_id": "2",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100018921.1.14",
          "form": "마카로니랑 상추 옥수수콘 무 오이냉국이 있었어요",
          "original_form": "마카로니랑 상추 옥수수콘 무 오이냉국이 있었어요",
          "speaker_id": "2",
          "time": "20210827 20:15"
        },
        {
          "id": "MDRW2100018921.1.15",
          "form": "반찬도 진짜 잘나오네요",
          "original_form": "반찬도 진짜 잘나오네요",
          "speaker_id": "1",
          "time": "20210827 20:16"
        },
        {
          "id": "MDRW2100018921.1.16",
          "form": "가게 이전하면서 서비스 더 내놓기 어려운데 대단해요",
          "original_form": "가게 이전하면서 서비스 더 내놓기 어려운데 대단해요",
          "speaker_id": "1",
          "time": "20210827 20:16"
        },
        {
          "id": "MDRW2100018921.1.17",
          "form": "그쵸!",
          "original_form": "그쵸!",
          "speaker_id": "2",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100018921.1.18",
          "form": "놀라운건 저 알아보시더라구요ㅋㅋㅋ",
          "original_form": "놀라운건 저 알아보시더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:18"
        },
        {
          "id": "MDRW2100018921.1.19",
          "form": "쫄면사리 자주 드셨죠~  이러면서요ㅋㅋ 은근 기뻤어요",
          "original_form": "쫄면사리 자주 드셨죠~  이러면서요ㅋㅋ 은근 기뻤어요",
          "speaker_id": "2",
          "time": "20210827 20:18"
        },
        {
          "id": "MDRW2100018921.1.20",
          "form": "단골이신가봐요?!",
          "original_form": "단골이신가봐요?!",
          "speaker_id": "1",
          "time": "20210827 20:18"
        },
        {
          "id": "MDRW2100018921.1.21",
          "form": "틈틈히 갔다고 생각했는데 아니였나봐요ㅋㅋ",
          "original_form": "틈틈히 갔다고 생각했는데 아니였나봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100018921.1.22",
          "form": "먹을때마다 오랜만 같았거든요 ㅋㅋㄱ",
          "original_form": "먹을때마다 오랜만 같았거든요 ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100018921.1.23",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "1",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100018921.1.24",
          "form": "되게 훈훈하게 생기거나 그래서 기억하신 거 아닐까요?",
          "original_form": "되게 훈훈하게 생기거나 그래서 기억하신 거 아닐까요?",
          "speaker_id": "1",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100018921.1.25",
          "form": "앗! 매번 쫄면사리가 인상적이셨던거 같아요ㅋㅋㅋ",
          "original_form": "앗! 매번 쫄면사리가 인상적이셨던거 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:23"
        },
        {
          "id": "MDRW2100018921.1.26",
          "form": "ㅋㅋ 그 정도면 쫄면사리 더 많이 주고 그럴 거 같은데 서비스 받아 본 적 있어요?",
          "original_form": "ㅋㅋ 그 정도면 쫄면사리 더 많이 주고 그럴 거 같은데 서비스 받아 본 적 있어요?",
          "speaker_id": "1",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100018921.1.27",
          "form": "아뇽ㅋㅋㅋ 이번이 처음이에요ㅋㄱㅋㅋㅋ",
          "original_form": "아뇽ㅋㅋㅋ 이번이 처음이에요ㅋㄱㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100018921.1.28",
          "form": "그래서 더 기뻤어욥",
          "original_form": "그래서 더 기뻤어욥",
          "speaker_id": "2",
          "time": "20210827 20:27"
        }
