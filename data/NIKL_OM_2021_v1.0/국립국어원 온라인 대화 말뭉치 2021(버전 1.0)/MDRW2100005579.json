        },
        {
          "id": "MDRW2100005579.1.2",
          "form": "도마뱀 2마리요",
          "original_form": "도마뱀 2마리요",
          "speaker_id": "2",
          "time": "20210821 23:09"
        },
        {
          "id": "MDRW2100005579.1.3",
          "form": "비어디 드래곤이요",
          "original_form": "비어디 드래곤이요",
          "speaker_id": "2",
          "time": "20210821 23:09"
        },
        {
          "id": "MDRW2100005579.1.4",
          "form": "키우시나요?",
          "original_form": "키우시나요?",
          "speaker_id": "2",
          "time": "20210821 23:10"
        },
        {
          "id": "MDRW2100005579.1.5",
          "form": "오? 도마뱀ㅋㅋㅋ",
          "original_form": "오? 도마뱀ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:10"
        },
        {
          "id": "MDRW2100005579.1.6",
          "form": "아니요 저는 키우지 않아요 자신이 없어서ㅋㅋㅋ",
          "original_form": "아니요 저는 키우지 않아요 자신이 없어서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:10"
        },
        {
          "id": "MDRW2100005579.1.7",
          "form": "언제부터 키우신거에요?",
          "original_form": "언제부터 키우신거에요?",
          "speaker_id": "1",
          "time": "20210821 23:10"
        },
        {
          "id": "MDRW2100005579.1.8",
          "form": "저도 아직도 자신 없어요",
          "original_form": "저도 아직도 자신 없어요",
          "speaker_id": "2",
          "time": "20210821 23:10"
        },
        {
          "id": "MDRW2100005579.1.9",
          "form": "끝까지 잘키울까 걱정돼요",
          "original_form": "끝까지 잘키울까 걱정돼요",
          "speaker_id": "2",
          "time": "20210821 23:10"
        },
        {
          "id": "MDRW2100005579.1.10",
          "form": "2년 됐어요",
          "original_form": "2년 됐어요",
          "speaker_id": "2",
          "time": "20210821 23:10"
        },
        {
          "id": "MDRW2100005579.1.11",
          "form": "저녀석들 요새 사춘기인가 까칠해요",
          "original_form": "저녀석들 요새 사춘기인가 까칠해요",
          "speaker_id": "2",
          "time": "20210821 23:11"
        },
        {
          "id": "MDRW2100005579.1.12",
          "form": "저 째려봐요 ㅋㅋ",
          "original_form": "저 째려봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:11"
        },
        {
          "id": "MDRW2100005579.1.13",
          "form": "앜ㅋㅋㅋㅋㅋㅋ사진 기능만 있었어도 보여달라고 하는건데ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ사진 기능만 있었어도 보여달라고 하는건데ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005579.1.14",
          "form": "때찌 해줘야 겠네요",
          "original_form": "때찌 해줘야 겠네요",
          "speaker_id": "1",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005579.1.15",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005579.1.16",
          "form": "용같아요",
          "original_form": "용같아요",
          "speaker_id": "2",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005579.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ이누무 더마뱀들!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ이누무 더마뱀들!!",
          "speaker_id": "1",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005579.1.18",
          "form": "암수인가요?",
          "original_form": "암수인가요?",
          "speaker_id": "1",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005579.1.19",
          "form": "뭔가 근엄해 보여요 ㅋㅋ",
          "original_form": "뭔가 근엄해 보여요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005579.1.20",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005579.1.21",
          "form": "암컷이요",
          "original_form": "암컷이요",
          "speaker_id": "2",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005579.1.22",
          "form": "따로따로 키워서 그런가 식성 성격 달라요",
          "original_form": "따로따로 키워서 그런가 식성 성격 달라요",
          "speaker_id": "2",
          "time": "20210821 23:13"
        },
        {
          "id": "MDRW2100005579.1.23",
          "form": "오 서로 같이 놀지도 않구요?",
          "original_form": "오 서로 같이 놀지도 않구요?",
          "speaker_id": "1",
          "time": "20210821 23:13"
        },
        {
          "id": "MDRW2100005579.1.24",
          "form": "네 싸워요",
          "original_form": "네 싸워요",
          "speaker_id": "2",
          "time": "20210821 23:14"
        },
        {
          "id": "MDRW2100005579.1.25",
          "form": "같이 놓으면 클나요",
          "original_form": "같이 놓으면 클나요",
          "speaker_id": "2",
          "time": "20210821 23:14"
        },
        {
          "id": "MDRW2100005579.1.26",
          "form": "저번에도 물려서 발가락 잘릴뻔했어요",
          "original_form": "저번에도 물려서 발가락 잘릴뻔했어요",
          "speaker_id": "2",
          "time": "20210821 23:14"
        },
        {
          "id": "MDRW2100005579.1.27",
          "form": "누구 발가락이요?",
          "original_form": "누구 발가락이요?",
          "speaker_id": "1",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005579.1.28",
          "form": "도마뱀 발가락 말고 name2님?????",
          "original_form": "도마뱀 발가락 말고 &name2&님?????",
          "speaker_id": "1",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005579.1.29",
          "form": "저희 name3이요",
          "original_form": "저희 &name3&이요",
          "speaker_id": "2",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005579.1.30",
          "form": "name4이가 물었어요",
          "original_form": "&name4&이가 물었어요",
          "speaker_id": "2",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005579.1.31",
          "form": "이름이예요 ㅋㅋ",
          "original_form": "이름이예요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005579.1.32",
          "form": "서로 물어요",
          "original_form": "서로 물어요",
          "speaker_id": "2",
          "time": "20210821 23:17"
        },
        {
          "id": "MDRW2100005579.1.33",
          "form": "사람은 공격 절대 안하는데 서로 물어요",
          "original_form": "사람은 공격 절대 안하는데 서로 물어요",
          "speaker_id": "2",
          "time": "20210821 23:17"
        },
        {
          "id": "MDRW2100005579.1.34",
          "form": "어ㅜㅜ 무섭다 성격이 있는 애들이네요",
          "original_form": "어ㅜㅜ 무섭다 성격이 있는 애들이네요",
          "speaker_id": "1",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005579.1.35",
          "form": "같은 통안에는 못넣으시겠군요",
          "original_form": "같은 통안에는 못넣으시겠군요",
          "speaker_id": "1",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005579.1.36",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005579.1.37",
          "form": "따로 키우라 하더라구요",
          "original_form": "따로 키우라 하더라구요",
          "speaker_id": "2",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005579.1.38",
          "form": "그래도 순해요 자기들끼리만 그러지 사람한테는 안그래요",
          "original_form": "그래도 순해요 자기들끼리만 그러지 사람한테는 안그래요",
          "speaker_id": "2",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005579.1.39",
          "form": "십년의 책임이 있어서 번려동물은 지금도 맘은 불편해요 걱정으로",
