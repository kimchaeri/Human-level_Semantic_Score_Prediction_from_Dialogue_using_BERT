        {
          "id": "MDRW2100022277.1.1",
          "form": "김밥좋아하세요?",
          "original_form": "김밥좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 11:57"
        },
        {
          "id": "MDRW2100022277.1.2",
          "form": "김밥이요?",
          "original_form": "김밥이요?",
          "speaker_id": "2",
          "time": "20210829 11:58"
        },
        {
          "id": "MDRW2100022277.1.3",
          "form": "네 저는 제일 좋아하는 음식중 하나에요",
          "original_form": "네 저는 제일 좋아하는 음식중 하나에요",
          "speaker_id": "1",
          "time": "20210829 11:58"
        },
        {
          "id": "MDRW2100022277.1.4",
          "form": "김밥 맛있죠",
          "original_form": "김밥 맛있죠",
          "speaker_id": "2",
          "time": "20210829 11:58"
        },
        {
          "id": "MDRW2100022277.1.5",
          "form": "네 완전요 ㅎㅎ 안질리고 계속들어가요",
          "original_form": "네 완전요 ㅎㅎ 안질리고 계속들어가요",
          "speaker_id": "1",
          "time": "20210829 11:58"
        },
        {
          "id": "MDRW2100022277.1.6",
          "form": "김밥은 그게 문제..ㅎㅎ",
          "original_form": "김밥은 그게 문제..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 11:58"
        },
        {
          "id": "MDRW2100022277.1.7",
          "form": "맞아요 한번 먹으면 너무 많이 먹게 되어서ㅠㅠ",
          "original_form": "맞아요 한번 먹으면 너무 많이 먹게 되어서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 11:59"
        },
        {
          "id": "MDRW2100022277.1.8",
          "form": "ㅎ그쵸 집에서 싸서 드세요?",
          "original_form": "ㅎ그쵸 집에서 싸서 드세요?",
          "speaker_id": "2",
          "time": "20210829 11:59"
        },
        {
          "id": "MDRW2100022277.1.9",
          "form": "평소에는 사서 먹는데 가끔은 싸먹어요",
          "original_form": "평소에는 사서 먹는데 가끔은 싸먹어요",
          "speaker_id": "1",
          "time": "20210829 11:59"
        },
        {
          "id": "MDRW2100022277.1.10",
          "form": "대단하시네요 손이 많이 갈텐데..",
          "original_form": "대단하시네요 손이 많이 갈텐데..",
          "speaker_id": "2",
          "time": "20210829 11:59"
        },
        {
          "id": "MDRW2100022277.1.11",
          "form": "그래서 아주 가끔만요",
          "original_form": "그래서 아주 가끔만요",
          "speaker_id": "1",
          "time": "20210829 11:59"
        },
        {
          "id": "MDRW2100022277.1.12",
          "form": "저는 참치김밥 좋아해요",
          "original_form": "저는 참치김밥 좋아해요",
          "speaker_id": "2",
          "time": "20210829 12:00"
        },
        {
          "id": "MDRW2100022277.1.13",
          "form": "저도 참치김밥 좋아해요",
          "original_form": "저도 참치김밥 좋아해요",
          "speaker_id": "1",
          "time": "20210829 12:00"
        },
        {
          "id": "MDRW2100022277.1.14",
          "form": "그치만 제일 좋아하는건 일반김밥요 ㅎㅎ 집김밥",
          "original_form": "그치만 제일 좋아하는건 일반김밥요 ㅎㅎ 집김밥",
