      "utterance": [
        {
          "id": "MDRW2100043070.1.1",
          "form": "쉬는시간이 지났네여 커피한잔 드시고 계시겠네요",
          "original_form": "쉬는시간이 지났네여 커피한잔 드시고 계시겠네요",
          "speaker_id": "2",
          "time": "20210907 14:19"
        },
        {
          "id": "MDRW2100043070.1.2",
          "form": "네ㅜㅜ",
          "original_form": "네ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 14:19"
        },
        {
          "id": "MDRW2100043070.1.3",
          "form": "퇴근이간절합니당",
          "original_form": "퇴근이간절합니당",
          "speaker_id": "1",
          "time": "20210907 14:19"
        },
        {
          "id": "MDRW2100043070.1.4",
          "form": "매운거 잘드시나용?",
          "original_form": "매운거 잘드시나용?",
          "speaker_id": "1",
          "time": "20210907 14:21"
        },
        {
          "id": "MDRW2100043070.1.5",
          "form": "적당히요 ㅎㅎ 못먹는건 아닌데 또 너무매우면 ㅠㅠ 힘들어요 엽떡2단계",
          "original_form": "적당히요 ㅎㅎ 못먹는건 아닌데 또 너무매우면 ㅠㅠ 힘들어요 엽떡2단계",
          "speaker_id": "2",
          "time": "20210907 14:22"
        },
        {
          "id": "MDRW2100043070.1.6",
          "form": "가능ㅋ",
          "original_form": "가능ㅋ",
          "speaker_id": "2",
          "time": "20210907 14:22"
        },
        {
          "id": "MDRW2100043070.1.7",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210907 14:24"
        },
        {
          "id": "MDRW2100043070.1.8",
          "form": "저도 잘먹지도 못먹지도 않는 중간!!!!",
          "original_form": "저도 잘먹지도 못먹지도 않는 중간!!!!",
          "speaker_id": "1",
          "time": "20210907 14:24"
        },
        {
          "id": "MDRW2100043070.1.9",
          "form": "엽떡은 몇단계 가능??",
          "original_form": "엽떡은 몇단계 가능??",
          "speaker_id": "2",
          "time": "20210907 14:26"
        },
        {
          "id": "MDRW2100043070.1.10",
          "form": "2단계여!!!!",
          "original_form": "2단계여!!!!",
          "speaker_id": "1",
          "time": "20210907 14:26"
        },
        {
          "id": "MDRW2100043070.1.11",
          "form": "딱이군요 저랑비슷 ㅎㅎㅎ근데 2단계도 먹다보면 매워서 ㅋㅋ 콧물나여",
          "original_form": "딱이군요 저랑비슷 ㅎㅎㅎ근데 2단계도 먹다보면 매워서 ㅋㅋ 콧물나여",
          "speaker_id": "2",
          "time": "20210907 14:27"
        },
        {
          "id": "MDRW2100043070.1.12",
          "form": "저도저도",
          "original_form": "저도저도",
          "speaker_id": "1",
          "time": "20210907 14:27"
        },
        {
          "id": "MDRW2100043070.1.13",
          "form": "코풀면서 먹어요",
          "original_form": "코풀면서 먹어요",
          "speaker_id": "1",
          "time": "20210907 14:27"
        },
        {
          "id": "MDRW2100043070.1.14",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:27"
        },
        {
          "id": "MDRW2100043070.1.15",
          "form": "근데 포기못함 1단계는 너무 안맵고",
          "original_form": "근데 포기못함 1단계는 너무 안맵고",
          "speaker_id": "2",
          "time": "20210907 14:27"
        },
        {
          "id": "MDRW2100043070.1.16",
          "form": "ㅋㅋ그쳐!!!!!",
          "original_form": "ㅋㅋ그쳐!!!!!",
          "speaker_id": "1",
          "time": "20210907 14:27"
        },
        {
          "id": "MDRW2100043070.1.17",
          "form": "마라탕은 매운맛으로!!",
          "original_form": "마라탕은 매운맛으로!!",
          "speaker_id": "1",
          "time": "20210907 14:28"
        },
        {
          "id": "MDRW2100043070.1.18",
          "form": "엽떡은 맵던데",
          "original_form": "엽떡은 맵던데",
          "speaker_id": "1",
          "time": "20210907 14:29"
        },
        {
          "id": "MDRW2100043070.1.19",
          "form": "마라탕은 무조건 매운맛!!!!",
          "original_form": "마라탕은 무조건 매운맛!!!!",
          "speaker_id": "1",
          "time": "20210907 14:29"
        },
        {
          "id": "MDRW2100043070.1.20",
          "form": "저 아직 마라탕 안먹어봤는데",
          "original_form": "저 아직 마라탕 안먹어봤는데",
          "speaker_id": "2",
          "time": "20210907 14:29"
        },
        {
          "id": "MDRW2100043070.1.21",
          "form": "무신맛이져",
          "original_form": "무신맛이져",
          "speaker_id": "2",
          "time": "20210907 14:29"
        },
        {
          "id": "MDRW2100043070.1.22",
          "form": "분명히 좋하실꺼에요",
          "original_form": "분명히 좋하실꺼에요",
          "speaker_id": "1",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043070.1.23",
          "form": "매운 샤브샤브같은 느낌이에욧",
          "original_form": "매운 샤브샤브같은 느낌이에욧",
          "speaker_id": "1",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043070.1.24",
          "form": "오 사진으로 봤을땐 약간 잡탕...ㅋㅋ느낌",
          "original_form": "오 사진으로 봤을땐 약간 잡탕...ㅋㅋ느낌",
          "speaker_id": "2",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100043070.1.25",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:30"
        },
