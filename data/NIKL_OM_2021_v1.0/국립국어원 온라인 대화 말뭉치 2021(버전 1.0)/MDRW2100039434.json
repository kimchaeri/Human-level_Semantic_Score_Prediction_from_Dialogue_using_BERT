      "utterance": [
        {
          "id": "MDRW2100039434.1.1",
          "form": "배드민턴 좋제잉 나도 좋아해",
          "original_form": "배드민턴 좋제잉 나도 좋아해",
          "speaker_id": "2",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100039434.1.2",
          "form": "넌 건강해지면 따로하고 싶은 운동있니",
          "original_form": "넌 건강해지면 따로하고 싶은 운동있니",
          "speaker_id": "1",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100039434.1.3",
          "form": "같이 배드민턴치면 짱재밌겠다",
          "original_form": "같이 배드민턴치면 짱재밌겠다",
          "speaker_id": "1",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100039434.1.4",
          "form": "너랑 같이 배드민턴 칠래ㅋㅋㅋㅋ",
          "original_form": "너랑 같이 배드민턴 칠래ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100039434.1.5",
          "form": "헉 통했다",
          "original_form": "헉 통했다",
          "speaker_id": "2",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100039434.1.6",
          "form": "그러게 운동취향 비슷해서 다행이다",
          "original_form": "그러게 운동취향 비슷해서 다행이다",
          "speaker_id": "1",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100039434.1.7",
          "form": "배드민턴도 좋고 주짓수도 배우고 싶어",
          "original_form": "배드민턴도 좋고 주짓수도 배우고 싶어",
          "speaker_id": "2",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100039434.1.8",
          "form": "넌 주짓수 배워봤어?",
          "original_form": "넌 주짓수 배워봤어?",
          "speaker_id": "2",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100039434.1.9",
          "form": "아니 내친구가",
          "original_form": "아니 내친구가",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100039434.1.10",
          "form": "주짓수 잘못 배웠다가",
          "original_form": "주짓수 잘못 배웠다가",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100039434.1.11",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100039434.1.12",
          "form": "팔부러져서",
          "original_form": "팔부러져서",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100039434.1.13",
          "form": "좀 무섭더라고",
          "original_form": "좀 무섭더라고",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100039434.1.14",
          "form": "어???!?!",
          "original_form": "어???!?!",
          "speaker_id": "2",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100039434.1.15",
          "form": "제대로 하는곳 아니면 안가려고",
          "original_form": "제대로 하는곳 아니면 안가려고",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100039434.1.16",
          "form": "세상에 어떻게 가르치면 팔이 부러져",
          "original_form": "세상에 어떻게 가르치면 팔이 부러져",
          "speaker_id": "2",
          "time": "20210903 17:37"
        },
        {
          "id": "MDRW2100039434.1.17",
          "form": "내말이 운동도 제대로 가르치는곳에 가야지",
          "original_form": "내말이 운동도 제대로 가르치는곳에 가야지",
          "speaker_id": "1",
          "time": "20210903 17:37"
        },
        {
          "id": "MDRW2100039434.1.18",
          "form": "잘못 배우면 건강하긴 커녕",
          "original_form": "잘못 배우면 건강하긴 커녕",
          "speaker_id": "1",
          "time": "20210903 17:37"
        },
        {
          "id": "MDRW2100039434.1.19",
          "form": "진짜 제대로 안전하게 가르치는 데 가야겠다",
          "original_form": "진짜 제대로 안전하게 가르치는 데 가야겠다",
          "speaker_id": "2",
          "time": "20210903 17:37"
        },
        {
          "id": "MDRW2100039434.1.20",
          "form": "그러게 건강하려다가 몸 망치겠네",
          "original_form": "그러게 건강하려다가 몸 망치겠네",
          "speaker_id": "2",
          "time": "20210903 17:38"
        },
        {
          "id": "MDRW2100039434.1.21",
          "form": "피티는 잘 가르치니?",
          "original_form": "피티는 잘 가르치니?",
          "speaker_id": "2",
          "time": "20210903 17:38"
        },
        {
          "id": "MDRW2100039434.1.22",
          "form": "그러니까말이여",
          "original_form": "그러니까말이여",
          "speaker_id": "1",
          "time": "20210903 17:38"
        },
        {
          "id": "MDRW2100039434.1.23",
          "form": "관장이 땍땍대서 그렇지",
          "original_form": "관장이 땍땍대서 그렇지",
          "speaker_id": "1",
          "time": "20210903 17:38"
        },
        {
          "id": "MDRW2100039434.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:38"
        },
        {
          "id": "MDRW2100039434.1.25",
          "form": "운동은 잘가르쳐",
          "original_form": "운동은 잘가르쳐",
          "speaker_id": "1",
          "time": "20210903 17:38"
        },
        {
          "id": "MDRW2100039434.1.26",
          "form": "땍땍대다니 어떻기 말하길래",
          "original_form": "땍땍대다니 어떻기 말하길래",
          "speaker_id": "2",
          "time": "20210903 17:38"
        },
        {
          "id": "MDRW2100039434.1.27",
          "form": "좀 불평이 많은 스타일이라",
          "original_form": "좀 불평이 많은 스타일이라",
          "speaker_id": "1",
          "time": "20210903 17:39"
        },
        {
          "id": "MDRW2100039434.1.28",
          "form": "운동할때 잔소리가 심해",
          "original_form": "운동할때 잔소리가 심해",
          "speaker_id": "1",
          "time": "20210903 17:39"
        },
        {
          "id": "MDRW2100039434.1.29",
          "form": "오..저런",
          "original_form": "오..저런",
          "speaker_id": "2",
          "time": "20210903 17:39"
        },
        {
          "id": "MDRW2100039434.1.30",
          "form": "깐깐징어야?",
          "original_form": "깐깐징어야?",
          "speaker_id": "2",
          "time": "20210903 17:39"
        },
        {
          "id": "MDRW2100039434.1.31",
          "form": "그래도 베테랑이라 자세가르쳐줄땐",
          "original_form": "그래도 베테랑이라 자세가르쳐줄땐",
          "speaker_id": "1",
          "time": "20210903 17:39"
        },
        {
          "id": "MDRW2100039434.1.32",
          "form": "돈이 좋긴좋더라",
          "original_form": "돈이 좋긴좋더라",
          "speaker_id": "1",
          "time": "20210903 17:40"
        }
      ]
