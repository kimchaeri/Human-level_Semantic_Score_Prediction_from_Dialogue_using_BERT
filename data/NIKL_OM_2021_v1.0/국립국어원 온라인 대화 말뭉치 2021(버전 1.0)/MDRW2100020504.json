      "utterance": [
        {
          "id": "MDRW2100020504.1.1",
          "form": "어디 피자좋아하세요",
          "original_form": "어디 피자좋아하세요",
          "speaker_id": "2",
          "time": "20210828 08:33"
        },
        {
          "id": "MDRW2100020504.1.2",
          "form": "저도 피자스쿨 좋아합니다",
          "original_form": "저도 피자스쿨 좋아합니다",
          "speaker_id": "1",
          "time": "20210828 08:33"
        },
        {
          "id": "MDRW2100020504.1.3",
          "form": "한곳 더 있었는데 기억이 안나네요",
          "original_form": "한곳 더 있었는데 기억이 안나네요",
          "speaker_id": "1",
          "time": "20210828 08:34"
        },
        {
          "id": "MDRW2100020504.1.4",
          "form": "배민한번 들어갔다올게요",
          "original_form": "배민한번 들어갔다올게요",
          "speaker_id": "1",
          "time": "20210828 08:34"
        },
        {
          "id": "MDRW2100020504.1.5",
          "form": "기억해봐요",
          "original_form": "기억해봐요",
          "speaker_id": "2",
          "time": "20210828 08:34"
        },
        {
          "id": "MDRW2100020504.1.6",
          "form": "궁금해서 잠이안올지도몰라",
          "original_form": "궁금해서 잠이안올지도몰라",
          "speaker_id": "2",
          "time": "20210828 08:34"
        },
        {
          "id": "MDRW2100020504.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 08:34"
        },
        {
          "id": "MDRW2100020504.1.8",
          "form": "반올림피자샵이 그곳에도 있나요",
          "original_form": "반올림피자샵이 그곳에도 있나요",
          "speaker_id": "1",
          "time": "20210828 08:34"
        },
        {
          "id": "MDRW2100020504.1.9",
          "form": "당연하죠",
          "original_form": "당연하죠",
          "speaker_id": "2",
          "time": "20210828 08:35"
        },
        {
          "id": "MDRW2100020504.1.10",
          "form": "아이유의 반올림 피자샵",
          "original_form": "아이유의 반올림 피자샵",
          "speaker_id": "2",
          "time": "20210828 08:35"
        },
        {
          "id": "MDRW2100020504.1.11",
          "form": "앗 체인이었군요",
          "original_form": "앗 체인이었군요",
          "speaker_id": "1",
          "time": "20210828 08:35"
        },
        {
          "id": "MDRW2100020504.1.12",
          "form": "오만가지 맛을 다 먹을 수 있는",
          "original_form": "오만가지 맛을 다 먹을 수 있는",
          "speaker_id": "2",
          "time": "20210828 08:35"
        },
        {
          "id": "MDRW2100020504.1.13",
          "form": "이지역은 여기게 좀 맛있답니다",
          "original_form": "이지역은 여기게 좀 맛있답니다",
          "speaker_id": "1",
          "time": "20210828 08:35"
        },
        {
          "id": "MDRW2100020504.1.14",
          "form": "피자샵은 갈릭디핑이 찐인거알죠",
          "original_form": "피자샵은 갈릭디핑이 찐인거알죠",
          "speaker_id": "2",
          "time": "20210828 08:35"
        },
        {
          "id": "MDRW2100020504.1.15",
          "form": "다릇곳이랑 달라요",
          "original_form": "다릇곳이랑 달라요",
          "speaker_id": "2",
          "time": "20210828 08:35"
        },
        {
          "id": "MDRW2100020504.1.16",
          "form": "하 뭘 좀 아시네요",
          "original_form": "하 뭘 좀 아시네요",
          "speaker_id": "1",
          "time": "20210828 08:35"
        },
        {
          "id": "MDRW2100020504.1.17",
          "form": "어떤피자 즐겨드세요",
          "original_form": "어떤피자 즐겨드세요",
          "speaker_id": "2",
          "time": "20210828 08:35"
        },
        {
          "id": "MDRW2100020504.1.18",
          "form": "저는 골고루 먹지만",
          "original_form": "저는 골고루 먹지만",
          "speaker_id": "1",
          "time": "20210828 08:36"
        },
        {
          "id": "MDRW2100020504.1.19",
          "form": "페퍼로니는 꼭 들어가야합니다",
          "original_form": "페퍼로니는 꼭 들어가야합니다",
          "speaker_id": "1",
          "time": "20210828 08:36"
        },
        {
          "id": "MDRW2100020504.1.20",
          "form": "페퍼로니 피자 좋아해요",
          "original_form": "페퍼로니 피자 좋아해요",
          "speaker_id": "1",
          "time": "20210828 08:36"
        },
        {
          "id": "MDRW2100020504.1.21",
          "form": "아 그거 얇게 저며진 페퍼로니 그것은 갓벽",
          "original_form": "아 그거 얇게 저며진 페퍼로니 그것은 갓벽",
          "speaker_id": "2",
          "time": "20210828 08:36"
        }
      ]
