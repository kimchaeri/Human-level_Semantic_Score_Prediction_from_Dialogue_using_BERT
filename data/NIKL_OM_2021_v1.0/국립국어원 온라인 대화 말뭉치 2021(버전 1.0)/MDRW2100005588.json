        {
          "id": "MDRW2100005588.1.2",
          "form": "게임 안하는뎅?",
          "original_form": "게임 안하는뎅?",
          "speaker_id": "1",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005588.1.3",
          "form": "거짓말",
          "original_form": "거짓말",
          "speaker_id": "2",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005588.1.4",
          "form": "진짜다 지금 안한다",
          "original_form": "진짜다 지금 안한다",
          "speaker_id": "1",
          "time": "20210821 23:12"
        },
        {
