      "utterance": [
        {
          "id": "MDRW2100013333.1.1",
          "form": "오늘은 초과못하는 날이라 6시 칼퇴",
          "original_form": "오늘은 초과못하는 날이라 6시 칼퇴",
          "speaker_id": "2",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100013333.1.2",
          "form": "오 좋네",
          "original_form": "오 좋네",
          "speaker_id": "1",
          "time": "20210825 15:15"
        },
        {
          "id": "MDRW2100013333.1.3",
          "form": "나도 1800 퇴근",
          "original_form": "나도 1800 퇴근",
          "speaker_id": "1",
          "time": "20210825 15:15"
        },
        {
          "id": "MDRW2100013333.1.4",
          "form": "굳",
          "original_form": "굳",
          "speaker_id": "2",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100013333.1.5",
          "form": "나 그리고 초과근무시간 다 찼어..이제 이번날 남은 날은 칼퇴야",
          "original_form": "나 그리고 초과근무시간 다 찼어..이제 이번날 남은 날은 칼퇴야",
          "speaker_id": "2",
          "time": "20210825 15:25"
        },
        {
          "id": "MDRW2100013333.1.6",
          "form": "오 좋네",
          "original_form": "오 좋네",
          "speaker_id": "1",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013333.1.7",
          "form": "칼퇴가 최고여",
          "original_form": "칼퇴가 최고여",
          "speaker_id": "1",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013333.1.8",
          "form": "차가너무막힌다 ㅜ 비가와서 그런가",
          "original_form": "차가너무막힌다 ㅜ 비가와서 그런가",
          "speaker_id": "2",
          "time": "20210825 18:55"
        },
        {
          "id": "MDRW2100013333.1.9",
          "form": "아직 차안이야?",
          "original_form": "아직 차안이야?",
          "speaker_id": "1",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100013333.1.10",
          "form": "집 도착?",
          "original_form": "집 도착?",
          "speaker_id": "1",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100013333.1.11",
          "form": "name2",
          "original_form": "&name2&",
          "speaker_id": "1",
          "time": "20210826 08:08"
        },
        {
          "id": "MDRW2100013333.1.12",
          "form": "출근했숴?",
          "original_form": "출근했숴?",
          "speaker_id": "1",
          "time": "20210826 08:08"
        },
        {
          "id": "MDRW2100013333.1.13",
          "form": "어제 피곤해서 그냥 뻗어버렸엉",
          "original_form": "어제 피곤해서 그냥 뻗어버렸엉",
          "speaker_id": "2",
          "time": "20210826 08:15"
        },
        {
          "id": "MDRW2100013333.1.14",
          "form": "도착했징",
          "original_form": "도착했징",
          "speaker_id": "2",
          "time": "20210826 08:15"
        },
        {
          "id": "MDRW2100013333.1.15",
          "form": "오늘은 안개가 그윽하니 날씨가 좋을거같당",
          "original_form": "오늘은 안개가 그윽하니 날씨가 좋을거같당",
          "speaker_id": "2",
          "time": "20210826 08:16"
        },
        {
          "id": "MDRW2100013333.1.16",
          "form": "오늘 과장 팀장 없어서 좋을거닽애",
          "original_form": "오늘 과장 팀장 없어서 좋을거닽애",
          "speaker_id": "2",
          "time": "20210826 08:16"
        },
        {
          "id": "MDRW2100013333.1.17",
          "form": "굳굳",
          "original_form": "굳굳",
          "speaker_id": "1",
          "time": "20210826 08:16"
        },
        {
          "id": "MDRW2100013333.1.18",
          "form": "오늘은 편한날",
          "original_form": "오늘은 편한날",
          "speaker_id": "1",
          "time": "20210826 08:16"
        },
        {
          "id": "MDRW2100013333.1.19",
          "form": "나도 차장 없다",
          "original_form": "나도 차장 없다",
          "speaker_id": "1",
          "time": "20210826 08:16"
        },
        {
          "id": "MDRW2100013333.1.20",
          "form": "일은 많다",
          "original_form": "일은 많다",
          "speaker_id": "1",
          "time": "20210826 08:16"
        },
        {
          "id": "MDRW2100013333.1.21",
          "form": "굳  나도 일은 많은데 놀아야징",
          "original_form": "굳  나도 일은 많은데 놀아야징",
          "speaker_id": "2",
          "time": "20210826 08:40"
        },
        {
          "id": "MDRW2100013333.1.22",
          "form": "내가 결재권자야 젠장",
          "original_form": "내가 결재권자야 젠장",
          "speaker_id": "2",
          "time": "20210826 08:40"
        },
        {
          "id": "MDRW2100013333.1.23",
          "form": "ㅋㅋㅋ 좋네",
          "original_form": "ㅋㅋㅋ 좋네",
          "speaker_id": "1",
          "time": "20210826 08:41"
        },
        {
          "id": "MDRW2100013333.1.24",
          "form": "너보다 일찍들어오 9급들 많아?",
          "original_form": "너보다 일찍들어오 9급들 많아?",
          "speaker_id": "1",
          "time": "20210826 08:41"
        },
        {
          "id": "MDRW2100013333.1.25",
          "form": "그럼  요새는 9급 7급 다 20대 중반이야",
          "original_form": "그럼  요새는 9급 7급 다 20대 중반이야",
          "speaker_id": "2",
          "time": "20210826 08:41"
        },
        {
          "id": "MDRW2100013333.1.26",
          "form": "나는 엄청 늦은거지",
          "original_form": "나는 엄청 늦은거지",
          "speaker_id": "2",
          "time": "20210826 08:41"
        },
        {
          "id": "MDRW2100013333.1.27",
          "form": "아 20대중반에 7급은 괜찮다",
          "original_form": "아 20대중반에 7급은 괜찮다",
          "speaker_id": "1",
          "time": "20210826 08:45"
        },
        {
          "id": "MDRW2100013333.1.28",
          "form": "어쨌든 결재는 나에게",
          "original_form": "어쨌든 결재는 나에게",
          "speaker_id": "1",
          "time": "20210826 08:54"
        },
        {
          "id": "MDRW2100013333.1.29",
          "form": "이번에 2등으로 붙은 여자애 24살이야..",
          "original_form": "이번에 2등으로 붙은 여자애 24살이야..",
          "speaker_id": "2",
          "time": "20210826 09:40"
        },
        {
          "id": "MDRW2100013333.1.30",
          "form": "자괴감이 든다..ㅋㅋㅋ",
          "original_form": "자괴감이 든다..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 09:40"
        },
        {
          "id": "MDRW2100013333.1.31",
          "form": "7급?",
          "original_form": "7급?",
          "speaker_id": "1",
          "time": "20210826 09:42"
        },
        {
          "id": "MDRW2100013333.1.32",
          "form": "근데 늦게 시작했잖아",
          "original_form": "근데 늦게 시작했잖아",
          "speaker_id": "1",
          "time": "20210826 09:42"
        },
        {
          "id": "MDRW2100013333.1.33",
          "form": "그럴 수 있지",
          "original_form": "그럴 수 있지",
          "speaker_id": "1",
