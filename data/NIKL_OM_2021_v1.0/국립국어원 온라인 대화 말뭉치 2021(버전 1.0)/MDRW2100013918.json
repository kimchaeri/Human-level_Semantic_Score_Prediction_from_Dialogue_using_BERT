      "utterance": [
        {
          "id": "MDRW2100013918.1.1",
          "form": "나는 강아자 두마리를 키우고있어",
          "original_form": "나는 강아자 두마리를 키우고있어",
          "speaker_id": "1",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013918.1.2",
          "form": "큰 애는 name3고",
          "original_form": "큰 애는 &name3&고",
          "speaker_id": "1",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013918.1.3",
          "form": "쿠키",
          "original_form": "쿠키",
          "speaker_id": "2",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013918.1.4",
          "form": "작은 애는 name4야",
          "original_form": "작은 애는 &name4&야",
          "speaker_id": "1",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013918.1.5",
          "form": "ㅎ그럴거같았어",
          "original_form": "ㅎ그럴거같았어",
          "speaker_id": "2",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013918.1.6",
          "form": "입맛이 까다로워서 나우 사료만 먹는 고급진 애들..",
          "original_form": "입맛이 까다로워서 나우 사료만 먹는 고급진 애들..",
          "speaker_id": "1",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013918.1.7",
          "form": "고구마말랭이를 가장 좋아해",
          "original_form": "고구마말랭이를 가장 좋아해",
          "speaker_id": "1",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013918.1.8",
          "form": "너는 반려동물을 키우고있니?",
          "original_form": "너는 반려동물을 키우고있니?",
          "speaker_id": "1",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013918.1.9",
          "form": "나도 고양이 키우는데 처방사료라비싸",
          "original_form": "나도 고양이 키우는데 처방사료라비싸",
          "speaker_id": "2",
          "time": "20210825 16:42"
        },
        {
          "id": "MDRW2100013918.1.10",
          "form": "간식주면 설사해서 잘안줘",
          "original_form": "간식주면 설사해서 잘안줘",
          "speaker_id": "2",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013918.1.11",
          "form": "그래서 성격이 모나졌어 물어 사람을",
          "original_form": "그래서 성격이 모나졌어 물어 사람을",
          "speaker_id": "2",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013918.1.12",
          "form": "어제 스크래쳐 산거 줬더니 좋아서 긁어",
          "original_form": "어제 스크래쳐 산거 줬더니 좋아서 긁어",
          "speaker_id": "2",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013918.1.13",
          "form": "어머 너가 조금 더 잘해주면 안물지 않을까?",
          "original_form": "어머 너가 조금 더 잘해주면 안물지 않을까?",
          "speaker_id": "1",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013918.1.14",
          "form": "어떻데 더잘해",
          "original_form": "어떻데 더잘해",
          "speaker_id": "2",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013918.1.15",
          "form": "오 고양이는 스크래쳐가 있구나",
          "original_form": "오 고양이는 스크래쳐가 있구나",
          "speaker_id": "1",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013918.1.16",
          "form": "우리 강아지는",
          "original_form": "우리 강아지는",
          "speaker_id": "1",
          "time": "20210825 16:43"
        },
        {
          "id": "MDRW2100013918.1.17",
          "form": "인형 많이줬는데",
          "original_form": "인형 많이줬는데",
          "speaker_id": "1",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013918.1.18",
          "form": "애기 때 처음으로 받은 인형만 좋아해",
          "original_form": "애기 때 처음으로 받은 인형만 좋아해",
          "speaker_id": "1",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013918.1.19",
          "form": "둘 다",
          "original_form": "둘 다",
          "speaker_id": "1",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013918.1.20",
          "form": "오 우리는 상자만 있으면돼",
          "original_form": "오 우리는 상자만 있으면돼",
          "speaker_id": "2",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013918.1.21",
          "form": "그게 첫 선물이라 기분이 좋은가봐",
          "original_form": "그게 첫 선물이라 기분이 좋은가봐",
          "speaker_id": "1",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013918.1.22",
          "form": "ㅋㅋㅋ귀엽다 애착인형인가",
          "original_form": "ㅋㅋㅋ귀엽다 애착인형인가",
          "speaker_id": "2",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013918.1.23",
          "form": "너덜너덜해서 꼬매고 또 꼬매고..",
          "original_form": "너덜너덜해서 꼬매고 또 꼬매고..",
          "speaker_id": "1",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013918.1.24",
          "form": "나도 집에인형많아",
          "original_form": "나도 집에인형많아",
          "speaker_id": "2",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013918.1.25",
          "form": "ㅎ..",
          "original_form": "ㅎ..",
          "speaker_id": "2",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013918.1.26",
          "form": "고양이껀아니고..",
          "original_form": "고양이껀아니고..",
          "speaker_id": "2",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013918.1.27",
          "form": "우리 강아지 줄래?",
          "original_form": "우리 강아지 줄래?",
          "speaker_id": "1",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013918.1.28",
          "form": "아니..",
          "original_form": "아니..",
          "speaker_id": "2",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013918.1.29",
          "form": "name3 name4를 내게 줘",
          "original_form": "&name3& &name4&를 내게 줘",
          "speaker_id": "2",
          "time": "20210825 16:45"
        },
