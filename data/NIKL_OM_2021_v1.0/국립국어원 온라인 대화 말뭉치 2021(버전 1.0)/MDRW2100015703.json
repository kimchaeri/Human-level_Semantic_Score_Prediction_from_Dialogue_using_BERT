      "utterance": [
        {
          "id": "MDRW2100015703.1.1",
          "form": "네이버웹툰중에 흑막여주 그거 보셨어요?",
          "original_form": "네이버웹툰중에 흑막여주 그거 보셨어요?",
          "speaker_id": "1",
          "time": "20210826 00:25"
        },
        {
          "id": "MDRW2100015703.1.2",
          "form": "허구내!!!!",
          "original_form": "허구내!!!!",
          "speaker_id": "2",
          "time": "20210826 00:29"
        },
        {
          "id": "MDRW2100015703.1.3",
          "form": "헉 네네!!!",
          "original_form": "헉 네네!!!",
          "speaker_id": "2",
          "time": "20210826 00:29"
        },
        {
          "id": "MDRW2100015703.1.4",
          "form": "그 밤톨",
          "original_form": "그 밤톨",
          "speaker_id": "2",
          "time": "20210826 00:29"
        },
        {
          "id": "MDRW2100015703.1.5",
          "form": "너무 귀엽죠",
          "original_form": "너무 귀엽죠",
          "speaker_id": "2",
          "time": "20210826 00:29"
        },
        {
          "id": "MDRW2100015703.1.6",
          "form": "밤톨이ㅠㅠㅜㅜ아 모시던 아가씨가 도련님이 되어버렸다 도 보고있어요!",
          "original_form": "밤톨이ㅠㅠㅜㅜ아 모시던 아가씨가 도련님이 되어버렸다 도 보고있어요!",
          "speaker_id": "1",
          "time": "20210826 00:30"
        },
        {
          "id": "MDRW2100015703.1.7",
          "form": "허얼",
          "original_form": "허얼",
          "speaker_id": "2",
          "time": "20210826 00:34"
        },
        {
          "id": "MDRW2100015703.1.8",
          "form": "진짜 잼난거만 쏙쏙 골라보시네요",
          "original_form": "진짜 잼난거만 쏙쏙 골라보시네요",
          "speaker_id": "2",
          "time": "20210826 00:34"
        },
        {
          "id": "MDRW2100015703.1.9",
          "form": "그 작품 재미있었던게 공녀 이런게 아니고 시녀잖아요 근데",
          "original_form": "그 작품 재미있었던게 공녀 이런게 아니고 시녀잖아요 근데",
          "speaker_id": "2",
          "time": "20210826 00:34"
        },
        {
          "id": "MDRW2100015703.1.10",
          "form": "공녀님(맞나?)을 모셨는데 그게 사실은 남주다라는점이",
          "original_form": "공녀님(맞나?)을 모셨는데 그게 사실은 남주다라는점이",
          "speaker_id": "2",
          "time": "20210826 00:35"
        },
        {
          "id": "MDRW2100015703.1.11",
          "form": "너무 재미있어어요",
          "original_form": "너무 재미있어어요",
          "speaker_id": "2",
          "time": "20210826 00:35"
        },
        {
          "id": "MDRW2100015703.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 맞아요 인생로판 있으세요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 맞아요 인생로판 있으세요?",
          "speaker_id": "1",
          "time": "20210826 00:35"
        },
        {
          "id": "MDRW2100015703.1.13",
          "form": "저는 ..어..",
          "original_form": "저는 ..어..",
          "speaker_id": "2",
          "time": "20210826 00:35"
        },
        {
          "id": "MDRW2100015703.1.14",
          "form": "그 남주의 엄마가 되었다ㅜ아시나요",
          "original_form": "그 남주의 엄마가 되었다ㅜ아시나요",
          "speaker_id": "2",
          "time": "20210826 00:35"
        },
        {
          "id": "MDRW2100015703.1.15",
          "form": "아니다",
          "original_form": "아니다",
          "speaker_id": "2",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100015703.1.16",
          "form": "괴물공작가의 계약공녀",
          "original_form": "괴물공작가의 계약공녀",
          "speaker_id": "2",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100015703.1.17",
          "form": "그거가 제일 잼써요",
          "original_form": "그거가 제일 잼써요",
          "speaker_id": "2",
          "time": "20210826 00:36"
        },
        {
          "id": "MDRW2100015703.1.18",
