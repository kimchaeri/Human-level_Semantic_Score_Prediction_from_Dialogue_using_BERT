      "utterance": [
        {
          "id": "MDRW2100029672.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210903 11:33"
        },
        {
          "id": "MDRW2100029672.1.2",
          "form": "네,안녕하세요~",
          "original_form": "네,안녕하세요~",
          "speaker_id": "2",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029672.1.3",
          "form": "이번 도쿄올림픽 보셨나요?",
          "original_form": "이번 도쿄올림픽 보셨나요?",
          "speaker_id": "1",
          "time": "20210903 11:35"
        },
        {
          "id": "MDRW2100029672.1.4",
          "form": "네~배구,양궁만봤어요~",
          "original_form": "네~배구,양궁만봤어요~",
          "speaker_id": "2",
          "time": "20210903 11:35"
        },
        {
          "id": "MDRW2100029672.1.5",
          "form": "배구 재밌었죠! 저는 여자배구가 진짜",
          "original_form": "배구 재밌었죠! 저는 여자배구가 진짜",
          "speaker_id": "1",
          "time": "20210903 11:36"
        },
        {
          "id": "MDRW2100029672.1.6",
          "form": "대박이었다고 생각해요",
          "original_form": "대박이었다고 생각해요",
          "speaker_id": "1",
          "time": "20210903 11:36"
        },
        {
          "id": "MDRW2100029672.1.7",
          "form": "맞아요 특히 김연경선수 대단해요",
          "original_form": "맞아요 특히 김연경선수 대단해요",
          "speaker_id": "2",
          "time": "20210903 11:36"
        },
        {
          "id": "MDRW2100029672.1.8",
          "form": "맞아요 희진 선수도 부상 입으신 채로 활약하셨고..",
          "original_form": "맞아요 희진 선수도 부상 입으신 채로 활약하셨고..",
          "speaker_id": "1",
          "time": "20210903 11:38"
        },
        {
          "id": "MDRW2100029672.1.9",
          "form": "한일전 때가 제일 간절했죠ㅠㅠㅠ",
          "original_form": "한일전 때가 제일 간절했죠ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 11:38"
        },
        {
          "id": "MDRW2100029672.1.10",
          "form": "맞아요 보면서 응원 제일했던거같아요",
          "original_form": "맞아요 보면서 응원 제일했던거같아요",
          "speaker_id": "2",
          "time": "20210903 13:16"
        },
        {
          "id": "MDRW2100029672.1.11",
          "form": "브라질선수들도 진짜 대단하더라구요",
          "original_form": "브라질선수들도 진짜 대단하더라구요",
          "speaker_id": "1",
          "time": "20210903 17:11"
        },
        {
          "id": "MDRW2100029672.1.12",
          "form": "그렇죠 강적이고 진짜  보면서도 감탄나오더라고요",
          "original_form": "그렇죠 강적이고 진짜  보면서도 감탄나오더라고요",
          "speaker_id": "2",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100029672.1.13",
          "form": "다들 키도 엄청 커서 부럽더라고요ㅠㅋㅋㅋㅋ",
          "original_form": "다들 키도 엄청 커서 부럽더라고요ㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100029672.1.14",
          "form": "그쵸 배구도 키가 커야되나봐요~",
          "original_form": "그쵸 배구도 키가 커야되나봐요~",
          "speaker_id": "2",
          "time": "20210904 18:34"
        },
        {
          "id": "MDRW2100029672.1.15",
          "form": "여자배구 선수들 이번에 예능 많이 출연하신대서",
          "original_form": "여자배구 선수들 이번에 예능 많이 출연하신대서",
          "speaker_id": "1",
          "time": "20210904 21:22"
        },
        {
          "id": "MDRW2100029672.1.16",
          "form": "엄청 기대돼요 ㅎㅎ",
          "original_form": "엄청 기대돼요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 21:22"
        },
        {
          "id": "MDRW2100029672.1.17",
          "form": "오~ 주로 김연경선수 출연만 봤는데 기대되네요",
          "original_form": "오~ 주로 김연경선수 출연만 봤는데 기대되네요",
          "speaker_id": "2",
