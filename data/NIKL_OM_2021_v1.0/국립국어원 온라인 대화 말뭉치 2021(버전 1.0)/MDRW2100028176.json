      "utterance": [
        {
          "id": "MDRW2100028176.1.1",
          "form": "아 적당한 와사비가 생명인데~",
          "original_form": "아 적당한 와사비가 생명인데~",
          "speaker_id": "2",
          "time": "20210902 18:36"
        },
        {
          "id": "MDRW2100028176.1.2",
          "form": "와사비+마요네즈 조합 너무 좋아요",
          "original_form": "와사비+마요네즈 조합 너무 좋아요",
          "speaker_id": "2",
          "time": "20210902 18:36"
        },
        {
          "id": "MDRW2100028176.1.3",
          "form": "악 저 원래 와사비 별로안좋아했는데",
          "original_form": "악 저 원래 와사비 별로안좋아했는데",
          "speaker_id": "1",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100028176.1.4",
          "form": "오늘 먹다보니 중독되더라고요",
          "original_form": "오늘 먹다보니 중독되더라고요",
          "speaker_id": "1",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100028176.1.5",
          "form": "와사비랑 마요네즈섞으면 맛있나요?",
          "original_form": "와사비랑 마요네즈섞으면 맛있나요?",
          "speaker_id": "1",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100028176.1.6",
          "form": "네네! 완전",
          "original_form": "네네! 완전",
          "speaker_id": "2",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100028176.1.7",
          "form": "참치나 크래미랑 섞어서 김밥드셔보세요",
          "original_form": "참치나 크래미랑 섞어서 김밥드셔보세요",
          "speaker_id": "2",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100028176.1.8",
          "form": "악 생각만해도",
          "original_form": "악 생각만해도",
          "speaker_id": "1",
          "time": "20210902 18:39"
        },
        {
          "id": "MDRW2100028176.1.9",
          "form": "배고파요",
          "original_form": "배고파요",
          "speaker_id": "1",
          "time": "20210902 18:39"
        },
        {
          "id": "MDRW2100028176.1.10",
          "form": "참치+마요+와사비",
          "original_form": "참치+마요+와사비",
          "speaker_id": "1",
          "time": "20210902 18:39"
        },
        {
          "id": "MDRW2100028176.1.11",
          "form": "그쵸...저두요.. 저녁뭐먹지",
          "original_form": "그쵸...저두요.. 저녁뭐먹지",
          "speaker_id": "2",
          "time": "20210902 18:39"
        },
        {
          "id": "MDRW2100028176.1.12",
          "form": "뭐드실거에요 저녁?",
          "original_form": "뭐드실거에요 저녁?",
          "speaker_id": "2",
          "time": "20210902 18:39"
        },
        {
          "id": "MDRW2100028176.1.13",
          "form": "전 삼각김밥이랑 그릭요거트 먹었답니다!",
          "original_form": "전 삼각김밥이랑 그릭요거트 먹었답니다!",
          "speaker_id": "1",
          "time": "20210902 18:40"
        },
        {
          "id": "MDRW2100028176.1.14",
          "form": "이미 드셨군여! 그릭요거트 좋아하시나요",
          "original_form": "이미 드셨군여! 그릭요거트 좋아하시나요",
          "speaker_id": "2",
          "time": "20210902 18:40"
        },
        {
          "id": "MDRW2100028176.1.15",
          "form": "네네 오랜만에 먹었는데 맛있네요",
          "original_form": "네네 오랜만에 먹었는데 맛있네요",
          "speaker_id": "1",
          "time": "20210902 18:41"
        },
        {
          "id": "MDRW2100028176.1.16",
          "form": "!!!",
          "original_form": "!!!",
          "speaker_id": "1",
          "time": "20210902 18:41"
        },
        {
          "id": "MDRW2100028176.1.17",
          "form": "저도 요거트 안먹은 지 꽤 된거 같아요 오랜만에 사갈까봐요",
          "original_form": "저도 요거트 안먹은 지 꽤 된거 같아요 오랜만에 사갈까봐요",
          "speaker_id": "2",
          "time": "20210902 18:42"
        },
        {
          "id": "MDRW2100028176.1.18",
          "form": "요거트+씨리얼 조합도 좋지않나요",
          "original_form": "요거트+씨리얼 조합도 좋지않나요",
          "speaker_id": "2",
          "time": "20210902 18:43"
        },
        {
          "id": "MDRW2100028176.1.19",
          "form": "왕맛있죠",
          "original_form": "왕맛있죠",
          "speaker_id": "1",
          "time": "20210902 18:44"
        },
        {
          "id": "MDRW2100028176.1.20",
          "form": "전 그릭요거트에",
          "original_form": "전 그릭요거트에",
          "speaker_id": "1",
          "time": "20210902 18:44"
        },
        {
          "id": "MDRW2100028176.1.21",
          "form": "파인애플 샤인머스캣 코코넛청크 조합을 먹었답니다",
          "original_form": "파인애플 샤인머스캣 코코넛청크 조합을 먹었답니다",
          "speaker_id": "1",
          "time": "20210902 18:44"
        },
        {
          "id": "MDRW2100028176.1.22",
          "form": "와우 완전 호화로운데요!!! 플러스 삼각김밥이라니 약간 오묘...하지만?ㅋㅋㅋㅋㅋ",
          "original_form": "와우 완전 호화로운데요!!! 플러스 삼각김밥이라니 약간 오묘...하지만?ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:46"
        },
        {
          "id": "MDRW2100028176.1.23",
          "form": "뜬금없이 탄수화물 보충(?)했어요",
          "original_form": "뜬금없이 탄수화물 보충(?)했어요",
          "speaker_id": "1",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100028176.1.24",
          "form": "중요하죠 탄수화물 ㅋㅋㅋㅋ",
          "original_form": "중요하죠 탄수화물 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100028176.1.25",
          "form": "탄수화물 굉장히 좋아합니다",
          "original_form": "탄수화물 굉장히 좋아합니다",
          "speaker_id": "2",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100028176.1.26",
          "form": "저는 고기보다 탄수화물 더 좋아해요 ㅋㅋㅋ",
          "original_form": "저는 고기보다 탄수화물 더 좋아해요 ㅋㅋㅋ",
