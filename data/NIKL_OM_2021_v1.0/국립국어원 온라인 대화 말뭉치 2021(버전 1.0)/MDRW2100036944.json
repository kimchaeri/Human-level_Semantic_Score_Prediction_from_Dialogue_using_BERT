      "utterance": [
        {
          "id": "MDRW2100036944.1.1",
          "form": "name2님!",
          "original_form": "&name2&님!",
          "speaker_id": "1",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100036944.1.2",
          "form": "그 때 어떤 분과 이야기하시고나서",
          "original_form": "그 때 어떤 분과 이야기하시고나서",
          "speaker_id": "1",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100036944.1.3",
          "form": "비혼의 길을 걷겠다고 다짐하셨다고 했잖아요~",
          "original_form": "비혼의 길을 걷겠다고 다짐하셨다고 했잖아요~",
          "speaker_id": "1",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100036944.1.4",
          "form": "그 이야기가 궁금합니다 ",
          "original_form": "그 이야기가 궁금합니다 ",
          "speaker_id": "1",
          "time": "20210831 15:28"
        },
        {
          "id": "MDRW2100036944.1.5",
          "form": "아 깊은 이야기는 아니었고,",
          "original_form": "아 깊은 이야기는 아니었고,",
          "speaker_id": "2",
          "time": "20210831 15:30"
        },
        {
          "id": "MDRW2100036944.1.6",
          "form": "제가 지금 장거리 중인데 마음이 좀 식은 상태 같다 이런저런 얘기중이었거든요",
          "original_form": "제가 지금 장거리 중인데 마음이 좀 식은 상태 같다 이런저런 얘기중이었거든요",
          "speaker_id": "2",
          "time": "20210831 15:31"
        },
        {
          "id": "MDRW2100036944.1.7",
          "form": "얘기하다보니 점점 마음 상태가 정리가 되고, 의미 없이 시간 보내고 쓰는 것 보다",
          "original_form": "얘기하다보니 점점 마음 상태가 정리가 되고, 의미 없이 시간 보내고 쓰는 것 보다",
          "speaker_id": "2",
          "time": "20210831 15:31"
        },
        {
          "id": "MDRW2100036944.1.8",
          "form": "네네네!",
          "original_form": "네네네!",
          "speaker_id": "1",
          "time": "20210831 15:31"
        },
        {
          "id": "MDRW2100036944.1.9",
          "form": "이 대화는 다른 제 3자와 하신거죠?",
          "original_form": "이 대화는 다른 제 3자와 하신거죠?",
          "speaker_id": "1",
          "time": "20210831 15:32"
        },
        {
          "id": "MDRW2100036944.1.10",
          "form": "나 자신에게 더 시간을 쓰고, 집중해보고싶다 싶어서",
          "original_form": "나 자신에게 더 시간을 쓰고, 집중해보고싶다 싶어서",
          "speaker_id": "2",
          "time": "20210831 15:32"
        },
        {
          "id": "MDRW2100036944.1.11",
          "form": "그쵸! 알고보니 비혼이었던 분이었어요 ㅎㅎ",
          "original_form": "그쵸! 알고보니 비혼이었던 분이었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 15:32"
        },
        {
          "id": "MDRW2100036944.1.12",
          "form": "오오 그러셨군요!!!!",
          "original_form": "오오 그러셨군요!!!!",
          "speaker_id": "1",
          "time": "20210831 15:32"
        },
        {
          "id": "MDRW2100036944.1.13",
          "form": "그래도 장거리 연애가 가능하다는 것 자체가",
          "original_form": "그래도 장거리 연애가 가능하다는 것 자체가",
          "speaker_id": "1",
          "time": "20210831 15:33"
        },
        {
          "id": "MDRW2100036944.1.14",
          "form": "name2님과 상대 남자분이",
          "original_form": "&name2&님과 상대 남자분이",
          "speaker_id": "1",
          "time": "20210831 15:33"
        },
        {
          "id": "MDRW2100036944.1.15",
          "form": "잔잔한 성격이어서 가능한 것 아닐까요?!",
          "original_form": "잔잔한 성격이어서 가능한 것 아닐까요?!",
          "speaker_id": "1",
          "time": "20210831 15:33"
        },
        {
          "id": "MDRW2100036944.1.16",
          "form": "결혼이란 제도가 뭔가",
          "original_form": "결혼이란 제도가 뭔가",
          "speaker_id": "1",
          "time": "20210831 15:33"
        },
        {
          "id": "MDRW2100036944.1.17",
          "form": "삶을 고리타분하게 만드는 것 같아요 ㅋㅋㅋㅋ ㅠㅠ",
          "original_form": "삶을 고리타분하게 만드는 것 같아요 ㅋㅋㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 15:33"
        },
        {
          "id": "MDRW2100036944.1.18",
          "form": "저는 잔잔한데... 만나도 그만, 안만나도 그만인데... 남자쪽은 만나러갈게, 왜 안만나러 와, 보러 오는 그 시간이 아깝냐 이런 쪽이라... 저는 많이 귀찮아졌습니다...ㅋㅋㅋㅋ",
          "original_form": "저는 잔잔한데... 만나도 그만, 안만나도 그만인데... 남자쪽은 만나러갈게, 왜 안만나러 와, 보러 오는 그 시간이 아깝냐 이런 쪽이라... 저는 많이 귀찮아졌습니다...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:34"
        },
        {
          "id": "MDRW2100036944.1.19",
          "form": "그래서 얼굴 보고 헤어질 그날을 기다리고 있어요 ㅋㅋㅋㅋㅋ",
          "original_form": "그래서 얼굴 보고 헤어질 그날을 기다리고 있어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:35"
        },
        {
          "id": "MDRW2100036944.1.20",
          "form": "헉 그렇군요....",
          "original_form": "헉 그렇군요....",
          "speaker_id": "1",
          "time": "20210831 21:20"
        },
        {
          "id": "MDRW2100036944.1.21",
          "form": "name2님 고생하고 계시네요...",
          "original_form": "&name2&님 고생하고 계시네요...",
          "speaker_id": "1",
          "time": "20210831 21:21"
        },
        {
          "id": "MDRW2100036944.1.22",
          "form": "만나러 가는 길이 버거울 때",
          "original_form": "만나러 가는 길이 버거울 때",
          "speaker_id": "1",
          "time": "20210831 21:21"
        },
        {
          "id": "MDRW2100036944.1.23",
          "form": "인연이 다한 것 같아요",
          "original_form": "인연이 다한 것 같아요",
          "speaker_id": "1",
          "time": "20210831 21:21"
        },
        {
          "id": "MDRW2100036944.1.24",
          "form": "게다가 장거리는 더더욱...",
          "original_form": "게다가 장거리는 더더욱...",
          "speaker_id": "1",
          "time": "20210831 21:21"
        },
        {
          "id": "MDRW2100036944.1.25",
          "form": "잘 지내던 커플도 힘들게 하는 게 장거리인데ㅠㅠ",
          "original_form": "잘 지내던 커플도 힘들게 하는 게 장거리인데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 21:21"
        },
        {
          "id": "MDRW2100036944.1.26",
          "form": "일찍 정리하게 돼서 다행이에요 ㅎㅎ",
          "original_form": "일찍 정리하게 돼서 다행이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 21:28"
        },
        {
          "id": "MDRW2100036944.1.27",
          "form": "시원섭섭할 줄 알았는데 생각정리 끝나니까 시원하고 후련하기만해요 ㅋㅋㅋㅋ",
          "original_form": "시원섭섭할 줄 알았는데 생각정리 끝나니까 시원하고 후련하기만해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:28"
        },
        {
          "id": "MDRW2100036944.1.28",
          "form": "그쵸...",
          "original_form": "그쵸...",
          "speaker_id": "1",
          "time": "20210831 21:31"
        },
        {
          "id": "MDRW2100036944.1.29",
          "form": "정말 다행입니다",
          "original_form": "정말 다행입니다",
          "speaker_id": "1",
          "time": "20210831 21:31"
        },
        {
          "id": "MDRW2100036944.1.30",
          "form": "스스로를 위해 좋은 선택을 하실거예요",
          "original_form": "스스로를 위해 좋은 선택을 하실거예요",
          "speaker_id": "1",
          "time": "20210831 21:31"
        },
        {
          "id": "MDRW2100036944.1.31",
          "form": "저는 전에 만났던 남자가 가성비를 너무 따졌어서",
          "original_form": "저는 전에 만났던 남자가 가성비를 너무 따졌어서",
          "speaker_id": "1",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100036944.1.32",
          "form": "아...지금 생각만 해도 스트레스...",
          "original_form": "아...지금 생각만 해도 스트레스...",
          "speaker_id": "1",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100036944.1.33",
          "form": "아.. 가성비에 목숨 거는 사람 진짜 피곤한데ㅠㅠ",
          "original_form": "아.. 가성비에 목숨 거는 사람 진짜 피곤한데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 21:35"
        },
        {
          "id": "MDRW2100036944.1.34",
          "form": "낭비 없는 건 좋지만, 또 너무 집착하고 그러면 옆에 있는 사람이 너무 힘들잖아요ㅠㅠ",
          "original_form": "낭비 없는 건 좋지만, 또 너무 집착하고 그러면 옆에 있는 사람이 너무 힘들잖아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100036944.1.35",
          "form": "맞아요....!!!!",
          "original_form": "맞아요....!!!!",
          "speaker_id": "1",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100036944.1.36",
          "form": "그리고 연인이라면 모름지기",
          "original_form": "그리고 연인이라면 모름지기",
          "speaker_id": "1",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100036944.1.37",
          "form": "분위기가 제일 좋아야하는거 아닐까요 ",
          "original_form": "분위기가 제일 좋아야하는거 아닐까요 ",
          "speaker_id": "1",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100036944.1.38",
          "form": "맞아요 ㅋㅋㅋㅋ 가성비에게 분위기는 사치라서 열심히 산통 깨고 그러겠죠?",
          "original_form": "맞아요 ㅋㅋㅋㅋ 가성비에게 분위기는 사치라서 열심히 산통 깨고 그러겠죠?",
          "speaker_id": "2",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100036944.1.39",
          "form": "그런 사람은 그냥 똑같은 사람 만나는게 제일 행복할 것 같아요!",
          "original_form": "그런 사람은 그냥 똑같은 사람 만나는게 제일 행복할 것 같아요!",
          "speaker_id": "2",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100036944.1.40",
          "form": "잘 보내주셨어요!",
          "original_form": "잘 보내주셨어요!",
          "speaker_id": "2",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100036944.1.41",
          "form": "맞아요 맞아요 헤헤",
          "original_form": "맞아요 맞아요 헤헤",
          "speaker_id": "1",
          "time": "20210901 01:11"
        },
        {
          "id": "MDRW2100036944.1.42",
          "form": "name2님께서 그렇게 말씀해주시니",
          "original_form": "&name2&님께서 그렇게 말씀해주시니",
          "speaker_id": "1",
          "time": "20210901 01:11"
        },
        {
          "id": "MDRW2100036944.1.43",
          "form": "안그래도 없는 미련이 더 사라졌어요!",
          "original_form": "안그래도 없는 미련이 더 사라졌어요!",
          "speaker_id": "1",
          "time": "20210901 01:11"
        }
