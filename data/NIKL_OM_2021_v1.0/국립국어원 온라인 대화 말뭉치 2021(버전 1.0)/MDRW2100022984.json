      "utterance": [
        {
          "id": "MDRW2100022984.1.1",
          "form": "자취 요리는 name1이 하신다고 했죠?",
          "original_form": "자취 요리는 &name1&이 하신다고 했죠?",
          "speaker_id": "2",
          "time": "20210829 23:42"
        },
        {
          "id": "MDRW2100022984.1.2",
          "form": "잘하시는 요리 어떤 거 있나요?",
          "original_form": "잘하시는 요리 어떤 거 있나요?",
          "speaker_id": "2",
          "time": "20210829 23:43"
        },
        {
          "id": "MDRW2100022984.1.3",
          "form": "저는 자취용만 하긴하는데요",
          "original_form": "저는 자취용만 하긴하는데요",
          "speaker_id": "1",
          "time": "20210829 23:43"
        },
        {
          "id": "MDRW2100022984.1.4",
          "form": "닭도리탕 ,콩불, 된장찌개 부대찌개이런거 자주해요",
          "original_form": "닭도리탕 ,콩불, 된장찌개 부대찌개이런거 자주해요",
          "speaker_id": "1",
          "time": "20210829 23:44"
        },
        {
          "id": "MDRW2100022984.1.5",
          "form": "와 대단하신데요...",
          "original_form": "와 대단하신데요...",
          "speaker_id": "2",
          "time": "20210829 23:50"
        },
        {
          "id": "MDRW2100022984.1.6",
          "form": "요리 잘하시는 분들 보면 대단한 것 같앙ㆍ",
          "original_form": "요리 잘하시는 분들 보면 대단한 것 같앙ㆍ",
          "speaker_id": "2",
          "time": "20210829 23:50"
        },
        {
          "id": "MDRW2100022984.1.7",
          "form": "같아요",
          "original_form": "같아요",
          "speaker_id": "2",
          "time": "20210829 23:50"
        },
        {
          "id": "MDRW2100022984.1.8",
          "form": "백종원님만 믿으세요",
          "original_form": "백종원님만 믿으세요",
          "speaker_id": "1",
          "time": "20210829 23:52"
        },
        {
          "id": "MDRW2100022984.1.9",
          "form": "광명이 있을 것입니다...",
          "original_form": "광명이 있을 것입니다...",
          "speaker_id": "1",
          "time": "20210829 23:52"
        },
        {
          "id": "MDRW2100022984.1.10",
          "form": "백종원님은 신이군요..",
          "original_form": "백종원님은 신이군요..",
          "speaker_id": "2",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100022984.1.11",
          "form": "네 그분은 많은 자취생들을 구원하셨어요",
          "original_form": "네 그분은 많은 자취생들을 구원하셨어요",
          "speaker_id": "1",
          "time": "20210829 23:57"
        },
        {
          "id": "MDRW2100022984.1.12",
          "form": "요리방법만 알려주셔서",
          "original_form": "요리방법만 알려주셔서",
          "speaker_id": "1",
          "time": "20210829 23:59"
        },
        {
          "id": "MDRW2100022984.1.13",
          "form": "식재료관리가 아직 미숙해요",
          "original_form": "식재료관리가 아직 미숙해요",
          "speaker_id": "1",
          "time": "20210829 23:59"
        },
        {
          "id": "MDRW2100022984.1.14",
          "form": "저도 예전에 백종원님 감자 짜글이 해봤는데",
          "original_form": "저도 예전에 백종원님 감자 짜글이 해봤는데",
          "speaker_id": "2",
          "time": "20210830 00:17"
        },
        {
          "id": "MDRW2100022984.1.15",
          "form": "그분 덕분에 새로운 맛을 알게 되는구나..깨달았어요",
          "original_form": "그분 덕분에 새로운 맛을 알게 되는구나..깨달았어요",
          "speaker_id": "2",
          "time": "20210830 00:17"
        },
        {
          "id": "MDRW2100022984.1.16",
          "form": "짜글이 맛있죠 한식류는 정말 밥한끼 뚝딱인거 같아요",
          "original_form": "짜글이 맛있죠 한식류는 정말 밥한끼 뚝딱인거 같아요",
          "speaker_id": "1",
          "time": "20210830 08:01"
        },
        {
          "id": "MDRW2100022984.1.17",
          "form": "네네ㅠㅠㅠ완전",
          "original_form": "네네ㅠㅠㅠ완전",
          "speaker_id": "2",
          "time": "20210830 13:46"
        },
        {
          "id": "MDRW2100022984.1.18",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "2",
          "time": "20210830 13:46"
        },
        {
          "id": "MDRW2100022984.1.19",
          "form": "네 샌드위치먹었어요",
          "original_form": "네 샌드위치먹었어요",
          "speaker_id": "1",
          "time": "20210830 13:47"
        },
        {
          "id": "MDRW2100022984.1.20",
          "form": "점심드셨나요?",
          "original_form": "점심드셨나요?",
          "speaker_id": "1",
          "time": "20210830 13:47"
        },
        {
          "id": "MDRW2100022984.1.21",
          "form": "네 순두부 찌개 먹었습니다!",
          "original_form": "네 순두부 찌개 먹었습니다!",
          "speaker_id": "2",
          "time": "20210830 13:55"
        },
        {
          "id": "MDRW2100022984.1.22",
          "form": "든든하게 드셨네요!",
          "original_form": "든든하게 드셨네요!",
          "speaker_id": "1",
          "time": "20210830 13:56"
        },
        {
          "id": "MDRW2100022984.1.23",
          "form": "네 어제 밥을 못 먹어서 오늘 밥 좀 먹었어요ㅎㅎ",
          "original_form": "네 어제 밥을 못 먹어서 오늘 밥 좀 먹었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 14:43"
        },
        {
          "id": "MDRW2100022984.1.24",
          "form": "찌개류 오랜만에 먹었는데 든든하고 좋네요",
          "original_form": "찌개류 오랜만에 먹었는데 든든하고 좋네요",
