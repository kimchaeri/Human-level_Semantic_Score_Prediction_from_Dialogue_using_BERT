        {
          "id": "MDRW2100020604.1.1",
          "form": "최근에 할인받아서 산 물건 있어요?",
          "original_form": "최근에 할인받아서 산 물건 있어요?",
          "speaker_id": "1",
          "time": "20210828 10:21"
        },
        {
          "id": "MDRW2100020604.1.2",
          "form": "없음 말고요 ㅎ",
          "original_form": "없음 말고요 ㅎ",
          "speaker_id": "1",
          "time": "20210829 10:56"
        },
        {
          "id": "MDRW2100020604.1.3",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210829 10:56"
        },
        {
          "id": "MDRW2100020604.1.4",
          "form": "죄송해요",
          "original_form": "죄송해요",
          "speaker_id": "2",
          "time": "20210829 10:56"
        },
        {
          "id": "MDRW2100020604.1.5",
          "form": "할인 받아서라..",
          "original_form": "할인 받아서라..",
          "speaker_id": "2",
          "time": "20210829 10:59"
        },
        {
          "id": "MDRW2100020604.1.6",
          "form": "음... 샴푸? 샀어요",
          "original_form": "음... 샴푸? 샀어요",
          "speaker_id": "2",
          "time": "20210829 10:59"
        },
        {
          "id": "MDRW2100020604.1.7",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210829 10:59"
        },
        {
          "id": "MDRW2100020604.1.8",
          "form": "엇 ㅎㅎ 혹시 쿤달꺼 사셨나요?",
          "original_form": "엇 ㅎㅎ 혹시 쿤달꺼 사셨나요?",
          "speaker_id": "1",
          "time": "20210829 11:00"
        },
        {
          "id": "MDRW2100020604.1.9",
          "form": "어.. 아니요ㅋㅋㅋㅋㅋ",
          "original_form": "어.. 아니요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:00"
        },
        {
          "id": "MDRW2100020604.1.10",
          "form": "그 무슨 행사한대서 탈모샴푸샀는데",
          "original_form": "그 무슨 행사한대서 탈모샴푸샀는데",
          "speaker_id": "2",
          "time": "20210829 11:00"
        },
        {
          "id": "MDRW2100020604.1.11",
          "form": "아버지 드리려구요",
          "original_form": "아버지 드리려구요",
          "speaker_id": "2",
          "time": "20210829 11:01"
        },
        {
          "id": "MDRW2100020604.1.12",
          "form": "쿤달꺼 좋은가요",
          "original_form": "쿤달꺼 좋은가요",
          "speaker_id": "2",
          "time": "20210829 11:01"
        },
        {
          "id": "MDRW2100020604.1.13",
          "form": "아~~ 최근에 쿤달에서 할인행사 했거든요",
          "original_form": "아~~ 최근에 쿤달에서 할인행사 했거든요",
          "speaker_id": "1",
          "time": "20210829 11:06"
        },
        {
          "id": "MDRW2100020604.1.14",
          "form": "네고왕 아세요?ㅋㅋ",
          "original_form": "네고왕 아세요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:06"
        },
        {
          "id": "MDRW2100020604.1.15",
          "form": "네 네고왕 알아요",
          "original_form": "네 네고왕 알아요",
          "speaker_id": "2",
          "time": "20210829 11:06"
        },
        {
          "id": "MDRW2100020604.1.16",
          "form": "네고왕을 했엏군요",
          "original_form": "네고왕을 했엏군요",
          "speaker_id": "2",
          "time": "20210829 11:06"
        },
        {
          "id": "MDRW2100020604.1.17",
          "form": "네 ㅎㅎ 저 네고왕행사로 샀어요 쿤달",
          "original_form": "네 ㅎㅎ 저 네고왕행사로 샀어요 쿤달",
          "speaker_id": "1",
          "time": "20210829 11:08"
        },
        {
          "id": "MDRW2100020604.1.18",
          "form": "어떤거 사셨어요",
          "original_form": "어떤거 사셨어요",
          "speaker_id": "2",
          "time": "20210829 11:08"
        },
        {
          "id": "MDRW2100020604.1.19",
          "form": "근데 쿨샴푸 쓸 때마다 눈이 따가워요 ㅜㅜ",
          "original_form": "근데 쿨샴푸 쓸 때마다 눈이 따가워요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 11:08"
        },
        {
          "id": "MDRW2100020604.1.20",
          "form": "아 쿨샴푸",
          "original_form": "아 쿨샴푸",
          "speaker_id": "2",
          "time": "20210829 11:08"
        },
        {
          "id": "MDRW2100020604.1.21",
          "form": "쿨샴푸 처음 써보는데 원래 이래요?",
          "original_form": "쿨샴푸 처음 써보는데 원래 이래요?",
          "speaker_id": "1",
          "time": "20210829 11:08"
        },
        {
          "id": "MDRW2100020604.1.22",
          "form": "화~한 느낌이 있으시구나",
          "original_form": "화~한 느낌이 있으시구나",
          "speaker_id": "2",
          "time": "20210829 11:08"
        },
        {
          "id": "MDRW2100020604.1.23",
          "form": "어 딱 그 느낌이였어요 ㅋㅋ",
          "original_form": "어 딱 그 느낌이였어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:08"
        },
        {
          "id": "MDRW2100020604.1.24",
          "form": "좀아무래도 박하가 첨가되어이써서",
          "original_form": "좀아무래도 박하가 첨가되어이써서",
