      "utterance": [
        {
          "id": "MDRW2100043546.1.1",
          "form": "스트링 치즈 좋아하세요?",
          "original_form": "스트링 치즈 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043546.1.2",
          "form": "저 엄청 좋아해요",
          "original_form": "저 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043546.1.3",
          "form": "어떤방식으로 주로 드세요?",
          "original_form": "어떤방식으로 주로 드세요?",
          "speaker_id": "1",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043546.1.4",
          "form": "그냥 렌지에 돌려서 찢어먹어도 맛있구요",
          "original_form": "그냥 렌지에 돌려서 찢어먹어도 맛있구요",
          "speaker_id": "2",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043546.1.5",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043546.1.6",
          "form": "계란말이 안에 넣으면 진짜 맛있어요",
          "original_form": "계란말이 안에 넣으면 진짜 맛있어요",
          "speaker_id": "2",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043546.1.7",
          "form": "오 피자치즈 처럼 늘어나나요?",
          "original_form": "오 피자치즈 처럼 늘어나나요?",
          "speaker_id": "1",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043546.1.8",
          "form": "네 근데 생긴게 동그랗고 길쭉해서 계란말이에 넣기 딱이에요",
          "original_form": "네 근데 생긴게 동그랗고 길쭉해서 계란말이에 넣기 딱이에요",
          "speaker_id": "2",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100043546.1.9",
          "form": "헉 그렇겠어요 계란말이 진짜 좋아하는데..",
          "original_form": "헉 그렇겠어요 계란말이 진짜 좋아하는데..",
          "speaker_id": "1",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100043546.1.10",
          "form": "그리고 저는 볶음밥에도  찢어서 올려먹기도 해요",
          "original_form": "그리고 저는 볶음밥에도  찢어서 올려먹기도 해요",
          "speaker_id": "2",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100043546.1.11",
          "form": "치즈 엄청 좋아하시나봐요ㅠ",
          "original_form": "치즈 엄청 좋아하시나봐요ㅠ",
          "speaker_id": "1",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100043546.1.12",
          "form": "네 치즈 너무 맛있어요",
          "original_form": "네 치즈 너무 맛있어요",
          "speaker_id": "2",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100043546.1.13",
          "form": "스트링치즈 계란말이 저도 해봐야겠어요 궁금",
          "original_form": "스트링치즈 계란말이 저도 해봐야겠어요 궁금",
