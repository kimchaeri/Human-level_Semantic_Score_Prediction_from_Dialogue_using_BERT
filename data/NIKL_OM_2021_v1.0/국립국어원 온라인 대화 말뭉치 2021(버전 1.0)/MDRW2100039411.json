      "utterance": [
        {
          "id": "MDRW2100039411.1.1",
          "form": "요즘 몸을 보면 탄력이 붙은게",
          "original_form": "요즘 몸을 보면 탄력이 붙은게",
          "speaker_id": "2",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100039411.1.2",
          "form": "보여서 기분이좋아",
          "original_form": "보여서 기분이좋아",
          "speaker_id": "2",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100039411.1.3",
          "form": "나도 오늘 하체건강 챙기려고 계단 오르기 했는데",
          "original_form": "나도 오늘 하체건강 챙기려고 계단 오르기 했는데",
          "speaker_id": "1",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100039411.1.4",
          "form": "예전엔 좀 둔해보이고 날렵해보이지 않았는데",
          "original_form": "예전엔 좀 둔해보이고 날렵해보이지 않았는데",
          "speaker_id": "2",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100039411.1.5",
          "form": "계단오르기 어때",
          "original_form": "계단오르기 어때",
          "speaker_id": "2",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100039411.1.6",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100039411.1.7",
          "form": "첫 날에는 한 번 오르는 것도 숨차서 골골댔는데",
          "original_form": "첫 날에는 한 번 오르는 것도 숨차서 골골댔는데",
          "speaker_id": "1",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039411.1.8",
          "form": "하체근육 쓰는 운동을 많이하는게 엄청중요하대",
          "original_form": "하체근육 쓰는 운동을 많이하는게 엄청중요하대",
          "speaker_id": "2",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039411.1.9",
          "form": "오늘은 도합 32층 올랐는데 버틸만 하더라",
          "original_form": "오늘은 도합 32층 올랐는데 버틸만 하더라",
          "speaker_id": "1",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039411.1.10",
          "form": "와 상상도 못하겠어",
          "original_form": "와 상상도 못하겠어",
          "speaker_id": "2",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039411.1.11",
          "form": "맞아 그 얘기 들은 것 같아",
          "original_form": "맞아 그 얘기 들은 것 같아",
          "speaker_id": "1",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100039411.1.12",
          "form": "32층을 어떻게 올라가지",
          "original_form": "32층을 어떻게 올라가지",
          "speaker_id": "2",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100039411.1.13",
          "form": "허벅지근육이 제2의 심장건강이라고 ㅋㅋ",
          "original_form": "허벅지근육이 제2의 심장건강이라고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100039411.1.14",
          "form": "내려올땐 엘베로 내려오니",
          "original_form": "내려올땐 엘베로 내려오니",
          "speaker_id": "2",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100039411.1.15",
          "form": "하체의 지방은 용서가 된다는 말이 있을 정도로",
          "original_form": "하체의 지방은 용서가 된다는 말이 있을 정도로",
          "speaker_id": "2",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100039411.1.16",
          "form": "응 근육 없는 상태에서 내려가면 무릎관절 뽀사질까봐 엘베 타",
          "original_form": "응 근육 없는 상태에서 내려가면 무릎관절 뽀사질까봐 엘베 타",
          "speaker_id": "1",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100039411.1.17",
          "form": "하체가 두꺼운게 몸에 좋아",
          "original_form": "하체가 두꺼운게 몸에 좋아",
          "speaker_id": "2",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100039411.1.18",
          "form": "노래 들으면서 계단올라가니",
          "original_form": "노래 들으면서 계단올라가니",
          "speaker_id": "2",
          "time": "20210903 17:23"
        }
      ]
