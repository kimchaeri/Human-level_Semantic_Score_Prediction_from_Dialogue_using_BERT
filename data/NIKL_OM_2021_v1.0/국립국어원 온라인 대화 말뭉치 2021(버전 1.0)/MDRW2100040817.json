      "utterance": [
        {
          "id": "MDRW2100040817.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210905 08:43"
        },
        {
          "id": "MDRW2100040817.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210905 08:48"
        },
        {
          "id": "MDRW2100040817.1.3",
          "form": "일하고계세요?",
          "original_form": "일하고계세요?",
          "speaker_id": "1",
          "time": "20210905 08:48"
        },
        {
          "id": "MDRW2100040817.1.4",
          "form": "5시간정도 알바해요!",
          "original_form": "5시간정도 알바해요!",
          "speaker_id": "2",
          "time": "20210905 08:49"
        },
        {
          "id": "MDRW2100040817.1.5",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210905 08:49"
        },
        {
          "id": "MDRW2100040817.1.6",
          "form": "어떤일하세요~?",
          "original_form": "어떤일하세요~?",
          "speaker_id": "1",
          "time": "20210905 08:49"
        },
        {
          "id": "MDRW2100040817.1.7",
          "form": "그냥 계산판매일이에요!",
          "original_form": "그냥 계산판매일이에요!",
          "speaker_id": "2",
          "time": "20210905 08:50"
        },
        {
          "id": "MDRW2100040817.1.8",
          "form": "일하시나요?",
          "original_form": "일하시나요?",
          "speaker_id": "2",
          "time": "20210905 08:50"
        },
        {
          "id": "MDRW2100040817.1.9",
          "form": "네~~",
          "original_form": "네~~",
          "speaker_id": "1",
          "time": "20210905 08:50"
        },
        {
          "id": "MDRW2100040817.1.10",
          "form": "재택하고싶습니다..",
          "original_form": "재택하고싶습니다..",
          "speaker_id": "1",
          "time": "20210905 08:50"
        },
        {
          "id": "MDRW2100040817.1.11",
          "form": "아ㅜㅠ~ 요새 많이 재택하던데",
          "original_form": "아ㅜㅠ~ 요새 많이 재택하던데",
          "speaker_id": "2",
          "time": "20210905 08:54"
        },
        {
          "id": "MDRW2100040817.1.12",
          "form": "그죠",
          "original_form": "그죠",
          "speaker_id": "1",
          "time": "20210905 08:55"
        },
        {
          "id": "MDRW2100040817.1.13",
          "form": "굳이 왜나가는지 모르겠어요 ㅋㅋㅋ",
          "original_form": "굳이 왜나가는지 모르겠어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:55"
        },
        {
          "id": "MDRW2100040817.1.14",
          "form": "집에서 할수있는일이신가봐요! 전 나가야하는지라 ㅜ",
          "original_form": "집에서 할수있는일이신가봐요! 전 나가야하는지라 ㅜ",
          "speaker_id": "2",
          "time": "20210905 09:02"
        },
        {
          "id": "MDRW2100040817.1.15",
          "form": "네네 글킨한데",
          "original_form": "네네 글킨한데",
          "speaker_id": "1",
          "time": "20210905 09:27"
        },
        {
          "id": "MDRW2100040817.1.16",
          "form": "나오라하면 나가긴해야죠 ㅜ",
          "original_form": "나오라하면 나가긴해야죠 ㅜ",
          "speaker_id": "1",
          "time": "20210905 09:27"
        },
        {
          "id": "MDRW2100040817.1.17",
          "form": "일은 일주일에 몇번 나가세요?",
          "original_form": "일은 일주일에 몇번 나가세요?",
          "speaker_id": "1",
          "time": "20210905 09:28"
        },
        {
          "id": "MDRW2100040817.1.18",
          "form": "평일에 5일이요!",
          "original_form": "평일에 5일이요!",
          "speaker_id": "2",
          "time": "20210905 10:24"
        },
        {
          "id": "MDRW2100040817.1.19",
          "form": "주말이 무지 소중하죠..",
          "original_form": "주말이 무지 소중하죠..",
          "speaker_id": "1",
          "time": "20210905 10:25"
        },
        {
          "id": "MDRW2100040817.1.20",
          "form": "누가 주4일제 도입좀ㅋㅋㅋ",
          "original_form": "누가 주4일제 도입좀ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:25"
        },
        {
          "id": "MDRW2100040817.1.21",
          "form": "그래두 ㅎㅎ공휴일없엇는데 대체공휴일다 생겼자나요!",
          "original_form": "그래두 ㅎㅎ공휴일없엇는데 대체공휴일다 생겼자나요!",
          "speaker_id": "2",
          "time": "20210905 10:26"
        },
        {
          "id": "MDRW2100040817.1.22",
          "form": "ㅋㄱㅋ다행이에요",
          "original_form": "ㅋㄱㅋ다행이에요",
          "speaker_id": "1",
          "time": "20210905 10:28"
        },
        {
          "id": "MDRW2100040817.1.23",
          "form": "지금 근무하시는곳 동료분은 괜찮으세요?",
          "original_form": "지금 근무하시는곳 동료분은 괜찮으세요?",
          "speaker_id": "1",
          "time": "20210905 10:29"
        },
        {
          "id": "MDRW2100040817.1.24",
          "form": "네네! 저 마감타임에 일하는데 혼자일해요 ㅎ 사장님이랑 교대해요 ㅎ",
          "original_form": "네네! 저 마감타임에 일하는데 혼자일해요 ㅎ 사장님이랑 교대해요 ㅎ",
          "speaker_id": "2",
          "time": "20210905 10:56"
        },
        {
          "id": "MDRW2100040817.1.25",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210905 10:58"
        },
        {
          "id": "MDRW2100040817.1.26",
          "form": "일스트레스는없어요 ㅎㅎ",
          "original_form": "일스트레스는없어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 11:01"
        },
        {
          "id": "MDRW2100040817.1.27",
          "form": "혼자 일하는게 더 편하겠어요",
          "original_form": "혼자 일하는게 더 편하겠어요",
          "speaker_id": "1",
          "time": "20210905 11:01"
        },
        {
          "id": "MDRW2100040817.1.28",
          "form": "좋네요!!",
          "original_form": "좋네요!!",
          "speaker_id": "1",
          "time": "20210905 11:01"
        }
      ]
    }
