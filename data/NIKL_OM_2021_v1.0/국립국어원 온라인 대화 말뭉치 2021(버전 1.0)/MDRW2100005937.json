      "utterance": [
        {
          "id": "MDRW2100005937.1.1",
          "form": "결혼하면 어떤 삶을 꿈꾸세요?",
          "original_form": "결혼하면 어떤 삶을 꿈꾸세요?",
          "speaker_id": "1",
          "time": "20210907 16:56"
        },
        {
          "id": "MDRW2100005937.1.2",
          "form": "로망이 하나 있긴한데",
          "original_form": "로망이 하나 있긴한데",
          "speaker_id": "2",
          "time": "20210907 16:57"
        },
        {
          "id": "MDRW2100005937.1.3",
          "form": "방 하나를 피방처럼...컴 두개 해놓구",
          "original_form": "방 하나를 피방처럼...컴 두개 해놓구",
          "speaker_id": "2",
          "time": "20210907 16:57"
        },
        {
          "id": "MDRW2100005937.1.4",
          "form": "먹을것도 차려놓고",
          "original_form": "먹을것도 차려놓고",
          "speaker_id": "2",
          "time": "20210907 16:58"
        },
        {
          "id": "MDRW2100005937.1.5",
          "form": "같이 게임하는거??",
          "original_form": "같이 게임하는거??",
          "speaker_id": "2",
          "time": "20210907 16:58"
        },
