      "utterance": [
        {
          "id": "MDRW2100012063.1.1",
          "form": "취업중이신가요?",
          "original_form": "취업중이신가요?",
          "speaker_id": "1",
          "time": "20210824 18:49"
        },
        {
          "id": "MDRW2100012063.1.2",
          "form": "육아휴직중입니다",
          "original_form": "육아휴직중입니다",
          "speaker_id": "2",
          "time": "20210824 18:50"
        },
        {
          "id": "MDRW2100012063.1.3",
          "form": "저는 백수예요",
          "original_form": "저는 백수예요",
          "speaker_id": "1",
          "time": "20210824 19:02"
        },
        {
          "id": "MDRW2100012063.1.4",
          "form": "재무팀에서 일하다가",
          "original_form": "재무팀에서 일하다가",
          "speaker_id": "1",
          "time": "20210824 19:02"
        },
        {
          "id": "MDRW2100012063.1.5",
          "form": "야근때문에 힘들어서",
          "original_form": "야근때문에 힘들어서",
          "speaker_id": "1",
          "time": "20210824 19:02"
        },
        {
          "id": "MDRW2100012063.1.6",
          "form": "그만두고 쉬고있어요~",
          "original_form": "그만두고 쉬고있어요~",
          "speaker_id": "1",
          "time": "20210824 19:02"
        },
        {
          "id": "MDRW2100012063.1.7",
          "form": "그러시군요 야근하면 보통 몇시에 퇴근하시나요",
          "original_form": "그러시군요 야근하면 보통 몇시에 퇴근하시나요",
          "speaker_id": "2",
          "time": "20210824 19:03"
        },
        {
          "id": "MDRW2100012063.1.8",
          "form": "새벽에 퇴근할때도 많고",
          "original_form": "새벽에 퇴근할때도 많고",
          "speaker_id": "1",
          "time": "20210824 19:03"
        },
        {
          "id": "MDRW2100012063.1.9",
          "form": "거의 밤 늦은시간이죠",
          "original_form": "거의 밤 늦은시간이죠",
          "speaker_id": "1",
          "time": "20210824 19:04"
        },
        {
          "id": "MDRW2100012063.1.10",
          "form": "와..새벽이면 정말 고생이네요",
          "original_form": "와..새벽이면 정말 고생이네요",
          "speaker_id": "2",
          "time": "20210824 19:04"
        },
        {
          "id": "MDRW2100012063.1.11",
          "form": "직원중에 안아픈사람이 없었어요",
          "original_form": "직원중에 안아픈사람이 없었어요",
          "speaker_id": "1",
          "time": "20210824 19:05"
        },
        {
          "id": "MDRW2100012063.1.12",
          "form": "일이 많았나봐요 업무량이",
          "original_form": "일이 많았나봐요 업무량이",
          "speaker_id": "2",
          "time": "20210824 19:07"
        },
        {
          "id": "MDRW2100012063.1.13",
          "form": "엄청났어요",
          "original_form": "엄청났어요",
          "speaker_id": "1",
          "time": "20210824 19:07"
        },
        {
          "id": "MDRW2100012063.1.14",
          "form": "썸써미님은 어떤일하시나요?",
          "original_form": "썸써미님은 어떤일하시나요?",
          "speaker_id": "1",
          "time": "20210824 19:07"
        },
        {
          "id": "MDRW2100012063.1.15",
          "form": "저는 공무원이예요~",
          "original_form": "저는 공무원이예요~",
          "speaker_id": "2",
          "time": "20210824 19:08"
        },
        {
          "id": "MDRW2100012063.1.16",
          "form": "업무 스트레스 있으신가요?",
          "original_form": "업무 스트레스 있으신가요?",
          "speaker_id": "1",
          "time": "20210824 19:08"
        },
        {
          "id": "MDRW2100012063.1.17",
          "form": "네 사람들 상대하는게 힘들죠~ 옥돔님은요?",
          "original_form": "네 사람들 상대하는게 힘들죠~ 옥돔님은요?",
          "speaker_id": "2",
          "time": "20210824 19:09"
        },
        {
          "id": "MDRW2100012063.1.18",
          "form": "역시 어딜가나 사람이 문제네요",
          "original_form": "역시 어딜가나 사람이 문제네요",
          "speaker_id": "1",
          "time": "20210824 19:11"
        }
      ]
