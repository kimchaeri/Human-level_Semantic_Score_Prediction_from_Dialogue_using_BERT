      "utterance": [
        {
          "id": "MDRW2100014238.1.1",
          "form": "좋아하는 게임 있으신가요?",
          "original_form": "좋아하는 게임 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014238.1.2",
          "form": "전 게임은 안좋아하는데",
          "original_form": "전 게임은 안좋아하는데",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014238.1.3",
          "form": "스벅각 베라각만해요 ㅋㅋ",
