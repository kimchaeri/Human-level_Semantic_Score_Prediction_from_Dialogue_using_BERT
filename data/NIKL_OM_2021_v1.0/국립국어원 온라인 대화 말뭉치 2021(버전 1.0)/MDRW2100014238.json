          "id": "MDRW2100014238.1.5",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014238.1.6",
          "form": "저는 모바일 게임을 즐겨합니다.",
          "original_form": "저는 모바일 게임을 즐겨합니다.",
          "speaker_id": "1",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100014238.1.7",
          "form": "아그러세요",
          "original_form": "아그러세요",
          "speaker_id": "2",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100014238.1.8",
          "form": "베스킨라빈스 31 술게임도 재밌지요.",
          "original_form": "베스킨라빈스 31 술게임도 재밌지요.",
          "speaker_id": "1",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100014238.1.9",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100014238.1.10",
          "form": "저거 두개밖에 못해요",
          "original_form": "저거 두개밖에 못해요",
          "speaker_id": "2",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014238.1.11",
          "form": "저희딸보니 스타뷰밸리",
          "original_form": "저희딸보니 스타뷰밸리",
          "speaker_id": "2",
          "time": "20210825 17:57"
        },
        {
