      "utterance": [
        {
          "id": "MDRW2100033803.1.1",
          "form": "즐겨먹는 음식있으세요?",
          "original_form": "즐겨먹는 음식있으세요?",
          "speaker_id": "1",
          "time": "20210825 03:16"
        },
        {
          "id": "MDRW2100033803.1.2",
          "form": "닭가.슴살이 금지어네요ㅋㅋ",
          "original_form": "닭가.슴살이 금지어네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 03:17"
        },
        {
          "id": "MDRW2100033803.1.3",
          "form": "닭가.슴살이랑",
          "original_form": "닭가.슴살이랑",
          "speaker_id": "2",
          "time": "20210825 03:17"
        },
        {
          "id": "MDRW2100033803.1.4",
          "form": "채소 자주먹어요!",
          "original_form": "채소 자주먹어요!",
          "speaker_id": "2",
          "time": "20210825 03:18"
        },
        {
          "id": "MDRW2100033803.1.5",
          "form": "어떤 채소 좋아하시나요",
          "original_form": "어떤 채소 좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 03:18"
        },
        {
          "id": "MDRW2100033803.1.6",
          "form": "아스파라거스요",
          "original_form": "아스파라거스요",
          "speaker_id": "2",
          "time": "20210825 03:18"
        },
        {
          "id": "MDRW2100033803.1.7",
          "form": "아스파라거스요? 어.. 먹어본 적은 없는데 무슨 맛인가요?",
          "original_form": "아스파라거스요? 어.. 먹어본 적은 없는데 무슨 맛인가요?",
          "speaker_id": "1",
          "time": "20210825 03:18"
        },
        {
          "id": "MDRW2100033803.1.8",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 03:18"
        },
        {
          "id": "MDRW2100033803.1.9",
          "form": "소금 후추 뿌려서 구우면 맛있다던데",
          "original_form": "소금 후추 뿌려서 구우면 맛있다던데",
          "speaker_id": "1",
          "time": "20210825 03:19"
        },
        {
          "id": "MDRW2100033803.1.10",
          "form": "아삭아삭한 땅콩 맛이에요",
          "original_form": "아삭아삭한 땅콩 맛이에요",
          "speaker_id": "2",
          "time": "20210825 03:19"
        },
        {
          "id": "MDRW2100033803.1.11",
          "form": "아 땅콩맛이구나ㅎ 안먹어봐서 몰랐어요",
          "original_form": "아 땅콩맛이구나ㅎ 안먹어봐서 몰랐어요",
          "speaker_id": "1",
          "time": "20210825 03:19"
        },
        {
          "id": "MDRW2100033803.1.12",
          "form": "저는 어.. 고구마..?",
          "original_form": "저는 어.. 고구마..?",
          "speaker_id": "1",
          "time": "20210825 03:19"
        },
        {
          "id": "MDRW2100033803.1.13",
          "form": "아무래도 먹기편하고 그래서 선호해요",
          "original_form": "아무래도 먹기편하고 그래서 선호해요",
          "speaker_id": "1",
          "time": "20210825 03:19"
        },
        {
          "id": "MDRW2100033803.1.14",
          "form": "어떤 고구마 좋아하세요?",
          "original_form": "어떤 고구마 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 03:19"
        },
        {
          "id": "MDRW2100033803.1.15",
          "form": "저는 밤고구마요 name2님은요",
          "original_form": "저는 밤고구마요 &name2&님은요",
          "speaker_id": "1",
          "time": "20210825 03:20"
        },
        {
          "id": "MDRW2100033803.1.16",
          "form": "저는 호박고구마요!",
          "original_form": "저는 호박고구마요!",
          "speaker_id": "2",
          "time": "20210825 03:20"
        },
        {
          "id": "MDRW2100033803.1.17",
          "form": "껍질채로 드세요?",
          "original_form": "껍질채로 드세요?",
          "speaker_id": "2",
          "time": "20210825 03:20"
        },
        {
          "id": "MDRW2100033803.1.18",
          "form": "아니요 껍질은 안먹어요ㅎ",
          "original_form": "아니요 껍질은 안먹어요ㅎ",
          "speaker_id": "1",
