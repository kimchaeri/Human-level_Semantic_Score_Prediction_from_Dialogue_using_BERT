      "utterance": [
        {
          "id": "MDRW2100019845.1.1",
          "form": "뭐 키우세요?",
          "original_form": "뭐 키우세요?",
          "speaker_id": "2",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100019845.1.2",
          "form": "지금은 아무 것도 안 키우고 있지만 어릴 때는 새를 키웠어요",
          "original_form": "지금은 아무 것도 안 키우고 있지만 어릴 때는 새를 키웠어요",
          "speaker_id": "1",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019845.1.3",
          "form": "우와 새를 키우시다니! 그래서 새튜브를 보셨군여ㅋㅋ 어떤 새에요?",
          "original_form": "우와 새를 키우시다니! 그래서 새튜브를 보셨군여ㅋㅋ 어떤 새에요?",
          "speaker_id": "2",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019845.1.4",
          "form": "문조 키웠었어요. 흰색 백문조",
          "original_form": "문조 키웠었어요. 흰색 백문조",
          "speaker_id": "1",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100019845.1.5",
          "form": "어머 너무 귀여웠겠네요! 예전에 문조키우는 웹툰 봤었는데ㅋㅋㅋ",
          "original_form": "어머 너무 귀여웠겠네요! 예전에 문조키우는 웹툰 봤었는데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:38"
        },
        {
          "id": "MDRW2100019845.1.6",
          "form": "한마리만 키웠어요?",
          "original_form": "한마리만 키웠어요?",
          "speaker_id": "2",
          "time": "20210827 22:38"
        },
        {
          "id": "MDRW2100019845.1.7",
          "form": "암수 한 쌍이었는데 알도 낳고 그랬지만 부화까진 어렵더라구요",
          "original_form": "암수 한 쌍이었는데 알도 낳고 그랬지만 부화까진 어렵더라구요",
          "speaker_id": "1",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019845.1.8",
          "form": "아 어렵군요ㅠ 정 많이 드셨겠어요",
          "original_form": "아 어렵군요ㅠ 정 많이 드셨겠어요",
          "speaker_id": "2",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019845.1.9",
          "form": "네 그래서 또 못 키우겠더라구요ㅠ",
          "original_form": "네 그래서 또 못 키우겠더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210827 22:45"
        },
        {
          "id": "MDRW2100019845.1.10",
          "form": "동물 한 번도 키워보신 적 없으신가요?",
          "original_form": "동물 한 번도 키워보신 적 없으신가요?",
          "speaker_id": "1",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019845.1.11",
          "form": "네 저는 이게 키웠다 해도 되려나",
          "original_form": "네 저는 이게 키웠다 해도 되려나",
          "speaker_id": "2",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019845.1.12",
          "form": "운동장에서 달팽이 주워와서 길렀던 초딩이었는데",
          "original_form": "운동장에서 달팽이 주워와서 길렀던 초딩이었는데",
          "speaker_id": "2",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019845.1.13",
          "form": "나름 열심히 상추도 먹이고",
          "original_form": "나름 열심히 상추도 먹이고",
          "speaker_id": "2",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019845.1.14",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019845.1.15",
          "form": "엄청 늘어나지 않나요?ㅋㅋ",
          "original_form": "엄청 늘어나지 않나요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019845.1.16",
          "form": "한 마리?",
          "original_form": "한 마리?",
          "speaker_id": "1",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019845.1.17",
          "form": "다섯마리 주워왔어요",
          "original_form": "다섯마리 주워왔어요",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019845.1.18",
          "form": "근데 키우다가 다 도망가버렸어요",
          "original_form": "근데 키우다가 다 도망가버렸어요",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019845.1.19",
          "form": "ㅋㅋ 도망가다니 ㅠ",
          "original_form": "ㅋㅋ 도망가다니 ㅠ",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019845.1.20",
          "form": "그냥 친구네 강아지만 귀여워하면서 지내요 흑흑",
          "original_form": "그냥 친구네 강아지만 귀여워하면서 지내요 흑흑",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100019845.1.21",
          "form": "온라인 집사가 좋아요ㅋㅋ",
          "original_form": "온라인 집사가 좋아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:53"
        }
      ]
