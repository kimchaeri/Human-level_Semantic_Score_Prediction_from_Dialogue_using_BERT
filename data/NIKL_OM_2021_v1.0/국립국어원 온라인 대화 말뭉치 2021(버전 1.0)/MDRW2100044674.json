      "utterance": [
        {
          "id": "MDRW2100044674.1.1",
          "form": "그렇게 자면 아침에 너무 피곤하죠ㅠ",
          "original_form": "그렇게 자면 아침에 너무 피곤하죠ㅠ",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044674.1.2",
          "form": "맞아요ㅜㅠ 괜히 깰때마다 불안하구",
          "original_form": "맞아요ㅜㅠ 괜히 깰때마다 불안하구",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044674.1.3",
          "form": "가위는 잘 안 눌리세요?",
          "original_form": "가위는 잘 안 눌리세요?",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044674.1.4",
          "form": "네! 가위는 잘 안눌려요",
          "original_form": "네! 가위는 잘 안눌려요",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044674.1.5",
          "form": "가위가 자다가 몸이 안움직이는건가요?",
          "original_form": "가위가 자다가 몸이 안움직이는건가요?",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044674.1.6",
          "form": "정신만 깨는거죠!",
          "original_form": "정신만 깨는거죠!",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044674.1.7",
          "form": "간혹 눈만 떠지기도 하고",
          "original_form": "간혹 눈만 떠지기도 하고",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044674.1.8",
          "form": "몸도 안움직이고 목소리도 안나와요",
          "original_form": "몸도 안움직이고 목소리도 안나와요",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044674.1.9",
          "form": "어 그럼 가위눌린적 몇번 있는것같아요",
          "original_form": "어 그럼 가위눌린적 몇번 있는것같아요",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044674.1.10",
          "form": "귀신볼까봐 무서유서 눈은 안뜨고",
          "original_form": "귀신볼까봐 무서유서 눈은 안뜨고",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044674.1.11",
          "form": "몸을 움직여보려고 발버둥친적은있어요",
          "original_form": "몸을 움직여보려고 발버둥친적은있어요",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044674.1.12",
          "form": "맞아요!!  잠을 깊게 못자거나 스트레스받고 그럴때 가위 눌리더라구요ㅠ",
          "original_form": "맞아요!!  잠을 깊게 못자거나 스트레스받고 그럴때 가위 눌리더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044674.1.13",
          "form": "그리고 가위눌리면",
          "original_form": "그리고 가위눌리면",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044674.1.14",
          "form": "그 날에 여러번 눌려요",
          "original_form": "그 날에 여러번 눌려요",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044674.1.15",
          "form": "안눌리는 날은 아예 안눌리고요",
          "original_form": "안눌리는 날은 아예 안눌리고요",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044674.1.16",
          "form": "아 저도 그런적 있어요 다시 잠들었다가 또 눌리고 ㄷㄷ",
          "original_form": "아 저도 그런적 있어요 다시 잠들었다가 또 눌리고 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044674.1.17",
          "form": "지이이인짜 무섭고 너무 피곤해요..ㅋㅋ 숙면이 정말 중요합니다",
          "original_form": "지이이인짜 무섭고 너무 피곤해요..ㅋㅋ 숙면이 정말 중요합니다",
          "speaker_id": "2",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044674.1.18",
          "form": "정말요ㅋㅋ 그리고 숙면하면",
          "original_form": "정말요ㅋㅋ 그리고 숙면하면",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044674.1.19",
          "form": "살도 살짝 빠져요",
          "original_form": "살도 살짝 빠져요",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044674.1.20",
          "form": "아 대박... 당장 자야 돼...",
          "original_form": "아 대박... 당장 자야 돼...",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044674.1.21",
          "form": "근데 아직도 너무 배부르네요 하 언제 잘 수 있을까",
          "original_form": "근데 아직도 너무 배부르네요 하 언제 잘 수 있을까",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044674.1.22",
          "form": "배부르면 더 잠이 잘오지않나요??",
          "original_form": "배부르면 더 잠이 잘오지않나요??",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044674.1.23",
          "form": "저는 토할거 같더라구욬ㅋㅋㅋㅋ",
          "original_form": "저는 토할거 같더라구욬ㅋㅋㅋㅋ",
