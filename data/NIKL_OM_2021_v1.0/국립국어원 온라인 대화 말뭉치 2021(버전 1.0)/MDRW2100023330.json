      "utterance": [
        {
          "id": "MDRW2100023330.1.1",
          "form": "게임 하시나요?!",
          "original_form": "게임 하시나요?!",
          "speaker_id": "1",
          "time": "20210830 00:12"
        },
        {
          "id": "MDRW2100023330.1.2",
          "form": "저 한번씩 맞고쳐요ㅎㅎ",
          "original_form": "저 한번씩 맞고쳐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 09:23"
        },
        {
          "id": "MDRW2100023330.1.3",
          "form": "게임 하시는거 있으신가요?",
          "original_form": "게임 하시는거 있으신가요?",
          "speaker_id": "2",
          "time": "20210830 09:23"
        },
        {
          "id": "MDRW2100023330.1.4",
          "form": "헐 저도 맞고 좋아해요!!",
          "original_form": "헐 저도 맞고 좋아해요!!",
          "speaker_id": "1",
          "time": "20210830 11:23"
        },
        {
          "id": "MDRW2100023330.1.5",
          "form": "저는 쿠킹덤 열심히 하고 있습니닷",
          "original_form": "저는 쿠킹덤 열심히 하고 있습니닷",
          "speaker_id": "1",
          "time": "20210830 11:23"
        },
        {
          "id": "MDRW2100023330.1.6",
          "form": "오 저 쿠키런 열심히했었는데ㅎㅎ",
          "original_form": "오 저 쿠키런 열심히했었는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 11:32"
        },
        {
          "id": "MDRW2100023330.1.7",
          "form": "저두 예전에 엄청 좋아하다가 쿠킹덤으로 갈아탔어요 ㅎ.ㅎ",
          "original_form": "저두 예전에 엄청 좋아하다가 쿠킹덤으로 갈아탔어요 ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210830 12:35"
        },
        {
          "id": "MDRW2100023330.1.8",
          "form": "컴퓨터 개임도 자주 하시나요?!",
          "original_form": "컴퓨터 개임도 자주 하시나요?!",
          "speaker_id": "1",
          "time": "20210830 12:35"
        },
        {
          "id": "MDRW2100023330.1.9",
          "form": "컴퓨터게임은 안한지 엄청 오래 되었어요!",
          "original_form": "컴퓨터게임은 안한지 엄청 오래 되었어요!",
          "speaker_id": "2",
          "time": "20210831 00:01"
        },
        {
          "id": "MDRW2100023330.1.10",
          "form": "하시는 게임 있나요?",
          "original_form": "하시는 게임 있나요?",
          "speaker_id": "2",
          "time": "20210831 00:01"
        },
        {
          "id": "MDRW2100023330.1.11",
          "form": "저는 오버워치 해요!!!!",
          "original_form": "저는 오버워치 해요!!!!",
          "speaker_id": "1",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100023330.1.12",
          "form": "아시나요?!",
          "original_form": "아시나요?!",
          "speaker_id": "1",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100023330.1.13",
          "form": "네 당연히 알아용ㅎㅎ",
          "original_form": "네 당연히 알아용ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100023330.1.14",
          "form": "캐릭터도 엄청 많더라고요",
          "original_form": "캐릭터도 엄청 많더라고요",
          "speaker_id": "2",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100023330.1.15",
          "form": "옆에서 슬쩍본게 다라서",
          "original_form": "옆에서 슬쩍본게 다라서",
          "speaker_id": "2",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100023330.1.16",
          "form": "맞아요!!!!! 멀미 나는 게임이러",
          "original_form": "맞아요!!!!! 멀미 나는 게임이러",
          "speaker_id": "1",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100023330.1.17",
          "form": "주변에서 하는 사람이 적더라고요 ㅠ.",
          "original_form": "주변에서 하는 사람이 적더라고요 ㅠ.",
          "speaker_id": "1",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100023330.1.18",
          "form": "저 혼자 합니다요",
          "original_form": "저 혼자 합니다요",
          "speaker_id": "1",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100023330.1.19",
          "form": "오 게임 잘하시나봐요ㅎㅎ",
          "original_form": "오 게임 잘하시나봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100023330.1.20",
          "form": "앗 잘하지는 못하지만 열심히 해보고 있어요 ㅎ.ㅎ !",
          "original_form": "앗 잘하지는 못하지만 열심히 해보고 있어요 ㅎ.ㅎ !",
          "speaker_id": "1",
          "time": "20210831 00:17"
        },
        {
          "id": "MDRW2100023330.1.21",
          "form": "저는 막 순발력있게 해야하는 게임엔 약해서ㅠㅠ",
          "original_form": "저는 막 순발력있게 해야하는 게임엔 약해서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 11:14"
        },
        {
          "id": "MDRW2100023330.1.22",
