        },
        {
          "id": "MDRW2100042681.1.2",
          "form": "오늘 주먹밥 샀어",
          "original_form": "오늘 주먹밥 샀어",
          "speaker_id": "1",
          "time": "20210907 01:12"
        },
        {
          "id": "MDRW2100042681.1.3",
          "form": "어떤 맛",
          "original_form": "어떤 맛",
          "speaker_id": "2",
          "time": "20210907 01:12"
        },
        {
          "id": "MDRW2100042681.1.4",
          "form": "김치 치즈 주먹밥",
          "original_form": "김치 치즈 주먹밥",
          "speaker_id": "1",
          "time": "20210907 01:12"
        },
        {
          "id": "MDRW2100042681.1.5",
          "form": "맛있겠다 어떤 브랜드야",
          "original_form": "맛있겠다 어떤 브랜드야",
          "speaker_id": "2",
          "time": "20210907 01:12"
        },
        {
          "id": "MDRW2100042681.1.6",
          "form": "씨제이 비비고 꺼야",
          "original_form": "씨제이 비비고 꺼야",
          "speaker_id": "1",
          "time": "20210907 01:12"
        },
        {
          "id": "MDRW2100042681.1.7",
          "form": "비비 어느 웬만하면 맛있지",
          "original_form": "비비 어느 웬만하면 맛있지",
          "speaker_id": "2",
          "time": "20210907 01:13"
        },
        {
          "id": "MDRW2100042681.1.8",
          "form": "에어프라이기에 돌려 먹는 거라 맛있더라",
          "original_form": "에어프라이기에 돌려 먹는 거라 맛있더라",
          "speaker_id": "1",
          "time": "20210907 01:13"
        },
        {
          "id": "MDRW2100042681.1.9",
          "form": "그럼 치즈가 예쁘게 녹겠다",
          "original_form": "그럼 치즈가 예쁘게 녹겠다",
          "speaker_id": "2",
          "time": "20210907 01:13"
        },
        {
          "id": "MDRW2100042681.1.10",
          "form": "누룽지처럼 바삭바삭해서 맛있었",
          "original_form": "누룽지처럼 바삭바삭해서 맛있었",
          "speaker_id": "1",
          "time": "20210907 01:13"
        },
        {
          "id": "MDRW2100042681.1.11",
          "form": "나는 김치볶음밥보다 간장 계란밥이 좋아",
          "original_form": "나는 김치볶음밥보다 간장 계란밥이 좋아",
          "speaker_id": "2",
          "time": "20210907 01:13"
        },
        {
          "id": "MDRW2100042681.1.12",
          "form": "나도 짠맛이 좋아",
          "original_form": "나도 짠맛이 좋아",
          "speaker_id": "1",
          "time": "20210907 01:14"
        },
        {
          "id": "MDRW2100042681.1.13",
          "form": "요즘은 냉동 음식이 참 잘 나와",
          "original_form": "요즘은 냉동 음식이 참 잘 나와",
          "speaker_id": "2",
          "time": "20210907 01:14"
        },
        {
          "id": "MDRW2100042681.1.14",
          "form": "그니까 말이야",
          "original_form": "그니까 말이야",
