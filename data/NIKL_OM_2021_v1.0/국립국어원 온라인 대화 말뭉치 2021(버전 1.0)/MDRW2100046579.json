      "utterance": [
        {
          "id": "MDRW2100046579.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211102 00:05"
        },
        {
          "id": "MDRW2100046579.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211102 00:05"
        },
        {
          "id": "MDRW2100046579.1.3",
          "form": "혹시 글램핑 가보셨나요",
          "original_form": "혹시 글램핑 가보셨나요",
          "speaker_id": "2",
          "time": "20211102 00:05"
        },
        {
          "id": "MDRW2100046579.1.4",
          "form": "아뇨 아직 못 가봤네요",
          "original_form": "아뇨 아직 못 가봤네요",
          "speaker_id": "1",
          "time": "20211102 00:06"
        },
        {
          "id": "MDRW2100046579.1.5",
          "form": "요즘 사람들이 글램핑 많이 가더라구요",
          "original_form": "요즘 사람들이 글램핑 많이 가더라구요",
          "speaker_id": "2",
          "time": "20211102 00:07"
        },
        {
          "id": "MDRW2100046579.1.6",
          "form": "캠핑도 안가보셨나용",
          "original_form": "캠핑도 안가보셨나용",
          "speaker_id": "2",
          "time": "20211102 00:07"
        },
        {
          "id": "MDRW2100046579.1.7",
          "form": "캠핑도 어릴 때 몇번 간거 말고는 안 갔네요 ㅎㅎ",
          "original_form": "캠핑도 어릴 때 몇번 간거 말고는 안 갔네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211102 00:12"
        },
        {
          "id": "MDRW2100046579.1.8",
          "form": "오 그래두",
          "original_form": "오 그래두",
          "speaker_id": "2",
          "time": "20211102 00:14"
        },
        {
          "id": "MDRW2100046579.1.9",
          "form": "어렸을 때 가보셨군요?!",
          "original_form": "어렸을 때 가보셨군요?!",
          "speaker_id": "2",
          "time": "20211102 00:14"
        },
        {
          "id": "MDRW2100046579.1.10",
          "form": "저는 캠핑은 살면서 한 번도 가본적이..",
          "original_form": "저는 캠핑은 살면서 한 번도 가본적이..",
          "speaker_id": "2",
          "time": "20211102 00:14"
        },
        {
          "id": "MDRW2100046579.1.11",
          "form": "읍서용",
          "original_form": "읍서용",
          "speaker_id": "2",
          "time": "20211102 00:14"
        },
        {
          "id": "MDRW2100046579.1.12",
          "form": "저도 간거만 기억나지 뭐 했는지는 기억이 안나요",
          "original_form": "저도 간거만 기억나지 뭐 했는지는 기억이 안나요",
          "speaker_id": "1",
          "time": "20211102 00:14"
        },
        {
          "id": "MDRW2100046579.1.13",
          "form": "아마..",
          "original_form": "아마..",
          "speaker_id": "2",
          "time": "20211102 00:14"
        },
        {
          "id": "MDRW2100046579.1.14",
          "form": "열심히 드셨을거에요",
          "original_form": "열심히 드셨을거에요",
          "speaker_id": "2",
          "time": "20211102 00:14"
        },
        {
          "id": "MDRW2100046579.1.15",
          "form": "캠핑은 먹으러가는거니까",
          "original_form": "캠핑은 먹으러가는거니까",
          "speaker_id": "2",
          "time": "20211102 00:15"
        },
        {
          "id": "MDRW2100046579.1.16",
          "form": "크크",
          "original_form": "크크",
          "speaker_id": "2",
          "time": "20211102 00:15"
        },
        {
          "id": "MDRW2100046579.1.17",
          "form": "아무래도 캠핑은 장비도 필요하고 그래서 진입장벽이 높은데",
          "original_form": "아무래도 캠핑은 장비도 필요하고 그래서 진입장벽이 높은데",
          "speaker_id": "2",
          "time": "20211102 00:15"
        },
        {
          "id": "MDRW2100046579.1.18",
          "form": "글램핑은 다 준비되어있으니까 요즘 사람들이 많이 가는거같드라구용",
          "original_form": "글램핑은 다 준비되어있으니까 요즘 사람들이 많이 가는거같드라구용",
          "speaker_id": "2",
          "time": "20211102 00:15"
        },
        {
          "id": "MDRW2100046579.1.19",
          "form": "강추드려용 비오는날에 가도 좋더라구요",
          "original_form": "강추드려용 비오는날에 가도 좋더라구요",
          "speaker_id": "2",
          "time": "20211102 00:15"
        },
        {
          "id": "MDRW2100046579.1.20",
          "form": "요새 여행도 못가서 심심한데 한번 가보고 싶네요 ㅎㅎ",
          "original_form": "요새 여행도 못가서 심심한데 한번 가보고 싶네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211102 00:21"
        },
        {
          "id": "MDRW2100046579.1.21",
          "form": "서울 사시나용?",
          "original_form": "서울 사시나용?",
          "speaker_id": "2",
          "time": "20211102 00:22"
        },
        {
          "id": "MDRW2100046579.1.22",
          "form": "저 대구 살아요",
          "original_form": "저 대구 살아요",
          "speaker_id": "1",
          "time": "20211102 00:22"
        },
        {
          "id": "MDRW2100046579.1.23",
          "form": "아앗",
          "original_form": "아앗",
          "speaker_id": "2",
          "time": "20211102 00:22"
        },
        {
          "id": "MDRW2100046579.1.24",
          "form": "서울 사시면",
          "original_form": "서울 사시면",
          "speaker_id": "2",
          "time": "20211102 00:23"
        },
        {
          "id": "MDRW2100046579.1.25",
          "form": "가평 추천드릴라그랬는데 ㅠ",
          "original_form": "가평 추천드릴라그랬는데 ㅠ",
          "speaker_id": "2",
          "time": "20211102 00:23"
        },
        {
          "id": "MDRW2100046579.1.26",
          "form": "대구는 제가 잘 몰라서........허허 그래도 좋은 곳이 많을거에요",
          "original_form": "대구는 제가 잘 몰라서........허허 그래도 좋은 곳이 많을거에요",
          "speaker_id": "2",
          "time": "20211102 00:23"
        },
        {
          "id": "MDRW2100046579.1.27",
          "form": "한 번 알아봐야겠어요 ㅎ",
          "original_form": "한 번 알아봐야겠어요 ㅎ",
          "speaker_id": "1",
          "time": "20211102 00:24"
        },
        {
          "id": "MDRW2100046579.1.28",
          "form": "네네 글램핑 진짜 좋아용 너무 추워지기 전에 가보세요 꼭 ㅎㅎ\r\n\r\n",
          "original_form": "네네 글램핑 진짜 좋아용 너무 추워지기 전에 가보세요 꼭 ㅎㅎ\r\n\r\n",
          "speaker_id": "2",
          "time": "20211102 00:24"
        }
      ]
    }
