      "utterance": [
        {
          "id": "MDRW2100012772.1.1",
          "form": "배달음식좋아하세요?",
          "original_form": "배달음식좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 01:00"
        },
        {
          "id": "MDRW2100012772.1.2",
          "form": "거의 vip지요",
          "original_form": "거의 vip지요",
          "speaker_id": "1",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100012772.1.3",
          "form": "좋다기보다 편해서요",
          "original_form": "좋다기보다 편해서요",
          "speaker_id": "1",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100012772.1.4",
          "form": "주로 뭘 배달시키세요?",
          "original_form": "주로 뭘 배달시키세요?",
          "speaker_id": "1",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100012772.1.5",
          "form": "전 오늘 저녁에 생선구이 시켰어요ㅎㅎ",
          "original_form": "전 오늘 저녁에 생선구이 시켰어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100012772.1.6",
          "form": "고갈비에 밥ㅎㅎㅎ",
          "original_form": "고갈비에 밥ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:03"
        },
        {
          "id": "MDRW2100012772.1.7",
          "form": "주로 뭘 시키세요?",
          "original_form": "주로 뭘 시키세요?",
          "speaker_id": "2",
          "time": "20210825 01:03"
        },
        {
          "id": "MDRW2100012772.1.8",
          "form": "워낙 다양히 시켜서ㅎ",
          "original_form": "워낙 다양히 시켜서ㅎ",
          "speaker_id": "1",
          "time": "20210825 01:03"
        },
        {
          "id": "MDRW2100012772.1.9",
          "form": "오늘은 전어회 시켰네요",
          "original_form": "오늘은 전어회 시켰네요",
          "speaker_id": "1",
          "time": "20210825 01:03"
        },
        {
          "id": "MDRW2100012772.1.10",
          "form": "고갈비는 제대로 갈비스럽게 크던가요",
          "original_form": "고갈비는 제대로 갈비스럽게 크던가요",
          "speaker_id": "1",
          "time": "20210825 01:04"
        },
        {
          "id": "MDRW2100012772.1.11",
          "form": "오ㅎㅎ 소주한잔 하셨어요?ㅎ",
          "original_form": "오ㅎㅎ 소주한잔 하셨어요?ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:05"
        },
        {
          "id": "MDRW2100012772.1.12",
          "form": "네 회에는 소주지요 ㅎ",
          "original_form": "네 회에는 소주지요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 01:05"
        },
        {
          "id": "MDRW2100012772.1.13",
          "form": "그렇게 안크고ㅎ 저도 고갈비라해서 엄청 큰 구이인줄 알았는데",
          "original_form": "그렇게 안크고ㅎ 저도 고갈비라해서 엄청 큰 구이인줄 알았는데",
          "speaker_id": "2",
          "time": "20210825 01:05"
        },
        {
          "id": "MDRW2100012772.1.14",
          "form": "양념에 발라진거드라구요ㅎㅎ",
          "original_form": "양념에 발라진거드라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:05"
        },
        {
          "id": "MDRW2100012772.1.15",
          "form": "오ㅎ 좋죠ㅎㅎ",
          "original_form": "오ㅎ 좋죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:05"
        },
        {
          "id": "MDRW2100012772.1.16",
          "form": "음주카톡은 몸에 안좋은데ㅎ 자니? 가 나올수도있어요ㅎ 조심하셔요ㅎ",
          "original_form": "음주카톡은 몸에 안좋은데ㅎ 자니? 가 나올수도있어요ㅎ 조심하셔요ㅎ",
          "speaker_id": "2",
          "time": "20210825 01:06"
        },
        {
          "id": "MDRW2100012772.1.17",
          "form": "아ㅜ 그냥 고등어면 고갈비라 하면 안되는데ㅜ",
          "original_form": "아ㅜ 그냥 고등어면 고갈비라 하면 안되는데ㅜ",
          "speaker_id": "1",
          "time": "20210825 01:06"
        },
        {
          "id": "MDRW2100012772.1.18",
          "form": "아닠ㅋ 그렇게 과음 안해요",
          "original_form": "아닠ㅋ 그렇게 과음 안해요",
          "speaker_id": "1",
          "time": "20210825 01:06"
        },
        {
          "id": "MDRW2100012772.1.19",
          "form": "ㅎㅎ 몸챙기셔야해요ㅎㅎ",
          "original_form": "ㅎㅎ 몸챙기셔야해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:07"
        },
        {
          "id": "MDRW2100012772.1.20",
          "form": "자니 안나오시게ㅎㄹ",
          "original_form": "자니 안나오시게ㅎㄹ",
          "speaker_id": "2",
          "time": "20210825 01:07"
        },
        {
          "id": "MDRW2100012772.1.21",
          "form": "반주를 드신거에요?",
          "original_form": "반주를 드신거에요?",
          "speaker_id": "2",
          "time": "20210825 01:07"
        },
        {
          "id": "MDRW2100012772.1.22",
          "form": "네 소주 한병도 안 마셨어요!",
          "original_form": "네 소주 한병도 안 마셨어요!",
          "speaker_id": "1",
          "time": "20210825 01:12"
        },
        {
          "id": "MDRW2100012772.1.23",
          "form": "ㅎㅎ 잘하셨네요~^^ㅎ 전어회 부럽네요ㅎㅎ",
          "original_form": "ㅎㅎ 잘하셨네요~^^ㅎ 전어회 부럽네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:12"
        },
        {
          "id": "MDRW2100012772.1.24",
          "form": "혼술? 아님 가족?",
          "original_form": "혼술? 아님 가족?",
          "speaker_id": "2",
          "time": "20210825 01:12"
        },
        {
          "id": "MDRW2100012772.1.25",
          "form": "배달 음식이 다양해지긴 했네요ㅎ 제철 전어회도 시켜지고ㄹ",
          "original_form": "배달 음식이 다양해지긴 했네요ㅎ 제철 전어회도 시켜지고ㄹ",
          "speaker_id": "2",
          "time": "20210825 01:12"
        },
        {
          "id": "MDRW2100012772.1.26",
          "form": "가족들이 회를 별로 안 좋아해요ㅎ",
          "original_form": "가족들이 회를 별로 안 좋아해요ㅎ",
          "speaker_id": "1",
          "time": "20210825 01:15"
        },
        {
          "id": "MDRW2100012772.1.27",
          "form": "혼술했지요 벌써 전어 물이 올랐더라고요",
          "original_form": "혼술했지요 벌써 전어 물이 올랐더라고요",
          "speaker_id": "1",
          "time": "20210825 01:15"
        },
        {
          "id": "MDRW2100012772.1.28",
          "form": "요샌 배달이 너무 잘돼 있어요",
          "original_form": "요샌 배달이 너무 잘돼 있어요",
          "speaker_id": "1",
          "time": "20210825 01:16"
        },
        {
          "id": "MDRW2100012772.1.29",
          "form": "오ㅎㅎ 진짜 먹고싶네요ㅎㅎ",
          "original_form": "오ㅎㅎ 진짜 먹고싶네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:16"
        },
        {
          "id": "MDRW2100012772.1.30",
          "form": "전어회~^^",
          "original_form": "전어회~^^",
          "speaker_id": "2",
          "time": "20210825 01:16"
        },
        {
          "id": "MDRW2100012772.1.31",
          "form": "맞아요ㅎ 왠만한거 다있어요ㅎ 전에 설렁탕에 소주한잔했아요ㅎ",
          "original_form": "맞아요ㅎ 왠만한거 다있어요ㅎ 전에 설렁탕에 소주한잔했아요ㅎ",
          "speaker_id": "2",
