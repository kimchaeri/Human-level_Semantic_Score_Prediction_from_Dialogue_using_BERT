      "utterance": [
        {
          "id": "MDRW2100011147.1.1",
          "form": "즐겨보는 프로그램 있나요?",
          "original_form": "즐겨보는 프로그램 있나요?",
          "speaker_id": "2",
          "time": "20210824 01:48"
        },
        {
          "id": "MDRW2100011147.1.2",
          "form": "유튜브 자주봅니다!",
          "original_form": "유튜브 자주봅니다!",
          "speaker_id": "1",
          "time": "20210824 01:48"
        },
        {
          "id": "MDRW2100011147.1.3",
          "form": "다들 유튜브보시네요 ㅎ",
          "original_form": "다들 유튜브보시네요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 01:50"
        },
        {
          "id": "MDRW2100011147.1.4",
          "form": "잼난게 있나요?",
          "original_form": "잼난게 있나요?",
          "speaker_id": "2",
          "time": "20210824 01:50"
        },
        {
          "id": "MDRW2100011147.1.5",
          "form": "여행유튜브 자주봅니다ㅎㅎ",
          "original_form": "여행유튜브 자주봅니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 01:52"
        },
        {
          "id": "MDRW2100011147.1.6",
          "form": "아 여행좋아하시나보네요",
          "original_form": "아 여행좋아하시나보네요",
          "speaker_id": "2",
          "time": "20210824 01:53"
        },
        {
          "id": "MDRW2100011147.1.7",
          "form": "전 주로 티비시청이라ㅎ",
          "original_form": "전 주로 티비시청이라ㅎ",
          "speaker_id": "2",
          "time": "20210824 01:53"
        },
        {
          "id": "MDRW2100011147.1.8",
          "form": "어떤 프로그램 보시나요",
          "original_form": "어떤 프로그램 보시나요",
          "speaker_id": "1",
          "time": "20210824 01:56"
        },
        {
          "id": "MDRW2100011147.1.9",
          "form": "주로 예능봐요 드라마도가끔ㅎ",
          "original_form": "주로 예능봐요 드라마도가끔ㅎ",
          "speaker_id": "2",
          "time": "20210824 01:57"
        },
        {
          "id": "MDRW2100011147.1.10",
          "form": "유퀴즈 전참시 요런거요",
          "original_form": "유퀴즈 전참시 요런거요",
          "speaker_id": "2",
          "time": "20210824 01:57"
        },
        {
          "id": "MDRW2100011147.1.11",
          "form": "오 자기님이시군요ㅎㅎ",
          "original_form": "오 자기님이시군요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 01:58"
        },
        {
          "id": "MDRW2100011147.1.12",
          "form": "꼬꼬무 좋아했는데 10월부터한대요",
          "original_form": "꼬꼬무 좋아했는데 10월부터한대요",
          "speaker_id": "2",
          "time": "20210824 01:58"
        },
        {
          "id": "MDRW2100011147.1.13",
          "form": "유재석 넘좋아서 나오는건 대부분보는것같아요",
          "original_form": "유재석 넘좋아서 나오는건 대부분보는것같아요",
          "speaker_id": "2",
          "time": "20210824 01:58"
        },
        {
          "id": "MDRW2100011147.1.14",
          "form": "오 저도 다 보는거네요ㅎㅎ",
          "original_form": "오 저도 다 보는거네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 01:59"
        },
        {
          "id": "MDRW2100011147.1.15",
          "form": "다 재밌어요 억지웃음도 아니고",
          "original_form": "다 재밌어요 억지웃음도 아니고",
          "speaker_id": "2",
          "time": "20210824 02:03"
        },
        {
          "id": "MDRW2100011147.1.16",
          "form": "꼬꼬무 밀린거 빨리 봐야되는데ㅠ",
          "original_form": "꼬꼬무 밀린거 빨리 봐야되는데ㅠ",
