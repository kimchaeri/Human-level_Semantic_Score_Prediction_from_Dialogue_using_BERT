          "original_form": "텀블러할인300이지",
          "speaker_id": "2",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100006135.1.3",
          "form": "일반카페도활성화되면좋겟다",
          "original_form": "일반카페도활성화되면좋겟다",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100006135.1.4",
          "form": "우린싸들고다녀",
          "original_form": "우린싸들고다녀",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100006135.1.5",
          "form": "나도매번들고다녀",
          "original_form": "나도매번들고다녀",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100006135.1.6",
          "form": "할인받거든",
          "original_form": "할인받거든",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100006135.1.7",
          "form": "장바구니도챙기기시작하니",
          "original_form": "장바구니도챙기기시작하니",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100006135.1.8",
          "form": "장바구니는 어디",
          "original_form": "장바구니는 어디",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100006135.1.9",
          "form": "할인받고적립되고좋아",
          "original_form": "할인받고적립되고좋아",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100006135.1.10",
          "form": "마트가?",
          "original_form": "마트가?",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100006135.1.11",
          "form": "우린그런거있어",
          "original_form": "우린그런거있어",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100006135.1.12",
          "form": "일회용품안받으면배달비할인해주기이런거좋다",
          "original_form": "일회용품안받으면배달비할인해주기이런거좋다",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100006135.1.13",
          "form": "난 커피집만 쓰네",
          "original_form": "난 커피집만 쓰네",
