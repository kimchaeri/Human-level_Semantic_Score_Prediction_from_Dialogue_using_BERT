      "utterance": [
        {
          "id": "MDRW2100040104.1.1",
          "form": "미리배우고가면 학교가서 이해가 빨라 좋대요",
          "original_form": "미리배우고가면 학교가서 이해가 빨라 좋대요",
          "speaker_id": "2",
          "time": "20210904 10:01"
        },
        {
          "id": "MDRW2100040104.1.2",
          "form": "아!!!",
          "original_form": "아!!!",
          "speaker_id": "1",
          "time": "20210904 10:01"
        },
        {
          "id": "MDRW2100040104.1.3",
          "form": "초등은 괜찮겠죠?",
          "original_form": "초등은 괜찮겠죠?",
          "speaker_id": "1",
          "time": "20210904 10:02"
        },
        {
          "id": "MDRW2100040104.1.4",
          "form": "안배우고 갔으면 선생님 이야기 하나도 몰랐을것같다고",
          "original_form": "안배우고 갔으면 선생님 이야기 하나도 몰랐을것같다고",
          "speaker_id": "2",
          "time": "20210904 10:02"
        },
        {
          "id": "MDRW2100040104.1.5",
          "form": "앗.. 그정도예요?? ㅠ",
          "original_form": "앗.. 그정도예요?? ㅠ",
          "speaker_id": "1",
          "time": "20210904 10:02"
        },
        {
          "id": "MDRW2100040104.1.6",
          "form": "영수만 다니고 나머진ebs영상으로",
          "original_form": "영수만 다니고 나머진ebs영상으로",
          "speaker_id": "2",
          "time": "20210904 10:02"
        },
        {
          "id": "MDRW2100040104.1.7",
          "form": "아~ 저 수능볼때도 ebs막 시작했었는데요",
          "original_form": "아~ 저 수능볼때도 ebs막 시작했었는데요",
          "speaker_id": "1",
          "time": "20210904 10:02"
        },
        {
          "id": "MDRW2100040104.1.8",
          "form": "영수는 ebs로 안되나봐요?",
          "original_form": "영수는 ebs로 안되나봐요?",
          "speaker_id": "1",
          "time": "20210904 10:03"
        },
        {
          "id": "MDRW2100040104.1.9",
          "form": "과학 사회는 ebs",
          "original_form": "과학 사회는 ebs",
          "speaker_id": "2",
          "time": "20210904 10:03"
        },
        {
          "id": "MDRW2100040104.1.10",
          "form": "애들이 과사는 ebs로도 된대요?",
          "original_form": "애들이 과사는 ebs로도 된대요?",
          "speaker_id": "1",
          "time": "20210904 10:03"
        },
        {
          "id": "MDRW2100040104.1.11",
          "form": "수학은 옆에서 모르는걸 알려줘야해서 학원으로",
          "original_form": "수학은 옆에서 모르는걸 알려줘야해서 학원으로",
          "speaker_id": "2",
          "time": "20210904 10:03"
        },
        {
          "id": "MDRW2100040104.1.12",
          "form": "아~ 혼자 못하는군요",
          "original_form": "아~ 혼자 못하는군요",
          "speaker_id": "1",
          "time": "20210904 10:04"
        },
        {
          "id": "MDRW2100040104.1.13",
          "form": "근데 셋이 다 공부를 잘하나요?",
          "original_form": "근데 셋이 다 공부를 잘하나요?",
          "speaker_id": "1",
          "time": "20210904 10:04"
        },
        {
          "id": "MDRW2100040104.1.14",
          "form": "네..자랑하네요..잘하는거~ㅎ",
          "original_form": "네..자랑하네요..잘하는거~ㅎ",
          "speaker_id": "2",
          "time": "20210904 10:05"
        },
        {
          "id": "MDRW2100040104.1.15",
          "form": "우와!!! 잘 키우셨나봐요",
          "original_form": "우와!!! 잘 키우셨나봐요",
          "speaker_id": "1",
          "time": "20210904 10:05"
        },
        {
          "id": "MDRW2100040104.1.16",
          "form": "하긴 잘하니까 집에서 ebs보고 하겠죠?",
          "original_form": "하긴 잘하니까 집에서 ebs보고 하겠죠?",
          "speaker_id": "1",
          "time": "20210904 10:05"
        },
        {
          "id": "MDRW2100040104.1.17",
          "form": "못하는 애들은 집에서 공부할 의지가 없을것 같아요.. ㅡ.ㅡ",
          "original_form": "못하는 애들은 집에서 공부할 의지가 없을것 같아요.. ㅡ.ㅡ",
          "speaker_id": "1",
