      "utterance": [
        {
          "id": "MDRW2100019664.1.1",
          "form": "진짜 모두가 한국 안티던데욬ㅋㅋ ㅠㅠ",
          "original_form": "진짜 모두가 한국 안티던데욬ㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 21:06"
        },
        {
          "id": "MDRW2100019664.1.2",
          "form": "그 일본심판이 젤 싫었어요 ..",
          "original_form": "그 일본심판이 젤 싫었어요 ..",
          "speaker_id": "2",
          "time": "20210827 21:06"
        },
        {
          "id": "MDRW2100019664.1.3",
          "form": "그쵸 선수들이 상대팀+심판 이랑 경기하는 느낌이라",
          "original_form": "그쵸 선수들이 상대팀+심판 이랑 경기하는 느낌이라",
          "speaker_id": "1",
          "time": "20210827 21:07"
        },
        {
          "id": "MDRW2100019664.1.4",
          "form": "진짜 어이없더라구요",
          "original_form": "진짜 어이없더라구요",
          "speaker_id": "1",
          "time": "20210827 21:07"
        },
        {
          "id": "MDRW2100019664.1.5",
          "form": "맞아요 .. 근데도 잘하더라구요",
          "original_form": "맞아요 .. 근데도 잘하더라구요",
          "speaker_id": "2",
          "time": "20210827 21:09"
        },
        {
          "id": "MDRW2100019664.1.6",
          "form": "코치님도 잘하시구 ㅎㅎ",
          "original_form": "코치님도 잘하시구 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:09"
        },
        {
          "id": "MDRW2100019664.1.7",
          "form": "맞아요 감독님도 귀여웠는데",
          "original_form": "맞아요 감독님도 귀여웠는데",
          "speaker_id": "1",
          "time": "20210827 21:10"
        },
        {
          "id": "MDRW2100019664.1.8",
          "form": "올림픽까지만 계약이라니 아쉬워요",
          "original_form": "올림픽까지만 계약이라니 아쉬워요",
          "speaker_id": "1",
          "time": "20210827 21:10"
        },
        {
          "id": "MDRW2100019664.1.9",
          "form": "맞아요 ㅜㅜ 뭔가 재계약 안하실듯한 느낌 ,,",
          "original_form": "맞아요 ㅜㅜ 뭔가 재계약 안하실듯한 느낌 ,,",
          "speaker_id": "2",
          "time": "20210827 21:10"
        },
        {
          "id": "MDRW2100019664.1.10",
          "form": "김연경선수 보고 오셨다니까 ㅠ",
          "original_form": "김연경선수 보고 오셨다니까 ㅠ",
          "speaker_id": "2",
          "time": "20210827 21:10"
        },
        {
          "id": "MDRW2100019664.1.11",
          "form": "그쵸 ㅠㅠ",
          "original_form": "그쵸 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 21:11"
        },
        {
          "id": "MDRW2100019664.1.12",
          "form": "다음에 올 감독도 좀 오픈마인드의 외국인 감독이었으면 좋겠어요",
          "original_form": "다음에 올 감독도 좀 오픈마인드의 외국인 감독이었으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210827 21:11"
        },
        {
          "id": "MDRW2100019664.1.13",
          "form": "아 맞아요 ㅎㅎ",
          "original_form": "아 맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:11"
        },
        {
          "id": "MDRW2100019664.1.14",
          "form": "올림픽에서 높이뛰기도 보셨나요?",
          "original_form": "올림픽에서 높이뛰기도 보셨나요?",
          "speaker_id": "1",
          "time": "20210827 21:11"
        },
        {
          "id": "MDRW2100019664.1.15",
          "form": "우상혁선수 ! 맞죠 ㅎㅎ",
          "original_form": "우상혁선수 ! 맞죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100019664.1.16",
          "form": "봤어요 ㅎㅎ",
          "original_form": "봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100019664.1.17",
          "form": "네 ㅋㅋㅋ 경기 재밌게 잘하더라구요",
          "original_form": "네 ㅋㅋㅋ 경기 재밌게 잘하더라구요",
          "speaker_id": "1",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100019664.1.18",
          "form": "맞아요 ㅋㅋ 그 충성 하는거 완전 멋져요",
          "original_form": "맞아요 ㅋㅋ 그 충성 하는거 완전 멋져요",
          "speaker_id": "2",
          "time": "20210827 21:13"
        },
        {
          "id": "MDRW2100019664.1.19",
          "form": "완전 인싸던데 ㅋㅋ",
          "original_form": "완전 인싸던데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:13"
        },
        {
          "id": "MDRW2100019664.1.20",
          "form": "높이뛰기 선수들이 다 카메라 앞에서 활발하더라구요",
          "original_form": "높이뛰기 선수들이 다 카메라 앞에서 활발하더라구요",
          "speaker_id": "1",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100019664.1.21",
          "form": "경기 끝난 선수들 구경하는 재미도 쏠쏠했어요",
          "original_form": "경기 끝난 선수들 구경하는 재미도 쏠쏠했어요",
          "speaker_id": "1",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100019664.1.22",
          "form": "맞아요ㅋㅋ 그 누워있는 선수 엄청 웃겼어요 ㅋㅋ",
          "original_form": "맞아요ㅋㅋ 그 누워있는 선수 엄청 웃겼어요 ㅋㅋ",
