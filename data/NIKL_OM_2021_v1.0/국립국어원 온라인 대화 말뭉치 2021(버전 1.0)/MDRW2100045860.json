      "utterance": [
        {
          "id": "MDRW2100045860.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210908 08:08"
        },
        {
          "id": "MDRW2100045860.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210908 08:09"
        },
        {
          "id": "MDRW2100045860.1.3",
          "form": "가을 좋아하시나여",
          "original_form": "가을 좋아하시나여",
          "speaker_id": "2",
          "time": "20210908 08:10"
        },
        {
          "id": "MDRW2100045860.1.4",
          "form": "네 가을 좋아해요 name2님은 어떤 계절 좋아하나요?",
          "original_form": "네 가을 좋아해요 &name2&님은 어떤 계절 좋아하나요?",
          "speaker_id": "1",
          "time": "20210908 08:12"
        },
        {
          "id": "MDRW2100045860.1.5",
          "form": "저도 가을 좋아해요",
          "original_form": "저도 가을 좋아해요",
          "speaker_id": "2",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100045860.1.6",
          "form": "요즘엔 계속 비왓는데 오늘은 맑아서 좋네요 ㅎㅎ",
          "original_form": "요즘엔 계속 비왓는데 오늘은 맑아서 좋네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045860.1.7",
          "form": "요즘 봄 가을이 짧은 것 같아요",
          "original_form": "요즘 봄 가을이 짧은 것 같아요",
          "speaker_id": "1",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045860.1.8",
          "form": "맞아요 가을 장마랬어요",
          "original_form": "맞아요 가을 장마랬어요",
          "speaker_id": "1",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045860.1.9",
          "form": "네 그래서 너무 아쉬워요...",
          "original_form": "네 그래서 너무 아쉬워요...",
          "speaker_id": "2",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045860.1.10",
          "form": "추위 많이 타시나요?",
          "original_form": "추위 많이 타시나요?",
          "speaker_id": "2",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100045860.1.11",
          "form": "아니요 저는 오히려 더위를 잘 타요 님은요?",
          "original_form": "아니요 저는 오히려 더위를 잘 타요 님은요?",
          "speaker_id": "1",
          "time": "20210908 08:16"
        },
        {
          "id": "MDRW2100045860.1.12",
          "form": "저는 추위 많타서 겨울이 길어지는게 너무 싫어요 ㅋㅋㅋ",
          "original_form": "저는 추위 많타서 겨울이 길어지는게 너무 싫어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:17"
        },
        {
          "id": "MDRW2100045860.1.13",
          "form": "저는 여름이 점점 길어져서 싫어요",
          "original_form": "저는 여름이 점점 길어져서 싫어요",
          "speaker_id": "1",
          "time": "20210908 08:17"
        },
        {
          "id": "MDRW2100045860.1.14",
          "form": "딱 가을이 적당해요",
          "original_form": "딱 가을이 적당해요",
          "speaker_id": "1",
          "time": "20210908 08:17"
        },
        {
          "id": "MDRW2100045860.1.15",
          "form": "맞아요 ㅋㅋ요즘이 예쁜 옷 입기도 딱 좋운 날씨 아닌가요?",
          "original_form": "맞아요 ㅋㅋ요즘이 예쁜 옷 입기도 딱 좋운 날씨 아닌가요?",
          "speaker_id": "2",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100045860.1.16",
          "form": "선선하고 기분 좋은 날씨에요",
          "original_form": "선선하고 기분 좋은 날씨에요",
          "speaker_id": "1",
          "time": "20210908 08:19"
        },
        {
          "id": "MDRW2100045860.1.17",
          "form": "오늘은 날씨 어때요?",
          "original_form": "오늘은 날씨 어때요?",
          "speaker_id": "1",
          "time": "20210908 08:19"
        },
        {
          "id": "MDRW2100045860.1.18",
          "form": "오늘은 구름 많긴해도 햇빛나고 좋아요!",
          "original_form": "오늘은 구름 많긴해도 햇빛나고 좋아요!",
          "speaker_id": "2",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100045860.1.19",
          "form": "거긴 어떤가요?",
          "original_form": "거긴 어떤가요?",
          "speaker_id": "2",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100045860.1.20",
          "form": "여기도요 약간 어둡지만 적당해요",
          "original_form": "여기도요 약간 어둡지만 적당해요",
          "speaker_id": "1",
          "time": "20210908 08:23"
        }
      ]
