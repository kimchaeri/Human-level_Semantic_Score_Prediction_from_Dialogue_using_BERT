        {
          "id": "MDRW2100006883.1.1",
          "form": "볶음밥이 먹고 싶어요",
          "original_form": "볶음밥이 먹고 싶어요",
          "speaker_id": "1",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100006883.1.2",
          "form": "어떤볶음밥이요",
          "original_form": "어떤볶음밥이요",
          "speaker_id": "2",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100006883.1.3",
          "form": "제일 무난한건 김치볶음밥이요",
          "original_form": "제일 무난한건 김치볶음밥이요",
          "speaker_id": "1",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100006883.1.4",
          "form": "햄이나 참치넣고요",
          "original_form": "햄이나 참치넣고요",
          "speaker_id": "2",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100006883.1.5",
          "form": "햄이 좋군요 양파랑 김치 쏭쏭 썰고요",
          "original_form": "햄이 좋군요 양파랑 김치 쏭쏭 썰고요",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100006883.1.6",
          "form": "참기름 마지막 한방울",
          "original_form": "참기름 마지막 한방울",
          "speaker_id": "2",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100006883.1.7",
          "form": "깨도 솔솔 뿌리고 계란후라이도 올려요",
          "original_form": "깨도 솔솔 뿌리고 계란후라이도 올려요",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100006883.1.8",
          "form": "마늘볶음밥도 맛나요",
          "original_form": "마늘볶음밥도 맛나요",
          "speaker_id": "2",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100006883.1.9",
          "form": "편마늘 살짝 건조해서 볶더라구요",
          "original_form": "편마늘 살짝 건조해서 볶더라구요",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100006883.1.10",
          "form": "거기에 계란만 넣어두",
          "original_form": "거기에 계란만 넣어두",
          "speaker_id": "2",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100006883.1.11",
          "form": "태국 똠얌소스 넣어서 똠얌볶음밥이요",
          "original_form": "태국 똠얌소스 넣어서 똠얌볶음밥이요",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100006883.1.12",
          "form": "해산물 넣죠?",
          "original_form": "해산물 넣죠?",
          "speaker_id": "2",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100006883.1.13",
          "form": "새우랑 오징어 있음 넣음 구수하져",
          "original_form": "새우랑 오징어 있음 넣음 구수하져",
          "speaker_id": "1",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100006883.1.14",
          "form": "구수하기보단 식감좋죠",
          "original_form": "구수하기보단 식감좋죠",
          "speaker_id": "2",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100006883.1.15",
          "form": "네 옥수수 알갱이가 식감에좋아요",
          "original_form": "네 옥수수 알갱이가 식감에좋아요",
          "speaker_id": "1",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100006883.1.16",
          "form": "소고기 볶음밥 자주 해먹었어요",
