      "utterance": [
        {
          "id": "MDRW2100043491.1.1",
          "form": "저는 회는 좋아하는데 육회는 별로요..",
          "original_form": "저는 회는 좋아하는데 육회는 별로요..",
          "speaker_id": "2",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100043491.1.2",
          "form": "헉 이유가 있나요?",
          "original_form": "헉 이유가 있나요?",
          "speaker_id": "1",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100043491.1.3",
          "form": "고기를 날걸로 먹는게 별로에요",
          "original_form": "고기를 날걸로 먹는게 별로에요",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043491.1.4",
          "form": "회도 날걸로 먹잖아요 ㅎㅎ",
          "original_form": "회도 날걸로 먹잖아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043491.1.5",
          "form": "회는 뭔가 안 싫은데...ㅋㅋ",
          "original_form": "회는 뭔가 안 싫은데...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043491.1.6",
          "form": "육회는 못드시는 분은 아예 안드시더라고요",
          "original_form": "육회는 못드시는 분은 아예 안드시더라고요",
          "speaker_id": "1",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043491.1.7",
          "form": "못먹는건 아닌데 선호하지는 않아요",
          "original_form": "못먹는건 아닌데 선호하지는 않아요",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043491.1.8",
          "form": "저도 거의 간장양념맛 배맛으로 먹는 것 같긴해요",
          "original_form": "저도 거의 간장양념맛 배맛으로 먹는 것 같긴해요",
          "speaker_id": "1",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043491.1.9",
          "form": "달달하고 쫀득하고 뭐 그렇죠?",
          "original_form": "달달하고 쫀득하고 뭐 그렇죠?",
          "speaker_id": "2",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100043491.1.10",
          "form": "네 근데 맛있어요 ㅋㅋ",
          "original_form": "네 근데 맛있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043491.1.11",
          "form": "저는 회도 붉은살은 안좋아해서..",
          "original_form": "저는 회도 붉은살은 안좋아해서..",
          "speaker_id": "2",
          "time": "20210907 19:16"
        },
        {
          "id": "MDRW2100043491.1.12",
          "form": "헉..참치같은거요?",
          "original_form": "헉..참치같은거요?",
          "speaker_id": "1",
          "time": "20210907 19:16"
        },
