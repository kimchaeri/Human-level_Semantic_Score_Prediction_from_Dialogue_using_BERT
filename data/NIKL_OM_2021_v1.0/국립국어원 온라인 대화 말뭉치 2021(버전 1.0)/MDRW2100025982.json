      "utterance": [
        {
          "id": "MDRW2100025982.1.1",
          "form": "반려동물 키우는거있으세요?",
          "original_form": "반려동물 키우는거있으세요?",
          "speaker_id": "1",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100025982.1.2",
          "form": "아뇨 자신이 없어서 아직 없네요",
          "original_form": "아뇨 자신이 없어서 아직 없네요",
          "speaker_id": "2",
          "time": "20210901 18:35"
        },
        {
          "id": "MDRW2100025982.1.3",
          "form": "나중에",
          "original_form": "나중에",
          "speaker_id": "2",
          "time": "20210901 18:35"
        },
        {
          "id": "MDRW2100025982.1.4",
          "form": "고양이를 키워보고 싶긴 해요",
          "original_form": "고양이를 키워보고 싶긴 해요",
          "speaker_id": "2",
          "time": "20210901 18:35"
        },
        {
          "id": "MDRW2100025982.1.5",
          "form": "키우시는 동물이 있나요?",
          "original_form": "키우시는 동물이 있나요?",
          "speaker_id": "2",
          "time": "20210901 18:35"
        },
        {
          "id": "MDRW2100025982.1.6",
          "form": "저는 동물은 아직 못키우고요ㅋㅋ",
          "original_form": "저는 동물은 아직 못키우고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:35"
        },
        {
          "id": "MDRW2100025982.1.7",
          "form": "구피 물고기 키우고있어요",
          "original_form": "구피 물고기 키우고있어요",
          "speaker_id": "1",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100025982.1.8",
          "form": "아 물질하시는구나",
          "original_form": "아 물질하시는구나",
          "speaker_id": "2",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100025982.1.9",
          "form": "관리가 어렵다 그러던데",
          "original_form": "관리가 어렵다 그러던데",
          "speaker_id": "2",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100025982.1.10",
          "form": "네네ㅋ",
          "original_form": "네네ㅋ",
          "speaker_id": "1",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100025982.1.11",
          "form": "물고기는 밥 주는 손같은 거 알아보나요?",
          "original_form": "물고기는 밥 주는 손같은 거 알아보나요?",
          "speaker_id": "2",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100025982.1.12",
          "form": "관리는 2주에 한번씩 어항청소만해주면되요",
          "original_form": "관리는 2주에 한번씩 어항청소만해주면되요",
          "speaker_id": "1",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100025982.1.13",
          "form": "네네 알아봐요.",
          "original_form": "네네 알아봐요.",
          "speaker_id": "1",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100025982.1.14",
          "form": "와 저한텐 그것도 벅차네요..",
          "original_form": "와 저한텐 그것도 벅차네요..",
          "speaker_id": "2",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100025982.1.15",
          "form": "손가면",
          "original_form": "손가면",
          "speaker_id": "1",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100025982.1.16",
          "form": "몰려와요ㅋㅋ",
          "original_form": "몰려와요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100025982.1.17",
          "form": "와ㅋㅋㅋㅋ신기하네요",
          "original_form": "와ㅋㅋㅋㅋ신기하네요",
          "speaker_id": "2",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100025982.1.18",
          "form": "맞아요.ㅋㅋ 그것도 힘들긴해요.ㅋ",
          "original_form": "맞아요.ㅋㅋ 그것도 힘들긴해요.ㅋ",
          "speaker_id": "1",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100025982.1.19",
          "form": "하루에 한번씩",
          "original_form": "하루에 한번씩",
          "speaker_id": "1",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100025982.1.20",
          "form": "구피 말고 다른 종류는",
          "original_form": "구피 말고 다른 종류는",
          "speaker_id": "2",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100025982.1.21",
          "form": "먹이주면되요ㅋ",
          "original_form": "먹이주면되요ㅋ",
          "speaker_id": "1",
          "time": "20210901 18:37"
        }
      ]
