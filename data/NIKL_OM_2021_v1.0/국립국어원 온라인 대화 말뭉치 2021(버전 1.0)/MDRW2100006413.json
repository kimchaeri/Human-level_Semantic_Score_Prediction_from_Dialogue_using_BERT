      "utterance": [
        {
          "id": "MDRW2100006413.1.1",
          "form": "코로나 끝나면 가고싶은 곳 잇으세요?",
          "original_form": "코로나 끝나면 가고싶은 곳 잇으세요?",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100006413.1.2",
          "form": "음 베트남이나 태국이요",
          "original_form": "음 베트남이나 태국이요",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100006413.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100006413.1.4",
          "form": "저는 몽골이요",
          "original_form": "저는 몽골이요",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100006413.1.5",
          "form": "오  몽골 뭔가 신비롭다",
          "original_form": "오  몽골 뭔가 신비롭다",
          "speaker_id": "1",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100006413.1.6",
          "form": "대자연을 느낄 수 있을것 같아요",
          "original_form": "대자연을 느낄 수 있을것 같아요",
          "speaker_id": "1",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100006413.1.7",
          "form": "네 대자연을 느끼면서 별보고 싶어요 ㅋㅋㅋㅋ",
          "original_form": "네 대자연을 느끼면서 별보고 싶어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100006413.1.8",
          "form": "저는 휴얌하고 싶어요",
          "original_form": "저는 휴얌하고 싶어요",
          "speaker_id": "1",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100006413.1.9",
          "form": "좋은 숙소에서 마사지 받으면서 ㅎㅎ",
          "original_form": "좋은 숙소에서 마사지 받으면서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100006413.1.10",
          "form": "맞아요 그것도 너무 좋죠 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 그것도 너무 좋죠 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100006413.1.11",
          "form": "태국마사지 받고싶네요 ㅋㅋㅋㅋ",
          "original_form": "태국마사지 받고싶네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100006413.1.12",
          "form": "해외여행 어디 다녀와보셨어요?",
          "original_form": "해외여행 어디 다녀와보셨어요?",
          "speaker_id": "1",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100006413.1.13",
          "form": "제일 많이 갓던곳은 일본이에요 ㅋㅋㅋㅋㅋ",
          "original_form": "제일 많이 갓던곳은 일본이에요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100006413.1.14",
          "form": "저도 일본 한번 여행했어는데 너무 좋았어요",
          "original_form": "저도 일본 한번 여행했어는데 너무 좋았어요",
          "speaker_id": "1",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100006413.1.15",
          "form": "어디 다녀오셧어요?",
          "original_form": "어디 다녀오셧어요?",
          "speaker_id": "2",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100006413.1.16",
          "form": "후카이도 한번 다녀왔어요",
          "original_form": "후카이도 한번 다녀왔어요",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100006413.1.17",
          "form": "부산에서 배타고 갔어요",
          "original_form": "부산에서 배타고 갔어요",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100006413.1.18",
          "form": "오 배를 타면 얼마나 걸려요?",
          "original_form": "오 배를 타면 얼마나 걸려요?",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100006413.1.19",
          "form": "제기억에 3시간 정도 걸렸던거 같아요",
          "original_form": "제기억에 3시간 정도 걸렸던거 같아요",
          "speaker_id": "1",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100006413.1.20",
          "form": "근데 배 무서웠어요",
          "original_form": "근데 배 무서웠어요",
          "speaker_id": "1",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100006413.1.21",
          "form": "부산에서 홋카이도까지 세시간밖에 안걸려요??",
          "original_form": "부산에서 홋카이도까지 세시간밖에 안걸려요??",
          "speaker_id": "2",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100006413.1.22",
          "form": "잘못 말했어요",
          "original_form": "잘못 말했어요",
          "speaker_id": "1",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100006413.1.23",
          "form": "후쿠오카 다녀왔어요",
          "original_form": "후쿠오카 다녀왔어요",
          "speaker_id": "1",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100006413.1.24",
          "form": "아 후쿠오카군요",
          "original_form": "아 후쿠오카군요",
          "speaker_id": "2",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100006413.1.25",
          "form": "저도 부산에서 배타고 후쿠오카 다녀온적 잇어요",
          "original_form": "저도 부산에서 배타고 후쿠오카 다녀온적 잇어요",
          "speaker_id": "2",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100006413.1.26",
          "form": "네 후쿠오카 한 번 다녀왔는데",
          "original_form": "네 후쿠오카 한 번 다녀왔는데",
          "speaker_id": "1",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100006413.1.27",
