      "utterance": [
        {
          "id": "MDRW2100040374.1.1",
          "form": "그생각과함께 이비싼걸왜여기..?  그럴거같아요",
          "original_form": "그생각과함께 이비싼걸왜여기..?  그럴거같아요",
          "speaker_id": "2",
          "time": "20210904 17:23"
        },
        {
          "id": "MDRW2100040374.1.2",
          "form": "오우",
          "original_form": "오우",
          "speaker_id": "2",
          "time": "20210904 17:23"
        },
        {
          "id": "MDRW2100040374.1.3",
          "form": "ㅋㅋ그나마 거기 새우튀김도 세트로 있었는데",
          "original_form": "ㅋㅋ그나마 거기 새우튀김도 세트로 있었는데",
          "speaker_id": "1",
          "time": "20210904 17:24"
        },
        {
          "id": "MDRW2100040374.1.4",
          "form": "그게 그나마 위안이..",
          "original_form": "그게 그나마 위안이..",
          "speaker_id": "1",
          "time": "20210904 17:24"
        },
        {
          "id": "MDRW2100040374.1.5",
          "form": "됐을리가요ㅜ",
          "original_form": "됐을리가요ㅜ",
          "speaker_id": "1",
          "time": "20210904 17:24"
        },
        {
          "id": "MDRW2100040374.1.6",
          "form": "아 근데 배민서 환불할래 다시 받을래 이래서",
          "original_form": "아 근데 배민서 환불할래 다시 받을래 이래서",
          "speaker_id": "1",
          "time": "20210904 17:24"
        },
        {
          "id": "MDRW2100040374.1.7",
          "form": "후자 택했거든요",
          "original_form": "후자 택했거든요",
          "speaker_id": "1",
          "time": "20210904 17:24"
        },
        {
          "id": "MDRW2100040374.1.8",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210904 17:24"
        },
        {
          "id": "MDRW2100040374.1.9",
          "form": "또한참기다려야하자나여..",
          "original_form": "또한참기다려야하자나여..",
          "speaker_id": "2",
          "time": "20210904 17:25"
        },
        {
          "id": "MDRW2100040374.1.10",
          "form": "근데 음식도 새로 주고 3000원 할인쿠폰도 주고",
          "original_form": "근데 음식도 새로 주고 3000원 할인쿠폰도 주고",
          "speaker_id": "1",
          "time": "20210904 17:25"
        },
        {
          "id": "MDRW2100040374.1.11",
          "form": "음식값도 그냥 피해보상으로 주던데요",
          "original_form": "음식값도 그냥 피해보상으로 주던데요",
          "speaker_id": "1",
          "time": "20210904 17:25"
        },
        {
          "id": "MDRW2100040374.1.12",
          "form": "그래서 주문 한시간반만에 받았지만 화가 가라앉았죠",
          "original_form": "그래서 주문 한시간반만에 받았지만 화가 가라앉았죠",
          "speaker_id": "1",
          "time": "20210904 17:25"
        },
        {
          "id": "MDRW2100040374.1.13",
          "form": "역시 최고의 치료는 금융치료",
          "original_form": "역시 최고의 치료는 금융치료",
          "speaker_id": "1",
          "time": "20210904 17:26"
        },
        {
          "id": "MDRW2100040374.1.14",
          "form": "오 나름 보상이 괜찮았네요 ㅇㅁㅇ  \r\n그나마 다행이예요",
          "original_form": "오 나름 보상이 괜찮았네요 ㅇㅁㅇ  \r\n그나마 다행이예요",
          "speaker_id": "2",
          "time": "20210904 17:26"
        },
        {
          "id": "MDRW2100040374.1.15",
          "form": "금융치료ㅋㄱㅈㅋㅈㄱㅈㄱㅈㄱ",
          "original_form": "금융치료ㅋㄱㅈㅋㅈㄱㅈㄱㅈㄱ",
          "speaker_id": "2",
          "time": "20210904 17:26"
        },
        {
          "id": "MDRW2100040374.1.16",
          "form": "가게엔 피해 안 간 것 같고",
          "original_form": "가게엔 피해 안 간 것 같고",
          "speaker_id": "1",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100040374.1.17",
          "form": "암튼 배민1 시스템 좀 별로예요!",
          "original_form": "암튼 배민1 시스템 좀 별로예요!",
          "speaker_id": "1",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100040374.1.18",
          "form": "라이더 연락처를 빨리 알았음 즉각 조치를 취했을텐데",
          "original_form": "라이더 연락처를 빨리 알았음 즉각 조치를 취했을텐데",
          "speaker_id": "1",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100040374.1.19",
          "form": "배민1진짜별로입니다ㅋㅋㅋ...",
          "original_form": "배민1진짜별로입니다ㅋㅋㅋ...",
          "speaker_id": "2",
          "time": "20210904 17:27"
        },
        {
          "id": "MDRW2100040374.1.20",
          "form": "할인쿠폰뿌리던데",
          "original_form": "할인쿠폰뿌리던데",
          "speaker_id": "2",
          "time": "20210904 17:28"
        },
        {
          "id": "MDRW2100040374.1.21",
          "form": "쓸데도없고ㄲㄱㅋ\r\n배달비가확올라가니까",
          "original_form": "쓸데도없고ㄲㄱㅋ\r\n배달비가확올라가니까",
          "speaker_id": "2",
          "time": "20210904 17:28"
        },
        {
          "id": "MDRW2100040374.1.22",
          "form": "의미도없고",
          "original_form": "의미도없고",
          "speaker_id": "2",
          "time": "20210904 17:28"
        },
        {
          "id": "MDRW2100040374.1.23",
          "form": "ㅎㅎ 배민1 입점한 가게들 기본 배달비도 올리던데요",
          "original_form": "ㅎㅎ 배민1 입점한 가게들 기본 배달비도 올리던데요",
          "speaker_id": "1",
          "time": "20210904 17:28"
        },
        {
          "id": "MDRW2100040374.1.24",
          "form": "근데 솔직히 배민1이든 아니든 빠른덴 빠르고 느린덴 느려요",
          "original_form": "근데 솔직히 배민1이든 아니든 빠른덴 빠르고 느린덴 느려요",
          "speaker_id": "1",
          "time": "20210904 17:29"
        },
        {
          "id": "MDRW2100040374.1.25",
          "form": "쿠폰 줘서 배민1  쓴거징",
          "original_form": "쿠폰 줘서 배민1  쓴거징",
          "speaker_id": "1",
          "time": "20210904 17:29"
        },
        {
          "id": "MDRW2100040374.1.26",
          "form": "네 같은가게라도 배달비가몇천원차이ㅋㅋㅋ.. 이게모람 했어요",
          "original_form": "네 같은가게라도 배달비가몇천원차이ㅋㅋㅋ.. 이게모람 했어요",
          "speaker_id": "2",
          "time": "20210904 17:29"
        },
        {
          "id": "MDRW2100040374.1.27",
          "form": "맞ㅇㆍ요 속도차이도뭐..",
          "original_form": "맞ㅇㆍ요 속도차이도뭐..",
          "speaker_id": "2",
          "time": "20210904 17:29"
        },
        {
          "id": "MDRW2100040374.1.28",
          "form": "그나마 배민 고객센터 응대  늦어 빈정상했는데",
          "original_form": "그나마 배민 고객센터 응대  늦어 빈정상했는데",
          "speaker_id": "1",
          "time": "20210904 17:30"
        },
        {
          "id": "MDRW2100040374.1.29",
          "form": "대응을 너무 저자세로 해주셔서 당황",
          "original_form": "대응을 너무 저자세로 해주셔서 당황",
          "speaker_id": "1",
          "time": "20210904 17:30"
        },
        {
          "id": "MDRW2100040374.1.30",
          "form": "그러네요 하도 안좋은글만봐서 편견있었는데\r\n아니네요ㅋㄲㅋㅋ",
          "original_form": "그러네요 하도 안좋은글만봐서 편견있었는데\r\n아니네요ㅋㄲㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:31"
        },
        {
          "id": "MDRW2100040374.1.31",
          "form": "그냥 재배달받겠다고 하니 먼저 피해보상  말 꺼내시더라구요",
          "original_form": "그냥 재배달받겠다고 하니 먼저 피해보상  말 꺼내시더라구요",
          "speaker_id": "1",
          "time": "20210904 17:31"
        },
        {
          "id": "MDRW2100040374.1.32",
          "form": "대뜸 미안하니 주문금액 돌려준다며",
          "original_form": "대뜸 미안하니 주문금액 돌려준다며",
          "speaker_id": "1",
          "time": "20210904 17:31"
        },
        {
          "id": "MDRW2100040374.1.33",
          "form": "계좌 포인트 둘중 하나 택하라고",
          "original_form": "계좌 포인트 둘중 하나 택하라고",
          "speaker_id": "1",
          "time": "20210904 17:31"
        },
        {
          "id": "MDRW2100040374.1.34",
          "form": "선택지까지ㅎㅎ",
          "original_form": "선택지까지ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 17:31"
        },
        {
          "id": "MDRW2100040374.1.35",
          "form": "쿠폰값까지 더해 입금됐던데요",
          "original_form": "쿠폰값까지 더해 입금됐던데요",
          "speaker_id": "1",
          "time": "20210904 17:32"
        },
        {
          "id": "MDRW2100040374.1.36",
          "form": "갑자기 창조경제가ㅋ",
          "original_form": "갑자기 창조경제가ㅋ",
          "speaker_id": "1",
          "time": "20210904 17:32"
        },
        {
          "id": "MDRW2100040374.1.37",
          "form": "와대박적ㅋㄱㅋㅋㅋㄱㅋ",
          "original_form": "와대박적ㅋㄱㅋㅋㅋㄱㅋ",
