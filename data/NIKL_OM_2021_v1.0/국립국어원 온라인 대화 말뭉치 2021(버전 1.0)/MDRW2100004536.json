      "utterance": [
        {
          "id": "MDRW2100004536.1.1",
          "form": "비가와서그런지",
          "original_form": "비가와서그런지",
          "speaker_id": "2",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004536.1.2",
          "form": "시원하네여",
          "original_form": "시원하네여",
          "speaker_id": "2",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004536.1.3",
          "form": "그러게요.. 오히려 선풍기 틀면 춥네요",
          "original_form": "그러게요.. 오히려 선풍기 틀면 춥네요",
          "speaker_id": "1",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100004536.1.4",
          "form": "전 그정돈 아냐...........선풍기있어야대..",
          "original_form": "전 그정돈 아냐...........선풍기있어야대..",
          "speaker_id": "2",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100004536.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 저는 입추 지난 후오 좀 춥게 느껴서 그런가...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 저는 입추 지난 후오 좀 춥게 느껴서 그런가...",
          "speaker_id": "1",
          "time": "20210821 20:33"
        },
        {
          "id": "MDRW2100004536.1.6",
          "form": "name1님은 추위를 잘 탄다..(메모)",
          "original_form": "&name1&님은 추위를 잘 탄다..(메모)",
          "speaker_id": "2",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100004536.1.7",
          "form": "내일도 비 오겠죠? 나가야하는디",
          "original_form": "내일도 비 오겠죠? 나가야하는디",
          "speaker_id": "2",
          "time": "20210821 20:58"
        },
        {
          "id": "MDRW2100004536.1.8",
          "form": "아무래도 이거 태풍인거 같으니까요",
          "original_form": "아무래도 이거 태풍인거 같으니까요",
          "speaker_id": "1",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100004536.1.9",
          "form": "비오겠다.... 계속...",
          "original_form": "비오겠다.... 계속...",
          "speaker_id": "1",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100004536.1.10",
          "form": "솔직히 저번 장마는 장마느낌이 아녔어...",
          "original_form": "솔직히 저번 장마는 장마느낌이 아녔어...",
          "speaker_id": "2",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100004536.1.11",
          "form": "장마 맛만 보여주고 끝남..",
          "original_form": "장마 맛만 보여주고 끝남..",
          "speaker_id": "2",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100004536.1.12",
          "form": "맞아요 딱 맛보기",
          "original_form": "맞아요 딱 맛보기",
          "speaker_id": "1",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100004536.1.13",
          "form": "비오는 더 치곤 습도도 나쁘진 않은것같아요",
          "original_form": "비오는 더 치곤 습도도 나쁘진 않은것같아요",
          "speaker_id": "2",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100004536.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100004536.1.15",
          "form": "내일도 내리면 어떨지 모르겠다..ㅠㅠ",
          "original_form": "내일도 내리면 어떨지 모르겠다..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100004536.1.16",
          "form": "내일도 내릴 것이다... 아마도..?",
          "original_form": "내일도 내릴 것이다... 아마도..?",
          "speaker_id": "2",
          "time": "20210821 21:06"
        },
        {
          "id": "MDRW2100004536.1.17",
          "form": "일주일 내내 온다곤 하는데",
          "original_form": "일주일 내내 온다곤 하는데",
          "speaker_id": "2",
          "time": "20210821 21:06"
        },
        {
          "id": "MDRW2100004536.1.18",
          "form": "전 사실 믿지는 않아서요..(저번주에 그런다 해놓곤 안옴",
          "original_form": "전 사실 믿지는 않아서요..(저번주에 그런다 해놓곤 안옴",
          "speaker_id": "2",
          "time": "20210821 21:06"
        },
        {
          "id": "MDRW2100004536.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 맞아..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 맞아..",
          "speaker_id": "1",
          "time": "20210821 21:06"
        },
        {
          "id": "MDRW2100004536.1.20",
          "form": "지난번은 딱 이틀 좀 내리고",
          "original_form": "지난번은 딱 이틀 좀 내리고",
          "speaker_id": "1",
          "time": "20210821 21:06"
        },
        {
          "id": "MDRW2100004536.1.21",
          "form": "끝나버렸다..",
          "original_form": "끝나버렸다..",
          "speaker_id": "1",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100004536.1.22",
          "form": "근데 이번에는 태풍이 북상하면서 생긴 장마니까",
          "original_form": "근데 이번에는 태풍이 북상하면서 생긴 장마니까",
          "speaker_id": "1",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100004536.1.23",
          "form": "비 많이 내릴듯해요",
          "original_form": "비 많이 내릴듯해요",
          "speaker_id": "1",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100004536.1.24",
          "form": "그러게여 비 다 내리면 본격적으로 선선해질것같아요",
          "original_form": "그러게여 비 다 내리면 본격적으로 선선해질것같아요",
          "speaker_id": "2",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100004536.1.25",
          "form": "옛날엔 가을도 꽤 햇살이 따가웠는데",
          "original_form": "옛날엔 가을도 꽤 햇살이 따가웠는데",
