      "utterance": [
        {
          "id": "MDRW2100011441.1.1",
          "form": "공연이나 전시 좋아하세요?",
          "original_form": "공연이나 전시 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 12:44"
        },
        {
          "id": "MDRW2100011441.1.2",
          "form": "좋아하는데 기회가 많이 없어서 많이 접하지는 못했어요",
          "original_form": "좋아하는데 기회가 많이 없어서 많이 접하지는 못했어요",
          "speaker_id": "1",
          "time": "20210824 12:45"
        },
        {
          "id": "MDRW2100011441.1.3",
          "form": "리틀송님은 좋아하시나요",
          "original_form": "리틀송님은 좋아하시나요",
          "speaker_id": "1",
          "time": "20210824 12:45"
        },
        {
          "id": "MDRW2100011441.1.4",
          "form": "네 저도 좋아해요!",
          "original_form": "네 저도 좋아해요!",
          "speaker_id": "2",
          "time": "20210824 12:54"
        },
        {
          "id": "MDRW2100011441.1.5",
          "form": "근데 요즘은 코로나때문에",
          "original_form": "근데 요즘은 코로나때문에",
          "speaker_id": "2",
          "time": "20210824 12:54"
        },
        {
          "id": "MDRW2100011441.1.6",
          "form": "잘 못가서 아쉬워요ㅠ",
          "original_form": "잘 못가서 아쉬워요ㅠ",
          "speaker_id": "2",
          "time": "20210824 12:54"
        },
        {
          "id": "MDRW2100011441.1.7",
          "form": "아쉽겠어요ㅠ",
          "original_form": "아쉽겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210824 12:54"
        },
        {
          "id": "MDRW2100011441.1.8",
          "form": "기억에 남는 공연있나요",
          "original_form": "기억에 남는 공연있나요",
          "speaker_id": "1",
          "time": "20210824 12:54"
        },
        {
          "id": "MDRW2100011441.1.9",
          "form": "기억에 남는 건 뮤지컬 엘리자벳이요!",
          "original_form": "기억에 남는 건 뮤지컬 엘리자벳이요!",
          "speaker_id": "2",
          "time": "20210824 12:57"
        },
        {
          "id": "MDRW2100011441.1.10",
          "form": "ㅎㅎ보고싶으신 공연 있나요?",
          "original_form": "ㅎㅎ보고싶으신 공연 있나요?",
          "speaker_id": "2",
          "time": "20210824 12:57"
        },
        {
          "id": "MDRW2100011441.1.11",
          "form": "저는 콘서트? 가고싶다는 생각했었어요",
          "original_form": "저는 콘서트? 가고싶다는 생각했었어요",
          "speaker_id": "1",
          "time": "20210824 12:58"
        },
        {
          "id": "MDRW2100011441.1.12",
          "form": "노래 듣다보면 아 이 가수의 노래는 직접 들어보고싶다",
          "original_form": "노래 듣다보면 아 이 가수의 노래는 직접 들어보고싶다",
          "speaker_id": "1",
          "time": "20210824 12:58"
        },
        {
          "id": "MDRW2100011441.1.13",
          "form": "하는 거 있잖아요 ㅎㅎ",
          "original_form": "하는 거 있잖아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 12:58"
        },
        {
          "id": "MDRW2100011441.1.14",
          "form": "오 콘서트도 좋죠~",
          "original_form": "오 콘서트도 좋죠~",
          "speaker_id": "2",
          "time": "20210824 13:00"
        },
        {
          "id": "MDRW2100011441.1.15",
          "form": "누구 콘서트 가고싶으세요?",
          "original_form": "누구 콘서트 가고싶으세요?",
          "speaker_id": "2",
          "time": "20210824 13:00"
        },
        {
          "id": "MDRW2100011441.1.16",
          "form": "저 악뮤요",
          "original_form": "저 악뮤요",
          "speaker_id": "1",
          "time": "20210824 13:01"
        },
        {
          "id": "MDRW2100011441.1.17",
          "form": "수현이 목소리를 좋아해서",
          "original_form": "수현이 목소리를 좋아해서",
          "speaker_id": "1",
          "time": "20210824 13:01"
        },
        {
          "id": "MDRW2100011441.1.18",
          "form": "콘서트 가보신 적 있으신가오",
          "original_form": "콘서트 가보신 적 있으신가오",
          "speaker_id": "1",
          "time": "20210824 13:01"
        },
        {
          "id": "MDRW2100011441.1.19",
          "form": "수현 너무좋죠ㅠㅜ 비긴어게인 너무 좋았어요ㅎㅎ",
          "original_form": "수현 너무좋죠ㅠㅜ 비긴어게인 너무 좋았어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 13:16"
        },
        {
          "id": "MDRW2100011441.1.20",
          "form": "전 어릴때 아이돌 좋아해서 아이돌콘서트갔었어요ㅎㅎ",
          "original_form": "전 어릴때 아이돌 좋아해서 아이돌콘서트갔었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 13:16"
        },
        {
          "id": "MDRW2100011441.1.21",
          "form": "어떤 아이돌 콘서트를 가보셨나요",
          "original_form": "어떤 아이돌 콘서트를 가보셨나요",
          "speaker_id": "1",
          "time": "20210824 13:21"
        },
        {
          "id": "MDRW2100011441.1.22",
          "form": "지금은 아닌데 동방신기 좋아해서 갔었어요ㅎㅎ",
          "original_form": "지금은 아닌데 동방신기 좋아해서 갔었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 13:27"
        },
        {
          "id": "MDRW2100011441.1.23",
          "form": "부끄럽네요~ㅎㅎ",
          "original_form": "부끄럽네요~ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 13:27"
        },
        {
          "id": "MDRW2100011441.1.24",
          "form": "아니에요 좋아하는 가수의 콘서트를 가는 일 만큼 설레는 일은 없는걸요",
          "original_form": "아니에요 좋아하는 가수의 콘서트를 가는 일 만큼 설레는 일은 없는걸요",
          "speaker_id": "1",
          "time": "20210824 13:30"
        },
        {
          "id": "MDRW2100011441.1.25",
          "form": "부끄러워하실 필요 없으세요",
          "original_form": "부끄러워하실 필요 없으세요",
          "speaker_id": "1",
          "time": "20210824 13:30"
        },
        {
          "id": "MDRW2100011441.1.26",
          "form": "공연은 재밌었나요",
          "original_form": "공연은 재밌었나요",
          "speaker_id": "1",
          "time": "20210824 13:30"
        },
        {
          "id": "MDRW2100011441.1.27",
          "form": "ㅋㅋㅋㅋㅋ지금은 안좋아하는데 그땐 넘 설레고 좋았어요ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋ지금은 안좋아하는데 그땐 넘 설레고 좋았어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 16:32"
        }
      ]
    }
