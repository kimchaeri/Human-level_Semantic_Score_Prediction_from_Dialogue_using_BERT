      "utterance": [
        {
          "id": "MDRW2100014952.1.1",
          "form": "야 니 최애 머냐",
          "original_form": "야 니 최애 머냐",
          "speaker_id": "1",
          "time": "20210825 21:20"
        },
        {
          "id": "MDRW2100014952.1.2",
          "form": "레몬에이드",
          "original_form": "레몬에이드",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014952.1.3",
          "form": "여름이라 상콤한게 땡기는거 인정",
          "original_form": "여름이라 상콤한게 땡기는거 인정",
          "speaker_id": "1",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014952.1.4",
          "form": "레몬에이드는 계절안타",
          "original_form": "레몬에이드는 계절안타",
          "speaker_id": "2",
          "time": "20210825 21:21"
        },
        {
          "id": "MDRW2100014952.1.5",
          "form": "아니야 여름에 먹음 더 조음",
          "original_form": "아니야 여름에 먹음 더 조음",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014952.1.6",
          "form": "난 요즘 오트라떼에 꽂힘",
          "original_form": "난 요즘 오트라떼에 꽂힘",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014952.1.7",
          "form": "이름만 보면 맛있는줄 모르겠어",
          "original_form": "이름만 보면 맛있는줄 모르겠어",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014952.1.8",
          "form": "오트밀 맛없거든",
          "original_form": "오트밀 맛없거든",
          "speaker_id": "2",
          "time": "20210825 21:24"
        },
        {
          "id": "MDRW2100014952.1.9",
          "form": "고소함",
          "original_form": "고소함",
          "speaker_id": "1",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014952.1.10",
          "form": "커피가 고소해서 조음",
          "original_form": "커피가 고소해서 조음",
          "speaker_id": "1",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014952.1.11",
          "form": "니도 먹어바바",
          "original_form": "니도 먹어바바",
          "speaker_id": "1",
          "time": "20210825 21:26"
        },
        {
          "id": "MDRW2100014952.1.12",
          "form": "난 커피는 화장실 자주가서 별로야",
          "original_form": "난 커피는 화장실 자주가서 별로야",
          "speaker_id": "2",
          "time": "20210825 21:32"
        },
        {
          "id": "MDRW2100014952.1.13",
          "form": "왜 그래도 맛있쟈나 신세계에 눈을 뜰거시야",
          "original_form": "왜 그래도 맛있쟈나 신세계에 눈을 뜰거시야",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014952.1.14",
          "form": "나중에 도전해볼게",
          "original_form": "나중에 도전해볼게",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014952.1.15",
          "form": "고뤠 후기 알라주기",
          "original_form": "고뤠 후기 알라주기",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100014952.1.16",
          "form": "엉 언니도 레몬에이드먹고 후기 좀",
          "original_form": "엉 언니도 레몬에이드먹고 후기 좀",
          "speaker_id": "2",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100014952.1.17",
          "form": "레모네이드먹고 후기를 남기라니 야 나도 먹어본지 오래다",
          "original_form": "레모네이드먹고 후기를 남기라니 야 나도 먹어본지 오래다",
          "speaker_id": "1",
