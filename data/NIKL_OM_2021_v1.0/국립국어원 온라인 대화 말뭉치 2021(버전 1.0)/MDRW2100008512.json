      "utterance": [
        {
          "id": "MDRW2100008512.1.1",
          "form": "우리도 담주에 비 안오는걸로 바꼈길래",
          "original_form": "우리도 담주에 비 안오는걸로 바꼈길래",
          "speaker_id": "2",
          "time": "20210822 16:19"
        },
        {
          "id": "MDRW2100008512.1.2",
          "form": "급 캠핑장 예약했다",
          "original_form": "급 캠핑장 예약했다",
          "speaker_id": "2",
          "time": "20210822 16:19"
        },
        {
          "id": "MDRW2100008512.1.3",
          "form": "오 대박 ㅋㅋㅋ",
          "original_form": "오 대박 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:19"
        },
        {
          "id": "MDRW2100008512.1.4",
          "form": "어디로?",
          "original_form": "어디로?",
          "speaker_id": "1",
          "time": "20210822 16:19"
        },
        {
          "id": "MDRW2100008512.1.5",
          "form": "좋다 좋다이ㅋㅋ",
          "original_form": "좋다 좋다이ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:19"
        },
        {
          "id": "MDRW2100008512.1.6",
          "form": "의령",
          "original_form": "의령",
          "speaker_id": "2",
          "time": "20210822 16:20"
        },
        {
          "id": "MDRW2100008512.1.7",
          "form": "설뫼캠핑장 이라고",
          "original_form": "설뫼캠핑장 이라고",
          "speaker_id": "2",
          "time": "20210822 16:20"
        },
        {
          "id": "MDRW2100008512.1.8",
          "form": "25천원 싸다 ㅎㅎ",
          "original_form": "25천원 싸다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 16:20"
        },
        {
          "id": "MDRW2100008512.1.9",
          "form": "비와서 취소했는데 날씨 바꼈네",
          "original_form": "비와서 취소했는데 날씨 바꼈네",
          "speaker_id": "2",
          "time": "20210822 16:20"
        },
        {
          "id": "MDRW2100008512.1.10",
          "form": "name3랑 2박3일",
          "original_form": "&name3&랑 2박3일",
          "speaker_id": "2",
          "time": "20210822 16:20"
        },
        {
          "id": "MDRW2100008512.1.11",
          "form": "오오 좋다ㅋㅋname3랑 또 아름다운시간 보내겠네ㅋ",
          "original_form": "오오 좋다ㅋㅋ&name3&랑 또 아름다운시간 보내겠네ㅋ",
          "speaker_id": "1",
          "time": "20210822 16:47"
        },
        {
          "id": "MDRW2100008512.1.12",
          "form": "좋구마이ㅋㅋㅋ",
          "original_form": "좋구마이ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:48"
        },
        {
          "id": "MDRW2100008512.1.13",
          "form": "우리는 이제 급하게 또 정리하고 집에 간다ㅋ",
          "original_form": "우리는 이제 급하게 또 정리하고 집에 간다ㅋ",
          "speaker_id": "1",
          "time": "20210822 16:48"
        },
        {
          "id": "MDRW2100008512.1.14",
          "form": "ㅋㅋㅋㅋㅋ 늘 바쁘노",
          "original_form": "ㅋㅋㅋㅋㅋ 늘 바쁘노",
          "speaker_id": "2",
          "time": "20210822 18:01"
        },
        {
          "id": "MDRW2100008512.1.15",
          "form": "늘 바쁘단다ㅋㅋㅋㅋㅋㅋ언니네도 바쁘구만ㅋㅋ",
          "original_form": "늘 바쁘단다ㅋㅋㅋㅋㅋㅋ언니네도 바쁘구만ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:03"
        },
        {
          "id": "MDRW2100008512.1.16",
          "form": "애들 다 씻기고왔다ㅋㅋ",
          "original_form": "애들 다 씻기고왔다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:06"
        },
        {
          "id": "MDRW2100008512.1.17",
          "form": "오늘 일찍 자면 좋겠다ㅋ",
          "original_form": "오늘 일찍 자면 좋겠다ㅋ",
          "speaker_id": "1",
          "time": "20210822 19:06"
        },
        {
          "id": "MDRW2100008512.1.18",
          "form": "ㅋㅋㅋㅋ 우리도 지금 유투브 보고있다",
          "original_form": "ㅋㅋㅋㅋ 우리도 지금 유투브 보고있다",
          "speaker_id": "2",
          "time": "20210822 19:10"
        },
        {
          "id": "MDRW2100008512.1.19",
          "form": "곧 재워야지",
          "original_form": "곧 재워야지",
          "speaker_id": "2",
          "time": "20210822 19:10"
        },
        {
          "id": "MDRW2100008512.1.20",
          "form": "재우나??",
          "original_form": "재우나??",
          "speaker_id": "2",
          "time": "20210822 20:59"
        },
        {
          "id": "MDRW2100008512.1.21",
          "form": "둘째는 자고ㅋㅋ",
          "original_form": "둘째는 자고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:59"
        },
        {
          "id": "MDRW2100008512.1.22",
          "form": "첫째는 드라마보고 잔대ㅋ",
          "original_form": "첫째는 드라마보고 잔대ㅋ",
          "speaker_id": "1",
          "time": "20210822 20:59"
        },
        {
          "id": "MDRW2100008512.1.23",
          "form": "언니는?",
          "original_form": "언니는?",
          "speaker_id": "1",
          "time": "20210822 20:59"
        },
        {
          "id": "MDRW2100008512.1.24",
          "form": "애들 자?",
          "original_form": "애들 자?",
          "speaker_id": "1",
          "time": "20210822 20:59"
        },
        {
          "id": "MDRW2100008512.1.25",
          "form": "드라마 ㅋㅋㅋ",
          "original_form": "드라마 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:30"
        },
        {
          "id": "MDRW2100008512.1.26",
          "form": "원은 자고 투는 잘랑말랑",
          "original_form": "원은 자고 투는 잘랑말랑",
          "speaker_id": "2",
          "time": "20210822 21:31"
        },
        {
          "id": "MDRW2100008512.1.27",
          "form": "ㅋㅋ  우리집 원은 라면드시고 또 놀라고ㅡㅡㅋ",
          "original_form": "ㅋㅋ  우리집 원은 라면드시고 또 놀라고ㅡㅡㅋ",
          "speaker_id": "1",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100008512.1.28",
          "form": "이제 자려고 누웠어ㅋ",
          "original_form": "이제 자려고 누웠어ㅋ",
          "speaker_id": "1",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100008512.1.29",
          "form": "ㅋㅋㅋㅋㅋㄱ 대단타 ㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㄱ 대단타 ㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210822 22:40"
        },
        {
          "id": "MDRW2100008512.1.30",
          "form": "라면 이라니 ㅋㅋㅋㅋ",
          "original_form": "라면 이라니 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:40"
        },
        {
          "id": "MDRW2100008512.1.31",
          "form": "그러니깐 이제잔다ㅋㅋㅋ",
          "original_form": "그러니깐 이제잔다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:49"
        },
        {
          "id": "MDRW2100008512.1.32",
          "form": "ㅋㅋㅋㅋㅋ 꿀잠",
          "original_form": "ㅋㅋㅋㅋㅋ 꿀잠",
          "speaker_id": "2",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100008512.1.33",
          "form": "ㅋㅋ 잘자야지ㅋㅋㅋㅋ잘 크지ㅋㅋ",
          "original_form": "ㅋㅋ 잘자야지ㅋㅋㅋㅋ잘 크지ㅋㅋ",
