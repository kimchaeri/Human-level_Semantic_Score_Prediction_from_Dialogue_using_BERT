      "utterance": [
        {
          "id": "MDRW2100021793.1.1",
          "form": "날 위해 빨리 먹어",
          "original_form": "날 위해 빨리 먹어",
          "speaker_id": "1",
          "time": "20210828 22:30"
        },
        {
          "id": "MDRW2100021793.1.2",
          "form": "왜 그래야하는지...?",
          "original_form": "왜 그래야하는지...?",
          "speaker_id": "2",
          "time": "20210828 22:31"
        },
        {
          "id": "MDRW2100021793.1.3",
          "form": "너의 궁금함과 나의 궁금함을 한 번에 충전하는 공리주의적 관점에서?",
          "original_form": "너의 궁금함과 나의 궁금함을 한 번에 충전하는 공리주의적 관점에서?",
          "speaker_id": "1",
          "time": "20210828 22:32"
        },
        {
          "id": "MDRW2100021793.1.4",
          "form": "ㅋㅋㅋ꽤 그럴듯하게 대답하는군요?",
          "original_form": "ㅋㅋㅋ꽤 그럴듯하게 대답하는군요?",
          "speaker_id": "2",
          "time": "20210828 22:33"
        },
        {
          "id": "MDRW2100021793.1.5",
          "form": "대학물먹은 사람이라구요",
          "original_form": "대학물먹은 사람이라구요",
          "speaker_id": "1",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100021793.1.6",
          "form": "여기서? 대학을??ㅋㅋㅋㅋㅋ",
          "original_form": "여기서? 대학을??ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100021793.1.7",
          "form": "치킨대학나오셨나",
          "original_form": "치킨대학나오셨나",
          "speaker_id": "2",
          "time": "20210828 22:35"
        },
        {
          "id": "MDRW2100021793.1.8",
          "form": "명예 졸업생할게",
          "original_form": "명예 졸업생할게",
          "speaker_id": "1",
          "time": "20210828 22:35"
        },
        {
          "id": "MDRW2100021793.1.9",
          "form": "명예졸업생님 낼도 치킨 드세요",
          "original_form": "명예졸업생님 낼도 치킨 드세요",
          "speaker_id": "2",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100021793.1.10",
          "form": "낼은 할머니 들어와서 한식 먹어요",
          "original_form": "낼은 할머니 들어와서 한식 먹어요",
          "speaker_id": "1",
          "time": "20210828 22:38"
        },
        {
          "id": "MDRW2100021793.1.11",
          "form": "맛있는거 해주시나요?",
          "original_form": "맛있는거 해주시나요?",
          "speaker_id": "2",
          "time": "20210828 22:39"
        },
        {
          "id": "MDRW2100021793.1.12",
          "form": "할머니 위주라 풀이 좀 많아요",
          "original_form": "할머니 위주라 풀이 좀 많아요",
          "speaker_id": "1",
          "time": "20210828 22:40"
        },
        {
          "id": "MDRW2100021793.1.13",
          "form": "안타깝네요ㅎ",
          "original_form": "안타깝네요ㅎ",
          "speaker_id": "2",
          "time": "20210828 22:40"
        },
        {
          "id": "MDRW2100021793.1.14",
          "form": "풀에 고기 싸먹으면 좋을텐데",
          "original_form": "풀에 고기 싸먹으면 좋을텐데",
          "speaker_id": "2",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021793.1.15",
          "form": "고기는 하나 나와서 살만핮니닼ㅋㅋㅋ",
          "original_form": "고기는 하나 나와서 살만핮니닼ㅋㅋㅋ",
