      "utterance": [
        {
          "id": "MDRW2100037313.1.1",
          "form": "여행가시면 주로 어떤 쇼핑을 하시나요?",
          "original_form": "여행가시면 주로 어떤 쇼핑을 하시나요?",
          "speaker_id": "1",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100037313.1.2",
          "form": "주로...먹는거 사오죠 기념품도 먹는걸로 뿌려요!",
          "original_form": "주로...먹는거 사오죠 기념품도 먹는걸로 뿌려요!",
          "speaker_id": "2",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100037313.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100037313.1.4",
          "form": "와우 정말 잘 맞는거 같네요ㅋㅋ",
          "original_form": "와우 정말 잘 맞는거 같네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100037313.1.5",
          "form": "무조건 지역 특산물이죠!ㅋㅋㅋ",
          "original_form": "무조건 지역 특산물이죠!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100037313.1.6",
          "form": "ㅎㅎ 여행 가서 산 특산물중 특히 맘에  드는게 있었나요.?",
          "original_form": "ㅎㅎ 여행 가서 산 특산물중 특히 맘에  드는게 있었나요.?",
          "speaker_id": "2",
          "time": "20210901 11:38"
        },
        {
          "id": "MDRW2100037313.1.7",
          "form": "여수에 젓갈요!",
          "original_form": "여수에 젓갈요!",
          "speaker_id": "1",
          "time": "20210901 11:39"
        },
        {
          "id": "MDRW2100037313.1.8",
          "form": "다시마쌈먹을때 진짜 맛있더라구요",
          "original_form": "다시마쌈먹을때 진짜 맛있더라구요",
          "speaker_id": "1",
          "time": "20210901 11:39"
        },
        {
          "id": "MDRW2100037313.1.9",
          "form": "오오 여수 진짜 맛있는 동네죠",
          "original_form": "오오 여수 진짜 맛있는 동네죠",
          "speaker_id": "2",
          "time": "20210901 11:39"
        },
        {
          "id": "MDRW2100037313.1.10",
          "form": "무슨 젓갈 사셨어요?",
          "original_form": "무슨 젓갈 사셨어요?",
          "speaker_id": "2",
          "time": "20210901 11:39"
        },
        {
          "id": "MDRW2100037313.1.11",
          "form": "낙지도 사고 속젓류로 샀어요",
          "original_form": "낙지도 사고 속젓류로 샀어요",
          "speaker_id": "1",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100037313.1.12",
          "form": "진짜 맛있어요ㅋㅋ",
          "original_form": "진짜 맛있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100037313.1.13",
          "form": "말 들으니 침 고이네요ㅎ",
          "original_form": "말 들으니 침 고이네요ㅎ",
          "speaker_id": "2",
          "time": "20210901 11:44"
        },
        {
          "id": "MDRW2100037313.1.14",
          "form": "저 가장 맛있던 여행지가 여수와 부산이었거든요",
          "original_form": "저 가장 맛있던 여행지가 여수와 부산이었거든요",
          "speaker_id": "2",
          "time": "20210901 11:44"
        },
        {
          "id": "MDRW2100037313.1.15",
          "form": "씁 부산은 뭐가 맛있으셨어요?",
          "original_form": "씁 부산은 뭐가 맛있으셨어요?",
          "speaker_id": "1",
          "time": "20210901 11:44"
        },
        {
          "id": "MDRW2100037313.1.16",
          "form": "심지어  여수서 통장어탕 사왔어요ㅎㅎ",
          "original_form": "심지어  여수서 통장어탕 사왔어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 11:45"
        },
        {
          "id": "MDRW2100037313.1.17",
          "form": "제가 부산살아서ㅋㅋ",
          "original_form": "제가 부산살아서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:45"
        },
        {
          "id": "MDRW2100037313.1.18",
          "form": "맛있겠다ㅜㅜ",
          "original_form": "맛있겠다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 11:45"
        },
        {
          "id": "MDRW2100037313.1.19",
          "form": "오오 저 부산은 꼼장어구이 백화양곱창 회센터",
          "original_form": "오오 저 부산은 꼼장어구이 백화양곱창 회센터",
          "speaker_id": "2",
          "time": "20210901 11:46"
        },
        {
          "id": "MDRW2100037313.1.20",
          "form": "여기 메인으로 가요",
          "original_form": "여기 메인으로 가요",
          "speaker_id": "2",
          "time": "20210901 11:46"
        },
        {
          "id": "MDRW2100037313.1.21",
          "form": "그리고 차이나타운 신발원?",
          "original_form": "그리고 차이나타운 신발원?",
          "speaker_id": "2",
          "time": "20210901 11:46"
        },
        {
          "id": "MDRW2100037313.1.22",
          "form": "어묵 사가고요ㅎ",
          "original_form": "어묵 사가고요ㅎ",
          "speaker_id": "2",
          "time": "20210901 11:48"
        },
        {
          "id": "MDRW2100037313.1.23",
          "form": "양곱창 소금구이 땡기네요ㅋㅋㄱ",
          "original_form": "양곱창 소금구이 땡기네요ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210901 11:48"
        },
        {
          "id": "MDRW2100037313.1.24",
          "form": "신발원은 요새 다시 대기가 많아졌어요",
          "original_form": "신발원은 요새 다시 대기가 많아졌어요",
          "speaker_id": "1",
          "time": "20210901 11:48"
        }
      ]
