        },
        {
          "id": "MDRW2100041683.1.2",
          "form": "기력도 없고 컵라면이나 먹을까 싶네ㅠ",
          "original_form": "기력도 없고 컵라면이나 먹을까 싶네ㅠ",
          "speaker_id": "2",
          "time": "20210906 14:33"
        },
        {
          "id": "MDRW2100041683.1.3",
          "form": "격하게 아무것도 하기 싫다ㅋㅋㅋ",
          "original_form": "격하게 아무것도 하기 싫다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:33"
        },
        {
          "id": "MDRW2100041683.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 ㅋㅋㅋ 현미 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 ㅋㅋㅋ 현미 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:33"
        },
        {
          "id": "MDRW2100041683.1.5",
          "form": "지금 널부러져 있다ㅋㅋㅋㅋ",
          "original_form": "지금 널부러져 있다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041683.1.6",
          "form": "와 진짜 백신 부터 해서",
          "original_form": "와 진짜 백신 부터 해서",
          "speaker_id": "2",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041683.1.7",
          "form": "그래 name2야 ㅋㅋ 오늘 놔줄게 ㅋㅋ",
          "original_form": "그래 &name2&야 ㅋㅋ 오늘 놔줄게 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041683.1.8",
          "form": "뭐 일이 바쁘고",
          "original_form": "뭐 일이 바쁘고",
          "speaker_id": "2",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041683.1.9",
          "form": "충전하고 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "충전하고 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041683.1.10",
          "form": "진짜 진이 다 빠지제 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "진짜 진이 다 빠지제 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041683.1.11",
          "form": "어 회복이 안 되네ㅋㅋㅋㅋ",
          "original_form": "어 회복이 안 되네ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041683.1.12",
          "form": "그렇지만",
          "original_form": "그렇지만",
          "speaker_id": "2",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041683.1.13",
          "form": "홧팅해라 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "홧팅해라 ㅋㅋㅋㅋㅋㅋㅋㅋ",
