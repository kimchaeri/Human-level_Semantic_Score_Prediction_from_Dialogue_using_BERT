      "utterance": [
        {
          "id": "MDRW2100012561.1.1",
          "form": "책 좋아하시느용",
          "original_form": "책 좋아하시느용",
          "speaker_id": "1",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100012561.1.2",
          "form": "좋아하진 않아요ㅋㅋ",
          "original_form": "좋아하진 않아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100012561.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100012561.1.4",
          "form": "전 원래 자주 읽다가",
          "original_form": "전 원래 자주 읽다가",
          "speaker_id": "1",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100012561.1.5",
          "form": "요새 안읽고 있어요ㅋㅋㅋㅋㅋ",
          "original_form": "요새 안읽고 있어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100012561.1.6",
          "form": "저는 그나마 재테크 관련 책읽어요",
          "original_form": "저는 그나마 재테크 관련 책읽어요",
          "speaker_id": "2",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100012561.1.7",
          "form": "부자되고싶어서ㅋㅋ",
          "original_form": "부자되고싶어서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100012561.1.8",
          "form": "아 어떤 거 읽으시나요",
          "original_form": "아 어떤 거 읽으시나요",
          "speaker_id": "1",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100012561.1.9",
          "form": "추천해주세요 ㅋㅋㅋㅋㅋ",
          "original_form": "추천해주세요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100012561.1.10",
          "form": "뉴욕주민 책이랑",
          "original_form": "뉴욕주민 책이랑",
          "speaker_id": "2",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100012561.1.11",
          "form": "차트의 기술",
          "original_form": "차트의 기술",
          "speaker_id": "2",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100012561.1.12",
          "form": "엘리어트 파동이론 읽었어요",
          "original_form": "엘리어트 파동이론 읽었어요",
          "speaker_id": "2",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100012561.1.13",
          "form": "오 도움 잘되나요?",
          "original_form": "오 도움 잘되나요?",
          "speaker_id": "1",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100012561.1.14",
          "form": "저도 부자가 되고싶네유",
          "original_form": "저도 부자가 되고싶네유",
          "speaker_id": "1",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100012561.1.15",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100012561.1.16",
          "form": "읽기만해서는 택도없고",
          "original_form": "읽기만해서는 택도없고",
          "speaker_id": "2",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100012561.1.17",
          "form": "공부해야해요ㅋㅋ",
          "original_form": "공부해야해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100012561.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ 공부 어렵더라구요 ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 공부 어렵더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100012561.1.19",
          "form": "좀 도움이 되고 있어요",
          "original_form": "좀 도움이 되고 있어요",
          "speaker_id": "2",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100012561.1.20",
          "form": "책으로 제태크 공부가 어려워요",
          "original_form": "책으로 제태크 공부가 어려워요",
          "speaker_id": "1",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100012561.1.21",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100012561.1.22",
          "form": "name1은 인상깊게 읽은책 있으신가요?",
          "original_form": "&name1&은 인상깊게 읽은책 있으신가요?",
          "speaker_id": "2",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100012561.1.23",
          "form": "음 인상깊게는 아니고",
          "original_form": "음 인상깊게는 아니고",
          "speaker_id": "1",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100012561.1.24",
          "form": "저 디어마이프렌즈 그거 읽고",
          "original_form": "저 디어마이프렌즈 그거 읽고",
          "speaker_id": "1",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100012561.1.25",
          "form": "울었어요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "울었어요ㅋㅋㅋㅋㅋㅋㅋ",
