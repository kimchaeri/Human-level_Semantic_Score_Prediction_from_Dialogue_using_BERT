      "utterance": [
        {
          "id": "MDRW2100002443.1.1",
          "form": "이북리더기 산 이후로",
          "original_form": "이북리더기 산 이후로",
          "speaker_id": "1",
          "time": "20210820 12:18"
        },
        {
          "id": "MDRW2100002443.1.2",
          "form": "그나마 책을 많이읽게되는데",
          "original_form": "그나마 책을 많이읽게되는데",
          "speaker_id": "1",
          "time": "20210820 12:18"
        },
        {
          "id": "MDRW2100002443.1.3",
          "form": "다 로판인거같아요",
          "original_form": "다 로판인거같아요",
          "speaker_id": "1",
          "time": "20210820 12:18"
        },
        {
          "id": "MDRW2100002443.1.4",
          "form": "로판이 로맨스판타지란 뜻인가요?",
          "original_form": "로판이 로맨스판타지란 뜻인가요?",
          "speaker_id": "2",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100002443.1.5",
          "form": "네 맞아요!!!",
          "original_form": "네 맞아요!!!",
          "speaker_id": "1",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100002443.1.6",
          "form": "보통 여성이 주인공인",
          "original_form": "보통 여성이 주인공인",
          "speaker_id": "1",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100002443.1.7",
          "form": "판타지소설입니다",
          "original_form": "판타지소설입니다",
          "speaker_id": "1",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100002443.1.8",
          "form": "전 요즘 책 안 읽은지 1년 넘은것 같아요 ㅠ ㅠ",
          "original_form": "전 요즘 책 안 읽은지 1년 넘은것 같아요 ㅠ ㅠ",
          "speaker_id": "2",
          "time": "20210820 12:20"
        },
        {
          "id": "MDRW2100002443.1.9",
          "form": "마지막에 읽은 책이 뭔가요???",
          "original_form": "마지막에 읽은 책이 뭔가요???",
          "speaker_id": "1",
          "time": "20210820 12:24"
        },
        {
          "id": "MDRW2100002443.1.10",
          "form": "나미야의 잡화점인가? 그거 읽었던 것 같아요",
          "original_form": "나미야의 잡화점인가? 그거 읽었던 것 같아요",
          "speaker_id": "2",
          "time": "20210820 12:28"
        },
        {
          "id": "MDRW2100002443.1.11",
          "form": "히가시노 게이고 소설 맞죠??",
          "original_form": "히가시노 게이고 소설 맞죠??",
          "speaker_id": "1",
          "time": "20210820 12:34"
        },
        {
          "id": "MDRW2100002443.1.12",
          "form": "그 작가는 시리즈가 너무 많아섴ㅋㅋ",
          "original_form": "그 작가는 시리즈가 너무 많아섴ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:34"
        },
        {
          "id": "MDRW2100002443.1.13",
          "form": "이번에 또 신간낸대요",
          "original_form": "이번에 또 신간낸대요",
          "speaker_id": "1",
          "time": "20210820 12:34"
        },
        {
          "id": "MDRW2100002443.1.14",
          "form": "아 진짜요? ㅋㅋㅋ막 그렇게 와닿지는 않은거 같은데",
          "original_form": "아 진짜요? ㅋㅋㅋ막 그렇게 와닿지는 않은거 같은데",
          "speaker_id": "2",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002443.1.15",
          "form": "전 추리소설좋아해요!",
          "original_form": "전 추리소설좋아해요!",
          "speaker_id": "2",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002443.1.16",
          "form": "소설보다는 만화? 이런거요 ㅋㅋㅋ",
          "original_form": "소설보다는 만화? 이런거요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002443.1.17",
          "form": "만화 어떤거보세요??!",
          "original_form": "만화 어떤거보세요??!",
          "speaker_id": "1",
          "time": "20210820 13:03"
        },
        {
          "id": "MDRW2100002443.1.18",
          "form": "주로 명탐정코난이요 ㅋㄱㄱ",
          "original_form": "주로 명탐정코난이요 ㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210820 13:45"
        },
        {
          "id": "MDRW2100002443.1.19",
          "form": "꿀잼",
          "original_form": "꿀잼",
          "speaker_id": "2",
          "time": "20210820 13:45"
        },
        {
          "id": "MDRW2100002443.1.20",
          "form": "코난재밌죠ㅠㅠㅠㅠ",
          "original_form": "코난재밌죠ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 13:47"
        },
        {
          "id": "MDRW2100002443.1.21",
          "form": "예전에 만화책 모으다가",
          "original_form": "예전에 만화책 모으다가",
          "speaker_id": "1",
          "time": "20210820 13:47"
        },
        {
          "id": "MDRW2100002443.1.22",
          "form": "70권 넘어가서 포기...",
          "original_form": "70권 넘어가서 포기...",
          "speaker_id": "1",
          "time": "20210820 13:47"
        },
        {
          "id": "MDRW2100002443.1.23",
          "form": "와우 ㅋㅋㅋ전 거의 만화방가서 빌려봐요",
          "original_form": "와우 ㅋㅋㅋ전 거의 만화방가서 빌려봐요",
