      "utterance": [
        {
          "id": "MDRW2100004998.1.1",
          "form": "무슨게임 좋아해요?",
          "original_form": "무슨게임 좋아해요?",
          "speaker_id": "1",
          "time": "20210821 19:51"
        },
        {
          "id": "MDRW2100004998.1.2",
          "form": "저 배틀그라운드같은 FPS나 롤 같은 게임이요!",
          "original_form": "저 배틀그라운드같은 FPS나 롤 같은 게임이요!",
          "speaker_id": "2",
          "time": "20210821 20:08"
        },
        {
          "id": "MDRW2100004998.1.3",
          "form": "얘기만 들어도 어려워보여요",
          "original_form": "얘기만 들어도 어려워보여요",
          "speaker_id": "1",
          "time": "20210821 20:09"
        },
        {
          "id": "MDRW2100004998.1.4",
          "form": "배틀그라운드는 뭐예요? 총쏘는건가요?",
          "original_form": "배틀그라운드는 뭐예요? 총쏘는건가요?",
          "speaker_id": "1",
          "time": "20210821 20:10"
        },
        {
          "id": "MDRW2100004998.1.5",
          "form": "네! 총쏘는 게임인데",
          "original_form": "네! 총쏘는 게임인데",
          "speaker_id": "2",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100004998.1.6",
          "form": "엄청 어렵긴하죠",
          "original_form": "엄청 어렵긴하죠",
          "speaker_id": "2",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100004998.1.7",
          "form": "스트레스는 풀리겠는데요",
          "original_form": "스트레스는 풀리겠는데요",
          "speaker_id": "1",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100004998.1.8",
          "form": "3D 울렁증 같은거 있으시면 좀 힘드시긴 할거에요",
          "original_form": "3D 울렁증 같은거 있으시면 좀 힘드시긴 할거에요",
          "speaker_id": "2",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100004998.1.9",
          "form": "스트레스 엄청 풀리고 재밌긴해요!",
          "original_form": "스트레스 엄청 풀리고 재밌긴해요!",
          "speaker_id": "2",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100004998.1.10",
          "form": "그런건 없어서 상관은 없을 것같은데",
          "original_form": "그런건 없어서 상관은 없을 것같은데",
          "speaker_id": "1",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100004998.1.11",
          "form": "좀 폭력적일까봐서요",
          "original_form": "좀 폭력적일까봐서요",
          "speaker_id": "1",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100004998.1.12",
          "form": "아무래도 총게임이니",
          "original_form": "아무래도 총게임이니",
          "speaker_id": "2",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100004998.1.13",
          "form": "폭력적이긴하죠",
          "original_form": "폭력적이긴하죠",
          "speaker_id": "2",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100004998.1.14",
          "form": "피도 나오구요",
          "original_form": "피도 나오구요",
          "speaker_id": "2",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100004998.1.15",
          "form": "잔인한건 좋아하긴해요ㅎㅎ",
          "original_form": "잔인한건 좋아하긴해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 21:03"
        },
        {
          "id": "MDRW2100004998.1.16",
          "form": "보는건 문제가 없을듯요",
          "original_form": "보는건 문제가 없을듯요",
          "speaker_id": "1",
          "time": "20210821 21:03"
        },
        {
          "id": "MDRW2100004998.1.17",
          "form": "스타크래프트 이런건 안하시나요?",
          "original_form": "스타크래프트 이런건 안하시나요?",
          "speaker_id": "1",
          "time": "20210821 21:04"
        },
        {
          "id": "MDRW2100004998.1.18",
          "form": "스타는 옛날에 자주했었어요!",
          "original_form": "스타는 옛날에 자주했었어요!",
          "speaker_id": "2",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100004998.1.19",
          "form": "지금은 안하는편",
          "original_form": "지금은 안하는편",
          "speaker_id": "2",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100004998.1.20",
          "form": "요즘은 보러도 많이 가나보던데",
          "original_form": "요즘은 보러도 많이 가나보던데",
          "speaker_id": "1",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100004998.1.21",
          "form": "이제 비제이쪽으로 유명해져서",
          "original_form": "이제 비제이쪽으로 유명해져서",
          "speaker_id": "2",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100004998.1.22",
          "form": "조금씩 다시 올라오긴하는거같아요",
          "original_form": "조금씩 다시 올라오긴하는거같아요",
          "speaker_id": "2",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100004998.1.23",
          "form": "아이템도 사고 그러세요 현질이라고 하나요ㅎㅎ",
          "original_form": "아이템도 사고 그러세요 현질이라고 하나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100004998.1.24",
          "form": "아 이미",
          "original_form": "아 이미",
          "speaker_id": "2",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100004998.1.25",
          "form": "현질은 좀 했습니다 ㅎㅎ",
          "original_form": "현질은 좀 했습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100004998.1.26",
          "form": "이쁜건 사야죠",
          "original_form": "이쁜건 사야죠",
          "speaker_id": "2",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100004998.1.27",
          "form": "ㅎㅎ 이쁜게 뭔가요? 장비사나요? 옷을사나",
          "original_form": "ㅎㅎ 이쁜게 뭔가요? 장비사나요? 옷을사나",
          "speaker_id": "1",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100004998.1.28",
          "form": "옷! 스킨이라고 하죠",
          "original_form": "옷! 스킨이라고 하죠",
          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100004998.1.29",
          "form": "옷도 사고",
          "original_form": "옷도 사고",
          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100004998.1.30",
          "form": "총 스킨같은것도 사고 그랬어요 ㅎㅎ",
          "original_form": "총 스킨같은것도 사고 그랬어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100004998.1.31",
          "form": "아이템도 팔고 그러시나요?",
          "original_form": "아이템도 팔고 그러시나요?",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100004998.1.32",
          "form": "아뇨! 팔거나 그러진않아요 소장용이나 사용 ㅎㅎ",
          "original_form": "아뇨! 팔거나 그러진않아요 소장용이나 사용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100004998.1.33",
          "form": "그렇군요 팔아서 재테크도 하시던데",
          "original_form": "그렇군요 팔아서 재테크도 하시던데",
          "speaker_id": "1",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100004998.1.34",
          "form": "아하 그런 종류는 주로 RPG죠!",
          "original_form": "아하 그런 종류는 주로 RPG죠!",
          "speaker_id": "2",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100004998.1.35",
          "form": "전 RPG는 잘 안해서 ㅎㅎ",
          "original_form": "전 RPG는 잘 안해서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100004998.1.36",
          "form": "아  게임세계도 복잡하군요",
          "original_form": "아  게임세계도 복잡하군요",
          "speaker_id": "1",
          "time": "20210821 21:44"
        }
      ]
