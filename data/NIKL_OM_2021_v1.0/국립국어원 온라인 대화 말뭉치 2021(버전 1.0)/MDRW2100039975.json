      "utterance": [
        {
          "id": "MDRW2100039975.1.1",
          "form": "부대찌개를 집에서 파는양념 안넣고",
          "original_form": "부대찌개를 집에서 파는양념 안넣고",
          "speaker_id": "1",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100039975.1.2",
          "form": "만들면 맛이 어때요?",
          "original_form": "만들면 맛이 어때요?",
          "speaker_id": "1",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100039975.1.3",
          "form": "부대찌개에 그 통조림콩이 결정적인 포인트래요",
          "original_form": "부대찌개에 그 통조림콩이 결정적인 포인트래요",
          "speaker_id": "2",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100039975.1.4",
          "form": "그 차이로 햄김치찌개와",
          "original_form": "그 차이로 햄김치찌개와",
          "speaker_id": "2",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100039975.1.5",
          "form": "부대찌개로 나뉜다고ㅎㅎ",
          "original_form": "부대찌개로 나뉜다고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100039975.1.6",
          "form": "아아 그렇군요 그럼 그 통조림은 시중에서 못사요?",
          "original_form": "아아 그렇군요 그럼 그 통조림은 시중에서 못사요?",
          "speaker_id": "1",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100039975.1.7",
          "form": "아뇨! 판대요!",
          "original_form": "아뇨! 판대요!",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039975.1.8",
          "form": "사실 저도 안사봐서ㅋㅋ",
          "original_form": "사실 저도 안사봐서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039975.1.9",
          "form": "앗 다행이네요",
          "original_form": "앗 다행이네요",
          "speaker_id": "1",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100039975.1.10",
          "form": "집에서 맛있게 할 수 있네요",
          "original_form": "집에서 맛있게 할 수 있네요",
          "speaker_id": "1",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100039975.1.11",
          "form": "네ㅋㅋ 대량으로 무한흡입가능해요",
          "original_form": "네ㅋㅋ 대량으로 무한흡입가능해요",
          "speaker_id": "2",
          "time": "20210904 00:11"
        },
        {
          "id": "MDRW2100039975.1.12",
          "form": "족발도 좋아하세요?",
          "original_form": "족발도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 00:14"
        },
        {
          "id": "MDRW2100039975.1.13",
          "form": "말하니 먹고싶어지네요 ㅜㅜ",
          "original_form": "말하니 먹고싶어지네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 00:14"
        },
        {
          "id": "MDRW2100039975.1.14",
          "form": "족발 최고죠",
          "original_form": "족발 최고죠",
          "speaker_id": "2",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100039975.1.15",
          "form": "닭발도 드시나요?",
          "original_form": "닭발도 드시나요?",
          "speaker_id": "2",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100039975.1.16",
          "form": "넹 닭발 안먹는 사람은",
          "original_form": "넹 닭발 안먹는 사람은",
          "speaker_id": "1",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100039975.1.17",
          "form": "왜안먹는거죠? ㅋㅋ",
          "original_form": "왜안먹는거죠? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100039975.1.18",
          "form": "ㅋㅋ 진짜요! 닭발에 주먹밥ㅜㅠ",
          "original_form": "ㅋㅋ 진짜요! 닭발에 주먹밥ㅜㅠ",
          "speaker_id": "2",
          "time": "20210904 07:27"
        },
        {
          "id": "MDRW2100039975.1.19",
          "form": "네 닭발 맛있어요",
          "original_form": "네 닭발 맛있어요",
          "speaker_id": "1",
          "time": "20210904 10:55"
        },
        {
          "id": "MDRW2100039975.1.20",
          "form": "토요일이니까 또 맛난거 먹어야죠!",
          "original_form": "토요일이니까 또 맛난거 먹어야죠!",
          "speaker_id": "2",
          "time": "20210904 11:18"
        },
        {
          "id": "MDRW2100039975.1.21",
          "form": "ㅎㅎㅎ 그러게요",
          "original_form": "ㅎㅎㅎ 그러게요",
          "speaker_id": "1",
          "time": "20210904 13:03"
        },
        {
          "id": "MDRW2100039975.1.22",
          "form": "이제서야 일어났어요",
          "original_form": "이제서야 일어났어요",
          "speaker_id": "1",
          "time": "20210904 13:03"
        }
