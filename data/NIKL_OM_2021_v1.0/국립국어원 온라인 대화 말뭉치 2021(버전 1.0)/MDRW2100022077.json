      "utterance": [
        {
          "id": "MDRW2100022077.1.1",
          "form": "저는 안먹어봤는데\n반응이 좋더라고요 ㅎㅎ",
          "original_form": "저는 안먹어봤는데\n반응이 좋더라고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 02:16"
        },
        {
          "id": "MDRW2100022077.1.2",
          "form": "오 검색해봤는데",
          "original_form": "오 검색해봤는데",
          "speaker_id": "1",
          "time": "20210829 02:17"
        },
        {
          "id": "MDRW2100022077.1.3",
          "form": "되게 저렴하네요",
          "original_form": "되게 저렴하네요",
          "speaker_id": "1",
          "time": "20210829 02:17"
        },
        {
          "id": "MDRW2100022077.1.4",
          "form": "간혹 할인쿠폰도 뿌린대서 동생이랑 먹어보려고요 ㅎㅎ",
          "original_form": "간혹 할인쿠폰도 뿌린대서 동생이랑 먹어보려고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 02:18"
        },
        {
          "id": "MDRW2100022077.1.5",
          "form": "저희집은 엄마가 라면스프랑 햄넣고 끓여주시는걸 주로먹는데\n맛잇더라고여 ㅎㅎ 고기들어간 김치찌개보다 좋아해요 ㅋㅋ",
          "original_form": "저희집은 엄마가 라면스프랑 햄넣고 끓여주시는걸 주로먹는데\n맛잇더라고여 ㅎㅎ 고기들어간 김치찌개보다 좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:19"
        },
        {
          "id": "MDRW2100022077.1.6",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "1",
          "time": "20210829 02:21"
        },
        {
          "id": "MDRW2100022077.1.7",
          "form": "그럼 라면맛만 나지 않나여?",
          "original_form": "그럼 라면맛만 나지 않나여?",
          "speaker_id": "1",
          "time": "20210829 02:21"
        },
        {
          "id": "MDRW2100022077.1.8",
          "form": "조미료맛이 최고긴 해요ㅠㅠ",
          "original_form": "조미료맛이 최고긴 해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 02:21"
        },
        {
          "id": "MDRW2100022077.1.9",
          "form": "ㅋㅋㅋㅋ 라면 반개만넣어서 ㅋㅋㅋ 알찬맛이나요 ㅋㅋㅋㅋ 조미료는 무조건옳죠 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 라면 반개만넣어서 ㅋㅋㅋ 알찬맛이나요 ㅋㅋㅋㅋ 조미료는 무조건옳죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:22"
        },
        {
          "id": "MDRW2100022077.1.10",
          "form": "ㅋㅋㅋㅋ아 배고파졌어요",
          "original_form": "ㅋㅋㅋㅋ아 배고파졌어요",
          "speaker_id": "1",
          "time": "20210829 02:24"
        },
        {
          "id": "MDRW2100022077.1.11",
          "form": "부대찌개 먹고싶다 따흐흑...",
          "original_form": "부대찌개 먹고싶다 따흐흑...",
          "speaker_id": "1",
          "time": "20210829 02:24"
        },
        {
          "id": "MDRW2100022077.1.12",
          "form": "저 아까 소화안됫엇는데\n치킨생각하면 배고프고..\n피자생각하면 더 배고프네요 ㅋㅋ",
          "original_form": "저 아까 소화안됫엇는데\n치킨생각하면 배고프고..\n피자생각하면 더 배고프네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:25"
        },
        {
          "id": "MDRW2100022077.1.13",
          "form": "앗 저 어제",
          "original_form": "앗 저 어제",
          "speaker_id": "1",
          "time": "20210829 02:26"
        },
        {
          "id": "MDRW2100022077.1.14",
          "form": "피자헛 먹엇어요 호호호호",
          "original_form": "피자헛 먹엇어요 호호호호",
          "speaker_id": "1",
          "time": "20210829 02:26"
        },
        {
          "id": "MDRW2100022077.1.15",
          "form": "아주 맛있었어요",
          "original_form": "아주 맛있었어요",
          "speaker_id": "1",
          "time": "20210829 02:27"
        },
        {
          "id": "MDRW2100022077.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ 오 무슨메뉴이신가요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 오 무슨메뉴이신가요",
          "speaker_id": "2",
          "time": "20210829 02:27"
        },
        {
          "id": "MDRW2100022077.1.17",
          "form": "뭐얐지... 프리미엄중에",
          "original_form": "뭐얐지... 프리미엄중에",
          "speaker_id": "1",
          "time": "20210829 02:27"
        },
        {
          "id": "MDRW2100022077.1.18",
          "form": "직화불고긴가...",
          "original_form": "직화불고긴가...",
          "speaker_id": "1",
          "time": "20210829 02:27"
        },
        {
          "id": "MDRW2100022077.1.19",
          "form": "ㅠㅠㅠ 피자는 페파로니랑 불고기가 제일 좋아요",
          "original_form": "ㅠㅠㅠ 피자는 페파로니랑 불고기가 제일 좋아요",
          "speaker_id": "1",
          "time": "20210829 02:28"
        },
        {
          "id": "MDRW2100022077.1.20",
          "form": "오 크러스트는요??",
          "original_form": "오 크러스트는요??",
          "speaker_id": "2",
          "time": "20210829 02:28"
        },
        {
          "id": "MDRW2100022077.1.21",
          "form": "그거였어요 리치골드!!!",
          "original_form": "그거였어요 리치골드!!!",
          "speaker_id": "1",
          "time": "20210829 02:28"
        },
        {
          "id": "MDRW2100022077.1.22",
          "form": "크러스트가 더 좋은데 기본옵션이 리치골드더라고요ㅠ",
          "original_form": "크러스트가 더 좋은데 기본옵션이 리치골드더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210829 02:28"
        },
        {
          "id": "MDRW2100022077.1.23",
          "form": "오 그러시구나..\n다른브랜드 피자 뭐드세여?",
          "original_form": "오 그러시구나..\n다른브랜드 피자 뭐드세여?",
          "speaker_id": "2",
          "time": "20210829 02:28"
        },
        {
          "id": "MDRW2100022077.1.24",
          "form": "리치골드 너무맛잇죠",
          "original_form": "리치골드 너무맛잇죠",
          "speaker_id": "2",
          "time": "20210829 02:28"
        },
        {
          "id": "MDRW2100022077.1.25",
          "form": "다른 브랜드는",
          "original_form": "다른 브랜드는",
          "speaker_id": "1",
          "time": "20210829 02:29"
        },
        {
          "id": "MDRW2100022077.1.26",
          "form": "도미노 아니면",
          "original_form": "도미노 아니면",
          "speaker_id": "1",
          "time": "20210829 02:29"
        },
        {
          "id": "MDRW2100022077.1.27",
          "form": "피자나라치킨공주도 맛있더라고요...",
          "original_form": "피자나라치킨공주도 맛있더라고요...",
          "speaker_id": "1",
          "time": "20210829 02:29"
        },
        {
          "id": "MDRW2100022077.1.28",
          "form": "드셔보셨나요..?",
          "original_form": "드셔보셨나요..?",
          "speaker_id": "1",
          "time": "20210829 02:29"
        }
      ]
    }
