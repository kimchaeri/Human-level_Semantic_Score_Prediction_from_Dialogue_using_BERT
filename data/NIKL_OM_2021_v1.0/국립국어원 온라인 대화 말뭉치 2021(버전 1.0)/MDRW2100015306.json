      "utterance": [
        {
          "id": "MDRW2100015306.1.1",
          "form": "뮤지컬좋아하시나요?",
          "original_form": "뮤지컬좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015306.1.2",
          "form": "네 좋아하는데 올해는 거의 못봤네요ㅠㅠ",
          "original_form": "네 좋아하는데 올해는 거의 못봤네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015306.1.3",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015306.1.4",
          "form": "여름은 이블데드인데",
          "original_form": "여름은 이블데드인데",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015306.1.5",
          "form": "오 재밌나요? 저는 사실 거의 입문이어서ㅠㅜ",
          "original_form": "오 재밌나요? 저는 사실 거의 입문이어서ㅠㅜ",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015306.1.6",
          "form": "저도 기대하는 캐스팅있으면 보는 편이라 잘은 모르는데",
          "original_form": "저도 기대하는 캐스팅있으면 보는 편이라 잘은 모르는데",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100015306.1.7",
          "form": "이블데드는 앞좌석은 여분 옷 들고가야 해요",
          "original_form": "이블데드는 앞좌석은 여분 옷 들고가야 해요",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015306.1.8",
          "form": "막 내려와서 피 뿌려요",
          "original_form": "막 내려와서 피 뿌려요",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015306.1.9",
          "form": "아 들은거같아요ㅋㅋㅋㅋ친구가 보러가자했던ㅎㅎ",
          "original_form": "아 들은거같아요ㅋㅋㅋㅋ친구가 보러가자했던ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015306.1.10",
          "form": "그러나 다 추억",
          "original_form": "그러나 다 추억",
          "speaker_id": "1",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015306.1.11",
          "form": "코로롱때문에 ㅜㅠ",
          "original_form": "코로롱때문에 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015306.1.12",
          "form": "그니까요...직장때문에 걸리면 안되서 정말 못가서 아쉬워요ㅠㅠ",
          "original_form": "그니까요...직장때문에 걸리면 안되서 정말 못가서 아쉬워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015306.1.13",
          "form": "진짜 심할땐 주말에 하루에 두편 보곤 했는데 ㅜㅠ",
          "original_form": "진짜 심할땐 주말에 하루에 두편 보곤 했는데 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015306.1.14",
          "form": "이젠 다 추억이네요",
          "original_form": "이젠 다 추억이네요",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015306.1.15",
          "form": "오 저는 주중에 보구 주말에 본적은 있어요ㅠㅠㅋㅋㅋ",
          "original_form": "오 저는 주중에 보구 주말에 본적은 있어요ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015306.1.16",
          "form": "주중에는 거리가 있어서",
          "original_form": "주중에는 거리가 있어서",
          "speaker_id": "1",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015306.1.17",
          "form": "아 저는 티켓팅을 못해서 주중표만 자꾸 구하게 되더라구요ㅠㅠㅋㅋ",
          "original_form": "아 저는 티켓팅을 못해서 주중표만 자꾸 구하게 되더라구요ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015306.1.18",
          "form": "맞아요 뮤지컬도 티켓팅 이 피켓팅이여서",
          "original_form": "맞아요 뮤지컬도 티켓팅 이 피켓팅이여서",
          "speaker_id": "1",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015306.1.19",
          "form": "전 새벽에 취켓팅 많이 했어요",
          "original_form": "전 새벽에 취켓팅 많이 했어요",
          "speaker_id": "1",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015306.1.20",
          "form": "저는 너무 힘들더라구요ㅋㅋㅋ포기하고 그냥 남는표ㅠㅠ",
          "original_form": "저는 너무 힘들더라구요ㅋㅋㅋ포기하고 그냥 남는표ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 22:49"
        }
      ]
    }
