      "utterance": [
        {
          "id": "MDRW2100044724.1.1",
          "form": "오늘 비빔밥 해 먹었어요",
          "original_form": "오늘 비빔밥 해 먹었어요",
          "speaker_id": "1",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044724.1.2",
          "form": "어떤 거 드셨어요",
          "original_form": "어떤 거 드셨어요",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044724.1.3",
          "form": "요새 고구마 줄기가 맛있더라고요",
          "original_form": "요새 고구마 줄기가 맛있더라고요",
          "speaker_id": "1",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044724.1.4",
          "form": "아삭아삭한",
          "original_form": "아삭아삭한",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044724.1.5",
          "form": "맞아요 힘내 식감도 참",
          "original_form": "맞아요 힘내 식감도 참",
          "speaker_id": "1",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044724.1.6",
          "form": "무슨 말이죠",
          "original_form": "무슨 말이죠",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044724.1.7",
          "form": "씹는 식감 이 아주 맛있어요",
          "original_form": "씹는 식감 이 아주 맛있어요",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044724.1.8",
          "form": "그렇죠 고구마 줄기랑 해서 비벼 드셨나요",
          "original_form": "그렇죠 고구마 줄기랑 해서 비벼 드셨나요",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044724.1.9",
          "form": "내 고구마 줄기랑 식은 밥 이 있어서",
          "original_form": "내 고구마 줄기랑 식은 밥 이 있어서",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044724.1.10",
          "form": "비 다 먹었어요",
          "original_form": "비 다 먹었어요",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044724.1.11",
          "form": "참기름",
          "original_form": "참기름",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044724.1.12",
          "form": "아니면 들기름",
          "original_form": "아니면 들기름",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044724.1.13",
          "form": "저는 들기름 다예요",
          "original_form": "저는 들기름 다예요",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044724.1.14",
          "form": "맞아요 들기름 참 맛있죠",
          "original_form": "맞아요 들기름 참 맛있죠",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044724.1.15",
          "form": "참기름 보다 고소한 거 같아요",
          "original_form": "참기름 보다 고소한 거 같아요",
