      "utterance": [
        {
          "id": "MDRW2100023375.1.1",
          "form": "안녕하세용 주말에 뭐 드셨어용~?",
          "original_form": "안녕하세용 주말에 뭐 드셨어용~?",
          "speaker_id": "1",
          "time": "20210830 01:07"
        },
        {
          "id": "MDRW2100023375.1.2",
          "form": "저는 치킨 먹었어용!",
          "original_form": "저는 치킨 먹었어용!",
          "speaker_id": "1",
          "time": "20210830 02:05"
        },
        {
          "id": "MDRW2100023375.1.3",
          "form": "답 늦어서 죄송합니다 저는 주말에 낙지덮밥 먹었어요~",
          "original_form": "답 늦어서 죄송합니다 저는 주말에 낙지덮밥 먹었어요~",
          "speaker_id": "2",
          "time": "20210830 13:50"
        },
        {
          "id": "MDRW2100023375.1.4",
          "form": "우왕 낙지덮밥 맛있겠네용",
          "original_form": "우왕 낙지덮밥 맛있겠네용",
          "speaker_id": "1",
          "time": "20210830 13:51"
        },
        {
          "id": "MDRW2100023375.1.5",
          "form": "매운거 잘드세요?",
          "original_form": "매운거 잘드세요?",
          "speaker_id": "1",
          "time": "20210830 13:51"
        },
        {
          "id": "MDRW2100023375.1.6",
          "form": "ㅋㅋ네 낙지가 탱글탱글하고 맛있었어요",
          "original_form": "ㅋㅋ네 낙지가 탱글탱글하고 맛있었어요",
          "speaker_id": "2",
          "time": "20210830 13:51"
        },
        {
          "id": "MDRW2100023375.1.7",
          "form": "매운거..불닭볶음면까지는 잘먹는 편이에요!!!",
          "original_form": "매운거..불닭볶음면까지는 잘먹는 편이에요!!!",
          "speaker_id": "2",
          "time": "20210830 13:51"
        },
        {
          "id": "MDRW2100023375.1.8",
          "form": "저도 저녁메뉴로 낙지 땡기네요ㅋㅋㅋㅋ",
          "original_form": "저도 저녁메뉴로 낙지 땡기네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:51"
        },
        {
          "id": "MDRW2100023375.1.9",
          "form": "헉 부러워요!!",
          "original_form": "헉 부러워요!!",
          "speaker_id": "1",
          "time": "20210830 13:51"
        },
        {
          "id": "MDRW2100023375.1.10",
          "form": "전 완전 매운거 못먹어서",
          "original_form": "전 완전 매운거 못먹어서",
          "speaker_id": "1",
          "time": "20210830 13:51"
        },
        {
          "id": "MDRW2100023375.1.11",
          "form": "불닭볶음면은 시도도 못해봤어요ㅠ",
          "original_form": "불닭볶음면은 시도도 못해봤어요ㅠ",
          "speaker_id": "1",
          "time": "20210830 13:52"
        },
        {
          "id": "MDRW2100023375.1.12",
          "form": "헉 매운거 못드시는군요ㅠ 그럼 엽떡 같은 것도 잘못드시나요",
          "original_form": "헉 매운거 못드시는군요ㅠ 그럼 엽떡 같은 것도 잘못드시나요",
          "speaker_id": "2",
          "time": "20210830 13:52"
        },
        {
          "id": "MDRW2100023375.1.13",
          "form": "넹넹 엽떡 1단계를 먹어봤는데",
          "original_form": "넹넹 엽떡 1단계를 먹어봤는데",
          "speaker_id": "1",
          "time": "20210830 13:54"
        },
        {
          "id": "MDRW2100023375.1.14",
          "form": "우유랑 같이 겨우 먹었어요ㅠㅠㅋㅋㅋㅋ",
          "original_form": "우유랑 같이 겨우 먹었어요ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:54"
        },
        {
          "id": "MDRW2100023375.1.15",
          "form": "보통 몇단계 드세용?",
          "original_form": "보통 몇단계 드세용?",
          "speaker_id": "1",
          "time": "20210830 13:54"
        },
        {
          "id": "MDRW2100023375.1.16",
          "form": "에고 고생하셨네요ㅋㅋㅠㅠ저는 2단계? 로 먹어요 너무 매워도 먹기가 힘들어서..",
          "original_form": "에고 고생하셨네요ㅋㅋㅠㅠ저는 2단계? 로 먹어요 너무 매워도 먹기가 힘들어서..",
          "speaker_id": "2",
          "time": "20210830 13:56"
        },
        {
          "id": "MDRW2100023375.1.17",
          "form": "그치만 떡볶이는..매워도 맛있죠 최고에용",
          "original_form": "그치만 떡볶이는..매워도 맛있죠 최고에용",
          "speaker_id": "2",
          "time": "20210830 13:56"
        },
        {
          "id": "MDRW2100023375.1.18",
          "form": "맞아요 떡볶이는 매워도 맛있어요ㅋㅋㅋ",
          "original_form": "맞아요 떡볶이는 매워도 맛있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:56"
        },
        {
          "id": "MDRW2100023375.1.19",
          "form": "좋아하시는 라면은 뭔가용?",
          "original_form": "좋아하시는 라면은 뭔가용?",
          "speaker_id": "1",
          "time": "20210830 13:57"
        },
        {
          "id": "MDRW2100023375.1.20",
          "form": "저 국물라면은 신라면 좋아하고 비빔면!!!진짜 좋아해요 대체로 볶음라면류를 좋아하는 것 같아요..",
          "original_form": "저 국물라면은 신라면 좋아하고 비빔면!!!진짜 좋아해요 대체로 볶음라면류를 좋아하는 것 같아요..",
          "speaker_id": "2",
          "time": "20210830 13:58"
        },
        {
          "id": "MDRW2100023375.1.21",
          "form": "비빔면!!",
          "original_form": "비빔면!!",
          "speaker_id": "1",
          "time": "20210830 13:58"
        },
        {
          "id": "MDRW2100023375.1.22",
          "form": "배홍동이랑 팔도랑",
          "original_form": "배홍동이랑 팔도랑",
          "speaker_id": "1",
          "time": "20210830 13:58"
        },
        {
          "id": "MDRW2100023375.1.23",
          "form": "진비빔이랑 또 뭐있져?",
          "original_form": "진비빔이랑 또 뭐있져?",
          "speaker_id": "1",
          "time": "20210830 13:58"
        },
        {
          "id": "MDRW2100023375.1.24",
          "form": "뭐가 제일 입에 맞으세요?",
          "original_form": "뭐가 제일 입에 맞으세요?",
          "speaker_id": "1",
          "time": "20210830 13:58"
        },
        {
          "id": "MDRW2100023375.1.25",
          "form": "진쫄비빔면도 있고 네넴띤도 있고..요즘 종류 진짜 많죠 저 이번 여름에는 열무비빔면에 꽂혔어요!",
          "original_form": "진쫄비빔면도 있고 네넴띤도 있고..요즘 종류 진짜 많죠 저 이번 여름에는 열무비빔면에 꽂혔어요!",
          "speaker_id": "2",
          "time": "20210830 13:59"
        },
        {
          "id": "MDRW2100023375.1.26",
          "form": "엇 이거",
          "original_form": "엇 이거",
          "speaker_id": "1",
          "time": "20210830 13:59"
        },
        {
          "id": "MDRW2100023375.1.27",
          "form": "삼양에서 나온거죠??",
          "original_form": "삼양에서 나온거죠??",
          "speaker_id": "1",
          "time": "20210830 13:59"
        },
        {
          "id": "MDRW2100023375.1.28",
          "form": "아는사람만 아는건데!",
          "original_form": "아는사람만 아는건데!",
          "speaker_id": "1",
          "time": "20210830 13:59"
        },
        {
          "id": "MDRW2100023375.1.29",
          "form": "저도 이거 최애에요",
          "original_form": "저도 이거 최애에요",
          "speaker_id": "1",
          "time": "20210830 13:59"
        }
      ]
