      "utterance": [
        {
          "id": "MDRW2100047344.1.1",
          "form": "당근마켓 하시나요",
          "original_form": "당근마켓 하시나요",
          "speaker_id": "2",
          "time": "20210826 12:42"
        },
        {
          "id": "MDRW2100047344.1.2",
          "form": "네 당근마켓 해요ㅎㅎㅎ name2 님은요?",
          "original_form": "네 당근마켓 해요ㅎㅎㅎ &name2& 님은요?",
          "speaker_id": "1",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100047344.1.3",
          "form": "저도 엄청해요 ㅋㅋㅋ",
          "original_form": "저도 엄청해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100047344.1.4",
          "form": "최근에 파는 거 있으세요?",
          "original_form": "최근에 파는 거 있으세요?",
          "speaker_id": "2",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100047344.1.5",
          "form": "전 최근에 프로틴바 팔았어요!",
          "original_form": "전 최근에 프로틴바 팔았어요!",
          "speaker_id": "1",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100047344.1.6",
          "form": "프로틴바! 어쩌다 그걸 파셨어요? 안먹는게 생기셨나요?",
          "original_form": "프로틴바! 어쩌다 그걸 파셨어요? 안먹는게 생기셨나요?",
          "speaker_id": "2",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100047344.1.7",
          "form": "네ㅋㅋㅋㅋ 먹다가 운동도 포기하고 해서 프로틴바도 팔았어요",
          "original_form": "네ㅋㅋㅋㅋ 먹다가 운동도 포기하고 해서 프로틴바도 팔았어요",
          "speaker_id": "1",
          "time": "20210826 13:09"
        },
        {
          "id": "MDRW2100047344.1.8",
          "form": "엌ㅋㅋㅋㅋ 운동하려고 프로틴바까지 장만하셨는데 파셨다니",
          "original_form": "엌ㅋㅋㅋㅋ 운동하려고 프로틴바까지 장만하셨는데 파셨다니",
          "speaker_id": "2",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100047344.1.9",
          "form": "많이 손해보셧나요",
          "original_form": "많이 손해보셧나요",
          "speaker_id": "2",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100047344.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ좀 손해는 봤지만",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ좀 손해는 봤지만",
          "speaker_id": "1",
          "time": "20210826 13:16"
        },
        {
          "id": "MDRW2100047344.1.11",
          "form": "0원보다는 만원이 나아서 그냥 팔았죠...",
          "original_form": "0원보다는 만원이 나아서 그냥 팔았죠...",
          "speaker_id": "1",
          "time": "20210826 13:16"
        },
        {
          "id": "MDRW2100047344.1.12",
          "form": "해외배송이라서 돈도 더 들었지만... 음...ㅋㅋㅋ",
          "original_form": "해외배송이라서 돈도 더 들었지만... 음...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:16"
        },
        {
          "id": "MDRW2100047344.1.13",
          "form": "헉 해외배송까지 시키신거엿다니",
          "original_form": "헉 해외배송까지 시키신거엿다니",
          "speaker_id": "2",
          "time": "20210826 13:16"
        },
        {
          "id": "MDRW2100047344.1.14",
          "form": "당시 열정이 엄청나셨나본데요",
          "original_form": "당시 열정이 엄청나셨나본데요",
          "speaker_id": "2",
          "time": "20210826 13:17"
        },
        {
          "id": "MDRW2100047344.1.15",
          "form": "예... 그 당시에는 4키로 정도 빼서 불붙나 싶다가 안하게 돼서요ㅋㅋㅋㅋㅋ",
          "original_form": "예... 그 당시에는 4키로 정도 빼서 불붙나 싶다가 안하게 돼서요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:18"
        },
        {
          "id": "MDRW2100047344.1.16",
          "form": "6키로까지 딱 빼면 운동복을 살생각이었거든요",
          "original_form": "6키로까지 딱 빼면 운동복을 살생각이었거든요",
          "speaker_id": "1",
          "time": "20210826 13:18"
        },
        {
          "id": "MDRW2100047344.1.17",
          "form": "악 이키로를 남기고..",
          "original_form": "악 이키로를 남기고..",
          "speaker_id": "2",
          "time": "20210826 13:18"
        },
        {
          "id": "MDRW2100047344.1.18",
          "form": "넘 아쉽네요 ㅍ",
          "original_form": "넘 아쉽네요 ㅍ",
          "speaker_id": "2",
          "time": "20210826 13:18"
        },
        {
          "id": "MDRW2100047344.1.19",
          "form": "프로틴도 드셨으면 근육도 많이 붙지 않았을까요",
          "original_form": "프로틴도 드셨으면 근육도 많이 붙지 않았을까요",
          "speaker_id": "2",
          "time": "20210826 13:18"
        },
        {
          "id": "MDRW2100047344.1.20",
          "form": "음... 그랬을까요? 홈트만 해서 근육 붙는지 안 붙는지도 모르고 했네요",
          "original_form": "음... 그랬을까요? 홈트만 해서 근육 붙는지 안 붙는지도 모르고 했네요",
          "speaker_id": "1",
          "time": "20210826 13:20"
        }
      ]
