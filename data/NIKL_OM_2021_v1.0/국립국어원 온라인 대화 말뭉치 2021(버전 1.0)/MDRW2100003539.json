      "utterance": [
        {
          "id": "MDRW2100003539.1.1",
          "form": "오늘 피드백 그지같이 와서..",
          "original_form": "오늘 피드백 그지같이 와서..",
          "speaker_id": "1",
          "time": "20210821 00:43"
        },
        {
          "id": "MDRW2100003539.1.2",
          "form": "정신 없었어요..ㅠㅠ",
          "original_form": "정신 없었어요..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 00:43"
        },
        {
          "id": "MDRW2100003539.1.3",
          "form": "왜? 무슨일이야?",
          "original_form": "왜? 무슨일이야?",
          "speaker_id": "2",
          "time": "20210821 00:43"
        },
        {
          "id": "MDRW2100003539.1.4",
          "form": "기껏 다 만들었는데,",
          "original_form": "기껏 다 만들었는데,",
          "speaker_id": "1",
          "time": "20210821 00:44"
        },
        {
          "id": "MDRW2100003539.1.5",
          "form": "처음부터 싹 다 수정해달라고 해서..ㅠㅠ",
          "original_form": "처음부터 싹 다 수정해달라고 해서..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 00:44"
        },
        {
          "id": "MDRW2100003539.1.6",
          "form": "왜 일찍 말 안해주고",
          "original_form": "왜 일찍 말 안해주고",
          "speaker_id": "2",
          "time": "20210821 00:44"
        },
        {
          "id": "MDRW2100003539.1.7",
          "form": "그러니까요.ㅠㅠ 맘에 안 들었음 진작에 말을 해주던가...",
          "original_form": "그러니까요.ㅠㅠ 맘에 안 들었음 진작에 말을 해주던가...",
          "speaker_id": "1",
          "time": "20210821 00:44"
        },
        {
          "id": "MDRW2100003539.1.8",
          "form": "이제와서 얘기하면,, 어떡하냐고,ㅠㅠ",
          "original_form": "이제와서 얘기하면,, 어떡하냐고,ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 00:44"
        },
        {
          "id": "MDRW2100003539.1.9",
          "form": "우리딸 힘들었겠네",
          "original_form": "우리딸 힘들었겠네",
          "speaker_id": "2",
          "time": "20210821 00:45"
        },
        {
          "id": "MDRW2100003539.1.10",
          "form": "속상속상..ㅠㅠ",
          "original_form": "속상속상..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 00:45"
        },
        {
          "id": "MDRW2100003539.1.11",
          "form": "진짜 힘도 쭉 빠지고..ㅠㅠ",
          "original_form": "진짜 힘도 쭉 빠지고..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 00:45"
        },
        {
          "id": "MDRW2100003539.1.12",
          "form": "엄마가 맛있는 거 해줘야겠네",
          "original_form": "엄마가 맛있는 거 해줘야겠네",
          "speaker_id": "2",
          "time": "20210821 00:45"
        },
        {
          "id": "MDRW2100003539.1.13",
          "form": "뭐 먹고 싶지도 않네요..",
          "original_form": "뭐 먹고 싶지도 않네요..",
          "speaker_id": "1",
          "time": "20210821 00:45"
        },
        {
          "id": "MDRW2100003539.1.14",
          "form": "일이 한번에 제대로 끝나면 좋을텐데",
          "original_form": "일이 한번에 제대로 끝나면 좋을텐데",
          "speaker_id": "1",
          "time": "20210821 00:45"
        },
        {
          "id": "MDRW2100003539.1.15",
          "form": "매번 이렇게 막히니까 지치기도 하고",
          "original_form": "매번 이렇게 막히니까 지치기도 하고",
          "speaker_id": "1",
          "time": "20210821 00:45"
        },
        {
          "id": "MDRW2100003539.1.16",
          "form": "이번 주말에 우리딸 푹 쉬어야겠다",
          "original_form": "이번 주말에 우리딸 푹 쉬어야겠다",
          "speaker_id": "2",
          "time": "20210821 00:45"
        },
        {
          "id": "MDRW2100003539.1.17",
          "form": "그러고 싶은데 머릿속은 너무 엉망이라.ㅠㅠ",
          "original_form": "그러고 싶은데 머릿속은 너무 엉망이라.ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 00:46"
        },
        {
          "id": "MDRW2100003539.1.18",
          "form": "그냥 아무생각하지 말고",
          "original_form": "그냥 아무생각하지 말고",
          "speaker_id": "2",
          "time": "20210821 00:46"
        },
        {
          "id": "MDRW2100003539.1.19",
          "form": "누워서 쉬어",
          "original_form": "누워서 쉬어",
          "speaker_id": "2",
          "time": "20210821 00:46"
        },
        {
          "id": "MDRW2100003539.1.20",
          "form": "아니면 아예 다른 걸 하던가",
          "original_form": "아니면 아예 다른 걸 하던가",
          "speaker_id": "2",
          "time": "20210821 00:47"
        },
        {
          "id": "MDRW2100003539.1.21",
          "form": "그게 낫겠어요~",
          "original_form": "그게 낫겠어요~",
          "speaker_id": "1",
          "time": "20210821 00:48"
        },
        {
          "id": "MDRW2100003539.1.22",
          "form": "영화를 보던가 해야겠네요",
          "original_form": "영화를 보던가 해야겠네요",
          "speaker_id": "1",
          "time": "20210821 00:48"
        },
        {
          "id": "MDRW2100003539.1.23",
          "form": "영화 괜찮네",
          "original_form": "영화 괜찮네",
          "speaker_id": "2",
          "time": "20210821 00:48"
        },
        {
          "id": "MDRW2100003539.1.24",
          "form": "재밌는 영화 보면서 웃으면 괜찮아 질거야~",
          "original_form": "재밌는 영화 보면서 웃으면 괜찮아 질거야~",
          "speaker_id": "2",
          "time": "20210821 00:48"
        }
      ]
