      "utterance": [
        {
          "id": "MDRW2100009462.1.1",
          "form": "불만가지면 조용히 하고 걍 버리고 오래요",
          "original_form": "불만가지면 조용히 하고 걍 버리고 오래요",
          "speaker_id": "2",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009462.1.2",
          "form": "아ㅋㅋㅋ. 직접?버리시는거세요?매일?",
          "original_form": "아ㅋㅋㅋ. 직접?버리시는거세요?매일?",
          "speaker_id": "1",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100009462.1.3",
          "form": "네 와이프 분리수거랑 음식물 쓰레기 버릴 줄 몰라여",
          "original_form": "네 와이프 분리수거랑 음식물 쓰레기 버릴 줄 몰라여",
          "speaker_id": "2",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009462.1.4",
          "form": "와 ~ 착하시네요ㅋㅋ",
          "original_form": "와 ~ 착하시네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009462.1.5",
          "form": "와이프말로는 다들 이렇게 한답니다",
          "original_form": "와이프말로는 다들 이렇게 한답니다",
          "speaker_id": "2",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009462.1.6",
          "form": "보통은근데ㅋㅋ 음쓰는 남자들이 해줘요ㅋㅋ",
          "original_form": "보통은근데ㅋㅋ 음쓰는 남자들이 해줘요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009462.1.7",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210822 23:42"
        },
        {
          "id": "MDRW2100009462.1.8",
          "form": "저는 근데  둘째없을땐ㅋㅋ 분리수거는 직접했고",
          "original_form": "저는 근데  둘째없을땐ㅋㅋ 분리수거는 직접했고",
          "speaker_id": "1",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009462.1.9",
          "form": "음쓰는ㅋㅋ 분쇄기가있어서 신경은 안썼는데",
          "original_form": "음쓰는ㅋㅋ 분쇄기가있어서 신경은 안썼는데",
          "speaker_id": "1",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009462.1.10",
          "form": "보통 얼베타면 남자분들이 다 하시긴 하더라구요",
          "original_form": "보통 얼베타면 남자분들이 다 하시긴 하더라구요",
          "speaker_id": "1",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009462.1.11",
          "form": "전 회식하고도 쓰레기 버리고 잤어요",
          "original_form": "전 회식하고도 쓰레기 버리고 잤어요",
          "speaker_id": "2",
          "time": "20210822 23:44"
        },
        {
          "id": "MDRW2100009462.1.12",
          "form": "왕창 모아서ㅋㅋ",
          "original_form": "왕창 모아서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:44"
        },
        {
          "id": "MDRW2100009462.1.13",
          "form": "매일은 힘드시겠네요ㅝㄱ",
          "original_form": "매일은 힘드시겠네요ㅝㄱ",
          "speaker_id": "1",
          "time": "20210822 23:44"
        },
        {
          "id": "MDRW2100009462.1.14",
          "form": "그래도 사랑합니다",
          "original_form": "그래도 사랑합니다",
          "speaker_id": "2",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100009462.1.15",
          "form": "제가 너무 좋아해서요",
          "original_form": "제가 너무 좋아해서요",
          "speaker_id": "2",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100009462.1.16",
          "form": "와~~~~~~~~ 사랑꾼이시네요",
          "original_form": "와~~~~~~~~ 사랑꾼이시네요",
          "speaker_id": "1",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100009462.1.17",
          "form": "사랑꾼이셔요",
          "original_form": "사랑꾼이셔요",
          "speaker_id": "1",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100009462.1.18",
          "form": "이래삽니다",
          "original_form": "이래삽니다",
          "speaker_id": "2",
          "time": "20210822 23:47"
        },
        {
          "id": "MDRW2100009462.1.19",
          "form": "보기 좋을것 같아요~",
          "original_form": "보기 좋을것 같아요~",
          "speaker_id": "1",
          "time": "20210822 23:48"
        },
        {
          "id": "MDRW2100009462.1.20",
          "form": "부럽네요~ 저희는 의리로 살아요",
          "original_form": "부럽네요~ 저희는 의리로 살아요",
