      "utterance": [
        {
          "id": "MDRW2100016774.1.1",
          "form": "문학 좋아하시나요?",
          "original_form": "문학 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016774.1.2",
          "form": "어릴땐 책 많이 읽었어요",
          "original_form": "어릴땐 책 많이 읽었어요",
          "speaker_id": "2",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100016774.1.3",
          "form": "지금은 읽고 계신 책이 있으신가요?",
          "original_form": "지금은 읽고 계신 책이 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100016774.1.4",
          "form": "요즘엔 없어용",
          "original_form": "요즘엔 없어용",
          "speaker_id": "2",
          "time": "20210826 22:27"
        },
        {
          "id": "MDRW2100016774.1.5",
          "form": "좋아하는 책 있으세요?",
          "original_form": "좋아하는 책 있으세요?",
          "speaker_id": "2",
          "time": "20210826 22:27"
        },
        {
          "id": "MDRW2100016774.1.6",
          "form": "저는 로마인 이야기 좋아해요",
          "original_form": "저는 로마인 이야기 좋아해요",
          "speaker_id": "1",
          "time": "20210826 22:42"
        },
        {
          "id": "MDRW2100016774.1.7",
          "form": "아하~최근엔 바빠서 책 읽을 시간이 없었더니 요즘 어떤 책이 핫한지 잘 몰라요ㅎㅎ",
          "original_form": "아하~최근엔 바빠서 책 읽을 시간이 없었더니 요즘 어떤 책이 핫한지 잘 몰라요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100016774.1.8",
          "form": "저두...ㅎㅎ 요즘엔 장르소설만 읽어요ㅠ",
          "original_form": "저두...ㅎㅎ 요즘엔 장르소설만 읽어요ㅠ",
          "speaker_id": "1",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100016774.1.9",
          "form": "저는 추리소설 좋아해요!",
          "original_form": "저는 추리소설 좋아해요!",
          "speaker_id": "2",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100016774.1.10",
          "form": "결말이 눈에 딱 보이는걸 좋아해서",
          "original_form": "결말이 눈에 딱 보이는걸 좋아해서",
          "speaker_id": "2",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100016774.1.11",
          "form": "범인 떡밥 딱딱 나오는게 좋더라구요",
          "original_form": "범인 떡밥 딱딱 나오는게 좋더라구요",
          "speaker_id": "2",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100016774.1.12",
          "form": "저는 떡밥을 잘 못 찾아가는데 name2님은 추리력이 좋으신가봐여...!",
          "original_form": "저는 떡밥을 잘 못 찾아가는데 &name2&님은 추리력이 좋으신가봐여...!",
          "speaker_id": "1",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100016774.1.13",
          "form": "추리력이라기보단 그냥 읽다보면 술술술 풀려나가는게 재밌더라구요!!",
          "original_form": "추리력이라기보단 그냥 읽다보면 술술술 풀려나가는게 재밌더라구요!!",
          "speaker_id": "2",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100016774.1.14",
          "form": "혹시 추천해주실 작품이 있으실까요? 듣다 보니 관심이 생기네요!",
          "original_form": "혹시 추천해주실 작품이 있으실까요? 듣다 보니 관심이 생기네요!",
          "speaker_id": "1",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100016774.1.15",
          "form": "제일 유명한건 용의자x의 헌신!",
          "original_form": "제일 유명한건 용의자x의 헌신!",
          "speaker_id": "2",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100016774.1.16",
          "form": "영화로도 나왔어요!",
          "original_form": "영화로도 나왔어요!",
          "speaker_id": "2",
          "time": "20210826 23:22"
        }
      ]
