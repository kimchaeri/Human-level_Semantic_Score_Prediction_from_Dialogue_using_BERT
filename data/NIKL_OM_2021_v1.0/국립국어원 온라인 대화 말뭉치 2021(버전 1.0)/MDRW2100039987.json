      "utterance": [
        {
          "id": "MDRW2100039987.1.1",
          "form": "또인단 ㅇㅇㅋㅋㅋㅋㅋ",
          "original_form": "또인단 ㅇㅇㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100039987.1.2",
          "form": "ㄹㅇ정말",
          "original_form": "ㄹㅇ정말",
          "speaker_id": "2",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100039987.1.3",
          "form": "사소한것부터 세계멸망에",
          "original_form": "사소한것부터 세계멸망에",
          "speaker_id": "2",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100039987.1.4",
          "form": "다가가려하는구나......",
          "original_form": "다가가려하는구나......",
          "speaker_id": "2",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100039987.1.5",
          "form": "???",
          "original_form": "???",
          "speaker_id": "2",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100039987.1.6",
          "form": "설마 텟페이",
          "original_form": "설마 텟페이",
          "speaker_id": "2",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100039987.1.7",
          "form": "죽음???",
          "original_form": "죽음???",
          "speaker_id": "2",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100039987.1.8",
          "form": "혹시 타타라스나에서",
          "original_form": "혹시 타타라스나에서",
          "speaker_id": "2",
          "time": "20210904 00:07"
        },
        {
          "id": "MDRW2100039987.1.9",
          "form": "재앙신의 힘 이용해서 돌리던게",
          "original_form": "재앙신의 힘 이용해서 돌리던게",
          "speaker_id": "2",
          "time": "20210904 00:07"
        },
        {
          "id": "MDRW2100039987.1.10",
          "form": "사안공장인가?",
          "original_form": "사안공장인가?",
          "speaker_id": "2",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100039987.1.11",
          "form": "그것까진 모르겠어",
          "original_form": "그것까진 모르겠어",
          "speaker_id": "1",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100039987.1.12",
          "form": "근데 코코미 비중 적단게 설마",
          "original_form": "근데 코코미 비중 적단게 설마",
          "speaker_id": "2",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100039987.1.13",
          "form": "근데 초반에",
          "original_form": "근데 초반에",
          "speaker_id": "1",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100039987.1.14",
          "form": "주인공 이대로 또",
          "original_form": "주인공 이대로 또",
          "speaker_id": "2",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100039987.1.15",
          "form": "이도에서 수정골수 달라고 했을때",
          "original_form": "이도에서 수정골수 달라고 했을때",
          "speaker_id": "1",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100039987.1.16",
          "form": "우인단이랑 싸우러감?",
          "original_form": "우인단이랑 싸우러감?",
          "speaker_id": "2",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100039987.1.17",
          "form": "수정골수 요구하는게 나름 복선이었던듯",
          "original_form": "수정골수 요구하는게 나름 복선이었던듯",
          "speaker_id": "1",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100039987.1.18",
          "form": "왜냐면 꺼라위키에서",
          "original_form": "왜냐면 꺼라위키에서",
          "speaker_id": "2",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100039987.1.19",
          "form": "예압 ㅎ",
          "original_form": "예압 ㅎ",
          "speaker_id": "1",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100039987.1.20",
          "form": "사안은 마신의 힘을 이용해서 가공되는걸로 추측된다고",
          "original_form": "사안은 마신의 힘을 이용해서 가공되는걸로 추측된다고",
          "speaker_id": "2",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100039987.1.21",
          "form": "아 하기야 수정골수가 어떻게 보면 재앙신의 유해? 니까",
          "original_form": "아 하기야 수정골수가 어떻게 보면 재앙신의 유해? 니까",
          "speaker_id": "1",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100039987.1.22",
          "form": "써있긴 했어",
          "original_form": "써있긴 했어",
          "speaker_id": "2",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100039987.1.23",
          "form": "그걸로 만드는 사안이면 그럴지도...",
          "original_form": "그걸로 만드는 사안이면 그럴지도...",
          "speaker_id": "1",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100039987.1.24",
          "form": "ㄹㅇ이거 우인단 조무래기들한테 안주는건",
          "original_form": "ㄹㅇ이거 우인단 조무래기들한테 안주는건",
          "speaker_id": "2",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100039987.1.25",
          "form": "자기들도 순식간에 몰살될까봐",
          "original_form": "자기들도 순식간에 몰살될까봐",
          "speaker_id": "2",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100039987.1.26",
          "form": "감당 가능인 애들만",
          "original_form": "감당 가능인 애들만",
          "speaker_id": "2",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100039987.1.27",
          "form": "주는건가본데",
          "original_form": "주는건가본데",
          "speaker_id": "2",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100039987.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그런듯?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그런듯?",
          "speaker_id": "1",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100039987.1.29",
          "form": "근데 이게 쓸 때마다 생명력을 갉아먹는데",
          "original_form": "근데 이게 쓸 때마다 생명력을 갉아먹는데",
          "speaker_id": "1",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100039987.1.30",
          "form": "아니 근데 저항군 얘네는 왜",
          "original_form": "아니 근데 저항군 얘네는 왜",
          "speaker_id": "2",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100039987.1.31",
          "form": "우인단 집정관?은 대체 어케 오래사는거지",
          "original_form": "우인단 집정관?은 대체 어케 오래사는거지",
          "speaker_id": "1",
          "time": "20210904 00:10"
        },
        {
          "id": "MDRW2100039987.1.32",
          "form": "갑자기 신의눈도 없던 애가 원소의 힘을 써서 공적을 세워오는데",
          "original_form": "갑자기 신의눈도 없던 애가 원소의 힘을 써서 공적을 세워오는데",
          "speaker_id": "2",
          "time": "20210904 00:10"
        }
      ]
    }
