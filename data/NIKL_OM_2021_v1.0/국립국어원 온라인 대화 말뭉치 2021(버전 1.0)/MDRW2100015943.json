      "utterance": [
        {
          "id": "MDRW2100015943.1.1",
          "form": "엄마도 어렸을때는 일 많이 해봤어",
          "original_form": "엄마도 어렸을때는 일 많이 해봤어",
          "speaker_id": "2",
          "time": "20210826 03:25"
        },
        {
          "id": "MDRW2100015943.1.2",
          "form": "무슨 일 했었는데",
          "original_form": "무슨 일 했었는데",
          "speaker_id": "1",
          "time": "20210826 03:25"
        },
        {
          "id": "MDRW2100015943.1.3",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210826 03:25"
        },
        {
          "id": "MDRW2100015943.1.4",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210826 03:25"
        },
        {
          "id": "MDRW2100015943.1.5",
          "form": "은행이랑 여행사에 오래있었지",
          "original_form": "은행이랑 여행사에 오래있었지",
          "speaker_id": "2",
          "time": "20210826 03:26"
        },
        {
          "id": "MDRW2100015943.1.6",
          "form": "여행사에서는 무슨일 했어?",
          "original_form": "여행사에서는 무슨일 했어?",
          "speaker_id": "1",
          "time": "20210826 03:26"
        },
        {
          "id": "MDRW2100015943.1.7",
          "form": "여행 계획 짜는 일 했어",
          "original_form": "여행 계획 짜는 일 했어",
          "speaker_id": "2",
          "time": "20210826 03:26"
        },
        {
          "id": "MDRW2100015943.1.8",
          "form": "근데 왜 우리 여행갈때는 안짜?",
          "original_form": "근데 왜 우리 여행갈때는 안짜?",
          "speaker_id": "1",
          "time": "20210826 03:26"
        },
        {
          "id": "MDRW2100015943.1.9",
          "form": "다 옛날일이지",
          "original_form": "다 옛날일이지",
          "speaker_id": "2",
          "time": "20210826 03:26"
        },
        {
          "id": "MDRW2100015943.1.10",
          "form": "그럼 은행에서는 무슨일 했어?",
          "original_form": "그럼 은행에서는 무슨일 했어?",
          "speaker_id": "1",
          "time": "20210826 03:27"
        },
        {
          "id": "MDRW2100015943.1.11",
          "form": "여러가지 업무 처리했지",
          "original_form": "여러가지 업무 처리했지",
          "speaker_id": "2",
          "time": "20210826 03:27"
        },
        {
          "id": "MDRW2100015943.1.12",
          "form": "일 잘했어?",
          "original_form": "일 잘했어?",
          "speaker_id": "1",
          "time": "20210826 03:27"
        },
        {
          "id": "MDRW2100015943.1.13",
          "form": "그럼 상사한테 칭찬 많이 받았지",
          "original_form": "그럼 상사한테 칭찬 많이 받았지",
          "speaker_id": "2",
          "time": "20210826 03:27"
        },
        {
          "id": "MDRW2100015943.1.14",
          "form": "무슨 칭찬?",
          "original_form": "무슨 칭찬?",
          "speaker_id": "1",
          "time": "20210826 03:27"
        },
        {
          "id": "MDRW2100015943.1.15",
          "form": "직급 금방 올려준다했어",
          "original_form": "직급 금방 올려준다했어",
          "speaker_id": "2",
          "time": "20210826 03:28"
        },
        {
          "id": "MDRW2100015943.1.16",
          "form": "그런데 왜 그만뒀어?",
          "original_form": "그런데 왜 그만뒀어?",
          "speaker_id": "1",
          "time": "20210826 03:28"
        },
        {
          "id": "MDRW2100015943.1.17",
          "form": "결혼하고 육아하느라 정신없었지",
          "original_form": "결혼하고 육아하느라 정신없었지",
          "speaker_id": "2",
          "time": "20210826 03:28"
        },
        {
          "id": "MDRW2100015943.1.18",
          "form": "아 나때문이다?",
          "original_form": "아 나때문이다?",
          "speaker_id": "1",
          "time": "20210826 03:28"
        },
        {
          "id": "MDRW2100015943.1.19",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210826 03:28"
        },
        {
          "id": "MDRW2100015943.1.20",
          "form": "엄마가 직장다니는거 상상이 안간다",
          "original_form": "엄마가 직장다니는거 상상이 안간다",
          "speaker_id": "1",
          "time": "20210826 03:29"
        },
        {
          "id": "MDRW2100015943.1.21",
          "form": "너 나이대부터 얼마나 많은 일을 해봤는데",
          "original_form": "너 나이대부터 얼마나 많은 일을 해봤는데",
          "speaker_id": "2",
          "time": "20210826 03:29"
        },
        {
          "id": "MDRW2100015943.1.22",
          "form": "더 있어?",
          "original_form": "더 있어?",
          "speaker_id": "1",
          "time": "20210826 03:29"
        },
        {
          "id": "MDRW2100015943.1.23",
          "form": "어릴땐 경험을 많이 해보는게 좋아서 여러가지 해봤었지",
          "original_form": "어릴땐 경험을 많이 해보는게 좋아서 여러가지 해봤었지",
          "speaker_id": "2",
          "time": "20210826 03:29"
        },
        {
          "id": "MDRW2100015943.1.24",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
