      "utterance": [
        {
          "id": "MDRW2100002993.1.1",
          "form": "저녁은 어떻게 하시기로 계획있으세요?",
          "original_form": "저녁은 어떻게 하시기로 계획있으세요?",
          "speaker_id": "2",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002993.1.2",
          "form": "아까 낮에 장본 불고기가 도착해서 불괴기 먹으려구요",
          "original_form": "아까 낮에 장본 불고기가 도착해서 불괴기 먹으려구요",
          "speaker_id": "1",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100002993.1.3",
          "form": "메뉴정하셨어요?",
          "original_form": "메뉴정하셨어요?",
          "speaker_id": "1",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100002993.1.4",
          "form": "맛있겠네요 전 낙곱새 시켜먹을려고요",
          "original_form": "맛있겠네요 전 낙곱새 시켜먹을려고요",
          "speaker_id": "2",
          "time": "20210820 17:27"
        },
        {
          "id": "MDRW2100002993.1.5",
          "form": "낙곱새 면 낙지곱창새우 인가요?",
          "original_form": "낙곱새 면 낙지곱창새우 인가요?",
          "speaker_id": "1",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100002993.1.6",
          "form": "맞아요 ㅎㅎ 밥도 같이 와서 비벼먹을려고요",
          "original_form": "맞아요 ㅎㅎ 밥도 같이 와서 비벼먹을려고요",
          "speaker_id": "2",
          "time": "20210820 17:30"
        },
        {
          "id": "MDRW2100002993.1.7",
          "form": "맵지않아요?",
          "original_form": "맵지않아요?",
          "speaker_id": "1",
          "time": "20210820 17:30"
        },
        {
          "id": "MDRW2100002993.1.8",
          "form": "맵기 조절가능한데 저번에  매운맛 했다가 죽을뻔 했어요 ㅋㅋ",
          "original_form": "맵기 조절가능한데 저번에  매운맛 했다가 죽을뻔 했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:31"
        },
        {
          "id": "MDRW2100002993.1.9",
          "form": "으악...생각만해도 아찔한데요",
          "original_form": "으악...생각만해도 아찔한데요",
          "speaker_id": "1",
          "time": "20210820 17:35"
        },
        {
          "id": "MDRW2100002993.1.10",
          "form": "근데 더 아찔한건 제 불고기가 타고있네요 ㅋ",
          "original_form": "근데 더 아찔한건 제 불고기가 타고있네요 ㅋ",
          "speaker_id": "1",
          "time": "20210820 17:35"
        },
        {
          "id": "MDRW2100002993.1.11",
          "form": "어머나 어쩌다가~ 조금 탄건 불맛으로 생각하고",
          "original_form": "어머나 어쩌다가~ 조금 탄건 불맛으로 생각하고",
          "speaker_id": "2",
          "time": "20210820 17:37"
        },
        {
          "id": "MDRW2100002993.1.12",
          "form": "양념이라 잘타나봐요 ㅎ",
          "original_form": "양념이라 잘타나봐요 ㅎ",
          "speaker_id": "1",
          "time": "20210820 17:38"
        },
        {
          "id": "MDRW2100002993.1.13",
          "form": "배달음식 자주 드시나요?",
          "original_form": "배달음식 자주 드시나요?",
          "speaker_id": "1",
          "time": "20210820 17:39"
        },
        {
          "id": "MDRW2100002993.1.14",
          "form": "거의 배달요 혼자라 해먹는거보다 배달이 더 싼듯요",
          "original_form": "거의 배달요 혼자라 해먹는거보다 배달이 더 싼듯요",
          "speaker_id": "2",
          "time": "20210820 17:40"
        },
        {
          "id": "MDRW2100002993.1.15",
          "form": "그렇기도 하겠어요 해먹으면 재료사는것도 남고 그러면 처치곤란이니",
          "original_form": "그렇기도 하겠어요 해먹으면 재료사는것도 남고 그러면 처치곤란이니",
          "speaker_id": "1",
          "time": "20210820 17:40"
        },
        {
          "id": "MDRW2100002993.1.16",
          "form": "네  맞아요 버리는게 많아요 어제도 양파를 버렸어요 ㅜ.ㅜ",
          "original_form": "네  맞아요 버리는게 많아요 어제도 양파를 버렸어요 ㅜ.ㅜ",
          "speaker_id": "2",
          "time": "20210820 17:41"
        },
        {
          "id": "MDRW2100002993.1.17",
          "form": "아고 아까버라...",
          "original_form": "아고 아까버라...",
          "speaker_id": "1",
