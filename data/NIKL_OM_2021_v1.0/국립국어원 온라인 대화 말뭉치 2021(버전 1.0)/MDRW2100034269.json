      "utterance": [
        {
          "id": "MDRW2100034269.1.1",
          "form": "아 맞아요....",
          "original_form": "아 맞아요....",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100034269.1.2",
          "form": "하 보영배우에게 제발 천재 해커나 이런 역할 들어오면 좋겠어요ㅠㅠㅠㅠ",
          "original_form": "하 보영배우에게 제발 천재 해커나 이런 역할 들어오면 좋겠어요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100034269.1.3",
          "form": "오 그것도 좋아요",
          "original_form": "오 그것도 좋아요",
          "speaker_id": "1",
          "time": "20210828 09:40"
        },
        {
          "id": "MDRW2100034269.1.4",
          "form": "남자랑 로맨스물은 너무 많이 했어요ㅋㅋ",
          "original_form": "남자랑 로맨스물은 너무 많이 했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 09:40"
        },
        {
          "id": "MDRW2100034269.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요!!!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요!!!!",
          "speaker_id": "2",
          "time": "20210828 09:43"
        },
        {
          "id": "MDRW2100034269.1.6",
          "form": "로맨스만 아니면 전 다 좋은 것 같아요",
          "original_form": "로맨스만 아니면 전 다 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210828 09:43"
        },
        {
          "id": "MDRW2100034269.1.7",
          "form": "보영배우 연기력과 얼굴을 로맨스로만 쓰는 거 낭비입니다...",
          "original_form": "보영배우 연기력과 얼굴을 로맨스로만 쓰는 거 낭비입니다...",
          "speaker_id": "2",
          "time": "20210828 09:44"
        },
        {
          "id": "MDRW2100034269.1.8",
          "form": "맞아요!!!! 심각한 낭비예요",
          "original_form": "맞아요!!!! 심각한 낭비예요",
          "speaker_id": "1",
          "time": "20210828 10:16"
        },
        {
          "id": "MDRW2100034269.1.9",
          "form": "그쵸?!?!?!?! 솔직히 다양한 역할 들어오면 좋겠어요",
          "original_form": "그쵸?!?!?!?! 솔직히 다양한 역할 들어오면 좋겠어요",
          "speaker_id": "2",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100034269.1.10",
          "form": "차라리 색다르게 요즘 자주 보이는 동성애로 했으면 좋겠네요ㅋㅋ",
          "original_form": "차라리 색다르게 요즘 자주 보이는 동성애로 했으면 좋겠네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100034269.1.11",
          "form": "아? 헐 너무 좋죠",
          "original_form": "아? 헐 너무 좋죠",
          "speaker_id": "2",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100034269.1.12",
          "form": "하...씁 진짜 좋은데요",
          "original_form": "하...씁 진짜 좋은데요",
          "speaker_id": "2",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100034269.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100034269.1.14",
          "form": "최고죠....",
          "original_form": "최고죠....",
          "speaker_id": "1",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100034269.1.15",
          "form": "망상으로만 채워야 하는 바람...",
          "original_form": "망상으로만 채워야 하는 바람...",
          "speaker_id": "1",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100034269.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아니 보영배우면 충분히 소화가능하고 잘할텐데 진짜 너무 아까워요ㅜㅜ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아니 보영배우면 충분히 소화가능하고 잘할텐데 진짜 너무 아까워요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100034269.1.17",
          "form": "앗 그럼 어쩌다 사장 보셨나요? 저 거기서 보영배우보고 또 반했거든요...",
          "original_form": "앗 그럼 어쩌다 사장 보셨나요? 저 거기서 보영배우보고 또 반했거든요...",
          "speaker_id": "2",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100034269.1.18",
          "form": "똑쟁이...",
          "original_form": "똑쟁이...",
          "speaker_id": "2",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100034269.1.19",
          "form": "아뇨 처음 들어봤어요 드라마예요??",
          "original_form": "아뇨 처음 들어봤어요 드라마예요??",
          "speaker_id": "1",
          "time": "20210828 10:20"
        },
        {
          "id": "MDRW2100034269.1.20",
          "form": "아니요 예능인데 조인성차태현배우가 했던 예능이에요!",
          "original_form": "아니요 예능인데 조인성차태현배우가 했던 예능이에요!",
          "speaker_id": "2",
          "time": "20210828 10:22"
        },
        {
          "id": "MDRW2100034269.1.21",
          "form": "거기 첫번째 게스트로 보영배우가 나왔거든요!",
          "original_form": "거기 첫번째 게스트로 보영배우가 나왔거든요!",
          "speaker_id": "2",
          "time": "20210828 10:22"
        },
        {
          "id": "MDRW2100034269.1.22",
          "form": "아아 저 클립만 봤어요",
          "original_form": "아아 저 클립만 봤어요",
          "speaker_id": "1",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100034269.1.23",
          "form": "저도 보영언니랑 사적으로...알고싶네요 ㅋㅋ",
          "original_form": "저도 보영언니랑 사적으로...알고싶네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100034269.1.24",
          "form": "헐 저도요",
          "original_form": "헐 저도요",
          "speaker_id": "2",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100034269.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100034269.1.26",
          "form": "두 배우가 너무 부럽더라고요 저 보영언니때문에 한화좋아랬는데 아놔..",
          "original_form": "두 배우가 너무 부럽더라고요 저 보영언니때문에 한화좋아랬는데 아놔..",
          "speaker_id": "2",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100034269.1.27",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100034269.1.28",
          "form": "차태현씨 부럽네요",
          "original_form": "차태현씨 부럽네요",
          "speaker_id": "1",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100034269.1.29",
