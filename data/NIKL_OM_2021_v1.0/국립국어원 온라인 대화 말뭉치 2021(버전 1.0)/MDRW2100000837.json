      "utterance": [
        {
          "id": "MDRW2100000837.1.1",
          "form": "이 주제 어때요?",
          "original_form": "이 주제 어때요?",
          "speaker_id": "1",
          "time": "20210819 19:53"
        },
        {
          "id": "MDRW2100000837.1.2",
          "form": "좋아요~",
          "original_form": "좋아요~",
          "speaker_id": "2",
          "time": "20210819 19:57"
        },
        {
          "id": "MDRW2100000837.1.3",
          "form": "연애중이신가요 ㅋㅋ",
          "original_form": "연애중이신가요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 19:57"
        },
        {
          "id": "MDRW2100000837.1.4",
          "form": "아뇨 ㅋㅋㅋ 솔로입니다..",
          "original_form": "아뇨 ㅋㅋㅋ 솔로입니다..",
          "speaker_id": "1",
          "time": "20210819 19:57"
        },
        {
          "id": "MDRW2100000837.1.5",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210819 19:57"
        },
        {
          "id": "MDRW2100000837.1.6",
          "form": "앗 저는 있습니다 ㅋㅋㅋㅋㅋㅋㅋㅋ...",
          "original_form": "앗 저는 있습니다 ㅋㅋㅋㅋㅋㅋㅋㅋ...",
          "speaker_id": "2",
          "time": "20210819 20:19"
        },
        {
          "id": "MDRW2100000837.1.7",
          "form": "전남친한테 대차게 차이고 새롭게 만났습니다...^^ㅋㅋㅋㅋ",
          "original_form": "전남친한테 대차게 차이고 새롭게 만났습니다...^^ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:19"
        },
        {
          "id": "MDRW2100000837.1.8",
          "form": "어떻게 만났나요?",
          "original_form": "어떻게 만났나요?",
          "speaker_id": "1",
          "time": "20210819 20:19"
        },
        {
          "id": "MDRW2100000837.1.9",
          "form": "음 아는언니의 친구의 동생!!!이었어요 ㅋㅋㅋ",
          "original_form": "음 아는언니의 친구의 동생!!!이었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:20"
        },
        {
          "id": "MDRW2100000837.1.10",
          "form": "name1님은 솔로시면 소개팅하시나여 ㅋㅋ",
          "original_form": "&name1&님은 솔로시면 소개팅하시나여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:21"
        },
        {
          "id": "MDRW2100000837.1.11",
          "form": "아니면 자만추??",
          "original_form": "아니면 자만추??",
          "speaker_id": "2",
          "time": "20210819 20:21"
        },
        {
          "id": "MDRW2100000837.1.12",
          "form": "보통은 친구사이에서 시작했어요",
          "original_form": "보통은 친구사이에서 시작했어요",
          "speaker_id": "1",
          "time": "20210819 20:24"
        },
        {
          "id": "MDRW2100000837.1.13",
          "form": "ㅜㅜ 저도 그게 제일 좋아요",
          "original_form": "ㅜㅜ 저도 그게 제일 좋아요",
          "speaker_id": "2",
          "time": "20210819 20:29"
        },
        {
          "id": "MDRW2100000837.1.14",
          "form": "모르는사람보단 아는데서 시작하는거 ㅎㅎㅎ",
          "original_form": "모르는사람보단 아는데서 시작하는거 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 20:29"
        },
        {
          "id": "MDRW2100000837.1.15",
          "form": "근데 졸업하고나니 사람만날곳이 .....없더라구여....",
          "original_form": "근데 졸업하고나니 사람만날곳이 .....없더라구여....",
          "speaker_id": "2",
          "time": "20210819 20:29"
        },
        {
          "id": "MDRW2100000837.1.16",
          "form": "그래서 소개팅으로 시작하셨나요?",
          "original_form": "그래서 소개팅으로 시작하셨나요?",
          "speaker_id": "1",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000837.1.17",
          "form": "맞아요 ㅠㅠㅠ",
          "original_form": "맞아요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000837.1.18",
          "form": "name1님은 이상형이 머에용 ㅋㅋ",
          "original_form": "&name1&님은 이상형이 머에용 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000837.1.19",
          "form": "저는 성격을 봐요",
          "original_form": "저는 성격을 봐요",
          "speaker_id": "1",
          "time": "20210819 20:36"
        },
        {
          "id": "MDRW2100000837.1.20",
          "form": "외모는 중간만 돼도 좋아요",
          "original_form": "외모는 중간만 돼도 좋아요",
          "speaker_id": "1",
          "time": "20210819 20:36"
        },
        {
          "id": "MDRW2100000837.1.21",
          "form": "오 성격",
          "original_form": "오 성격",
          "speaker_id": "2",
          "time": "20210819 20:46"
        },
        {
          "id": "MDRW2100000837.1.22",
          "form": "욕하면 싫으신가요",
          "original_form": "욕하면 싫으신가요",
          "speaker_id": "2",
          "time": "20210819 20:46"
        },
        {
          "id": "MDRW2100000837.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:46"
        },
        {
          "id": "MDRW2100000837.1.24",
          "form": "욕 달고사는거는 별로에요",
          "original_form": "욕 달고사는거는 별로에요",
          "speaker_id": "1",
          "time": "20210819 20:47"
        },
        {
          "id": "MDRW2100000837.1.25",
