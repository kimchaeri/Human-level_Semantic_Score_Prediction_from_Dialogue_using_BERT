      "utterance": [
        {
          "id": "MDRW2100027835.1.1",
          "form": "하도 어리게 보는 사람들이 많아서 보컬트레이닝도 했어요",
          "original_form": "하도 어리게 보는 사람들이 많아서 보컬트레이닝도 했어요",
          "speaker_id": "2",
          "time": "20210902 15:31"
        },
        {
          "id": "MDRW2100027835.1.2",
          "form": "노래좀 부르나봐요?",
          "original_form": "노래좀 부르나봐요?",
          "speaker_id": "1",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100027835.1.3",
          "form": "목소리라도 다르게 할려고 간거에요ㅋㅋㅋㅋ",
          "original_form": "목소리라도 다르게 할려고 간거에요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100027835.1.4",
          "form": "근데 최근에 막내고모가",
          "original_form": "근데 최근에 막내고모가",
          "speaker_id": "2",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100027835.1.5",
          "form": "전화왔는데 야 너 왜케 아저씨됐어...",
          "original_form": "전화왔는데 야 너 왜케 아저씨됐어...",
          "speaker_id": "2",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100027835.1.6",
          "form": "외적으로 닮으면 목소리도 닮나보다 신기",
          "original_form": "외적으로 닮으면 목소리도 닮나보다 신기",
          "speaker_id": "1",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100027835.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:39"
        },
        {
          "id": "MDRW2100027835.1.8",
          "form": "아조씨를 아조씨라고 하죠 뭐라해요",
          "original_form": "아조씨를 아조씨라고 하죠 뭐라해요",
          "speaker_id": "1",
          "time": "20210902 15:40"
        },
        {
          "id": "MDRW2100027835.1.9",
          "form": "....어허",
          "original_form": "....어허",
          "speaker_id": "2",
          "time": "20210902 15:40"
        },
        {
          "id": "MDRW2100027835.1.10",
          "form": "팩트라 할말이없네",
          "original_form": "팩트라 할말이없네",
          "speaker_id": "2",
          "time": "20210902 15:40"
        },
        {
          "id": "MDRW2100027835.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100027835.1.12",
          "form": "ㅈㅅ",
          "original_form": "ㅈㅅ",
          "speaker_id": "1",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100027835.1.13",
          "form": "늙어도 그얼굴일듯 지금좀만고생하면",
          "original_form": "늙어도 그얼굴일듯 지금좀만고생하면",
          "speaker_id": "1",
          "time": "20210902 15:41"
        },
        {
          "id": "MDRW2100027835.1.14",
          "form": "예?",
          "original_form": "예?",
          "speaker_id": "2",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100027835.1.15",
          "form": "칭찬인가",
          "original_form": "칭찬인가",
          "speaker_id": "2",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100027835.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100027835.1.17",
          "form": "아니 근데 진짜 저 학생으로봄",
          "original_form": "아니 근데 진짜 저 학생으로봄",
          "speaker_id": "2",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100027835.1.18",
          "form": "예 칭찬^^ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "예 칭찬^^ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100027835.1.19",
          "form": "에이",
          "original_form": "에이",
          "speaker_id": "1",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100027835.1.20",
          "form": "선넘으시네",
          "original_form": "선넘으시네",
          "speaker_id": "1",
          "time": "20210902 15:44"
        },
        {
          "id": "MDRW2100027835.1.21",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210902 15:45"
        },
        {
          "id": "MDRW2100027835.1.22",
          "form": "님보다 어리게볼수도",
          "original_form": "님보다 어리게볼수도",
          "speaker_id": "2",
          "time": "20210902 15:45"
        },
        {
          "id": "MDRW2100027835.1.23",
          "form": "에이 말도안돼",
          "original_form": "에이 말도안돼",
          "speaker_id": "1",
          "time": "20210902 15:45"
        },
        {
          "id": "MDRW2100027835.1.24",
          "form": "그건아닐듯",
          "original_form": "그건아닐듯",
          "speaker_id": "1",
          "time": "20210902 15:45"
        },
        {
          "id": "MDRW2100027835.1.25",
          "form": "저아직고딩으로봄",
          "original_form": "저아직고딩으로봄",
          "speaker_id": "1",
          "time": "20210902 15:46"
        },
        {
          "id": "MDRW2100027835.1.26",
          "form": "오 그건 안되겠네요ㅇㅇ",
          "original_form": "오 그건 안되겠네요ㅇㅇ",
          "speaker_id": "2",
          "time": "20210902 15:46"
        },
        {
          "id": "MDRW2100027835.1.27",
          "form": "전 이제 대딩으로 보는듯",
          "original_form": "전 이제 대딩으로 보는듯",
          "speaker_id": "2",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100027835.1.28",
          "form": "저번달에 집보러가니까 학생 돈있어? 이런 느낌으로 말해서ㅋㅋ",
          "original_form": "저번달에 집보러가니까 학생 돈있어? 이런 느낌으로 말해서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100027835.1.29",
          "form": "웃어넘김",
          "original_form": "웃어넘김",
          "speaker_id": "2",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100027835.1.30",
          "form": "하도 당했더니",
          "original_form": "하도 당했더니",
          "speaker_id": "2",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100027835.1.31",
          "form": "그거 팁달라고 그러는거아님?",
          "original_form": "그거 팁달라고 그러는거아님?",
          "speaker_id": "1",
          "time": "20210902 15:48"
        },
        {
          "id": "MDRW2100027835.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:48"
        },
        {
          "id": "MDRW2100027835.1.33",
          "form": "아 머야",
          "original_form": "아 머야",
          "speaker_id": "2",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027835.1.34",
          "form": "주식장 닫혔네",
          "original_form": "주식장 닫혔네",
          "speaker_id": "2",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027835.1.35",
          "form": "채팅하고나서 주식을 잘 못보네요",
          "original_form": "채팅하고나서 주식을 잘 못보네요",
          "speaker_id": "2",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027835.1.36",
          "form": "일하면서 힐끔힐끔 봐주야되는데",
          "original_form": "일하면서 힐끔힐끔 봐주야되는데",
          "speaker_id": "2",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100027835.1.37",
          "form": "주식장은 컴터로도볼수잇자나요",
          "original_form": "주식장은 컴터로도볼수잇자나요",
          "speaker_id": "1",
          "time": "20210902 15:59"
        },
        {
          "id": "MDRW2100027835.1.38",
          "form": "그건 좀..",
          "original_form": "그건 좀..",
          "speaker_id": "2",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100027835.1.39",
          "form": "선 넘은 느낌이라",
          "original_form": "선 넘은 느낌이라",
          "speaker_id": "2",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100027835.1.40",
          "form": "일과시간에 겜하는거랑 같죠",
          "original_form": "일과시간에 겜하는거랑 같죠",
          "speaker_id": "2",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100027835.1.41",
          "form": "겜 몇개 깔려있긴 하지만",
          "original_form": "겜 몇개 깔려있긴 하지만",
          "speaker_id": "2",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100027835.1.42",
          "form": "암튼",
          "original_form": "암튼",
          "speaker_id": "2",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100027835.1.43",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100027835.1.44",
          "form": "무슨차입니까?",
          "original_form": "무슨차입니까?",
          "speaker_id": "1",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100027835.1.45",
          "form": "눈치보이는 차이?",
          "original_form": "눈치보이는 차이?",
          "speaker_id": "2",
          "time": "20210902 17:05"
        },
        {
          "id": "MDRW2100027835.1.46",
          "form": "아 일 들어왔어요",
          "original_form": "아 일 들어왔어요",
          "speaker_id": "2",
          "time": "20210902 17:05"
        },
        {
          "id": "MDRW2100027835.1.47",
          "form": "설계하나 하긴했는데",
          "original_form": "설계하나 하긴했는데",
          "speaker_id": "2",
          "time": "20210902 17:05"
        },
        {
          "id": "MDRW2100027835.1.48",
          "form": "하나는 업체가 묻지도 따지지도 않고 일감 보냈네",
          "original_form": "하나는 업체가 묻지도 따지지도 않고 일감 보냈네",
          "speaker_id": "2",
          "time": "20210902 17:05"
        },
        {
          "id": "MDRW2100027835.1.49",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:06"
        },
        {
          "id": "MDRW2100027835.1.50",
          "form": "그와중에 일하셨네",
          "original_form": "그와중에 일하셨네",
          "speaker_id": "1",
          "time": "20210902 17:06"
        },
        {
          "id": "MDRW2100027835.1.51",
          "form": "하긴 저도오늘할꺼다함",
          "original_form": "하긴 저도오늘할꺼다함",
          "speaker_id": "1",
          "time": "20210902 17:07"
        },
        {
          "id": "MDRW2100027835.1.52",
          "form": "에이씨 야근이네요ㅠ",
          "original_form": "에이씨 야근이네요ㅠ",
          "speaker_id": "2",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100027835.1.53",
          "form": "허겁지겁 쳐냈지만",
          "original_form": "허겁지겁 쳐냈지만",
          "speaker_id": "2",
          "time": "20210902 17:56"
        },
        {
          "id": "MDRW2100027835.1.54",
          "form": "어림도없지",
          "original_form": "어림도없지",
          "speaker_id": "2",
