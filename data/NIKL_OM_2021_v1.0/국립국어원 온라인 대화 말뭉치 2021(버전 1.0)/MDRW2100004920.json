        },
        {
          "id": "MDRW2100004920.1.2",
          "form": "그럼 교수 정강이를 걷어차면",
          "original_form": "그럼 교수 정강이를 걷어차면",
          "speaker_id": "2",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004920.1.3",
          "form": "아이쿠! 종강이야",
          "original_form": "아이쿠! 종강이야",
          "speaker_id": "2",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004920.1.4",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004920.1.5",
          "form": "노잼이야",
          "original_form": "노잼이야",
          "speaker_id": "1",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004920.1.6",
          "form": "왜그래..",
          "original_form": "왜그래..",
          "speaker_id": "2",
          "time": "20210821 19:27"
        },
        {
          "id": "MDRW2100004920.1.7",
          "form": "개강이 너무 싫어요",
          "original_form": "개강이 너무 싫어요",
          "speaker_id": "1",
          "time": "20210821 19:28"
        },
        {
          "id": "MDRW2100004920.1.8",
          "form": "과제가 벌써 있어요",
          "original_form": "과제가 벌써 있어요",
          "speaker_id": "1",
          "time": "20210821 19:28"
        },
        {
          "id": "MDRW2100004920.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ방학인데!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ방학인데!!",
          "speaker_id": "2",
          "time": "20210821 19:28"
        },
        {
          "id": "MDRW2100004920.1.10",
          "form": "교수님이 메일을 주셨어",
          "original_form": "교수님이 메일을 주셨어",
          "speaker_id": "1",
          "time": "20210821 19:29"
        },
        {
          "id": "MDRW2100004920.1.11",
          "form": "3주차 발표 하겠는가?",
          "original_form": "3주차 발표 하겠는가?",
          "speaker_id": "1",
          "time": "20210821 19:29"
        },
        {
          "id": "MDRW2100004920.1.12",
          "form": "학생은 긍정할 수 밖에...",
          "original_form": "학생은 긍정할 수 밖에...",
          "speaker_id": "1",
          "time": "20210821 19:29"
        },
        {
          "id": "MDRW2100004920.1.13",
          "form": "있는듯 하면서 없는 선택권..",
          "original_form": "있는듯 하면서 없는 선택권..",
          "speaker_id": "2",
          "time": "20210821 19:29"
        },
        {
          "id": "MDRW2100004920.1.14",
          "form": "그냥 없는 거지 선택권...",
          "original_form": "그냥 없는 거지 선택권...",
          "speaker_id": "1",
          "time": "20210821 19:32"
        },
        {
          "id": "MDRW2100004920.1.15",
          "form": "후후..",
          "original_form": "후후..",
          "speaker_id": "2",
          "time": "20210821 19:33"
        },
        {
          "id": "MDRW2100004920.1.16",
          "form": "동아리 많이 하고 싶었는데",
          "original_form": "동아리 많이 하고 싶었는데",
          "speaker_id": "1",
          "time": "20210821 19:34"
        },
        {
          "id": "MDRW2100004920.1.17",
          "form": "방학 다 지나갔어",
          "original_form": "방학 다 지나갔어",
          "speaker_id": "1",
          "time": "20210821 19:34"
        },
        {
          "id": "MDRW2100004920.1.18",
          "form": "앞으로 잘하자..",
          "original_form": "앞으로 잘하자..",
          "speaker_id": "2",
          "time": "20210821 19:34"
        },
        {
          "id": "MDRW2100004920.1.19",
          "form": "코로나가 없어야 학교를 가쥐!!!!!",
          "original_form": "코로나가 없어야 학교를 가쥐!!!!!",
