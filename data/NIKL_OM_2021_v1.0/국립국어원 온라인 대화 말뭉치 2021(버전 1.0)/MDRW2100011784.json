      "utterance": [
        {
          "id": "MDRW2100011784.1.1",
          "form": "건강 중요하지",
          "original_form": "건강 중요하지",
          "speaker_id": "2",
          "time": "20210824 16:03"
        },
        {
          "id": "MDRW2100011784.1.2",
          "form": "다이어트 할때",
          "original_form": "다이어트 할때",
          "speaker_id": "1",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011784.1.3",
          "form": "많이 먹고 운동해서",
          "original_form": "많이 먹고 운동해서",
          "speaker_id": "1",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011784.1.4",
          "form": "많이 빼겠다는건",
          "original_form": "많이 빼겠다는건",
          "speaker_id": "1",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011784.1.5",
          "form": "비에 흠뻑 젖은 옷을",
          "original_form": "비에 흠뻑 젖은 옷을",
          "speaker_id": "1",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011784.1.6",
          "form": "드라이기로 말리겠다는 말이랑 같대",
          "original_form": "드라이기로 말리겠다는 말이랑 같대",
          "speaker_id": "1",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011784.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011784.1.8",
          "form": "근육돼지가 아니라",
          "original_form": "근육돼지가 아니라",
          "speaker_id": "1",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011784.1.9",
          "form": "덜 효율적이긴하지만",
          "original_form": "덜 효율적이긴하지만",
          "speaker_id": "2",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011784.1.10",
          "form": "그냥 근육 코끼리가 되어야 할듯",
          "original_form": "그냥 근육 코끼리가 되어야 할듯",
          "speaker_id": "1",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011784.1.11",
          "form": "어쨋든 목적은 달성하네",
          "original_form": "어쨋든 목적은 달성하네",
          "speaker_id": "2",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011784.1.12",
          "form": "ㅇㅇ근육코끼리 고고",
          "original_form": "ㅇㅇ근육코끼리 고고",
          "speaker_id": "2",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011784.1.13",
          "form": "어우",
          "original_form": "어우",
          "speaker_id": "1",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011784.1.14",
          "form": "꼭 다이어트를 마른몸 되려고 하는건 아니니까뭐",
          "original_form": "꼭 다이어트를 마른몸 되려고 하는건 아니니까뭐",
          "speaker_id": "2",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011784.1.15",
          "form": "난 키크고근육질 되고싶다",
          "original_form": "난 키크고근육질 되고싶다",
          "speaker_id": "2",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011784.1.16",
          "form": "근데 운동하는건",
          "original_form": "근데 운동하는건",
          "speaker_id": "1",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011784.1.17",
          "form": "진짜 필요한것 같다",
          "original_form": "진짜 필요한것 같다",
          "speaker_id": "1",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011784.1.18",
          "form": "맞음",
          "original_form": "맞음",
          "speaker_id": "2",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011784.1.19",
          "form": "체력도 체력이고",
          "original_form": "체력도 체력이고",
          "speaker_id": "1",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011784.1.20",
          "form": "예전에 한혜진이",
          "original_form": "예전에 한혜진이",
          "speaker_id": "1",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011784.1.21",
          "form": "자기 몸도 컨트롤 못하면",
          "original_form": "자기 몸도 컨트롤 못하면",
          "speaker_id": "1",
          "time": "20210824 16:06"
        },
        {
          "id": "MDRW2100011784.1.22",
          "form": "뭘 컨트롤 ㅎ할 수 있겠냐고",
          "original_form": "뭘 컨트롤 ㅎ할 수 있겠냐고",
          "speaker_id": "1",
          "time": "20210824 16:06"
        },
        {
          "id": "MDRW2100011784.1.23",
          "form": "맞음맞음",
          "original_form": "맞음맞음",
          "speaker_id": "2",
          "time": "20210824 16:06"
        },
        {
          "id": "MDRW2100011784.1.24",
          "form": "몸관리가 제일 쉬운거라고 말했을때",
          "original_form": "몸관리가 제일 쉬운거라고 말했을때",
          "speaker_id": "1",
          "time": "20210824 16:06"
        },
        {
          "id": "MDRW2100011784.1.25",
          "form": "뼈맞았다 ㅋㅋㅋㅋ",
          "original_form": "뼈맞았다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:06"
        },
        {
          "id": "MDRW2100011784.1.26",
          "form": "진짜 충격이었어",
          "original_form": "진짜 충격이었어",
          "speaker_id": "1",
          "time": "20210824 16:06"
        }
      ]
    }
