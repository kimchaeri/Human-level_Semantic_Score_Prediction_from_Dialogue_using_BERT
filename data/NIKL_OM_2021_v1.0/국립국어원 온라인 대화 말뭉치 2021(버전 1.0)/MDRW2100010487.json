      "utterance": [
        {
          "id": "MDRW2100010487.1.1",
          "form": "다이어트중이신가요?",
          "original_form": "다이어트중이신가요?",
          "speaker_id": "2",
          "time": "20210823 22:15"
        },
        {
          "id": "MDRW2100010487.1.2",
          "form": "아니요 어느순간부터 다이어트 잘 안해요",
          "original_form": "아니요 어느순간부터 다이어트 잘 안해요",
          "speaker_id": "1",
          "time": "20210823 22:16"
        },
        {
          "id": "MDRW2100010487.1.3",
          "form": "저두요 살은 자연스레 빠지더라구요ㅜㅜ",
          "original_form": "저두요 살은 자연스레 빠지더라구요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 22:16"
        },
        {
          "id": "MDRW2100010487.1.4",
          "form": "살과 근육이 함께 빠지니까 볼품없어요",
          "original_form": "살과 근육이 함께 빠지니까 볼품없어요",
          "speaker_id": "1",
          "time": "20210823 22:16"
        },
        {
          "id": "MDRW2100010487.1.5",
          "form": "그래서 요즘 근력위주 운동중이에요",
          "original_form": "그래서 요즘 근력위주 운동중이에요",
          "speaker_id": "2",
          "time": "20210823 22:16"
        },
        {
          "id": "MDRW2100010487.1.6",
          "form": "어떤 근력운동하세요?",
          "original_form": "어떤 근력운동하세요?",
          "speaker_id": "1",
          "time": "20210823 22:17"
        },
        {
          "id": "MDRW2100010487.1.7",
          "form": "복근이랑 코어운동 위주로 하고있어요",
          "original_form": "복근이랑 코어운동 위주로 하고있어요",
          "speaker_id": "2",
          "time": "20210823 22:17"
        },
        {
          "id": "MDRW2100010487.1.8",
          "form": "코어운동 너무 힘들지않나요ㅜㅜ?",
          "original_form": "코어운동 너무 힘들지않나요ㅜㅜ?",
          "speaker_id": "1",
          "time": "20210823 22:18"
        },
        {
          "id": "MDRW2100010487.1.9",
          "form": "처음엔 힘든데 티가 나는 운동이라 좋아해요",
          "original_form": "처음엔 힘든데 티가 나는 운동이라 좋아해요",
          "speaker_id": "2",
          "time": "20210823 22:19"
        },
        {
          "id": "MDRW2100010487.1.10",
          "form": "복근이 잘 생기는 몸이라 더 뿌듯해요",
          "original_form": "복근이 잘 생기는 몸이라 더 뿌듯해요",
          "speaker_id": "2",
          "time": "20210823 22:19"
        },
        {
          "id": "MDRW2100010487.1.11",
          "form": "축복받으셨네요ㅋㅋ",
          "original_form": "축복받으셨네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:19"
        },
        {
          "id": "MDRW2100010487.1.12",
          "form": "전 필라테스를 다닐까 고민중이",
          "original_form": "전 필라테스를 다닐까 고민중이",
          "speaker_id": "1",
          "time": "20210823 22:19"
        },
        {
          "id": "MDRW2100010487.1.13",
          "form": "필라테스 근력운동 끝판왕이래요",
          "original_form": "필라테스 근력운동 끝판왕이래요",
          "speaker_id": "2",
          "time": "20210823 22:20"
        },
        {
          "id": "MDRW2100010487.1.14",
          "form": "몸선이 진짜 이뻐진대요",
          "original_form": "몸선이 진짜 이뻐진대요",
          "speaker_id": "1",
          "time": "20210823 22:20"
        },
        {
          "id": "MDRW2100010487.1.15",
          "form": "코로나 끝나면 운동 다니고싶어요",
          "original_form": "코로나 끝나면 운동 다니고싶어요",
          "speaker_id": "2",
          "time": "20210823 22:20"
        },
        {
          "id": "MDRW2100010487.1.16",
          "form": "지금은 걷기운동으로 만족입니다",
          "original_form": "지금은 걷기운동으로 만족입니다",
          "speaker_id": "2",
          "time": "20210823 22:20"
        },
        {
          "id": "MDRW2100010487.1.17",
          "form": "지금은 홈트뿐이에요",
          "original_form": "지금은 홈트뿐이에요",
          "speaker_id": "1",
          "time": "20210823 22:21"
        },
        {
          "id": "MDRW2100010487.1.18",
          "form": "저 운동 시작했어요",
          "original_form": "저 운동 시작했어요",
