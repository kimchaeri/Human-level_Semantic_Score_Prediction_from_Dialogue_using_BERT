      "utterance": [
        {
          "id": "MDRW2100040407.1.1",
          "form": "사고싶은 중고물품이있는데",
          "original_form": "사고싶은 중고물품이있는데",
          "speaker_id": "2",
          "time": "20210904 18:28"
        },
        {
          "id": "MDRW2100040407.1.2",
          "form": "가격이너무 비싸요",
          "original_form": "가격이너무 비싸요",
          "speaker_id": "2",
          "time": "20210904 18:28"
        },
        {
          "id": "MDRW2100040407.1.3",
          "form": "머요?,",
          "original_form": "머요?,",
          "speaker_id": "1",
          "time": "20210904 18:28"
        },
        {
          "id": "MDRW2100040407.1.4",
          "form": "링클바진데요 중고물품인데 정가보다 5천원 정도 싸더라구요",
          "original_form": "링클바진데요 중고물품인데 정가보다 5천원 정도 싸더라구요",
          "speaker_id": "2",
          "time": "20210904 18:29"
        },
        {
          "id": "MDRW2100040407.1.5",
          "form": "어디껀데요",
          "original_form": "어디껀데요",
          "speaker_id": "1",
          "time": "20210904 18:29"
        },
        {
          "id": "MDRW2100040407.1.6",
          "form": "마가린핑거스요 정가 95000인가 그런데 매물이 9만원이에요",
          "original_form": "마가린핑거스요 정가 95000인가 그런데 매물이 9만원이에요",
          "speaker_id": "2",
          "time": "20210904 18:30"
        },
        {
          "id": "MDRW2100040407.1.7",
          "form": "새거라고는 하는데..",
          "original_form": "새거라고는 하는데..",
          "speaker_id": "2",
          "time": "20210904 18:30"
        },
        {
          "id": "MDRW2100040407.1.8",
          "form": "컥 비싸다",
          "original_form": "컥 비싸다",
          "speaker_id": "1",
          "time": "20210904 18:30"
        },
        {
          "id": "MDRW2100040407.1.9",
          "form": "입어보심 ?",
          "original_form": "입어보심 ?",
          "speaker_id": "1",
          "time": "20210904 18:30"
        },
        {
          "id": "MDRW2100040407.1.10",
          "form": "다른색으로 입어봤어요 근데 제가 사고싶은 색상은 품절",
          "original_form": "다른색으로 입어봤어요 근데 제가 사고싶은 색상은 품절",
          "speaker_id": "2",
          "time": "20210904 18:30"
        },
        {
          "id": "MDRW2100040407.1.11",
          "form": "오 핏 조아요?",
          "original_form": "오 핏 조아요?",
          "speaker_id": "1",
          "time": "20210904 18:31"
        },
        {
          "id": "MDRW2100040407.1.12",
          "form": "살짝 길이 잘라야하긴 하는데 바지가 특이하고 이쁘더라구요",
          "original_form": "살짝 길이 잘라야하긴 하는데 바지가 특이하고 이쁘더라구요",
          "speaker_id": "2",
          "time": "20210904 18:31"
        },
        {
          "id": "MDRW2100040407.1.13",
          "form": "언제입어봤대요?",
          "original_form": "언제입어봤대요?",
          "speaker_id": "1",
          "time": "20210904 18:32"
        },
        {
          "id": "MDRW2100040407.1.14",
          "form": "다른색을 샀거든요",
          "original_form": "다른색을 샀거든요",
          "speaker_id": "2",
          "time": "20210904 18:38"
        },
        {
          "id": "MDRW2100040407.1.15",
          "form": "그래서 집에서 입어봤죠",
          "original_form": "그래서 집에서 입어봤죠",
          "speaker_id": "2",
          "time": "20210904 18:38"
        },
        {
          "id": "MDRW2100040407.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그러셧구나 착샷 공유하시지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그러셧구나 착샷 공유하시지",
          "speaker_id": "1",
          "time": "20210904 18:38"
        },
        {
          "id": "MDRW2100040407.1.17",
          "form": "아직 길이수선을 못했어요 ㅠ",
          "original_form": "아직 길이수선을 못했어요 ㅠ",
          "speaker_id": "2",
          "time": "20210904 18:38"
        },
        {
          "id": "MDRW2100040407.1.18",
          "form": "ㅋㅋㅋㅋ  궁금해요",
          "original_form": "ㅋㅋㅋㅋ  궁금해요",
          "speaker_id": "1",
          "time": "20210904 18:40"
        },
        {
