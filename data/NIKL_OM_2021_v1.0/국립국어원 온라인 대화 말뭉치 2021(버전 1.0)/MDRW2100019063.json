      "utterance": [
        {
          "id": "MDRW2100019063.1.1",
          "form": "운동 어떤거 하세요?",
          "original_form": "운동 어떤거 하세요?",
          "speaker_id": "2",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019063.1.2",
          "form": "등산 자주해요..",
          "original_form": "등산 자주해요..",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019063.1.3",
          "form": "산 가보셧어요?",
          "original_form": "산 가보셧어요?",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019063.1.4",
          "form": "숨쉬기 운동만 해요 ㅋㅋ",
          "original_form": "숨쉬기 운동만 해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019063.1.5",
          "form": "ㅋㅋㅋ 아무거도 안하시는군여...",
          "original_form": "ㅋㅋㅋ 아무거도 안하시는군여...",
          "speaker_id": "1",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019063.1.6",
          "form": "코로나땜에 홈트 많이 시작하시던데..",
          "original_form": "코로나땜에 홈트 많이 시작하시던데..",
          "speaker_id": "1",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019063.1.7",
          "form": "그래서 뱃살이 ㅎㅎ\r\n건강 생각해서 운동해야 하는데 귀차너서..",
          "original_form": "그래서 뱃살이 ㅎㅎ\r\n건강 생각해서 운동해야 하는데 귀차너서..",
          "speaker_id": "2",
          "time": "20210827 15:05"
        },
        {
          "id": "MDRW2100019063.1.8",
          "form": "운동은 전혀 안해보셧어요?",
          "original_form": "운동은 전혀 안해보셧어요?",
          "speaker_id": "1",
          "time": "20210827 15:06"
        },
        {
          "id": "MDRW2100019063.1.9",
          "form": "음... 네 !!\r\n필라테스 해보고 싶긴해요...",
          "original_form": "음... 네 !!\r\n필라테스 해보고 싶긴해요...",
          "speaker_id": "2",
          "time": "20210827 15:07"
        },
        {
          "id": "MDRW2100019063.1.10",
          "form": "필라테스 은근 어렵다던데..",
          "original_form": "필라테스 은근 어렵다던데..",
          "speaker_id": "1",
          "time": "20210827 15:08"
        },
        {
          "id": "MDRW2100019063.1.11",
          "form": "아.. 몸이 뻣뻣해서 더 힘드려나요",
          "original_form": "아.. 몸이 뻣뻣해서 더 힘드려나요",
          "speaker_id": "2",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019063.1.12",
          "form": "앞으로 팔내려서 땅에 닿아요?",
          "original_form": "앞으로 팔내려서 땅에 닿아요?",
          "speaker_id": "1",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100019063.1.13",
          "form": "허리 숙여서 손끝만 닿아요 ㅋㅋ",
          "original_form": "허리 숙여서 손끝만 닿아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:15"
        },
        {
          "id": "MDRW2100019063.1.14",
          "form": "손바닥 아닿아아여? 혹시 사고로 허리에.철심같은거 심으셧나여? ㅋㅋㅋㅋ",
          "original_form": "손바닥 아닿아아여? 혹시 사고로 허리에.철심같은거 심으셧나여? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:16"
        },
        {
          "id": "MDRW2100019063.1.15",
          "form": "눼???? 서서 허리 굽혀 손바닥이 닿는다구요????",
          "original_form": "눼???? 서서 허리 굽혀 손바닥이 닿는다구요????",
          "speaker_id": "2",
          "time": "20210827 15:30"
        },
        {
          "id": "MDRW2100019063.1.16",
          "form": "마음이 올곧으면 다 닿는건줄.. ㅋㅋㅋ",
          "original_form": "마음이 올곧으면 다 닿는건줄.. ㅋㅋㅋ",
