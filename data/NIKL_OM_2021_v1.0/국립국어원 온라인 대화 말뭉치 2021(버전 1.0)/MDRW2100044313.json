      "utterance": [
        {
          "id": "MDRW2100044313.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044313.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044313.1.3",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044313.1.4",
          "form": "네 닭볶음탕 먹었어요",
          "original_form": "네 닭볶음탕 먹었어요",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044313.1.5",
          "form": "name2님은 드셨나요?",
          "original_form": "&name2&님은 드셨나요?",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044313.1.6",
          "form": "황태국 먹었어요",
          "original_form": "황태국 먹었어요",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044313.1.7",
          "form": "조리 해 드셨어요?",
          "original_form": "조리 해 드셨어요?",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044313.1.8",
          "form": "네 사촌동생이 끓여줬어요",
          "original_form": "네 사촌동생이 끓여줬어요",
          "speaker_id": "1",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044313.1.9",
          "form": "황태국 시원하셨겠네요",
          "original_form": "황태국 시원하셨겠네요",
          "speaker_id": "1",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044313.1.10",
          "form": "직접해줘서 더 맛있었겠네요\r\n전 식당에서 먹었어요ㅎ\r\n오랜만에 황태 맛있더라구요",
          "original_form": "직접해줘서 더 맛있었겠네요\r\n전 식당에서 먹었어요ㅎ\r\n오랜만에 황태 맛있더라구요",
          "speaker_id": "2",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044313.1.11",
          "form": "식사는 매번 직접 해 드세요?",
          "original_form": "식사는 매번 직접 해 드세요?",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044313.1.12",
          "form": "아니요 사먹는 게 더 많은 것 같아요 ㅠㅠ",
          "original_form": "아니요 사먹는 게 더 많은 것 같아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044313.1.13",
          "form": "아니면 밀키트로 해먹어요!",
          "original_form": "아니면 밀키트로 해먹어요!",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044313.1.14",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044313.1.15",
          "form": "저는 무조건 식당가요ㅎ",
          "original_form": "저는 무조건 식당가요ㅎ",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044313.1.16",
          "form": "그래서 매번 다른 메뉴라는 장점이 있어요",
          "original_form": "그래서 매번 다른 메뉴라는 장점이 있어요",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044313.1.17",
          "form": "ㅋㅋㅋㅋ아아",
          "original_form": "ㅋㅋㅋㅋ아아",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044313.1.18",
          "form": "배달은 잘 안 드세요?",
          "original_form": "배달은 잘 안 드세요?",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044313.1.19",
          "form": "배달은 가끔 먹어요",
          "original_form": "배달은 가끔 먹어요",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044313.1.20",
          "form": "치킨이나 찜닭같은ㅋ",
          "original_form": "치킨이나 찜닭같은ㅋ",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044313.1.21",
          "form": "엇 둘다 닭이네요ㅋㅋㅋ",
          "original_form": "엇 둘다 닭이네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044313.1.22",
          "form": "분식은 보통 포장하구요\r\n배달 잘 드세요?",
          "original_form": "분식은 보통 포장하구요\r\n배달 잘 드세요?",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044313.1.23",
          "form": "저는 주로 배달이요...",
          "original_form": "저는 주로 배달이요...",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044313.1.24",
          "form": "코로나라 요샌 외식하기도 찜찜한 것 같아요ㅠㅠ",
          "original_form": "코로나라 요샌 외식하기도 찜찜한 것 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044313.1.25",
          "form": "전 회사 식당이라ㅎ",
          "original_form": "전 회사 식당이라ㅎ",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044313.1.26",
          "form": "거의 매끼 회사에서 해결해요\r\n아님 포장, 배달ㅎ",
          "original_form": "거의 매끼 회사에서 해결해요\r\n아님 포장, 배달ㅎ",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044313.1.27",
          "form": "코로나라 외식 어렵져\r\n근데 또 들어보면 다들 다니는 것 같아요ㅜㅜ",
          "original_form": "코로나라 외식 어렵져\r\n근데 또 들어보면 다들 다니는 것 같아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044313.1.28",
          "form": "오 좋네요",
          "original_form": "오 좋네요",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044313.1.29",
          "form": "구내식당이면 가격도 저렴하겠어요",
          "original_form": "구내식당이면 가격도 저렴하겠어요",
          "speaker_id": "1",
          "time": "20210907 22:24"
        }
      ]
    }
