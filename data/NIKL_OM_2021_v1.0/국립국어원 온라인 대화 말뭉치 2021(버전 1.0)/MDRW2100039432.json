      "utterance": [
        {
          "id": "MDRW2100039432.1.1",
          "form": "요즘 날씨 선선하지 않나요?",
          "original_form": "요즘 날씨 선선하지 않나요?",
          "speaker_id": "1",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100039432.1.2",
          "form": "맞아요 가을느낌 물씬나져",
          "original_form": "맞아요 가을느낌 물씬나져",
          "speaker_id": "2",
          "time": "20210903 17:37"
        },
        {
          "id": "MDRW2100039432.1.3",
          "form": "아침저녁으론 제법 쌀쌀해요",
          "original_form": "아침저녁으론 제법 쌀쌀해요",
          "speaker_id": "2",
          "time": "20210903 17:37"
        },
        {
          "id": "MDRW2100039432.1.4",
          "form": "네 가디건 챙겨야 할거 같아요!",
          "original_form": "네 가디건 챙겨야 할거 같아요!",
          "speaker_id": "1",
          "time": "20210903 17:38"
        },
        {
          "id": "MDRW2100039432.1.5",
          "form": "낮에는 아직 좀 덥긴 더운거 같아요ㅎㅎ",
          "original_form": "낮에는 아직 좀 덥긴 더운거 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 17:38"
        },
        {
          "id": "MDRW2100039432.1.6",
          "form": "낮은 아직 반팔이죠 ㅋㅋㅋ",
          "original_form": "낮은 아직 반팔이죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:39"
        },
        {
          "id": "MDRW2100039432.1.7",
          "form": "슬슬 얇은 긴팔티 하나 사고싶더라구요",
          "original_form": "슬슬 얇은 긴팔티 하나 사고싶더라구요",
          "speaker_id": "2",
          "time": "20210903 17:39"
        },
        {
          "id": "MDRW2100039432.1.8",
          "form": "이제 장만 하셔야 하는 군요!",
          "original_form": "이제 장만 하셔야 하는 군요!",
          "speaker_id": "1",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100039432.1.9",
          "form": "저는 얼마전에 가을초겨울 옷을 샀더니 언능 쌀쌀해졌으면 하는 마음입니다ㅠㅋ",
          "original_form": "저는 얼마전에 가을초겨울 옷을 샀더니 언능 쌀쌀해졌으면 하는 마음입니다ㅠㅋ",
          "speaker_id": "1",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100039432.1.10",
          "form": "네ㅠㅜ 옷장어딘가에 처박혀 있긴 할텐데 ㅋㅋ 계절바뀔때마다 새옷은 사고싶더라구요",
          "original_form": "네ㅠㅜ 옷장어딘가에 처박혀 있긴 할텐데 ㅋㅋ 계절바뀔때마다 새옷은 사고싶더라구요",
          "speaker_id": "2",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039432.1.11",
          "form": "맞아요 옷가게들은 벌써 가을겨울 옷 디피해놔서 사고 싶어지긴 해요ㅎㅎ",
          "original_form": "맞아요 옷가게들은 벌써 가을겨울 옷 디피해놔서 사고 싶어지긴 해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039432.1.12",
          "form": "이번주 계신곳은 비가 왔나요?",
          "original_form": "이번주 계신곳은 비가 왔나요?",
          "speaker_id": "1",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100039432.1.13",
          "form": "네 추적추적 이삼일 비가 오다말다했어요",
          "original_form": "네 추적추적 이삼일 비가 오다말다했어요",
          "speaker_id": "2",
          "time": "20210903 17:46"
        },
        {
          "id": "MDRW2100039432.1.14",
          "form": "그렇군요 확실히 그 더웠던 여름이 언제였나 싶어요",
          "original_form": "그렇군요 확실히 그 더웠던 여름이 언제였나 싶어요",
          "speaker_id": "1",
          "time": "20210903 17:48"
        },
        {
          "id": "MDRW2100039432.1.15",
          "form": "이리 선선해 진거 보면ㅎㅎ",
          "original_form": "이리 선선해 진거 보면ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 17:48"
        },
        {
          "id": "MDRW2100039432.1.16",
          "form": "이런 날씨엔 기분도 센티멘탈해지죠 ㅎㅎㅎ",
          "original_form": "이런 날씨엔 기분도 센티멘탈해지죠 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 17:50"
        },
        {
          "id": "MDRW2100039432.1.17",
          "form": "네 새벽에 깨어있는게 넘 좋은 날씨가 왔어요ㅎㅎ",
          "original_form": "네 새벽에 깨어있는게 넘 좋은 날씨가 왔어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 17:51"
        },
        {
          "id": "MDRW2100039432.1.18",
          "form": "요즘 미세먼지도 없어서 넘 좋네요",
          "original_form": "요즘 미세먼지도 없어서 넘 좋네요",
          "speaker_id": "1",
          "time": "20210903 17:51"
        },
        {
          "id": "MDRW2100039432.1.19",
          "form": "오 일찍일어나시나봐요",
          "original_form": "오 일찍일어나시나봐요",
          "speaker_id": "2",
          "time": "20210903 17:52"
        },
        {
          "id": "MDRW2100039432.1.20",
          "form": "아뇨 늦게 자는 올빼미 입니다...아침 일어나는거 힘들어요ㅜㅠㅋ",
          "original_form": "아뇨 늦게 자는 올빼미 입니다...아침 일어나는거 힘들어요ㅜㅠㅋ",
          "speaker_id": "1",
          "time": "20210903 17:53"
        }
      ]
