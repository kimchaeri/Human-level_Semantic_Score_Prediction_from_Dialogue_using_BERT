      "utterance": [
        {
          "id": "MDRW2100037527.1.1",
          "form": "이제 여유시간 좀 생겼는데",
          "original_form": "이제 여유시간 좀 생겼는데",
          "speaker_id": "1",
          "time": "20210901 10:51"
        },
        {
          "id": "MDRW2100037527.1.2",
          "form": "드라마 뭐볼까요! 왓쳐고민중인데",
          "original_form": "드라마 뭐볼까요! 왓쳐고민중인데",
          "speaker_id": "1",
          "time": "20210901 10:52"
        },
        {
          "id": "MDRW2100037527.1.3",
          "form": "혹시 우리집에멸망이들어왔다? 보셨어요?",
          "original_form": "혹시 우리집에멸망이들어왔다? 보셨어요?",
          "speaker_id": "1",
          "time": "20210901 10:52"
        },
        {
          "id": "MDRW2100037527.1.4",
          "form": "박보영나온거",
          "original_form": "박보영나온거",
          "speaker_id": "1",
          "time": "20210901 10:52"
        },
        {
          "id": "MDRW2100037527.1.5",
          "form": "왓쳐 쉽게 추천하고싶진 않아요 ㅋㅋ 멸망 그거 서인국때문에 보고싶었는데 재밌나요?",
          "original_form": "왓쳐 쉽게 추천하고싶진 않아요 ㅋㅋ 멸망 그거 서인국때문에 보고싶었는데 재밌나요?",
          "speaker_id": "2",
          "time": "20210901 11:27"
        },
        {
          "id": "MDRW2100037527.1.6",
          "form": "저도 안 봤어요ㅋㅋㅋㅋ박보영 서인국 둘다 좋아해서",
          "original_form": "저도 안 봤어요ㅋㅋㅋㅋ박보영 서인국 둘다 좋아해서",
          "speaker_id": "1",
          "time": "20210901 11:32"
        },
        {
          "id": "MDRW2100037527.1.7",
          "form": "볼까말까 고민중.....",
          "original_form": "볼까말까 고민중.....",
          "speaker_id": "1",
          "time": "20210901 11:32"
        },
        {
          "id": "MDRW2100037527.1.8",
          "form": "평이 엄청 좋진 않았던거 같은데 서인국때문에 끌리긴합니다 ㅋㅋㅋ 어떤거 끌리세요? 장르?",
          "original_form": "평이 엄청 좋진 않았던거 같은데 서인국때문에 끌리긴합니다 ㅋㅋㅋ 어떤거 끌리세요? 장르?",
          "speaker_id": "2",
          "time": "20210901 11:34"
        },
        {
          "id": "MDRW2100037527.1.9",
          "form": "갑자기 생각낚는데",
          "original_form": "갑자기 생각낚는데",
          "speaker_id": "1",
          "time": "20210901 11:34"
        },
        {
          "id": "MDRW2100037527.1.10",
          "form": "오월의 청춘 볼까하고요ㅋㅋㅋㅋ 얘기해주셨던",
          "original_form": "오월의 청춘 볼까하고요ㅋㅋㅋㅋ 얘기해주셨던",
          "speaker_id": "1",
          "time": "20210901 11:34"
        },
        {
          "id": "MDRW2100037527.1.11",
          "form": "지금은 장르는 딱 안떠올라요!",
          "original_form": "지금은 장르는 딱 안떠올라요!",
          "speaker_id": "1",
          "time": "20210901 11:35"
        },
        {
          "id": "MDRW2100037527.1.12",
          "form": "오월의 청춘 솔직히 좀 예상가는 결말과 분위기인데 그래도 잘만들었고 볼만했어요 그런작품들 많아져야죠!",
          "original_form": "오월의 청춘 솔직히 좀 예상가는 결말과 분위기인데 그래도 잘만들었고 볼만했어요 그런작품들 많아져야죠!",
          "speaker_id": "2",
          "time": "20210901 11:40"
        },
        {
          "id": "MDRW2100037527.1.13",
          "form": "왕이된남자 보셨어요?",
          "original_form": "왕이된남자 보셨어요?",
          "speaker_id": "2",
          "time": "20210901 11:40"
        },
        {
          "id": "MDRW2100037527.1.14",
          "form": "진구 나오는거죠?",
          "original_form": "진구 나오는거죠?",
          "speaker_id": "1",
          "time": "20210901 11:41"
        },
        {
          "id": "MDRW2100037527.1.15",
          "form": "봤어요ㅋㅋㅋ",
          "original_form": "봤어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:41"
        },
        {
          "id": "MDRW2100037527.1.16",
          "form": "그거 재밌잖아요 ㅜㅜ 뭘 추천해도 이미 본게 많으셔서 도전의식이 불타네요",
          "original_form": "그거 재밌잖아요 ㅜㅜ 뭘 추천해도 이미 본게 많으셔서 도전의식이 불타네요",
          "speaker_id": "2",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100037527.1.17",
          "form": "혼술남녀는 당연히 보셨겠죠",
          "original_form": "혼술남녀는 당연히 보셨겠죠",
          "speaker_id": "2",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100037527.1.18",
          "form": "ㅋㅋㅋㅋ어디 한번 제취향을 저격해보시죠!",
          "original_form": "ㅋㅋㅋㅋ어디 한번 제취향을 저격해보시죠!",
          "speaker_id": "1",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100037527.1.19",
          "form": "헐 그거안봤어요",
          "original_form": "헐 그거안봤어요",
          "speaker_id": "1",
          "time": "20210901 11:44"
        },
        {
          "id": "MDRW2100037527.1.20",
          "form": "공명인가 누가 시험떨어지고 공부안하고 술마시는 장면만 알아요ㅋㅋㅋㅋ재밌어요?",
          "original_form": "공명인가 누가 시험떨어지고 공부안하고 술마시는 장면만 알아요ㅋㅋㅋㅋ재밌어요?",
          "speaker_id": "1",
          "time": "20210901 11:44"
        },
        {
          "id": "MDRW2100037527.1.21",
          "form": "그때 공시준비하느라 안봤거든요ㅋㅋ",
          "original_form": "그때 공시준비하느라 안봤거든요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:44"
        },
        {
          "id": "MDRW2100037527.1.22",
          "form": "그거진짜재밌어요 ㅜㅜ",
          "original_form": "그거진짜재밌어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 11:45"
        },
        {
          "id": "MDRW2100037527.1.23",
          "form": "키 되게 연기잘해요 ㅋㅋㅋㅋ",
          "original_form": "키 되게 연기잘해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:45"
        },
        {
          "id": "MDRW2100037527.1.24",
          "form": "진짜요?ㅋㅋㅋ지금 달리러 갑니다ㅋㅋㅋㅋㅋ",
          "original_form": "진짜요?ㅋㅋㅋ지금 달리러 갑니다ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:49"
        },
        {
          "id": "MDRW2100037527.1.25",
          "form": "아니 ..... 잠시만여.....",
          "original_form": "아니 ..... 잠시만여.....",
          "speaker_id": "1",
          "time": "20210901 11:50"
        },
        {
          "id": "MDRW2100037527.1.26",
          "form": "그거 본방달렸었는데 엄청재밌게봤어요 ㅋㅋㅋ",
          "original_form": "그거 본방달렸었는데 엄청재밌게봤어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:50"
        },
        {
          "id": "MDRW2100037527.1.27",
          "form": "이거 첫 장면이 생맥주나오는데요?",
          "original_form": "이거 첫 장면이 생맥주나오는데요?",
          "speaker_id": "1",
          "time": "20210901 11:50"
        },
        {
          "id": "MDRW2100037527.1.28",
          "form": "ㅋㅋㅋㅋㅋ지금 맥주 까면서 봐야할 거같은데",
          "original_form": "ㅋㅋㅋㅋㅋ지금 맥주 까면서 봐야할 거같은데",
          "speaker_id": "1",
          "time": "20210901 11:50"
        },
        {
          "id": "MDRW2100037527.1.29",
          "form": "계속 술은 나옵니다 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "계속 술은 나옵니다 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:50"
        },
        {
          "id": "MDRW2100037527.1.30",
          "form": "계속술마시는장면으로 시작하고 중간에 계속 술나와요 ㅋㅋㅋ 제목값 합니다 ㅋㅋㅋ",
          "original_form": "계속술마시는장면으로 시작하고 중간에 계속 술나와요 ㅋㅋㅋ 제목값 합니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100037527.1.31",
          "form": "하...지금일단 보다가",
          "original_form": "하...지금일단 보다가",
          "speaker_id": "1",
          "time": "20210901 11:52"
        },
        {
          "id": "MDRW2100037527.1.32",
          "form": "아무래도 저녁엔 맥주마시면서 볼거같아요ㅋㅋㅋ",
          "original_form": "아무래도 저녁엔 맥주마시면서 볼거같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:52"
        }
      ]
    }
