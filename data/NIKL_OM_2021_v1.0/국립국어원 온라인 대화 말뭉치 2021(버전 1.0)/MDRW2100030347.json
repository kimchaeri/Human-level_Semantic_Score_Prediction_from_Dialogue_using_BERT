      "utterance": [
        {
          "id": "MDRW2100030347.1.1",
          "form": "일본식이랑 차이점 모르겠어요",
          "original_form": "일본식이랑 차이점 모르겠어요",
          "speaker_id": "2",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100030347.1.2",
          "form": "소스가 달을 거 같아요",
          "original_form": "소스가 달을 거 같아요",
          "speaker_id": "1",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100030347.1.3",
          "form": "돈까스도 있는데 그게 일본식이에요",
          "original_form": "돈까스도 있는데 그게 일본식이에요",
          "speaker_id": "2",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100030347.1.4",
          "form": "다를거",
          "original_form": "다를거",
          "speaker_id": "1",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100030347.1.5",
          "form": "우동은 그냥 김치 우동입니다",
          "original_form": "우동은 그냥 김치 우동입니다",
          "speaker_id": "2",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100030347.1.6",
          "form": "옛날식 돈까스도 맛있죠",
          "original_form": "옛날식 돈까스도 맛있죠",
          "speaker_id": "1",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100030347.1.7",
          "form": "서면에 돈까스 맛집 생각나나요",
          "original_form": "서면에 돈까스 맛집 생각나나요",
          "speaker_id": "1",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100030347.1.8",
          "form": "노포역에서 사 먹었던 우동이이 생각이 나네요",
          "original_form": "노포역에서 사 먹었던 우동이이 생각이 나네요",
          "speaker_id": "2",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100030347.1.9",
          "form": "3,500원 이었죠",
          "original_form": "3,500원 이었죠",
          "speaker_id": "1",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100030347.1.10",
          "form": "서면 돈까스 집 저렴하고 맛있었죠",
          "original_form": "서면 돈까스 집 저렴하고 맛있었죠",
          "speaker_id": "2",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100030347.1.11",
          "form": "모든 역에서 먹어야 돼요",
          "original_form": "모든 역에서 먹어야 돼요",
          "speaker_id": "1",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100030347.1.12",
          "form": "양도 많구요",
          "original_form": "양도 많구요",
          "speaker_id": "2",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100030347.1.13",
          "form": "그 돈까스 집 사라졌죠",
          "original_form": "그 돈까스 집 사라졌죠",
          "speaker_id": "1",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100030347.1.14",
          "form": "우리가 남기고 옆에 있던 고등학생이 먹었던 거 생각나네요",
          "original_form": "우리가 남기고 옆에 있던 고등학생이 먹었던 거 생각나네요",
          "speaker_id": "2",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100030347.1.15",
          "form": "어디로 갔을까요",
          "original_form": "어디로 갔을까요",
          "speaker_id": "1",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100030347.1.16",
          "form": "추억의 맛집이네요",
          "original_form": "추억의 맛집이네요",
          "speaker_id": "2",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100030347.1.17",
          "form": "남성여고 쫄우동 생각나네요",
          "original_form": "남성여고 쫄우동 생각나네요",
          "speaker_id": "1",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100030347.1.18",
          "form": "음성인식 정말 편리하네요",
          "original_form": "음성인식 정말 편리하네요",
          "speaker_id": "2",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030347.1.19",
          "form": "맞아요 학생이 먹었죠",
          "original_form": "맞아요 학생이 먹었죠",
          "speaker_id": "1",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030347.1.20",
          "form": "정말 맛있었죠",
          "original_form": "정말 맛있었죠",
          "speaker_id": "2",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030347.1.21",
          "form": "완전 편리합니다",
          "original_form": "완전 편리합니다",
          "speaker_id": "1",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030347.1.22",
          "form": "그 기억이 오래 가네요",
          "original_form": "그 기억이 오래 가네요",
          "speaker_id": "2",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030347.1.23",
          "form": "신기방기",
          "original_form": "신기방기",
          "speaker_id": "1",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030347.1.24",
          "form": "신기방기도 인식하네요",
          "original_form": "신기방기도 인식하네요",
          "speaker_id": "1",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030347.1.25",
          "form": "음식 얘기는 정말 풍부하네요",
          "original_form": "음식 얘기는 정말 풍부하네요",
          "speaker_id": "2",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030347.1.26",
          "form": "이야기거리가 끊기지 않아요",
          "original_form": "이야기거리가 끊기지 않아요",
          "speaker_id": "2",
          "time": "20210904 00:02"
        }
