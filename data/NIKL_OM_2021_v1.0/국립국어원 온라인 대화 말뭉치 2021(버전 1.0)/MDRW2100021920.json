      "utterance": [
        {
          "id": "MDRW2100021920.1.1",
          "form": "랜선집사는 너무 아쉬워요",
          "original_form": "랜선집사는 너무 아쉬워요",
          "speaker_id": "1",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100021920.1.2",
          "form": "치타라니 날쌘가요?",
          "original_form": "치타라니 날쌘가요?",
          "speaker_id": "2",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100021920.1.3",
          "form": "만지고싶고 안고 싶어요",
          "original_form": "만지고싶고 안고 싶어요",
          "speaker_id": "1",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100021920.1.4",
          "form": "넵 무섭",
          "original_form": "넵 무섭",
          "speaker_id": "1",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100021920.1.5",
          "form": "까매요",
          "original_form": "까매요",
          "speaker_id": "1",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100021920.1.6",
          "form": "그쵸.. 랜선집사는 대리만족이긴 한데 ㅋㅋ",
          "original_form": "그쵸.. 랜선집사는 대리만족이긴 한데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100021920.1.7",
          "form": "근데 내가 키우지않으면 고양이는 만지기 힘든거 같아요",
          "original_form": "근데 내가 키우지않으면 고양이는 만지기 힘든거 같아요",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100021920.1.8",
          "form": "재택근무 하시나요?",
          "original_form": "재택근무 하시나요?",
          "speaker_id": "1",
          "time": "20210829 22:39"
        },
        {
          "id": "MDRW2100021920.1.9",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210829 22:42"
        },
        {
          "id": "MDRW2100021920.1.10",
          "form": "하시나요?",
          "original_form": "하시나요?",
          "speaker_id": "2",
          "time": "20210829 22:43"
        },
        {
          "id": "MDRW2100021920.1.11",
          "form": "네 가끔",
          "original_form": "네 가끔",
          "speaker_id": "1",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100021920.1.12",
          "form": "월말이라 요즘은 못해요",
          "original_form": "월말이라 요즘은 못해요",
          "speaker_id": "1",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100021920.1.13",
          "form": "월말은 일이 많으니까요",
          "original_form": "월말은 일이 많으니까요",
          "speaker_id": "2",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100021920.1.14",
          "form": "네 이럴때 재택하다 사고치면 ㅜㅜ",
          "original_form": "네 이럴때 재택하다 사고치면 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 22:58"
        },
        {
          "id": "MDRW2100021920.1.15",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "2",
          "time": "20210829 22:58"
        },
        {
          "id": "MDRW2100021920.1.16",
          "form": "재택은 장단점이...",
          "original_form": "재택은 장단점이...",
          "speaker_id": "1",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100021920.1.17",
          "form": "월말 월초는 일이 몰려있다보니까 다른일 하기도 힘들고 ㅠㅠ",
          "original_form": "월말 월초는 일이 몰려있다보니까 다른일 하기도 힘들고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100021920.1.18",
          "form": "맞아요 긴장하게 되고",
          "original_form": "맞아요 긴장하게 되고",
          "speaker_id": "1",
