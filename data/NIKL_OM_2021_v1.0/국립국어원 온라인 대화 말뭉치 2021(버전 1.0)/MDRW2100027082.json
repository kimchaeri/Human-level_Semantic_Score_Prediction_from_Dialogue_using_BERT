      "utterance": [
        {
          "id": "MDRW2100027082.1.1",
          "form": "어디 가고픈데 있으신가요",
          "original_form": "어디 가고픈데 있으신가요",
          "speaker_id": "2",
          "time": "20210902 09:19"
        },
        {
          "id": "MDRW2100027082.1.2",
          "form": "저는 부산 가고 싶네요",
          "original_form": "저는 부산 가고 싶네요",
          "speaker_id": "1",
          "time": "20210902 09:20"
        },
        {
          "id": "MDRW2100027082.1.3",
          "form": "부산 좋죠 뭐 드시고 싶으신가요",
          "original_form": "부산 좋죠 뭐 드시고 싶으신가요",
          "speaker_id": "2",
          "time": "20210902 09:20"
        },
        {
          "id": "MDRW2100027082.1.4",
          "form": "밀면 먹고 싶어요",
          "original_form": "밀면 먹고 싶어요",
          "speaker_id": "1",
          "time": "20210902 09:21"
        },
        {
          "id": "MDRW2100027082.1.5",
          "form": "집주변에 있던 밀면집이 사라져서ㅠ",
          "original_form": "집주변에 있던 밀면집이 사라져서ㅠ",
          "speaker_id": "1",
          "time": "20210902 09:21"
        },
        {
          "id": "MDRW2100027082.1.6",
          "form": "돼지국밥도!",
          "original_form": "돼지국밥도!",
          "speaker_id": "2",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100027082.1.7",
          "form": "맞아요 국밥도 먹고 싶어요",
          "original_form": "맞아요 국밥도 먹고 싶어요",
          "speaker_id": "1",
          "time": "20210902 09:52"
        },
        {
          "id": "MDRW2100027082.1.8",
          "form": "ㅋㅋ아 여수도 가보고 싶은데",
          "original_form": "ㅋㅋ아 여수도 가보고 싶은데",
          "speaker_id": "2",
          "time": "20210902 09:55"
        },
        {
          "id": "MDRW2100027082.1.9",
          "form": "가보셨어요?",
          "original_form": "가보셨어요?",
          "speaker_id": "2",
          "time": "20210902 09:55"
        },
        {
          "id": "MDRW2100027082.1.10",
          "form": "딱 한번 가봤어요",
          "original_form": "딱 한번 가봤어요",
          "speaker_id": "1",
          "time": "20210902 09:55"
        },
        {
          "id": "MDRW2100027082.1.11",
          "form": "이쁜가요 ㅎㅎ",
          "original_form": "이쁜가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 09:58"
        },
        {
          "id": "MDRW2100027082.1.12",
          "form": "아쉽지만 밤바다에는 못가봤어요",
          "original_form": "아쉽지만 밤바다에는 못가봤어요",
          "speaker_id": "1",
          "time": "20210902 10:00"
        },
        {
          "id": "MDRW2100027082.1.13",
          "form": "ㅋㅋ여수 밤바다가 보고 싶은데요 사실",
          "original_form": "ㅋㅋ여수 밤바다가 보고 싶은데요 사실",
          "speaker_id": "2",
          "time": "20210902 10:06"
        },
        {
          "id": "MDRW2100027082.1.14",
          "form": "노래 같은 감성을 느낄 수 있을까요",
          "original_form": "노래 같은 감성을 느낄 수 있을까요",
          "speaker_id": "1",
          "time": "20210902 10:09"
        },
        {
          "id": "MDRW2100027082.1.15",
          "form": "사실 거품 아닐까요 ㅋㅋ",
          "original_form": "사실 거품 아닐까요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100027082.1.16",
          "form": "그럴 수도 있죠",
          "original_form": "그럴 수도 있죠",
          "speaker_id": "1",
          "time": "20210902 10:30"
        },
        {
          "id": "MDRW2100027082.1.17",
          "form": "여행 가고 싶네요 ㅎㅎ",
          "original_form": "여행 가고 싶네요 ㅎㅎ",
          "speaker_id": "2",
