      "utterance": [
        {
          "id": "MDRW2100005251.1.1",
          "form": "스포츠 뭐좋아하세요?",
          "original_form": "스포츠 뭐좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 21:28"
        },
        {
          "id": "MDRW2100005251.1.2",
          "form": "올림픽 즐겨봤어요ㅎ",
          "original_form": "올림픽 즐겨봤어요ㅎ",
          "speaker_id": "1",
          "time": "20210821 21:29"
        },
        {
          "id": "MDRW2100005251.1.3",
          "form": "어릴때부터 올림픽은 날새며 보고했는데",
          "original_form": "어릴때부터 올림픽은 날새며 보고했는데",
          "speaker_id": "1",
          "time": "20210821 21:29"
        },
        {
          "id": "MDRW2100005251.1.4",
          "form": "올림픽 보셨어요?",
          "original_form": "올림픽 보셨어요?",
          "speaker_id": "1",
          "time": "20210821 21:29"
        },
        {
          "id": "MDRW2100005251.1.5",
          "form": "잠깐 잠깐봤아요ㅎ",
          "original_form": "잠깐 잠깐봤아요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005251.1.6",
          "form": "원래 올림픽이나 월드컵 이런거 많이 챙겨보는 편인데 이번올림픽은 안보게되더라구요ㅎ",
          "original_form": "원래 올림픽이나 월드컵 이런거 많이 챙겨보는 편인데 이번올림픽은 안보게되더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:31"
        },
        {
          "id": "MDRW2100005251.1.7",
          "form": "어떤 종목을 응원했어요?",
          "original_form": "어떤 종목을 응원했어요?",
          "speaker_id": "2",
          "time": "20210821 21:31"
        },
        {
          "id": "MDRW2100005251.1.8",
          "form": "주말엔 거의다챙겨보고 ㅎ 회사서는 양궁 몰래틀고보고ㅎ",
          "original_form": "주말엔 거의다챙겨보고 ㅎ 회사서는 양궁 몰래틀고보고ㅎ",
          "speaker_id": "1",
          "time": "20210821 21:32"
        },
        {
          "id": "MDRW2100005251.1.9",
          "form": "양궁이 최고인듯요",
          "original_form": "양궁이 최고인듯요",
          "speaker_id": "1",
          "time": "20210821 21:32"
        },
        {
          "id": "MDRW2100005251.1.10",
          "form": "맞아요ㅎㅎ 양궁이 최고였어요ㅎ",
          "original_form": "맞아요ㅎㅎ 양궁이 최고였어요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100005251.1.11",
          "form": "여자배구는 보셨어요?",
          "original_form": "여자배구는 보셨어요?",
          "speaker_id": "2",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100005251.1.12",
          "form": "심장떨릴거같은데 다들강심장인가봐요",
          "original_form": "심장떨릴거같은데 다들강심장인가봐요",
          "speaker_id": "1",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100005251.1.13",
          "form": "배구도 봤어요",
          "original_form": "배구도 봤어요",
          "speaker_id": "1",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100005251.1.14",
          "form": "한일전 짜릿",
          "original_form": "한일전 짜릿",
          "speaker_id": "1",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100005251.1.15",
          "form": "맞아요ㅎㅎ 한일전 엄청 응원했네요ㅎ",
          "original_form": "맞아요ㅎㅎ 한일전 엄청 응원했네요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005251.1.16",
          "form": "양궁도 재밌었고ㅎ 다재밌었는데 아쉬운게 야구랑 축구ㅠ",
          "original_form": "양궁도 재밌었고ㅎ 다재밌었는데 아쉬운게 야구랑 축구ㅠ",
          "speaker_id": "2",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100005251.1.17",
          "form": "저도 그래서 보다말았어요",
          "original_form": "저도 그래서 보다말았어요",
          "speaker_id": "1",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005251.1.18",
          "form": "주로 양궁 펜싱 유도 체조",
          "original_form": "주로 양궁 펜싱 유도 체조",
          "speaker_id": "1",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005251.1.19",
          "form": "다이빙도 보고 수영  이것저것 다보긴 했네요",
          "original_form": "다이빙도 보고 수영  이것저것 다보긴 했네요",
          "speaker_id": "1",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005251.1.20",
          "form": "야구축구만빼고",
          "original_form": "야구축구만빼고",
          "speaker_id": "1",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100005251.1.21",
          "form": "엄청 많이 보셨내요ㅎㅎ",
          "original_form": "엄청 많이 보셨내요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100005251.1.22",
          "form": "와 그럴시간이 있으셨어요?",
          "original_form": "와 그럴시간이 있으셨어요?",
          "speaker_id": "2",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100005251.1.23",
          "form": "전 하이라이트만 봤던것같애요ㅎㅎ",
          "original_form": "전 하이라이트만 봤던것같애요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100005251.1.24",
          "form": "저녁에하는것도 많아서 퇴근함 올림픽만 본거같아요",
          "original_form": "저녁에하는것도 많아서 퇴근함 올림픽만 본거같아요",
          "speaker_id": "1",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005251.1.25",
          "form": "스포츠를 원래 좋아하신가보네여 ㅎ",
          "original_form": "스포츠를 원래 좋아하신가보네여 ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005251.1.26",
          "form": "여자가 좋아하기 쉽지않는데",
          "original_form": "여자가 좋아하기 쉽지않는데",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005251.1.27",
          "form": "경기 관람하시는것도 좋아하세요?",
          "original_form": "경기 관람하시는것도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 21:40"
        },
        {
          "id": "MDRW2100005251.1.28",
          "form": "관람은 어릴때잠깐ㅋ",
          "original_form": "관람은 어릴때잠깐ㅋ",
          "speaker_id": "1",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005251.1.29",
          "form": "농구좋아했어요",
          "original_form": "농구좋아했어요",
          "speaker_id": "1",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005251.1.30",
          "form": "농구 좋죠ㅎㅎ",
          "original_form": "농구 좋죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005251.1.31",
          "form": "전 나이가있어서ㅎ 이상민세대에요ㅎ 문경민하고ㅎㅎ",
          "original_form": "전 나이가있어서ㅎ 이상민세대에요ㅎ 문경민하고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005251.1.32",
          "form": "저도요ㅎ",
          "original_form": "저도요ㅎ",
          "speaker_id": "1",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005251.1.33",
          "form": "전 김훈좋아했어요",
          "original_form": "전 김훈좋아했어요",
          "speaker_id": "1",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005251.1.34",
          "form": "ㅎㅎ 생각나네요ㅎ",
          "original_form": "ㅎㅎ 생각나네요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005251.1.35",
          "form": "그때도 지금도 농구선수들은 어찌 키도크고 잘생겼을까요?",
          "original_form": "그때도 지금도 농구선수들은 어찌 키도크고 잘생겼을까요?",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005251.1.36",
          "form": "거기다 운동도 잘해ㅎ",
          "original_form": "거기다 운동도 잘해ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005251.1.37",
          "form": "그렇게요 펜싱선수들 이번에보니 다잘생겼더라구요",
          "original_form": "그렇게요 펜싱선수들 이번에보니 다잘생겼더라구요",
          "speaker_id": "1",
          "time": "20210821 21:51"
        }
      ]
    }
