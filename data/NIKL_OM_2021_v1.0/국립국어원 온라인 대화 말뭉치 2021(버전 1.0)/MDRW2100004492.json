      "utterance": [
        {
          "id": "MDRW2100004492.1.1",
          "form": "비와요?",
          "original_form": "비와요?",
          "speaker_id": "1",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004492.1.2",
          "form": "여기는 비가 내리네요",
          "original_form": "여기는 비가 내리네요",
          "speaker_id": "2",
          "time": "20210821 16:49"
        },
        {
          "id": "MDRW2100004492.1.3",
          "form": "요즘 일교차가 많이 크죠",
          "original_form": "요즘 일교차가 많이 크죠",
          "speaker_id": "2",
          "time": "20210821 16:49"
        },
        {
          "id": "MDRW2100004492.1.4",
          "form": "네 가늠을 할 수가 없어요",
          "original_form": "네 가늠을 할 수가 없어요",
          "speaker_id": "1",
          "time": "20210821 16:50"
        },
        {
          "id": "MDRW2100004492.1.5",
          "form": "물난리는 안났죠?",
          "original_form": "물난리는 안났죠?",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004492.1.6",
          "form": "중국이랑 일본은 침수 때문에 난리네요",
          "original_form": "중국이랑 일본은 침수 때문에 난리네요",
          "speaker_id": "2",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004492.1.7",
          "form": "네 1000 왔다고",
          "original_form": "네 1000 왔다고",
          "speaker_id": "1",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004492.1.8",
          "form": "비 오는 날에는 외출도 힘들어지네요",
          "original_form": "비 오는 날에는 외출도 힘들어지네요",
          "speaker_id": "2",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004492.1.9",
          "form": "바람도 많이 불죠?",
          "original_form": "바람도 많이 불죠?",
          "speaker_id": "2",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004492.1.10",
          "form": "네 추워요",
          "original_form": "네 추워요",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004492.1.11",
          "form": "아직 8월인데 벌써 가을 같네요",
          "original_form": "아직 8월인데 벌써 가을 같네요",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004492.1.12",
          "form": "이번 여름 더위가 장난 아니였는데 어떻게 보냈나요?",
          "original_form": "이번 여름 더위가 장난 아니였는데 어떻게 보냈나요?",
          "speaker_id": "2",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004492.1.13",
          "form": "숨막혀서 죽더라구요",
          "original_form": "숨막혀서 죽더라구요",
          "speaker_id": "1",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004492.1.14",
          "form": "덥운데다 습하기까지 해서 더 힘든 것 같아요",
          "original_form": "덥운데다 습하기까지 해서 더 힘든 것 같아요",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004492.1.15",
          "form": "이번 장마 언제 끝난다고 하나요?",
          "original_form": "이번 장마 언제 끝난다고 하나요?",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004492.1.16",
          "form": "다음주내내이던데요",
          "original_form": "다음주내내이던데요",
          "speaker_id": "1",
          "time": "20210821 17:08"
        },
        {
          "id": "MDRW2100004492.1.17",
          "form": "생각보다 오래 가네요",
          "original_form": "생각보다 오래 가네요",
          "speaker_id": "2",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004492.1.18",
          "form": "여름보다 가을장마가 더 말썽이네요",
          "original_form": "여름보다 가을장마가 더 말썽이네요",
          "speaker_id": "2",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004492.1.19",
          "form": "그르게요",
          "original_form": "그르게요",
          "speaker_id": "1",
