      "utterance": [
        {
          "id": "MDRW2100014300.1.1",
          "form": "동호회 가입하신 적 있으세요?",
          "original_form": "동호회 가입하신 적 있으세요?",
          "speaker_id": "1",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100014300.1.2",
          "form": "아니요 name2은 있으신가요?",
          "original_form": "아니요 &name2&은 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014300.1.3",
          "form": "전 등산동호회 가입 신청서만 내고",
          "original_form": "전 등산동호회 가입 신청서만 내고",
          "speaker_id": "1",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100014300.1.4",
          "form": "참석을 안했던 기억이 나네요.",
          "original_form": "참석을 안했던 기억이 나네요.",
          "speaker_id": "1",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100014300.1.5",
          "form": "앜ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014300.1.6",
          "form": "죄송해요 너무 웃기네요ㅋㅋㅋㅋ",
          "original_form": "죄송해요 너무 웃기네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014300.1.7",
          "form": "그럼 보통 연락 안오나요?",
          "original_form": "그럼 보통 연락 안오나요?",
          "speaker_id": "2",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014300.1.8",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014300.1.9",
          "form": "연락 왔는데 애매한데? 생각하다",
          "original_form": "연락 왔는데 애매한데? 생각하다",
          "speaker_id": "1",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014300.1.10",
          "form": "차단을 눌렀더랬죠",
          "original_form": "차단을 눌렀더랬죠",
          "speaker_id": "1",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014300.1.11",
          "form": "근데 신청서를 내셨던 이유가 있나요?",
          "original_form": "근데 신청서를 내셨던 이유가 있나요?",
          "speaker_id": "2",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100014300.1.12",
          "form": "그땐 흥미로워 보였어요",
          "original_form": "그땐 흥미로워 보였어요",
          "speaker_id": "1",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014300.1.13",
          "form": "뭔가 열쩡!!!! 파이팅!!!",
          "original_form": "뭔가 열쩡!!!! 파이팅!!!",
          "speaker_id": "1",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014300.1.14",
          "form": "이런 느낌이요",
          "original_form": "이런 느낌이요",
          "speaker_id": "1",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014300.1.15",
          "form": "아 근데 그 열정이 며칠만에 사그라져 버렸군요",
          "original_form": "아 근데 그 열정이 며칠만에 사그라져 버렸군요",
          "speaker_id": "2",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100014300.1.16",
          "form": "그렇죠 바람처럼 날아갔죠",
          "original_form": "그렇죠 바람처럼 날아갔죠",
          "speaker_id": "1",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100014300.1.17",
          "form": "혹시 그럼 동호회 말고 정기적으로 나가시는 모임 있으세요?",
          "original_form": "혹시 그럼 동호회 말고 정기적으로 나가시는 모임 있으세요?",
          "speaker_id": "1",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100014300.1.18",
          "form": "저는 진짜 집순이라 없어요",
          "original_form": "저는 진짜 집순이라 없어요",
          "speaker_id": "2",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100014300.1.19",
          "form": "name2은 있으신가요?",
          "original_form": "&name2&은 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100014300.1.20",
          "form": "전 봉사는 오래 다녔는데",
          "original_form": "전 봉사는 오래 다녔는데",
          "speaker_id": "1",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100014300.1.21",
          "form": "회사다니면서 시간 맞추기가 어려워서",
          "original_form": "회사다니면서 시간 맞추기가 어려워서",
          "speaker_id": "1",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100014300.1.22",
          "form": "요새는 다니는 곳 없네요",
          "original_form": "요새는 다니는 곳 없네요",
          "speaker_id": "1",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100014300.1.23",
          "form": "그래도 봉사 오래 다니셨다니 멋지신데요",
          "original_form": "그래도 봉사 오래 다니셨다니 멋지신데요",
          "speaker_id": "2",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100014300.1.24",
          "form": "관성적으로 다니다 보니깐 그렇더라구요",
          "original_form": "관성적으로 다니다 보니깐 그렇더라구요",
          "speaker_id": "1",
          "time": "20210825 18:39"
        },
        {
          "id": "MDRW2100014300.1.25",
          "form": "요샌 줌으로 모임이나 회의를 많이 하던대",
          "original_form": "요샌 줌으로 모임이나 회의를 많이 하던대",
          "speaker_id": "1",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014300.1.26",
          "form": "집순이에게 줌 모임은 더 좋은 선택지인가요?",
          "original_form": "집순이에게 줌 모임은 더 좋은 선택지인가요?",
          "speaker_id": "1",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014300.1.27",
          "form": "그쵸 집순이는 나가는 것도 귀찮거든요ㅋㅋㅋ",
          "original_form": "그쵸 집순이는 나가는 것도 귀찮거든요ㅋㅋㅋ",
