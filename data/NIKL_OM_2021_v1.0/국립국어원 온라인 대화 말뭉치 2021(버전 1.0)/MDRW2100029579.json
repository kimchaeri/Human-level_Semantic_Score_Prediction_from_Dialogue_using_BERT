      "utterance": [
        {
          "id": "MDRW2100029579.1.1",
          "form": "마라탕 좋아하세요?",
          "original_form": "마라탕 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 10:36"
        },
        {
          "id": "MDRW2100029579.1.2",
          "form": "네 정말 좋아해요",
          "original_form": "네 정말 좋아해요",
          "speaker_id": "2",
          "time": "20210903 10:37"
        },
        {
          "id": "MDRW2100029579.1.3",
          "form": "마라탕 좋아하시나요?",
          "original_form": "마라탕 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210903 10:37"
        },
        {
          "id": "MDRW2100029579.1.4",
          "form": "네! 어제도 먹었어요",
          "original_form": "네! 어제도 먹었어요",
          "speaker_id": "1",
          "time": "20210903 10:37"
        },
        {
          "id": "MDRW2100029579.1.5",
          "form": "매운 거 잘 드세요?",
          "original_form": "매운 거 잘 드세요?",
          "speaker_id": "1",
          "time": "20210903 10:38"
        },
        {
          "id": "MDRW2100029579.1.6",
          "form": "네 정말 매운거 좋아해요",
          "original_form": "네 정말 매운거 좋아해요",
          "speaker_id": "2",
          "time": "20210903 10:38"
        },
        {
          "id": "MDRW2100029579.1.7",
          "form": "저는 맵찔이라 엽떡 같은 건 쳐다도 안보는데",
          "original_form": "저는 맵찔이라 엽떡 같은 건 쳐다도 안보는데",
          "speaker_id": "1",
          "time": "20210903 10:39"
        },
        {
          "id": "MDRW2100029579.1.8",
          "form": "라면이나 그냥 음식은 매운걸 좋아해요 칼칼한거",
          "original_form": "라면이나 그냥 음식은 매운걸 좋아해요 칼칼한거",
          "speaker_id": "2",
          "time": "20210903 10:39"
        },
        {
          "id": "MDRW2100029579.1.9",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "2",
          "time": "20210903 10:39"
        },
        {
          "id": "MDRW2100029579.1.10",
          "form": "마라탕은 맵기 보통으로 하면 괜찮더라고요",
          "original_form": "마라탕은 맵기 보통으로 하면 괜찮더라고요",
          "speaker_id": "1",
          "time": "20210903 10:39"
        },
        {
          "id": "MDRW2100029579.1.11",
          "form": "전 신라면 정도의 맴기로 해요!",
          "original_form": "전 신라면 정도의 맴기로 해요!",
          "speaker_id": "2",
          "time": "20210903 10:39"
        },
        {
          "id": "MDRW2100029579.1.12",
          "form": "뭔가 떡볶이의 매움이랑 마라탕의 매움은 좀 다르게 느껴지는 것 같아요",
          "original_form": "뭔가 떡볶이의 매움이랑 마라탕의 매움은 좀 다르게 느껴지는 것 같아요",
          "speaker_id": "1",
          "time": "20210903 10:40"
        },
        {
          "id": "MDRW2100029579.1.13",
          "form": "마라탕에 뭐 넣는 거 좋아하세요?",
          "original_form": "마라탕에 뭐 넣는 거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 10:40"
        },
        {
          "id": "MDRW2100029579.1.14",
          "form": "약간 마라탕은 화 한것같아요",
          "original_form": "약간 마라탕은 화 한것같아요",
          "speaker_id": "2",
          "time": "20210903 10:41"
        },
        {
          "id": "MDRW2100029579.1.15",
          "form": "전 화한거 좋아해요 일단 청경채 숙주 버섯 완자 가끔 소세지 야채위주로넣어요",
          "original_form": "전 화한거 좋아해요 일단 청경채 숙주 버섯 완자 가끔 소세지 야채위주로넣어요",
          "speaker_id": "2",
          "time": "20210903 10:41"
        },
        {
          "id": "MDRW2100029579.1.16",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 10:41"
        },
        {
          "id": "MDRW2100029579.1.17",
          "form": "마라탕ㅇ에 뭐 넣는거 좋아하시나요?",
          "original_form": "마라탕ㅇ에 뭐 넣는거 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210903 10:41"
        },
        {
          "id": "MDRW2100029579.1.18",
          "form": "전 버섯 많이 넣고 고기추가에 중국당면이랑 숙주 주로 넣어요",
          "original_form": "전 버섯 많이 넣고 고기추가에 중국당면이랑 숙주 주로 넣어요",
          "speaker_id": "1",
          "time": "20210903 10:43"
        },
        {
          "id": "MDRW2100029579.1.19",
          "form": "꼬치류는 비싸서 잘 안넣는데 어젠 친구들이랑 먹는다고 새우꼬치를 많이 넣었더니 맛있더라고요",
          "original_form": "꼬치류는 비싸서 잘 안넣는데 어젠 친구들이랑 먹는다고 새우꼬치를 많이 넣었더니 맛있더라고요",
          "speaker_id": "1",
          "time": "20210903 10:43"
        },
        {
          "id": "MDRW2100029579.1.20",
          "form": "새우꼬치 맛있겠어요!!",
          "original_form": "새우꼬치 맛있겠어요!!",
          "speaker_id": "2",
          "time": "20210903 10:45"
        },
        {
          "id": "MDRW2100029579.1.21",
          "form": "ㅁ맞아요 항상 무게로 결정되니까 부담될때ㄷ많은 것 같아요",
          "original_form": "ㅁ맞아요 항상 무게로 결정되니까 부담될때ㄷ많은 것 같아요",
          "speaker_id": "2",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100029579.1.22",
          "form": "고기는 뭐 추가하시나요?",
          "original_form": "고기는 뭐 추가하시나요?",
          "speaker_id": "2",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100029579.1.23",
          "form": "어제는 두 그릇 시켜서 소고기 양고기 하나씩 추가했어요",
          "original_form": "어제는 두 그릇 시켜서 소고기 양고기 하나씩 추가했어요",
          "speaker_id": "1",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100029579.1.24",
          "form": "보통은 둘 중에 그때그때 끌리는 거 넣어요",
          "original_form": "보통은 둘 중에 그때그때 끌리는 거 넣어요",
          "speaker_id": "1",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100029579.1.25",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "20210903 10:46"
        },
        {
          "id": "MDRW2100029579.1.26",
          "form": "저도 양고기 좋아해요 소고기는 넣어본적 없는 것 같은데 다음에 넣어봐야겠어요 ㅎㅎ",
          "original_form": "저도 양고기 좋아해요 소고기는 넣어본적 없는 것 같은데 다음에 넣어봐야겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 10:47"
        },
        {
          "id": "MDRW2100029579.1.27",
          "form": "거의 양고기를 넣는 것 같아요",
          "original_form": "거의 양고기를 넣는 것 같아요",
          "speaker_id": "2",
          "time": "20210903 10:47"
        },
        {
          "id": "MDRW2100029579.1.28",
          "form": "갑자기 마라탕이 먹고싶네요 ㅠ",
          "original_form": "갑자기 마라탕이 먹고싶네요 ㅠ",
          "speaker_id": "2",
          "time": "20210903 10:47"
        },
        {
          "id": "MDRW2100029579.1.29",
          "form": "마라샹궈 드셔보셨나요?",
          "original_form": "마라샹궈 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210903 10:47"
        },
        {
          "id": "MDRW2100029579.1.30",
          "form": "제가 어제 간 집은 소고기가 더 맛있더라고요",
          "original_form": "제가 어제 간 집은 소고기가 더 맛있더라고요",
          "speaker_id": "1",
          "time": "20210903 10:47"
        },
        {
          "id": "MDRW2100029579.1.31",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210903 10:47"
        },
        {
          "id": "MDRW2100029579.1.32",
          "form": "샹궈도 맛있죠 그 자극적인 맛...",
          "original_form": "샹궈도 맛있죠 그 자극적인 맛...",
          "speaker_id": "1",
          "time": "20210903 10:47"
        },
        {
          "id": "MDRW2100029579.1.33",
          "form": "끊을 수가 없어요",
          "original_form": "끊을 수가 없어요",
          "speaker_id": "1",
          "time": "20210903 10:47"
        },
        {
          "id": "MDRW2100029579.1.34",
          "form": "저 샹궈는 먹어본적 없는데 먹어보고싶어요",
          "original_form": "저 샹궈는 먹어본적 없는데 먹어보고싶어요",
          "speaker_id": "2",
          "time": "20210903 10:51"
        },
        {
          "id": "MDRW2100029579.1.35",
          "form": "마라탕과 비슷한가요??",
          "original_form": "마라탕과 비슷한가요??",
          "speaker_id": "2",
          "time": "20210903 10:51"
        },
        {
          "id": "MDRW2100029579.1.36",
          "form": "좀 다른데 볶음이라 그런가 더 자극적인 느낌이에요",
          "original_form": "좀 다른데 볶음이라 그런가 더 자극적인 느낌이에요",
          "speaker_id": "1",
          "time": "20210903 10:51"
        },
        {
          "id": "MDRW2100029579.1.37",
          "form": "와 너무 맛있을 것 같아요",
          "original_form": "와 너무 맛있을 것 같아요",
          "speaker_id": "2",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100029579.1.38",
          "form": "ㅠ술도 좋아하시나요?",
          "original_form": "ㅠ술도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100029579.1.39",
          "form": "아 저는 술은 잘 안마셔요 학생 땐 친구들이랑 칵테일바는 자주 갔는데",
          "original_form": "아 저는 술은 잘 안마셔요 학생 땐 친구들이랑 칵테일바는 자주 갔는데",
          "speaker_id": "1",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100029579.1.40",
          "form": "지금 사는 곳이 좀 시골이라 그런 가게가 없네요",
          "original_form": "지금 사는 곳이 좀 시골이라 그런 가게가 없네요",
          "speaker_id": "1",
          "time": "20210903 10:53"
        },
        {
          "id": "MDRW2100029579.1.41",
          "form": "아 그렇군요 마라샹궈와 맥주가 기가막히게 어울릴까 궁금했어요 ㅎㅎ 다음에 제가 먹어봐야겠어요",
          "original_form": "아 그렇군요 마라샹궈와 맥주가 기가막히게 어울릴까 궁금했어요 ㅎㅎ 다음에 제가 먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210903 10:53"
        },
        {
          "id": "MDRW2100029579.1.42",
          "form": "맥주랑도 많이 먹더라고요!",
          "original_form": "맥주랑도 많이 먹더라고요!",
          "speaker_id": "1",
          "time": "20210903 10:56"
        },
        {
