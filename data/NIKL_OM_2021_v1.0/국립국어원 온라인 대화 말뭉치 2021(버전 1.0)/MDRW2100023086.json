      "utterance": [
        {
          "id": "MDRW2100023086.1.1",
          "form": "근데 name1은 일이 안힘드세요?",
          "original_form": "근데 &name1&은 일이 안힘드세요?",
          "speaker_id": "2",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023086.1.2",
          "form": "전에 이야기했을때 별로 안힘들다고하셔서ㅎㅎ 진짜 안힘든지 아니면 괜찮은척하시는건지?ㅎ",
          "original_form": "전에 이야기했을때 별로 안힘들다고하셔서ㅎㅎ 진짜 안힘든지 아니면 괜찮은척하시는건지?ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100023086.1.3",
          "form": "그냥 일도 습관 같아요 ㅎㅎ",
          "original_form": "그냥 일도 습관 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023086.1.4",
          "form": "그래서 그런가 봐요",
          "original_form": "그래서 그런가 봐요",
          "speaker_id": "1",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100023086.1.5",
          "form": "어제 바로잤는데 자다 비 엄청 와서 ㅎㅎ",
          "original_form": "어제 바로잤는데 자다 비 엄청 와서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023086.1.6",
          "form": "어제 많이내렸아요?",
          "original_form": "어제 많이내렸아요?",
          "speaker_id": "2",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023086.1.7",
          "form": "몇번 깨서 ㅋㅋ 잔건지어쩐건지 ㅎㅎ",
          "original_form": "몇번 깨서 ㅋㅋ 잔건지어쩐건지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023086.1.8",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023086.1.9",
          "form": "저희집은 들어가면 감옥같애서ㅎㅎㅎ",
          "original_form": "저희집은 들어가면 감옥같애서ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023086.1.10",
          "form": "안들려요ㅎ",
          "original_form": "안들려요ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023086.1.11",
          "form": "잠자면서 예민하시구나ㅎ",
          "original_form": "잠자면서 예민하시구나ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023086.1.12",
          "form": "아항  그러시구낭 ㅡㅁ ㅡ 전 오히려 작은 소리 잘들어서 ㅎㅎ",
          "original_form": "아항  그러시구낭 ㅡㅁ ㅡ 전 오히려 작은 소리 잘들어서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023086.1.13",
          "form": "그럴수있져ㅎ",
          "original_form": "그럴수있져ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023086.1.14",
          "form": "전 쓰러져자는 편이라",
          "original_form": "전 쓰러져자는 편이라",
          "speaker_id": "2",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023086.1.15",
          "form": "소리에 예민 ㅋ",
          "original_form": "소리에 예민 ㅋ",
          "speaker_id": "1",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023086.1.16",
          "form": "아무리깨워도 안일어나요ㅎㅎㅎㄹㅎ",
          "original_form": "아무리깨워도 안일어나요ㅎㅎㅎㄹㅎ",
          "speaker_id": "2",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023086.1.17",
          "form": "오티는  아직도 진행중이에요 ?",
          "original_form": "오티는  아직도 진행중이에요 ?",
          "speaker_id": "1",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023086.1.18",
          "form": "계속한대요ㅎ",
          "original_form": "계속한대요ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023086.1.19",
          "form": "아 떨리네요ㅎ",
          "original_form": "아 떨리네요ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023086.1.20",
          "form": "왜용",
          "original_form": "왜용",
          "speaker_id": "1",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023086.1.21",
          "form": "여기 다 여자같음요....",
          "original_form": "여기 다 여자같음요....",
          "speaker_id": "2",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023086.1.22",
          "form": "나만남자인듯 싶어요ㅠ",
          "original_form": "나만남자인듯 싶어요ㅠ",
          "speaker_id": "2",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100023086.1.23",
          "form": "마이크로 각오를 해라고하네요ㅎ",
          "original_form": "마이크로 각오를 해라고하네요ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023086.1.24",
          "form": "오홈",
          "original_form": "오홈",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023086.1.25",
          "form": "청일점",
          "original_form": "청일점",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023086.1.26",
          "form": "근데 심심이한다고 별로 안듣고있어요ㅎㅎㅎㅎㅎ",
          "original_form": "근데 심심이한다고 별로 안듣고있어요ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023086.1.27",
          "form": "에이ㅎ 그런거 필여없어요ㅎㅎ",
          "original_form": "에이ㅎ 그런거 필여없어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023086.1.28",
          "form": "전 상관없는데ㅎ 나머지분들이 불편할까봐ㅎ",
          "original_form": "전 상관없는데ㅎ 나머지분들이 불편할까봐ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100023086.1.29",
          "form": "여자분들은 아무 생각 없을걸여 ㅎㅎ",
          "original_form": "여자분들은 아무 생각 없을걸여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100023086.1.30",
          "form": "그런가ㅎㅎ 나만 신경쓰는건가요?ㅎㅎ",
          "original_form": "그런가ㅎㅎ 나만 신경쓰는건가요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023086.1.31",
          "form": "이게 재밌는게 남탕 속에 여자 하나면  여자는 잘살아 남는데",
          "original_form": "이게 재밌는게 남탕 속에 여자 하나면  여자는 잘살아 남는데",
          "speaker_id": "1",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023086.1.32",
          "form": "재밌게 해야죠뭐ㅎ",
          "original_form": "재밌게 해야죠뭐ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023086.1.33",
          "form": "맞아요ㅎ",
          "original_form": "맞아요ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023086.1.34",
          "form": "여탕속  남자하나면 보통 적응 못하고 도망가요 ㅎㅎ",
          "original_form": "여탕속  남자하나면 보통 적응 못하고 도망가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023086.1.35",
          "form": "남자는 안그래요 ㅡㅎ",
          "original_form": "남자는 안그래요 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023086.1.36",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023086.1.37",
          "form": "비유적확하심ㅎ",
          "original_form": "비유적확하심ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023086.1.38",
          "form": "그럼 알바 이야기해볼까요?ㅎㅎ",
          "original_form": "그럼 알바 이야기해볼까요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100023086.1.39",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "1",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100023086.1.40",
          "form": "알바뭐뭐하셨어요?",
          "original_form": "알바뭐뭐하셨어요?",
