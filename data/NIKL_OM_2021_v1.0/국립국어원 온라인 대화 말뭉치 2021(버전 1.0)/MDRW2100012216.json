      "utterance": [
        {
          "id": "MDRW2100012216.1.1",
          "form": "항공권을 저렴하게 살려면",
          "original_form": "항공권을 저렴하게 살려면",
          "speaker_id": "1",
          "time": "20210824 20:24"
        },
        {
          "id": "MDRW2100012216.1.2",
          "form": "그것도 팁이!있어?",
          "original_form": "그것도 팁이!있어?",
          "speaker_id": "2",
          "time": "20210824 20:24"
        },
        {
          "id": "MDRW2100012216.1.3",
          "form": "요즘 다 인터넷으로 다 공유",
          "original_form": "요즘 다 인터넷으로 다 공유",
          "speaker_id": "1",
          "time": "20210824 20:24"
        },
        {
          "id": "MDRW2100012216.1.4",
          "form": "ㅋㅋ신기하다",
          "original_form": "ㅋㅋ신기하다",
          "speaker_id": "2",
          "time": "20210824 20:25"
        },
        {
          "id": "MDRW2100012216.1.5",
          "form": "여행고수들은 그래서 싸게 여행 다니는 거다",
          "original_form": "여행고수들은 그래서 싸게 여행 다니는 거다",
          "speaker_id": "1",
          "time": "20210824 20:25"
        },
        {
          "id": "MDRW2100012216.1.6",
          "form": "아~와닿네 난 하수",
          "original_form": "아~와닿네 난 하수",
          "speaker_id": "2",
          "time": "20210824 20:25"
        },
        {
          "id": "MDRW2100012216.1.7",
          "form": "ㅋㅋㅋ항공권도 무조건 싸게 살 수 있는게 아니고 법칙이 있다",
          "original_form": "ㅋㅋㅋ항공권도 무조건 싸게 살 수 있는게 아니고 법칙이 있다",
          "speaker_id": "1",
          "time": "20210824 20:26"
        },
        {
          "id": "MDRW2100012216.1.8",
          "form": "법칙? 뭔데",
          "original_form": "법칙? 뭔데",
          "speaker_id": "2",
          "time": "20210824 20:26"
        },
        {
          "id": "MDRW2100012216.1.9",
          "form": "아까 여행패키지 처럼 항공권도 싸지는 게 있음",
          "original_form": "아까 여행패키지 처럼 항공권도 싸지는 게 있음",
          "speaker_id": "1",
          "time": "20210824 20:26"
        },
        {
          "id": "MDRW2100012216.1.10",
          "form": "그래 언제 싸지는데",
          "original_form": "그래 언제 싸지는데",
          "speaker_id": "2",
          "time": "20210824 20:27"
        },
        {
          "id": "MDRW2100012216.1.11",
          "form": "응 일단 6개월 24주전에 미리 산다면 싸다",
          "original_form": "응 일단 6개월 24주전에 미리 산다면 싸다",
          "speaker_id": "1",
          "time": "20210824 20:27"
        },
        {
          "id": "MDRW2100012216.1.12",
          "form": "아~그렇구나 그럼 자유여행 갈때 참고 해야겠다",
          "original_form": "아~그렇구나 그럼 자유여행 갈때 참고 해야겠다",
          "speaker_id": "2",
          "time": "20210824 20:28"
        },
        {
          "id": "MDRW2100012216.1.13",
          "form": "응 그리고 땡처리 항공원",
          "original_form": "응 그리고 땡처리 항공원",
          "speaker_id": "1",
          "time": "20210824 20:28"
        },
        {
          "id": "MDRW2100012216.1.14",
          "form": "땡처리?",
