      "utterance": [
        {
          "id": "MDRW2100027694.1.1",
          "form": "온라인 피티니스 서비스란",
          "original_form": "온라인 피티니스 서비스란",
          "speaker_id": "1",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100027694.1.2",
          "form": "무엇이요",
          "original_form": "무엇이요",
          "speaker_id": "2",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100027694.1.3",
          "form": "닌텐도 스위치 링피트 같은 건가",
          "original_form": "닌텐도 스위치 링피트 같은 건가",
          "speaker_id": "1",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100027694.1.4",
          "form": "링피트 기억안나서 닌텐도 스위치까지 썼다",
          "original_form": "링피트 기억안나서 닌텐도 스위치까지 썼다",
          "speaker_id": "1",
          "time": "20210902 14:18"
        },
        {
          "id": "MDRW2100027694.1.5",
          "form": "인스타로 봐주는 건줄",
          "original_form": "인스타로 봐주는 건줄",
          "speaker_id": "2",
          "time": "20210902 14:19"
        },
        {
          "id": "MDRW2100027694.1.6",
          "form": "그거 아냐? 이소라 다이어트 영상 같은...",
          "original_form": "그거 아냐? 이소라 다이어트 영상 같은...",
          "speaker_id": "2",
          "time": "20210902 14:19"
        },
        {
          "id": "MDRW2100027694.1.7",
          "form": "하여튼 홈트 ㅋㅋㅋ",
          "original_form": "하여튼 홈트 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:20"
        },
        {
          "id": "MDRW2100027694.1.8",
          "form": "링피트도 홈트잖아ㅋㅋㅋㅋㅋ",
          "original_form": "링피트도 홈트잖아ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:20"
        },
        {
          "id": "MDRW2100027694.1.9",
          "form": "아 근데 난 이거 별로 재미없더라",
          "original_form": "아 근데 난 이거 별로 재미없더라",
          "speaker_id": "1",
          "time": "20210902 14:20"
        },
        {
          "id": "MDRW2100027694.1.10",
          "form": "한시간해두 땀 안나",
          "original_form": "한시간해두 땀 안나",
          "speaker_id": "1",
          "time": "20210902 14:20"
        },
        {
          "id": "MDRW2100027694.1.11",
          "form": "운동이 되긴 해?",
          "original_form": "운동이 되긴 해?",
          "speaker_id": "2",
          "time": "20210902 14:20"
        },
        {
          "id": "MDRW2100027694.1.12",
          "form": "안되네",
          "original_form": "안되네",
          "speaker_id": "2",
          "time": "20210902 14:20"
        },
        {
          "id": "MDRW2100027694.1.13",
          "form": "그냥 땀이 없으신거 아니야?",
          "original_form": "그냥 땀이 없으신거 아니야?",
          "speaker_id": "2",
          "time": "20210902 14:21"
        },
        {
          "id": "MDRW2100027694.1.14",
          "form": "you",
          "original_form": "you",
          "speaker_id": "2",
          "time": "20210902 14:21"
        },
        {
          "id": "MDRW2100027694.1.15",
          "form": "땀이 나야 운동이 되는 거 같은데",
          "original_form": "땀이 나야 운동이 되는 거 같은데",
          "speaker_id": "1",
          "time": "20210902 14:22"
        },
        {
          "id": "MDRW2100027694.1.16",
          "form": "땀도 안나고 숨도 안차고ㅋㅋㅋㅋㅋ",
          "original_form": "땀도 안나고 숨도 안차고ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:22"
        },
        {
          "id": "MDRW2100027694.1.17",
          "form": "체력거지 되어서 느긋하게 하는걸로 시작했는데",
          "original_form": "체력거지 되어서 느긋하게 하는걸로 시작했는데",
          "speaker_id": "1",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100027694.1.18",
          "form": "레벨 30중 13? 이 처음이엇고",
          "original_form": "레벨 30중 13? 이 처음이엇고",
          "speaker_id": "1",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100027694.1.19",
          "form": "20까지 가도 땀 안나드라",
          "original_form": "20까지 가도 땀 안나드라",
          "speaker_id": "1",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100027694.1.20",
          "form": "예?",
          "original_form": "예?",
          "speaker_id": "2",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100027694.1.21",
          "form": "저라면 죽었을듯",
          "original_form": "저라면 죽었을듯",
          "speaker_id": "2",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100027694.1.22",
          "form": "나도 하고싶었는데",
          "original_form": "나도 하고싶었는데",
          "speaker_id": "2",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100027694.1.23",
          "form": "누가 옆에서 감시하지안ㄹ우면",
          "original_form": "누가 옆에서 감시하지안ㄹ우면",
          "speaker_id": "2",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027694.1.24",
          "form": "내다던질거라...",
          "original_form": "내다던질거라...",
          "speaker_id": "2",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027694.1.25",
          "form": "운동하려면 진짜 피트니스를 등록하거나",
          "original_form": "운동하려면 진짜 피트니스를 등록하거나",
          "speaker_id": "2",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027694.1.26",
          "form": "운동기구를....음...",
          "original_form": "운동기구를....음...",
          "speaker_id": "2",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027694.1.27",
          "form": "운동기구를 해도",
          "original_form": "운동기구를 해도",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027694.1.28",
          "form": "아마 안될듯",
          "original_form": "아마 안될듯",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027694.1.29",
          "form": "사실 온라인으로 피트니스를 할 수 있는 사람은",
          "original_form": "사실 온라인으로 피트니스를 할 수 있는 사람은",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027694.1.30",
          "form": "뭘해도 잘할 사람 아닌가",
          "original_form": "뭘해도 잘할 사람 아닌가",
          "speaker_id": "1",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027694.1.31",
          "form": "맞는 말입니다.",
          "original_form": "맞는 말입니다.",
          "speaker_id": "2",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027694.1.32",
          "form": "나는 아닐뿐",
          "original_form": "나는 아닐뿐",
          "speaker_id": "2",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027694.1.33",
          "form": "밖에서 운동 못하는 사람은 사실 온라인으로도 못한다고 샌각해",
          "original_form": "밖에서 운동 못하는 사람은 사실 온라인으로도 못한다고 샌각해",
          "speaker_id": "1",
          "time": "20210902 14:31"
        },
        {
          "id": "MDRW2100027694.1.34",
          "form": "꾸준한게 필요한데 강제력이 없어서ㅋㅋㅋㅋㅋㅋ",
          "original_form": "꾸준한게 필요한데 강제력이 없어서ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:32"
        },
        {
          "id": "MDRW2100027694.1.35",
          "form": "온라인이 되는 사람은 밖에서 운동하는 사람이 추가로 하는 것일 것 같다",
          "original_form": "온라인이 되는 사람은 밖에서 운동하는 사람이 추가로 하는 것일 것 같다",
          "speaker_id": "1",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100027694.1.36",
          "form": "카메라 켜놓고 하는 건 아닌가",
          "original_form": "카메라 켜놓고 하는 건 아닌가",
          "speaker_id": "2",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100027694.1.37",
          "form": "카메라로 사람이 봐주는 거야?? 더 부담되는데?",
          "original_form": "카메라로 사람이 봐주는 거야?? 더 부담되는데?",
          "speaker_id": "1",
          "time": "20210902 14:36"
        },
        {
          "id": "MDRW2100027694.1.38",
          "form": "그냥 사람 만나서 할래ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그냥 사람 만나서 할래ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:37"
        },
        {
          "id": "MDRW2100027694.1.39",
          "form": "원격 수업도 안되는데",
          "original_form": "원격 수업도 안되는데",
          "speaker_id": "1",
          "time": "20210902 14:38"
        },
        {
          "id": "MDRW2100027694.1.40",
          "form": "원격 운동이 될까요 선생님 ",
          "original_form": "원격 운동이 될까요 선생님 ",
          "speaker_id": "1",
          "time": "20210902 14:38"
        },
        {
          "id": "MDRW2100027694.1.41",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그런건가욬ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그런건가욬ㅋㅋㅋㅋㅋ",
