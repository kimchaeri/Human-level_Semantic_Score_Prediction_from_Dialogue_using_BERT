      "utterance": [
        {
          "id": "MDRW2100030318.1.1",
          "form": "1학년만 다녔어서 저도 학교생활이란게 딱히 ㅋㅋ..",
          "original_form": "1학년만 다녔어서 저도 학교생활이란게 딱히 ㅋㅋ..",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030318.1.2",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030318.1.3",
          "form": "큐ㅜㅜㅠ",
          "original_form": "큐ㅜㅜㅠ",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030318.1.4",
          "form": "과는 무슨과 다니세요?",
          "original_form": "과는 무슨과 다니세요?",
          "speaker_id": "1",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030318.1.5",
          "form": "저는 도예과에요!",
          "original_form": "저는 도예과에요!",
          "speaker_id": "2",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100030318.1.6",
          "form": "오.. 도예과는 미대인가요",
          "original_form": "오.. 도예과는 미대인가요",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030318.1.7",
          "form": "도자기관련?",
          "original_form": "도자기관련?",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100030318.1.8",
          "form": "넵 미대죠!",
          "original_form": "넵 미대죠!",
          "speaker_id": "2",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100030318.1.9",
          "form": "멋있네요 미대생",
          "original_form": "멋있네요 미대생",
          "speaker_id": "1",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100030318.1.10",
          "form": "도예과라는건 처음들어봐요!",
          "original_form": "도예과라는건 처음들어봐요!",
          "speaker_id": "1",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030318.1.11",
          "form": "정말요...?? 흔한과는 아니죠ㅜㅜ",
          "original_form": "정말요...?? 흔한과는 아니죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100030318.1.12",
          "form": "주변에 미대간 친구들도 몇없어서 그런지",
          "original_form": "주변에 미대간 친구들도 몇없어서 그런지",
          "speaker_id": "1",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030318.1.13",
          "form": "거의 그림쪽으로 간건 봤었어요",
          "original_form": "거의 그림쪽으로 간건 봤었어요",
          "speaker_id": "1",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030318.1.14",
          "form": "저는 예고나와서 전부 미대였는데ㅋㅋㅋㅋ",
          "original_form": "저는 예고나와서 전부 미대였는데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030318.1.15",
          "form": "거의 그림이죠",
          "original_form": "거의 그림이죠",
          "speaker_id": "2",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100030318.1.16",
          "form": "아 예고나오셨구나",
          "original_form": "아 예고나오셨구나",
          "speaker_id": "1",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100030318.1.17",
          "form": "저는 일반 사립고 나왔어서 ㅋㅋ",
          "original_form": "저는 일반 사립고 나왔어서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100030318.1.18",
          "form": "음악이랑 예술관련 가는 친구들보면 신기하더라구여",
          "original_form": "음악이랑 예술관련 가는 친구들보면 신기하더라구여",
          "speaker_id": "1",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100030318.1.19",
          "form": "저는 국립예고 나왔어요 짱이죠",
          "original_form": "저는 국립예고 나왔어요 짱이죠",
          "speaker_id": "2",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100030318.1.20",
          "form": "오... 저는 인문계가 더..",
          "original_form": "오... 저는 인문계가 더..",
          "speaker_id": "2",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100030318.1.21",
          "form": "좋은 고등학교인가봐요",
          "original_form": "좋은 고등학교인가봐요",
          "speaker_id": "1",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030318.1.22",
          "form": "잘몰라서 죄송합니다 ㅋㅋㅋ..",
          "original_form": "잘몰라서 죄송합니다 ㅋㅋㅋ..",
