      "utterance": [
        {
          "id": "MDRW2100030590.1.1",
          "form": "좋아하는 스포츠 경기 있으신가요?",
          "original_form": "좋아하는 스포츠 경기 있으신가요?",
          "speaker_id": "1",
          "time": "20210904 10:22"
        },
        {
          "id": "MDRW2100030590.1.2",
          "form": "흠...테니스??",
          "original_form": "흠...테니스??",
          "speaker_id": "2",
          "time": "20210904 10:24"
        },
        {
          "id": "MDRW2100030590.1.3",
          "form": "경기 잘안봐요 ㅋㅋ  올림픽가튼거만..",
          "original_form": "경기 잘안봐요 ㅋㅋ  올림픽가튼거만..",
          "speaker_id": "2",
          "time": "20210904 10:24"
        },
        {
          "id": "MDRW2100030590.1.4",
          "form": "앗 저두요 ㅋㅋㅋ",
          "original_form": "앗 저두요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:24"
        },
        {
          "id": "MDRW2100030590.1.5",
          "form": "이번 여자배구 보셨나요?",
          "original_form": "이번 여자배구 보셨나요?",
          "speaker_id": "1",
          "time": "20210904 10:24"
        },
        {
          "id": "MDRW2100030590.1.6",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210904 10:24"
        },
        {
          "id": "MDRW2100030590.1.7",
          "form": "쩌럿죠",
          "original_form": "쩌럿죠",
          "speaker_id": "2",
          "time": "20210904 10:24"
        },
        {
          "id": "MDRW2100030590.1.8",
          "form": "직관가고싶더라구ㅇ요",
          "original_form": "직관가고싶더라구ㅇ요",
          "speaker_id": "2",
          "time": "20210904 10:24"
        },
        {
          "id": "MDRW2100030590.1.9",
          "form": "진짜 이번에 못보러 간거 너무 아쉬워요",
          "original_form": "진짜 이번에 못보러 간거 너무 아쉬워요",
          "speaker_id": "1",
          "time": "20210904 10:25"
        },
        {
          "id": "MDRW2100030590.1.10",
          "form": "저런건 직접 봐야하는데 ㅠ",
          "original_form": "저런건 직접 봐야하는데 ㅠ",
          "speaker_id": "1",
          "time": "20210904 10:26"
        },
        {
          "id": "MDRW2100030590.1.11",
          "form": "그죠 ㅠ 파리올림픽 가고시펑유...",
          "original_form": "그죠 ㅠ 파리올림픽 가고시펑유...",
          "speaker_id": "2",
          "time": "20210904 10:27"
        },
        {
          "id": "MDRW2100030590.1.12",
          "form": "진짜요!!",
          "original_form": "진짜요!!",
          "speaker_id": "1",
          "time": "20210904 10:30"
        },
        {
          "id": "MDRW2100030590.1.13",
          "form": "파리 전역을 거의 다 쓴다고 하던데",
          "original_form": "파리 전역을 거의 다 쓴다고 하던데",
          "speaker_id": "1",
          "time": "20210904 10:30"
        },
        {
          "id": "MDRW2100030590.1.14",
          "form": "이번 기회에 직관도 해보고 유럽여행도 겸해서",
          "original_form": "이번 기회에 직관도 해보고 유럽여행도 겸해서",
          "speaker_id": "1",
          "time": "20210904 10:31"
        },
        {
          "id": "MDRW2100030590.1.15",
          "form": "올림픽 보러 가고 싶어요",
          "original_form": "올림픽 보러 가고 싶어요",
          "speaker_id": "1",
          "time": "20210904 10:31"
        },
        {
          "id": "MDRW2100030590.1.16",
          "form": "ㄹㅇㄹㅇ... 박터지겟죠...",
          "original_form": "ㄹㅇㄹㅇ... 박터지겟죠...",
          "speaker_id": "2",
          "time": "20210904 10:35"
        },
        {
          "id": "MDRW2100030590.1.17",
          "form": "그쵸 ㅠ 그치만 이번에 넘  대단했어서 한 경기 정돈 직접 보고 싶은걸요 ㅠㅠ",
          "original_form": "그쵸 ㅠ 그치만 이번에 넘  대단했어서 한 경기 정돈 직접 보고 싶은걸요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 10:37"
        },
        {
          "id": "MDRW2100030590.1.18",
          "form": "그..베르사유궁전서도 경기한대요 거기가고파요",
          "original_form": "그..베르사유궁전서도 경기한대요 거기가고파요",
          "speaker_id": "2",
          "time": "20210904 10:37"
        },
        {
          "id": "MDRW2100030590.1.19",
          "form": "그 부분때문에 제일 가고 싶었어요...",
          "original_form": "그 부분때문에 제일 가고 싶었어요...",
          "speaker_id": "1",
          "time": "20210904 10:38"
        },
        {
          "id": "MDRW2100030590.1.20",
          "form": "ㅜㅜㅜㅜ ㅇ흑흑... 가면넘좋겟다",
          "original_form": "ㅜㅜㅜㅜ ㅇ흑흑... 가면넘좋겟다",
          "speaker_id": "2",
          "time": "20210904 10:38"
        },
        {
          "id": "MDRW2100030590.1.21",
          "form": "이번 펜싱도 진짜멋졌는데 보셨나요?",
          "original_form": "이번 펜싱도 진짜멋졌는데 보셨나요?",
          "speaker_id": "1",
          "time": "20210904 10:40"
        },
        {
          "id": "MDRW2100030590.1.22",
          "form": "네네... 펜싱..하고싶어지더라구요",
          "original_form": "네네... 펜싱..하고싶어지더라구요",
          "speaker_id": "2",
          "time": "20210904 10:42"
        }
      ]
