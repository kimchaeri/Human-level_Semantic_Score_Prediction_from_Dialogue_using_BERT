      "utterance": [
        {
          "id": "MDRW2100003731.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210821 11:21"
        },
        {
          "id": "MDRW2100003731.1.2",
          "form": "네 저는 강아지 키우는데",
          "original_form": "네 저는 강아지 키우는데",
          "speaker_id": "1",
          "time": "20210821 11:21"
        },
        {
          "id": "MDRW2100003731.1.3",
          "form": "보고싶네요!",
          "original_form": "보고싶네요!",
          "speaker_id": "1",
          "time": "20210821 11:21"
        },
        {
          "id": "MDRW2100003731.1.4",
          "form": "헉 저도 강아지키워요",
          "original_form": "헉 저도 강아지키워요",
          "speaker_id": "2",
          "time": "20210821 11:23"
        },
        {
          "id": "MDRW2100003731.1.5",
          "form": "퍼그랑 비숑 키우는데 name1님은요?",
          "original_form": "퍼그랑 비숑 키우는데 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210821 11:23"
        },
        {
          "id": "MDRW2100003731.1.6",
          "form": "우와 종이 뭔가요?",
          "original_form": "우와 종이 뭔가요?",
          "speaker_id": "1",
          "time": "20210821 11:23"
        },
        {
          "id": "MDRW2100003731.1.7",
          "form": "저는 페키니즈 키워요 ㅎㅎ",
          "original_form": "저는 페키니즈 키워요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 11:23"
        },
        {
          "id": "MDRW2100003731.1.8",
          "form": "비슷하네요",
          "original_form": "비슷하네요",
          "speaker_id": "2",
          "time": "20210821 11:24"
        },
        {
          "id": "MDRW2100003731.1.9",
          "form": "단두종 병 많지않나요?",
          "original_form": "단두종 병 많지않나요?",
          "speaker_id": "2",
          "time": "20210821 11:24"
        },
        {
          "id": "MDRW2100003731.1.10",
          "form": "흰색 털이라서 너무 귀여운데 털이 너무 많이 빠져요ㅜㅜ",
          "original_form": "흰색 털이라서 너무 귀여운데 털이 너무 많이 빠져요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 11:24"
        },
        {
          "id": "MDRW2100003731.1.11",
          "form": "저희집 퍼그는 단백질알러지 있거즌요",
          "original_form": "저희집 퍼그는 단백질알러지 있거즌요",
          "speaker_id": "2",
          "time": "20210821 11:25"
        },
        {
          "id": "MDRW2100003731.1.12",
          "form": "저희집 강아지는 허리디스크가 있어서",
          "original_form": "저희집 강아지는 허리디스크가 있어서",
          "speaker_id": "1",
          "time": "20210821 11:25"
        },
        {
          "id": "MDRW2100003731.1.13",
          "form": "털갈이 대박이죠...저희도 지금 털파티중이에요",
          "original_form": "털갈이 대박이죠...저희도 지금 털파티중이에요",
          "speaker_id": "2",
          "time": "20210821 11:25"
        },
        {
          "id": "MDRW2100003731.1.14",
          "form": "걱정이에요 코도골고ㅠㅠ",
          "original_form": "걱정이에요 코도골고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 11:25"
        },
        {
          "id": "MDRW2100003731.1.15",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "2",
          "time": "20210821 11:25"
        },
        {
          "id": "MDRW2100003731.1.16",
          "form": "코골이는 단두종은 어쩔수없대요",
          "original_form": "코골이는 단두종은 어쩔수없대요",
          "speaker_id": "2",
          "time": "20210821 11:25"
        },
        {
          "id": "MDRW2100003731.1.17",
          "form": "다리 짧은 애들은 다 있더라구요!",
          "original_form": "다리 짧은 애들은 다 있더라구요!",
          "speaker_id": "1",
          "time": "20210821 11:25"
        },
        {
          "id": "MDRW2100003731.1.18",
          "form": "허리디스크 아이고 어떡하냐",
          "original_form": "허리디스크 아이고 어떡하냐",
          "speaker_id": "2",
          "time": "20210821 11:25"
        }
      ]
