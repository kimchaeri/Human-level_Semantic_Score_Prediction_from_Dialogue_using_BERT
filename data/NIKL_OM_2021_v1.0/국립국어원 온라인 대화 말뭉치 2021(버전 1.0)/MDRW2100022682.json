        },
        {
          "id": "MDRW2100022682.1.2",
          "form": "최근구매하신거",
          "original_form": "최근구매하신거",
          "speaker_id": "1",
          "time": "20210829 16:58"
        },
        {
          "id": "MDRW2100022682.1.3",
          "form": "있으세요?",
          "original_form": "있으세요?",
          "speaker_id": "1",
          "time": "20210829 16:58"
        },
        {
          "id": "MDRW2100022682.1.4",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022682.1.5",
          "form": "최근에",
          "original_form": "최근에",
          "speaker_id": "2",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022682.1.6",
          "form": "고양이 간식 구매했어요.",
          "original_form": "고양이 간식 구매했어요.",
          "speaker_id": "2",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022682.1.7",
          "form": "고양이와 같이 사시는구나",
          "original_form": "고양이와 같이 사시는구나",
          "speaker_id": "1",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022682.1.8",
          "form": "네 두마리요",
          "original_form": "네 두마리요",
          "speaker_id": "2",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022682.1.9",
          "form": "고양이 좋아하세요?",
          "original_form": "고양이 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022682.1.10",
          "form": "네 깜찍한생명체죠",
          "original_form": "네 깜찍한생명체죠",
          "speaker_id": "1",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022682.1.11",
          "form": "뭐사셨어요?",
          "original_form": "뭐사셨어요?",
          "speaker_id": "1",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022682.1.12",
          "form": "고양이 간식샀어요",
          "original_form": "고양이 간식샀어요",
          "speaker_id": "2",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022682.1.13",
          "form": "차오츄르 샀어요",
          "original_form": "차오츄르 샀어요",
          "speaker_id": "2",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022682.1.14",
          "form": "차오츄르인가요",
          "original_form": "차오츄르인가요",
          "speaker_id": "1",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022682.1.15",
          "form": "네 name1도 사보셨나요",
          "original_form": "네 &name1&도 사보셨나요",
          "speaker_id": "2",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022682.1.16",
          "form": "네모간식이름이 기억이 안나요",
          "original_form": "네모간식이름이 기억이 안나요",
          "speaker_id": "1",
          "time": "20210829 17:02"
        },
        {
          "id": "MDRW2100022682.1.17",
          "form": "제가 사 본 네모난 간식은 템테이션이었어요",
          "original_form": "제가 사 본 네모난 간식은 템테이션이었어요",
          "speaker_id": "2",
          "time": "20210829 17:03"
        },
        {
          "id": "MDRW2100022682.1.18",
          "form": "템테이션 사셨나요?",
          "original_form": "템테이션 사셨나요?",
          "speaker_id": "2",
          "time": "20210829 17:03"
        },
        {
          "id": "MDRW2100022682.1.19",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210829 17:03"
        },
        {
          "id": "MDRW2100022682.1.20",
          "form": "연어맛이었어요",
          "original_form": "연어맛이었어요",
          "speaker_id": "1",
          "time": "20210829 17:03"
        },
        {
          "id": "MDRW2100022682.1.21",
          "form": "애옹 옷도 사시나요?",
          "original_form": "애옹 옷도 사시나요?",
          "speaker_id": "1",
          "time": "20210829 17:03"
        },
        {
          "id": "MDRW2100022682.1.22",
          "form": "그러셨구나 고양이옷은 전에는 샀었는데 잘 안입어서",
          "original_form": "그러셨구나 고양이옷은 전에는 샀었는데 잘 안입어서",
          "speaker_id": "2",
          "time": "20210829 17:04"
        },
        {
          "id": "MDRW2100022682.1.23",
          "form": "개키우는 집에 선물보내고",
          "original_form": "개키우는 집에 선물보내고",
          "speaker_id": "2",
          "time": "20210829 17:04"
        },
        {
          "id": "MDRW2100022682.1.24",
          "form": "했어요",
          "original_form": "했어요",
          "speaker_id": "2",
          "time": "20210829 17:04"
        },
        {
          "id": "MDRW2100022682.1.25",
          "form": "애옹이 입었다면 더 귀여웠겠어요",
          "original_form": "애옹이 입었다면 더 귀여웠겠어요",
          "speaker_id": "1",
          "time": "20210829 17:04"
        },
        {
          "id": "MDRW2100022682.1.26",
          "form": "네 귀엽죠 말도못해요",
          "original_form": "네 귀엽죠 말도못해요",
          "speaker_id": "2",
          "time": "20210829 17:04"
        },
        {
          "id": "MDRW2100022682.1.27",
          "form": "꽃게 사실건가요?",
          "original_form": "꽃게 사실건가요?",
