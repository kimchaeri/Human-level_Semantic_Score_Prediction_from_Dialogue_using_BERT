      "utterance": [
        {
          "id": "MDRW2100038288.1.1",
          "form": "아침 뭐 드셨나요?",
          "original_form": "아침 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210902 08:32"
        },
        {
          "id": "MDRW2100038288.1.2",
          "form": "저는 아직이요:)",
          "original_form": "저는 아직이요:)",
          "speaker_id": "1",
          "time": "20210902 08:32"
        },
        {
          "id": "MDRW2100038288.1.3",
          "form": "식사 하셨어요?",
          "original_form": "식사 하셨어요?",
          "speaker_id": "1",
          "time": "20210902 08:32"
        },
        {
          "id": "MDRW2100038288.1.4",
          "form": "방금 먹었어요",
          "original_form": "방금 먹었어요",
          "speaker_id": "2",
          "time": "20210902 08:33"
        },
        {
          "id": "MDRW2100038288.1.5",
          "form": "뭐드셨어요?",
          "original_form": "뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210902 08:33"
        },
        {
          "id": "MDRW2100038288.1.6",
          "form": "어제 남은 카레 먹었어요",
          "original_form": "어제 남은 카레 먹었어요",
          "speaker_id": "2",
          "time": "20210902 08:35"
        },
        {
          "id": "MDRW2100038288.1.7",
          "form": "카레 직접 만들어 드세요?",
          "original_form": "카레 직접 만들어 드세요?",
          "speaker_id": "1",
          "time": "20210902 08:35"
        },
        {
          "id": "MDRW2100038288.1.8",
          "form": "네 직접 만들어 먹어요ㅎㅎ",
          "original_form": "네 직접 만들어 먹어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 08:36"
        },
        {
          "id": "MDRW2100038288.1.9",
          "form": "감자 양파 돼지고기 넣어서요",
          "original_form": "감자 양파 돼지고기 넣어서요",
          "speaker_id": "2",
          "time": "20210902 08:37"
        },
        {
          "id": "MDRW2100038288.1.10",
          "form": "우왕 :) 대단하세요",
          "original_form": "우왕 :) 대단하세요",
          "speaker_id": "1",
          "time": "20210902 08:40"
        },
        {
          "id": "MDRW2100038288.1.11",
          "form": "어릴때 엄마가 해주시던\r\n기억이 나네요",
          "original_form": "어릴때 엄마가 해주시던\r\n기억이 나네요",
          "speaker_id": "1",
          "time": "20210902 08:40"
        },
        {
          "id": "MDRW2100038288.1.12",
          "form": "저도 부모님이 해주시던거로",
          "original_form": "저도 부모님이 해주시던거로",
          "speaker_id": "2",
          "time": "20210902 08:41"
        },
        {
          "id": "MDRW2100038288.1.13",
          "form": "배우고 만드는 거예요",
          "original_form": "배우고 만드는 거예요",
          "speaker_id": "2",
          "time": "20210902 08:41"
        },
        {
          "id": "MDRW2100038288.1.14",
          "form": "엄마표레시피군요",
          "original_form": "엄마표레시피군요{emoji:♡}",
          "speaker_id": "1",
          "time": "20210902 08:42"
        },
        {
          "id": "MDRW2100038288.1.15",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 08:44"
        },
        {
          "id": "MDRW2100038288.1.16",
          "form": "어때요?\r\n엄마가 해주던 그 맛인가요? ㅋㅋ",
          "original_form": "어때요?\r\n엄마가 해주던 그 맛인가요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 08:46"
        },
        {
          "id": "MDRW2100038288.1.17",
          "form": "약간 다른거 같아요",
          "original_form": "약간 다른거 같아요",
          "speaker_id": "2",
          "time": "20210902 08:47"
        },
        {
          "id": "MDRW2100038288.1.18",
          "form": "진짜 손맛이 있는거 같아요 ㅋㅋ",
          "original_form": "진짜 손맛이 있는거 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 08:48"
        },
        {
          "id": "MDRW2100038288.1.19",
          "form": "저도 엄마레시피 제가 해도\r\n엄마가 해주던 맛은 아니더라구요ㅠ",
          "original_form": "저도 엄마레시피 제가 해도\r\n엄마가 해주던 맛은 아니더라구요ㅠ",
