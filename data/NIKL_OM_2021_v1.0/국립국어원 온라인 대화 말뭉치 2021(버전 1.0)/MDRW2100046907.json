      "utterance": [
        {
          "id": "MDRW2100046907.1.1",
          "form": "틱톡댄스 챌린지 해 봤어?",
          "original_form": "틱톡댄스 챌린지 해 봤어?",
          "speaker_id": "1",
          "time": "20211006 13:38"
        },
        {
          "id": "MDRW2100046907.1.2",
          "form": "요즘은 뭐가 핫하지?",
          "original_form": "요즘은 뭐가 핫하지?",
          "speaker_id": "1",
          "time": "20211006 13:38"
        },
        {
          "id": "MDRW2100046907.1.3",
          "form": "역시 스우파 헤이마마인가",
          "original_form": "역시 스우파 헤이마마인가",
          "speaker_id": "1",
          "time": "20211006 13:38"
        },
        {
          "id": "MDRW2100046907.1.4",
          "form": "스우파 재미있어?",
          "original_form": "스우파 재미있어?",
          "speaker_id": "2",
          "time": "20211006 19:38"
        },
        {
          "id": "MDRW2100046907.1.5",
          "form": "요새 다들 틱톡하기는 하더라",
          "original_form": "요새 다들 틱톡하기는 하더라",
          "speaker_id": "2",
          "time": "20211006 19:38"
        },
        {
          "id": "MDRW2100046907.1.6",
          "form": "스우파 안보지... 근데 유튜브 쇼츠?에도 틱톡 그런건 많이 뜨더라구",
          "original_form": "스우파 안보지... 근데 유튜브 쇼츠?에도 틱톡 그런건 많이 뜨더라구",
          "speaker_id": "1",
          "time": "20211006 20:21"
        },
        {
          "id": "MDRW2100046907.1.7",
          "form": "헤이마마는 진짜 많이봤음.",
          "original_form": "헤이마마는 진짜 많이봤음.",
          "speaker_id": "1",
          "time": "20211006 20:21"
        },
        {
          "id": "MDRW2100046907.1.8",
          "form": "틱톡이 이만큼 커졌다는게 진짜 신기하지...",
          "original_form": "틱톡이 이만큼 커졌다는게 진짜 신기하지...",
          "speaker_id": "1",
          "time": "20211006 20:23"
        },
        {
          "id": "MDRW2100046907.1.9",
          "form": "바인도 비슷했는데 망했잖어",
          "original_form": "바인도 비슷했는데 망했잖어",
          "speaker_id": "1",
          "time": "20211006 20:23"
        },
        {
          "id": "MDRW2100046907.1.10",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20211006 20:25"
        },
        {
          "id": "MDRW2100046907.1.11",
          "form": "틱톡 개인정보 이슈도 있었는데",
          "original_form": "틱톡 개인정보 이슈도 있었는데",
          "speaker_id": "2",
          "time": "20211006 20:26"
        },
        {
          "id": "MDRW2100046907.1.12",
          "form": "다들 틱톡 엄청 하더라",
          "original_form": "다들 틱톡 엄청 하더라",
          "speaker_id": "2",
          "time": "20211006 20:26"
        },
        {
          "id": "MDRW2100046907.1.13",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20211006 20:33"
        },
        {
          "id": "MDRW2100046907.1.14",
          "form": "연예인들도 꽤나하는거같더라구",
          "original_form": "연예인들도 꽤나하는거같더라구",
          "speaker_id": "1",
          "time": "20211006 20:34"
        },
        {
          "id": "MDRW2100046907.1.15",
          "form": "현아랑 이던? 둘이하는거도 올라왔던거같고...",
          "original_form": "현아랑 이던? 둘이하는거도 올라왔던거같고...",
          "speaker_id": "1",
          "time": "20211006 20:34"
        },
        {
          "id": "MDRW2100046907.1.16",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20211006 20:34"
        },
        {
          "id": "MDRW2100046907.1.17",
          "form": "뭐 걔들만 하는것도 아니고",
          "original_form": "뭐 걔들만 하는것도 아니고",
          "speaker_id": "1",
          "time": "20211006 20:34"
        },
        {
          "id": "MDRW2100046907.1.18",
          "form": "가성비가 좋은느낌?",
          "original_form": "가성비가 좋은느낌?",
          "speaker_id": "2",
          "time": "20211006 20:35"
        },
        {
          "id": "MDRW2100046907.1.19",
          "form": "유튜브보다 영상은 훨씬 짧은데",
          "original_form": "유튜브보다 영상은 훨씬 짧은데",
          "speaker_id": "2",
          "time": "20211006 20:35"
        },
        {
          "id": "MDRW2100046907.1.20",
          "form": "비슷한 효과를 보니까",
          "original_form": "비슷한 효과를 보니까",
          "speaker_id": "2",
          "time": "20211006 20:35"
        },
        {
          "id": "MDRW2100046907.1.21",
          "form": "아 맞네...",
          "original_form": "아 맞네...",
          "speaker_id": "1",
          "time": "20211006 20:41"
        },
        {
          "id": "MDRW2100046907.1.22",
          "form": "어쨌든 이용자 많고 그런것도 영향을 끼치겠굼",
          "original_form": "어쨌든 이용자 많고 그런것도 영향을 끼치겠굼",
          "speaker_id": "1",
          "time": "20211006 20:42"
        },
        {
          "id": "MDRW2100046907.1.23",
          "form": "ㅇㅇ 그게 필수기는 하지",
          "original_form": "ㅇㅇ 그게 필수기는 하지",
          "speaker_id": "2",
          "time": "20211006 20:42"
        },
        {
          "id": "MDRW2100046907.1.24",
          "form": "요즘은 이런 플랫폼들도 워낙 많아지긴 했는데",
          "original_form": "요즘은 이런 플랫폼들도 워낙 많아지긴 했는데",
          "speaker_id": "1",
          "time": "20211006 20:42"
        },
        {
          "id": "MDRW2100046907.1.25",
          "form": "어쨌든 큰 곳이 더더욱 커지는건 어쩔수없지만 넘 끔찍한듯--",
          "original_form": "어쨌든 큰 곳이 더더욱 커지는건 어쩔수없지만 넘 끔찍한듯--",
          "speaker_id": "1",
          "time": "20211006 20:42"
        },
        {
          "id": "MDRW2100046907.1.26",
          "form": "만든곳이 중국이라 더더욱...",
          "original_form": "만든곳이 중국이라 더더욱...",
          "speaker_id": "1",
          "time": "20211006 20:43"
        },
        {
          "id": "MDRW2100046907.1.27",
          "form": "미국이랑은 이제 분리됐댔나?",
          "original_form": "미국이랑은 이제 분리됐댔나?",
          "speaker_id": "1",
          "time": "20211006 20:43"
        },
        {
          "id": "MDRW2100046907.1.28",
          "form": "그랬나? 나도 그건 몰랐네",
          "original_form": "그랬나? 나도 그건 몰랐네",
          "speaker_id": "2",
          "time": "20211006 20:43"
        },
        {
          "id": "MDRW2100046907.1.29",
          "form": "엉 아닌가?",
          "original_form": "엉 아닌가?",
          "speaker_id": "1",
          "time": "20211006 20:44"
        },
        {
          "id": "MDRW2100046907.1.30",
          "form": "미국에서 틱톡에 이의를 제기했었던듯?",
          "original_form": "미국에서 틱톡에 이의를 제기했었던듯?",
          "speaker_id": "1",
          "time": "20211006 20:44"
        },
        {
          "id": "MDRW2100046907.1.31",
          "form": "미국에서 이의제기한거는 아는데",
          "original_form": "미국에서 이의제기한거는 아는데",
          "speaker_id": "2",
          "time": "20211006 20:44"
        },
        {
          "id": "MDRW2100046907.1.32",
          "form": "분리된다는 얘기가 있었구나",
          "original_form": "분리된다는 얘기가 있었구나",
          "speaker_id": "2",
          "time": "20211006 20:44"
        },
        {
          "id": "MDRW2100046907.1.33",
          "form": "아 걍 차단한다고 했다가",
          "original_form": "아 걍 차단한다고 했다가",
          "speaker_id": "1",
          "time": "20211006 20:44"
        },
        {
          "id": "MDRW2100046907.1.34",
          "form": "미중관계 한창 나쁠때말야",
          "original_form": "미중관계 한창 나쁠때말야",
          "speaker_id": "1",
          "time": "20211006 20:45"
        },
        {
          "id": "MDRW2100046907.1.35",
          "form": "이제 걍 풀린듯?",
          "original_form": "이제 걍 풀린듯?",
          "speaker_id": "1",
          "time": "20211006 20:45"
        },
        {
          "id": "MDRW2100046907.1.36",
          "form": "그 차단 해제 조건으로",
          "original_form": "그 차단 해제 조건으로",
          "speaker_id": "1",
          "time": "20211006 20:45"
        },
        {
          "id": "MDRW2100046907.1.37",
          "form": "미국기업의 틱톡 인수가 있었나봐",
          "original_form": "미국기업의 틱톡 인수가 있었나봐",
          "speaker_id": "1",
          "time": "20211006 20:45"
        },
        {
          "id": "MDRW2100046907.1.38",
          "form": "그게 어케됐지?",
          "original_form": "그게 어케됐지?",
          "speaker_id": "1",
          "time": "20211006 20:45"
        },
        {
          "id": "MDRW2100046907.1.39",
          "form": "옛날에 차단했던거 풀리고 그냥 인수는 없어진듯",
          "original_form": "옛날에 차단했던거 풀리고 그냥 인수는 없어진듯",
          "speaker_id": "2",
          "time": "20211006 20:46"
        },
        {
          "id": "MDRW2100046907.1.40",
          "form": "오라클에서 인수했다는 말도 있고., 뭔지 모르겠음--",
          "original_form": "오라클에서 인수했다는 말도 있고., 뭔지 모르겠음--",
          "speaker_id": "1",
          "time": "20211006 20:46"
        },
        {
          "id": "MDRW2100046907.1.41",
          "form": "아 인수한댔다가",
          "original_form": "아 인수한댔다가",
          "speaker_id": "1",
          "time": "20211006 20:46"
        },
        {
          "id": "MDRW2100046907.1.42",
          "form": "바이든 취임하고 안하기로 했나봄",
          "original_form": "바이든 취임하고 안하기로 했나봄",
          "speaker_id": "1",
          "time": "20211006 20:46"
        },
        {
          "id": "MDRW2100046907.1.43",
          "form": "ㅇㅇ 그런가봐",
          "original_form": "ㅇㅇ 그런가봐",
          "speaker_id": "2",
          "time": "20211006 20:46"
        },
        {
          "id": "MDRW2100046907.1.44",
          "form": "옛날에 유튜브 보다가 틱톡 광고나오면 진짜 별로였는데",
          "original_form": "옛날에 유튜브 보다가 틱톡 광고나오면 진짜 별로였는데",
          "speaker_id": "2",
          "time": "20211006 20:47"
        },
        {
          "id": "MDRW2100046907.1.45",
          "form": "맞아... 사실 나는 아직도. 틱톡극혐",
          "original_form": "맞아... 사실 나는 아직도. 틱톡극혐",
          "speaker_id": "1",
          "time": "20211006 20:49"
        },
        {
          "id": "MDRW2100046907.1.46",
          "form": "하지만 이미 숏폼콘텐츠에 중독되어버린 나?!",
          "original_form": "하지만 이미 숏폼콘텐츠에 중독되어버린 나?!",
          "speaker_id": "1",
          "time": "20211006 20:49"
        },
        {
          "id": "MDRW2100046907.1.47",
          "form": "이게 다 유튜브 숏츠 때문이다.",
          "original_form": "이게 다 유튜브 숏츠 때문이다.",
          "speaker_id": "1",
          "time": "20211006 20:49"
        },
        {
          "id": "MDRW2100046907.1.48",
          "form": "유튜브 숏츠는 재미있는거 많지",
          "original_form": "유튜브 숏츠는 재미있는거 많지",
          "speaker_id": "2",
          "time": "20211006 20:50"
        },
        {
          "id": "MDRW2100046907.1.49",
          "form": "다들.. 틱톡을 많이들 하는구나...",
          "original_form": "다들.. 틱톡을 많이들 하는구나...",
          "speaker_id": "1",
          "time": "20211006 20:50"
        },
        {
          "id": "MDRW2100046907.1.50",
          "form": "참..",
          "original_form": "참..",
          "speaker_id": "1",
          "time": "20211006 20:50"
        }
      ]
    }
  ]
}
