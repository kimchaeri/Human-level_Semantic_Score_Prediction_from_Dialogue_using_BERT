      "utterance": [
        {
          "id": "MDRW2100005875.1.1",
          "form": "우리도 텀블러 있지?",
          "original_form": "우리도 텀블러 있지?",
          "speaker_id": "1",
          "time": "20210907 14:58"
        },
        {
          "id": "MDRW2100005875.1.2",
          "form": "엄청 많아요",
          "original_form": "엄청 많아요",
          "speaker_id": "2",
          "time": "20210907 14:58"
        },
        {
          "id": "MDRW2100005875.1.3",
          "form": "필요해요?",
          "original_form": "필요해요?",
          "speaker_id": "2",
          "time": "20210907 14:58"
        },
        {
          "id": "MDRW2100005875.1.4",
          "form": "응 다음 입원때 가져가게",
          "original_form": "응 다음 입원때 가져가게",
          "speaker_id": "1",
          "time": "20210907 15:03"
        },
        {
          "id": "MDRW2100005875.1.5",
          "form": "챙겨놓을게요",
          "original_form": "챙겨놓을게요",
          "speaker_id": "2",
          "time": "20210907 15:04"
        },
        {
          "id": "MDRW2100005875.1.6",
          "form": "고마워요",
          "original_form": "고마워요",
          "speaker_id": "1",
          "time": "20210907 15:10"
        },
        {
          "id": "MDRW2100005875.1.7",
          "form": "텀블러 쓰면 환경보호도 되고 좋죠",
          "original_form": "텀블러 쓰면 환경보호도 되고 좋죠",
          "speaker_id": "2",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100005875.1.8",
          "form": "요새는 많이들 쓰더라구요",
          "original_form": "요새는 많이들 쓰더라구요",
          "speaker_id": "2",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100005875.1.9",
          "form": "그래요",
          "original_form": "그래요",
          "speaker_id": "1",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100005875.1.10",
          "form": "카페에도 가져오면 할인해주고 그러더라구요",
          "original_form": "카페에도 가져오면 할인해주고 그러더라구요",
          "speaker_id": "2",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100005875.1.11",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100005875.1.12",
          "form": "요즘 친환경~ 친환경 하잖아요",
          "original_form": "요즘 친환경~ 친환경 하잖아요",
