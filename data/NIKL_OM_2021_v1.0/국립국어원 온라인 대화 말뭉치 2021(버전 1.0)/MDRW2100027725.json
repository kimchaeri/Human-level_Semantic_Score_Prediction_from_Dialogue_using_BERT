      "utterance": [
        {
          "id": "MDRW2100027725.1.1",
          "form": "ㅇㅇ어제왔엉!!",
          "original_form": "ㅇㅇ어제왔엉!!",
          "speaker_id": "2",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027725.1.2",
          "form": "아직 안먹어봄 오늘 먹으려고",
          "original_form": "아직 안먹어봄 오늘 먹으려고",
          "speaker_id": "2",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027725.1.3",
          "form": "홈플 쿠폰 끝났대 혹시 받아뒀어? 받아둔건 쓸 수 있어",
          "original_form": "홈플 쿠폰 끝났대 혹시 받아뒀어? 받아둔건 쓸 수 있어",
          "speaker_id": "2",
          "time": "20210902 15:02"
        },
        {
          "id": "MDRW2100027725.1.4",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027725.1.5",
          "form": "굿굿",
          "original_form": "굿굿",
          "speaker_id": "1",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027725.1.6",
          "form": "폭립산거 방금 먹어봤당!!!",
          "original_form": "폭립산거 방금 먹어봤당!!!",
          "speaker_id": "2",
          "time": "20210902 18:52"
        },
        {
          "id": "MDRW2100027725.1.7",
          "form": "어! 저녁이다ㅋ",
          "original_form": "어! 저녁이다ㅋ",
          "speaker_id": "1",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100027725.1.8",
          "form": "무슨맛?",
          "original_form": "무슨맛?",
          "speaker_id": "1",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100027725.1.9",
          "form": "마라맛이랑 바베큐맛!",
          "original_form": "마라맛이랑 바베큐맛!",
          "speaker_id": "2",
          "time": "20210902 19:21"
        },
        {
          "id": "MDRW2100027725.1.10",
          "form": "마라맛 어때?",
          "original_form": "마라맛 어때?",
          "speaker_id": "1",
          "time": "20210902 19:21"
        },
        {
          "id": "MDRW2100027725.1.11",
          "form": "별로여 짜고 맛 별로",
          "original_form": "별로여 짜고 맛 별로",
          "speaker_id": "2",
          "time": "20210902 19:21"
        },
        {
          "id": "MDRW2100027725.1.12",
          "form": "바베큐맛이 맛있더라",
          "original_form": "바베큐맛이 맛있더라",
          "speaker_id": "2",
          "time": "20210902 19:22"
        },
        {
          "id": "MDRW2100027725.1.13",
          "form": "살짝 달긴한데 괜춘",
          "original_form": "살짝 달긴한데 괜춘",
          "speaker_id": "2",
          "time": "20210902 19:22"
        },
        {
          "id": "MDRW2100027725.1.14",
          "form": "마라 향신료 맛이 별로 였나보다",
          "original_form": "마라 향신료 맛이 별로 였나보다",
          "speaker_id": "1",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100027725.1.15",
          "form": "마라보다 그 안에 양념이 별로인듯 단맛이 없어서 더 그런거 같기도함",
          "original_form": "마라보다 그 안에 양념이 별로인듯 단맛이 없어서 더 그런거 같기도함",
          "speaker_id": "2",
          "time": "20210902 19:32"
        },
        {
          "id": "MDRW2100027725.1.16",
          "form": "마라는 매운맛인데 안매웠어?",
          "original_form": "마라는 매운맛인데 안매웠어?",
          "speaker_id": "1",
          "time": "20210902 19:36"
        },
        {
          "id": "MDRW2100027725.1.17",
          "form": "많이 맵진않던데 계속먹음 매운느낌?",
          "original_form": "많이 맵진않던데 계속먹음 매운느낌?",
          "speaker_id": "2",
          "time": "20210902 19:37"
        },
        {
          "id": "MDRW2100027725.1.18",
          "form": "맵기 낮춰나왔나보다ㅎ",
          "original_form": "맵기 낮춰나왔나보다ㅎ",
          "speaker_id": "1",
          "time": "20210902 19:40"
        },
        {
          "id": "MDRW2100027725.1.19",
          "form": "다음엔 바베큐맛으로 해야겠네",
          "original_form": "다음엔 바베큐맛으로 해야겠네",
          "speaker_id": "1",
          "time": "20210902 19:40"
        },
        {
          "id": "MDRW2100027725.1.20",
          "form": "ㅇㅇ바베큐는 담에도 사려고",
          "original_form": "ㅇㅇ바베큐는 담에도 사려고",
          "speaker_id": "2",
          "time": "20210902 19:40"
        }
      ]
