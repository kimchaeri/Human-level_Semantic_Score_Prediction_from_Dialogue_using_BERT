      "utterance": [
        {
          "id": "MDRW2100020084.1.1",
          "form": "좋아하는 연예인있으세요?",
          "original_form": "좋아하는 연예인있으세요?",
          "speaker_id": "1",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020084.1.2",
          "form": "저는 두루두루 다 좋아해요ㅋㅋㅋ",
          "original_form": "저는 두루두루 다 좋아해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020084.1.3",
          "form": "연예인은 아닌데 요즘은 국가대표 방송들 찾아봐요",
          "original_form": "연예인은 아닌데 요즘은 국가대표 방송들 찾아봐요",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020084.1.4",
          "form": "유퀴즈 안산님 나온거 봤어요!",
          "original_form": "유퀴즈 안산님 나온거 봤어요!",
          "speaker_id": "1",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020084.1.5",
          "form": "아 안산선수 너무 멋있고 귀엽죠ㅠㅠ",
          "original_form": "아 안산선수 너무 멋있고 귀엽죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020084.1.6",
          "form": "요즘 제일 관심가는 선수에요",
          "original_form": "요즘 제일 관심가는 선수에요",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020084.1.7",
          "form": "올림픽 보셨어요?",
          "original_form": "올림픽 보셨어요?",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020084.1.8",
          "form": "안산님경기는 다봤죠! 너무 귀여워요 ㅜㅜ",
          "original_form": "안산님경기는 다봤죠! 너무 귀여워요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020084.1.9",
          "form": "이번 올림픽 별로 재미없을줄 알았는데 막상 시작하니 다 챙겨보게 되더라구요ㅋㅋㅋ",
          "original_form": "이번 올림픽 별로 재미없을줄 알았는데 막상 시작하니 다 챙겨보게 되더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020084.1.10",
          "form": "클라이밍도 너무 흥미로웠어요",
          "original_form": "클라이밍도 너무 흥미로웠어요",
          "speaker_id": "2",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020084.1.11",
          "form": "맞아요 ㅜㅜ 그 이름이 기억안나는데 엄청 어린선수 너무 기특하고 예뻤어요 ㅜ",
          "original_form": "맞아요 ㅜㅜ 그 이름이 기억안나는데 엄청 어린선수 너무 기특하고 예뻤어요 ㅜ",
          "speaker_id": "1",
          "time": "20210827 23:45"
        },
        {
          "id": "MDRW2100020084.1.12",
          "form": "그쵸그쵸 엄청 장했어요",
          "original_form": "그쵸그쵸 엄청 장했어요",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020084.1.13",
          "form": "근육도 완전 멋있고.. 올림픽보니 운동하고 싶어지더라구요ㅋㅋㅋ",
          "original_form": "근육도 완전 멋있고.. 올림픽보니 운동하고 싶어지더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020084.1.14",
          "form": "저는 그 머리끈 질끈 묶은거 너무 캐릭터같고 귀엽더라구요 ㅋㅋㅋ",
          "original_form": "저는 그 머리끈 질끈 묶은거 너무 캐릭터같고 귀엽더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020084.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ엌 맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ엌 맞아요",
          "speaker_id": "2",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020084.1.16",
          "form": "이번 선수들은 다 하나씩 매력 포인트가 있는듯",
          "original_form": "이번 선수들은 다 하나씩 매력 포인트가 있는듯",
          "speaker_id": "2",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100020084.1.17",
          "form": "탁구 신유빈 선수도 삐약거리는게 참 귀엽더라구요",
          "original_form": "탁구 신유빈 선수도 삐약거리는게 참 귀엽더라구요",
          "speaker_id": "2",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100020084.1.18",
          "form": "정말요 ㅜㅜ 진짜 병아리그자체였어요 ㅜㅜ 찐애기 ㅜㅜ",
          "original_form": "정말요 ㅜㅜ 진짜 병아리그자체였어요 ㅜㅜ 찐애기 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100020084.1.19",
          "form": "그 선수보려고 이번주 놀면뭐하니도 챙겨봤어요ㅋㅋㅋ",
          "original_form": "그 선수보려고 이번주 놀면뭐하니도 챙겨봤어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020084.1.20",
          "form": "선수들 방송에서 인터뷰 많이 해줬으면 좋겠어요",
          "original_form": "선수들 방송에서 인터뷰 많이 해줬으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020084.1.21",
          "form": "배구쪽도 보고싶은데 방송나온거없죠?",
          "original_form": "배구쪽도 보고싶은데 방송나온거없죠?",
          "speaker_id": "1",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100020084.1.22",
          "form": "네 ㅠㅠㅠ 배구 최애인데 배구는 잘 안나오네요",
          "original_form": "네 ㅠㅠㅠ 배구 최애인데 배구는 잘 안나오네요",
          "speaker_id": "2",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100020084.1.23",
          "form": "노는언니 예고에서 본거같기도 한데",
          "original_form": "노는언니 예고에서 본거같기도 한데",
          "speaker_id": "2",
          "time": "20210827 23:57"
        }
      ]
    }
