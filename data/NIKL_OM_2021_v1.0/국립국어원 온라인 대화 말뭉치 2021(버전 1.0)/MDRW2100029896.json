      "utterance": [
        {
          "id": "MDRW2100029896.1.1",
          "form": "콜라보 상품 좋아하세요?",
          "original_form": "콜라보 상품 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 16:13"
        },
        {
          "id": "MDRW2100029896.1.2",
          "form": "넵 제가 산건 아닌데",
          "original_form": "넵 제가 산건 아닌데",
          "speaker_id": "2",
          "time": "20210903 16:14"
        },
        {
          "id": "MDRW2100029896.1.3",
          "form": "이번에 z플립3 내면서",
          "original_form": "이번에 z플립3 내면서",
          "speaker_id": "2",
          "time": "20210903 16:14"
        },
        {
          "id": "MDRW2100029896.1.4",
          "form": "악세사리 콜라보한게 너무 예쁘더라구요",
          "original_form": "악세사리 콜라보한게 너무 예쁘더라구요",
          "speaker_id": "2",
          "time": "20210903 16:15"
        },
        {
          "id": "MDRW2100029896.1.5",
          "form": "이마트나 래미안 이런데까지 콜라보한 것도 너무 재밌었구요ㅋㅋ",
          "original_form": "이마트나 래미안 이런데까지 콜라보한 것도 너무 재밌었구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:15"
        },
        {
          "id": "MDRW2100029896.1.6",
          "form": "역시 대기업이라 콜라보 다양하게 했네요",
          "original_form": "역시 대기업이라 콜라보 다양하게 했네요",
          "speaker_id": "1",
          "time": "20210903 17:30"
        },
        {
          "id": "MDRW2100029896.1.7",
          "form": "캐릭터 콜라보 좋아하세요?",
          "original_form": "캐릭터 콜라보 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 18:10"
        },
        {
          "id": "MDRW2100029896.1.8",
          "form": "전 라인 샐리 카카오 라이언 나오면 잘 구입하는 편이에요",
          "original_form": "전 라인 샐리 카카오 라이언 나오면 잘 구입하는 편이에요",
          "speaker_id": "1",
          "time": "20210903 18:11"
        },
        {
          "id": "MDRW2100029896.1.9",
          "form": "샐리가 오리 맞죠? 진짜 귀엽더라구요",
          "original_form": "샐리가 오리 맞죠? 진짜 귀엽더라구요",
          "speaker_id": "2",
          "time": "20210903 18:18"
        },
        {
          "id": "MDRW2100029896.1.10",
          "form": "저는 피카츄 좋아해서 예전에 콜라보할때 이것저것 샀어요!",
          "original_form": "저는 피카츄 좋아해서 예전에 콜라보할때 이것저것 샀어요!",
          "speaker_id": "2",
          "time": "20210903 18:19"
        },
        {
          "id": "MDRW2100029896.1.11",
          "form": "그런데 피카츄는 콜라보를 자주하지는 않더라구요ㅠ",
          "original_form": "그런데 피카츄는 콜라보를 자주하지는 않더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210903 18:19"
        },
        {
          "id": "MDRW2100029896.1.12",
          "form": "병아리에요ㅋㅋ",
          "original_form": "병아리에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:21"
        },
        {
          "id": "MDRW2100029896.1.13",
          "form": "피카츄도 귀엽죠 콜라보는 잘 안 하는군요",
          "original_form": "피카츄도 귀엽죠 콜라보는 잘 안 하는군요",
          "speaker_id": "1",
          "time": "20210903 18:21"
        },
        {
          "id": "MDRW2100029896.1.14",
          "form": "헉 오리가 아니였다니",
          "original_form": "헉 오리가 아니였다니",
          "speaker_id": "2",
          "time": "20210903 18:22"
        },
        {
          "id": "MDRW2100029896.1.15",
          "form": "잘 헷갈려해요 저도 첨엔 그랬어요",
          "original_form": "잘 헷갈려해요 저도 첨엔 그랬어요",
          "speaker_id": "1",
          "time": "20210903 18:24"
        },
        {
          "id": "MDRW2100029896.1.16",
          "form": "아무래도 라인이나 카카오 캐릭터가 다른 캐릭터보다 다양한 분야에 걸쳐서 콜라보를 많이 하는 것 같긴해요ㅋㅋㅋ",
          "original_form": "아무래도 라인이나 카카오 캐릭터가 다른 캐릭터보다 다양한 분야에 걸쳐서 콜라보를 많이 하는 것 같긴해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:25"
        },
        {
          "id": "MDRW2100029896.1.17",
          "form": "저 잡지 콜라보 부록으로 나온 샐리저금통도 갖고 있었는데",
          "original_form": "저 잡지 콜라보 부록으로 나온 샐리저금통도 갖고 있었는데",
          "speaker_id": "2",
          "time": "20210903 18:26"
        },
        {
          "id": "MDRW2100029896.1.18",
          "form": "방금전까지 오리인줄만ㅋㅋㅋ",
          "original_form": "방금전까지 오리인줄만ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:26"
        },
        {
          "id": "MDRW2100029896.1.19",
          "form": "그거 사고 싶었는데요 잡지 품절되서 못샀던 ㅠㅋㅋ",
          "original_form": "그거 사고 싶었는데요 잡지 품절되서 못샀던 ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:28"
        },
        {
          "id": "MDRW2100029896.1.20",
          "form": "아이고 진짜 아쉽네요ㅠㅠ 콜라보 또 해주면 좋을텐데ㅠㅠ",
          "original_form": "아이고 진짜 아쉽네요ㅠㅠ 콜라보 또 해주면 좋을텐데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 18:31"
        },
        {
          "id": "MDRW2100029896.1.21",
          "form": "샐리 진짜 귀여운데 콜라보에선 보통 브라운을 많이 봐서",
          "original_form": "샐리 진짜 귀여운데 콜라보에선 보통 브라운을 많이 봐서",
          "speaker_id": "2",
          "time": "20210903 18:32"
        },
        {
          "id": "MDRW2100029896.1.22",
          "form": "샐리도 더 다양하게 여기저기서 보고싶어요!",
          "original_form": "샐리도 더 다양하게 여기저기서 보고싶어요!",
          "speaker_id": "2",
          "time": "20210903 18:32"
        },
        {
          "id": "MDRW2100029896.1.23",
          "form": "맞아요 샐리가 더 귀여운데 콜라보가 부족해요",
          "original_form": "맞아요 샐리가 더 귀여운데 콜라보가 부족해요",
          "speaker_id": "1",
          "time": "20210903 18:33"
        },
        {
          "id": "MDRW2100029896.1.24",
          "form": "참 이번에 질병관리청이랑 했더라구요",
          "original_form": "참 이번에 질병관리청이랑 했더라구요",
          "speaker_id": "1",
          "time": "20210903 18:33"
        },
        {
          "id": "MDRW2100029896.1.25",
          "form": "오 지금 찾아봤어요",
          "original_form": "오 지금 찾아봤어요",
          "speaker_id": "2",
          "time": "20210903 18:34"
        },
        {
          "id": "MDRW2100029896.1.26",
          "form": "샐리도 당당하게 한 자리를 함께했는데",
          "original_form": "샐리도 당당하게 한 자리를 함께했는데",
          "speaker_id": "2",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100029896.1.27",
          "form": "좀 더 크게 그려줬으면 좋겠어요!",
          "original_form": "좀 더 크게 그려줬으면 좋겠어요!",
          "speaker_id": "2",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100029896.1.28",
          "form": "근데 너무 귀엽네요ㅋㅋㅋㅠ",
          "original_form": "근데 너무 귀엽네요ㅋㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210903 18:35"
        },
        {
          "id": "MDRW2100029896.1.29",
          "form": "갑자기 국가기관이랑 콜라보해서 놀랐어요 오프라인으로 봤거든요",
          "original_form": "갑자기 국가기관이랑 콜라보해서 놀랐어요 오프라인으로 봤거든요",
          "speaker_id": "1",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100029896.1.30",
          "form": "반가웠겠어요! 좋아하는 캐릭터가 콜라보한거 우연히 알게되면 기분좋던데",
          "original_form": "반가웠겠어요! 좋아하는 캐릭터가 콜라보한거 우연히 알게되면 기분좋던데",
          "speaker_id": "2",
          "time": "20210903 18:48"
        },
        {
          "id": "MDRW2100029896.1.31",
          "form": "저도 언제 콜라보하는지 모르고 놀러갔다가 피카츄상? 같은거 세워져싰어서 놀란적이 있거든요ㅋㅋㅋ",
          "original_form": "저도 언제 콜라보하는지 모르고 놀러갔다가 피카츄상? 같은거 세워져싰어서 놀란적이 있거든요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:49"
        }
      ]
