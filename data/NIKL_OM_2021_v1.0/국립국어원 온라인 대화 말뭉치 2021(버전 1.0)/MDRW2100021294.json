      "utterance": [
        {
          "id": "MDRW2100021294.1.1",
          "form": "네 매일봐요",
          "original_form": "네 매일봐요",
          "speaker_id": "2",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100021294.1.2",
          "form": "보세요??",
          "original_form": "보세요??",
          "speaker_id": "2",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100021294.1.3",
          "form": "네네 저도거의매일ㅋㅋ",
          "original_form": "네네 저도거의매일ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100021294.1.4",
          "form": "누구보세요?",
          "original_form": "누구보세요?",
          "speaker_id": "2",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100021294.1.5",
          "form": "먹방은 히밥이요!",
          "original_form": "먹방은 히밥이요!",
          "speaker_id": "1",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100021294.1.6",
          "form": "들어봤어요!ㅋㅋㅋ",
          "original_form": "들어봤어요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100021294.1.7",
          "form": "먹방 요새 제일 인기더라구요",
          "original_form": "먹방 요새 제일 인기더라구요",
          "speaker_id": "2",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100021294.1.8",
          "form": "어떤거보세요?",
          "original_form": "어떤거보세요?",
          "speaker_id": "1",
          "time": "20210828 17:28"
        },
        {
          "id": "MDRW2100021294.1.9",
          "form": "음 저는 햄지랑 나도요!",
          "original_form": "음 저는 햄지랑 나도요!",
          "speaker_id": "2",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100021294.1.10",
          "form": "햄지가 그 중국참교육 맞나요",
          "original_form": "햄지가 그 중국참교육 맞나요",
          "speaker_id": "1",
          "time": "20210828 17:30"
        },
        {
          "id": "MDRW2100021294.1.11",
          "form": "모르겠어요 그거까지는ㅋㅋ",
          "original_form": "모르겠어요 그거까지는ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100021294.1.12",
          "form": "나도는 첨들어보네요!",
          "original_form": "나도는 첨들어보네요!",
          "speaker_id": "1",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100021294.1.13",
          "form": "구독자 백만명 넘어요 인기많은데 나중에 함 찾아보셔요 ㅋㅋㅋㅋ",
          "original_form": "구독자 백만명 넘어요 인기많은데 나중에 함 찾아보셔요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100021294.1.14",
          "form": "와 백만이요.?",
          "original_form": "와 백만이요.?",
          "speaker_id": "1",
          "time": "20210828 17:35"
        },
        {
          "id": "MDRW2100021294.1.15",
          "form": "네 ㅋㅋㅋ 인기많더라구요",
          "original_form": "네 ㅋㅋㅋ 인기많더라구요",
