      "utterance": [
        {
          "id": "MDRW2100023128.1.1",
          "form": "짧게?",
          "original_form": "짧게?",
          "speaker_id": "2",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100023128.1.2",
          "form": "엄청 짧은 단발 아니면 아예 남자 같은 머리",
          "original_form": "엄청 짧은 단발 아니면 아예 남자 같은 머리",
          "speaker_id": "1",
          "time": "20210829 22:01"
        },
        {
          "id": "MDRW2100023128.1.3",
          "form": "근데 잘 어울릴까?",
          "original_form": "근데 잘 어울릴까?",
          "speaker_id": "2",
          "time": "20210829 22:01"
        },
        {
          "id": "MDRW2100023128.1.4",
          "form": "잘 어울릴지는 모르겠어 근데 나 어릴 때 해봤었어",
          "original_form": "잘 어울릴지는 모르겠어 근데 나 어릴 때 해봤었어",
          "speaker_id": "1",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100023128.1.5",
          "form": "주변 반응이 어땠어?",
          "original_form": "주변 반응이 어땠어?",
          "speaker_id": "2",
          "time": "20210829 22:02"
        },
        {
          "id": "MDRW2100023128.1.6",
          "form": "그 때는 초등학교 다닐 때 라 내가 남잔 줄 아는 사람들이 많았어",
          "original_form": "그 때는 초등학교 다닐 때 라 내가 남잔 줄 아는 사람들이 많았어",
          "speaker_id": "1",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100023128.1.7",
          "form": "ㅋㅋㅋㅋ 그럴 수 있지",
          "original_form": "ㅋㅋㅋㅋ 그럴 수 있지",
          "speaker_id": "2",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100023128.1.8",
          "form": "아예 짧은 단발로 짤라 볼까도 싶어",
          "original_form": "아예 짧은 단발로 짤라 볼까도 싶어",
          "speaker_id": "1",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100023128.1.9",
          "form": "짧은 단발 좋은거 같은데?",
          "original_form": "짧은 단발 좋은거 같은데?",
          "speaker_id": "2",
          "time": "20210829 22:05"
        },
        {
          "id": "MDRW2100023128.1.10",
          "form": "아예 턱선 위로 올라간 단발",
          "original_form": "아예 턱선 위로 올라간 단발",
          "speaker_id": "1",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100023128.1.11",
          "form": "최양락 짤 같은거 말하는거야?",
          "original_form": "최양락 짤 같은거 말하는거야?",
          "speaker_id": "2",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100023128.1.12",
          "form": "ㅋㅋㅋㅋㅋ응",
          "original_form": "ㅋㅋㅋㅋㅋ응",
          "speaker_id": "1",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100023128.1.13",
          "form": "괜찮을 거 같은데?",
          "original_form": "괜찮을 거 같은데?",
          "speaker_id": "2",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100023128.1.14",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "1",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100023128.1.15",
