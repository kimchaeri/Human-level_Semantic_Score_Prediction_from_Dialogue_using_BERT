      "utterance": [
        {
          "id": "MDRW2100000730.1.1",
          "form": "요즘 온라인으로 음식 많이 구매하는 것 같아요.",
          "original_form": "요즘 온라인으로 음식 많이 구매하는 것 같아요.",
          "speaker_id": "1",
          "time": "20210819 18:47"
        },
        {
          "id": "MDRW2100000730.1.2",
          "form": "밀키트가 잘나와서 배달 횟수가 줄었어요",
          "original_form": "밀키트가 잘나와서 배달 횟수가 줄었어요",
          "speaker_id": "2",
          "time": "20210819 18:48"
        },
        {
          "id": "MDRW2100000730.1.3",
          "form": "맞아요. 요즘 밀키트 너무 좋죠.",
          "original_form": "맞아요. 요즘 밀키트 너무 좋죠.",
          "speaker_id": "1",
          "time": "20210819 18:48"
        },
        {
          "id": "MDRW2100000730.1.4",
          "form": "주변에 트레이더스가 있어서 가끔 사오는데",
          "original_form": "주변에 트레이더스가 있어서 가끔 사오는데",
          "speaker_id": "1",
          "time": "20210819 18:48"
        },
        {
          "id": "MDRW2100000730.1.5",
          "form": "밀푀유나베. 부대찌개 좋더라구요",
          "original_form": "밀푀유나베. 부대찌개 좋더라구요",
          "speaker_id": "2",
          "time": "20210819 18:49"
        },
        {
          "id": "MDRW2100000730.1.6",
          "form": "네 그냥 끓이기만 하면 되더라고요 ㅎㅎ",
          "original_form": "네 그냥 끓이기만 하면 되더라고요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 18:49"
        },
        {
          "id": "MDRW2100000730.1.7",
          "form": "너무 편해요.",
          "original_form": "너무 편해요.",
          "speaker_id": "1",
          "time": "20210819 18:49"
        },
        {
          "id": "MDRW2100000730.1.8",
          "form": "배달하거나 재료사서 만드는 것보다 저렴한것 같아요",
          "original_form": "배달하거나 재료사서 만드는 것보다 저렴한것 같아요",
          "speaker_id": "2",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100000730.1.9",
          "form": "맞아요. 재료 사는 것보다 저렴해요",
          "original_form": "맞아요. 재료 사는 것보다 저렴해요",
          "speaker_id": "1",
          "time": "20210819 18:54"
        },
        {
          "id": "MDRW2100000730.1.10",
          "form": "버려지는 것도 없고",
          "original_form": "버려지는 것도 없고",
          "speaker_id": "1",
          "time": "20210819 18:54"
        },
        {
          "id": "MDRW2100000730.1.11",
          "form": "저녁은 뭐드시나요?",
          "original_form": "저녁은 뭐드시나요?",
          "speaker_id": "2",
          "time": "20210819 18:57"
        },
        {
          "id": "MDRW2100000730.1.12",
          "form": "이미 먹었는데 고등어조림이랑 제육볶음 먹었어요.",
          "original_form": "이미 먹었는데 고등어조림이랑 제육볶음 먹었어요.",
          "speaker_id": "1",
          "time": "20210819 18:58"
        },
        {
          "id": "MDRW2100000730.1.13",
          "form": "아직 안드셨나요?",
          "original_form": "아직 안드셨나요?",
          "speaker_id": "1",
          "time": "20210819 18:58"
        },
        {
          "id": "MDRW2100000730.1.14",
          "form": "저두요 ㅋ 김치제육볶음 먹었어요",
          "original_form": "저두요 ㅋ 김치제육볶음 먹었어요",
          "speaker_id": "2",
          "time": "20210819 19:01"
        },
        {
          "id": "MDRW2100000730.1.15",
          "form": "매일 식사 메뉴 고르기가 어렵네요 ㅎ",
          "original_form": "매일 식사 메뉴 고르기가 어렵네요 ㅎ",
          "speaker_id": "1",
          "time": "20210819 19:01"
        },
        {
          "id": "MDRW2100000730.1.16",
          "form": "음식조리법이 비슷비슷하니 살짝 질리기도 해요. 한재료 가지고 이렇게도 해보고 저렇게도 해보고 해야하는데 만들수있는 메뉴가 한정적이에요.",
          "original_form": "음식조리법이 비슷비슷하니 살짝 질리기도 해요. 한재료 가지고 이렇게도 해보고 저렇게도 해보고 해야하는데 만들수있는 메뉴가 한정적이에요.",
          "speaker_id": "2",
          "time": "20210819 19:05"
        },
        {
          "id": "MDRW2100000730.1.17",
          "form": "그렇긴 하죠.",
          "original_form": "그렇긴 하죠.",
          "speaker_id": "1",
          "time": "20210819 19:17"
        },
        {
          "id": "MDRW2100000730.1.18",
          "form": "누가 메뉴 좀 정해주면 좋겠네요 ㅎㅎ",
          "original_form": "누가 메뉴 좀 정해주면 좋겠네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 19:17"
        },
        {
          "id": "MDRW2100000730.1.19",
          "form": "전 누가 밥좀 해줬으면 좋겠어요 ㅋ",
          "original_form": "전 누가 밥좀 해줬으면 좋겠어요 ㅋ",
          "speaker_id": "2",
          "time": "20210819 19:18"
        },
        {
          "id": "MDRW2100000730.1.20",
          "form": "저도요 우렁각시 나터났으면 ㅋ",
          "original_form": "저도요 우렁각시 나터났으면 ㅋ",
          "speaker_id": "1",
          "time": "20210819 19:18"
        },
        {
          "id": "MDRW2100000730.1.21",
          "form": "내일은 뭐해드시나요",
          "original_form": "내일은 뭐해드시나요",
          "speaker_id": "1",
          "time": "20210819 19:19"
        },
        {
          "id": "MDRW2100000730.1.22",
          "form": "아침은 간단하게 시리얼이나 토스트요",
          "original_form": "아침은 간단하게 시리얼이나 토스트요",
          "speaker_id": "2",
          "time": "20210819 19:20"
        },
        {
          "id": "MDRW2100000730.1.23",
          "form": "점심저녁은 미정",
          "original_form": "점심저녁은 미정",
          "speaker_id": "2",
          "time": "20210819 19:20"
        },
        {
          "id": "MDRW2100000730.1.24",
          "form": "저도 아침엔 토스트 먹어야겠네요",
          "original_form": "저도 아침엔 토스트 먹어야겠네요",
          "speaker_id": "1",
          "time": "20210819 19:21"
        },
        {
          "id": "MDRW2100000730.1.25",
          "form": "가끔은 학교 식단표 따라해요",
          "original_form": "가끔은 학교 식단표 따라해요",
          "speaker_id": "1",
          "time": "20210819 19:21"
        },
        {
          "id": "MDRW2100000730.1.26",
          "form": "학교식단표 좋네요",
          "original_form": "학교식단표 좋네요",
          "speaker_id": "2",
          "time": "20210819 19:22"
        },
        {
          "id": "MDRW2100000730.1.27",
          "form": "메뉴 고르기 어려워서요 ㅎ",
          "original_form": "메뉴 고르기 어려워서요 ㅎ",
          "speaker_id": "1",
          "time": "20210819 19:22"
        },
        {
          "id": "MDRW2100000730.1.28",
          "form": "늘상 고민해도 거의 같은것만 먹는것 같아요",
          "original_form": "늘상 고민해도 거의 같은것만 먹는것 같아요",
          "speaker_id": "2",
          "time": "20210819 19:23"
        },
        {
          "id": "MDRW2100000730.1.29",
          "form": "저희도 그래요",
          "original_form": "저희도 그래요",
          "speaker_id": "1",
          "time": "20210819 19:23"
        },
        {
          "id": "MDRW2100000730.1.30",
          "form": "비슷한 반찬들",
          "original_form": "비슷한 반찬들",
          "speaker_id": "1",
          "time": "20210819 19:23"
        },
        {
          "id": "MDRW2100000730.1.31",
          "form": "먹는 것만 먹고",
          "original_form": "먹는 것만 먹고",
          "speaker_id": "1",
          "time": "20210819 19:24"
        },
        {
          "id": "MDRW2100000730.1.32",
          "form": "배달도 비슷한 것만 시켜요 ㅋ",
          "original_form": "배달도 비슷한 것만 시켜요 ㅋ",
          "speaker_id": "2",
          "time": "20210819 19:24"
        },
        {
          "id": "MDRW2100000730.1.33",
          "form": "반찬 어떤 거 잘해드세요?",
          "original_form": "반찬 어떤 거 잘해드세요?",
          "speaker_id": "1",
          "time": "20210819 19:25"
        },
        {
          "id": "MDRW2100000730.1.34",
          "form": "김치찌개 두루치기 찜닭 떢볶이.된장찌개. 미역국",
          "original_form": "김치찌개 두루치기 찜닭 떢볶이.된장찌개. 미역국",
          "speaker_id": "2",
          "time": "20210819 19:28"
        },
        {
          "id": "MDRW2100000730.1.35",
          "form": "최근에 먹은 요리에요",
          "original_form": "최근에 먹은 요리에요",
          "speaker_id": "2",
          "time": "20210819 19:28"
        },
        {
          "id": "MDRW2100000730.1.36",
          "form": "와 음식 잘하시나봐요",
          "original_form": "와 음식 잘하시나봐요",
          "speaker_id": "1",
          "time": "20210819 19:29"
        },
        {
          "id": "MDRW2100000730.1.37",
          "form": "푸짐하네요 ㅎㅎ",
          "original_form": "푸짐하네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 19:29"
        },
        {
          "id": "MDRW2100000730.1.38",
          "form": "음식을 잘하지는 못해요",
          "original_form": "음식을 잘하지는 못해요",
          "speaker_id": "2",
          "time": "20210819 19:29"
        },
        {
          "id": "MDRW2100000730.1.39",
          "form": "맛있겠어요",
          "original_form": "맛있겠어요",
