      "utterance": [
        {
          "id": "MDRW2100046161.1.1",
          "form": "안녕하세요 식사하셨나요?",
          "original_form": "안녕하세요 식사하셨나요?",
          "speaker_id": "1",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100046161.1.2",
          "form": "아직이요 ㅠ점심메뉴고민중이에요",
          "original_form": "아직이요 ㅠ점심메뉴고민중이에요",
          "speaker_id": "2",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046161.1.3",
          "form": "오오 뭐 드실 생각이세요?",
          "original_form": "오오 뭐 드실 생각이세요?",
          "speaker_id": "1",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100046161.1.4",
          "form": "음 순두부찌개랑 연어덮밥주에 뭐가좋을까요",
          "original_form": "음 순두부찌개랑 연어덮밥주에 뭐가좋을까요",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046161.1.5",
          "form": "오오오 둘다 맛있을 거 같네요",
          "original_form": "오오오 둘다 맛있을 거 같네요",
          "speaker_id": "1",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046161.1.6",
          "form": "약간 따뜻한게 끌리기도해요",
          "original_form": "약간 따뜻한게 끌리기도해요",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100046161.1.7",
          "form": "맞아요 좀 쌀쌀하다고 느낄 정도의 날씨라 따뜻한게 좋죠",
          "original_form": "맞아요 좀 쌀쌀하다고 느낄 정도의 날씨라 따뜻한게 좋죠",
          "speaker_id": "1",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100046161.1.8",
          "form": "아침은드셨나요?",
          "original_form": "아침은드셨나요?",
          "speaker_id": "2",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100046161.1.9",
          "form": "네 저는 아침식사했어요!",
          "original_form": "네 저는 아침식사했어요!",
          "speaker_id": "1",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100046161.1.10",
          "form": "어떤거드셨어요?",
          "original_form": "어떤거드셨어요?",
          "speaker_id": "2",
          "time": "20210908 10:57"
        },
        {
          "id": "MDRW2100046161.1.11",
          "form": "카레랑 두부조림 먹었어요!",
          "original_form": "카레랑 두부조림 먹었어요!",
          "speaker_id": "1",
          "time": "20210908 10:58"
        },
        {
          "id": "MDRW2100046161.1.12",
          "form": "카레너무좋아요!",
          "original_form": "카레너무좋아요!",
          "speaker_id": "2",
          "time": "20210908 10:59"
        },
        {
          "id": "MDRW2100046161.1.13",
          "form": "요리해서드셨나요?",
          "original_form": "요리해서드셨나요?",
          "speaker_id": "2",
          "time": "20210908 10:59"
        },
        {
          "id": "MDRW2100046161.1.14",
          "form": "네! 요리해서 먹었어요",
          "original_form": "네! 요리해서 먹었어요",
