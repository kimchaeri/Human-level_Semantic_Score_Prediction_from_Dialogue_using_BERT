      "utterance": [
        {
          "id": "MDRW2100025702.1.1",
          "form": "아 근데 원래 화이자맞으면",
          "original_form": "아 근데 원래 화이자맞으면",
          "speaker_id": "2",
          "time": "20210901 16:10"
        },
        {
          "id": "MDRW2100025702.1.2",
          "form": "2차 언제 맞나요",
          "original_form": "2차 언제 맞나요",
          "speaker_id": "1",
          "time": "20210901 16:10"
        },
        {
          "id": "MDRW2100025702.1.3",
          "form": "가음께가뻐근한가",
          "original_form": "가음께가뻐근한가",
          "speaker_id": "2",
          "time": "20210901 16:10"
        },
        {
          "id": "MDRW2100025702.1.4",
          "form": "방금맞고나옴ㅋㅋㄱㅋㅋ",
          "original_form": "방금맞고나옴ㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:10"
        },
        {
          "id": "MDRW2100025702.1.5",
          "form": "ㅇㅇ네...",
          "original_form": "ㅇㅇ네...",
          "speaker_id": "1",
          "time": "20210901 16:10"
        },
        {
          "id": "MDRW2100025702.1.6",
          "form": "아 웃겨",
          "original_form": "아 웃겨",
          "speaker_id": "1",
          "time": "20210901 16:10"
        },
        {
          "id": "MDRW2100025702.1.7",
          "form": "쇄골아래가 뻐근하닼ㅋㅋㅋㅋㅋㄱ",
          "original_form": "쇄골아래가 뻐근하닼ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210901 16:10"
        },
        {
          "id": "MDRW2100025702.1.8",
          "form": "아니 누가 2차지만 화이자로는 1차여서 안 아프다고한겨",
          "original_form": "아니 누가 2차지만 화이자로는 1차여서 안 아프다고한겨",
          "speaker_id": "1",
          "time": "20210901 16:11"
        },
        {
          "id": "MDRW2100025702.1.9",
          "form": "아니 개아픈디",
          "original_form": "아니 개아픈디",
          "speaker_id": "2",
          "time": "20210901 16:11"
        },
        {
          "id": "MDRW2100025702.1.10",
          "form": "확실히 아제보단 덜 아프긴한디",
          "original_form": "확실히 아제보단 덜 아프긴한디",
          "speaker_id": "1",
          "time": "20210901 16:11"
        },
        {
          "id": "MDRW2100025702.1.11",
          "form": "어제많이아팠어??",
          "original_form": "어제많이아팠어??",
          "speaker_id": "2",
          "time": "20210901 16:11"
        },
        {
          "id": "MDRW2100025702.1.12",
          "form": "이것도 뼈쑤시는 몸살 옴",
          "original_form": "이것도 뼈쑤시는 몸살 옴",
          "speaker_id": "1",
          "time": "20210901 16:11"
        },
        {
          "id": "MDRW2100025702.1.13",
          "form": "근데 애매한거 알지",
          "original_form": "근데 애매한거 알지",
          "speaker_id": "1",
          "time": "20210901 16:11"
        },
        {
          "id": "MDRW2100025702.1.14",
          "form": "아안돼...",
          "original_form": "아안돼...",
          "speaker_id": "2",
          "time": "20210901 16:11"
        },
        {
          "id": "MDRW2100025702.1.15",
          "form": "약 먹을정도는 아니아",
          "original_form": "약 먹을정도는 아니아",
          "speaker_id": "1",
          "time": "20210901 16:11"
        },
        {
          "id": "MDRW2100025702.1.16",
          "form": "선생님 무서워요",
          "original_form": "선생님 무서워요",
          "speaker_id": "2",
          "time": "20210901 16:12"
        },
        {
          "id": "MDRW2100025702.1.17",
          "form": "아제처럼 아프면 그냥 바로 먹는데",
          "original_form": "아제처럼 아프면 그냥 바로 먹는데",
          "speaker_id": "1",
          "time": "20210901 16:12"
        },
        {
          "id": "MDRW2100025702.1.18",
          "form": "이건 그 정도는 아닌데",
          "original_form": "이건 그 정도는 아닌데",
          "speaker_id": "1",
          "time": "20210901 16:12"
        },
        {
          "id": "MDRW2100025702.1.19",
          "form": "아 나 아제너무아팠는데",
          "original_form": "아 나 아제너무아팠는데",
          "speaker_id": "2",
          "time": "20210901 16:12"
        },
        {
          "id": "MDRW2100025702.1.20",
          "form": "두통과 몸살 기운은 있어ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "두통과 몸살 기운은 있어ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:12"
        },
        {
          "id": "MDRW2100025702.1.21",
          "form": "아 너무싫은데...",
          "original_form": "아 너무싫은데...",
          "speaker_id": "2",
          "time": "20210901 16:13"
        },
        {
          "id": "MDRW2100025702.1.22",
          "form": "오늘 맛난거 드셔서 면역력 확보하세요...",
          "original_form": "오늘 맛난거 드셔서 면역력 확보하세요...",
          "speaker_id": "1",
          "time": "20210901 16:13"
        },
        {
          "id": "MDRW2100025702.1.23",
          "form": "내일도 출근???",
          "original_form": "내일도 출근???",
          "speaker_id": "1",
          "time": "20210901 16:13"
        },
        {
          "id": "MDRW2100025702.1.24",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210901 16:13"
        },
        {
          "id": "MDRW2100025702.1.25",
          "form": "오늘 유명한쌀국수집가서",
          "original_form": "오늘 유명한쌀국수집가서",
          "speaker_id": "2",
          "time": "20210901 16:13"
        },
        {
          "id": "MDRW2100025702.1.26",
          "form": "닭고기쌀국수먹었아7",
          "original_form": "닭고기쌀국수먹었아7",
          "speaker_id": "2",
          "time": "20210901 16:14"
        },
        {
          "id": "MDRW2100025702.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ잘했어 고기를 먹어야 튼튼이야",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ잘했어 고기를 먹어야 튼튼이야",
          "speaker_id": "1",
          "time": "20210901 16:14"
        },
        {
          "id": "MDRW2100025702.1.28",
          "form": "그래도 드디어 2차까지 맞으셨군요",
          "original_form": "그래도 드디어 2차까지 맞으셨군요",
          "speaker_id": "1",
          "time": "20210901 16:14"
        },
        {
          "id": "MDRW2100025702.1.29",
          "form": "ㅇㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210901 16:14"
        },
        {
          "id": "MDRW2100025702.1.30",
          "form": "저도이제끝",
          "original_form": "저도이제끝",
          "speaker_id": "2",
          "time": "20210901 16:14"
        },
        {
          "id": "MDRW2100025702.1.31",
          "form": "부스터말는다는9는데",
          "original_form": "부스터말는다는9는데",
          "speaker_id": "2",
          "time": "20210901 16:15"
        },
        {
          "id": "MDRW2100025702.1.32",
          "form": "맞는다는데",
          "original_form": "맞는다는데",
          "speaker_id": "2",
          "time": "20210901 16:15"
        },
        {
          "id": "MDRW2100025702.1.33",
          "form": "한동안은 괜찮겠지",
          "original_form": "한동안은 괜찮겠지",
          "speaker_id": "2",
