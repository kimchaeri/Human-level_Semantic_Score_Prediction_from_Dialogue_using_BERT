        },
        {
          "id": "MDRW2100001493.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211015 17:17"
        },
        {
          "id": "MDRW2100001493.1.3",
          "form": "혹시 주식이나 비트코인에 관심 많으세요?",
          "original_form": "혹시 주식이나 비트코인에 관심 많으세요?",
          "speaker_id": "2",
          "time": "20211015 17:17"
        },
        {
          "id": "MDRW2100001493.1.4",
          "form": "코인하고 있어요",
          "original_form": "코인하고 있어요",
          "speaker_id": "1",
          "time": "20211015 17:17"
        },
        {
          "id": "MDRW2100001493.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20211015 17:17"
        },
        {
          "id": "MDRW2100001493.1.6",
          "form": "오 코인하시는구나!",
          "original_form": "오 코인하시는구나!",
          "speaker_id": "2",
          "time": "20211015 17:17"
        },
        {
          "id": "MDRW2100001493.1.7",
          "form": "저는 둘 다 안해요 쫄보라서ㅠㅠ",
          "original_form": "저는 둘 다 안해요 쫄보라서ㅠㅠ",
          "speaker_id": "2",
          "time": "20211015 17:17"
        },
        {
          "id": "MDRW2100001493.1.8",
          "form": "근데 주식은 나중에 할 생각이 있어요!",
          "original_form": "근데 주식은 나중에 할 생각이 있어요!",
          "speaker_id": "2",
          "time": "20211015 17:18"
        },
        {
          "id": "MDRW2100001493.1.9",
          "form": "코인이 주식보단 쉬워요 ㅋㅋ",
          "original_form": "코인이 주식보단 쉬워요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211015 17:18"
        },
        {
          "id": "MDRW2100001493.1.10",
          "form": "소액이라도 해보세요 재미로",
          "original_form": "소액이라도 해보세요 재미로",
          "speaker_id": "1",
          "time": "20211015 17:18"
        },
        {
          "id": "MDRW2100001493.1.11",
          "form": "아 그런가요? ㅋㅋㅋㅋ",
          "original_form": "아 그런가요? ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211015 17:18"
        },
        {
          "id": "MDRW2100001493.1.12",
          "form": "넵 ㅋㅋ 퇴근 6시죠?",
          "original_form": "넵 ㅋㅋ 퇴근 6시죠?",
          "speaker_id": "1",
          "time": "20211015 17:18"
        },
        {
          "id": "MDRW2100001493.1.13",
          "form": "엇 퇴근시간 잘 찍으셨네요!",
          "original_form": "엇 퇴근시간 잘 찍으셨네요!",
          "speaker_id": "2",
          "time": "20211015 17:19"
        },
        {
          "id": "MDRW2100001493.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211015 17:19"
        },
        {
          "id": "MDRW2100001493.1.15",
          "form": "님도 일하고계세요?",
          "original_form": "님도 일하고계세요?",
          "speaker_id": "2",
          "time": "20211015 17:19"
        },
        {
          "id": "MDRW2100001493.1.16",
