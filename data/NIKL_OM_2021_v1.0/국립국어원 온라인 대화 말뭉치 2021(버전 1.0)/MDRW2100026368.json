      "utterance": [
        {
          "id": "MDRW2100026368.1.1",
          "form": "학교...까마득하지만 진짜 아쉬운게 중고딩때 사진이 별로없어요ㅠㅠ",
          "original_form": "학교...까마득하지만 진짜 아쉬운게 중고딩때 사진이 별로없어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100026368.1.2",
          "form": "저도여",
          "original_form": "저도여",
          "speaker_id": "1",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100026368.1.3",
          "form": "그. 때 못생겼다구 사진 안 찍었는데 ㅋㅋㅋㅋ",
          "original_form": "그. 때 못생겼다구 사진 안 찍었는데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100026368.1.4",
          "form": "지금 보면 걍 찍어둘걸 그랬어요 다 추억인데ㅠ",
          "original_form": "지금 보면 걍 찍어둘걸 그랬어요 다 추억인데ㅠ",
          "speaker_id": "1",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100026368.1.5",
          "form": "대리님 학교 여고였어요?",
          "original_form": "대리님 학교 여고였어요?",
          "speaker_id": "1",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100026368.1.6",
          "form": "ㅋㅋㅋㅋㅋ졸업사진은 흑역사",
          "original_form": "ㅋㅋㅋㅋㅋ졸업사진은 흑역사",
          "speaker_id": "2",
          "time": "20210902 23:01"
        },
        {
          "id": "MDRW2100026368.1.7",
          "form": "공학이였는데 남녀반 찢어진...여고나다름없었죠",
          "original_form": "공학이였는데 남녀반 찢어진...여고나다름없었죠",
          "speaker_id": "2",
          "time": "20210902 23:01"
        },
        {
          "id": "MDRW2100026368.1.8",
          "form": "저는 범생이과였으니깐요ㅋㅋㅋ",
          "original_form": "저는 범생이과였으니깐요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:01"
        },
        {
          "id": "MDRW2100026368.1.9",
          "form": "아하 저는 여고 나왔는데",
          "original_form": "아하 저는 여고 나왔는데",
          "speaker_id": "1",
          "time": "20210902 23:03"
        },
        {
          "id": "MDRW2100026368.1.10",
          "form": "재미있었던 거 같아여 ㅋㅋㅋㅋ",
          "original_form": "재미있었던 거 같아여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:03"
        },
        {
          "id": "MDRW2100026368.1.11",
          "form": "저도 졸업사진 흑역사임ㅋㅋㅋㅋㅋ",
          "original_form": "저도 졸업사진 흑역사임ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:04"
        },
        {
          "id": "MDRW2100026368.1.12",
          "form": "아 진짜 왜그러고 찍얼는지",
          "original_form": "아 진짜 왜그러고 찍얼는지",
          "speaker_id": "1",
          "time": "20210902 23:04"
        },
        {
          "id": "MDRW2100026368.1.13",
          "form": "ㅋㅋㅋㅋㅋ졸사...안경끼고찍지는 않았죠?",
          "original_form": "ㅋㅋㅋㅋㅋ졸사...안경끼고찍지는 않았죠?",
          "speaker_id": "2",
          "time": "20210902 23:04"
        },
        {
          "id": "MDRW2100026368.1.14",
          "form": "맞아여 고딩때가 젤 재밌었어요",
          "original_form": "맞아여 고딩때가 젤 재밌었어요",
          "speaker_id": "2",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100026368.1.15",
          "form": "만우절도ㅋㅋㅋ",
          "original_form": "만우절도ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100026368.1.16",
          "form": "그랬는대요ㅠㅠ",
          "original_form": "그랬는대요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100026368.1.17",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100026368.1.18",
          "form": "저 대학생때까지 안경끼구다님",
          "original_form": "저 대학생때까지 안경끼구다님",
          "speaker_id": "1",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100026368.1.19",
          "form": "렌즈낀 거 생각보다 얼마 안 됐어요",
          "original_form": "렌즈낀 거 생각보다 얼마 안 됐어요",
          "speaker_id": "1",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100026368.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ아진짜여?! 렌즈안끼고요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아진짜여?! 렌즈안끼고요?",
          "speaker_id": "2",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100026368.1.21",
          "form": "와우 도수가 높아서",
          "original_form": "와우 도수가 높아서",
          "speaker_id": "2",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100026368.1.22",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100026368.1.23",
          "form": "진짜 범생이죠",
          "original_form": "진짜 범생이죠",
          "speaker_id": "1",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100026368.1.24",
          "form": "완전 극과극이에요ㅋㅋㅋ",
          "original_form": "완전 극과극이에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100026368.1.25",
          "form": "화장도 렌즈도 다 24살때부터 시작함ㅋㅋ",
          "original_form": "화장도 렌즈도 다 24살때부터 시작함ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100026368.1.26",
          "form": "렌즈낀게 백배이쁨ㅋㅋㅋ",
          "original_form": "렌즈낀게 백배이쁨ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100026368.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ내과네......",
          "original_form": "ㅋㅋㅋㅋㅋㅋ내과네......",
          "speaker_id": "2",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100026368.1.28",
          "form": "ㅋㅋㅋㅋㅋ모나리자로 이십몇년산듯",
          "original_form": "ㅋㅋㅋㅋㅋ모나리자로 이십몇년산듯",
          "speaker_id": "2",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100026368.1.29",
          "form": "백배나?? 그정도에여???",
          "original_form": "백배나?? 그정도에여???",
          "speaker_id": "1",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100026368.1.30",
          "form": "저는 안경낀거나 안 낀거나 비슷하다구 생각하고 다니는데...",
          "original_form": "저는 안경낀거나 안 낀거나 비슷하다구 생각하고 다니는데...",
          "speaker_id": "1",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100026368.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100026368.1.32",
          "form": "전 고등학교 때 진짜 엄청 누가봐도 얌전하고 공부만 하게 하고다님ㅋㅋ",
          "original_form": "전 고등학교 때 진짜 엄청 누가봐도 얌전하고 공부만 하게 하고다님ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100026368.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ백배는 적은 수라고 생각합니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ백배는 적은 수라고 생각합니다",
          "speaker_id": "2",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100026368.1.34",
          "form": "뿔태안경에 올빽머리하고",
          "original_form": "뿔태안경에 올빽머리하고",
          "speaker_id": "1",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100026368.1.35",
          "form": "ㅋㅋㅋㅋ아 졸사 ... 궁금해지네요",
          "original_form": "ㅋㅋㅋㅋ아 졸사 ... 궁금해지네요",
          "speaker_id": "2",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100026368.1.36",
          "form": "대학때도 좀 그러고 다님 대신 머리는 풀었음ㅋㅋ",
          "original_form": "대학때도 좀 그러고 다님 대신 머리는 풀었음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100026368.1.37",
          "form": "123학년 때까지 안경끼고 화장 안 하구 다니다가",
          "original_form": "123학년 때까지 안경끼고 화장 안 하구 다니다가",
          "speaker_id": "1",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100026368.1.38",
          "form": "ㅋㅋㅋㅋㅋ근데 진짜 꾸미는것도 부지런해야",
          "original_form": "ㅋㅋㅋㅋㅋ근데 진짜 꾸미는것도 부지런해야",
          "speaker_id": "2",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100026368.1.39",
          "form": "맞아여",
          "original_form": "맞아여",
          "speaker_id": "1",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100026368.1.40",
          "form": "시간이 배로 들잖아요",
          "original_form": "시간이 배로 들잖아요",
          "speaker_id": "1",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100026368.1.41",
          "form": "글구 학생때는 관심도 없었던 거 같아요",
          "original_form": "글구 학생때는 관심도 없었던 거 같아요",
          "speaker_id": "1",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100026368.1.42",
          "form": "그쵸 저 아직 화장도 잘못함ㅋㅋㅋㅋ",
          "original_form": "그쵸 저 아직 화장도 잘못함ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:10"
        },
        {
          "id": "MDRW2100026368.1.43",
          "form": "퍼스널 컬러 해볼생각있어여?",
          "original_form": "퍼스널 컬러 해볼생각있어여?",
          "speaker_id": "2",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100026368.1.44",
          "form": "아녀",
          "original_form": "아녀",
