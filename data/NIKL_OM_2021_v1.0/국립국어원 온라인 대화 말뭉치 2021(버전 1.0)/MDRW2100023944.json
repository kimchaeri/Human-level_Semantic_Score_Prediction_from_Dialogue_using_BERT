        },
        {
          "id": "MDRW2100023944.1.2",
          "form": "멍뭉이 키워용?",
          "original_form": "멍뭉이 키워용?",
          "speaker_id": "1",
          "time": "20210830 20:28"
        },
        {
          "id": "MDRW2100023944.1.3",
          "form": "아니요~",
          "original_form": "아니요~",
          "speaker_id": "2",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100023944.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100023944.1.5",
          "form": "저희 부모님이 키우시는데 귀여워요 ㅜㅜ",
          "original_form": "저희 부모님이 키우시는데 귀여워요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100023944.1.6",
          "form": "우와ㅏㅏ",
          "original_form": "우와ㅏㅏ",
          "speaker_id": "2",
          "time": "20210830 20:31"
        },
        {
          "id": "MDRW2100023944.1.7",
          "form": "한 마리키우시나요?",
          "original_form": "한 마리키우시나요?",
          "speaker_id": "2",
          "time": "20210830 20:31"
        },
        {
          "id": "MDRW2100023944.1.8",
          "form": "네네 두마리 키우기엔 사고를 감당하지 못해서요 ㅋㅋㅋㅋ",
          "original_form": "네네 두마리 키우기엔 사고를 감당하지 못해서요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:32"
        },
        {
          "id": "MDRW2100023944.1.9",
          "form": "비글 섞인 종이에요",
          "original_form": "비글 섞인 종이에요",
          "speaker_id": "1",
          "time": "20210830 20:32"
        },
        {
          "id": "MDRW2100023944.1.10",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100023944.1.11",
          "form": "완전 귀엽겠네요",
          "original_form": "완전 귀엽겠네요",
          "speaker_id": "2",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100023944.1.12",
          "form": "저도 강아지 키우고 싶은데",
          "original_form": "저도 강아지 키우고 싶은데",
          "speaker_id": "2",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100023944.1.13",
          "form": "알러지가 있어서 ㅠㅠ 못키워요 ㅠ",
          "original_form": "알러지가 있어서 ㅠㅠ 못키워요 ㅠ",
          "speaker_id": "2",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100023944.1.14",
          "form": "허걱 무슨 알러지 있으세요?",
          "original_form": "허걱 무슨 알러지 있으세요?",
          "speaker_id": "1",
          "time": "20210830 20:35"
        },
        {
          "id": "MDRW2100023944.1.15",
          "form": "털 알러지가 있어서요ㅠㅠ",
          "original_form": "털 알러지가 있어서요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 20:38"
        },
        {
          "id": "MDRW2100023944.1.16",
          "form": "동물들 너무 좋아하는데 ㅠ",
          "original_form": "동물들 너무 좋아하는데 ㅠ",
          "speaker_id": "2",
          "time": "20210830 20:38"
        },
        {
          "id": "MDRW2100023944.1.17",
          "form": "건강이 반대하네요~ ㅠ",
          "original_form": "건강이 반대하네요~ ㅠ",
          "speaker_id": "2",
          "time": "20210830 20:38"
        },
        {
          "id": "MDRW2100023944.1.18",
          "form": "아이구 어떡해요",
          "original_form": "아이구 어떡해요",
          "speaker_id": "1",
          "time": "20210830 20:39"
        },
        {
          "id": "MDRW2100023944.1.19",
          "form": "그럼 강아지 뿐만 아니라 모든 동물 다 그래요?",
          "original_form": "그럼 강아지 뿐만 아니라 모든 동물 다 그래요?",
          "speaker_id": "1",
          "time": "20210830 20:39"
        },
        {
          "id": "MDRW2100023944.1.20",
          "form": "네 그런것 같아요ㅠ",
          "original_form": "네 그런것 같아요ㅠ",
          "speaker_id": "2",
          "time": "20210830 20:40"
        },
        {
          "id": "MDRW2100023944.1.21",
          "form": "name1님은 알러지 같은 것 없으세요?",
          "original_form": "&name1&님은 알러지 같은 것 없으세요?",
          "speaker_id": "2",
          "time": "20210830 20:40"
        },
        {
          "id": "MDRW2100023944.1.22",
          "form": "저는 알러지 비염이 있는데",
          "original_form": "저는 알러지 비염이 있는데",
          "speaker_id": "1",
          "time": "20210830 20:43"
        },
        {
          "id": "MDRW2100023944.1.23",
          "form": "알러지 원인 검사 했는데도 못찾았어요 ㅠㅠ힝힝",
          "original_form": "알러지 원인 검사 했는데도 못찾았어요 ㅠㅠ힝힝",
          "speaker_id": "1",
          "time": "20210830 20:43"
        },
        {
          "id": "MDRW2100023944.1.24",
          "form": "아이고~",
          "original_form": "아이고~",
          "speaker_id": "2",
          "time": "20210830 20:43"
        },
        {
          "id": "MDRW2100023944.1.25",
          "form": "저도 비염있어서",
          "original_form": "저도 비염있어서",
          "speaker_id": "2",
          "time": "20210830 20:43"
        },
        {
          "id": "MDRW2100023944.1.26",
          "form": "그 고통 알아요 ㅋㅋ큐ㅠㅠ",
          "original_form": "그 고통 알아요 ㅋㅋ큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 20:43"
        },
        {
          "id": "MDRW2100023944.1.27",
          "form": "ㅠㅠ 님은 원인 찾으셨어요?",
          "original_form": "ㅠㅠ 님은 원인 찾으셨어요?",
          "speaker_id": "1",
          "time": "20210830 20:44"
        },
        {
          "id": "MDRW2100023944.1.28",
          "form": "피검사보다 더 상위 검사가 있나.. 의심 되는 물질도 전혀 없어서 모르겠네요 저는",
          "original_form": "피검사보다 더 상위 검사가 있나.. 의심 되는 물질도 전혀 없어서 모르겠네요 저는",
          "speaker_id": "1",
          "time": "20210830 20:44"
        },
        {
          "id": "MDRW2100023944.1.29",
          "form": "저는 검사를 안해봐서 모르겠네요 ㅠ",
          "original_form": "저는 검사를 안해봐서 모르겠네요 ㅠ",
          "speaker_id": "2",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100023944.1.30",
          "form": "강아지들이랑 놀면",
          "original_form": "강아지들이랑 놀면",
          "speaker_id": "2",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100023944.1.31",
          "form": "목쪽 피부가 가려워서 ㅠ",
          "original_form": "목쪽 피부가 가려워서 ㅠ",
          "speaker_id": "2",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100023944.1.32",
          "form": "아 그래서 바로 직감적으로 알아차렸구나",
          "original_form": "아 그래서 바로 직감적으로 알아차렸구나",
          "speaker_id": "1",
          "time": "20210830 20:49"
        },
        {
          "id": "MDRW2100023944.1.33",
          "form": "저도 그렇게 바로 알아차리면 좋겠네요 ㅜ",
          "original_form": "저도 그렇게 바로 알아차리면 좋겠네요 ㅜ",
          "speaker_id": "1",
          "time": "20210830 20:49"
        }
      ]
