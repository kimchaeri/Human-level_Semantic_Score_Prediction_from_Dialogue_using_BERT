      "utterance": [
        {
          "id": "MDRW2100018082.1.1",
          "form": "자극적인 음식을 주로 좋아하는 거 같아요",
          "original_form": "자극적인 음식을 주로 좋아하는 거 같아요",
          "speaker_id": "1",
          "time": "20210826 22:27"
        },
        {
          "id": "MDRW2100018082.1.2",
          "form": "자연그대로 음식은.안젛아해요?",
          "original_form": "자연그대로 음식은.안젛아해요?",
          "speaker_id": "2",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100018082.1.3",
          "form": "샐러드나...",
          "original_form": "샐러드나...",
          "speaker_id": "2",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100018082.1.4",
          "form": "과일이나..",
          "original_form": "과일이나..",
          "speaker_id": "2",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100018082.1.5",
          "form": "맛집도 많이 다니세유?",
          "original_form": "맛집도 많이 다니세유?",
          "speaker_id": "2",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100018082.1.6",
          "form": "예전엔 많이 찾아갔는데 요새는 못 갔어요 ㅠㅠ 가는건 좋아해요",
          "original_form": "예전엔 많이 찾아갔는데 요새는 못 갔어요 ㅠㅠ 가는건 좋아해요",
          "speaker_id": "1",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018082.1.7",
          "form": "홍대 같은데요?",
          "original_form": "홍대 같은데요?",
          "speaker_id": "2",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100018082.1.8",
          "form": "네 거기에 신박한 집이 많더라구요",
          "original_form": "네 거기에 신박한 집이 많더라구요",
          "speaker_id": "1",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100018082.1.9",
          "form": "인싸들만 가는데 아녜여? ㅋㅋㅋㅋ",
          "original_form": "인싸들만 가는데 아녜여? ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100018082.1.10",
          "form": "저는 아싸지만 슬쩍 끼어들어봤어요 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 아싸지만 슬쩍 끼어들어봤어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100018082.1.11",
          "form": "다른지역 맛집도 가시나요?",
          "original_form": "다른지역 맛집도 가시나요?",
          "speaker_id": "2",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100018082.1.12",
          "form": "전 거기.. 종로 옛날마을 같은데",
          "original_form": "전 거기.. 종로 옛날마을 같은데",
          "speaker_id": "2",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100018082.1.13",
          "form": "가보고 싶어요",
          "original_form": "가보고 싶어요",
          "speaker_id": "2",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100018082.1.14",
          "form": "음 네 강남쪽도 많이 간거같아요",
          "original_form": "음 네 강남쪽도 많이 간거같아요",
          "speaker_id": "1",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100018082.1.15",
          "form": "초 인싸 미식가이시군여..",
          "original_form": "초 인싸 미식가이시군여..",
          "speaker_id": "2",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100018082.1.16",
          "form": "강남은 강남역 근처요?",
          "original_form": "강남은 강남역 근처요?",
          "speaker_id": "2",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018082.1.17",
          "form": "앜ㅋㅋㅋㅋ 맛집 잘 안다니세요??",
          "original_form": "앜ㅋㅋㅋㅋ 맛집 잘 안다니세요??",
          "speaker_id": "1",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018082.1.18",
          "form": "네 그 근처에 많더라구요",
          "original_form": "네 그 근처에 많더라구요",
          "speaker_id": "1",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018082.1.19",
          "form": "전 맛알못이라... \r\n그저 배부르면 그거시 산해진미라..",
          "original_form": "전 맛알못이라... \r\n그저 배부르면 그거시 산해진미라..",
          "speaker_id": "2",
          "time": "20210826 23:21"
        },
        {
          "id": "MDRW2100018082.1.20",
          "form": "혀가 천박해서..",
          "original_form": "혀가 천박해서..",
          "speaker_id": "2",
          "time": "20210826 23:21"
        }
      ]
