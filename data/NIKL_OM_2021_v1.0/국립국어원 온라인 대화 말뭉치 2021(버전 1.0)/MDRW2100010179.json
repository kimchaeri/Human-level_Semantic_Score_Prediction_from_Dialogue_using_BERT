      "utterance": [
        {
          "id": "MDRW2100010179.1.1",
          "form": "운수좋은 날 주인공을 올려치기하는 걸 듣는 나는 그냥 어리둥절해져서 이게 맞는 건가 싶었음",
          "original_form": "운수좋은 날 주인공을 올려치기하는 걸 듣는 나는 그냥 어리둥절해져서 이게 맞는 건가 싶었음",
          "speaker_id": "2",
          "time": "20210823 15:15"
        },
        {
          "id": "MDRW2100010179.1.2",
          "form": "그니까 그걸 학생들한테 가르친다는게",
          "original_form": "그니까 그걸 학생들한테 가르친다는게",
          "speaker_id": "1",
          "time": "20210823 15:16"
        },
        {
          "id": "MDRW2100010179.1.3",
          "form": "너무 유해해",
          "original_form": "너무 유해해",
          "speaker_id": "1",
          "time": "20210823 15:16"
        },
        {
          "id": "MDRW2100010179.1.4",
          "form": "학생들은 아무생각 없이 그런걸 받아들일수 있잖아",
          "original_form": "학생들은 아무생각 없이 그런걸 받아들일수 있잖아",
          "speaker_id": "1",
          "time": "20210823 15:16"
        },
        {
          "id": "MDRW2100010179.1.5",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210823 15:16"
        },
        {
          "id": "MDRW2100010179.1.6",
          "form": "그리고 봄봄 맞나",
          "original_form": "그리고 봄봄 맞나",
          "speaker_id": "2",
          "time": "20210823 15:16"
        },
        {
          "id": "MDRW2100010179.1.7",
          "form": "그리고 남자가 쓴 시도 보면",
          "original_form": "그리고 남자가 쓴 시도 보면",
          "speaker_id": "1",
          "time": "20210823 15:16"
        },
        {
          "id": "MDRW2100010179.1.8",
          "form": "ㅇㅇㅇ봄봄은뭐였음?",
          "original_form": "ㅇㅇㅇ봄봄은뭐였음?",
          "speaker_id": "1",
          "time": "20210823 15:16"
        },
        {
          "id": "MDRW2100010179.1.9",
          "form": "먼저 얘기하삼",
          "original_form": "먼저 얘기하삼",
          "speaker_id": "2",
          "time": "20210823 15:16"
        },
        {
          "id": "MDRW2100010179.1.10",
          "form": "ㄱ아",
          "original_form": "ㄱ아",
          "speaker_id": "1",
          "time": "20210823 15:16"
        },
        {
          "id": "MDRW2100010179.1.11",
          "form": "아니 여자 몸매 얘기만 엄청 해",
          "original_form": "아니 여자 몸매 얘기만 엄청 해",
          "speaker_id": "1",
          "time": "20210823 15:17"
        },
        {
          "id": "MDRW2100010179.1.12",
          "form": "그래놓고예술성이있대",
          "original_form": "그래놓고예술성이있대",
          "speaker_id": "1",
          "time": "20210823 15:17"
        },
        {
          "id": "MDRW2100010179.1.13",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210823 15:17"
        },
        {
          "id": "MDRW2100010179.1.14",
          "form": "너하고싶은말은뭐임",
          "original_form": "너하고싶은말은뭐임",
          "speaker_id": "1",
          "time": "20210823 15:17"
        },
        {
          "id": "MDRW2100010179.1.15",
          "form": "욕할 때도 여자 욕함",
          "original_form": "욕할 때도 여자 욕함",
          "speaker_id": "2",
          "time": "20210823 15:17"
        },
        {
          "id": "MDRW2100010179.1.16",
          "form": "봄봄 작가 여자 스토킹함",
          "original_form": "봄봄 작가 여자 스토킹함",
          "speaker_id": "2",
          "time": "20210823 15:17"
        },
        {
          "id": "MDRW2100010179.1.17",
          "form": "아 봄봄 교과서에 있나?",
          "original_form": "아 봄봄 교과서에 있나?",
          "speaker_id": "1",
          "time": "20210823 15:17"
        },
        {
          "id": "MDRW2100010179.1.18",
          "form": "소설이네",
          "original_form": "소설이네",
          "speaker_id": "1",
          "time": "20210823 15:18"
        },
        {
          "id": "MDRW2100010179.1.19",
          "form": "아이거그건가",
          "original_form": "아이거그건가",
          "speaker_id": "1",
          "time": "20210823 15:18"
        },
        {
          "id": "MDRW2100010179.1.20",
          "form": "그감자얘기",
          "original_form": "그감자얘기",
          "speaker_id": "1",
          "time": "20210823 15:18"
        },
        {
          "id": "MDRW2100010179.1.21",
          "form": "감자는 동백꽃일걸",
          "original_form": "감자는 동백꽃일걸",
          "speaker_id": "2",
          "time": "20210823 15:18"
        },
        {
          "id": "MDRW2100010179.1.22",
          "form": "봄봄은 애 감기 들려서 죽는거일걸",
          "original_form": "봄봄은 애 감기 들려서 죽는거일걸",
          "speaker_id": "2",
          "time": "20210823 15:19"
        },
        {
          "id": "MDRW2100010179.1.23",
          "form": "둘다 김유정 작품은 맞음",
          "original_form": "둘다 김유정 작품은 맞음",
          "speaker_id": "2",
          "time": "20210823 15:19"
        },
        {
          "id": "MDRW2100010179.1.24",
          "form": "아 그런 작가 교과서에 나온다는거 자체가",
          "original_form": "아 그런 작가 교과서에 나온다는거 자체가",
          "speaker_id": "1",
          "time": "20210823 15:19"
        },
        {
          "id": "MDRW2100010179.1.25",
          "form": "참",
          "original_form": "참",
          "speaker_id": "1",
          "time": "20210823 15:19"
        },
        {
          "id": "MDRW2100010179.1.26",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210823 15:19"
        },
        {
          "id": "MDRW2100010179.1.27",
          "form": "아마성별반대였으면",
          "original_form": "아마성별반대였으면",
          "speaker_id": "1",
          "time": "20210823 15:19"
        },
        {
          "id": "MDRW2100010179.1.28",
          "form": "진작에다뒤집어졋을듯",
          "original_form": "진작에다뒤집어졋을듯",
          "speaker_id": "1",
          "time": "20210823 15:19"
        },
        {
          "id": "MDRW2100010179.1.29",
          "form": "일단 그 작가 이름으로 안나옴",
          "original_form": "일단 그 작가 이름으로 안나옴",
          "speaker_id": "2",
          "time": "20210823 15:19"
        },
        {
          "id": "MDRW2100010179.1.30",
          "form": "남편 이름으로 나왔겠지",
          "original_form": "남편 이름으로 나왔겠지",
          "speaker_id": "2",
          "time": "20210823 15:20"
        },
        {
          "id": "MDRW2100010179.1.31",
          "form": "아 현실반영",
          "original_form": "아 현실반영",
          "speaker_id": "1",
          "time": "20210823 15:20"
        },
        {
          "id": "MDRW2100010179.1.32",
          "form": "소름이네",
          "original_form": "소름이네",
          "speaker_id": "1",
          "time": "20210823 15:20"
        },
        {
          "id": "MDRW2100010179.1.33",
          "form": "그니까 나중에서야 이름 되찾고 그랬구나 하고",
          "original_form": "그니까 나중에서야 이름 되찾고 그랬구나 하고",
          "speaker_id": "2",
          "time": "20210823 15:20"
        }
      ]
