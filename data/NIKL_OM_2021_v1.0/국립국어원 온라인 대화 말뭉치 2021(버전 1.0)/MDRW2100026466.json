        },
        {
          "id": "MDRW2100026466.1.3",
          "form": "네 안녕하세요^^",
          "original_form": "네 안녕하세요^^",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026466.1.4",
          "form": "저 있어요!",
          "original_form": "저 있어요!",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026466.1.5",
          "form": "맘이베베랑 삼성스마트폰카페욬ㅋㅋ",
          "original_form": "맘이베베랑 삼성스마트폰카페욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100026466.1.6",
          "form": "오 ㅋㅋ 저도 맘이베베 카페에 가입되어있어요 ㅋㅋ",
          "original_form": "오 ㅋㅋ 저도 맘이베베 카페에 가입되어있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026466.1.7",
          "form": "거기에서 활동 자주하시나요?",
          "original_form": "거기에서 활동 자주하시나요?",
          "speaker_id": "1",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026466.1.8",
          "form": "대부분 글 읽기만하고",
          "original_form": "대부분 글 읽기만하고",
          "speaker_id": "2",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026466.1.9",
          "form": "댓글다는정도로? 하고있어요^^",
          "original_form": "댓글다는정도로? 하고있어요^^",
          "speaker_id": "2",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026466.1.10",
          "form": "핫딜방만 잘보고 다른건 잘안해요.ㅎㅎ",
          "original_form": "핫딜방만 잘보고 다른건 잘안해요.ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026466.1.11",
          "form": "ㅋㅋㅋ저도 그정도로 하고 있어요 ㅋㅋ",
          "original_form": "ㅋㅋㅋ저도 그정도로 하고 있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100026466.1.12",
          "form": "맘이베베 한동안 얼마나 들어갔는지 ㅋㅋ",
          "original_form": "맘이베베 한동안 얼마나 들어갔는지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100026466.1.13",
          "form": "그쵸 저도 한때는 중독수준이었는데",
          "original_form": "그쵸 저도 한때는 중독수준이었는데",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100026466.1.14",
          "form": "요즘엔 좀 덜하네요",
          "original_form": "요즘엔 좀 덜하네요",
          "speaker_id": "2",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100026466.1.15",
          "form": "맞아요 저도 중독수준 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 저도 중독수준 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026466.1.16",
          "form": "요즘에는 좀 알뜰해진 느낌이요 ㅋㅋㅋ 재테크카페 이런데 또 보고 자극받거든요 ㅋㅋ",
          "original_form": "요즘에는 좀 알뜰해진 느낌이요 ㅋㅋㅋ 재테크카페 이런데 또 보고 자극받거든요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026466.1.17",
          "form": "오~멋지세요!ㅎㅎ 커뮤니티 활동의 좋은예..ㅎㅎ",
          "original_form": "오~멋지세요!ㅎㅎ 커뮤니티 활동의 좋은예..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026466.1.18",
          "form": "네넨 가계부 적는 사람들꺼 보면은 와 싶어요 ㅋㅋ",
          "original_form": "네넨 가계부 적는 사람들꺼 보면은 와 싶어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026466.1.19",
          "form": "ㅋㅋ멋지네요 옛날에는 카페활동하면 정모도 하고 그랬나보던데",
          "original_form": "ㅋㅋ멋지네요 옛날에는 카페활동하면 정모도 하고 그랬나보던데",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100026466.1.20",
          "form": "코시국엔 불가한 일이지만요~",
          "original_form": "코시국엔 불가한 일이지만요~",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100026466.1.21",
          "form": "맞아요 ㅋㅋㅋ 맘카페 이런데서 정모 막 하면 나가려고 했었는데 ㅋㅋ 그뒤에 뭐 ㅋㅋㅋ 없네요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ 맘카페 이런데서 정모 막 하면 나가려고 했었는데 ㅋㅋ 그뒤에 뭐 ㅋㅋㅋ 없네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100026466.1.22",
          "form": "전 초보운전 카페 가입했는데요 ㅋㅋ",
