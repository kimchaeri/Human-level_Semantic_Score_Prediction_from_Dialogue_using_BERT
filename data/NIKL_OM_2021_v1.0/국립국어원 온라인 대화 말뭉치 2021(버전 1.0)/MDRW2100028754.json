        },
        {
          "id": "MDRW2100028754.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028754.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028754.1.5",
          "form": "그게 은근 서운하고",
          "original_form": "그게 은근 서운하고",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028754.1.6",
          "form": "그 서운함이 오래가요",
          "original_form": "그 서운함이 오래가요",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028754.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028754.1.8",
          "form": "아플땐 무조건 최선을 다해줘야지!",
          "original_form": "아플땐 무조건 최선을 다해줘야지!",
          "speaker_id": "2",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100028754.1.9",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028754.1.10",
          "form": "이번에 2차맞고",
          "original_form": "이번에 2차맞고",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028754.1.11",
          "form": "컨디션 안좋을때",
          "original_form": "컨디션 안좋을때",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028754.1.12",
          "form": "지켜봐요",
          "original_form": "지켜봐요",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028754.1.13",
          "form": "두눈을 부릅뜨고",
          "original_form": "두눈을 부릅뜨고",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028754.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100028754.1.15",
          "form": "아픈데 지켜볼 여유가 있을랑가",
          "original_form": "아픈데 지켜볼 여유가 있을랑가",
          "speaker_id": "2",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100028754.1.16",
          "form": "여친",
          "original_form": "여친",
          "speaker_id": "1",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028754.1.17",
          "form": "지금상태",
          "original_form": "지금상태",
          "speaker_id": "1",
          "time": "20210902 21:54"
        },
        {
          "id": "MDRW2100028754.1.18",
          "form": "열이 계속 오르락내리락",
          "original_form": "열이 계속 오르락내리락",
          "speaker_id": "1",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028754.1.19",
          "form": "아이고 죽겠다 연발중....",
          "original_form": "아이고 죽겠다 연발중....",
          "speaker_id": "1",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028754.1.20",
          "form": "아유 ㅠ 저희도 오늘 체육쌤 진짜 상태 안좋아서 조퇴했어요",
          "original_form": "아유 ㅠ 저희도 오늘 체육쌤 진짜 상태 안좋아서 조퇴했어요",
          "speaker_id": "2",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028754.1.21",
          "form": "한숨 자고 일어났나어",
          "original_form": "한숨 자고 일어났나어",
          "speaker_id": "2",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028754.1.22",
          "form": "남자도 예외없다던데",
          "original_form": "남자도 예외없다던데",
          "speaker_id": "1",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028754.1.23",
          "form": "여친 오늘 학교 못가고",
          "original_form": "여친 오늘 학교 못가고",
          "speaker_id": "1",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100028754.1.24",
          "form": "계속 잠만잤어요",
          "original_form": "계속 잠만잤어요",
          "speaker_id": "1",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028754.1.25",
          "form": "아 학교도 쉬셨구나",
          "original_form": "아 학교도 쉬셨구나",
          "speaker_id": "2",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028754.1.26",
          "form": "이게 방학때 다 맞았어야됐을건데",
          "original_form": "이게 방학때 다 맞았어야됐을건데",
          "speaker_id": "2",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028754.1.27",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "1",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028754.1.28",
          "form": "다들 백신 맞고 나오셔서 일하는 거 보면 너무 안타깝고 ㅋㅋㅋ",
          "original_form": "다들 백신 맞고 나오셔서 일하는 거 보면 너무 안타깝고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028754.1.29",
          "form": "갑자기 밀려가지고",
          "original_form": "갑자기 밀려가지고",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028754.1.30",
          "form": "남친은 제가 백신맞고 상태안좋아서 주말에 못만나니까",
          "original_form": "남친은 제가 백신맞고 상태안좋아서 주말에 못만나니까",
          "speaker_id": "2",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028754.1.31",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028754.1.32",
          "form": "통화 열심히 해야겠다구 말하던데",
          "original_form": "통화 열심히 해야겠다구 말하던데",
          "speaker_id": "2",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028754.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028754.1.34",
          "form": "가능할까요",
          "original_form": "가능할까요",
          "speaker_id": "1",
          "time": "20210902 21:57"
        },
        {
          "id": "MDRW2100028754.1.35",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100028754.1.36",
          "form": "여친 죽을라하던데ㅜㅜ",
          "original_form": "여친 죽을라하던데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100028754.1.37",
          "form": "ㅋㅋㅋㅋ전화통화 못할거 같은데",
          "original_form": "ㅋㅋㅋㅋ전화통화 못할거 같은데",
          "speaker_id": "2",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028754.1.38",
          "form": "본인은 이미 2차까지 다 맞으셨기에",
          "original_form": "본인은 이미 2차까지 다 맞으셨기에",
          "speaker_id": "2",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028754.1.39",
          "form": "잠 푹주무세요",
          "original_form": "잠 푹주무세요",
          "speaker_id": "1",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028754.1.40",
          "form": "2차가 멀쩡했었기에 괜찮은가",
