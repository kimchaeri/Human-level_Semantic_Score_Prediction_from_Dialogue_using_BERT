        {
          "id": "MDRW2100034414.1.1",
          "form": "혼자 사시나요?",
          "original_form": "혼자 사시나요?",
          "speaker_id": "2",
          "time": "20210828 22:40"
        },
        {
          "id": "MDRW2100034414.1.2",
          "form": "아니요 룸메이트랑 같이살아요",
          "original_form": "아니요 룸메이트랑 같이살아요",
          "speaker_id": "1",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100034414.1.3",
          "form": "혼자사시나요?",
          "original_form": "혼자사시나요?",
          "speaker_id": "1",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100034414.1.4",
          "form": "저는 가족들이랑 살아요",
          "original_form": "저는 가족들이랑 살아요",
          "speaker_id": "2",
          "time": "20210828 22:42"
        },
        {
          "id": "MDRW2100034414.1.5",
          "form": "자취하고싶어요ㅠㅠ",
          "original_form": "자취하고싶어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 22:42"
        },
        {
          "id": "MDRW2100034414.1.6",
          "form": "왜 자취하고 싶으세요?",
          "original_form": "왜 자취하고 싶으세요?",
          "speaker_id": "1",
          "time": "20210828 22:42"
        },
        {
          "id": "MDRW2100034414.1.7",
          "form": "혼자만의 시간이 너무 없는것 같아요ㅠㅠ",
          "original_form": "혼자만의 시간이 너무 없는것 같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 23:54"
        },
        {
          "id": "MDRW2100034414.1.8",
          "form": "자취하면 자유로운게 장점이긴 한거 같아요",
          "original_form": "자취하면 자유로운게 장점이긴 한거 같아요",
          "speaker_id": "1",
          "time": "20210828 23:57"
        },
        {
          "id": "MDRW2100034414.1.9",
          "form": "그쵸 자유가 그리워요ㅋㅋ",
          "original_form": "그쵸 자유가 그리워요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:57"
        },
        {
          "id": "MDRW2100034414.1.10",
          "form": "집안일은 자주 하시나요?",
          "original_form": "집안일은 자주 하시나요?",
          "speaker_id": "1",
          "time": "20210828 23:58"
        },
        {
          "id": "MDRW2100034414.1.11",
          "form": "자취할땐 잘했는데 본가 들어오고부터는 아무것도 안해요...ㅋㅋ",
          "original_form": "자취할땐 잘했는데 본가 들어오고부터는 아무것도 안해요...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100034414.1.12",
          "form": "자취하셨었구나",
          "original_form": "자취하셨었구나",
          "speaker_id": "1",
          "time": "20210829 00:48"
        },
        {
          "id": "MDRW2100034414.1.13",
          "form": "학교다닐때 하신거예요?",
          "original_form": "학교다닐때 하신거예요?",
          "speaker_id": "1",
          "time": "20210829 00:48"
        },
        {
          "id": "MDRW2100034414.1.14",
          "form": "네네 1년정도 한것같아요",
          "original_form": "네네 1년정도 한것같아요",
          "speaker_id": "2",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100034414.1.15",
          "form": "한번 자유를 즐기셔가지고 그리워하시는거였군요ㅋㅋㅋ",
          "original_form": "한번 자유를 즐기셔가지고 그리워하시는거였군요ㅋㅋㅋ",
