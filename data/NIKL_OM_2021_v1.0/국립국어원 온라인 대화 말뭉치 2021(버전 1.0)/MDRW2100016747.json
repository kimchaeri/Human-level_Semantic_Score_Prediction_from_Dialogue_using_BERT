      "utterance": [
        {
          "id": "MDRW2100016747.1.1",
          "form": "좋아하시는 프로그램 있으세요?",
          "original_form": "좋아하시는 프로그램 있으세요?",
          "speaker_id": "1",
          "time": "20210826 15:07"
        },
        {
          "id": "MDRW2100016747.1.2",
          "form": "티비가 없어서 없어요",
          "original_form": "티비가 없어서 없어요",
          "speaker_id": "2",
          "time": "20210826 15:07"
        },
        {
          "id": "MDRW2100016747.1.3",
          "form": "있으세요?",
          "original_form": "있으세요?",
          "speaker_id": "2",
          "time": "20210826 15:07"
        },
        {
          "id": "MDRW2100016747.1.4",
          "form": "저는 펜트하우스랑 광자매봐요",
          "original_form": "저는 펜트하우스랑 광자매봐요",
          "speaker_id": "1",
          "time": "20210826 15:07"
        },
        {
          "id": "MDRW2100016747.1.5",
          "form": "펜트하우스가 그렇게 자극적이라던데 어때요?",
          "original_form": "펜트하우스가 그렇게 자극적이라던데 어때요?",
          "speaker_id": "2",
          "time": "20210826 15:08"
        },
        {
          "id": "MDRW2100016747.1.6",
          "form": "완전 막장이에요 ㅋㅋㅋ 작가가 원래 그렇더라고요",
          "original_form": "완전 막장이에요 ㅋㅋㅋ 작가가 원래 그렇더라고요",
          "speaker_id": "1",
          "time": "20210826 15:09"
        },
        {
          "id": "MDRW2100016747.1.7",
          "form": "2편은 몇번봤었는데 이번에도 많이 죽나요?",
          "original_form": "2편은 몇번봤었는데 이번에도 많이 죽나요?",
          "speaker_id": "2",
          "time": "20210826 15:09"
        },
        {
          "id": "MDRW2100016747.1.8",
          "form": "요샌 일주일에 한번만해서 사실 기억이 잘 안나는데 ㅋㅋㅋ 중요한 사람 한명 죽었어요",
          "original_form": "요샌 일주일에 한번만해서 사실 기억이 잘 안나는데 ㅋㅋㅋ 중요한 사람 한명 죽었어요",
          "speaker_id": "1",
          "time": "20210826 15:10"
        },
        {
          "id": "MDRW2100016747.1.9",
          "form": "헉 또 ....죽는구나",
          "original_form": "헉 또 ....죽는구나",
          "speaker_id": "2",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100016747.1.10",
          "form": "진짜 극단적이네요",
          "original_form": "진짜 극단적이네요",
          "speaker_id": "2",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100016747.1.11",
          "form": "워낙 죽었다 살았다해서",
          "original_form": "워낙 죽었다 살았다해서",
          "speaker_id": "1",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100016747.1.12",
          "form": "또 살려나했는데 안 살거같아요 ㅋㅋㅋ",
          "original_form": "또 살려나했는데 안 살거같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100016747.1.13",
          "form": "근데 그맛에 보는거 같아요ㅋㅋㅋ",
          "original_form": "근데 그맛에 보는거 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:12"
        },
        {
          "id": "MDRW2100016747.1.14",
          "form": "맞아요 반전에 반전을 거듭하는 맛이져 ㅋㅋㅋ",
          "original_form": "맞아요 반전에 반전을 거듭하는 맛이져 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:13"
        },
        {
          "id": "MDRW2100016747.1.15",
          "form": "잠깐봤는데 완전 자극적이라 눈을 못떼겠더라구요",
          "original_form": "잠깐봤는데 완전 자극적이라 눈을 못떼겠더라구요",
          "speaker_id": "2",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100016747.1.16",
          "form": "맞아요 인기요소인거같아요",
