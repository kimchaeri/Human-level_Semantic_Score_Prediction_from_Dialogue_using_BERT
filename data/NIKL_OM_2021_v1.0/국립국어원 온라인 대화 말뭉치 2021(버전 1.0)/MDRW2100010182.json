      "utterance": [
        {
          "id": "MDRW2100010182.1.1",
          "form": "마켓컬리에 먹을 것 좀 담아둘까?? ㅎㅎ",
          "original_form": "마켓컬리에 먹을 것 좀 담아둘까?? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 15:18"
        },
        {
          "id": "MDRW2100010182.1.2",
          "form": "마켓컬리도 쎄일하나?",
          "original_form": "마켓컬리도 쎄일하나?",
          "speaker_id": "2",
          "time": "20210823 15:19"
        },
        {
          "id": "MDRW2100010182.1.3",
          "form": "세일품목도 있을껄?",
          "original_form": "세일품목도 있을껄?",
          "speaker_id": "1",
          "time": "20210823 15:19"
        },
        {
          "id": "MDRW2100010182.1.4",
          "form": "야금야금 필요한 재료들 하나씩 하나씩 담아두지 뭐 ㅋㅋ",
          "original_form": "야금야금 필요한 재료들 하나씩 하나씩 담아두지 뭐 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:20"
        },
        {
          "id": "MDRW2100010182.1.5",
          "form": "응응 그러자",
          "original_form": "응응 그러자",
          "speaker_id": "2",
          "time": "20210823 15:20"
        },
        {
          "id": "MDRW2100010182.1.6",
          "form": "계란 한판이랑~",
          "original_form": "계란 한판이랑~",
          "speaker_id": "1",
          "time": "20210823 15:20"
        },
        {
          "id": "MDRW2100010182.1.7",
          "form": "대표님이 오늘 맛집 추천해줌",
          "original_form": "대표님이 오늘 맛집 추천해줌",
          "speaker_id": "2",
          "time": "20210823 15:20"
        },
        {
          "id": "MDRW2100010182.1.8",
          "form": "오~~어디???",
          "original_form": "오~~어디???",
          "speaker_id": "1",
          "time": "20210823 15:20"
        },
        {
          "id": "MDRW2100010182.1.9",
          "form": "우레옥에 불고기가 맛나데 \r\n근데 을지로야ㅠㅠ\r\n한번 가보고싶긴한데",
          "original_form": "우레옥에 불고기가 맛나데 \r\n근데 을지로야ㅠㅠ\r\n한번 가보고싶긴한데",
          "speaker_id": "2",
          "time": "20210823 15:21"
        },
        {
          "id": "MDRW2100010182.1.10",
          "form": "자기도 돈가스 추천해드려~ 제주에 연돈 맛있다고 ㅋ",
          "original_form": "자기도 돈가스 추천해드려~ 제주에 연돈 맛있다고 ㅋ",
          "speaker_id": "1",
          "time": "20210823 15:21"
        },
        {
          "id": "MDRW2100010182.1.11",
          "form": "ㅋㅋ 내가 먹어보지도 않고?",
          "original_form": "ㅋㅋ 내가 먹어보지도 않고?",
          "speaker_id": "2",
          "time": "20210823 15:22"
        },
        {
          "id": "MDRW2100010182.1.12",
          "form": "무조건 맛있다고 해 ㅋㅋ",
          "original_form": "무조건 맛있다고 해 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:22"
        },
        {
          "id": "MDRW2100010182.1.13",
          "form": "돈가스소스도 있는데 돈가스도 좀 튀겨먹을까?ㅋㅋ",
          "original_form": "돈가스소스도 있는데 돈가스도 좀 튀겨먹을까?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:22"
        },
        {
          "id": "MDRW2100010182.1.14",
          "form": "왜 그래야하죠?",
          "original_form": "왜 그래야하죠?",
          "speaker_id": "2",
          "time": "20210823 15:22"
        },
        {
          "id": "MDRW2100010182.1.15",
          "form": "소스 유통기한은?",
          "original_form": "소스 유통기한은?",
          "speaker_id": "2",
          "time": "20210823 15:22"
        },
        {
          "id": "MDRW2100010182.1.16",
          "form": "그리고 소스 엄청 맛없어ㅋ",
          "original_form": "그리고 소스 엄청 맛없어ㅋ",
          "speaker_id": "2",
          "time": "20210823 15:22"
        },
        {
          "id": "MDRW2100010182.1.17",
          "form": "좀 남았을껄? ㅋ",
          "original_form": "좀 남았을껄? ㅋ",
          "speaker_id": "1",
          "time": "20210823 15:22"
        },
        {
          "id": "MDRW2100010182.1.18",
          "form": "집에 있는 소스는 경양식 돈가스에 어울리는 소스",
          "original_form": "집에 있는 소스는 경양식 돈가스에 어울리는 소스",
          "speaker_id": "1",
          "time": "20210823 15:23"
        },
        {
          "id": "MDRW2100010182.1.19",
          "form": "응 돈가스도 좀 담아봐ㅋ",
          "original_form": "응 돈가스도 좀 담아봐ㅋ",
          "speaker_id": "2",
          "time": "20210823 15:24"
        },
        {
          "id": "MDRW2100010182.1.20",
          "form": "아직 담아두고 있진 않아 ㅋㅋ",
          "original_form": "아직 담아두고 있진 않아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:25"
        },
        {
          "id": "MDRW2100010182.1.21",
          "form": "ㅋㅋ 후기 잘봐야함",
          "original_form": "ㅋㅋ 후기 잘봐야함",
          "speaker_id": "2",
          "time": "20210823 15:25"
        },
        {
          "id": "MDRW2100010182.1.22",
          "form": "우리 맛집 리스트 다시 작성하자ㅋ",
          "original_form": "우리 맛집 리스트 다시 작성하자ㅋ",
          "speaker_id": "2",
          "time": "20210823 15:26"
        },
        {
          "id": "MDRW2100010182.1.23",
          "form": "아!! 그릅시다 ㅋㅋ",
          "original_form": "아!! 그릅시다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:27"
        },
        {
          "id": "MDRW2100010182.1.24",
          "form": "카톡 공지로 올리자ㅋ",
          "original_form": "카톡 공지로 올리자ㅋ",
          "speaker_id": "2",
          "time": "20210823 15:27"
        },
        {
          "id": "MDRW2100010182.1.25",
          "form": "평일엔 장본걸로 식재료 쓰고, 주말엔 맛집가고 ㅋㅋ",
          "original_form": "평일엔 장본걸로 식재료 쓰고, 주말엔 맛집가고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:27"
        },
        {
          "id": "MDRW2100010182.1.26",
          "form": "응 그러자\r\n언제 렌트해서 휴게소도 다니자!",
          "original_form": "응 그러자\r\n언제 렌트해서 휴게소도 다니자!",
          "speaker_id": "2",
          "time": "20210823 15:28"
        },
        {
          "id": "MDRW2100010182.1.27",
          "form": "코로나 좀 진정되면 가야겠지만ㅠㅠ",
          "original_form": "코로나 좀 진정되면 가야겠지만ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 15:28"
        },
        {
          "id": "MDRW2100010182.1.28",
          "form": "휴게소 몇군데를 가려고 ㅋㅋ",
          "original_form": "휴게소 몇군데를 가려고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:28"
        },
        {
          "id": "MDRW2100010182.1.29",
          "form": "ㅋㅋ 여러군데?",
          "original_form": "ㅋㅋ 여러군데?",
          "speaker_id": "2",
          "time": "20210823 15:28"
        },
        {
          "id": "MDRW2100010182.1.30",
          "form": "휴게소마다 맛난게 다르지 않아?",
          "original_form": "휴게소마다 맛난게 다르지 않아?",
          "speaker_id": "2",
          "time": "20210823 15:29"
        },
        {
          "id": "MDRW2100010182.1.31",
          "form": "그렇기도 하지 ㅋㅋㅋ",
          "original_form": "그렇기도 하지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:30"
        },
        {
          "id": "MDRW2100010182.1.32",
          "form": "이러다 식재료 구하러 현지로 다닐 기세여 ㅋㅋ",
          "original_form": "이러다 식재료 구하러 현지로 다닐 기세여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:31"
        },
        {
          "id": "MDRW2100010182.1.33",
          "form": "ㅋㅋ 그건좀 오바시구요",
          "original_form": "ㅋㅋ 그건좀 오바시구요",
          "speaker_id": "2",
          "time": "20210823 15:31"
        },
        {
          "id": "MDRW2100010182.1.34",
          "form": "오늘저녁 뭐먹지?",
          "original_form": "오늘저녁 뭐먹지?",
          "speaker_id": "2",
          "time": "20210823 15:31"
        },
        {
          "id": "MDRW2100010182.1.35",
          "form": "오늘 저녁은...",
          "original_form": "오늘 저녁은...",
          "speaker_id": "1",
          "time": "20210823 15:32"
        },
        {
          "id": "MDRW2100010182.1.36",
          "form": "오늘 저녁은??",
          "original_form": "오늘 저녁은??",
          "speaker_id": "2",
          "time": "20210823 15:32"
        },
        {
          "id": "MDRW2100010182.1.37",
          "form": "마라탕과 마라샹궈와     꿔바로우와 소주",
          "original_form": "마라탕과 마라샹궈와     꿔바로우와 소주",
          "speaker_id": "1",
          "time": "20210823 15:32"
        },
        {
          "id": "MDRW2100010182.1.38",
          "form": "소주는 안돼!",
          "original_form": "소주는 안돼!",
          "speaker_id": "2",
          "time": "20210823 15:32"
        },
        {
          "id": "MDRW2100010182.1.39",
          "form": "안되긴 뭐가 안돼!!",
          "original_form": "안되긴 뭐가 안돼!!",
          "speaker_id": "1",
          "time": "20210823 15:32"
        },
        {
          "id": "MDRW2100010182.1.40",
          "form": "나랑 약속했잖소\r\n말이 자꾸만 바껴",
          "original_form": "나랑 약속했잖소\r\n말이 자꾸만 바껴",
          "speaker_id": "2",
          "time": "20210823 15:33"
        },
        {
          "id": "MDRW2100010182.1.41",
          "form": "아니되오",
          "original_form": "아니되오",
          "speaker_id": "2",
          "time": "20210823 15:33"
        },
        {
          "id": "MDRW2100010182.1.42",
          "form": "에휴~",
          "original_form": "에휴~",
          "speaker_id": "1",
          "time": "20210823 15:37"
        },
        {
          "id": "MDRW2100010182.1.43",
          "form": "한숨쉴게 아니요.\r\n금.토.일 삼일 연장 술 마셨잖소!",
          "original_form": "한숨쉴게 아니요.\r\n금.토.일 삼일 연장 술 마셨잖소!",
          "speaker_id": "2",
          "time": "20210823 15:38"
        },
        {
          "id": "MDRW2100010182.1.44",
          "form": "안먹어! 안먹어!!",
          "original_form": "안먹어! 안먹어!!",
          "speaker_id": "1",
          "time": "20210823 15:39"
        },
        {
          "id": "MDRW2100010182.1.45",
          "form": "저녁 굶어 ㅋ",
          "original_form": "저녁 굶어 ㅋ",
          "speaker_id": "1",
          "time": "20210823 15:39"
        },
        {
          "id": "MDRW2100010182.1.46",
          "form": "ㅋㅋ  난 아니되오!\r\n벌써 배가 고파질려고 하고있엉",
          "original_form": "ㅋㅋ  난 아니되오!\r\n벌써 배가 고파질려고 하고있엉",
          "speaker_id": "2",
          "time": "20210823 15:42"
        },
        {
          "id": "MDRW2100010182.1.47",
          "form": "간식 없어?",
          "original_form": "간식 없어?",
          "speaker_id": "1",
          "time": "20210823 15:44"
        },
        {
          "id": "MDRW2100010182.1.48",
          "form": "간식 없지요",
          "original_form": "간식 없지요",
          "speaker_id": "2",
          "time": "20210823 15:45"
        },
        {
          "id": "MDRW2100010182.1.49",
          "form": "지금 배고픈건 아니야",
          "original_form": "지금 배고픈건 아니야",
          "speaker_id": "2",
          "time": "20210823 15:45"
        },
        {
          "id": "MDRW2100010182.1.50",
          "form": "그럼굶어야짘ㅋ",
          "original_form": "그럼굶어야짘ㅋ",
          "speaker_id": "1",
          "time": "20210823 15:45"
        },
        {
          "id": "MDRW2100010182.1.51",
          "form": "휴게소 음식이 자꾸 아른거려ㅋ",
          "original_form": "휴게소 음식이 자꾸 아른거려ㅋ",
          "speaker_id": "2",
          "time": "20210823 15:46"
        },
        {
          "id": "MDRW2100010182.1.52",
          "form": "맛녀석 휴유증인가ㅋ",
          "original_form": "맛녀석 휴유증인가ㅋ",
          "speaker_id": "2",
          "time": "20210823 15:47"
        },
        {
          "id": "MDRW2100010182.1.53",
          "form": "휴게소도 맛집이지 ㅋㅋ",
          "original_form": "휴게소도 맛집이지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 19:17"
        },
        {
          "id": "MDRW2100010182.1.54",
          "form": "자기 지금 가장 먹고싶은게 모야?",
          "original_form": "자기 지금 가장 먹고싶은게 모야?",
          "speaker_id": "2",
          "time": "20210823 19:58"
        },
        {
          "id": "MDRW2100010182.1.55",
          "form": "술??? ㅋㅋ",
          "original_form": "술??? ㅋㅋ",
          "speaker_id": "1",
