      "utterance": [
        {
          "id": "MDRW2100034321.1.1",
          "form": "주말인데",
          "original_form": "주말인데",
          "speaker_id": "2",
          "time": "20210828 12:26"
        },
        {
          "id": "MDRW2100034321.1.2",
          "form": "오늘 드라마 이렁 거 뭐 볼겅가요?",
          "original_form": "오늘 드라마 이렁 거 뭐 볼겅가요?",
          "speaker_id": "2",
          "time": "20210828 12:26"
        },
        {
          "id": "MDRW2100034321.1.3",
          "form": "주말 드라마는 보는게 없어요",
          "original_form": "주말 드라마는 보는게 없어요",
          "speaker_id": "1",
          "time": "20210828 12:28"
        },
        {
          "id": "MDRW2100034321.1.4",
          "form": "재밌는거 하나요?",
          "original_form": "재밌는거 하나요?",
          "speaker_id": "1",
          "time": "20210828 12:28"
        },
        {
          "id": "MDRW2100034321.1.5",
          "form": "아뇨 전 그냥 재방으로",
          "original_form": "아뇨 전 그냥 재방으로",
          "speaker_id": "2",
          "time": "20210828 12:29"
        },
        {
          "id": "MDRW2100034321.1.6",
          "form": "여러개 챙겨보거든요ㅋㅋ",
          "original_form": "여러개 챙겨보거든요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:29"
        },
        {
          "id": "MDRW2100034321.1.7",
          "form": "그래요? 요새 잘 보는거 있어요?",
          "original_form": "그래요? 요새 잘 보는거 있어요?",
          "speaker_id": "1",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100034321.1.8",
          "form": "전 요즘",
          "original_form": "전 요즘",
          "speaker_id": "2",
          "time": "20210828 12:48"
        },
        {
          "id": "MDRW2100034321.1.9",
          "form": "나는.솔로",
          "original_form": "나는.솔로",
          "speaker_id": "2",
          "time": "20210828 12:48"
        },
        {
          "id": "MDRW2100034321.1.10",
          "form": "이 예능 보고있어요ㅋㅋ",
          "original_form": "이 예능 보고있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:48"
        },
        {
          "id": "MDRW2100034321.1.11",
          "form": "로맨스 예능이죠?",
          "original_form": "로맨스 예능이죠?",
          "speaker_id": "1",
          "time": "20210828 12:50"
        },
        {
          "id": "MDRW2100034321.1.12",
          "form": "ㅋㅋㅋㅋㅋ 네.. 맞아요",
          "original_form": "ㅋㅋㅋㅋㅋ 네.. 맞아요",
          "speaker_id": "2",
          "time": "20210828 13:58"
        },
        {
          "id": "MDRW2100034321.1.13",
          "form": "본적이없어서ㅋㅋㅋ 예전에는 엠씨들 호들갑 떠는 맛으로 봤는데",
          "original_form": "본적이없어서ㅋㅋㅋ 예전에는 엠씨들 호들갑 떠는 맛으로 봤는데",
          "speaker_id": "1",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100034321.1.14",
          "form": "요샌 그런거 없죠?",
          "original_form": "요샌 그런거 없죠?",
          "speaker_id": "1",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100034321.1.15",
          "form": "ㅋㅋㅋㅋㅋ조금 그렇긴해여",
          "original_form": "ㅋㅋㅋㅋㅋ조금 그렇긴해여",
          "speaker_id": "2",
          "time": "20210828 14:02"
        },
        {
          "id": "MDRW2100034321.1.16",
          "form": "전 요새 놀면뭐하니 잘 보고있어요",
          "original_form": "전 요새 놀면뭐하니 잘 보고있어요",
          "speaker_id": "1",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100034321.1.17",
          "form": "ㅎ 아 거기에",
          "original_form": "ㅎ 아 거기에",
          "speaker_id": "2",
          "time": "20210828 14:40"
        },
        {
          "id": "MDRW2100034321.1.18",
          "form": "무도 멤버들 나오죠?",
          "original_form": "무도 멤버들 나오죠?",
          "speaker_id": "2",
          "time": "20210828 14:40"
        },
        {
          "id": "MDRW2100034321.1.19",
          "form": "네 요번에 무도 멤버들 나왔어요",
          "original_form": "네 요번에 무도 멤버들 나왔어요",
          "speaker_id": "1",
          "time": "20210828 14:53"
        },
        {
          "id": "MDRW2100034321.1.20",
          "form": "ㅋㅋㅋㅋ재밌죠",
          "original_form": "ㅋㅋㅋㅋ재밌죠",
          "speaker_id": "2",
          "time": "20210828 14:56"
        },
        {
          "id": "MDRW2100034321.1.21",
          "form": "네 재밌더라구요ㅋㅋㅋㅋ",
          "original_form": "네 재밌더라구요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:57"
        },
        {
          "id": "MDRW2100034321.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ 다시 무도느낌 나더라구요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 다시 무도느낌 나더라구요",
