      "utterance": [
        {
          "id": "MDRW2100023963.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210830 20:39"
        },
        {
          "id": "MDRW2100023963.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210830 20:46"
        },
        {
          "id": "MDRW2100023963.1.3",
          "form": "식사하셨나요?",
          "original_form": "식사하셨나요?",
          "speaker_id": "2",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100023963.1.4",
          "form": "네 name2님은 드셨나요??",
          "original_form": "네 &name2&님은 드셨나요??",
          "speaker_id": "1",
          "time": "20210830 20:48"
        },
        {
          "id": "MDRW2100023963.1.5",
          "form": "네, 적당히 만두를 반찬삼아서요",
          "original_form": "네, 적당히 만두를 반찬삼아서요",
          "speaker_id": "2",
          "time": "20210830 20:50"
        },
        {
          "id": "MDRW2100023963.1.6",
          "form": "name1님은 뭐 드셨나요?",
          "original_form": "&name1&님은 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210830 20:50"
        },
        {
          "id": "MDRW2100023963.1.7",
          "form": "저는 김밥 먹었어요 어떤 만두 드셨나요??",
          "original_form": "저는 김밥 먹었어요 어떤 만두 드셨나요??",
          "speaker_id": "1",
          "time": "20210830 20:51"
        },
        {
          "id": "MDRW2100023963.1.8",
          "form": "편의점 냉동만두였어요",
          "original_form": "편의점 냉동만두였어요",
          "speaker_id": "2",
          "time": "20210830 20:53"
        },
        {
          "id": "MDRW2100023963.1.9",
          "form": "백종원껀데",
          "original_form": "백종원껀데",
          "speaker_id": "2",
          "time": "20210830 20:53"
        },
        {
          "id": "MDRW2100023963.1.10",
          "form": "해물만두였던가",
          "original_form": "해물만두였던가",
          "speaker_id": "2",
          "time": "20210830 20:53"
        },
        {
          "id": "MDRW2100023963.1.11",
          "form": "새우 들어간건가요??",
          "original_form": "새우 들어간건가요??",
          "speaker_id": "1",
          "time": "20210830 20:54"
        },
        {
          "id": "MDRW2100023963.1.12",
          "form": "네, 다져서 들어가있었어요",
          "original_form": "네, 다져서 들어가있었어요",
          "speaker_id": "2",
          "time": "20210830 20:57"
        },
        {
          "id": "MDRW2100023963.1.13",
          "form": "좋아하는 음식 있으신가요?",
          "original_form": "좋아하는 음식 있으신가요?",
          "speaker_id": "1",
          "time": "20210830 20:58"
        },
        {
          "id": "MDRW2100023963.1.14",
          "form": "만두 좋아하고",
          "original_form": "만두 좋아하고",
          "speaker_id": "2",
          "time": "20210830 21:00"
        },
        {
          "id": "MDRW2100023963.1.15",
          "form": "김치전, 김치찌개랑",
          "original_form": "김치전, 김치찌개랑",
          "speaker_id": "2",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100023963.1.16",
          "form": "치킨 좋아해요",
          "original_form": "치킨 좋아해요",
          "speaker_id": "2",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100023963.1.17",
          "form": "치킨 좋아하시는 브랜드 있으신가요??",
          "original_form": "치킨 좋아하시는 브랜드 있으신가요??",
          "speaker_id": "1",
          "time": "20210830 21:02"
        },
        {
          "id": "MDRW2100023963.1.18",
          "form": "전 후라이드파라서요",
          "original_form": "전 후라이드파라서요",
          "speaker_id": "2",
          "time": "20210830 21:03"
        },
        {
          "id": "MDRW2100023963.1.19",
          "form": "그 황금올리브.,",
          "original_form": "그 황금올리브.,",
          "speaker_id": "2",
          "time": "20210830 21:04"
        },
        {
          "id": "MDRW2100023963.1.20",
          "form": "비싸지만 가끔 사먹으면 돈값하는 그 치킨...",
          "original_form": "비싸지만 가끔 사먹으면 돈값하는 그 치킨...",
          "speaker_id": "2",
          "time": "20210830 21:04"
        },
        {
          "id": "MDRW2100023963.1.21",
          "form": "오 그러시군요 황금올리브 맛있죠",
          "original_form": "오 그러시군요 황금올리브 맛있죠",
          "speaker_id": "1",
          "time": "20210830 21:05"
        }
      ]
