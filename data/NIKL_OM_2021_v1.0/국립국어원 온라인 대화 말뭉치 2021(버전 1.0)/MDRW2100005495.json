      "utterance": [
        {
          "id": "MDRW2100005495.1.1",
          "form": "최근에 산거중에 맘에드는 물건 있으세요?",
          "original_form": "최근에 산거중에 맘에드는 물건 있으세요?",
          "speaker_id": "1",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005495.1.2",
          "form": "문어인형이욥 ㅎㅎ",
          "original_form": "문어인형이욥 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:43"
        },
        {
          "id": "MDRW2100005495.1.3",
          "form": "혹시 뒤집으면 표정바뀌는 그건가요?",
          "original_form": "혹시 뒤집으면 표정바뀌는 그건가요?",
          "speaker_id": "1",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005495.1.4",
          "form": "네!!! 맞아요",
          "original_form": "네!!! 맞아요",
          "speaker_id": "2",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005495.1.5",
          "form": "그거 너무 귀엽던데 유행인가요??",
          "original_form": "그거 너무 귀엽던데 유행인가요??",
          "speaker_id": "1",
          "time": "20210821 22:56"
        },
        {
          "id": "MDRW2100005495.1.6",
          "form": "조금 유명해져서 유행처럼 되긴 했죠??",
          "original_form": "조금 유명해져서 유행처럼 되긴 했죠??",
          "speaker_id": "2",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005495.1.7",
          "form": "저는 원래 살 생각 없었는데",
          "original_form": "저는 원래 살 생각 없었는데",
          "speaker_id": "2",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005495.1.8",
          "form": "뒤집는 것 때문에 두가지 색인디",
          "original_form": "뒤집는 것 때문에 두가지 색인디",
          "speaker_id": "2",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005495.1.9",
          "form": "색 둘 다 맘에 드는 거 발견하자마자",
          "original_form": "색 둘 다 맘에 드는 거 발견하자마자",
          "speaker_id": "2",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005495.1.10",
          "form": "살 수밖에 없었어요ㅠ",
          "original_form": "살 수밖에 없었어요ㅠ",
          "speaker_id": "2",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005495.1.11",
          "form": "아 저는 분홍파랑밖에 못봤는데",
          "original_form": "아 저는 분홍파랑밖에 못봤는데",
          "speaker_id": "1",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005495.1.12",
          "form": "다른색도 있나봐요??",
          "original_form": "다른색도 있나봐요??",
          "speaker_id": "1",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005495.1.13",
          "form": "네네 색 조합이 꽤나 다양하더라고요",
          "original_form": "네네 색 조합이 꽤나 다양하더라고요",
          "speaker_id": "2",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005495.1.14",
          "form": "인터넷에서는 분홍파랑 분홍초록 막 이랬는데",
          "original_form": "인터넷에서는 분홍파랑 분홍초록 막 이랬는데",
          "speaker_id": "2",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005495.1.15",
          "form": "하늘파랑 보자마자",
          "original_form": "하늘파랑 보자마자",
          "speaker_id": "2",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005495.1.16",
          "form": "문어가 사라고 외치는 거 같아서 샀어욬ㅋㅋㅋ",
          "original_form": "문어가 사라고 외치는 거 같아서 샀어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005495.1.17",
          "form": "ㅋㅋㅋㅋ그건 사서 가방에 매고 다니나요?",
          "original_form": "ㅋㅋㅋㅋ그건 사서 가방에 매고 다니나요?",
          "speaker_id": "1",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005495.1.18",
          "form": "네ㅋㅋㅋㅋ 파란 포인트 있는 에코백에 달고 다녀요",
          "original_form": "네ㅋㅋㅋㅋ 파란 포인트 있는 에코백에 달고 다녀요",
          "speaker_id": "2",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005495.1.19",
          "form": "문어 단 뒤로 더 애착 가서 맨날 들고 다녀요 ㅋㅋㅋ",
          "original_form": "문어 단 뒤로 더 애착 가서 맨날 들고 다녀요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005495.1.20",
          "form": "에코백하나면 모든 옷에 맬 수 있어서 좋죠 ㅎㅎ",
          "original_form": "에코백하나면 모든 옷에 맬 수 있어서 좋죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005495.1.21",
          "form": "그런 문어는 하나에 가격이 얼마나하나요?",
          "original_form": "그런 문어는 하나에 가격이 얼마나하나요?",
          "speaker_id": "1",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005495.1.22",
          "form": "인터넷 보면",
          "original_form": "인터넷 보면",
          "speaker_id": "2",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005495.1.23",
          "form": "싼 건 2000원 정도에도 파는데",
          "original_form": "싼 건 2000원 정도에도 파는데",
          "speaker_id": "2",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005495.1.24",
          "form": "저는 교보문고에서 사서 조금 비쌌어요ㅠ 5000원.........",
          "original_form": "저는 교보문고에서 사서 조금 비쌌어요ㅠ 5000원.........",
          "speaker_id": "2",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005495.1.25",
          "form": "그래도 특이한 문어면 그정도 가치는 있는 것 같아요!",
          "original_form": "그래도 특이한 문어면 그정도 가치는 있는 것 같아요!",
          "speaker_id": "1",
          "time": "20210821 23:26"
        },
        {
          "id": "MDRW2100005495.1.26",
          "form": "인터넷은 배송비도 드니까요",
          "original_form": "인터넷은 배송비도 드니까요",
          "speaker_id": "1",
          "time": "20210821 23:26"
        }
      ]
