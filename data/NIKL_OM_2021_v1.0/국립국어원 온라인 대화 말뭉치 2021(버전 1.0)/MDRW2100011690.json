      "utterance": [
        {
          "id": "MDRW2100011690.1.1",
          "form": "게임 좋아하시나요",
          "original_form": "게임 좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 10:11"
        },
        {
          "id": "MDRW2100011690.1.2",
          "form": "네 게임 좋아해요!",
          "original_form": "네 게임 좋아해요!",
          "speaker_id": "2",
          "time": "20210825 10:15"
        },
        {
          "id": "MDRW2100011690.1.3",
          "form": "어떤 게임 하시나요",
          "original_form": "어떤 게임 하시나요",
          "speaker_id": "1",
          "time": "20210825 10:18"
        },
        {
          "id": "MDRW2100011690.1.4",
          "form": "배틀그라운드 롤 오버워치 이터널 리턴이요!",
          "original_form": "배틀그라운드 롤 오버워치 이터널 리턴이요!",
          "speaker_id": "2",
          "time": "20210825 11:26"
        },
        {
          "id": "MDRW2100011690.1.5",
          "form": "오 배그 예전에 했었어요",
          "original_form": "오 배그 예전에 했었어요",
          "speaker_id": "1",
          "time": "20210825 11:26"
        },
        {
          "id": "MDRW2100011690.1.6",
          "form": "이제 얼리억세스랑 초반이",
          "original_form": "이제 얼리억세스랑 초반이",
          "speaker_id": "2",
          "time": "20210825 11:42"
        },
        {
          "id": "MDRW2100011690.1.7",
          "form": "가장 재밌긴했었죠",
          "original_form": "가장 재밌긴했었죠",
          "speaker_id": "2",
          "time": "20210825 11:42"
        },
        {
          "id": "MDRW2100011690.1.8",
          "form": "지금도 잘나가나요 배그?",
          "original_form": "지금도 잘나가나요 배그?",
          "speaker_id": "1",
          "time": "20210825 11:45"
        },
        {
          "id": "MDRW2100011690.1.9",
          "form": "이번에 블랙핑크랑 콜라보도 하고",
          "original_form": "이번에 블랙핑크랑 콜라보도 하고",
          "speaker_id": "2",
          "time": "20210825 11:46"
        },
        {
          "id": "MDRW2100011690.1.10",
          "form": "마동석도 나오고 손흥민도 했긴한데",
          "original_form": "마동석도 나오고 손흥민도 했긴한데",
          "speaker_id": "2",
          "time": "20210825 11:46"
        },
        {
          "id": "MDRW2100011690.1.11",
          "form": "우선 옛날만큼은 잘나가지는 않습니다 ㅋㅋ",
          "original_form": "우선 옛날만큼은 잘나가지는 않습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:47"
        },
        {
          "id": "MDRW2100011690.1.12",
          "form": "와 손흥민대박",
          "original_form": "와 손흥민대박",
          "speaker_id": "1",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100011690.1.13",
          "form": "콜라보랑 여러 부분에선 노력하더라고요",
          "original_form": "콜라보랑 여러 부분에선 노력하더라고요",
          "speaker_id": "2",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100011690.1.14",
          "form": "진즉에 좀 할것이지 ㅠㅠ",
          "original_form": "진즉에 좀 할것이지 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100011690.1.15",
          "form": "크래프톤 상장도햇자나요ㅎㅎ",
          "original_form": "크래프톤 상장도햇자나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 12:25"
        },
        {
          "id": "MDRW2100011690.1.16",
          "form": "그쵸 상장도 했고",
          "original_form": "그쵸 상장도 했고",
          "speaker_id": "2",
          "time": "20210825 12:43"
        },
        {
          "id": "MDRW2100011690.1.17",
          "form": "배그리그를 좀 살리면 어떨까싶기도 하네요",
          "original_form": "배그리그를 좀 살리면 어떨까싶기도 하네요",
          "speaker_id": "2",
          "time": "20210825 12:43"
        },
        {
          "id": "MDRW2100011690.1.18",
          "form": "맞아요 리그",
          "original_form": "맞아요 리그",
