      "utterance": [
        {
          "id": "MDRW2100013809.1.1",
          "form": "됐다 된장에 밥 먹어야지",
          "original_form": "됐다 된장에 밥 먹어야지",
          "speaker_id": "2",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100013809.1.2",
          "form": "아 글쿤",
          "original_form": "아 글쿤",
          "speaker_id": "1",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100013809.1.3",
          "form": "그래 나중에 답해라",
          "original_form": "그래 나중에 답해라",
          "speaker_id": "2",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100013809.1.4",
          "form": "그럼 계란말이해줄겡",
          "original_form": "그럼 계란말이해줄겡",
          "speaker_id": "1",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100013809.1.5",
          "form": "맛있겠네",
          "original_form": "맛있겠네",
          "speaker_id": "2",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100013809.1.6",
          "form": "웅웅 ㅋ 국이랑 밥 먹을때 계란말이가 굿",
          "original_form": "웅웅 ㅋ 국이랑 밥 먹을때 계란말이가 굿",
          "speaker_id": "1",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100013809.1.7",
          "form": "근데 번거롭다",
          "original_form": "근데 번거롭다",
          "speaker_id": "2",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100013809.1.8",
          "form": "아니당 금방할수있다",
          "original_form": "아니당 금방할수있다",
          "speaker_id": "1",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100013809.1.9",
          "form": "그러면 하나 만들어 보든지",
          "original_form": "그러면 하나 만들어 보든지",
          "speaker_id": "2",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100013809.1.10",
          "form": "알았어용 ㅋ 좀따 해줄게 계란 있나용?",
          "original_form": "알았어용 ㅋ 좀따 해줄게 계란 있나용?",
          "speaker_id": "1",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100013809.1.11",
          "form": "계란 많이 있다",
          "original_form": "계란 많이 있다",
          "speaker_id": "2",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100013809.1.12",
          "form": "오키 ㅋ 그럼 많이 만들어야겠군",
