      "utterance": [
        {
          "id": "MDRW2100025099.1.1",
          "form": "ㅋㅋㅋㅋㅋ아 왜 작은 솥이에여!!!!",
          "original_form": "ㅋㅋㅋㅋㅋ아 왜 작은 솥이에여!!!!",
          "speaker_id": "1",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100025099.1.2",
          "form": "믿어보세여!!!",
          "original_form": "믿어보세여!!!",
          "speaker_id": "1",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100025099.1.3",
          "form": "ㅋㅋㄱㅋㅋㅋㅋㅋ큰솥에했다가",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋㅋ큰솥에했다가",
          "speaker_id": "2",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100025099.1.4",
          "form": "취향을타면 처리를할수가....",
          "original_form": "취향을타면 처리를할수가....",
          "speaker_id": "2",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100025099.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100025099.1.6",
          "form": "하 카레하니까",
          "original_form": "하 카레하니까",
          "speaker_id": "1",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100025099.1.7",
          "form": "누텔라커리 쳐봐야겠네요",
          "original_form": "누텔라커리 쳐봐야겠네요",
          "speaker_id": "2",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100025099.1.8",
          "form": "카레가루 바르고 구운 갈치가 먹고싶네여",
          "original_form": "카레가루 바르고 구운 갈치가 먹고싶네여",
          "speaker_id": "1",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100025099.1.9",
          "form": "오....",
          "original_form": "오....",
          "speaker_id": "2",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100025099.1.10",
          "form": "저도 생선에카레가루 바른거 좋아해요",
          "original_form": "저도 생선에카레가루 바른거 좋아해요",
          "speaker_id": "2",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100025099.1.11",
          "form": "헉 그쵸",
          "original_form": "헉 그쵸",
          "speaker_id": "1",
          "time": "20210831 23:07"
        },
        {
          "id": "MDRW2100025099.1.12",
          "form": "역시 배우신분...",
          "original_form": "역시 배우신분...",
          "speaker_id": "2",
          "time": "20210831 23:07"
        },
        {
          "id": "MDRW2100025099.1.13",
          "form": "근데 왜 급식때 친구들은 싫어했을까요",
          "original_form": "근데 왜 급식때 친구들은 싫어했을까요",
          "speaker_id": "1",
          "time": "20210831 23:07"
        },
        {
          "id": "MDRW2100025099.1.14",
          "form": "바보들 히히",
          "original_form": "바보들 히히",
          "speaker_id": "1",
          "time": "20210831 23:07"
        },
        {
          "id": "MDRW2100025099.1.15",
          "form": "그러게요...전 카레가루 입혀 튀긴 생선나오는날",
          "original_form": "그러게요...전 카레가루 입혀 튀긴 생선나오는날",
          "speaker_id": "2",
          "time": "20210831 23:07"
        },
        {
          "id": "MDRW2100025099.1.16",
          "form": "제일맛있게먹었는데..",
          "original_form": "제일맛있게먹었는데..",
          "speaker_id": "2",
          "time": "20210831 23:07"
        },
        {
          "id": "MDRW2100025099.1.17",
          "form": "그쳐",
          "original_form": "그쳐",
          "speaker_id": "1",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100025099.1.18",
          "form": "혹시 그럼",
          "original_form": "혹시 그럼",
          "speaker_id": "1",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100025099.1.19",
          "form": "생선까스도 좋아하셨나요?",
          "original_form": "생선까스도 좋아하셨나요?",
          "speaker_id": "1",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100025099.1.20",
          "form": "엄첨좋아하죠",
          "original_form": "엄첨좋아하죠",
          "speaker_id": "2",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100025099.1.21",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100025099.1.22",
          "form": "ㅋㅋㄱㅋㅋㄱㄱ 저희가족 다 생까러버...",
          "original_form": "ㅋㅋㄱㅋㅋㄱㄱ 저희가족 다 생까러버...",
          "speaker_id": "2",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100025099.1.23",
          "form": "그럼 코다리찜은?",
          "original_form": "그럼 코다리찜은?",
          "speaker_id": "1",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100025099.1.24",
          "form": "코다리찜은 저희가 많이안나와가지고",
          "original_form": "코다리찜은 저희가 많이안나와가지고",
          "speaker_id": "2",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100025099.1.25",
          "form": "무난무난했었어용",
          "original_form": "무난무난했었어용",
          "speaker_id": "2",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100025099.1.26",
          "form": "회사에 자주가는 백반집이",
          "original_form": "회사에 자주가는 백반집이",
          "speaker_id": "2",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100025099.1.27",
          "form": "앗 전 그건 시러햇어요ㅠ",
          "original_form": "앗 전 그건 시러햇어요ㅠ",
          "speaker_id": "1",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100025099.1.28",
          "form": "코다리강정 이런 거..",
          "original_form": "코다리강정 이런 거..",
          "speaker_id": "1",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100025099.1.29",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100025099.1.30",
          "form": "생까자주나와서",
          "original_form": "생까자주나와서",
          "speaker_id": "2",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100025099.1.31",
          "form": "매우좋습니다",
          "original_form": "매우좋습니다",
          "speaker_id": "2",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100025099.1.32",
          "form": "오늘도사실생까였죠",
          "original_form": "오늘도사실생까였죠",
          "speaker_id": "2",
          "time": "20210831 23:09"
        },
        {
