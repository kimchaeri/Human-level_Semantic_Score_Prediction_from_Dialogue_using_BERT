      "utterance": [
        {
          "id": "MDRW2100009970.1.1",
          "form": "집에서도 커피 내려드세요?",
          "original_form": "집에서도 커피 내려드세요?",
          "speaker_id": "1",
          "time": "20210823 12:30"
        },
        {
          "id": "MDRW2100009970.1.2",
          "form": "아니요 저는 커피는 안마셔요",
          "original_form": "아니요 저는 커피는 안마셔요",
          "speaker_id": "2",
          "time": "20210823 12:34"
        },
        {
          "id": "MDRW2100009970.1.3",
          "form": "커피 집에서 내려드시나요?",
          "original_form": "커피 집에서 내려드시나요?",
          "speaker_id": "2",
          "time": "20210823 12:34"
        },
        {
          "id": "MDRW2100009970.1.4",
          "form": "네 커피에 맛들려서요",
          "original_form": "네 커피에 맛들려서요",
          "speaker_id": "1",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100009970.1.5",
          "form": "아무래도 카페의 시대이다보니까 물들었어요ㅋ",
          "original_form": "아무래도 카페의 시대이다보니까 물들었어요ㅋ",
          "speaker_id": "1",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100009970.1.6",
          "form": "ㅋㅋㅋㅋㅋ 집에서 내리면",
          "original_form": "ㅋㅋㅋㅋㅋ 집에서 내리면",
          "speaker_id": "2",
          "time": "20210823 13:49"
        },
        {
          "id": "MDRW2100009970.1.7",
          "form": "향기도 좋겠어요 ㅠㅠ",
          "original_form": "향기도 좋겠어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 13:49"
        },
        {
          "id": "MDRW2100009970.1.8",
          "form": "네 그래서 귀찮아도 갈지만",
          "original_form": "네 그래서 귀찮아도 갈지만",
          "speaker_id": "1",
          "time": "20210823 13:53"
        },
        {
          "id": "MDRW2100009970.1.9",
          "form": "역시 시간이 지나면 확 달라져요",
          "original_form": "역시 시간이 지나면 확 달라져요",
          "speaker_id": "1",
          "time": "20210823 13:53"
        },
        {
          "id": "MDRW2100009970.1.10",
          "form": "ㅋㅋㅋㅋㅋ 뭔가 여유로울 거 같아요",
          "original_form": "ㅋㅋㅋㅋㅋ 뭔가 여유로울 거 같아요",
          "speaker_id": "2",
          "time": "20210823 14:30"
        },
        {
          "id": "MDRW2100009970.1.11",
          "form": "네 그래도 갈 때 기분은 좋아요",
          "original_form": "네 그래도 갈 때 기분은 좋아요",
          "speaker_id": "1",
          "time": "20210823 16:03"
        },
        {
          "id": "MDRW2100009970.1.12",
          "form": "무슨 음식 좋아하세요?",
          "original_form": "무슨 음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 16:03"
        },
        {
          "id": "MDRW2100009970.1.13",
          "form": "저 떡볶이 마라탕.. 이런거 좋아해료",
          "original_form": "저 떡볶이 마라탕.. 이런거 좋아해료",
          "speaker_id": "2",
          "time": "20210823 16:22"
        },
        {
          "id": "MDRW2100009970.1.14",
          "form": "뭐 좋아하세요?",
          "original_form": "뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 16:22"
        },
        {
          "id": "MDRW2100009970.1.15",
          "form": "와 매코미시네요",
          "original_form": "와 매코미시네요",
          "speaker_id": "1",
          "time": "20210823 16:49"
        },
        {
          "id": "MDRW2100009970.1.16",
          "form": "저도 떡볶이 마라탕 극호",
          "original_form": "저도 떡볶이 마라탕 극호",
          "speaker_id": "1",
          "time": "20210823 16:49"
        },
        {
          "id": "MDRW2100009970.1.17",
          "form": "헐 ㅎ 저랑 통하시군요",
          "original_form": "헐 ㅎ 저랑 통하시군요",
          "speaker_id": "2",
          "time": "20210823 17:34"
        },
        {
          "id": "MDRW2100009970.1.18",
          "form": "저거 진짜 맛있어요",
          "original_form": "저거 진짜 맛있어요",
          "speaker_id": "2",
          "time": "20210823 17:34"
        },
        {
          "id": "MDRW2100009970.1.19",
          "form": "마라탕은 중독성이 강해서ㅋㅋ",
          "original_form": "마라탕은 중독성이 강해서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:55"
        },
        {
          "id": "MDRW2100009970.1.20",
          "form": "마라탕에 그 건두부 맛있어요",
          "original_form": "마라탕에 그 건두부 맛있어요",
          "speaker_id": "1",
          "time": "20210824 11:56"
        },
        {
          "id": "MDRW2100009970.1.21",
          "form": "맞아요... 푸주랑 정말 사랑이에요 ㅠ",
          "original_form": "맞아요... 푸주랑 정말 사랑이에요 ㅠ",
          "speaker_id": "2",
          "time": "20210824 12:08"
        },
        {
          "id": "MDRW2100009970.1.22",
          "form": "아 푸주!!!",
          "original_form": "아 푸주!!!",
          "speaker_id": "1",
          "time": "20210824 12:08"
        },
        {
          "id": "MDRW2100009970.1.23",
          "form": "집에서 만들어도 그 맛이 나려나",
          "original_form": "집에서 만들어도 그 맛이 나려나",
          "speaker_id": "1",
          "time": "20210824 12:09"
        },
        {
          "id": "MDRW2100009970.1.24",
          "form": "저 가꿈 만들어 먹는데",
          "original_form": "저 가꿈 만들어 먹는데",
          "speaker_id": "2",
          "time": "20210824 12:41"
        },
        {
          "id": "MDRW2100009970.1.25",
          "form": "맛있더라구요",
          "original_form": "맛있더라구요",
          "speaker_id": "2",
          "time": "20210824 12:41"
        },
        {
          "id": "MDRW2100009970.1.26",
          "form": "와 대단하시네요!",
          "original_form": "와 대단하시네요!",
          "speaker_id": "1",
          "time": "20210824 14:40"
        }
      ]
    }
