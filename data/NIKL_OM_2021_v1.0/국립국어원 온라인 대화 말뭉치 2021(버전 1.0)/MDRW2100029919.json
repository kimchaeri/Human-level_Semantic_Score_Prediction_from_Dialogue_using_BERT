      "utterance": [
        {
          "id": "MDRW2100029919.1.1",
          "form": "네ㅋㅋ사실 네시부터 준비다해노코 10분전에 대기타고잇어요ㅋㅋ",
          "original_form": "네ㅋㅋ사실 네시부터 준비다해노코 10분전에 대기타고잇어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:33"
        },
        {
          "id": "MDRW2100029919.1.2",
          "form": "오 퇴근하면",
          "original_form": "오 퇴근하면",
          "speaker_id": "1",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100029919.1.3",
          "form": "바로 집으로?",
          "original_form": "바로 집으로?",
          "speaker_id": "1",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100029919.1.4",
          "form": "네ㅋㅋ칼퇴",
          "original_form": "네ㅋㅋ칼퇴",
          "speaker_id": "2",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100029919.1.5",
          "form": "십분걷고",
          "original_form": "십분걷고",
          "speaker_id": "2",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100029919.1.6",
          "form": "집이에여",
          "original_form": "집이에여",
          "speaker_id": "2",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100029919.1.7",
          "form": "근데도 숨쉬기운동하는삶",
          "original_form": "근데도 숨쉬기운동하는삶",
          "speaker_id": "2",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100029919.1.8",
          "form": "겁나게으르네여",
          "original_form": "겁나게으르네여",
          "speaker_id": "2",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100029919.1.9",
          "form": "가깝네요",
          "original_form": "가깝네요",
          "speaker_id": "1",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100029919.1.10",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100029919.1.11",
          "form": "병원근처 집이네요?",
          "original_form": "병원근처 집이네요?",
          "speaker_id": "1",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100029919.1.12",
          "form": "네ㅋㅋ",
          "original_form": "네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100029919.1.13",
          "form": "출퇴근",
          "original_form": "출퇴근",
          "speaker_id": "2",
          "time": "20210903 16:36"
        },
        {
