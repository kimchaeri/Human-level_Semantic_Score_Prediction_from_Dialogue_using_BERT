          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100029919.1.15",
          "form": "얼마걸리시나요",
          "original_form": "얼마걸리시나요",
          "speaker_id": "2",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100029919.1.16",
          "form": "차로 20분입니다",
          "original_form": "차로 20분입니다",
          "speaker_id": "1",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100029919.1.17",
          "form": "바다 건나서 회사로 출근합니다",
          "original_form": "바다 건나서 회사로 출근합니다",
          "speaker_id": "1",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100029919.1.18",
          "form": "신기신기",
