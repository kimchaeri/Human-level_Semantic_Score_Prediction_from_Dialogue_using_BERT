      "utterance": [
        {
          "id": "MDRW2100014525.1.1",
          "form": "저는 워치4  샀어요",
          "original_form": "저는 워치4  샀어요",
          "speaker_id": "1",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100014525.1.2",
          "form": "오 저도 그거 사고싶어요",
          "original_form": "오 저도 그거 사고싶어요",
          "speaker_id": "2",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100014525.1.3",
          "form": "비써죠?",
          "original_form": "비써죠?",
          "speaker_id": "2",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100014525.1.4",
          "form": "기능리 뭐가 좋아요?",
          "original_form": "기능리 뭐가 좋아요?",
          "speaker_id": "2",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100014525.1.5",
          "form": "기능이",
          "original_form": "기능이",
          "speaker_id": "2",
          "time": "20210825 19:40"
        },
        {
          "id": "MDRW2100014525.1.6",
          "form": "원래 액티브2사려고 하다가",
          "original_form": "원래 액티브2사려고 하다가",
          "speaker_id": "1",
          "time": "20210825 19:41"
        },
        {
          "id": "MDRW2100014525.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 19:41"
        },
        {
          "id": "MDRW2100014525.1.8",
          "form": "4나온다고해서 기다려서가격보고 결정할랬는데요",
          "original_form": "4나온다고해서 기다려서가격보고 결정할랬는데요",
          "speaker_id": "1",
          "time": "20210825 19:41"
        },
        {
          "id": "MDRW2100014525.1.9",
          "form": "가격이 비슷하더라구요",
          "original_form": "가격이 비슷하더라구요",
          "speaker_id": "1",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100014525.1.10",
          "form": "오 그럼 새로나온거사죠",
          "original_form": "오 그럼 새로나온거사죠",
          "speaker_id": "2",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100014525.1.11",
          "form": "그리고 사전예약하니 티9만원에 카드할인하니",
          "original_form": "그리고 사전예약하니 티9만원에 카드할인하니",
          "speaker_id": "1",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100014525.1.12",
          "form": "16만원에 샀어요",
          "original_form": "16만원에 샀어요",
          "speaker_id": "1",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100014525.1.13",
          "form": "혈압은 아직 안재어지나요?",
          "original_form": "혈압은 아직 안재어지나요?",
          "speaker_id": "2",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100014525.1.14",
          "form": "네 신상! ㅎㅎ",
          "original_form": "네 신상! ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100014525.1.15",
          "form": "오 싼데요",
          "original_form": "오 싼데요",
          "speaker_id": "2",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100014525.1.16",
          "form": "체지방 측정되요",
          "original_form": "체지방 측정되요",
          "speaker_id": "1",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100014525.1.17",
          "form": "역시 사전예약이 최고입니다",
          "original_form": "역시 사전예약이 최고입니다",
          "speaker_id": "2",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100014525.1.18",
          "form": "인바디 ㅋㅋ",
          "original_form": "인바디 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100014525.1.19",
          "form": "알 모르고싶다",
          "original_form": "알 모르고싶다",
          "speaker_id": "2",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014525.1.20",
          "form": "네 사전예약이 이리좋은지 몰랐어요",
          "original_form": "네 사전예약이 이리좋은지 몰랐어요",
          "speaker_id": "1",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014525.1.21",
          "form": "최신의 위력이네요",
          "original_form": "최신의 위력이네요",
          "speaker_id": "2",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014525.1.22",
          "form": "선물 팍팍 할인 팍팍",
          "original_form": "선물 팍팍 할인 팍팍",
          "speaker_id": "2",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014525.1.23",
          "form": "다른분들은 비싼 만보기라고 ㅎㅎ",
          "original_form": "다른분들은 비싼 만보기라고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014525.1.24",
          "form": "저도 모르고 폰 비싸게 사고 선물도 못받고",
          "original_form": "저도 모르고 폰 비싸게 사고 선물도 못받고",
          "speaker_id": "2",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014525.1.25",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014525.1.26",
          "form": "전 그만보기 갖고싶어요 ㅋㅋ",
          "original_form": "전 그만보기 갖고싶어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:43"
        }
      ]
