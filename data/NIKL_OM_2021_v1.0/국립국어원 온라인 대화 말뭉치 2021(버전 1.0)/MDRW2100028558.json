      "utterance": [
        {
          "id": "MDRW2100028558.1.1",
          "form": "비와요?",
          "original_form": "비와요?",
          "speaker_id": "1",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100028558.1.2",
          "form": "사당 쪽이데 안 오네요",
          "original_form": "사당 쪽이데 안 오네요",
          "speaker_id": "2",
          "time": "20210902 20:47"
        },
        {
          "id": "MDRW2100028558.1.3",
          "form": "여기도요",
          "original_form": "여기도요",
          "speaker_id": "1",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028558.1.4",
          "form": "일단 오늘도",
          "original_form": "일단 오늘도",
          "speaker_id": "2",
          "time": "20210903 08:38"
        },
        {
          "id": "MDRW2100028558.1.5",
          "form": "안 오는거보면",
          "original_form": "안 오는거보면",
          "speaker_id": "2",
          "time": "20210903 08:38"
        },
        {
          "id": "MDRW2100028558.1.6",
          "form": "장마는 끝난 거 같네요",
          "original_form": "장마는 끝난 거 같네요",
          "speaker_id": "2",
          "time": "20210903 08:38"
        },
        {
          "id": "MDRW2100028558.1.7",
          "form": "여긴 비가",
          "original_form": "여긴 비가",
          "speaker_id": "1",
          "time": "20210903 09:32"
        },
        {
          "id": "MDRW2100028558.1.8",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "2",
          "time": "20210903 09:49"
        },
        {
          "id": "MDRW2100028558.1.9",
          "form": "솔직히 막",
          "original_form": "솔직히 막",
          "speaker_id": "2",
          "time": "20210903 09:49"
        },
        {
          "id": "MDRW2100028558.1.10",
          "form": "넓은 나라도 아닌데",
          "original_form": "넓은 나라도 아닌데",
          "speaker_id": "2",
          "time": "20210903 09:49"
        },
        {
          "id": "MDRW2100028558.1.11",
          "form": "지역별로 날씨가",
          "original_form": "지역별로 날씨가",
          "speaker_id": "2",
          "time": "20210903 09:49"
        },
        {
          "id": "MDRW2100028558.1.12",
          "form": "다르네요",
          "original_form": "다르네요",
          "speaker_id": "2",
          "time": "20210903 09:49"
        },
        {
          "id": "MDRW2100028558.1.13",
          "form": "습하죠?",
          "original_form": "습하죠?",
          "speaker_id": "1",
          "time": "20210903 09:56"
        },
        {
          "id": "MDRW2100028558.1.14",
          "form": "네...",
          "original_form": "네...",
          "speaker_id": "2",
          "time": "20210903 09:58"
        },
        {
          "id": "MDRW2100028558.1.15",
          "form": "온도는 가을 비슷한데",
          "original_form": "온도는 가을 비슷한데",
          "speaker_id": "2",
          "time": "20210903 09:58"
        },
        {
          "id": "MDRW2100028558.1.16",
          "form": "습해서...참;;",
          "original_form": "습해서...참;;",
          "speaker_id": "2",
          "time": "20210903 09:58"
        },
        {
          "id": "MDRW2100028558.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210903 10:00"
        },
        {
          "id": "MDRW2100028558.1.18",
          "form": "솔직히 습도만 낮았어도",
          "original_form": "솔직히 습도만 낮았어도",
          "speaker_id": "2",
          "time": "20210903 10:02"
        },
        {
          "id": "MDRW2100028558.1.19",
          "form": "여름이 훨씬 더 편했을텐데",
          "original_form": "여름이 훨씬 더 편했을텐데",
          "speaker_id": "2",
          "time": "20210903 10:02"
        },
        {
          "id": "MDRW2100028558.1.20",
          "form": "그래도 작년보다는",
          "original_form": "그래도 작년보다는",
          "speaker_id": "2",
          "time": "20210903 10:02"
        },
        {
          "id": "MDRW2100028558.1.21",
          "form": "비가 덜...왔나???",
          "original_form": "비가 덜...왔나???",
          "speaker_id": "2",
          "time": "20210903 10:03"
        },
        {
          "id": "MDRW2100028558.1.22",
          "form": "그런듯",
          "original_form": "그런듯",
          "speaker_id": "1",
          "time": "20210903 10:12"
        },
        {
          "id": "MDRW2100028558.1.23",
          "form": "그러면",
          "original_form": "그러면",
          "speaker_id": "2",
          "time": "20210903 12:45"
        },
        {
          "id": "MDRW2100028558.1.24",
          "form": "아직도 비오시나요",
          "original_form": "아직도 비오시나요",
