      "utterance": [
        {
          "id": "MDRW2100003421.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210820 22:54"
        },
        {
          "id": "MDRW2100003421.1.2",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 22:54"
        },
        {
          "id": "MDRW2100003421.1.3",
          "form": "네 안녕하세요 게임 좋아해요",
          "original_form": "네 안녕하세요 게임 좋아해요",
          "speaker_id": "2",
          "time": "20210820 22:55"
        },
        {
          "id": "MDRW2100003421.1.4",
          "form": "오버워치랑 배그요",
          "original_form": "오버워치랑 배그요",
          "speaker_id": "2",
          "time": "20210820 22:55"
        },
        {
          "id": "MDRW2100003421.1.5",
          "form": "다른 플스 게임은 스토리 보는거 좋아해요 ㅋㅋㅋ 유튜브로",
          "original_form": "다른 플스 게임은 스토리 보는거 좋아해요 ㅋㅋㅋ 유튜브로",
          "speaker_id": "2",
          "time": "20210820 22:55"
        },
        {
          "id": "MDRW2100003421.1.6",
          "form": "롤은 어려워서 잘 안보게돼요",
          "original_form": "롤은 어려워서 잘 안보게돼요",
          "speaker_id": "2",
          "time": "20210820 22:56"
        },
        {
          "id": "MDRW2100003421.1.7",
          "form": "아 저 모바일 배그 해봤어요!",
          "original_form": "아 저 모바일 배그 해봤어요!",
          "speaker_id": "1",
          "time": "20210820 22:56"
        },
        {
          "id": "MDRW2100003421.1.8",
          "form": "오 어땠어요??",
          "original_form": "오 어땠어요??",
          "speaker_id": "2",
          "time": "20210820 22:56"
        },
        {
          "id": "MDRW2100003421.1.9",
          "form": "저 태블릿으로 해서 적이 잘보여서 재밌더라구요ㅋㅋㅋㅋㅋ",
          "original_form": "저 태블릿으로 해서 적이 잘보여서 재밌더라구요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:56"
        },
        {
          "id": "MDRW2100003421.1.10",
          "form": "저도 해봤는데 컨트롤이 좀 어렵더라고요",
          "original_form": "저도 해봤는데 컨트롤이 좀 어렵더라고요",
          "speaker_id": "2",
          "time": "20210820 22:56"
        },
        {
          "id": "MDRW2100003421.1.11",
          "form": "오 ! 신박하네요 저도 태블릿으로 해봐야겠어요 ㅋㅋㅋㅋ",
          "original_form": "오 ! 신박하네요 저도 태블릿으로 해봐야겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:57"
        },
        {
          "id": "MDRW2100003421.1.12",
          "form": "그리고 pc버전도 해볼까 하다가 어려워보여서 아직 안해봤어요",
          "original_form": "그리고 pc버전도 해볼까 하다가 어려워보여서 아직 안해봤어요",
          "speaker_id": "1",
          "time": "20210820 22:57"
        },
        {
          "id": "MDRW2100003421.1.13",
          "form": "네네 폰보다 잘보여요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "네네 폰보다 잘보여요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:57"
        },
        {
          "id": "MDRW2100003421.1.14",
          "form": "배그 잘하는 사람이 너무 많아서 어렵대요 ㅠㅠ",
          "original_form": "배그 잘하는 사람이 너무 많아서 어렵대요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 22:57"
        },
        {
          "id": "MDRW2100003421.1.15",
          "form": "특히 한국인들이 잘한다죠ㅋㅋㅋ",
          "original_form": "특히 한국인들이 잘한다죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:57"
        },
        {
          "id": "MDRW2100003421.1.16",
          "form": "ㅋㅋㅋㅋㅋ 맞아요 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 맞아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:58"
        },
        {
          "id": "MDRW2100003421.1.17",
          "form": "총게임은 한국인이죠 !!",
          "original_form": "총게임은 한국인이죠 !!",
          "speaker_id": "2",
          "time": "20210820 22:58"
        },
        {
          "id": "MDRW2100003421.1.18",
          "form": "저는 콘솔은 주로 스위치로 하고 폰게임은 쿠킹덤해요!",
          "original_form": "저는 콘솔은 주로 스위치로 하고 폰게임은 쿠킹덤해요!",
          "speaker_id": "1",
          "time": "20210820 22:58"
        },
        {
          "id": "MDRW2100003421.1.19",
          "form": "ㅠㅠ 저 쿠킹덤... 꿀조합 찾으려고 그 새로 계정 파는거 있죠",
          "original_form": "ㅠㅠ 저 쿠킹덤... 꿀조합 찾으려고 그 새로 계정 파는거 있죠",
          "speaker_id": "2",
          "time": "20210820 22:59"
        },
        {
          "id": "MDRW2100003421.1.20",
          "form": "그거 한 20번 하다가 관뒀어요",
          "original_form": "그거 한 20번 하다가 관뒀어요",
          "speaker_id": "2",
          "time": "20210820 22:59"
        },
        {
          "id": "MDRW2100003421.1.21",
          "form": "엇저도 그거 초반에 한참했었는데ㅠㅠ",
          "original_form": "엇저도 그거 초반에 한참했었는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 22:59"
        },
        {
          "id": "MDRW2100003421.1.22",
          "form": "처음에 그거 하는지도 몰라서 에스프레소쿠키 하나 나왔다고 좋아하면서 했거든요?",
          "original_form": "처음에 그거 하는지도 몰라서 에스프레소쿠키 하나 나왔다고 좋아하면서 했거든요?",
          "speaker_id": "2",
          "time": "20210820 22:59"
        },
        {
          "id": "MDRW2100003421.1.23",
          "form": "레벨업 할수록 너무 격차가 커져서",
          "original_form": "레벨업 할수록 너무 격차가 커져서",
          "speaker_id": "2",
          "time": "20210820 22:59"
        },
        {
          "id": "MDRW2100003421.1.24",
          "form": "최상의 조합은 포기하고 그냥 어느정도만 갖추고 진행했네요..",
          "original_form": "최상의 조합은 포기하고 그냥 어느정도만 갖추고 진행했네요..",
          "speaker_id": "1",
          "time": "20210820 22:59"
        },
        {
          "id": "MDRW2100003421.1.25",
          "form": "그냥 그만뒀어요 ㅠㅠ",
          "original_form": "그냥 그만뒀어요 ㅠㅠ",
          "speaker_id": "2",
