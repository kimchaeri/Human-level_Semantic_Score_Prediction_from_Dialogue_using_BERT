      "utterance": [
        {
          "id": "MDRW2100003159.1.1",
          "form": "귤을 많이 드셔셔 학생들 피부들이 다 좋겠어요",
          "original_form": "귤을 많이 드셔셔 학생들 피부들이 다 좋겠어요",
          "speaker_id": "1",
          "time": "20210820 19:53"
        },
        {
          "id": "MDRW2100003159.1.2",
          "form": "그럴리가요..ㅋㅋㅋ여기 햇볕이 너무 강렬해서 주근깨가 많아요..",
          "original_form": "그럴리가요..ㅋㅋㅋ여기 햇볕이 너무 강렬해서 주근깨가 많아요..",
          "speaker_id": "2",
          "time": "20210820 19:54"
        },
        {
          "id": "MDRW2100003159.1.3",
          "form": "기미인가...?",
          "original_form": "기미인가...?",
          "speaker_id": "2",
          "time": "20210820 19:54"
        },
        {
          "id": "MDRW2100003159.1.4",
          "form": "아! 날씨가 좋아서인가",
          "original_form": "아! 날씨가 좋아서인가",
          "speaker_id": "1",
          "time": "20210820 19:54"
        },
        {
          "id": "MDRW2100003159.1.5",
          "form": "피부과 다니세요??",
          "original_form": "피부과 다니세요??",
          "speaker_id": "2",
          "time": "20210820 19:54"
        },
        {
          "id": "MDRW2100003159.1.6",
          "form": "아니요. 근데 가봐야할듯해요",
          "original_form": "아니요. 근데 가봐야할듯해요",
          "speaker_id": "1",
          "time": "20210820 19:54"
        },
        {
          "id": "MDRW2100003159.1.7",
          "form": "저도 기미ㅠ",
          "original_form": "저도 기미ㅠ",
          "speaker_id": "1",
          "time": "20210820 19:54"
        },
        {
          "id": "MDRW2100003159.1.8",
          "form": "저도 출산하고 기미가 늘어서",
          "original_form": "저도 출산하고 기미가 늘어서",
          "speaker_id": "2",
          "time": "20210820 19:54"
        },
        {
          "id": "MDRW2100003159.1.9",
          "form": "이제 다니기 시작해야지 맘먹었는게 확진자가 계속 느네요.ㅠㅠ",
          "original_form": "이제 다니기 시작해야지 맘먹었는게 확진자가 계속 느네요.ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 19:55"
        },
        {
          "id": "MDRW2100003159.1.10",
          "form": "아 정기적으로 관리 받으세요?",
          "original_form": "아 정기적으로 관리 받으세요?",
          "speaker_id": "1",
          "time": "20210820 19:55"
        },
        {
          "id": "MDRW2100003159.1.11",
          "form": "확실히 도움이 되나요?",
          "original_form": "확실히 도움이 되나요?",
          "speaker_id": "1",
          "time": "20210820 19:55"
        },
        {
          "id": "MDRW2100003159.1.12",
          "form": "아이 낳기 전에 받고 근 4~5년만에 받으려고 했는데..",
          "original_form": "아이 낳기 전에 받고 근 4~5년만에 받으려고 했는데..",
          "speaker_id": "2",
          "time": "20210820 19:56"
        },
        {
          "id": "MDRW2100003159.1.13",
          "form": "계속 꾸준히 받은 친구들 보면.. 다르긴 다르죠..ㅋㅋㅋ",
          "original_form": "계속 꾸준히 받은 친구들 보면.. 다르긴 다르죠..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 19:56"
        },
        {
          "id": "MDRW2100003159.1.14",
          "form": "ㅠㅠ 받고싶다",
          "original_form": "ㅠㅠ 받고싶다",
          "speaker_id": "1",
          "time": "20210820 19:57"
        },
        {
          "id": "MDRW2100003159.1.15",
          "form": "피부만 좋아져도 기분전환 되겠어요",
          "original_form": "피부만 좋아져도 기분전환 되겠어요",
          "speaker_id": "1",
          "time": "20210820 19:57"
        },
        {
          "id": "MDRW2100003159.1.16",
          "form": "근데 기미는 한번만 받으면 속깊이 있던 애들이 올라와서 몇번 받아야한다고 하더라고요.",
          "original_form": "근데 기미는 한번만 받으면 속깊이 있던 애들이 올라와서 몇번 받아야한다고 하더라고요.",
          "speaker_id": "2",
          "time": "20210820 19:58"
        },
        {
          "id": "MDRW2100003159.1.17",
          "form": "상담만 진행하고 더 하질 못해서...ㅠㅠ",
          "original_form": "상담만 진행하고 더 하질 못해서...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 19:58"
        },
        {
          "id": "MDRW2100003159.1.18",
          "form": "그게 고민되어요",
          "original_form": "그게 고민되어요",
          "speaker_id": "1",
          "time": "20210820 19:58"
        },
        {
          "id": "MDRW2100003159.1.19",
          "form": "꾸준히 받고",
          "original_form": "꾸준히 받고",
          "speaker_id": "1",
          "time": "20210820 19:58"
        },
        {
          "id": "MDRW2100003159.1.20",
          "form": "계속 관리하고ㅠ",
          "original_form": "계속 관리하고ㅠ",
          "speaker_id": "1",
          "time": "20210820 19:58"
        },
        {
          "id": "MDRW2100003159.1.21",
          "form": "그러니까요...ㅠ 다시 올라오고 관리 안되면 헛돈 쓰는거니까요.ㅠㅠ",
          "original_form": "그러니까요...ㅠ 다시 올라오고 관리 안되면 헛돈 쓰는거니까요.ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 19:59"
        },
        {
          "id": "MDRW2100003159.1.22",
          "form": "연예인처럼 비행기에서 한통씩 크림 쓸 것도 아니고ㅋㅋ",
          "original_form": "연예인처럼 비행기에서 한통씩 크림 쓸 것도 아니고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 19:59"
        },
        {
          "id": "MDRW2100003159.1.23",
          "form": "하하하하...  관리하면 확실히 좋은거 같긴해서..",
          "original_form": "하하하하...  관리하면 확실히 좋은거 같긴해서..",
          "speaker_id": "2",
          "time": "20210820 20:04"
        },
        {
          "id": "MDRW2100003159.1.24",
          "form": "하긴하는데 잘하는 짓인지는 모르겠네요.",
          "original_form": "하긴하는데 잘하는 짓인지는 모르겠네요.",
          "speaker_id": "2",
          "time": "20210820 20:05"
        },
        {
          "id": "MDRW2100003159.1.25",
          "form": "에고 하면 좋죠",
          "original_form": "에고 하면 좋죠",
          "speaker_id": "1",
          "time": "20210820 20:35"
        },
        {
          "id": "MDRW2100003159.1.26",
          "form": "자기에게 쓰는 돈은 아깝지 않지요ㅎㅎ",
          "original_form": "자기에게 쓰는 돈은 아깝지 않지요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 20:36"
        },
        {
          "id": "MDRW2100003159.1.27",
          "form": "화장품같은걸로 같이 관리해야죠",
          "original_form": "화장품같은걸로 같이 관리해야죠",
          "speaker_id": "2",
          "time": "20210820 21:04"
        },
        {
          "id": "MDRW2100003159.1.28",
          "form": "요즘 기미화장품들 갑자기 광고 많이하던데",
          "original_form": "요즘 기미화장품들 갑자기 광고 많이하던데",
          "speaker_id": "2",
          "time": "20210820 21:04"
        }
      ]
