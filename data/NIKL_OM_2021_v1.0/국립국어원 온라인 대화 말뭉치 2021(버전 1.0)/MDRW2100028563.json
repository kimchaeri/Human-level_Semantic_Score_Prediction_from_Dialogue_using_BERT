      "utterance": [
        {
          "id": "MDRW2100028563.1.1",
          "form": "거기 날씨는 어땠어요?",
          "original_form": "거기 날씨는 어땠어요?",
          "speaker_id": "1",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100028563.1.2",
          "form": "요즘 일기예보가 안 맞잖아요 비가 많이 왔어요",
          "original_form": "요즘 일기예보가 안 맞잖아요 비가 많이 왔어요",
          "speaker_id": "2",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100028563.1.3",
          "form": "여기도 비가 왔다갔다 했네요",
          "original_form": "여기도 비가 왔다갔다 했네요",
          "speaker_id": "1",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100028563.1.4",
          "form": "진짜요 장마라더니 정말 그런가봐오",
          "original_form": "진짜요 장마라더니 정말 그런가봐오",
          "speaker_id": "2",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100028563.1.5",
          "form": "이젠 여름장마가 아니가 가을장만가봐요",
          "original_form": "이젠 여름장마가 아니가 가을장만가봐요",
          "speaker_id": "1",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100028563.1.6",
          "form": "시원해지는 건 좋은데 출근하기가 너무 힘들어요 습하고",
          "original_form": "시원해지는 건 좋은데 출근하기가 너무 힘들어요 습하고",
          "speaker_id": "2",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028563.1.7",
          "form": "그쵸 계속 비가 오니 빨래를 못해요",
          "original_form": "그쵸 계속 비가 오니 빨래를 못해요",
          "speaker_id": "1",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028563.1.8",
          "form": "공감!! 빨래에서 꿉꿉한 냄새가 나는 게 짜증나요",
          "original_form": "공감!! 빨래에서 꿉꿉한 냄새가 나는 게 짜증나요",
          "speaker_id": "2",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028563.1.9",
          "form": "얼른 화창해져서 산책이라도 하고싶네요",
          "original_form": "얼른 화창해져서 산책이라도 하고싶네요",
          "speaker_id": "1",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100028563.1.10",
          "form": "비만 오다 겨울이 되는건 아니겠죠...ㅠ",
          "original_form": "비만 오다 겨울이 되는건 아니겠죠...ㅠ",
          "speaker_id": "1",
          "time": "20210902 20:57"
        },
        {
          "id": "MDRW2100028563.1.11",
          "form": "요즘 워낙 이상기후라 그것도 이상하지 않을 듯...",
          "original_form": "요즘 워낙 이상기후라 그것도 이상하지 않을 듯...",
          "speaker_id": "2",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028563.1.12",
          "form": "저도 퇴근은 걸으면 40분이라 걸어서 하는데... 요즘 못 걸으니까 너무 피곤해요",
          "original_form": "저도 퇴근은 걸으면 40분이라 걸어서 하는데... 요즘 못 걸으니까 너무 피곤해요",
          "speaker_id": "2",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100028563.1.13",
          "form": "와 40분 오래 걸으시네요",
          "original_form": "와 40분 오래 걸으시네요",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028563.1.14",
          "form": "운동삼아 걷는 거네요?",
          "original_form": "운동삼아 걷는 거네요?",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028563.1.15",
          "form": "딱 그러고 집에 와서 바로 씻으면 기분 좋으니까 ㅋㅋ",
          "original_form": "딱 그러고 집에 와서 바로 씻으면 기분 좋으니까 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028563.1.16",
          "form": "저도 앉아서 일하다보니 안 걸으면 몸이 아파서 힘들더라구요",
          "original_form": "저도 앉아서 일하다보니 안 걸으면 몸이 아파서 힘들더라구요",
          "speaker_id": "2",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028563.1.17",
          "form": "아무튼 비가 와서 운동도 못 하고 괴롭네요",
          "original_form": "아무튼 비가 와서 운동도 못 하고 괴롭네요",
          "speaker_id": "2",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028563.1.18",
          "form": "그쵸 앉아있는것도 지쳐요",
          "original_form": "그쵸 앉아있는것도 지쳐요",
          "speaker_id": "1",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028563.1.19",
          "form": "얼른 날이 맑아지길!",
          "original_form": "얼른 날이 맑아지길!",
          "speaker_id": "1",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028563.1.20",
          "form": "제발... 꿉꿉하지만 않아도 만점이에요",
          "original_form": "제발... 꿉꿉하지만 않아도 만점이에요",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028563.1.21",
          "form": "꿉꿉하지 않은 날을 바라며...",
          "original_form": "꿉꿉하지 않은 날을 바라며...",
          "speaker_id": "1",
