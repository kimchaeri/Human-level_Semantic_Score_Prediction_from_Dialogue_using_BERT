      "utterance": [
        {
          "id": "MDRW2100009653.1.1",
          "form": "유튜브 자주 봐?",
          "original_form": "유튜브 자주 봐?",
          "speaker_id": "1",
          "time": "20210823 02:28"
        },
        {
          "id": "MDRW2100009653.1.2",
          "form": "나 요즘 진짜 자주 보는 듯",
          "original_form": "나 요즘 진짜 자주 보는 듯",
          "speaker_id": "2",
          "time": "20210823 02:29"
        },
        {
          "id": "MDRW2100009653.1.3",
          "form": "뭐 자주 봐?",
          "original_form": "뭐 자주 봐?",
          "speaker_id": "1",
          "time": "20210823 02:29"
        },
        {
          "id": "MDRW2100009653.1.4",
          "form": "나는 강과장이라는 절약유튜버 많이 봐",
          "original_form": "나는 강과장이라는 절약유튜버 많이 봐",
          "speaker_id": "1",
          "time": "20210823 02:29"
        },
        {
          "id": "MDRW2100009653.1.5",
          "form": "오 그 분 좋아?!",
          "original_form": "오 그 분 좋아?!",
          "speaker_id": "2",
          "time": "20210823 02:30"
        },
        {
          "id": "MDRW2100009653.1.6",
          "form": "절약 유튜번데 보기 좋아 사람냄새가 나서 좋아",
          "original_form": "절약 유튜번데 보기 좋아 사람냄새가 나서 좋아",
          "speaker_id": "1",
          "time": "20210823 02:30"
        },
        {
          "id": "MDRW2100009653.1.7",
          "form": "최근에 결혼했는데 그것도 재밌어",
          "original_form": "최근에 결혼했는데 그것도 재밌어",
          "speaker_id": "1",
          "time": "20210823 02:30"
        },
        {
          "id": "MDRW2100009653.1.8",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210823 02:31"
        },
        {
          "id": "MDRW2100009653.1.9",
          "form": "난 요즘",
          "original_form": "난 요즘",
          "speaker_id": "2",
          "time": "20210823 02:31"
        },
        {
          "id": "MDRW2100009653.1.10",
          "form": "좋아해",
          "original_form": "좋아해",
          "speaker_id": "2",
          "time": "20210823 02:31"
        },
        {
          "id": "MDRW2100009653.1.11",
          "form": "요즘이라는 유튜버야?",
          "original_form": "요즘이라는 유튜버야?",
          "speaker_id": "1",
          "time": "20210823 02:31"
        },
        {
          "id": "MDRW2100009653.1.12",
          "form": "카테고리가 뭐야?",
          "original_form": "카테고리가 뭐야?",
          "speaker_id": "1",
          "time": "20210823 02:31"
        },
        {
          "id": "MDRW2100009653.1.13",
          "form": "아 이게",
          "original_form": "아 이게",
          "speaker_id": "2",
          "time": "20210823 02:33"
        },
        {
          "id": "MDRW2100009653.1.14",
          "form": "발명 쓰   레  기 걸",
          "original_form": "발명 쓰   레  기 걸",
          "speaker_id": "2",
          "time": "20210823 02:33"
        },
        {
          "id": "MDRW2100009653.1.15",
          "form": "이게 걸렸구나 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ ㅠㅠ",
          "original_form": "이게 걸렸구나 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 02:33"
        },
        {
          "id": "MDRW2100009653.1.16",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ 무슨 말인지 모르겠어!",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ 무슨 말인지 모르겠어!",
          "speaker_id": "1",
          "time": "20210823 02:34"
        },
        {
          "id": "MDRW2100009653.1.17",
          "form": "발쓰걸... 재밌어",
          "original_form": "발쓰걸... 재밌어",
          "speaker_id": "2",
          "time": "20210823 02:34"
        },
        {
          "id": "MDRW2100009653.1.18",
          "form": "다음에 봐봐",
          "original_form": "다음에 봐봐",
          "speaker_id": "2",
          "time": "20210823 02:35"
        },
        {
          "id": "MDRW2100009653.1.19",
          "form": "뭘까 신기하네",
          "original_form": "뭘까 신기하네",
          "speaker_id": "1",
          "time": "20210823 02:36"
        },
        {
          "id": "MDRW2100009653.1.20",
          "form": "다음에 봐볼게 ㅎㅎ",
          "original_form": "다음에 봐볼게 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 02:36"
        },
        {
          "id": "MDRW2100009653.1.21",
          "form": "나도 강과장 찾아볼게",
          "original_form": "나도 강과장 찾아볼게",
          "speaker_id": "2",
          "time": "20210823 02:46"
        },
        {
          "id": "MDRW2100009653.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 02:46"
        },
        {
          "id": "MDRW2100009653.1.23",
          "form": "강과장 영상 방금 올라와서 보고 있어 ㅋㅋㅋㅋ",
          "original_form": "강과장 영상 방금 올라와서 보고 있어 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 02:47"
        }
      ]
