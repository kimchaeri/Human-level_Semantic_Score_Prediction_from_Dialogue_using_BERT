      "utterance": [
        {
          "id": "MDRW2100037323.1.1",
          "form": "나 오늘 영화보고왔어",
          "original_form": "나 오늘 영화보고왔어",
          "speaker_id": "2",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100037323.1.2",
          "form": "어 정말?",
          "original_form": "어 정말?",
          "speaker_id": "1",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100037323.1.3",
          "form": "영화관에서 본거야m",
          "original_form": "영화관에서 본거야m",
          "speaker_id": "1",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100037323.1.4",
          "form": "응응 신세계 백화점에 갔어",
          "original_form": "응응 신세계 백화점에 갔어",
          "speaker_id": "2",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100037323.1.5",
          "form": "사람은 많았어?",
          "original_form": "사람은 많았어?",
          "speaker_id": "1",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100037323.1.6",
          "form": "우와 신세계라니 멋있다",
          "original_form": "우와 신세계라니 멋있다",
          "speaker_id": "1",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100037323.1.7",
          "form": "너 신세계 메가박스 가본적있니??",
          "original_form": "너 신세계 메가박스 가본적있니??",
          "speaker_id": "2",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100037323.1.8",
          "form": "거기 극장은 어때?작지?",
          "original_form": "거기 극장은 어때?작지?",
          "speaker_id": "1",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100037323.1.9",
          "form": "음 별로없었어 열명정도??",
          "original_form": "음 별로없었어 열명정도??",
          "speaker_id": "2",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100037323.1.10",
          "form": "나는 겉에서만 봤어",
          "original_form": "나는 겉에서만 봤어",
          "speaker_id": "1",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100037323.1.11",
          "form": "신세계 건물은 참 좋아",
          "original_form": "신세계 건물은 참 좋아",
          "speaker_id": "1",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100037323.1.12",
          "form": "영화는 뭐 봤어?",
          "original_form": "영화는 뭐 봤어?",
          "speaker_id": "1",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100037323.1.13",
          "form": "관마다다른데 그래두 칠곡보단 나아!",
          "original_form": "관마다다른데 그래두 칠곡보단 나아!",
          "speaker_id": "2",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100037323.1.14",
          "form": "아 그건 당연하지!",
          "original_form": "아 그건 당연하지!",
          "speaker_id": "1",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100037323.1.15",
          "form": "나는 코다 라는 영화를 봒어",
          "original_form": "나는 코다 라는 영화를 봒어",
          "speaker_id": "2",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100037323.1.16",
          "form": "ost가 너무 좋아서 지금도 듣고있어",
          "original_form": "ost가 너무 좋아서 지금도 듣고있어",
          "speaker_id": "2",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100037323.1.17",
          "form": "코다노 치즈에 관한 이야기야?",
          "original_form": "코다노 치즈에 관한 이야기야?",
          "speaker_id": "1",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100037323.1.18",
          "form": "코다노치즈 좋아해ㅜ나는",
          "original_form": "코다노치즈 좋아해ㅜ나는",
          "speaker_id": "1",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100037323.1.19",
          "form": "아니 ㅠㅠ 나도 뜻을 몰랐는데",
          "original_form": "아니 ㅠㅠ 나도 뜻을 몰랐는데",
          "speaker_id": "2",
          "time": "20210831 22:33"
        }
      ]
