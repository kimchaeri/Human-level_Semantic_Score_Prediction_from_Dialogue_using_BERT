      "utterance": [
        {
          "id": "MDRW2100005977.1.1",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210907 18:12"
        },
        {
          "id": "MDRW2100005977.1.2",
          "form": "그런가욬ㅋㅋㅋㅋㅋ",
          "original_form": "그런가욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:12"
        },
        {
          "id": "MDRW2100005977.1.3",
          "form": "1년마다 갱신되는데. 그 이전 세대 실비보험은",
          "original_form": "1년마다 갱신되는데. 그 이전 세대 실비보험은",
          "speaker_id": "1",
          "time": "20210907 18:16"
        },
        {
          "id": "MDRW2100005977.1.4",
          "form": "내가 얼마를 썼건간에, 내 나잇대의 친구들이 얼마나 보험사에 손해를 입혔냐에 따라서 결정되어서...",
          "original_form": "내가 얼마를 썼건간에, 내 나잇대의 친구들이 얼마나 보험사에 손해를 입혔냐에 따라서 결정되어서...",
          "speaker_id": "1",
          "time": "20210907 18:16"
        },
        {
          "id": "MDRW2100005977.1.5",
          "form": "보험료가 많이 인상될 가능성이 크구~",
          "original_form": "보험료가 많이 인상될 가능성이 크구~",
          "speaker_id": "1",
          "time": "20210907 18:16"
        },
        {
          "id": "MDRW2100005977.1.6",
          "form": "그 이후는 연대책임은 없어졌어요",
          "original_form": "그 이후는 연대책임은 없어졌어요",
          "speaker_id": "1",
          "time": "20210907 18:16"
        },
        {
          "id": "MDRW2100005977.1.7",
          "form": "name2님 실비 있으시지 않으셔욤?",
          "original_form": "&name2&님 실비 있으시지 않으셔욤?",
          "speaker_id": "1",
          "time": "20210907 18:17"
        },
        {
          "id": "MDRW2100005977.1.8",
          "form": "전 왠지 1세대? ,,걍냅둿죠 ㅋㅋㅋㅋ",
          "original_form": "전 왠지 1세대? ,,걍냅둿죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100005977.1.9",
          "form": "엄마가들엇어요",
          "original_form": "엄마가들엇어요",
          "speaker_id": "2",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100005977.1.10",
          "form": "2008년이 들엇을걸욬ㅋㅋㅋㅋ",
          "original_form": "2008년이 들엇을걸욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100005977.1.11",
          "form": "아하 많이 다치시는게 이득인데",
          "original_form": "아하 많이 다치시는게 이득인데",
          "speaker_id": "1",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100005977.1.12",
          "form": "다치면 안되니까 ... ㅠ",
          "original_form": "다치면 안되니까 ... ㅠ",
          "speaker_id": "1",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100005977.1.13",
          "form": "근디 실비만 든건 아닌거같은게 보험료 칠팔마넌씩 나온대요 ㅋㅋㅋㅋ",
          "original_form": "근디 실비만 든건 아닌거같은게 보험료 칠팔마넌씩 나온대요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100005977.1.14",
          "form": "도수치료라도 받으셔요",
          "original_form": "도수치료라도 받으셔요",
          "speaker_id": "1",
          "time": "20210907 18:20"
        },
        {
          "id": "MDRW2100005977.1.15",
          "form": "ㅋㅋㅋㅋㅋ 그래야하나",
          "original_form": "ㅋㅋㅋㅋㅋ 그래야하나",
          "speaker_id": "2",
          "time": "20210907 18:20"
        },
        {
          "id": "MDRW2100005977.1.16",
          "form": "ㅋ ㅋㅋㅋㅋㅋ 여기가 랜덤으로 만나는 장만 아니였으면",
          "original_form": "ㅋ ㅋㅋㅋㅋㅋ 여기가 랜덤으로 만나는 장만 아니였으면",
          "speaker_id": "1",
          "time": "20210907 18:20"
        },
        {
          "id": "MDRW2100005977.1.17",
          "form": "아픈데가많긴한데 ㅋㅋㅋㅋㅋ",
          "original_form": "아픈데가많긴한데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:20"
        },
        {
          "id": "MDRW2100005977.1.18",
          "form": "뭐 가입되어있었는지 봐드렸을텐데",
          "original_form": "뭐 가입되어있었는지 봐드렸을텐데",
          "speaker_id": "1",
          "time": "20210907 18:20"
        },
        {
          "id": "MDRW2100005977.1.19",
          "form": "헉 진짜요",
          "original_form": "헉 진짜요",
          "speaker_id": "1",
          "time": "20210907 18:20"
        },
        {
          "id": "MDRW2100005977.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ잔고장이많아서...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ잔고장이많아서...",
          "speaker_id": "2",
          "time": "20210907 18:20"
        },
        {
          "id": "MDRW2100005977.1.21",
          "form": "신청도귀차나요ㅠㅠ",
          "original_form": "신청도귀차나요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 18:20"
        },
        {
          "id": "MDRW2100005977.1.22",
          "form": "ㅠ...건강 챙깁시다...",
          "original_form": "ㅠ...건강 챙깁시다...",
          "speaker_id": "1",
          "time": "20210907 18:21"
        },
        {
          "id": "MDRW2100005977.1.23",
          "form": "무튼 보험 하구 있습니다..",
          "original_form": "무튼 보험 하구 있습니다..",
          "speaker_id": "1",
          "time": "20210907 18:21"
        },
        {
          "id": "MDRW2100005977.1.24",
          "form": "휴대폰은 뭐쓰세욤?",
          "original_form": "휴대폰은 뭐쓰세욤?",
          "speaker_id": "1",
          "time": "20210907 18:21"
        },
        {
          "id": "MDRW2100005977.1.25",
          "form": "s10e욬ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "s10e욬ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:22"
        },
        {
          "id": "MDRW2100005977.1.26",
          "form": "꼬물",
          "original_form": "꼬물",
