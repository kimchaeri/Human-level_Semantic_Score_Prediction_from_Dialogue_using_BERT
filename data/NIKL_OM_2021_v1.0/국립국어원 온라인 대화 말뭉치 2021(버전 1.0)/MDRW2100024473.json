      "utterance": [
        {
          "id": "MDRW2100024473.1.1",
          "form": "좋아하시는 음료있으세요?",
          "original_form": "좋아하시는 음료있으세요?",
          "speaker_id": "2",
          "time": "20210831 11:02"
        },
        {
          "id": "MDRW2100024473.1.2",
          "form": "콜라요ㅋㅋ",
          "original_form": "콜라요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:08"
        },
        {
          "id": "MDRW2100024473.1.3",
          "form": "코카콜라요 아님 펩시요?",
          "original_form": "코카콜라요 아님 펩시요?",
          "speaker_id": "2",
          "time": "20210831 11:09"
        },
        {
          "id": "MDRW2100024473.1.4",
          "form": "코카콜라죠ㅎㅎ.",
          "original_form": "코카콜라죠ㅎㅎ.",
          "speaker_id": "1",
          "time": "20210831 11:10"
        },
        {
          "id": "MDRW2100024473.1.5",
          "form": "제로도 많이 나왔더라구요",
          "original_form": "제로도 많이 나왔더라구요",
          "speaker_id": "2",
          "time": "20210831 11:11"
        },
        {
          "id": "MDRW2100024473.1.6",
          "form": "전 제로 맛이없더라구요ㅠ흑",
          "original_form": "전 제로 맛이없더라구요ㅠ흑",
          "speaker_id": "1",
          "time": "20210831 11:12"
        },
        {
          "id": "MDRW2100024473.1.7",
          "form": "그쵸?ㅜㅜ",
          "original_form": "그쵸?ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 11:26"
        },
        {
          "id": "MDRW2100024473.1.8",
          "form": "역시 칼로리가 있는게ㅋㅋ",
          "original_form": "역시 칼로리가 있는게ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 11:26"
        },
        {
          "id": "MDRW2100024473.1.9",
          "form": "네ㅜㅜ단맛이 빠지니ㅠ",
          "original_form": "네ㅜㅜ단맛이 빠지니ㅠ",
          "speaker_id": "1",
          "time": "20210831 11:32"
        },
        {
          "id": "MDRW2100024473.1.10",
          "form": "ㅋㅋㅋㅋㅋ탄산 좋아하시나요?",
          "original_form": "ㅋㅋㅋㅋㅋ탄산 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210831 11:51"
        },
        {
          "id": "MDRW2100024473.1.11",
          "form": "네ㅎ느끼한거먹으면 한잔꼭 해야개운하더라구요ㅋㅋ",
          "original_form": "네ㅎ느끼한거먹으면 한잔꼭 해야개운하더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:53"
        },
        {
          "id": "MDRW2100024473.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맞아요 치킨에는 꼭 탄산이죠ㅎ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맞아요 치킨에는 꼭 탄산이죠ㅎ",
          "speaker_id": "2",
          "time": "20210831 11:59"
        },
        {
          "id": "MDRW2100024473.1.13",
          "form": "맞아요 이래서 살이안 빠지져",
          "original_form": "맞아요 이래서 살이안 빠지져",
          "speaker_id": "1",
          "time": "20210831 12:01"
        },
        {
          "id": "MDRW2100024473.1.14",
          "form": "ㅋㅋㅋㅋㅋ살은 어쩔 수 없는듯ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ살은 어쩔 수 없는듯ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:01"
        },
        {
          "id": "MDRW2100024473.1.15",
          "form": "맞아어ㅠㅠ맛난게 너무 많으니까",
          "original_form": "맞아어ㅠㅠ맛난게 너무 많으니까",
          "speaker_id": "1",
