      "utterance": [
        {
          "id": "MDRW2100034309.1.1",
          "form": "긴팔 입고 싶어요",
          "original_form": "긴팔 입고 싶어요",
          "speaker_id": "2",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100034309.1.2",
          "form": "걷기좋은 날씨가 오길 기다려요",
          "original_form": "걷기좋은 날씨가 오길 기다려요",
          "speaker_id": "2",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100034309.1.3",
          "form": "눈은 좋아하세요?",
          "original_form": "눈은 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100034309.1.4",
          "form": "네 진짜 좋아요",
          "original_form": "네 진짜 좋아요",
          "speaker_id": "1",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100034309.1.5",
          "form": "밟는거 좋아해요 눈",
          "original_form": "밟는거 좋아해요 눈",
          "speaker_id": "1",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100034309.1.6",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100034309.1.7",
          "form": "저도 부산출신이라 눈 좋아해요",
          "original_form": "저도 부산출신이라 눈 좋아해요",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100034309.1.8",
          "form": "많이 못봐서요",
          "original_form": "많이 못봐서요",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100034309.1.9",
          "form": "헉 부산은 눈많이 안내리나요??",
          "original_form": "헉 부산은 눈많이 안내리나요??",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100034309.1.10",
          "form": "네...부산은 눈다운 눈은 일년에 한번도 안오기도해요",
          "original_form": "네...부산은 눈다운 눈은 일년에 한번도 안오기도해요",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100034309.1.11",
          "form": "헐 기온이 높아서요??",
          "original_form": "헐 기온이 높아서요??",
          "speaker_id": "1",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100034309.1.12",
          "form": "네 보통 그래요",
          "original_form": "네 보통 그래요",
          "speaker_id": "2",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100034309.1.13",
          "form": "아하 전서울사는데 눈많이 오더라구요",
          "original_form": "아하 전서울사는데 눈많이 오더라구요",
          "speaker_id": "1",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100034309.1.14",
          "form": "이번겨울이 작년보다 추울까요??",
          "original_form": "이번겨울이 작년보다 추울까요??",
          "speaker_id": "1",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100034309.1.15",
          "form": "글쎄요...작년에 너무 추워서 올해는 덜하면 좋겠어요ㅠ",
          "original_form": "글쎄요...작년에 너무 추워서 올해는 덜하면 좋겠어요ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100034309.1.16",
          "form": "여름은 더 덥지않았나요 작년보다?",
          "original_form": "여름은 더 덥지않았나요 작년보다?",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100034309.1.17",
          "form": "비가안와서 ㅠㅠ",
          "original_form": "비가안와서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100034309.1.18",
          "form": "엄청더 더웠어요ㅠㅠ",
          "original_form": "엄청더 더웠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100034309.1.19",
          "form": "작년엔 별로 안더웠는데ㅠㅠ",
          "original_form": "작년엔 별로 안더웠는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100034309.1.20",
          "form": "그러니까요 ㅠ 작년여름이 젤좋았어요",
          "original_form": "그러니까요 ㅠ 작년여름이 젤좋았어요",
          "speaker_id": "1",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100034309.1.21",
          "form": "비도 많이 와서 시원하고",
          "original_form": "비도 많이 와서 시원하고",
          "speaker_id": "1",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100034309.1.22",
          "form": "그죠... 오늘은 그래도 맑은데 덜덥네요",
          "original_form": "그죠... 오늘은 그래도 맑은데 덜덥네요",
          "speaker_id": "2",
          "time": "20210828 16:49"
        }
      ]
