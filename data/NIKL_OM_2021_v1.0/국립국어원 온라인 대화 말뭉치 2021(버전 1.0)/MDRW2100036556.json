      "utterance": [
        {
          "id": "MDRW2100036556.1.1",
          "form": "스페인인 투우로 유명하잖아",
          "original_form": "스페인인 투우로 유명하잖아",
          "speaker_id": "2",
          "time": "20210831 00:35"
        },
        {
          "id": "MDRW2100036556.1.2",
          "form": "엉 밀양 소싸움처럼",
          "original_form": "엉 밀양 소싸움처럼",
          "speaker_id": "1",
          "time": "20210831 00:35"
        },
        {
          "id": "MDRW2100036556.1.3",
          "form": "응 경기장에서 투우사와 빨강 망토 .그리고 소",
          "original_form": "응 경기장에서 투우사와 빨강 망토 .그리고 소",
          "speaker_id": "2",
          "time": "20210831 00:36"
        },
        {
          "id": "MDRW2100036556.1.4",
          "form": "응 이건 동물보호 뭐라 안하네",
          "original_form": "응 이건 동물보호 뭐라 안하네",
          "speaker_id": "1",
          "time": "20210831 00:36"
        },
        {
          "id": "MDRW2100036556.1.5",
          "form": "그러게 이 투우가 북아프리카 이슬람교도어에 의해 전파가 되었다네",
          "original_form": "그러게 이 투우가 북아프리카 이슬람교도어에 의해 전파가 되었다네",
          "speaker_id": "2",
          "time": "20210831 00:37"
        },
        {
          "id": "MDRW2100036556.1.6",
          "form": "원래 있던게 아니네",
          "original_form": "원래 있던게 아니네",
          "speaker_id": "1",
          "time": "20210831 00:37"
        },
        {
          "id": "MDRW2100036556.1.7",
          "form": "응 귀족들의 오락에서 일반으로 옮김",
          "original_form": "응 귀족들의 오락에서 일반으로 옮김",
          "speaker_id": "2",
          "time": "20210831 00:38"
        },
        {
          "id": "MDRW2100036556.1.8",
          "form": "투우사가 일반 시민이겠다",
          "original_form": "투우사가 일반 시민이겠다",
          "speaker_id": "1",
          "time": "20210831 00:38"
        },
        {
          "id": "MDRW2100036556.1.9",
          "form": "매년 3월 중순에 투우가 경기가 시작해서 7개월 계속된다네",
          "original_form": "매년 3월 중순에 투우가 경기가 시작해서 7개월 계속된다네",
          "speaker_id": "2",
          "time": "20210831 00:38"
        },
        {
          "id": "MDRW2100036556.1.10",
          "form": "시기가 있구나",
          "original_form": "시기가 있구나",
          "speaker_id": "1",
          "time": "20210831 00:39"
        },
        {
          "id": "MDRW2100036556.1.11",
          "form": "응 동물보호단체 때문에 투우가 금지된 곳도 있데",
          "original_form": "응 동물보호단체 때문에 투우가 금지된 곳도 있데",
          "speaker_id": "2",
          "time": "20210831 00:39"
        },
        {
          "id": "MDRW2100036556.1.12",
          "form": "그래야지 완전 오락으로 하는거니",
          "original_form": "그래야지 완전 오락으로 하는거니",
          "speaker_id": "1",
          "time": "20210831 00:40"
        },
        {
          "id": "MDRW2100036556.1.13",
          "form": "응 점점 투유 경기도 없어질듯",
          "original_form": "응 점점 투유 경기도 없어질듯",
          "speaker_id": "2",
          "time": "20210831 00:43"
        },
        {
          "id": "MDRW2100036556.1.14",
          "form": "멕시코에도 투우경기장이 있데",
          "original_form": "멕시코에도 투우경기장이 있데",
          "speaker_id": "1",
          "time": "20210831 00:43"
        },
        {
          "id": "MDRW2100036556.1.15",
