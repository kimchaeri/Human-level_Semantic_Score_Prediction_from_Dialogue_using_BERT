      "utterance": [
        {
          "id": "MDRW2100043691.1.1",
          "form": "운동하시는거있으세요",
          "original_form": "운동하시는거있으세요",
          "speaker_id": "1",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043691.1.2",
          "form": "오 저는 요가해요!",
          "original_form": "오 저는 요가해요!",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043691.1.3",
          "form": "요 며칠 못하긴 했지만 ㅋㅋㅋㅋㅋ",
          "original_form": "요 며칠 못하긴 했지만 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043691.1.4",
          "form": "요가 집에서해요 배우러가시는거에여?",
          "original_form": "요가 집에서해요 배우러가시는거에여?",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043691.1.5",
          "form": "배우러 다녀요 히히",
          "original_form": "배우러 다녀요 히히",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043691.1.6",
          "form": "혼자 하기는 무리가 있더라구요",
          "original_form": "혼자 하기는 무리가 있더라구요",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043691.1.7",
          "form": "크으 배우는거 엄청 힘들어보이던데 어때여",
          "original_form": "크으 배우는거 엄청 힘들어보이던데 어때여",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043691.1.8",
          "form": "가기 귀찮은것만 빼면 괜찮아요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "가기 귀찮은것만 빼면 괜찮아요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043691.1.9",
          "form": "빡센건 일부러 빠지거든요 히히",
          "original_form": "빡센건 일부러 빠지거든요 히히",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043691.1.10",
          "form": "아하 ㅋㅋㅋㅋ그런좋은방법이",
          "original_form": "아하 ㅋㅋㅋㅋ그런좋은방법이",
          "speaker_id": "1",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043691.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 적당히 나가다보면 언젠가 근육이 붙겠쬬",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 적당히 나가다보면 언젠가 근육이 붙겠쬬",
          "speaker_id": "2",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043691.1.12",
          "form": "ㅋㅋㅋㅋㅋ자세교정은 확실히되나여",
          "original_form": "ㅋㅋㅋㅋㅋ자세교정은 확실히되나여",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043691.1.13",
          "form": "오 그럼요",
          "original_form": "오 그럼요",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043691.1.14",
          "form": "그건 확실함다",
          "original_form": "그건 확실함다",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043691.1.15",
          "form": "키도컸어요 ㅋㅋㅋㅋ",
          "original_form": "키도컸어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043691.1.16",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043691.1.17",
          "form": "허리골반목 다안좋은데ㅜ 진짜 함가봐야겠네요",
          "original_form": "허리골반목 다안좋은데ㅜ 진짜 함가봐야겠네요",
          "speaker_id": "1",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043691.1.18",
          "form": "오 디스크에 요가가 진짜 좋대요!",
          "original_form": "오 디스크에 요가가 진짜 좋대요!",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043691.1.19",
          "form": "그 요가도 종류가 많던데 어떤거배우세요",
          "original_form": "그 요가도 종류가 많던데 어떤거배우세요",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043691.1.20",
          "form": "저는 웬만한거 다해요!",
          "original_form": "저는 웬만한거 다해요!",
          "speaker_id": "2",
          "time": "20210907 21:13"
        }
      ]
