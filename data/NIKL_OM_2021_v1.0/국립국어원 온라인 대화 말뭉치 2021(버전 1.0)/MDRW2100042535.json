      "utterance": [
        {
          "id": "MDRW2100042535.1.1",
          "form": "물놀이쌤 진짜..애들보랴..",
          "original_form": "물놀이쌤 진짜..애들보랴..",
          "speaker_id": "2",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042535.1.2",
          "form": "극한직업이셨을것같네요",
          "original_form": "극한직업이셨을것같네요",
          "speaker_id": "2",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042535.1.3",
          "form": "바위에서 뛰어내리지마라..?",
          "original_form": "바위에서 뛰어내리지마라..?",
          "speaker_id": "2",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042535.1.4",
          "form": "네네 ㅋㅋㅋㅋㅋㅋ어찌나 돌던지고놀던지",
          "original_form": "네네 ㅋㅋㅋㅋㅋㅋ어찌나 돌던지고놀던지",
          "speaker_id": "1",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042535.1.5",
          "form": "ㅋㅋㅋ아이고",
          "original_form": "ㅋㅋㅋ아이고",
          "speaker_id": "2",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042535.1.6",
          "form": "보통 남자애들이 더많으니까 ㅋㅋㅋㅋ넘격하게놀아요 물공격하고",
          "original_form": "보통 남자애들이 더많으니까 ㅋㅋㅋㅋ넘격하게놀아요 물공격하고",
          "speaker_id": "1",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042535.1.7",
          "form": "생각만해도 아찔하네요",
          "original_form": "생각만해도 아찔하네요",
          "speaker_id": "2",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042535.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042535.1.9",
          "form": "그래도 그만큼 기억은 오래남은듯여 ㅋㅋㅋㅋㅋㅋㅋ고생한만큼 ㅋㅋㅋ",
          "original_form": "그래도 그만큼 기억은 오래남은듯여 ㅋㅋㅋㅋㅋㅋㅋ고생한만큼 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042535.1.10",
          "form": "ㅋㅋㅋㅋㅋ그쵸",
          "original_form": "ㅋㅋㅋㅋㅋ그쵸",
          "speaker_id": "2",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042535.1.11",
          "form": "학교다닐때 동아리활동하셨나요?",
          "original_form": "학교다닐때 동아리활동하셨나요?",
          "speaker_id": "1",
          "time": "20210906 22:42"
        },
        {
          "id": "MDRW2100042535.1.12",
          "form": "아 제가 학교다닐때는 ca라고",
          "original_form": "아 제가 학교다닐때는 ca라고",
          "speaker_id": "2",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042535.1.13",
          "form": "있어서 ㅋㅋㅋ 1년에 하나씩 댄스동아리 토론부 배드민턴부 했었던것같습니다",
          "original_form": "있어서 ㅋㅋㅋ 1년에 하나씩 댄스동아리 토론부 배드민턴부 했었던것같습니다",
          "speaker_id": "2",
          "time": "20210906 22:43"
        },
        {
          "id": "MDRW2100042535.1.14",
          "form": "대학때는 안했엇어요 ㅎㅎ",
          "original_form": "대학때는 안했엇어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042535.1.15",
          "form": "ㅋㅋㅋㅋ아 저도 ca있었어요 저때는 초딩때",
          "original_form": "ㅋㅋㅋㅋ아 저도 ca있었어요 저때는 초딩때",
          "speaker_id": "1",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042535.1.16",
          "form": "혹시 활동하셧었나요?:)",
          "original_form": "혹시 활동하셧었나요?:)",
          "speaker_id": "2",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042535.1.17",
          "form": "앗 ㅎㅎ네네",
          "original_form": "앗 ㅎㅎ네네",
          "speaker_id": "2",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042535.1.18",
          "form": "ca는 교내 부서느낌이라 거의독서부 영화감상부 이런거했었구여",
          "original_form": "ca는 교내 부서느낌이라 거의독서부 영화감상부 이런거했었구여",
          "speaker_id": "1",
          "time": "20210906 22:44"
        },
        {
          "id": "MDRW2100042535.1.19",
          "form": "동아리는 연극동아리했었어요",
          "original_form": "동아리는 연극동아리했었어요",
          "speaker_id": "1",
          "time": "20210906 22:45"
        },
        {
          "id": "MDRW2100042535.1.20",
          "form": "댄동이면 춤좀 배우셨겠어요",
          "original_form": "댄동이면 춤좀 배우셨겠어요",
          "speaker_id": "1",
          "time": "20210906 22:45"
        },
        {
          "id": "MDRW2100042535.1.21",
          "form": "와 연극동아리면 연기배우신건가요?:)",
          "original_form": "와 연극동아리면 연기배우신건가요?:)",
          "speaker_id": "2",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042535.1.22",
          "form": "몸부림에 가까웠지만 ㅎㅎ 재밌었어요!",
          "original_form": "몸부림에 가까웠지만 ㅎㅎ 재밌었어요!",
          "speaker_id": "2",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042535.1.23",
          "form": "ㅋㅋㅋㅋ애들끼리 대본보고 한 수준이에요",
          "original_form": "ㅋㅋㅋㅋ애들끼리 대본보고 한 수준이에요",
          "speaker_id": "1",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100042535.1.24",
          "form": "전 연기더럽게못해서 축제마다 탈락 \r\nㅜ ㅋㅋㅋㅋㅋ",
          "original_form": "전 연기더럽게못해서 축제마다 탈락 \r\nㅜ ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
