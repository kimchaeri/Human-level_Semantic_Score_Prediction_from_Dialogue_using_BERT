      "utterance": [
        {
          "id": "MDRW2100013580.1.1",
          "form": "그럼 치킨은 뭐 좋아해?",
          "original_form": "그럼 치킨은 뭐 좋아해?",
          "speaker_id": "2",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013580.1.2",
          "form": "난 뿌링클?? 느끼한거 좋아하는거같아",
          "original_form": "난 뿌링클?? 느끼한거 좋아하는거같아",
          "speaker_id": "1",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013580.1.3",
          "form": "너는??",
          "original_form": "너는??",
          "speaker_id": "1",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013580.1.4",
          "form": "나는 간장치킨 좋아해ㅎ 짭쪼롬한 게 좋더라고",
          "original_form": "나는 간장치킨 좋아해ㅎ 짭쪼롬한 게 좋더라고",
          "speaker_id": "2",
          "time": "20210825 15:14"
        },
        {
          "id": "MDRW2100013580.1.5",
          "form": "오 교촌 자주먹을듯?",
          "original_form": "오 교촌 자주먹을듯?",
          "speaker_id": "1",
          "time": "20210825 15:14"
        },
        {
          "id": "MDRW2100013580.1.6",
          "form": "ㅋㅋㅋ아닌가",
          "original_form": "ㅋㅋㅋ아닌가",
          "speaker_id": "1",
          "time": "20210825 15:14"
        },
        {
          "id": "MDRW2100013580.1.7",
          "form": "교촌 맛있는데ㅋㅋㅋ 배달료 너무 비싸서 집 앞에 사쿠사쿠에서 주로 사먹어",
          "original_form": "교촌 맛있는데ㅋㅋㅋ 배달료 너무 비싸서 집 앞에 사쿠사쿠에서 주로 사먹어",
          "speaker_id": "2",
          "time": "20210825 15:14"
        },
        {
          "id": "MDRW2100013580.1.8",
          "form": "이름 되게 특이하다ㅋㅋㅋ",
          "original_form": "이름 되게 특이하다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:16"
        },
        {
          "id": "MDRW2100013580.1.9",
          "form": "왜 사쿠사쿠인지 알아??",
          "original_form": "왜 사쿠사쿠인지 알아??",
          "speaker_id": "1",
          "time": "20210825 15:16"
        },
        {
          "id": "MDRW2100013580.1.10",
          "form": "아니ㅋㅋㅋㅋ 그거까지는 잘 모르겠어 생각을 안해봤네",
          "original_form": "아니ㅋㅋㅋㅋ 그거까지는 잘 모르겠어 생각을 안해봤네",
          "speaker_id": "2",
          "time": "20210825 15:16"
        },
        {
          "id": "MDRW2100013580.1.11",
          "form": "치킨 말고 찜닭이나 닭갈비 같은 건 자주 먹어?",
          "original_form": "치킨 말고 찜닭이나 닭갈비 같은 건 자주 먹어?",
          "speaker_id": "2",
          "time": "20210825 15:17"
        },
        {
          "id": "MDRW2100013580.1.12",
          "form": "ㅋㅋㅋ글쿤 암튼 교촌 배달비 비싼건 인정ㅜ",
          "original_form": "ㅋㅋㅋ글쿤 암튼 교촌 배달비 비싼건 인정ㅜ",
          "speaker_id": "1",
          "time": "20210825 15:17"
        },
        {
          "id": "MDRW2100013580.1.13",
          "form": "찜닭은 주로 친구만날때?? 자주먹는듯",
          "original_form": "찜닭은 주로 친구만날때?? 자주먹는듯",
          "speaker_id": "1",
          "time": "20210825 15:17"
        },
        {
          "id": "MDRW2100013580.1.14",
          "form": "납작당면 너무좋아!!",
          "original_form": "납작당면 너무좋아!!",
          "speaker_id": "1",
          "time": "20210825 15:17"
        },
        {
          "id": "MDRW2100013580.1.15",
          "form": "오 ㅡㅋㅋㅋ 나랑 통했다 방금 그거 질문하려고 했거든",
          "original_form": "오 ㅡㅋㅋㅋ 나랑 통했다 방금 그거 질문하려고 했거든",
          "speaker_id": "2",
          "time": "20210825 15:17"
        },
        {
          "id": "MDRW2100013580.1.16",
          "form": "납작 당면 vs 가는 당면ㅋㅋㅋ",
          "original_form": "납작 당면 vs 가는 당면ㅋㅋㅋ",
