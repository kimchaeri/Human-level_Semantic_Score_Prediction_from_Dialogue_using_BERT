        },
        {
          "id": "MDRW2100013580.1.20",
          "form": "나도ㅋㅋㅋ 납작당면 먹으니까 가는당면이 맛없게 느껴져ㅜㅜ",
          "original_form": "나도ㅋㅋㅋ 납작당면 먹으니까 가는당면이 맛없게 느껴져ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 15:20"
