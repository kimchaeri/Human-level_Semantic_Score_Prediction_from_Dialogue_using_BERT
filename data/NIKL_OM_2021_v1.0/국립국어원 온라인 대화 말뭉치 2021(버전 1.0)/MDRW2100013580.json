          "time": "20210825 15:18"
        },
        {
          "id": "MDRW2100013580.1.17",
          "form": "당연 납작이지ㅋㅋㅋ",
          "original_form": "당연 납작이지ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:18"
