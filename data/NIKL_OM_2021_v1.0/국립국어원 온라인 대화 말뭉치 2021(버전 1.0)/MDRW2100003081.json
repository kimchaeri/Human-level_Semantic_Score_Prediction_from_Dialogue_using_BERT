      "utterance": [
        {
          "id": "MDRW2100003081.1.1",
          "form": "일 하시고 계세요?",
          "original_form": "일 하시고 계세요?",
          "speaker_id": "2",
          "time": "20210820 19:31"
        },
        {
          "id": "MDRW2100003081.1.2",
          "form": "앗늦게봤네용 네하고있어요~~",
          "original_form": "앗늦게봤네용 네하고있어요~~",
          "speaker_id": "1",
          "time": "20210820 20:02"
        },
        {
          "id": "MDRW2100003081.1.3",
          "form": "오 저두요",
          "original_form": "오 저두요",
          "speaker_id": "2",
          "time": "20210820 20:17"
        },
        {
          "id": "MDRW2100003081.1.4",
          "form": "일은 어떠세요?",
          "original_form": "일은 어떠세요?",
          "speaker_id": "2",
          "time": "20210820 20:17"
        },
        {
          "id": "MDRW2100003081.1.5",
          "form": "ㅠㅠ정말 하기싫지만 돈때문에 어쩔수없네요 모든 직장인들의 맘아닐까욬ㅋㅋ",
          "original_form": "ㅠㅠ정말 하기싫지만 돈때문에 어쩔수없네요 모든 직장인들의 맘아닐까욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 21:07"
        },
        {
          "id": "MDRW2100003081.1.6",
          "form": "ㅋㅋ 역시 저두요",
          "original_form": "ㅋㅋ 역시 저두요",
          "speaker_id": "2",
          "time": "20210820 21:11"
        },
        {
          "id": "MDRW2100003081.1.7",
          "form": "전 퇴사가 너무 하고싶어요",
          "original_form": "전 퇴사가 너무 하고싶어요",
          "speaker_id": "2",
          "time": "20210820 21:11"
        },
        {
          "id": "MDRW2100003081.1.8",
          "form": "저도요ㅠㅠ 그러려면 이직준비해야하는데 그 준비가 넘힘들것같아서 퇴사를 다시 생각하게되구요ㅠㅠ",
          "original_form": "저도요ㅠㅠ 그러려면 이직준비해야하는데 그 준비가 넘힘들것같아서 퇴사를 다시 생각하게되구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 21:12"
        },
        {
          "id": "MDRW2100003081.1.9",
          "form": "딜레마에 빠졌어요ㅠㅠ",
          "original_form": "딜레마에 빠졌어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 21:12"
        },
        {
          "id": "MDRW2100003081.1.10",
          "form": "아 그렇죠",
          "original_form": "아 그렇죠",
          "speaker_id": "2",
          "time": "20210820 21:14"
        },
        {
          "id": "MDRW2100003081.1.11",
          "form": "만약 전 이번에 퇴사 한다면",
          "original_form": "만약 전 이번에 퇴사 한다면",
          "speaker_id": "2",
          "time": "20210820 21:14"
        },
        {
          "id": "MDRW2100003081.1.12",
          "form": "일단 이직은 미뤄두고 할 거 같아요",
          "original_form": "일단 이직은 미뤄두고 할 거 같아요",
          "speaker_id": "2",
          "time": "20210820 21:14"
        },
        {
          "id": "MDRW2100003081.1.13",
          "form": "ㅠㅠ퇴사하면서 장기간 여행가고싶은데 코로나때문에 상황이 마땅치않네요",
          "original_form": "ㅠㅠ퇴사하면서 장기간 여행가고싶은데 코로나때문에 상황이 마땅치않네요",
          "speaker_id": "1",
          "time": "20210820 21:15"
        },
        {
          "id": "MDRW2100003081.1.14",
          "form": "그건 그래요 코로나땐 퇴사도 다들 말리잖아요",
          "original_form": "그건 그래요 코로나땐 퇴사도 다들 말리잖아요",
          "speaker_id": "2",
          "time": "20210820 21:16"
        },
        {
          "id": "MDRW2100003081.1.15",
          "form": "취업난이라 흑",
          "original_form": "취업난이라 흑",
          "speaker_id": "2",
          "time": "20210820 21:16"
        },
        {
          "id": "MDRW2100003081.1.16",
          "form": "ㅠㅠ정말 코로나가 퇴사를 말리네요ㅠㅠㅠ 더욱 상황이 어려워지는것같아요ㅠㅠ",
          "original_form": "ㅠㅠ정말 코로나가 퇴사를 말리네요ㅠㅠㅠ 더욱 상황이 어려워지는것같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 21:17"
        },
        {
          "id": "MDRW2100003081.1.17",
          "form": "맞아요 시기가 시기인만큼 고민이크죠 ㅠ",
          "original_form": "맞아요 시기가 시기인만큼 고민이크죠 ㅠ",
          "speaker_id": "2",
          "time": "20210820 21:19"
        },
        {
          "id": "MDRW2100003081.1.18",
          "form": "ㅠㅠ코로나가 빨리 없어져야 이직시장도 활발해질것 같아요ㅠㅠ",
          "original_form": "ㅠㅠ코로나가 빨리 없어져야 이직시장도 활발해질것 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 21:20"
        },
        {
          "id": "MDRW2100003081.1.19",
          "form": "맞아요 ㅠㅠㅠ 취업난이 해결좀...",
          "original_form": "맞아요 ㅠㅠㅠ 취업난이 해결좀...",
          "speaker_id": "2",
          "time": "20210820 21:21"
        }
      ]
