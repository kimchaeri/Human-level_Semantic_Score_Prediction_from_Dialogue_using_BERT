      "utterance": [
        {
          "id": "MDRW2100000675.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210819 17:33"
        },
        {
          "id": "MDRW2100000675.1.2",
          "form": "전 최근에 쿠팡으로 과일들을샀어요",
          "original_form": "전 최근에 쿠팡으로 과일들을샀어요",
          "speaker_id": "1",
          "time": "20210819 17:33"
        },
        {
          "id": "MDRW2100000675.1.3",
          "form": "오~ 그렇군요 쿠팡에 과일은 어땠나요",
          "original_form": "오~ 그렇군요 쿠팡에 과일은 어땠나요",
          "speaker_id": "2",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000675.1.4",
          "form": "새벽배송으로 와서좋았어요!품질도좋더라고요",
          "original_form": "새벽배송으로 와서좋았어요!품질도좋더라고요",
          "speaker_id": "1",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000675.1.5",
          "form": "ㅋㅋㅋ광고같네요",
          "original_form": "ㅋㅋㅋ광고같네요",
          "speaker_id": "1",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000675.1.6",
          "form": "광고 ㅋㅋ\r\n제가 사는 지역은 새벽배송이안되서 아쉽네요ㅜ",
          "original_form": "광고 ㅋㅋ\r\n제가 사는 지역은 새벽배송이안되서 아쉽네요ㅜ",
          "speaker_id": "2",
          "time": "20210819 17:46"
        },
        {
          "id": "MDRW2100000675.1.7",
          "form": "ㅜㅜ아진짜요? 아쉽네요",
          "original_form": "ㅜㅜ아진짜요? 아쉽네요",
          "speaker_id": "1",
          "time": "20210819 17:46"
        },
        {
          "id": "MDRW2100000675.1.8",
          "form": "쿠팡할인도많고좋은데",
          "original_form": "쿠팡할인도많고좋은데",
          "speaker_id": "1",
          "time": "20210819 17:47"
        },
        {
          "id": "MDRW2100000675.1.9",
          "form": "인터넷으로 산 과일 기타 등등 실패 많이해서 쉽게 구매 못하겠어요",
          "original_form": "인터넷으로 산 과일 기타 등등 실패 많이해서 쉽게 구매 못하겠어요",
          "speaker_id": "2",
          "time": "20210819 17:47"
        },
        {
          "id": "MDRW2100000675.1.10",
          "form": "저는 ㅋㅋ쇼핑직접가는거 귀찮아해서 걍 소셜이용해요",
          "original_form": "저는 ㅋㅋ쇼핑직접가는거 귀찮아해서 걍 소셜이용해요",
          "speaker_id": "1",
          "time": "20210819 17:47"
        },
        {
          "id": "MDRW2100000675.1.11",
          "form": "지마켓도 많이쓰구",
          "original_form": "지마켓도 많이쓰구",
          "speaker_id": "1",
          "time": "20210819 17:47"
        },
        {
          "id": "MDRW2100000675.1.12",
          "form": "그러고보니 지마켓 귤은 성공해봤네요",
          "original_form": "그러고보니 지마켓 귤은 성공해봤네요",
          "speaker_id": "2",
          "time": "20210819 17:47"
        },
        {
          "id": "MDRW2100000675.1.13",
          "form": "ㅎㅎ지마켓 스마일클럽가입하면 할인혜택많아유",
          "original_form": "ㅎㅎ지마켓 스마일클럽가입하면 할인혜택많아유",
          "speaker_id": "1",
          "time": "20210819 17:48"
        },
        {
          "id": "MDRW2100000675.1.14",
          "form": "추천추천",
          "original_form": "추천추천",
          "speaker_id": "1",
          "time": "20210819 17:48"
        },
        {
          "id": "MDRW2100000675.1.15",
          "form": "3년째 이용중이에요 ^^",
          "original_form": "3년째 이용중이에요 ^^",
          "speaker_id": "2",
          "time": "20210819 17:49"
        },
        {
          "id": "MDRW2100000675.1.16",
          "form": "인터넷 쇼핑없이는 살수가없어요~ㅋㅋ",
          "original_form": "인터넷 쇼핑없이는 살수가없어요~ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:50"
        },
        {
          "id": "MDRW2100000675.1.17",
          "form": "오늘도 4박스가 현관앞에 하하하",
          "original_form": "오늘도 4박스가 현관앞에 하하하",
          "speaker_id": "2",
          "time": "20210819 17:50"
        },
        {
          "id": "MDRW2100000675.1.18",
          "form": "ㅋㅋㄱㅋㅋㅋㅋㄱ와우",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋㄱ와우",
          "speaker_id": "1",
          "time": "20210819 17:50"
        },
        {
          "id": "MDRW2100000675.1.19",
          "form": "뭐사셨어요",
          "original_form": "뭐사셨어요",
          "speaker_id": "1",
          "time": "20210819 17:50"
        },
        {
          "id": "MDRW2100000675.1.20",
          "form": "쿠팡맨도 없는 지역이다보니  인터넷 쇼핑은 필수에요~",
          "original_form": "쿠팡맨도 없는 지역이다보니  인터넷 쇼핑은 필수에요~",
          "speaker_id": "2",
          "time": "20210819 17:50"
        },
        {
          "id": "MDRW2100000675.1.21",
          "form": "오늘은 홈쇼핑 영양밥과 쿠팡에서 아이 스티커와^^;;\r\n목욕타올이 왔어요",
          "original_form": "오늘은 홈쇼핑 영양밥과 쿠팡에서 아이 스티커와^^;;\r\n목욕타올이 왔어요",
          "speaker_id": "2",
          "time": "20210819 17:51"
        },
        {
          "id": "MDRW2100000675.1.22",
