      "utterance": [
        {
          "id": "MDRW2100033075.1.1",
          "form": "저는 안보지만..",
          "original_form": "저는 안보지만..",
          "speaker_id": "1",
          "time": "20210906 15:35"
        },
        {
          "id": "MDRW2100033075.1.2",
          "form": "오 스포츠!",
          "original_form": "오 스포츠!",
          "speaker_id": "2",
          "time": "20210906 15:38"
        },
        {
          "id": "MDRW2100033075.1.3",
          "form": "좋아하는 운동 경기 있으셍ㅅ?",
          "original_form": "좋아하는 운동 경기 있으셍ㅅ?",
          "speaker_id": "1",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100033075.1.4",
          "form": "요즘 다들 여자배구에 꽂히셨던데!",
          "original_form": "요즘 다들 여자배구에 꽂히셨던데!",
          "speaker_id": "1",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100033075.1.5",
          "form": "오홍 김연경선수 좋아해용",
          "original_form": "오홍 김연경선수 좋아해용",
          "speaker_id": "2",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100033075.1.6",
          "form": "올림픽 할때 열심히 보셨나요!",
          "original_form": "올림픽 할때 열심히 보셨나요!",
          "speaker_id": "1",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100033075.1.7",
          "form": "그정돈아니지만",
          "original_form": "그정돈아니지만",
          "speaker_id": "2",
          "time": "20210906 15:42"
        },
        {
          "id": "MDRW2100033075.1.8",
          "form": "양궁을 많이본거같아요",
          "original_form": "양궁을 많이본거같아요",
          "speaker_id": "2",
          "time": "20210906 15:42"
        },
        {
          "id": "MDRW2100033075.1.9",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:42"
        },
        {
          "id": "MDRW2100033075.1.10",
          "form": "저도 양궁 재미있게 봤어요",
          "original_form": "저도 양궁 재미있게 봤어요",
          "speaker_id": "1",
          "time": "20210906 15:47"
        },
        {
          "id": "MDRW2100033075.1.11",
          "form": "의외로 근대 5종도 저는 재밌더라구요!",
          "original_form": "의외로 근대 5종도 저는 재밌더라구요!",
          "speaker_id": "1",
          "time": "20210906 15:47"
        },
        {
          "id": "MDRW2100033075.1.12",
          "form": "5종은머죵",
          "original_form": "5종은머죵",
          "speaker_id": "2",
          "time": "20210906 16:07"
        },
        {
          "id": "MDRW2100033075.1.13",
          "form": "사격 펜싱 수영 승마?",
          "original_form": "사격 펜싱 수영 승마?",
          "speaker_id": "1",
          "time": "20210906 16:13"
        },
        {
          "id": "MDRW2100033075.1.14",
          "form": "한가지는 기억이 안나는데",
          "original_form": "한가지는 기억이 안나는데",
          "speaker_id": "1",
          "time": "20210906 16:13"
        },
        {
          "id": "MDRW2100033075.1.15",
          "form": "한 선수가 이 모든 종목을 다 하고 점수내서 메달 따는거요!",
          "original_form": "한 선수가 이 모든 종목을 다 하고 점수내서 메달 따는거요!",
          "speaker_id": "1",
          "time": "20210906 16:13"
        },
        {
          "id": "MDRW2100033075.1.16",
          "form": "헐랭",
          "original_form": "헐랭",
          "speaker_id": "2",
          "time": "20210906 16:26"
        },
        {
          "id": "MDRW2100033075.1.17",
          "form": "그사람은",
          "original_form": "그사람은",
          "speaker_id": "2",
          "time": "20210906 16:26"
        },
        {
          "id": "MDRW2100033075.1.18",
          "form": "다재다능이겠네요!!",
          "original_form": "다재다능이겠네요!!",
          "speaker_id": "2",
          "time": "20210906 16:26"
        },
        {
          "id": "MDRW2100033075.1.19",
          "form": "맞아요! 메달도 따는거보니까 너무 대단하더라구요!",
          "original_form": "맞아요! 메달도 따는거보니까 너무 대단하더라구요!",
          "speaker_id": "1",
          "time": "20210906 16:37"
        },
        {
          "id": "MDRW2100033075.1.20",
          "form": "멋지네요 어떠케 그럴수있징..",
          "original_form": "멋지네요 어떠케 그럴수있징..",
          "speaker_id": "2",
          "time": "20210906 18:23"
        },
        {
          "id": "MDRW2100033075.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:23"
        },
        {
          "id": "MDRW2100033075.1.22",
          "form": "그만큼 열심히 하시지않으셨을까요",
          "original_form": "그만큼 열심히 하시지않으셨을까요",
          "speaker_id": "1",
          "time": "20210906 19:16"
        },
        {
          "id": "MDRW2100033075.1.23",
          "form": "그러켔됴??",
          "original_form": "그러켔됴??",
          "speaker_id": "2",
          "time": "20210906 19:20"
        },
        {
          "id": "MDRW2100033075.1.24",
          "form": "그재능 저한테 1개만 ㅠㅠ",
          "original_form": "그재능 저한테 1개만 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 19:20"
        },
        {
          "id": "MDRW2100033075.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ진짜 그 재능 한개만 있어두",
          "original_form": "ㅋㅋㅋㅋㅋㅋ진짜 그 재능 한개만 있어두",
          "speaker_id": "1",
          "time": "20210906 20:45"
        },
        {
          "id": "MDRW2100033075.1.26",
          "form": "올림픽가능일거같아요",
          "original_form": "올림픽가능일거같아요",
          "speaker_id": "1",
          "time": "20210906 20:45"
        },
        {
          "id": "MDRW2100033075.1.27",
          "form": "ㅋㅋㅋㅋㅋ그러니까요",
          "original_form": "ㅋㅋㅋㅋㅋ그러니까요",
          "speaker_id": "2",
          "time": "20210907 08:02"
        },
        {
          "id": "MDRW2100033075.1.28",
          "form": "신기하네요...",
          "original_form": "신기하네요...",
          "speaker_id": "2",
          "time": "20210907 08:02"
        }
      ]
    }
