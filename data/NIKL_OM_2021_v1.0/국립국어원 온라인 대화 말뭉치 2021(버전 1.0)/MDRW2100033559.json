      "utterance": [
        {
          "id": "MDRW2100033559.1.1",
          "form": "좋아요!",
          "original_form": "좋아요!",
          "speaker_id": "1",
          "time": "20210824 14:16"
        },
        {
          "id": "MDRW2100033559.1.2",
          "form": "뭐 사고싶은 물건 있어",
          "original_form": "뭐 사고싶은 물건 있어",
          "speaker_id": "1",
          "time": "20210824 14:16"
        },
        {
          "id": "MDRW2100033559.1.3",
          "form": "요?",
          "original_form": "요?",
          "speaker_id": "1",
          "time": "20210824 14:16"
        },
        {
          "id": "MDRW2100033559.1.4",
          "form": "저는 아이패드.. 사고싶내요 ㅎ",
          "original_form": "저는 아이패드.. 사고싶내요 ㅎ",
          "speaker_id": "1",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100033559.1.5",
          "form": "어떤 용도로 쓰시게요?",
          "original_form": "어떤 용도로 쓰시게요?",
          "speaker_id": "2",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100033559.1.6",
          "form": "공부할때 되게 유용하게썻는대",
          "original_form": "공부할때 되게 유용하게썻는대",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100033559.1.7",
          "form": "이번에 액정이 나갔거든요 ㅠㅠ",
          "original_form": "이번에 액정이 나갔거든요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100033559.1.8",
          "form": "맞아요 필기할 때 편하더라고요",
          "original_form": "맞아요 필기할 때 편하더라고요",
          "speaker_id": "2",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100033559.1.9",
          "form": "저는 탭 쓰고 있어요",
          "original_form": "저는 탭 쓰고 있어요",
          "speaker_id": "2",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100033559.1.10",
          "form": "액정 수리 안 하시고 새로 사실 건가요?",
          "original_form": "액정 수리 안 하시고 새로 사실 건가요?",
          "speaker_id": "2",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100033559.1.11",
          "form": "속 lcd가 나갔는대",
          "original_form": "속 lcd가 나갔는대",
          "speaker_id": "1",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100033559.1.12",
          "form": "수리비가 60이래요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "수리비가 60이래요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100033559.1.13",
          "form": "꺅",
          "original_form": "꺅",
          "speaker_id": "2",
          "time": "20210824 14:26"
        },
        {
          "id": "MDRW2100033559.1.14",
          "form": "너무하네요",
          "original_form": "너무하네요",
          "speaker_id": "2",
          "time": "20210824 14:26"
        },
        {
          "id": "MDRW2100033559.1.15",
          "form": "패드 케이스 안 쓰셨나요?",
          "original_form": "패드 케이스 안 쓰셨나요?",
          "speaker_id": "2",
          "time": "20210824 14:27"
        },
        {
          "id": "MDRW2100033559.1.16",
          "form": "케이스 썼는데 ㅠ 어쩌다 꾹 눌린건지 겉은 멀쩡하고 안만 까매졌어요",
          "original_form": "케이스 썼는데 ㅠ 어쩌다 꾹 눌린건지 겉은 멀쩡하고 안만 까매졌어요",
          "speaker_id": "1",
          "time": "20210824 14:54"
        },
        {
          "id": "MDRW2100033559.1.17",
          "form": "저도 참 황당하내요 ㅠ",
          "original_form": "저도 참 황당하내요 ㅠ",
          "speaker_id": "1",
          "time": "20210824 14:54"
        },
        {
          "id": "MDRW2100033559.1.18",
          "form": "아이고ㅠㅠ 그래서 수리비가 비싸군요ㅠㅠ",
          "original_form": "아이고ㅠㅠ 그래서 수리비가 비싸군요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 14:55"
        },
        {
          "id": "MDRW2100033559.1.19",
          "form": "사면 같은 기종으로 사실 건가요?",
          "original_form": "사면 같은 기종으로 사실 건가요?",
          "speaker_id": "2",
          "time": "20210824 14:55"
        },
        {
          "id": "MDRW2100033559.1.20",
          "form": "제가 프로쓰는데",
          "original_form": "제가 프로쓰는데",
          "speaker_id": "1",
          "time": "20210824 19:39"
        },
        {
          "id": "MDRW2100033559.1.21",
          "form": "최신걸로 사도 가격에선 큰 차이가 없어서",
          "original_form": "최신걸로 사도 가격에선 큰 차이가 없어서",
          "speaker_id": "1",
          "time": "20210824 19:39"
        },
        {
          "id": "MDRW2100033559.1.22",
          "form": "신형으로 생각중이에요",
          "original_form": "신형으로 생각중이에요",
          "speaker_id": "1",
          "time": "20210824 19:39"
        },
        {
          "id": "MDRW2100033559.1.23",
          "form": "이왕이면 신형 사서 오래 쓰셔요ㅎㅎ",
          "original_form": "이왕이면 신형 사서 오래 쓰셔요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 19:40"
        },
        {
          "id": "MDRW2100033559.1.24",
          "form": "저도 처음에 몇 번 놓쳐서 떨어트리고",
          "original_form": "저도 처음에 몇 번 놓쳐서 떨어트리고",
          "speaker_id": "2",
          "time": "20210824 19:40"
        },
        {
          "id": "MDRW2100033559.1.25",
          "form": "예쁘진 않지만 튼튼한 케이스로 하나 장만했어요ㅠㅠ",
          "original_form": "예쁘진 않지만 튼튼한 케이스로 하나 장만했어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 19:41"
        },
        {
          "id": "MDRW2100033559.1.26",
          "form": "튼튼한개 최고죠 ㅠㅠ",
          "original_form": "튼튼한개 최고죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 19:44"
        },
        {
          "id": "MDRW2100033559.1.27",
          "form": "예쁜건 파우치로!",
          "original_form": "예쁜건 파우치로!",
          "speaker_id": "1",
          "time": "20210824 19:44"
        },
        {
          "id": "MDRW2100033559.1.28",
          "form": "맞아요! 요즘 파우치 예쁜 거 진짜 많아요",
          "original_form": "맞아요! 요즘 파우치 예쁜 거 진짜 많아요",
          "speaker_id": "2",
          "time": "20210824 19:47"
        },
        {
          "id": "MDRW2100033559.1.29",
          "form": "귀여운 캐릭터상품을 엄청많이 팔죠!",
          "original_form": "귀여운 캐릭터상품을 엄청많이 팔죠!",
          "speaker_id": "1",
          "time": "20210824 20:37"
        },
        {
          "id": "MDRW2100033559.1.30",
          "form": "스티커보면 지갑이 근질근질 ㅠㅠ",
          "original_form": "스티커보면 지갑이 근질근질 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 20:37"
        }
      ]
