      "utterance": [
        {
          "id": "MDRW2100000439.1.1",
          "form": "혹시 해리포터 좋아하세요??",
          "original_form": "혹시 해리포터 좋아하세요??",
          "speaker_id": "1",
          "time": "20210819 16:04"
        },
        {
          "id": "MDRW2100000439.1.2",
          "form": "너무 좋아해요",
          "original_form": "너무 좋아해요",
          "speaker_id": "2",
          "time": "20210819 16:05"
        },
        {
          "id": "MDRW2100000439.1.3",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210819 16:05"
        },
        {
          "id": "MDRW2100000439.1.4",
          "form": "당연하죠ㅠㅜ해리랑 같이 자랐어요ㅋㅋㅋㅋ",
          "original_form": "당연하죠ㅠㅜ해리랑 같이 자랐어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:05"
        },
        {
          "id": "MDRW2100000439.1.5",
          "form": "저는 마법사의 돌을 제일 좋아해요ㅋㅋ",
          "original_form": "저는 마법사의 돌을 제일 좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:05"
        },
        {
          "id": "MDRW2100000439.1.6",
          "form": "으엉 마법사의 돌",
          "original_form": "으엉 마법사의 돌",
          "speaker_id": "2",
          "time": "20210819 16:06"
        },
        {
          "id": "MDRW2100000439.1.7",
          "form": "진짜 오백번 본 것 같아요",
          "original_form": "진짜 오백번 본 것 같아요",
          "speaker_id": "2",
          "time": "20210819 16:06"
        },
        {
          "id": "MDRW2100000439.1.8",
          "form": "근데 저 반지의 제왕은 안 봤어요.",
          "original_form": "근데 저 반지의 제왕은 안 봤어요.",
          "speaker_id": "2",
          "time": "20210819 16:07"
        },
        {
          "id": "MDRW2100000439.1.9",
          "form": "혹시 보셨어요?",
          "original_form": "혹시 보셨어요?",
          "speaker_id": "2",
          "time": "20210819 16:07"
        },
        {
          "id": "MDRW2100000439.1.10",
          "form": "진짜요?ㅋㅋㅋ저는 두번정도 봤어요ㅋㅋㅋ",
          "original_form": "진짜요?ㅋㅋㅋ저는 두번정도 봤어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:07"
        },
        {
          "id": "MDRW2100000439.1.11",
          "form": "반지의 제왕은 책으로 읽은적은 없어요ㅜ",
          "original_form": "반지의 제왕은 책으로 읽은적은 없어요ㅜ",
          "speaker_id": "1",
          "time": "20210819 16:07"
        },
        {
          "id": "MDRW2100000439.1.12",
          "form": "요새 읽으시는 책 있으신가요? 저는 달까지 가자 읽어요ㅎㅎ",
          "original_form": "요새 읽으시는 책 있으신가요? 저는 달까지 가자 읽어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 16:09"
        },
        {
          "id": "MDRW2100000439.1.13",
          "form": "이상하게 반지의 제왕은 볼 때마다 졸아요",
          "original_form": "이상하게 반지의 제왕은 볼 때마다 졸아요",
          "speaker_id": "2",
          "time": "20210819 16:09"
        },
        {
          "id": "MDRW2100000439.1.14",
          "form": "저는 김초엽 씨 소설에 빠져서 읽고 있어요",
          "original_form": "저는 김초엽 씨 소설에 빠져서 읽고 있어요",
          "speaker_id": "2",
          "time": "20210819 16:09"
        },
        {
          "id": "MDRW2100000439.1.15",
          "form": "우와 어떤 소설인가요??",
          "original_form": "우와 어떤 소설인가요??",
          "speaker_id": "1",
          "time": "20210819 16:09"
        },
        {
          "id": "MDRW2100000439.1.16",
          "form": "SF소설인데",
          "original_form": "SF소설인데",
          "speaker_id": "2",
          "time": "20210819 16:10"
        },
        {
          "id": "MDRW2100000439.1.17",
          "form": "우리가 빛의 속도로 갈 수 없다면 이라는 책을 보고 반해서",
          "original_form": "우리가 빛의 속도로 갈 수 없다면 이라는 책을 보고 반해서",
          "speaker_id": "2",
          "time": "20210819 16:10"
        },
        {
          "id": "MDRW2100000439.1.18",
          "form": "이 작가님 책 도장깨기하고 있어요",
          "original_form": "이 작가님 책 도장깨기하고 있어요",
          "speaker_id": "2",
          "time": "20210819 16:10"
        },
        {
          "id": "MDRW2100000439.1.19",
          "form": "우와ㅋㅋㅋㅋㅋ판타지랑 sf 장르 좋아하시나봐요 혹시 달러구트 꿈백화점 안 보셨음 이것도 함 읽어보세요 재밌더라구요!",
          "original_form": "우와ㅋㅋㅋㅋㅋ판타지랑 sf 장르 좋아하시나봐요 혹시 달러구트 꿈백화점 안 보셨음 이것도 함 읽어보세요 재밌더라구요!",
          "speaker_id": "1",
          "time": "20210819 16:12"
        },
        {
          "id": "MDRW2100000439.1.20",
          "form": "오 감사해요!",
          "original_form": "오 감사해요!",
          "speaker_id": "2",
          "time": "20210819 16:14"
        },
        {
          "id": "MDRW2100000439.1.21",
          "form": "꼭 읽어 볼게요",
          "original_form": "꼭 읽어 볼게요",
          "speaker_id": "2",
          "time": "20210819 16:14"
        },
        {
          "id": "MDRW2100000439.1.22",
          "form": "책 읽는 것도 좋아하는데 사는 것도 좋아해요",
          "original_form": "책 읽는 것도 좋아하는데 사는 것도 좋아해요",
          "speaker_id": "2",
          "time": "20210819 16:14"
        },
        {
          "id": "MDRW2100000439.1.23",
          "form": "읽지도 않으면서 모으기ㅋㅋㅋㅋ",
          "original_form": "읽지도 않으면서 모으기ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:14"
        },
        {
          "id": "MDRW2100000439.1.24",
          "form": "ㅋㅋㅋㅋ저도 모아두는 걸 좋아해서 표지 마음에 들면 샀던 책이 꽤 있어요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ저도 모아두는 걸 좋아해서 표지 마음에 들면 샀던 책이 꽤 있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000439.1.25",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210819 16:21"
        },
        {
          "id": "MDRW2100000439.1.26",
          "form": "표지하니까 옛날 생각나네요",
          "original_form": "표지하니까 옛날 생각나네요",
          "speaker_id": "2",
          "time": "20210819 16:21"
        },
        {
          "id": "MDRW2100000439.1.27",
          "form": "잡지도 많이 샀어요",
          "original_form": "잡지도 많이 샀어요",
          "speaker_id": "2",
          "time": "20210819 16:21"
        },
        {
          "id": "MDRW2100000439.1.28",
          "form": "와ㅋㅋㅋㄱ저도요 초딩때 와와일공구 같은거 많이 샀었죠ㅋㅋ",
          "original_form": "와ㅋㅋㅋㄱ저도요 초딩때 와와일공구 같은거 많이 샀었죠ㅋㅋ",
