      "utterance": [
        {
          "id": "MDRW2100030568.1.1",
          "form": "근데 당직이면",
          "original_form": "근데 당직이면",
          "speaker_id": "1",
          "time": "20210904 09:41"
        },
        {
          "id": "MDRW2100030568.1.2",
          "form": "뭐하시나요",
          "original_form": "뭐하시나요",
          "speaker_id": "1",
          "time": "20210904 09:41"
        },
        {
          "id": "MDRW2100030568.1.3",
          "form": "숨쉬기인가요",
          "original_form": "숨쉬기인가요",
          "speaker_id": "1",
          "time": "20210904 09:41"
        },
        {
          "id": "MDRW2100030568.1.4",
          "form": "민원전화 받고 처리해줘요",
          "original_form": "민원전화 받고 처리해줘요",
          "speaker_id": "2",
          "time": "20210904 10:03"
        },
        {
          "id": "MDRW2100030568.1.5",
          "form": "오 공무원이시군요!!",
          "original_form": "오 공무원이시군요!!",
          "speaker_id": "1",
          "time": "20210904 10:04"
        },
        {
          "id": "MDRW2100030568.1.6",
          "form": "공기업입니다 ㅎㅎ",
          "original_form": "공기업입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 10:05"
        },
        {
          "id": "MDRW2100030568.1.7",
          "form": "오늘은 크게 전화가 없어서 다행입니다",
          "original_form": "오늘은 크게 전화가 없어서 다행입니다",
          "speaker_id": "2",
          "time": "20210904 10:05"
        },
        {
          "id": "MDRW2100030568.1.8",
          "form": "오잉 저도 공기업인데 ㅋㅋ",
          "original_form": "오잉 저도 공기업인데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:28"
        },
        {
          "id": "MDRW2100030568.1.9",
          "form": "공기업도 당직이 있군요",
          "original_form": "공기업도 당직이 있군요",
          "speaker_id": "1",
          "time": "20210904 10:28"
        },
        {
          "id": "MDRW2100030568.1.10",
          "form": "교통공사신가요? ㅋㅋ",
          "original_form": "교통공사신가요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 10:36"
        },
        {
          "id": "MDRW2100030568.1.11",
          "form": "네네 있습니다ㅠㅠ",
          "original_form": "네네 있습니다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 10:36"
        },
        {
          "id": "MDRW2100030568.1.12",
          "form": "네ㅋㅋ 고통공사입니다",
          "original_form": "네ㅋㅋ 고통공사입니다",
          "speaker_id": "1",
          "time": "20210904 13:59"
        },
        {
          "id": "MDRW2100030568.1.13",
          "form": "ㅋㅋㅋㅋ그러시구나",
          "original_form": "ㅋㅋㅋㅋ그러시구나",
          "speaker_id": "2",
          "time": "20210904 14:13"
        },
        {
          "id": "MDRW2100030568.1.14",
          "form": "같이일하는 직원들은 어떠세요",
          "original_form": "같이일하는 직원들은 어떠세요",
          "speaker_id": "2",
          "time": "20210904 14:14"
        },
        {
          "id": "MDRW2100030568.1.15",
          "form": "ㅋㅋ괜찮은편이에요",
          "original_form": "ㅋㅋ괜찮은편이에요",
          "speaker_id": "1",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100030568.1.16",
          "form": "저희는 케바케에요 ㅋㅋㅋ",
          "original_form": "저희는 케바케에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100030568.1.17",
          "form": "왜나면 우리역에는 늙은이들이 적거든요",
          "original_form": "왜나면 우리역에는 늙은이들이 적거든요",
          "speaker_id": "1",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100030568.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100030568.1.19",
          "form": "나도안잘리지만 쟤도 안잘려서 짜중나요",
          "original_form": "나도안잘리지만 쟤도 안잘려서 짜중나요",
          "speaker_id": "2",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100030568.1.20",
          "form": "저흰 또래가 더해요",
          "original_form": "저흰 또래가 더해요",
          "speaker_id": "2",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100030568.1.21",
          "form": "동기들이 더하답니다",
          "original_form": "동기들이 더하답니다",
          "speaker_id": "2",
          "time": "20210904 15:48"
        },
        {
          "id": "MDRW2100030568.1.22",
          "form": "허허 왜 그런대요",
          "original_form": "허허 왜 그런대요",
          "speaker_id": "1",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100030568.1.23",
          "form": "몰라요 재택근무 다녀오니 제 서랍을 열어놨네요",
          "original_form": "몰라요 재택근무 다녀오니 제 서랍을 열어놨네요",
          "speaker_id": "2",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100030568.1.24",
          "form": "ㅋㅋ진짜 이상한 양반들일세",
          "original_form": "ㅋㅋ진짜 이상한 양반들일세",
          "speaker_id": "1",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100030568.1.25",
          "form": "그쵸 ㅋㅋ보안 걸리면 책임 질건지",
          "original_form": "그쵸 ㅋㅋ보안 걸리면 책임 질건지",
          "speaker_id": "2",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100030568.1.26",
          "form": "이렁게 젤 스트레스에요",
          "original_form": "이렁게 젤 스트레스에요",
          "speaker_id": "2",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100030568.1.27",
          "form": "별거아닌걸로",
          "original_form": "별거아닌걸로",
          "speaker_id": "2",
          "time": "20210904 15:51"
        }
      ]
    }
  ]
