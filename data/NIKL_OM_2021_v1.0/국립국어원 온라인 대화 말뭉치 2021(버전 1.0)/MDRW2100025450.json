      "utterance": [
        {
          "id": "MDRW2100025450.1.1",
          "form": "싸이월드해보셨어요?",
          "original_form": "싸이월드해보셨어요?",
          "speaker_id": "1",
          "time": "20210901 11:34"
        },
        {
          "id": "MDRW2100025450.1.2",
          "form": "싸이월드 하셨나요?",
          "original_form": "싸이월드 하셨나요?",
          "speaker_id": "2",
          "time": "20210901 11:34"
        },
        {
          "id": "MDRW2100025450.1.3",
          "form": "전에 했었죠",
          "original_form": "전에 했었죠",
          "speaker_id": "2",
          "time": "20210901 11:34"
        },
        {
          "id": "MDRW2100025450.1.4",
          "form": "오래전 전의 흑염룡..",
          "original_form": "오래전 전의 흑염룡..",
          "speaker_id": "2",
          "time": "20210901 11:34"
        },
        {
          "id": "MDRW2100025450.1.5",
          "form": "네  전 주로 음악을 샀죠ㅋㅋ",
          "original_form": "네  전 주로 음악을 샀죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:34"
        },
        {
          "id": "MDRW2100025450.1.6",
          "form": "후후",
          "original_form": "후후",
          "speaker_id": "2",
          "time": "20210901 11:34"
        },
        {
          "id": "MDRW2100025450.1.7",
          "form": "크",
          "original_form": "크",
          "speaker_id": "1",
          "time": "20210901 11:34"
        },
        {
          "id": "MDRW2100025450.1.8",
          "form": "사진 부활 두렵네요",
          "original_form": "사진 부활 두렵네요",
          "speaker_id": "2",
          "time": "20210901 11:34"
        },
        {
          "id": "MDRW2100025450.1.9",
          "form": "복원한다고 하면",
          "original_form": "복원한다고 하면",
          "speaker_id": "1",
          "time": "20210901 11:34"
        },
        {
          "id": "MDRW2100025450.1.10",
          "form": "여럿이 무서워할듯..",
          "original_form": "여럿이 무서워할듯..",
          "speaker_id": "1",
          "time": "20210901 11:34"
        },
        {
          "id": "MDRW2100025450.1.11",
          "form": "복원된거죠?",
          "original_form": "복원된거죠?",
          "speaker_id": "2",
          "time": "20210901 11:35"
        },
        {
          "id": "MDRW2100025450.1.12",
          "form": "확인을 안해봐서",
          "original_form": "확인을 안해봐서",
          "speaker_id": "2",
          "time": "20210901 11:35"
        },
        {
          "id": "MDRW2100025450.1.13",
          "form": "할머니 살아계실때 사진이 거기 있어서",
          "original_form": "할머니 살아계실때 사진이 거기 있어서",
          "speaker_id": "2",
          "time": "20210901 11:35"
        },
        {
          "id": "MDRW2100025450.1.14",
          "form": "다시 들어가보고싶긴하네요",
          "original_form": "다시 들어가보고싶긴하네요",
          "speaker_id": "2",
          "time": "20210901 11:35"
        },
        {
          "id": "MDRW2100025450.1.15",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210901 11:35"
        },
        {
          "id": "MDRW2100025450.1.16",
          "form": "그건 좋네요ㅠ",
          "original_form": "그건 좋네요ㅠ",
          "speaker_id": "1",
          "time": "20210901 11:35"
        },
        {
          "id": "MDRW2100025450.1.17",
          "form": "복원이 되나요?",
          "original_form": "복원이 되나요?",
          "speaker_id": "1",
          "time": "20210901 11:36"
        },
        {
          "id": "MDRW2100025450.1.18",
          "form": "추억이 담겨 있으니까요",
          "original_form": "추억이 담겨 있으니까요",
          "speaker_id": "2",
          "time": "20210901 11:36"
        },
        {
          "id": "MDRW2100025450.1.19",
          "form": "두려워서 안열어봤습니다",
          "original_form": "두려워서 안열어봤습니다",
          "speaker_id": "2",
          "time": "20210901 11:36"
        },
        {
          "id": "MDRW2100025450.1.20",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:36"
        },
        {
          "id": "MDRW2100025450.1.21",
          "form": "전 탈퇴했어서ㅜㅜ",
          "original_form": "전 탈퇴했어서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 11:37"
        },
        {
          "id": "MDRW2100025450.1.22",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210901 11:39"
        },
        {
          "id": "MDRW2100025450.1.23",
          "form": "음악 나오고 아바타 꾸미고",
          "original_form": "음악 나오고 아바타 꾸미고",
          "speaker_id": "2",
          "time": "20210901 11:39"
        },
        {
          "id": "MDRW2100025450.1.24",
          "form": "잼났었는데 그죠",
          "original_form": "잼났었는데 그죠",
          "speaker_id": "2",
          "time": "20210901 11:39"
        },
        {
          "id": "MDRW2100025450.1.25",
          "form": "노래가 다들 좋았어요",
          "original_form": "노래가 다들 좋았어요",
          "speaker_id": "1",
          "time": "20210901 11:41"
        },
        {
          "id": "MDRW2100025450.1.26",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210901 11:41"
        }
      ]
