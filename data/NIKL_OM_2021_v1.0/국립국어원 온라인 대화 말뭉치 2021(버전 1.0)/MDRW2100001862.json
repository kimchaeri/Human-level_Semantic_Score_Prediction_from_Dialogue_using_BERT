      "utterance": [
        {
          "id": "MDRW2100001862.1.1",
          "form": "베트남 가봤어요?",
          "original_form": "베트남 가봤어요?",
          "speaker_id": "2",
          "time": "20210819 21:47"
        },
        {
          "id": "MDRW2100001862.1.2",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210819 21:48"
        },
        {
          "id": "MDRW2100001862.1.3",
          "form": "제 동생이 가서 이야기는 해줬어요",
          "original_form": "제 동생이 가서 이야기는 해줬어요",
          "speaker_id": "1",
          "time": "20210819 21:48"
        },
        {
          "id": "MDRW2100001862.1.4",
          "form": "아주 저렴하게 럭셔리 갔다올수 있어좋아요",
          "original_form": "아주 저렴하게 럭셔리 갔다올수 있어좋아요",
          "speaker_id": "2",
          "time": "20210819 21:48"
        },
        {
          "id": "MDRW2100001862.1.5",
          "form": "동생은 베트남 음식이 잘 안 맞았대요",
          "original_form": "동생은 베트남 음식이 잘 안 맞았대요",
          "speaker_id": "1",
          "time": "20210819 21:49"
        },
        {
          "id": "MDRW2100001862.1.6",
          "form": "베트남 하니깐 쌀국수 생각나네요",
          "original_form": "베트남 하니깐 쌀국수 생각나네요",
          "speaker_id": "1",
          "time": "20210819 21:50"
        },
        {
          "id": "MDRW2100001862.1.7",
          "form": "쌀국수 맛있겠다",
          "original_form": "쌀국수 맛있겠다",
          "speaker_id": "1",
          "time": "20210819 21:50"
        },
        {
          "id": "MDRW2100001862.1.8",
          "form": "맛집 검색해서 가봤는데 괜찮았어요",
          "original_form": "맛집 검색해서 가봤는데 괜찮았어요",
          "speaker_id": "2",
          "time": "20210819 21:51"
        },
        {
          "id": "MDRW2100001862.1.9",
          "form": "현지 맛집은 다르겠죠?",
          "original_form": "현지 맛집은 다르겠죠?",
          "speaker_id": "1",
          "time": "20210819 21:53"
        },
        {
          "id": "MDRW2100001862.1.10",
          "form": "물가가 싸서 좋겠더라구요",
          "original_form": "물가가 싸서 좋겠더라구요",
          "speaker_id": "1",
          "time": "20210819 21:53"
        },
        {
          "id": "MDRW2100001862.1.11",
          "form": "스위스나 북유럽 생각하면 천국",
          "original_form": "스위스나 북유럽 생각하면 천국",
          "speaker_id": "1",
          "time": "20210819 21:53"
        },
        {
          "id": "MDRW2100001862.1.12",
          "form": "물가가 싸서 가는거 같아요",
          "original_form": "물가가 싸서 가는거 같아요",
          "speaker_id": "2",
          "time": "20210819 21:54"
        },
        {
          "id": "MDRW2100001862.1.13",
          "form": "가깝기도 하구요",
          "original_form": "가깝기도 하구요",
          "speaker_id": "1",
          "time": "20210819 21:55"
        },
        {
          "id": "MDRW2100001862.1.14",
          "form": "맞아요 비행기로 몇시간 안걸리니",
          "original_form": "맞아요 비행기로 몇시간 안걸리니",
          "speaker_id": "2",
          "time": "20210819 21:55"
        },
        {
          "id": "MDRW2100001862.1.15",
          "form": "베트남에 뭐 있더라",
          "original_form": "베트남에 뭐 있더라",
          "speaker_id": "1",
          "time": "20210819 21:58"
        },
        {
          "id": "MDRW2100001862.1.16",
          "form": "생각나는게 쌀국수네",
          "original_form": "생각나는게 쌀국수네",
          "speaker_id": "1",
          "time": "20210819 21:58"
        },
        {
          "id": "MDRW2100001862.1.17",
          "form": "콩커피",
          "original_form": "콩커피",
          "speaker_id": "2",
          "time": "20210819 21:58"
        },
        {
          "id": "MDRW2100001862.1.18",
          "form": "맞다 오토바이 많지 않아요?",
          "original_form": "맞다 오토바이 많지 않아요?",
          "speaker_id": "1",
          "time": "20210819 21:58"
        },
        {
          "id": "MDRW2100001862.1.19",
          "form": "커피도 유명하던데",
          "original_form": "커피도 유명하던데",
          "speaker_id": "1",
          "time": "20210819 21:58"
        },
        {
          "id": "MDRW2100001862.1.20",
          "form": "엄청 많아요 거기다 무질서",
          "original_form": "엄청 많아요 거기다 무질서",
          "speaker_id": "2",
          "time": "20210819 21:58"
        },
        {
          "id": "MDRW2100001862.1.21",
          "form": "동생이 사온적 있어요 기억난다",
          "original_form": "동생이 사온적 있어요 기억난다",
          "speaker_id": "1",
          "time": "20210819 21:58"
        },
        {
          "id": "MDRW2100001862.1.22",
          "form": "무질서예요?",
          "original_form": "무질서예요?",
          "speaker_id": "1",
          "time": "20210819 21:58"
        },
        {
          "id": "MDRW2100001862.1.23",
          "form": "완전 시장통이겠다",
          "original_form": "완전 시장통이겠다",
          "speaker_id": "1",
          "time": "20210819 21:59"
        },
        {
          "id": "MDRW2100001862.1.24",
          "form": "차보다 더 우선요 저러다 사고 날까봐 놀랬어요",
          "original_form": "차보다 더 우선요 저러다 사고 날까봐 놀랬어요",
          "speaker_id": "2",
          "time": "20210819 22:00"
        },
        {
          "id": "MDRW2100001862.1.25",
          "form": "교통이 엉망진창이구만",
          "original_form": "교통이 엉망진창이구만",
          "speaker_id": "1",
          "time": "20210819 22:01"
        },
        {
          "id": "MDRW2100001862.1.26",
          "form": "베트남은 베트남 사람이랑 전화로 일해본 적만 있어서",
          "original_form": "베트남은 베트남 사람이랑 전화로 일해본 적만 있어서",
          "speaker_id": "1",
          "time": "20210819 22:02"
        },
        {
          "id": "MDRW2100001862.1.27",
          "form": "네 신호도 안지키고",
          "original_form": "네 신호도 안지키고",
          "speaker_id": "2",
          "time": "20210819 22:02"
        },
        {
          "id": "MDRW2100001862.1.28",
          "form": "근데 한번은 가고싶네요",
          "original_form": "근데 한번은 가고싶네요",
          "speaker_id": "1",
          "time": "20210819 22:02"
        },
        {
          "id": "MDRW2100001862.1.29",
          "form": "우기때말고 가세요",
          "original_form": "우기때말고 가세요",
          "speaker_id": "2",
          "time": "20210819 22:02"
        },
        {
          "id": "MDRW2100001862.1.30",
          "form": "우기때 갔나요?",
          "original_form": "우기때 갔나요?",
          "speaker_id": "1",
          "time": "20210819 22:05"
        },
        {
          "id": "MDRW2100001862.1.31",
          "form": "비슷한 시기에 가서 과일도 맛없고 그랬어요",
          "original_form": "비슷한 시기에 가서 과일도 맛없고 그랬어요",
          "speaker_id": "2",
          "time": "20210819 22:06"
        },
        {
          "id": "MDRW2100001862.1.32",
          "form": "베트남도 동남아라서 과일 괜찮을텐데 영 시원찮았나봐요",
          "original_form": "베트남도 동남아라서 과일 괜찮을텐데 영 시원찮았나봐요",
          "speaker_id": "1",
          "time": "20210819 22:07"
        },
        {
          "id": "MDRW2100001862.1.33",
          "form": "당도가 떨어지더라고요",
          "original_form": "당도가 떨어지더라고요",
          "speaker_id": "2",
          "time": "20210819 22:07"
        }
      ]
    }
