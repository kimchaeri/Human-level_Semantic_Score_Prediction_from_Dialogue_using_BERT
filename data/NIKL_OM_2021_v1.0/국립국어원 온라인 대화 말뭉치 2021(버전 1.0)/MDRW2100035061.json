      "utterance": [
        {
          "id": "MDRW2100035061.1.1",
          "form": "고민 있니",
          "original_form": "고민 있니",
          "speaker_id": "2",
          "time": "20210829 01:41"
        },
        {
          "id": "MDRW2100035061.1.2",
          "form": "인턴 때문에 고민이야",
          "original_form": "인턴 때문에 고민이야",
          "speaker_id": "1",
          "time": "20210829 01:41"
        },
        {
          "id": "MDRW2100035061.1.3",
          "form": "인턴 되고 나서 좋아했잖아",
          "original_form": "인턴 되고 나서 좋아했잖아",
          "speaker_id": "2",
          "time": "20210829 01:41"
        },
        {
          "id": "MDRW2100035061.1.4",
          "form": "스트레스 받는 일이 생겼어",
          "original_form": "스트레스 받는 일이 생겼어",
          "speaker_id": "1",
          "time": "20210829 01:41"
        },
        {
          "id": "MDRW2100035061.1.5",
          "form": "어떤 것인지 말해봐",
          "original_form": "어떤 것인지 말해봐",
          "speaker_id": "2",
          "time": "20210829 01:41"
        },
        {
          "id": "MDRW2100035061.1.6",
          "form": "직장 선배가 나를 안 좋아해",
          "original_form": "직장 선배가 나를 안 좋아해",
          "speaker_id": "1",
          "time": "20210829 01:41"
        },
        {
          "id": "MDRW2100035061.1.7",
          "form": "대체 왜 그러지",
          "original_form": "대체 왜 그러지",
          "speaker_id": "2",
          "time": "20210829 01:42"
        },
        {
          "id": "MDRW2100035061.1.8",
          "form": "나를 일 시키는 게 귀찮다고 생각하는 것 같아",
          "original_form": "나를 일 시키는 게 귀찮다고 생각하는 것 같아",
          "speaker_id": "1",
          "time": "20210829 01:42"
        },
        {
          "id": "MDRW2100035061.1.9",
          "form": "그치만 크게 사수가 해야 할 일인걸",
          "original_form": "그치만 크게 사수가 해야 할 일인걸",
          "speaker_id": "2",
          "time": "20210829 01:42"
        },
        {
          "id": "MDRW2100035061.1.10",
          "form": "나도 그렇게 생각해",
          "original_form": "나도 그렇게 생각해",
          "speaker_id": "1",
          "time": "20210829 01:42"
        },
        {
          "id": "MDRW2100035061.1.11",
          "form": "스트레스가 이만저만이 아니겠다",
          "original_form": "스트레스가 이만저만이 아니겠다",
          "speaker_id": "2",
          "time": "20210829 01:42"
        },
        {
          "id": "MDRW2100035061.1.12",
          "form": "자꾸 눈치를 보게 돼",
          "original_form": "자꾸 눈치를 보게 돼",
          "speaker_id": "1",
          "time": "20210829 01:42"
        },
        {
          "id": "MDRW2100035061.1.13",
          "form": "내가 도울 수 있는 게 있다면 좋을 텐데",
          "original_form": "내가 도울 수 있는 게 있다면 좋을 텐데",
          "speaker_id": "2",
          "time": "20210829 01:42"
        },
        {
          "id": "MDRW2100035061.1.14",
          "form": "말만으로도 고맙다",
          "original_form": "말만으로도 고맙다",
          "speaker_id": "1",
          "time": "20210829 01:43"
        }
