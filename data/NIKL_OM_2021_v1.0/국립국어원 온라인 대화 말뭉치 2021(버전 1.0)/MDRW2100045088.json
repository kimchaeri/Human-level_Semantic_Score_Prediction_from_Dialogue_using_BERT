      "utterance": [
        {
          "id": "MDRW2100045088.1.1",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045088.1.2",
          "form": "비타500도 받아서 먹었어요",
          "original_form": "비타500도 받아서 먹었어요",
          "speaker_id": "1",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045088.1.3",
          "form": "맛난거 먹고 영양제는 따로 챙기면",
          "original_form": "맛난거 먹고 영양제는 따로 챙기면",
          "speaker_id": "2",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045088.1.4",
          "form": "그것도 방법이죠",
          "original_form": "그것도 방법이죠",
          "speaker_id": "2",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045088.1.5",
          "form": "어디서 뭘 그렇게 받아 먹어요?",
          "original_form": "어디서 뭘 그렇게 받아 먹어요?",
          "speaker_id": "2",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045088.1.6",
          "form": "진짜 비타민이 들어있긴 할까요?",
          "original_form": "진짜 비타민이 들어있긴 할까요?",
          "speaker_id": "1",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045088.1.7",
          "form": "그냥 알바에서 받았어요",
          "original_form": "그냥 알바에서 받았어요",
          "speaker_id": "1",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045088.1.8",
          "form": "들어있긴 하죠",
          "original_form": "들어있긴 하죠",
          "speaker_id": "2",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045088.1.9",
          "form": "오 그럼 건강해지긴 했겠네",
          "original_form": "오 그럼 건강해지긴 했겠네",
          "speaker_id": "1",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045088.1.10",
          "form": "500만큼 건강해지는 음료 비타 500",
          "original_form": "500만큼 건강해지는 음료 비타 500",
          "speaker_id": "2",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045088.1.11",
          "form": "저도 오늘 마셨는데",
          "original_form": "저도 오늘 마셨는데",
          "speaker_id": "2",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045088.1.12",
          "form": "으...",
          "original_form": "으...",
          "speaker_id": "1",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045088.1.13",
          "form": "요즘은 박카스에이드도 많이 먹잖아요",
          "original_form": "요즘은 박카스에이드도 많이 먹잖아요",
          "speaker_id": "1",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045088.1.14",
          "form": "얼싸박?",
          "original_form": "얼싸박?",
          "speaker_id": "1",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045088.1.15",
          "form": "건강해지세요",
          "original_form": "건강해지세요",
          "speaker_id": "1",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045088.1.16",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045088.1.17",
          "form": "그거랑 박카스 젤리도 있잖아요",
          "original_form": "그거랑 박카스 젤리도 있잖아요",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045088.1.18",
          "form": "먹어보셨어요??",
          "original_form": "먹어보셨어요??",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045088.1.19",
          "form": "네 맛없어요",
          "original_form": "네 맛없어요",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045088.1.20",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045088.1.21",
          "form": "그냥... 영양제 주제에 먹을 만 하네 정도",
          "original_form": "그냥... 영양제 주제에 먹을 만 하네 정도",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045088.1.22",
          "form": "다른 젤리들도 많던데",
          "original_form": "다른 젤리들도 많던데",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045088.1.23",
          "form": "요구르트젤리?",
          "original_form": "요구르트젤리?",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045088.1.24",
          "form": "그냥 영양제 젤리가 더 맛있음",
          "original_form": "그냥 영양제 젤리가 더 맛있음",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045088.1.25",
          "form": "그것도 맛 별로...",
          "original_form": "그것도 맛 별로...",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045088.1.26",
          "form": "아뇨 딸기맛느낌",
          "original_form": "아뇨 딸기맛느낌",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045088.1.27",
          "form": "텐텐처럼요!",
          "original_form": "텐텐처럼요!",
          "speaker_id": "1",
          "time": "20210908 00:51"
        }
      ]
    }
