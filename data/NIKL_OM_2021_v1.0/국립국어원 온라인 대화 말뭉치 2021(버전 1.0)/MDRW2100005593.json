      "utterance": [
        {
          "id": "MDRW2100005593.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210821 23:13"
        },
        {
          "id": "MDRW2100005593.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210821 23:13"
        },
        {
          "id": "MDRW2100005593.1.3",
          "form": "온앤오프팬이신가요ㅋㅋ",
          "original_form": "온앤오프팬이신가요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:13"
        },
        {
          "id": "MDRW2100005593.1.4",
          "form": "어떤일하세요?",
          "original_form": "어떤일하세요?",
          "speaker_id": "1",
          "time": "20210821 23:14"
        },
        {
          "id": "MDRW2100005593.1.5",
          "form": "앗ㅋㅋㅋ맞아요",
          "original_form": "앗ㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005593.1.6",
          "form": "저는 그냥 학생입니다!",
          "original_form": "저는 그냥 학생입니다!",
          "speaker_id": "2",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005593.1.7",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005593.1.8",
          "form": "저는 직장인이에요",
          "original_form": "저는 직장인이에요",
          "speaker_id": "1",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005593.1.9",
          "form": "알바하세여?",
          "original_form": "알바하세여?",
          "speaker_id": "1",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005593.1.10",
          "form": "아뇨 알바 구해볼까 하는데 아직 안 하고 있네요...ㅋㅋㅋㅜㅜ",
          "original_form": "아뇨 알바 구해볼까 하는데 아직 안 하고 있네요...ㅋㅋㅋㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005593.1.11",
          "form": "알바 구하기 어렵다고하더라고요ㅜㅠ",
          "original_form": "알바 구하기 어렵다고하더라고요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005593.1.12",
          "form": "맞아요 예전에 방학때 몇 군데 연락해봤는데 다 이미 구했다고 해서... 그 뒤로 귀찮아서 그냥 포기했어요ㅋㅋㅋ",
          "original_form": "맞아요 예전에 방학때 몇 군데 연락해봤는데 다 이미 구했다고 해서... 그 뒤로 귀찮아서 그냥 포기했어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005593.1.13",
          "form": "ㅜㅜㅜㅜ대학생이신거져",
          "original_form": "ㅜㅜㅜㅜ대학생이신거져",
          "speaker_id": "1",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005593.1.14",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "2",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005593.1.15",
          "form": "취업 준비하기 힘드시겠어요ㅠ",
          "original_form": "취업 준비하기 힘드시겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005593.1.16",
          "form": "맞아요 취준... 저는 아직은 안 하는데 힘들 것 같아요ㅠㅠ",
          "original_form": "맞아요 취준... 저는 아직은 안 하는데 힘들 것 같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 23:22"
        },
        {
          "id": "MDRW2100005593.1.17",
          "form": "취준 힘드셨나요?ㅠㅠ",
          "original_form": "취준 힘드셨나요?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 23:22"
        },
        {
          "id": "MDRW2100005593.1.18",
          "form": "저는 준비 많이 안하고",
          "original_form": "저는 준비 많이 안하고",
          "speaker_id": "1",
          "time": "20210821 23:23"
        },
        {
          "id": "MDRW2100005593.1.19",
          "form": "운좋게 돼서",
          "original_form": "운좋게 돼서",
          "speaker_id": "1",
          "time": "20210821 23:23"
        },
        {
          "id": "MDRW2100005593.1.20",
          "form": "그렇게힘들지않았어요..",
          "original_form": "그렇게힘들지않았어요..",
          "speaker_id": "1",
          "time": "20210821 23:23"
        },
        {
          "id": "MDRW2100005593.1.21",
          "form": "헐 멋있으세요...",
          "original_form": "헐 멋있으세요...",
          "speaker_id": "2",
          "time": "20210821 23:24"
        },
        {
          "id": "MDRW2100005593.1.22",
          "form": "저도 그랬으면...",
          "original_form": "저도 그랬으면...",
          "speaker_id": "2",
          "time": "20210821 23:24"
        }
      ]
