        },
        {
          "id": "MDRW2100033951.1.3",
          "form": "구매 예정인 물건 있어?",
          "original_form": "구매 예정인 물건 있어?",
          "speaker_id": "2",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100033951.1.4",
          "form": "예정은 아닌데",
          "original_form": "예정은 아닌데",
          "speaker_id": "1",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100033951.1.5",
          "form": "스피커나 헤드셋 사고 싶어",
          "original_form": "스피커나 헤드셋 사고 싶어",
          "speaker_id": "1",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100033951.1.6",
          "form": "아 좋지~",
          "original_form": "아 좋지~",
          "speaker_id": "2",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100033951.1.7",
          "form": "봐둔 모델 있어?",
          "original_form": "봐둔 모델 있어?",
          "speaker_id": "2",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100033951.1.8",
          "form": "아직 안 찾아봤어",
          "original_form": "아직 안 찾아봤어",
          "speaker_id": "1",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100033951.1.9",
          "form": "그냥 생각만ㅋㅋㅋ",
          "original_form": "그냥 생각만ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100033951.1.10",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100033951.1.11",
          "form": "나 운동화 하나 살까 생각중",
          "original_form": "나 운동화 하나 살까 생각중",
          "speaker_id": "2",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100033951.1.12",
          "form": "오 운동화!",
          "original_form": "오 운동화!",
          "speaker_id": "1",
          "time": "20210825 16:35"
        },
        {
          "id": "MDRW2100033951.1.13",
          "form": "어떤 브랜드?",
          "original_form": "어떤 브랜드?",
          "speaker_id": "1",
          "time": "20210825 16:36"
        },
        {
          "id": "MDRW2100033951.1.14",
          "form": "아디다스나 휠라",
          "original_form": "아디다스나 휠라",
          "speaker_id": "2",
          "time": "20210825 16:36"
        },
        {
          "id": "MDRW2100033951.1.15",
          "form": "너는 어디 꺼 신어?",
          "original_form": "너는 어디 꺼 신어?",
          "speaker_id": "2",
          "time": "20210825 16:36"
        },
        {
          "id": "MDRW2100033951.1.16",
          "form": "나는 휠라",
          "original_form": "나는 휠라",
          "speaker_id": "1",
          "time": "20210825 16:36"
        },
        {
          "id": "MDRW2100033951.1.17",
          "form": "휠라 괜찮아?",
          "original_form": "휠라 괜찮아?",
          "speaker_id": "2",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100033951.1.18",
          "form": "응 디자인도 꽤 예뻐",
