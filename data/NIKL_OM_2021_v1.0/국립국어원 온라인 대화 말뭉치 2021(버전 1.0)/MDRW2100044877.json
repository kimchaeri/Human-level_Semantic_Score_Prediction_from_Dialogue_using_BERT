      "utterance": [
        {
          "id": "MDRW2100044877.1.1",
          "form": "구매희망물건있으서요?",
          "original_form": "구매희망물건있으서요?",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044877.1.2",
          "form": "저 오늘 램프랑 거울 샀어요!!",
          "original_form": "저 오늘 램프랑 거울 샀어요!!",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044877.1.3",
          "form": "아직도 남은 위시리스트는 가을 이불이요!!",
          "original_form": "아직도 남은 위시리스트는 가을 이불이요!!",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044877.1.4",
          "form": "램프?",
          "original_form": "램프?",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044877.1.5",
          "form": "향기나는거요?",
          "original_form": "향기나는거요?",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044877.1.6",
          "form": "아 그거말고 조명이요!!",
          "original_form": "아 그거말고 조명이요!!",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044877.1.7",
          "form": "탁상 조명ㅁ",
          "original_form": "탁상 조명ㅁ",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044877.1.8",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044877.1.9",
          "form": "전 실반지샀어요",
          "original_form": "전 실반지샀어요",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044877.1.10",
          "form": "우와!! 저도 반지는 실반지 좋아해요 ㅋㅋㅋㅋㅋ",
          "original_form": "우와!! 저도 반지는 실반지 좋아해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100044877.1.11",
          "form": "두꺼운건 제 손에 안어울려서ㅜㅜ",
          "original_form": "두꺼운건 제 손에 안어울려서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100044877.1.12",
          "form": "전 실반지가 안 어울리는데",
          "original_form": "전 실반지가 안 어울리는데",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100044877.1.13",
          "form": "사봤어요ㅋㅋㅋㅋ",
          "original_form": "사봤어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100044877.1.14",
          "form": "마디가 두꺼워서 반지는 다 안어울려요,",
          "original_form": "마디가 두꺼워서 반지는 다 안어울려요,",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100044877.1.15",
          "form": "저는 손가락이 짧아서 반지 잘안끼긴해여 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 손가락이 짧아서 반지 잘안끼긴해여 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100044877.1.16",
          "form": "그래도 악세서리는 ㅋㅋㅋㅋ안해도 사게되는 마력이 있죠 ㅋㅋㅋㅋㅋ",
          "original_form": "그래도 악세서리는 ㅋㅋㅋㅋ안해도 사게되는 마력이 있죠 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100044877.1.17",
          "form": "마자용ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "마자용ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100044877.1.18",
          "form": "귀고리도 사고싶은게 많아요",
          "original_form": "귀고리도 사고싶은게 많아요",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100044877.1.19",
          "form": "저는 귀도 안뚫었거든요? 근데 종종 귀걸이 구경합니다 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 귀도 안뚫었거든요? 근데 종종 귀걸이 구경합니다 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100044877.1.20",
          "form": "ㄱㅋㅋㅋㅋ액서사리는 묘한 마력이 있어요",
          "original_form": "ㄱㅋㅋㅋㅋ액서사리는 묘한 마력이 있어요",
          "speaker_id": "2",
          "time": "20210908 01:12"
        },
        {
          "id": "MDRW2100044877.1.21",
          "form": "그니깐요 ㅋㅋㅋㅋㅋ 막상 사고 잘 하지도 않는데ㅜㅜ ㅋㅋㅋㅋㅋ",
          "original_form": "그니깐요 ㅋㅋㅋㅋㅋ 막상 사고 잘 하지도 않는데ㅜㅜ ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100044877.1.22",
          "form": "어떤종류의 귀고리를 주로 구매하시나요?",
          "original_form": "어떤종류의 귀고리를 주로 구매하시나요?",
          "speaker_id": "2",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100044877.1.23",
          "form": "아 귀를 안뚫어서 구경만하고 구매는 안하구요 ㅋㅋㅋㅋㅋ이어커프는 몇개 샀어요!!! ㅋㅋㅋ",
          "original_form": "아 귀를 안뚫어서 구경만하고 구매는 안하구요 ㅋㅋㅋㅋㅋ이어커프는 몇개 샀어요!!! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100044877.1.24",
          "form": "예전에 귀찌도 사봤는데 그건 귀가 너무 아프더라구요",
          "original_form": "예전에 귀찌도 사봤는데 그건 귀가 너무 아프더라구요",
          "speaker_id": "1",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100044877.1.25",
          "form": "전 귀를 뚫기는 해서 이거저거 하는데",
          "original_form": "전 귀를 뚫기는 해서 이거저거 하는데",
          "speaker_id": "2",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100044877.1.26",
          "form": "아 이어커프는 저도 하고싶어요",
          "original_form": "아 이어커프는 저도 하고싶어요",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100044877.1.27",
          "form": "귀찌 많이 아픈가요?",
          "original_form": "귀찌 많이 아픈가요?",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100044877.1.28",
          "form": "네 ㅜㅜ귀찌는 그냥 진짜 귀에 압력이라 ㅜㅜ너무 아파서 머리까지 아프더라구욬ㅋㅋㅋㅋ",
          "original_form": "네 ㅜㅜ귀찌는 그냥 진짜 귀에 압력이라 ㅜㅜ너무 아파서 머리까지 아프더라구욬ㅋㅋㅋㅋ",
