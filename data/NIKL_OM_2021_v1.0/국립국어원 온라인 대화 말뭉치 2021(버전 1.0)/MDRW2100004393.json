        },
        {
          "id": "MDRW2100004393.1.2",
          "form": "최근에 재밌게 본 책 있으신가요?",
          "original_form": "최근에 재밌게 본 책 있으신가요?",
          "speaker_id": "1",
          "time": "20210821 16:15"
        },
        {
          "id": "MDRW2100004393.1.3",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004393.1.4",
          "form": "여름고래라는 책",
          "original_form": "여름고래라는 책",
          "speaker_id": "2",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004393.1.5",
          "form": "굉장히 즐겁게 봤어요",
          "original_form": "굉장히 즐겁게 봤어요",
          "speaker_id": "2",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004393.1.6",
          "form": "츠지무라 미즈키 작가님 작품인데",
          "original_form": "츠지무라 미즈키 작가님 작품인데",
          "speaker_id": "2",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004393.1.7",
          "form": "문체가 취향이더라고요",
          "original_form": "문체가 취향이더라고요",
          "speaker_id": "2",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004393.1.8",
          "form": "혹시 얼음 고래인가요?",
          "original_form": "혹시 얼음 고래인가요?",
          "speaker_id": "1",
          "time": "20210821 16:18"
        },
        {
          "id": "MDRW2100004393.1.9",
          "form": "검색해봤는데 안 나와서요ㅠㅠ",
          "original_form": "검색해봤는데 안 나와서요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 16:18"
        },
        {
          "id": "MDRW2100004393.1.10",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:21"
        },
        {
          "id": "MDRW2100004393.1.11",
          "form": "이럴수가",
          "original_form": "이럴수가",
          "speaker_id": "2",
          "time": "20210821 16:21"
        },
        {
          "id": "MDRW2100004393.1.12",
          "form": "제가 잘못 말씀드렸네요",
          "original_form": "제가 잘못 말씀드렸네요",
          "speaker_id": "2",
          "time": "20210821 16:21"
        },
        {
          "id": "MDRW2100004393.1.13",
          "form": "얼음고래가 맞아요",
          "original_form": "얼음고래가 맞아요",
          "speaker_id": "2",
          "time": "20210821 16:21"
        },
        {
          "id": "MDRW2100004393.1.14",
          "form": "여름 분위기 낭낭한 책인데",
          "original_form": "여름 분위기 낭낭한 책인데",
          "speaker_id": "2",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004393.1.15",
          "form": "그생각을 하다보니",
          "original_form": "그생각을 하다보니",
          "speaker_id": "2",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004393.1.16",
          "form": "여름고래라고 쳐버린것같아요",
          "original_form": "여름고래라고 쳐버린것같아요",
          "speaker_id": "2",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004393.1.17",
          "form": "여름분위기가 낭낭!! 너무 좋아서 꼭 읽어보고 싶네요",
          "original_form": "여름분위기가 낭낭!! 너무 좋아서 꼭 읽어보고 싶네요",
          "speaker_id": "1",
          "time": "20210821 16:23"
        },
        {
          "id": "MDRW2100004393.1.18",
          "form": "근데 일본문학 특유의 느낌이 강해서",
          "original_form": "근데 일본문학 특유의 느낌이 강해서",
          "speaker_id": "2",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004393.1.19",
          "form": "호불호가 갈릴 순 있을것같아요",
          "original_form": "호불호가 갈릴 순 있을것같아요",
          "speaker_id": "2",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004393.1.20",
          "form": "name1님은 최근 재밌게 본 도서",
          "original_form": "&name1&님은 최근 재밌게 본 도서",
          "speaker_id": "2",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004393.1.21",
          "form": "추천해주실래요?",
          "original_form": "추천해주실래요?",
          "speaker_id": "2",
          "time": "20210821 16:24"
        },
        {
          "id": "MDRW2100004393.1.22",
          "form": "자연 과학 도서인데 마이크브라운 작가의  <나는 어쩌다 명왕성을 죽였나> 재밌게 봤어요",
          "original_form": "자연 과학 도서인데 마이크브라운 작가의  <나는 어쩌다 명왕성을 죽였나> 재밌게 봤어요",
          "speaker_id": "1",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004393.1.23",
          "form": "자연 과학 도서인데 내용이 어렵지 않고",
          "original_form": "자연 과학 도서인데 내용이 어렵지 않고",
          "speaker_id": "1",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004393.1.24",
          "form": "다큐멘터리 보는 것처럼 술술 읽히더라고요",
          "original_form": "다큐멘터리 보는 것처럼 술술 읽히더라고요",
          "speaker_id": "1",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004393.1.25",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004393.1.26",
          "form": "재밌을것같아요",
          "original_form": "재밌을것같아요",
          "speaker_id": "2",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004393.1.27",
          "form": "자연 과학 도서가 어렵지 않다니",
          "original_form": "자연 과학 도서가 어렵지 않다니",
          "speaker_id": "2",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004393.1.28",
          "form": "신기해서라도 읽어봐야겠내요",
          "original_form": "신기해서라도 읽어봐야겠내요",
          "speaker_id": "2",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004393.1.29",
          "form": "선생님한테 재밌는 이야기 해주세요~하고 옆에서 이야기 듣는 거 같은 책이었어요. 크게 어려운 용어도 없었고 재밌었어요",
          "original_form": "선생님한테 재밌는 이야기 해주세요~하고 옆에서 이야기 듣는 거 같은 책이었어요. 크게 어려운 용어도 없었고 재밌었어요",
          "speaker_id": "1",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004393.1.30",
          "form": "술술 읽힐것 같네요",
          "original_form": "술술 읽힐것 같네요",
          "speaker_id": "2",
          "time": "20210821 16:33"
        },
        {
          "id": "MDRW2100004393.1.31",
          "form": "안그래도 곧 도서관 가려했는데",
          "original_form": "안그래도 곧 도서관 가려했는데",
          "speaker_id": "2",
          "time": "20210821 16:33"
        },
        {
          "id": "MDRW2100004393.1.32",
          "form": "빌려서 봐야겠어요",
          "original_form": "빌려서 봐야겠어요",
          "speaker_id": "2",
          "time": "20210821 16:33"
        },
        {
          "id": "MDRW2100004393.1.33",
          "form": "참 마시멜로 이야기라는 책 아세요?",
          "original_form": "참 마시멜로 이야기라는 책 아세요?",
          "speaker_id": "2",
          "time": "20210821 16:33"
        },
        {
          "id": "MDRW2100004393.1.34",
          "form": "방금 찾아봤는데 표지가 참 예쁘네요",
          "original_form": "방금 찾아봤는데 표지가 참 예쁘네요",
          "speaker_id": "1",
          "time": "20210821 16:36"
        },
        {
          "id": "MDRW2100004393.1.35",
          "form": "마시멜로 실험과 관련이 있나요?",
          "original_form": "마시멜로 실험과 관련이 있나요?",
          "speaker_id": "1",
          "time": "20210821 16:36"
        },
        {
          "id": "MDRW2100004393.1.36",
          "form": "마시멜로 실험을 이야기하면서",
          "original_form": "마시멜로 실험을 이야기하면서",
          "speaker_id": "2",
          "time": "20210821 16:39"
        },
        {
          "id": "MDRW2100004393.1.37",
          "form": "성공을 위한 교훈을 들려주는 내용이에요",
          "original_form": "성공을 위한 교훈을 들려주는 내용이에요",
          "speaker_id": "2",
          "time": "20210821 16:40"
        },
        {
          "id": "MDRW2100004393.1.38",
          "form": "청소년때 참 좋아하던 책이라",
          "original_form": "청소년때 참 좋아하던 책이라",
          "speaker_id": "2",
          "time": "20210821 16:40"
        },
        {
          "id": "MDRW2100004393.1.39",
          "form": "생각이 났네요",
          "original_form": "생각이 났네요",
          "speaker_id": "2",
          "time": "20210821 16:40"
        },
        {
          "id": "MDRW2100004393.1.40",
          "form": "저는 중학생 때 저스트어 모멘트 정말 재밌게 읽었어요",
          "original_form": "저는 중학생 때 저스트어 모멘트 정말 재밌게 읽었어요",
          "speaker_id": "1",
          "time": "20210821 16:44"
        },
        {
          "id": "MDRW2100004393.1.41",
          "form": "잔잔하게 교훈을 주는 책들이 필독 도서로 많았는데",
          "original_form": "잔잔하게 교훈을 주는 책들이 필독 도서로 많았는데",
          "speaker_id": "1",
          "time": "20210821 16:44"
        },
        {
          "id": "MDRW2100004393.1.42",
          "form": "기억에 오래 남는 거 같아요",
          "original_form": "기억에 오래 남는 거 같아요",
          "speaker_id": "1",
          "time": "20210821 16:44"
        },
        {
          "id": "MDRW2100004393.1.43",
          "form": "오 읽어본 적 없는 책이네요",
          "original_form": "오 읽어본 적 없는 책이네요",
          "speaker_id": "2",
          "time": "20210821 16:45"
        },
        {
          "id": "MDRW2100004393.1.44",
          "form": "그것도 한 번 읽어봐야겠어요",
          "original_form": "그것도 한 번 읽어봐야겠어요",
          "speaker_id": "2",
          "time": "20210821 16:45"
        },
        {
          "id": "MDRW2100004393.1.45",
          "form": "책 추천 감사해요 ㅎㅎ",
          "original_form": "책 추천 감사해요 ㅎㅎ",
          "speaker_id": "2",
