      "utterance": [
        {
          "id": "MDRW2100031056.1.1",
          "form": "동물 키우세요?",
          "original_form": "동물 키우세요?",
          "speaker_id": "1",
          "time": "20210904 15:58"
        },
        {
          "id": "MDRW2100031056.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210904 15:58"
        },
        {
          "id": "MDRW2100031056.1.3",
          "form": "요증  넘 귀여운게 많아서",
          "original_form": "요증  넘 귀여운게 많아서",
          "speaker_id": "2",
          "time": "20210904 15:59"
        },
        {
          "id": "MDRW2100031056.1.4",
          "form": "저도 맨날 생각만해요ㅎ",
          "original_form": "저도 맨날 생각만해요ㅎ",
          "speaker_id": "1",
          "time": "20210904 15:59"
        },
        {
          "id": "MDRW2100031056.1.5",
          "form": "키울까 생각은하는데",
          "original_form": "키울까 생각은하는데",
          "speaker_id": "2",
          "time": "20210904 15:59"
        },
        {
          "id": "MDRW2100031056.1.6",
          "form": "그쵸 너무 귀여워요",
          "original_form": "그쵸 너무 귀여워요",
          "speaker_id": "1",
          "time": "20210904 15:59"
        },
        {
          "id": "MDRW2100031056.1.7",
          "form": "공동주택이라 안될거같아요",
          "original_form": "공동주택이라 안될거같아요",
          "speaker_id": "2",
          "time": "20210904 15:59"
        },
        {
          "id": "MDRW2100031056.1.8",
          "form": "어떤 종 좋아하세요?",
          "original_form": "어떤 종 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 15:59"
        },
        {
          "id": "MDRW2100031056.1.9",
          "form": "포메라니안이요ㅋㅋ",
          "original_form": "포메라니안이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:59"
        },
        {
          "id": "MDRW2100031056.1.10",
          "form": "안좋아할수가없는 외모더라구요",
          "original_form": "안좋아할수가없는 외모더라구요",
          "speaker_id": "2",
          "time": "20210904 16:00"
        },
        {
          "id": "MDRW2100031056.1.11",
          "form": "포메 귀엽죠",
          "original_form": "포메 귀엽죠",
          "speaker_id": "1",
          "time": "20210904 16:00"
        },
        {
          "id": "MDRW2100031056.1.12",
          "form": "저는 고양이 키우고싶어요",
          "original_form": "저는 고양이 키우고싶어요",
          "speaker_id": "1",
          "time": "20210904 16:00"
        },
        {
          "id": "MDRW2100031056.1.13",
          "form": "귀여운 외모에 버르장머리가 없더라구요",
          "original_form": "귀여운 외모에 버르장머리가 없더라구요",
          "speaker_id": "2",
          "time": "20210904 16:00"
        },
        {
          "id": "MDRW2100031056.1.14",
          "form": "ㅎㅎ 그건 포메나 고양이 공통이네요",
          "original_form": "ㅎㅎ 그건 포메나 고양이 공통이네요",
          "speaker_id": "1",
          "time": "20210904 16:00"
        },
        {
          "id": "MDRW2100031056.1.15",
          "form": "저희 옆집도 포메기르는데",
          "original_form": "저희 옆집도 포메기르는데",
          "speaker_id": "1",
          "time": "20210904 16:01"
        },
        {
          "id": "MDRW2100031056.1.16",
          "form": "어쩜 이쁜것들은 다들 도도한지",
          "original_form": "어쩜 이쁜것들은 다들 도도한지",
          "speaker_id": "2",
          "time": "20210904 16:01"
        },
        {
          "id": "MDRW2100031056.1.17",
          "form": "사람 지나가면 엄청 짖어요",
          "original_form": "사람 지나가면 엄청 짖어요",
          "speaker_id": "1",
          "time": "20210904 16:01"
        },
        {
          "id": "MDRW2100031056.1.18",
          "form": "그래도 보면 귀엽긴하네요ㅎ",
          "original_form": "그래도 보면 귀엽긴하네요ㅎ",
          "speaker_id": "1",
          "time": "20210904 16:01"
        }
      ]
