      "utterance": [
        {
          "id": "MDRW2100024786.1.1",
          "form": "ㅎㅎ 물론이지",
          "original_form": "ㅎㅎ 물론이지",
          "speaker_id": "2",
          "time": "20210831 18:37"
        },
        {
          "id": "MDRW2100024786.1.2",
          "form": "ㅋㅋㅋㅋ그렇지 인생의낙은",
          "original_form": "ㅋㅋㅋㅋ그렇지 인생의낙은",
          "speaker_id": "1",
          "time": "20210831 18:37"
        },
        {
          "id": "MDRW2100024786.1.3",
          "form": "칼퇴뿐이지",
          "original_form": "칼퇴뿐이지",
          "speaker_id": "1",
          "time": "20210831 18:38"
        },
        {
          "id": "MDRW2100024786.1.4",
          "form": "그러고보니 너희는 청소 어케해?",
          "original_form": "그러고보니 너희는 청소 어케해?",
          "speaker_id": "2",
          "time": "20210831 18:38"
        },
        {
          "id": "MDRW2100024786.1.5",
          "form": "옹",
          "original_form": "옹",
          "speaker_id": "1",
          "time": "20210831 18:38"
        },
        {
          "id": "MDRW2100024786.1.6",
          "form": "큰 곳이라 따로 청소업체 고용하지?",
          "original_form": "큰 곳이라 따로 청소업체 고용하지?",
          "speaker_id": "2",
          "time": "20210831 18:38"
        },
        {
          "id": "MDRW2100024786.1.7",
          "form": "기본적인 건물 화장실 이런데는",
          "original_form": "기본적인 건물 화장실 이런데는",
          "speaker_id": "1",
          "time": "20210831 18:38"
        },
        {
          "id": "MDRW2100024786.1.8",
          "form": "고용하고",
          "original_form": "고용하고",
          "speaker_id": "1",
          "time": "20210831 18:38"
        },
        {
          "id": "MDRW2100024786.1.9",
          "form": "개인좌석 여자휴게실",
          "original_form": "개인좌석 여자휴게실",
          "speaker_id": "1",
          "time": "20210831 18:38"
        },
        {
          "id": "MDRW2100024786.1.10",
          "form": "이런데는 개인/당번",
          "original_form": "이런데는 개인/당번",
          "speaker_id": "1",
          "time": "20210831 18:39"
        },
        {
          "id": "MDRW2100024786.1.11",
          "form": "정해져있음",
          "original_form": "정해져있음",
          "speaker_id": "1",
          "time": "20210831 18:39"
        },
        {
          "id": "MDRW2100024786.1.12",
          "form": "너희는?",
          "original_form": "너희는?",
          "speaker_id": "1",
          "time": "20210831 18:39"
        },
        {
          "id": "MDRW2100024786.1.13",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210831 18:39"
        },
        {
          "id": "MDRW2100024786.1.14",
          "form": "우리는 건물 자체는 진흥원 건물이다보니",
          "original_form": "우리는 건물 자체는 진흥원 건물이다보니",
          "speaker_id": "2",
          "time": "20210831 18:39"
        },
        {
          "id": "MDRW2100024786.1.15",
          "form": "화장실이 공용이라 관리하시는 분들이 따로 계시고",
          "original_form": "화장실이 공용이라 관리하시는 분들이 따로 계시고",
          "speaker_id": "2",
          "time": "20210831 18:39"
        },
        {
          "id": "MDRW2100024786.1.16",
          "form": "사무실은 주 1회 아침회의 끝나고 다같이 ㅇㅇ",
          "original_form": "사무실은 주 1회 아침회의 끝나고 다같이 ㅇㅇ",
          "speaker_id": "2",
          "time": "20210831 18:40"
        },
        {
          "id": "MDRW2100024786.1.17",
          "form": "뭐 비슷하네ㅋㅋㅋ",
          "original_form": "뭐 비슷하네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:41"
        },
        {
          "id": "MDRW2100024786.1.18",
          "form": "너희는 대표들",
          "original_form": "너희는 대표들",
          "speaker_id": "1",
          "time": "20210831 18:41"
        },
        {
          "id": "MDRW2100024786.1.19",
          "form": "포트 물떠주고 이런거안하지?",
          "original_form": "포트 물떠주고 이런거안하지?",
          "speaker_id": "1",
          "time": "20210831 18:41"
        },
        {
          "id": "MDRW2100024786.1.20",
          "form": "ㅇㅇㅇㅇ 그런 해줘야하는 건 없어",
          "original_form": "ㅇㅇㅇㅇ 그런 해줘야하는 건 없어",
          "speaker_id": "2",
          "time": "20210831 18:44"
        },
        {
          "id": "MDRW2100024786.1.21",
          "form": "그니까 수평적인데가",
          "original_form": "그니까 수평적인데가",
          "speaker_id": "1",
          "time": "20210831 18:44"
        },
        {
          "id": "MDRW2100024786.1.22",
          "form": "그런게좋아",
          "original_form": "그런게좋아",
          "speaker_id": "1",
          "time": "20210831 18:44"
        },
        {
          "id": "MDRW2100024786.1.23",
          "form": "으휴 아직도 그런 꼰문화가 남아있다니",
          "original_form": "으휴 아직도 그런 꼰문화가 남아있다니",
          "speaker_id": "2",
          "time": "20210831 18:45"
        },
        {
          "id": "MDRW2100024786.1.24",
          "form": "자기가 먹을 물은 자기가 떠와랏",
          "original_form": "자기가 먹을 물은 자기가 떠와랏",
          "speaker_id": "2",
          "time": "20210831 18:45"
        },
        {
          "id": "MDRW2100024786.1.25",
          "form": "전무가 예전에",
          "original_form": "전무가 예전에",
          "speaker_id": "1",
          "time": "20210831 18:45"
        },
        {
          "id": "MDRW2100024786.1.26",
          "form": "등산문화?한달에한번",
          "original_form": "등산문화?한달에한번",
          "speaker_id": "1",
          "time": "20210831 18:46"
        },
        {
          "id": "MDRW2100024786.1.27",
          "form": "회식 일주일한번",
          "original_form": "회식 일주일한번",
          "speaker_id": "1",
          "time": "20210831 18:46"
        },
        {
          "id": "MDRW2100024786.1.28",
          "form": "이런식으로하고",
          "original_form": "이런식으로하고",
          "speaker_id": "1",
          "time": "20210831 18:46"
        },
        {
          "id": "MDRW2100024786.1.29",
          "form": "등반 끝까지못한사람들",
          "original_form": "등반 끝까지못한사람들",
          "speaker_id": "1",
          "time": "20210831 18:46"
        },
        {
          "id": "MDRW2100024786.1.30",
          "form": "막 너희가 이래서안된다느니",
          "original_form": "막 너희가 이래서안된다느니",
          "speaker_id": "1",
          "time": "20210831 18:46"
        },
        {
          "id": "MDRW2100024786.1.31",
          "form": "으 미쳤다",
          "original_form": "으 미쳤다",
          "speaker_id": "2",
          "time": "20210831 18:46"
        },
        {
          "id": "MDRW2100024786.1.32",
          "form": "완전체네",
          "original_form": "완전체네",
          "speaker_id": "2",
          "time": "20210831 18:46"
        },
        {
          "id": "MDRW2100024786.1.33",
          "form": "회식 하지말고 가족이랑 외식이나 해",
          "original_form": "회식 하지말고 가족이랑 외식이나 해",
          "speaker_id": "2",
          "time": "20210831 18:47"
        },
        {
          "id": "MDRW2100024786.1.34",
          "form": "ㄹㅇ진자",
          "original_form": "ㄹㅇ진자",
          "speaker_id": "1",
          "time": "20210831 18:49"
        },
