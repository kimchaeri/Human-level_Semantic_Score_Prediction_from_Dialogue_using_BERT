      "utterance": [
        {
          "id": "MDRW2100008684.1.1",
          "form": "좋아하는 음식 있으세여?",
          "original_form": "좋아하는 음식 있으세여?",
          "speaker_id": "1",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008684.1.2",
          "form": "저는 라면 좋아해요",
          "original_form": "저는 라면 좋아해요",
          "speaker_id": "2",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100008684.1.3",
          "form": "어떤 라면 가장 좋아하세요?",
          "original_form": "어떤 라면 가장 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 19:06"
        },
        {
          "id": "MDRW2100008684.1.4",
          "form": "신라면이랑 불닭 좋아해요",
          "original_form": "신라면이랑 불닭 좋아해요",
          "speaker_id": "2",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008684.1.5",
          "form": "베이직 중에서도 젤 베이직 스타일ㅎㅎ",
          "original_form": "베이직 중에서도 젤 베이직 스타일ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 19:24"
        },
        {
          "id": "MDRW2100008684.1.6",
          "form": "전 매운 걸 못 먹어서 튀김우동이나 진라면순한맛 좋아해요ㅎㅎ",
          "original_form": "전 매운 걸 못 먹어서 튀김우동이나 진라면순한맛 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 20:37"
        },
        {
          "id": "MDRW2100008684.1.7",
          "form": "불닭은 한가닥 먹고 너무 힘들었어요ㅠㅠ",
          "original_form": "불닭은 한가닥 먹고 너무 힘들었어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 20:38"
        },
        {
          "id": "MDRW2100008684.1.8",
          "form": "저도 매운걸 잘먹는 편은 아니라 눈물 흘리면서 먹어요",
          "original_form": "저도 매운걸 잘먹는 편은 아니라 눈물 흘리면서 먹어요",
          "speaker_id": "2",
          "time": "20210823 00:02"
        },
        {
          "id": "MDRW2100008684.1.9",
          "form": "맞아요 그런 분들 계세요ㅎㄹ",
          "original_form": "맞아요 그런 분들 계세요ㅎㄹ",
          "speaker_id": "1",
          "time": "20210823 01:04"
        },
        {
          "id": "MDRW2100008684.1.10",
          "form": "매운 거 먹으면 스트레스가 풀린다더라고요",
          "original_form": "매운 거 먹으면 스트레스가 풀린다더라고요",
          "speaker_id": "1",
          "time": "20210823 01:04"
        },
        {
          "id": "MDRW2100008684.1.11",
          "form": "맞아요 학업 스트레스 매운거로 풀어요 저는",
          "original_form": "맞아요 학업 스트레스 매운거로 풀어요 저는",
          "speaker_id": "2",
          "time": "20210823 08:07"
        },
        {
          "id": "MDRW2100008684.1.12",
          "form": "그러시구나ㅜㅜ 신라면 볶음면 드셔보셨어요?",
          "original_form": "그러시구나ㅜㅜ 신라면 볶음면 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210823 10:43"
        },
        {
          "id": "MDRW2100008684.1.13",
          "form": "아직이요 혹시 드셔보셨나요",
          "original_form": "아직이요 혹시 드셔보셨나요",
          "speaker_id": "2",
          "time": "20210823 10:51"
        },
        {
          "id": "MDRW2100008684.1.14",
          "form": "아니요 매울가봐 엄두도 못 내고 있어요",
          "original_form": "아니요 매울가봐 엄두도 못 내고 있어요",
          "speaker_id": "1",
          "time": "20210823 11:50"
        },
        {
          "id": "MDRW2100008684.1.15",
          "form": "다음번에 장보러 나갈 때 사와야겠네요",
          "original_form": "다음번에 장보러 나갈 때 사와야겠네요",
          "speaker_id": "2",
          "time": "20210823 11:51"
        },
        {
          "id": "MDRW2100008684.1.16",
          "form": "봉지가 더 맛있대요!",
          "original_form": "봉지가 더 맛있대요!",
          "speaker_id": "1",
          "time": "20210823 11:53"
        },
        {
          "id": "MDRW2100008684.1.17",
          "form": "아하 좋은 정보 감사합니다",
          "original_form": "아하 좋은 정보 감사합니다",
          "speaker_id": "2",
          "time": "20210823 11:54"
        }
      ]
