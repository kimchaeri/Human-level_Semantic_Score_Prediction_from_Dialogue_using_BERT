      "utterance": [
        {
          "id": "MDRW2100003612.1.1",
          "form": "가장 최근 자동차극장에서\r\n본 영화는 뭐에요?",
          "original_form": "가장 최근 자동차극장에서\r\n본 영화는 뭐에요?",
          "speaker_id": "1",
          "time": "20210821 08:26"
        },
        {
          "id": "MDRW2100003612.1.2",
          "form": "저 보스베이비2요 ㅋㄱㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저 보스베이비2요 ㅋㄱㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 08:27"
        },
        {
          "id": "MDRW2100003612.1.3",
          "form": "재미있게 보셨나요?",
          "original_form": "재미있게 보셨나요?",
          "speaker_id": "1",
          "time": "20210821 08:27"
        },
        {
          "id": "MDRW2100003612.1.4",
          "form": "어느 분의 픽인가요?ㅋㅋ",
          "original_form": "어느 분의 픽인가요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 08:27"
        },
        {
          "id": "MDRW2100003612.1.5",
          "form": "픽이 아니라",
          "original_form": "픽이 아니라",
          "speaker_id": "2",
          "time": "20210821 08:28"
        },
        {
          "id": "MDRW2100003612.1.6",
          "form": "자동차극장이 작아서",
          "original_form": "자동차극장이 작아서",
          "speaker_id": "2",
          "time": "20210821 08:28"
        },
        {
          "id": "MDRW2100003612.1.7",
          "form": "상영하는게 그것뿐...",
          "original_form": "상영하는게 그것뿐...",
          "speaker_id": "2",
          "time": "20210821 08:28"
        },
        {
          "id": "MDRW2100003612.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 08:28"
        },
        {
          "id": "MDRW2100003612.1.9",
          "form": "여친이 애니 좋아해서",
          "original_form": "여친이 애니 좋아해서",
          "speaker_id": "2",
          "time": "20210821 08:28"
        },
        {
          "id": "MDRW2100003612.1.10",
          "form": "아ㅜㅜㅋㅋ\r\n선택권이 없었네영ㅋㅋ",
          "original_form": "아ㅜㅜㅋㅋ\r\n선택권이 없었네영ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 08:29"
        },
        {
          "id": "MDRW2100003612.1.11",
          "form": "사실",
          "original_form": "사실",
          "speaker_id": "2",
          "time": "20210821 08:29"
        },
        {
          "id": "MDRW2100003612.1.12",
          "form": "마블영화 보고팠는데....",
          "original_form": "마블영화 보고팠는데....",
          "speaker_id": "2",
          "time": "20210821 08:29"
        },
        {
          "id": "MDRW2100003612.1.13",
          "form": "블랙위도우 ㅜ",
          "original_form": "블랙위도우 ㅜ",
          "speaker_id": "2",
          "time": "20210821 08:30"
        },
        {
          "id": "MDRW2100003612.1.14",
          "form": "마블은 매니아층들이 많죠ㅋㅋ",
          "original_form": "마블은 매니아층들이 많죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 08:30"
        },
        {
          "id": "MDRW2100003612.1.15",
          "form": "저도 아이들 낳기 전에는\r\n영화 좋아해서 개봉하는\r\n영화들 날짜까지 체크해가며\r\n보고 했었는데ㅋ 아이들 생기니\r\n영화랑도 점점 멀어지더라구요",
          "original_form": "저도 아이들 낳기 전에는\r\n영화 좋아해서 개봉하는\r\n영화들 날짜까지 체크해가며\r\n보고 했었는데ㅋ 아이들 생기니\r\n영화랑도 점점 멀어지더라구요",
          "speaker_id": "1",
          "time": "20210821 08:31"
        },
        {
          "id": "MDRW2100003612.1.16",
          "form": "아이들 재우고\r\n밤에 넷플 보는게 낙인거 같아요 ㅋ",
          "original_form": "아이들 재우고\r\n밤에 넷플 보는게 낙인거 같아요 ㅋ",
          "speaker_id": "1",
          "time": "20210821 08:31"
        },
        {
          "id": "MDRW2100003612.1.17",
          "form": "넷플에서 뭐 보세요?",
          "original_form": "넷플에서 뭐 보세요?",
          "speaker_id": "2",
          "time": "20210821 08:32"
        },
        {
          "id": "MDRW2100003612.1.18",
          "form": "추천좀...ㅋㅋ",
          "original_form": "추천좀...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 08:32"
        },
        {
          "id": "MDRW2100003612.1.19",
          "form": "요즘은 슬의생이랑 알고있지만\r\n보고있어요 ㅋㅋㅋ",
          "original_form": "요즘은 슬의생이랑 알고있지만\r\n보고있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 08:32"
        },
        {
          "id": "MDRW2100003612.1.20",
          "form": "킹덤 보셨었나요?",
          "original_form": "킹덤 보셨었나요?",
          "speaker_id": "1",
          "time": "20210821 08:32"
        },
        {
          "id": "MDRW2100003612.1.21",
          "form": "아 ㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 08:33"
        },
        {
          "id": "MDRW2100003612.1.22",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 08:33"
        },
        {
          "id": "MDRW2100003612.1.23",
          "form": "저 킹덤 진짜 좋아해요",
          "original_form": "저 킹덤 진짜 좋아해요",
          "speaker_id": "2",
          "time": "20210821 08:33"
        },
        {
          "id": "MDRW2100003612.1.24",
          "form": "킹덤 보려고 넷플 가입했었는데",
          "original_form": "킹덤 보려고 넷플 가입했었는데",
          "speaker_id": "1",
          "time": "20210821 08:33"
        },
        {
          "id": "MDRW2100003612.1.25",
          "form": "당시 저는 임신중이라 안봤었거든요ㅋㅋ",
          "original_form": "당시 저는 임신중이라 안봤었거든요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 08:34"
        },
        {
          "id": "MDRW2100003612.1.26",
          "form": "지금2까지 나오고\r\n얼마전에 아신전 나온거죠?",
          "original_form": "지금2까지 나오고\r\n얼마전에 아신전 나온거죠?",
          "speaker_id": "1",
          "time": "20210821 08:34"
        },
        {
          "id": "MDRW2100003612.1.27",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 08:34"
        },
        {
          "id": "MDRW2100003612.1.28",
          "form": "아주 감질나요",
          "original_form": "아주 감질나요",
          "speaker_id": "2",
          "time": "20210821 08:34"
        },
        {
          "id": "MDRW2100003612.1.29",
          "form": "잠도 안자고 보던데ㅋㅋ",
          "original_form": "잠도 안자고 보던데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 08:34"
        },
        {
          "id": "MDRW2100003612.1.30",
          "form": "저도 봐야겠어요ㅋ",
          "original_form": "저도 봐야겠어요ㅋ",
          "speaker_id": "1",
          "time": "20210821 08:35"
        }
      ]
