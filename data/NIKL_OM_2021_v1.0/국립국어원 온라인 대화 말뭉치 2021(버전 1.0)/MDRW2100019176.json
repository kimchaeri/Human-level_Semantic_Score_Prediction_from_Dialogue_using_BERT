          "id": "MDRW2100019176.1.1",
          "form": "필드에서 궁 쓰면",
          "original_form": "필드에서 궁 쓰면",
          "speaker_id": "2",
          "time": "20210827 15:56"
        },
        {
          "id": "MDRW2100019176.1.2",
          "form": "불나서 궁범위 밖으로 도망가야되는데",
          "original_form": "불나서 궁범위 밖으로 도망가야되는데",
          "speaker_id": "2",
          "time": "20210827 15:57"
        },
        {
          "id": "MDRW2100019176.1.3",
          "form": "그럼 몹들도 나 쫒아와서",
          "original_form": "그럼 몹들도 나 쫒아와서",
          "speaker_id": "2",
          "time": "20210827 15:57"
        },
        {
          "id": "MDRW2100019176.1.4",
          "form": "궁 아무도 안맞아ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "궁 아무도 안맞아ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:57"
        },
        {
          "id": "MDRW2100019176.1.5",
          "form": "아 ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019176.1.6",
          "form": "진짜 캐릭터 짤 때 스킬같은건 좀 더 생각을 해보고 만들어야지",
          "original_form": "진짜 캐릭터 짤 때 스킬같은건 좀 더 생각을 해보고 만들어야지",
          "speaker_id": "1",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019176.1.7",
          "form": "아무리 기본 지급 캐릭터라고 해도 ㅎ",
          "original_form": "아무리 기본 지급 캐릭터라고 해도 ㅎ",
          "speaker_id": "1",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019176.1.8",
          "form": "베넷궁은",
          "original_form": "베넷궁은",
          "speaker_id": "2",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019176.1.9",
          "form": "불 원소만 붙지",
          "original_form": "불 원소만 붙지",
          "speaker_id": "2",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019176.1.10",
          "form": "필드에 불 안나거든",
          "original_form": "필드에 불 안나거든",
          "speaker_id": "2",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019176.1.11",
          "form": "본인도 불원소 안붙어?",
          "original_form": "본인도 불원소 안붙어?",
          "speaker_id": "1",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019176.1.12",
          "form": "원소는 붙는데",
          "original_form": "원소는 붙는데",
          "speaker_id": "2",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019176.1.13",
          "form": "그 불타는 효과있잖아",
          "original_form": "그 불타는 효과있잖아",
          "speaker_id": "2",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019176.1.14",
          "form": "그거 없이 원소만 붙어",
          "original_form": "그거 없이 원소만 붙어",
          "speaker_id": "2",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019176.1.15",
          "form": "그 장판위에 서있으면",
          "original_form": "그 장판위에 서있으면",
          "speaker_id": "2",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019176.1.16",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "1",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019176.1.17",
          "form": "다이루크도 불 자꾸 붙어서",
          "original_form": "다이루크도 불 자꾸 붙어서",
          "speaker_id": "1",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019176.1.18",
          "form": "전기츄츄족 있으면",
          "original_form": "전기츄츄족 있으면",
          "speaker_id": "1",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100019176.1.19",
          "form": "과부하로 튕김;;;",
          "original_form": "과부하로 튕김;;;",
          "speaker_id": "1",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100019176.1.20",
          "form": "그리고 공격력증가+피통회복",
          "original_form": "그리고 공격력증가+피통회복",
          "speaker_id": "2",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100019176.1.21",
          "form": "아 얘도 스킬은",
          "original_form": "아 얘도 스킬은",
          "speaker_id": "2",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100019176.1.22",
          "form": "불 붙어",
          "original_form": "불 붙어",
          "speaker_id": "2",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100019176.1.23",
          "form": "근데 불칼 한번 휘두르기라서",
          "original_form": "근데 불칼 한번 휘두르기라서",
          "speaker_id": "2",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100019176.1.24",
          "form": "휘두르고 빼면 돼",
          "original_form": "휘두르고 빼면 돼",
          "speaker_id": "2",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100019176.1.25",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "1",
          "time": "20210827 16:01"
        },
        {
          "id": "MDRW2100019176.1.26",
          "form": "언니 주인공 원소 뭐써?",
          "original_form": "언니 주인공 원소 뭐써?",
          "speaker_id": "2",
          "time": "20210827 16:01"
        },
        {
          "id": "MDRW2100019176.1.27",
          "form": "나 그때그때 다른데",
          "original_form": "나 그때그때 다른데",
          "speaker_id": "1",
          "time": "20210827 16:01"
        },
        {
          "id": "MDRW2100019176.1.28",
          "form": "그냥 그 필드 속성 따라서 쓰고있어",
          "original_form": "그냥 그 필드 속성 따라서 쓰고있어",
          "speaker_id": "1",
          "time": "20210827 16:01"
        },
        {
          "id": "MDRW2100019176.1.29",
          "form": "원래 이나즈마 탐험 했어서 전기로 하다가",
          "original_form": "원래 이나즈마 탐험 했어서 전기로 하다가",
          "speaker_id": "1",
          "time": "20210827 16:01"
        },
        {
          "id": "MDRW2100019176.1.30",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210827 16:02"
        },
        {
          "id": "MDRW2100019176.1.31",
          "form": "난 바위 들고다녀",
          "original_form": "난 바위 들고다녀",
          "speaker_id": "2",
          "time": "20210827 16:02"
        },
        {
          "id": "MDRW2100019176.1.32",
          "form": "요즘은 리월 보물상자 찾고 있어서 바위로 쓰늨 중",
          "original_form": "요즘은 리월 보물상자 찾고 있어서 바위로 쓰늨 중",
          "speaker_id": "1",
          "time": "20210827 16:02"
        },
        {
          "id": "MDRW2100019176.1.33",
          "form": "전기 솔직히",
          "original_form": "전기 솔직히",
          "speaker_id": "2",
          "time": "20210827 16:02"
        },
        {
          "id": "MDRW2100019176.1.34",
          "form": "구려",
          "original_form": "구려",
          "speaker_id": "2",
          "time": "20210827 16:02"
        },
        {
          "id": "MDRW2100019176.1.35",
          "form": "그렇긴 해 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그렇긴 해 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:02"
        },
        {
          "id": "MDRW2100019176.1.36",
          "form": "그리고 속성 바꿀 때마다 특성 첨부터 키워야하는게",
