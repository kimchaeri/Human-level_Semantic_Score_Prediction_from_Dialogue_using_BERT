          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044303.1.3",
          "form": "그러니까 ㅋㅋㅋㅋㅋ모니카샘은 넘멋지고요 ㅠ",
          "original_form": "그러니까 ㅋㅋㅋㅋㅋ모니카샘은 넘멋지고요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044303.1.4",
          "form": "맞다 광자매 드라마",
          "original_form": "맞다 광자매 드라마",
          "speaker_id": "1",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044303.1.5",
          "form": "반전 그거 봤나 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "반전 그거 봤나 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044303.1.6",
          "form": "반전도 잇낰ㅋㅋㅋ뭔ㅔ",
          "original_form": "반전도 잇낰ㅋㅋㅋ뭔ㅔ",
          "speaker_id": "2",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044303.1.7",
          "form": "시엄마 암걸렷대?....",
          "original_form": "시엄마 암걸렷대?....",
          "speaker_id": "2",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044303.1.8",
          "form": "아닠ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아닠ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044303.1.9",
          "form": "그 막내 친아빠 어쩌구 그건 들엇지",
          "original_form": "그 막내 친아빠 어쩌구 그건 들엇지",
          "speaker_id": "1",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044303.1.10",
          "form": "어어",
          "original_form": "어어",
          "speaker_id": "2",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044303.1.11",
          "form": "사기꾼",
          "original_form": "사기꾼",
          "speaker_id": "2",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044303.1.12",
          "form": "그거 결국 막내딸이 알게됫거든ㅋㅋㅋㅋㅋ",
          "original_form": "그거 결국 막내딸이 알게됫거든ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044303.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ이미 알고잇대?..",
          "original_form": "ㅋㅋㅋㅋㅋㅋ이미 알고잇대?..",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044303.1.14",
          "form": "ㅋㅋㅋㅋㅋ아니 막 결국 그 사기꾼이 찾아감ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ아니 막 결국 그 사기꾼이 찾아감ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044303.1.15",
          "form": "친자그거보여주면섴ㅋㅋㅋ",
          "original_form": "친자그거보여주면섴ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044303.1.16",
          "form": "사기꾼 역시....ㅋㅋㅋ큐ㅠ",
          "original_form": "사기꾼 역시....ㅋㅋㅋ큐ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044303.1.17",
          "form": "ㅋㅋㅋㅋㅋ그래서 막 알게됫는데",
          "original_form": "ㅋㅋㅋㅋㅋ그래서 막 알게됫는데",
          "speaker_id": "1",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044303.1.18",
          "form": "위에 두자매도 알게됫자나",
          "original_form": "위에 두자매도 알게됫자나",
          "speaker_id": "1",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044303.1.19",
          "form": "걔네도....?",
          "original_form": "걔네도....?",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044303.1.20",
          "form": "반전뭔데 ㅋㅋㅋ흥미진진",
          "original_form": "반전뭔데 ㅋㅋㅋ흥미진진",
          "speaker_id": "2",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044303.1.21",
          "form": "ㅇㅇㅇ가족모여있엇는데 막내딸이 막 찾아가서 사실이냐고물음ㅌㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅇ가족모여있엇는데 막내딸이 막 찾아가서 사실이냐고물음ㅌㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044303.1.22",
          "form": "ㅇㅇㅇ 물엇는데?...",
          "original_form": "ㅇㅇㅇ 물엇는데?...",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
