      "utterance": [
        {
          "id": "MDRW2100044111.1.1",
          "form": "노란 빛에 밥이랑 같이 먹는...??",
          "original_form": "노란 빛에 밥이랑 같이 먹는...??",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044111.1.2",
          "form": "국인가요?",
          "original_form": "국인가요?",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044111.1.3",
          "form": "아니요! 국 아닙니다!",
          "original_form": "아니요! 국 아닙니다!",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044111.1.4",
          "form": "국물이 거의 거의 거의 없어요",
          "original_form": "국물이 거의 거의 거의 없어요",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044111.1.5",
          "form": "주재료가 뭔가요! 혹시 계란인가요?",
          "original_form": "주재료가 뭔가요! 혹시 계란인가요?",
          "speaker_id": "2",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044111.1.6",
          "form": "네 계란 들어가요!",
          "original_form": "네 계란 들어가요!",
          "speaker_id": "1",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044111.1.7",
          "form": "계란과 또 하나가 주 재료예요",
          "original_form": "계란과 또 하나가 주 재료예요",
          "speaker_id": "1",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044111.1.8",
          "form": "토마토계란볶음인가요?",
          "original_form": "토마토계란볶음인가요?",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044111.1.9",
          "form": "맞으면 대박이다",
          "original_form": "맞으면 대박이다",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044111.1.10",
          "form": "할 대박",
          "original_form": "할 대박",
          "speaker_id": "1",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044111.1.11",
          "form": "딩동댕 맞습니다 짝짝짝",
          "original_form": "딩동댕 맞습니다 짝짝짝",
          "speaker_id": "1",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044111.1.12",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044111.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ너무 쉽게 맞추셔",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ너무 쉽게 맞추셔",
          "speaker_id": "1",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044111.1.14",
          "form": "으쓱하네요 호호",
          "original_form": "으쓱하네요 호호",
          "speaker_id": "2",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044111.1.15",
          "form": "혹시 궁예신가요",
          "original_form": "혹시 궁예신가요",
          "speaker_id": "1",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044111.1.16",
          "form": "관심법을 쓰시나봐",
          "original_form": "관심법을 쓰시나봐",
          "speaker_id": "1",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044111.1.17",
          "form": "안대차고 있습니다",
          "original_form": "안대차고 있습니다",
          "speaker_id": "2",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044111.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044111.1.19",
          "form": "토마토계란볶음이 중국음식이에요ㅋㅋㅋㄱ",
          "original_form": "토마토계란볶음이 중국음식이에요ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044111.1.20",
          "form": "저 중학생 때 방과후중국어시간에 만들었거든요",
          "original_form": "저 중학생 때 방과후중국어시간에 만들었거든요",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044111.1.21",
          "form": "이렇게 도움이 되네요ㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "이렇게 도움이 되네요ㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044111.1.22",
          "form": "어 진짜요?????? 대박",
          "original_form": "어 진짜요?????? 대박",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044111.1.23",
          "form": "중국 음식이었구나",
          "original_form": "중국 음식이었구나",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044111.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ검색해도",
          "original_form": "ㅋㅋㅋㅋㅋㅋ검색해도",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044111.1.25",
          "form": "한식인지 양식인지 뭔지 안 나오고",
          "original_form": "한식인지 양식인지 뭔지 안 나오고",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044111.1.26",
          "form": "백종원만 나와서",
          "original_form": "백종원만 나와서",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044111.1.27",
          "form": "뭐야 백종원이 만들었나 싶었거든요ㅋㅋㅋㅋㅋ",
          "original_form": "뭐야 백종원이 만들었나 싶었거든요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044111.1.28",
          "form": "여기서 들을 줄은 몰랐어요 신기하다",
          "original_form": "여기서 들을 줄은 몰랐어요 신기하다",
          "speaker_id": "1",
          "time": "20210907 21:52"
        }
      ]
