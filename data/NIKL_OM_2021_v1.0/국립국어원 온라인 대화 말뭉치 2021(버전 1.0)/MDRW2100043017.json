      "utterance": [
        {
          "id": "MDRW2100043017.1.1",
          "form": "점심 드셨어요?",
          "original_form": "점심 드셨어요?",
          "speaker_id": "1",
          "time": "20210907 13:09"
        },
        {
          "id": "MDRW2100043017.1.2",
          "form": "점심식사 하셨나요?",
          "original_form": "점심식사 하셨나요?",
          "speaker_id": "1",
          "time": "20210907 13:10"
        },
        {
          "id": "MDRW2100043017.1.3",
          "form": "직원식당에서 그냥 백반먹엇어욤",
          "original_form": "직원식당에서 그냥 백반먹엇어욤",
          "speaker_id": "2",
          "time": "20210907 13:10"
        },
        {
          "id": "MDRW2100043017.1.4",
          "form": "하셧나요?",
          "original_form": "하셧나요?",
          "speaker_id": "2",
          "time": "20210907 13:10"
        },
        {
          "id": "MDRW2100043017.1.5",
          "form": "저도 구내식당에서 떡국이랑 제육",
          "original_form": "저도 구내식당에서 떡국이랑 제육",
          "speaker_id": "1",
          "time": "20210907 13:10"
        },
        {
          "id": "MDRW2100043017.1.6",
          "form": "ㅋㅋ맛나겟네여",
          "original_form": "ㅋㅋ맛나겟네여",
          "speaker_id": "2",
          "time": "20210907 13:10"
        },
        {
          "id": "MDRW2100043017.1.7",
          "form": "저희는 걍 쏘쏘함다",
          "original_form": "저희는 걍 쏘쏘함다",
          "speaker_id": "2",
          "time": "20210907 13:10"
        },
        {
          "id": "MDRW2100043017.1.8",
          "form": "네 급식이 잘 나오는 편이예요",
          "original_form": "네 급식이 잘 나오는 편이예요",
          "speaker_id": "1",
          "time": "20210907 13:15"
        },
        {
          "id": "MDRW2100043017.1.9",
          "form": "내일은 치킨 나온다고 하더라고요ㅎㅎ",
          "original_form": "내일은 치킨 나온다고 하더라고요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 13:15"
        },
        {
          "id": "MDRW2100043017.1.10",
          "form": "오오 학생이신가요",
          "original_form": "오오 학생이신가요",
          "speaker_id": "2",
          "time": "20210907 13:15"
        },
        {
          "id": "MDRW2100043017.1.11",
          "form": "급식?",
          "original_form": "급식?",
          "speaker_id": "2",
          "time": "20210907 13:15"
        },
        {
          "id": "MDRW2100043017.1.12",
          "form": "아ㅎㅎ 그냥 회사에서 나오는 거라",
          "original_form": "아ㅎㅎ 그냥 회사에서 나오는 거라",
          "speaker_id": "1",
          "time": "20210907 13:15"
        },
        {
          "id": "MDRW2100043017.1.13",
          "form": "급식이라고ㅋㅋ",
          "original_form": "급식이라고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:16"
        },
        {
          "id": "MDRW2100043017.1.14",
          "form": "아ㅋㅋ그러셧구낭",
          "original_form": "아ㅋㅋ그러셧구낭",
          "speaker_id": "2",
          "time": "20210907 13:16"
        },
        {
          "id": "MDRW2100043017.1.15",
          "form": "쫌따 점심시간 끝나면",
          "original_form": "쫌따 점심시간 끝나면",
          "speaker_id": "2",
          "time": "20210907 13:16"
        },
        {
          "id": "MDRW2100043017.1.16",
          "form": "커피 또 때려넣어야겟어여",
          "original_form": "커피 또 때려넣어야겟어여",
          "speaker_id": "2",
          "time": "20210907 13:16"
        },
        {
          "id": "MDRW2100043017.1.17",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 13:16"
        },
        {
          "id": "MDRW2100043017.1.18",
          "form": "커피타임?",
          "original_form": "커피타임?",
          "speaker_id": "1",
          "time": "20210907 13:16"
        },
        {
          "id": "MDRW2100043017.1.19",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 13:16"
        },
        {
          "id": "MDRW2100043017.1.20",
          "form": "잠와여",
          "original_form": "잠와여",
          "speaker_id": "2",
          "time": "20210907 13:16"
        },
        {
          "id": "MDRW2100043017.1.21",
          "form": "식사하고나면 커피 한잔 해줘야죠",
          "original_form": "식사하고나면 커피 한잔 해줘야죠",
          "speaker_id": "1",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100043017.1.22",
          "form": "하루에 아메리카노 두잔은 꼭 먹어요ㅜ",
          "original_form": "하루에 아메리카노 두잔은 꼭 먹어요ㅜ",
          "speaker_id": "2",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100043017.1.23",
          "form": "저도 요즘은 커피 없인 일하기 힘들더라고요",
          "original_form": "저도 요즘은 커피 없인 일하기 힘들더라고요",
          "speaker_id": "1",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100043017.1.24",
          "form": "안그럼 일할수없어요",
          "original_form": "안그럼 일할수없어요",
          "speaker_id": "2",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100043017.1.25",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100043017.1.26",
