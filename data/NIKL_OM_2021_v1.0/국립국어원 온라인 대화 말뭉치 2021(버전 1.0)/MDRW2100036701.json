      "utterance": [
        {
          "id": "MDRW2100036701.1.1",
          "form": "그래가 어제 name3아빠가 신랑한테 전화와서",
          "original_form": "그래가 어제 &name3&아빠가 신랑한테 전화와서",
          "speaker_id": "2",
          "time": "20210831 09:41"
        },
        {
          "id": "MDRW2100036701.1.2",
          "form": "혼낼까봐 말하기가 좀 그런데 하지마라고 말을 해야될거같아서 전화한다고",
          "original_form": "혼낼까봐 말하기가 좀 그런데 하지마라고 말을 해야될거같아서 전화한다고",
          "speaker_id": "2",
          "time": "20210831 09:41"
        },
        {
          "id": "MDRW2100036701.1.3",
          "form": "웅웅 아고",
          "original_form": "웅웅 아고",
          "speaker_id": "1",
          "time": "20210831 09:41"
        },
        {
          "id": "MDRW2100036701.1.4",
          "form": "나도 언니한테 사과하고",
          "original_form": "나도 언니한테 사과하고",
          "speaker_id": "2",
          "time": "20210831 09:41"
        },
        {
          "id": "MDRW2100036701.1.5",
          "form": "진짜....아....",
          "original_form": "진짜....아....",
          "speaker_id": "2",
          "time": "20210831 09:41"
        },
        {
          "id": "MDRW2100036701.1.6",
          "form": "아 ㅠㅠㅠㅠㅠ몬산다 ㅋㅋㅋ",
          "original_form": "아 ㅠㅠㅠㅠㅠ몬산다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:41"
        },
        {
          "id": "MDRW2100036701.1.7",
          "form": "신랑도 너무 상질나서",
          "original_form": "신랑도 너무 상질나서",
          "speaker_id": "2",
          "time": "20210831 09:42"
        },
        {
          "id": "MDRW2100036701.1.8",
          "form": "그정도로?? ㅋㅋㅋ 많이 속상했네",
          "original_form": "그정도로?? ㅋㅋㅋ 많이 속상했네",
          "speaker_id": "1",
          "time": "20210831 09:42"
        },
        {
          "id": "MDRW2100036701.1.9",
          "form": "원 무한의계단도 다 지운다고",
          "original_form": "원 무한의계단도 다 지운다고",
          "speaker_id": "2",
          "time": "20210831 09:42"
        },
        {
          "id": "MDRW2100036701.1.10",
          "form": "어제 진짜...너무 화나는거야",
          "original_form": "어제 진짜...너무 화나는거야",
          "speaker_id": "2",
          "time": "20210831 09:42"
        },
        {
          "id": "MDRW2100036701.1.11",
          "form": "아 ㅠㅠㅠㅠ 아구 어제 난리났었네",
          "original_form": "아 ㅠㅠㅠㅠ 아구 어제 난리났었네",
          "speaker_id": "1",
          "time": "20210831 09:42"
        },
        {
          "id": "MDRW2100036701.1.12",
          "form": "다른 사람이 소중하게 여기는거는 왜 말도 없이 몰래 지웠냐고",
          "original_form": "다른 사람이 소중하게 여기는거는 왜 말도 없이 몰래 지웠냐고",
          "speaker_id": "2",
          "time": "20210831 09:42"
        },
        {
          "id": "MDRW2100036701.1.13",
          "form": "한번만 더 다른사람꺼 만지고",
          "original_form": "한번만 더 다른사람꺼 만지고",
          "speaker_id": "2",
          "time": "20210831 09:42"
        },
        {
          "id": "MDRW2100036701.1.14",
          "form": "첫째는 장난이었는데 ㅠㅠ 일이 커짐..",
          "original_form": "첫째는 장난이었는데 ㅠㅠ 일이 커짐..",
          "speaker_id": "1",
          "time": "20210831 09:42"
        },
        {
          "id": "MDRW2100036701.1.15",
          "form": "몰래 지우고 하면",
          "original_form": "몰래 지우고 하면",
          "speaker_id": "2",
          "time": "20210831 09:42"
        },
        {
          "id": "MDRW2100036701.1.16",
          "form": "감옥간다고 ㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "감옥간다고 ㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210831 09:42"
        },
        {
          "id": "MDRW2100036701.1.17",
          "form": "ㅋㅋㅋ감옥 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ감옥 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:42"
        },
        {
          "id": "MDRW2100036701.1.18",
          "form": "그니께",
          "original_form": "그니께",
          "speaker_id": "2",
          "time": "20210831 09:42"
        },
        {
          "id": "MDRW2100036701.1.19",
          "form": "경찰부부 출동 ㅋㅋ",
          "original_form": "경찰부부 출동 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:43"
        },
        {
          "id": "MDRW2100036701.1.20",
          "form": "오늘은 진짜 니가 모르고 장난치려고 한거라 그냥 조용히 넘어가지만",
          "original_form": "오늘은 진짜 니가 모르고 장난치려고 한거라 그냥 조용히 넘어가지만",
          "speaker_id": "2",
