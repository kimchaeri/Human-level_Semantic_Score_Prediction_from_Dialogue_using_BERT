      "utterance": [
        {
          "id": "MDRW2100013498.1.1",
          "form": "물놀이같은건 좋아하세요?",
          "original_form": "물놀이같은건 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 14:47"
        },
        {
          "id": "MDRW2100013498.1.2",
          "form": "물에 몸 담구는거 좋아해요",
          "original_form": "물에 몸 담구는거 좋아해요",
          "speaker_id": "2",
          "time": "20210825 14:48"
        },
        {
          "id": "MDRW2100013498.1.3",
          "form": "수영이나 물 튀기는건",
          "original_form": "수영이나 물 튀기는건",
          "speaker_id": "2",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100013498.1.4",
          "form": "오 네네",
          "original_form": "오 네네",
          "speaker_id": "1",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100013498.1.5",
          "form": "제가 몸치라 되게 엉성해요",
          "original_form": "제가 몸치라 되게 엉성해요",
          "speaker_id": "2",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100013498.1.6",
          "form": "저도요 ㅋㅋㅋ ㅜ 맥주병이에요",
          "original_form": "저도요 ㅋㅋㅋ ㅜ 맥주병이에요",
          "speaker_id": "1",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100013498.1.7",
          "form": "물에서 몸을 더 못 가누게 되어요",
          "original_form": "물에서 몸을 더 못 가누게 되어요",
          "speaker_id": "1",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100013498.1.8",
          "form": "저는 잠수함처럼 계속 가라앉더라고요",
          "original_form": "저는 잠수함처럼 계속 가라앉더라고요",
          "speaker_id": "2",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100013498.1.9",
          "form": "역시 수중의 세계는 어려워요",
          "original_form": "역시 수중의 세계는 어려워요",
          "speaker_id": "2",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100013498.1.10",
          "form": "잠수함ㅋㅋㅋㅋ ㅜ",
          "original_form": "잠수함ㅋㅋㅋㅋ ㅜ",
          "speaker_id": "1",
          "time": "20210825 14:53"
        },
        {
          "id": "MDRW2100013498.1.11",
          "form": "여름에 다들 물놀이 가는데 좀 부러웠어욬ㅋ",
          "original_form": "여름에 다들 물놀이 가는데 좀 부러웠어욬ㅋ",
          "speaker_id": "1",
          "time": "20210825 14:53"
        },
        {
          "id": "MDRW2100013498.1.12",
          "form": "물놀이하면 그 역동적이고 유쾌한 느낌인데 저도 그게 부러워요",
          "original_form": "물놀이하면 그 역동적이고 유쾌한 느낌인데 저도 그게 부러워요",
          "speaker_id": "2",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013498.1.13",
          "form": "바닷가 가면 튜브 하나 들고",
          "original_form": "바닷가 가면 튜브 하나 들고",
          "speaker_id": "2",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013498.1.14",
          "form": "맞아요 맞아요",
          "original_form": "맞아요 맞아요",
          "speaker_id": "1",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013498.1.15",
          "form": "하반신을 바다에 담구는게 제 물놀이라",
          "original_form": "하반신을 바다에 담구는게 제 물놀이라",
          "speaker_id": "2",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013498.1.16",
          "form": "청량하고.. 시원해보이고..",
          "original_form": "청량하고.. 시원해보이고..",
          "speaker_id": "1",
          "time": "20210825 14:56"
        },
        {
          "id": "MDRW2100013498.1.17",
          "form": "저도요 ㅋㅋㅋ 발담그면 끝",
          "original_form": "저도요 ㅋㅋㅋ 발담그면 끝",
          "speaker_id": "1",
          "time": "20210825 14:56"
        },
        {
          "id": "MDRW2100013498.1.18",
          "form": "그렇게 가만히 있으면 해초가 된 기분이 들어요",
          "original_form": "그렇게 가만히 있으면 해초가 된 기분이 들어요",
          "speaker_id": "2",
          "time": "20210825 14:56"
        },
        {
          "id": "MDRW2100013498.1.19",
          "form": "해촠ㅋㅋㅋㅋㅋ 약간 미역 체험",
          "original_form": "해촠ㅋㅋㅋㅋㅋ 약간 미역 체험",
          "speaker_id": "1",
          "time": "20210825 14:57"
        }
      ]
