      "utterance": [
        {
          "id": "MDRW2100009012.1.1",
          "form": "쌀쌀해지니 전 마라탕이 당기더라고요",
          "original_form": "쌀쌀해지니 전 마라탕이 당기더라고요",
          "speaker_id": "2",
          "time": "20210822 20:30"
        },
        {
          "id": "MDRW2100009012.1.2",
          "form": "매운맛도 워낙 좋아해서",
          "original_form": "매운맛도 워낙 좋아해서",
          "speaker_id": "2",
          "time": "20210822 20:30"
        },
        {
          "id": "MDRW2100009012.1.3",
          "form": "매운 것 잘 드시나요?",
          "original_form": "매운 것 잘 드시나요?",
          "speaker_id": "2",
          "time": "20210822 20:30"
        },
        {
          "id": "MDRW2100009012.1.4",
          "form": "아니요 잘 못먹어요ㅜㅜ",
          "original_form": "아니요 잘 못먹어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 20:31"
        },
        {
          "id": "MDRW2100009012.1.5",
          "form": "마라탕은 한번도 안먹어봤어요~ 마라샹궈 마라탕...",
          "original_form": "마라탕은 한번도 안먹어봤어요~ 마라샹궈 마라탕...",
          "speaker_id": "1",
          "time": "20210822 20:31"
        },
        {
          "id": "MDRW2100009012.1.6",
          "form": "하이디라오에서 훠궈를 먹어봤는데 경험한걸로 만족...\r\n 한뒤로 ㅋㅋㅋㅋ",
          "original_form": "하이디라오에서 훠궈를 먹어봤는데 경험한걸로 만족...\r\n 한뒤로 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:32"
        },
        {
          "id": "MDRW2100009012.1.7",
          "form": "아ㅜ 매운거 잘 못드시면 난이도가 높죠",
          "original_form": "아ㅜ 매운거 잘 못드시면 난이도가 높죠",
          "speaker_id": "2",
          "time": "20210822 20:32"
        },
        {
          "id": "MDRW2100009012.1.8",
          "form": "전 이상하게 훠궈는 또 입에 안 맞더라고요ㅎ",
          "original_form": "전 이상하게 훠궈는 또 입에 안 맞더라고요ㅎ",
          "speaker_id": "2",
          "time": "20210822 20:33"
        },
        {
          "id": "MDRW2100009012.1.9",
          "form": "저는 훠궈도 맛있는지 모르겠더라구요..",
          "original_form": "저는 훠궈도 맛있는지 모르겠더라구요..",
          "speaker_id": "1",
          "time": "20210822 20:33"
        },
        {
          "id": "MDRW2100009012.1.10",
          "form": "입안만 화하고 맛이 없었어요",
          "original_form": "입안만 화하고 맛이 없었어요",
          "speaker_id": "1",
          "time": "20210822 20:33"
        },
        {
          "id": "MDRW2100009012.1.11",
          "form": "마라탕은 비슷한 맛이 아닌가봐요@",
          "original_form": "마라탕은 비슷한 맛이 아닌가봐요@",
          "speaker_id": "1",
          "time": "20210822 20:33"
        },
        {
          "id": "MDRW2100009012.1.12",
          "form": "ㅎㅎ 그쵸. 명동 하이디라오  갔는데 취향 아님",
          "original_form": "ㅎㅎ 그쵸. 명동 하이디라오  갔는데 취향 아님",
          "speaker_id": "2",
          "time": "20210822 20:33"
        },
        {
          "id": "MDRW2100009012.1.13",
          "form": "마라탕도 천지차이인데 전 라화쿵부 좋아해요",
          "original_form": "마라탕도 천지차이인데 전 라화쿵부 좋아해요",
          "speaker_id": "2",
          "time": "20210822 20:33"
        },
        {
          "id": "MDRW2100009012.1.14",
          "form": "백탕은 한번 도전해보세요",
          "original_form": "백탕은 한번 도전해보세요",
          "speaker_id": "2",
          "time": "20210822 20:34"
        },
        {
          "id": "MDRW2100009012.1.15",
          "form": "사골국물 쓰는곳은 깊은 맛 나요",
          "original_form": "사골국물 쓰는곳은 깊은 맛 나요",
          "speaker_id": "2",
          "time": "20210822 20:34"
        },
        {
          "id": "MDRW2100009012.1.16",
          "form": "라화쿵부 ㅋㅋㅋㅋㅋ 쿵푸팬더생각나욬ㅋㅋㅋㅋㅋ",
          "original_form": "라화쿵부 ㅋㅋㅋㅋㅋ 쿵푸팬더생각나욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:37"
        },
        {
          "id": "MDRW2100009012.1.17",
          "form": "마라탕 종류중에 백탕이 있나요?",
          "original_form": "마라탕 종류중에 백탕이 있나요?",
          "speaker_id": "1",
          "time": "20210822 20:37"
        },
        {
          "id": "MDRW2100009012.1.18",
          "form": "저도 명동 하이디라오에서 먹었어요!",
          "original_form": "저도 명동 하이디라오에서 먹었어요!",
          "speaker_id": "1",
          "time": "20210822 20:37"
        },
        {
          "id": "MDRW2100009012.1.19",
          "form": "그것이 처음이자 마지막...",
          "original_form": "그것이 처음이자 마지막...",
          "speaker_id": "1",
          "time": "20210822 20:37"
        },
        {
          "id": "MDRW2100009012.1.20",
          "form": "네 백탕 파는 곳도 있어요. 라화쿵부도 백탕 파는지는 잘 모르겠지만",
          "original_form": "네 백탕 파는 곳도 있어요. 라화쿵부도 백탕 파는지는 잘 모르겠지만",
          "speaker_id": "2",
          "time": "20210822 20:38"
        },
        {
          "id": "MDRW2100009012.1.21",
          "form": "제 인생에 훠궈도 딱 세번이 전부예요ㅎ",
          "original_form": "제 인생에 훠궈도 딱 세번이 전부예요ㅎ",
          "speaker_id": "2",
          "time": "20210822 20:38"
        },
        {
          "id": "MDRW2100009012.1.22",
          "form": "국내서 두번 중국서 한번",
          "original_form": "국내서 두번 중국서 한번",
          "speaker_id": "2",
          "time": "20210822 20:38"
        },
        {
          "id": "MDRW2100009012.1.23",
          "form": "세번 다 제 입맛 아님ㅜ",
          "original_form": "세번 다 제 입맛 아님ㅜ",
          "speaker_id": "2",
          "time": "20210822 20:38"
        },
        {
          "id": "MDRW2100009012.1.24",
          "form": "제 친구는 임신했는데 일끝나구 마라샹궈 먹으러 간다고 하더라구요 ㅋㅋㅋ",
          "original_form": "제 친구는 임신했는데 일끝나구 마라샹궈 먹으러 간다고 하더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:39"
        },
        {
          "id": "MDRW2100009012.1.25",
          "form": "입맛에 맞으면 엄청 먹는거 같아요 ㅎㅎ",
          "original_form": "입맛에 맞으면 엄청 먹는거 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 20:39"
        },
        {
          "id": "MDRW2100009012.1.26",
          "form": "마라맛 중독되면 정말 시도때도 없이 생각나요",
          "original_form": "마라맛 중독되면 정말 시도때도 없이 생각나요",
          "speaker_id": "2",
          "time": "20210822 20:40"
        },
        {
          "id": "MDRW2100009012.1.27",
          "form": "저도 거진  중독 수준이라",
          "original_form": "저도 거진  중독 수준이라",
          "speaker_id": "2",
          "time": "20210822 20:40"
        },
        {
          "id": "MDRW2100009012.1.28",
          "form": "한창때는 일주일에 네번 먹었어요ㅎ",
          "original_form": "한창때는 일주일에 네번 먹었어요ㅎ",
          "speaker_id": "2",
          "time": "20210822 20:40"
        },
        {
          "id": "MDRW2100009012.1.29",
          "form": "매운 거 못 드시면 신라면은 드실 수 있으세요?",
          "original_form": "매운 거 못 드시면 신라면은 드실 수 있으세요?",
          "speaker_id": "2",
          "time": "20210822 20:40"
        },
        {
          "id": "MDRW2100009012.1.30",
          "form": "신라면은 먹어요! ㅋㅋㅋ 근데 저희집에서 주로 먹는 종류는 아니에요 ㅎㅎ",
          "original_form": "신라면은 먹어요! ㅋㅋㅋ 근데 저희집에서 주로 먹는 종류는 아니에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 20:41"
        },
        {
          "id": "MDRW2100009012.1.31",
          "form": "저희집은 요즘 진라면 매운맛 먹어요!",
          "original_form": "저희집은 요즘 진라면 매운맛 먹어요!",
          "speaker_id": "1",
          "time": "20210822 20:41"
        },
        {
          "id": "MDRW2100009012.1.32",
          "form": "아ㅎ 그럼 안성탕면 파인가요?  아님 진매?",
          "original_form": "아ㅎ 그럼 안성탕면 파인가요?  아님 진매?",
          "speaker_id": "2",
          "time": "20210822 20:41"
        },
        {
          "id": "MDRW2100009012.1.33",
          "form": "아ㅎ 저도 진매파예요ㅎ",
          "original_form": "아ㅎ 저도 진매파예요ㅎ",
          "speaker_id": "2",
          "time": "20210822 20:41"
        },
        {
          "id": "MDRW2100009012.1.34",
          "form": "불닭볶음면도 먹을수있어요! ㅋㅋㅋ 하지만 너무 매운것...",
          "original_form": "불닭볶음면도 먹을수있어요! ㅋㅋㅋ 하지만 너무 매운것...",
          "speaker_id": "1",
          "time": "20210822 20:42"
        },
        {
          "id": "MDRW2100009012.1.35",
          "form": "맛있더라구요 진라면!!",
          "original_form": "맛있더라구요 진라면!!",
          "speaker_id": "1",
          "time": "20210822 20:42"
        },
        {
          "id": "MDRW2100009012.1.36",
          "form": "아니ㅎㅎ 은근 다 드시네요",
          "original_form": "아니ㅎㅎ 은근 다 드시네요",
          "speaker_id": "2",
          "time": "20210822 20:42"
        },
        {
          "id": "MDRW2100009012.1.37",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210822 20:42"
        },
        {
          "id": "MDRW2100009012.1.38",
          "form": "불닭볶음면은 너무 달아서",
          "original_form": "불닭볶음면은 너무 달아서",
          "speaker_id": "2",
          "time": "20210822 20:42"
        },
        {
          "id": "MDRW2100009012.1.39",
          "form": "까르보불닭을 더 좋아해요",
          "original_form": "까르보불닭을 더 좋아해요",
          "speaker_id": "2",
          "time": "20210822 20:42"
        },
        {
          "id": "MDRW2100009012.1.40",
          "form": "ㅋㅋㅋ근데 매운맛을 즐기진않아요. 요즘 매운맛기본단계가 높아진거 같아서ㅜ 잘먹는편 아니라거 생각해요ㅜㅋㅋ",
          "original_form": "ㅋㅋㅋ근데 매운맛을 즐기진않아요. 요즘 매운맛기본단계가 높아진거 같아서ㅜ 잘먹는편 아니라거 생각해요ㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:43"
        },
        {
          "id": "MDRW2100009012.1.41",
          "form": "한국인에게는 매운 맛이 디폴트라ㅜ",
          "original_form": "한국인에게는 매운 맛이 디폴트라ㅜ",
          "speaker_id": "2",
          "time": "20210822 20:43"
        },
        {
          "id": "MDRW2100009012.1.42",
          "form": "그 앵그리너구리? 그것도 좀 맵더라구요. 짜기도하고",
          "original_form": "그 앵그리너구리? 그것도 좀 맵더라구요. 짜기도하고",
          "speaker_id": "1",
          "time": "20210822 20:43"
        },
        {
          "id": "MDRW2100009012.1.43",
          "form": "맞아요 맵고 짜고.",
          "original_form": "맞아요 맵고 짜고.",
          "speaker_id": "2",
          "time": "20210822 20:43"
        },
        {
          "id": "MDRW2100009012.1.44",
          "form": "딱 건강에 안 좋은 맛ㅎㅎ",
          "original_form": "딱 건강에 안 좋은 맛ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 20:43"
        },
        {
          "id": "MDRW2100009012.1.45",
          "form": "저는 그냥 너구리 좋아해여 ㅋㅋ",
          "original_form": "저는 그냥 너구리 좋아해여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:44"
        }
      ]
    }
