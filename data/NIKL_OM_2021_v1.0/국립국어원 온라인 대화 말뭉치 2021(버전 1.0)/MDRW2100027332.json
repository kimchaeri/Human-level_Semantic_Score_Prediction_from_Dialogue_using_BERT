      "utterance": [
        {
          "id": "MDRW2100027332.1.1",
          "form": "프리랜서세요?",
          "original_form": "프리랜서세요?",
          "speaker_id": "2",
          "time": "20210902 11:51"
        },
        {
          "id": "MDRW2100027332.1.2",
          "form": "아님 재택근무?",
          "original_form": "아님 재택근무?",
          "speaker_id": "2",
          "time": "20210902 11:52"
        },
        {
          "id": "MDRW2100027332.1.3",
          "form": "재택인데 주로 카페서해요",
          "original_form": "재택인데 주로 카페서해요",
          "speaker_id": "1",
          "time": "20210902 11:53"
        },
        {
          "id": "MDRW2100027332.1.4",
          "form": "집에선 눕게 되어서요",
          "original_form": "집에선 눕게 되어서요",
          "speaker_id": "1",
          "time": "20210902 11:53"
        },
        {
          "id": "MDRW2100027332.1.5",
          "form": "도서관도 종종 가요",
          "original_form": "도서관도 종종 가요",
          "speaker_id": "1",
          "time": "20210902 11:53"
        },
        {
          "id": "MDRW2100027332.1.6",
          "form": "프리랜서에요!",
          "original_form": "프리랜서에요!",
          "speaker_id": "1",
          "time": "20210902 11:53"
        },
        {
          "id": "MDRW2100027332.1.7",
          "form": "님은요? 출근하셨나요",
          "original_form": "님은요? 출근하셨나요",
          "speaker_id": "1",
          "time": "20210902 11:53"
        },
        {
          "id": "MDRW2100027332.1.8",
          "form": "아하 그렇군요",
          "original_form": "아하 그렇군요",
          "speaker_id": "2",
          "time": "20210902 11:54"
        },
        {
          "id": "MDRW2100027332.1.9",
          "form": "저도 재택근무할때... 점심시간에 누워 잤어요 그냥 ㅋㅋㅋㅋ",
          "original_form": "저도 재택근무할때... 점심시간에 누워 잤어요 그냥 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:54"
        },
        {
          "id": "MDRW2100027332.1.10",
          "form": "네 저는 오늘 사무실이에요",
          "original_form": "네 저는 오늘 사무실이에요",
          "speaker_id": "2",
          "time": "20210902 11:54"
        },
        {
          "id": "MDRW2100027332.1.11",
          "form": "사무실이 좋아요",
          "original_form": "사무실이 좋아요",
          "speaker_id": "1",
          "time": "20210902 11:55"
        },
        {
          "id": "MDRW2100027332.1.12",
          "form": "능률이 오르지요",
          "original_form": "능률이 오르지요",
          "speaker_id": "1",
          "time": "20210902 11:55"
        },
        {
          "id": "MDRW2100027332.1.13",
          "form": "오죽하면 저도 나왔을까요ㅜ",
          "original_form": "오죽하면 저도 나왔을까요ㅜ",
          "speaker_id": "1",
          "time": "20210902 11:56"
        },
        {
          "id": "MDRW2100027332.1.14",
          "form": "하지만 출퇴근은 너무 끔찍해요 ㅋㅋ",
          "original_form": "하지만 출퇴근은 너무 끔찍해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:56"
        },
        {
          "id": "MDRW2100027332.1.15",
          "form": "싫은 사람들과 마주치는것도 고통스럽구요 ㅋㅋㅋㅋ",
          "original_form": "싫은 사람들과 마주치는것도 고통스럽구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:56"
        },
        {
          "id": "MDRW2100027332.1.16",
          "form": "업무는 익숙해지셨나요?",
          "original_form": "업무는 익숙해지셨나요?",
          "speaker_id": "1",
          "time": "20210902 11:56"
        },
        {
          "id": "MDRW2100027332.1.17",
          "form": "사람이 힘들죠ㅜ",
          "original_form": "사람이 힘들죠ㅜ",
          "speaker_id": "1",
          "time": "20210902 11:56"
        },
        {
          "id": "MDRW2100027332.1.18",
          "form": "네네 오래 하던 업무여서",
          "original_form": "네네 오래 하던 업무여서",
          "speaker_id": "2",
          "time": "20210902 11:56"
        },
        {
          "id": "MDRW2100027332.1.19",
          "form": "저는 디자인이라 재택인데 미팅할때 미쳐부러요",
          "original_form": "저는 디자인이라 재택인데 미팅할때 미쳐부러요",
          "speaker_id": "1",
          "time": "20210902 11:56"
        },
        {
          "id": "MDRW2100027332.1.20",
          "form": "정답이 없는 일이에요",
          "original_form": "정답이 없는 일이에요",
          "speaker_id": "1",
          "time": "20210902 11:57"
        },
        {
          "id": "MDRW2100027332.1.21",
          "form": "업무가 맞다니 부럽습니다^^",
          "original_form": "업무가 맞다니 부럽습니다^^",
          "speaker_id": "1",
          "time": "20210902 11:57"
        },
        {
          "id": "MDRW2100027332.1.22",
          "form": "작업한다고 하시길래 뭔가 그런쪽이 아닐까 생각했어요 ㅎㅎ",
          "original_form": "작업한다고 하시길래 뭔가 그런쪽이 아닐까 생각했어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 12:00"
        },
        {
          "id": "MDRW2100027332.1.23",
          "form": "네 멍때리고 있어요",
          "original_form": "네 멍때리고 있어요",
          "speaker_id": "1",
          "time": "20210902 12:01"
        },
        {
          "id": "MDRW2100027332.1.24",
          "form": "창작이라 생각이 많아요",
          "original_form": "창작이라 생각이 많아요",
          "speaker_id": "1",
          "time": "20210902 12:01"
        },
        {
          "id": "MDRW2100027332.1.25",
          "form": "점심시간이군요.",
          "original_form": "점심시간이군요.",
          "speaker_id": "1",
          "time": "20210902 12:01"
        },
        {
          "id": "MDRW2100027332.1.26",
          "form": "저는 카페에서^^",
          "original_form": "저는 카페에서^^",
          "speaker_id": "1",
          "time": "20210902 12:01"
        },
        {
          "id": "MDRW2100027332.1.27",
          "form": "저는 베이글 사러가려고요",
          "original_form": "저는 베이글 사러가려고요",
          "speaker_id": "2",
          "time": "20210902 12:02"
        },
        {
          "id": "MDRW2100027332.1.28",
          "form": "사무실에서 식사하시겠어요",
          "original_form": "사무실에서 식사하시겠어요",
          "speaker_id": "1",
          "time": "20210902 12:03"
        },
        {
          "id": "MDRW2100027332.1.29",
          "form": "후딱다녀오세요^",
          "original_form": "후딱다녀오세요^",
          "speaker_id": "1",
          "time": "20210902 12:03"
        },
        {
          "id": "MDRW2100027332.1.30",
          "form": "ㅋㅋㅋㅋㅋ 생각을 바꿔서 자리에 앉았어요",
          "original_form": "ㅋㅋㅋㅋㅋ 생각을 바꿔서 자리에 앉았어요",
          "speaker_id": "2",
          "time": "20210902 12:07"
        },
        {
          "id": "MDRW2100027332.1.31",
          "form": "매장에서 먹으려구요",
          "original_form": "매장에서 먹으려구요",
          "speaker_id": "2",
          "time": "20210902 12:07"
        }
      ]
