      "utterance": [
        {
          "id": "MDRW2100018579.1.1",
          "form": "식사하셨나요",
          "original_form": "식사하셨나요",
          "speaker_id": "1",
          "time": "20210827 08:47"
        },
        {
          "id": "MDRW2100018579.1.2",
          "form": "아직이요 식사하셨어요?",
          "original_form": "아직이요 식사하셨어요?",
          "speaker_id": "2",
          "time": "20210827 08:47"
        },
        {
          "id": "MDRW2100018579.1.3",
          "form": "저두 식전이요",
          "original_form": "저두 식전이요",
          "speaker_id": "1",
          "time": "20210827 08:48"
        },
        {
          "id": "MDRW2100018579.1.4",
          "form": "아침은 좀 거르려구요ㅎ",
          "original_form": "아침은 좀 거르려구요ㅎ",
          "speaker_id": "1",
          "time": "20210827 08:48"
        },
        {
          "id": "MDRW2100018579.1.5",
          "form": "아침식사 안하시는편이신가요",
          "original_form": "아침식사 안하시는편이신가요",
          "speaker_id": "1",
          "time": "20210827 08:48"
        },
        {
          "id": "MDRW2100018579.1.6",
          "form": "아침에 잠이 더 중요해서 거르고 출근하는 편이에요",
          "original_form": "아침에 잠이 더 중요해서 거르고 출근하는 편이에요",
          "speaker_id": "2",
          "time": "20210827 08:49"
        },
        {
          "id": "MDRW2100018579.1.7",
          "form": "아무래도 바쁜 현대인들에게는 밥보다는 잠이 더 소중하죠",
          "original_form": "아무래도 바쁜 현대인들에게는 밥보다는 잠이 더 소중하죠",
          "speaker_id": "1",
          "time": "20210827 08:49"
        },
        {
          "id": "MDRW2100018579.1.8",
          "form": "즐겨드시는 음식이 있으신가요",
          "original_form": "즐겨드시는 음식이 있으신가요",
          "speaker_id": "1",
          "time": "20210827 08:50"
        },
        {
          "id": "MDRW2100018579.1.9",
          "form": "저는 초밥이나 젤리같은거 좋아해요",
          "original_form": "저는 초밥이나 젤리같은거 좋아해요",
          "speaker_id": "2",
          "time": "20210827 08:51"
        },
        {
          "id": "MDRW2100018579.1.10",
          "form": "어떤거 즐겨드세요?",
          "original_form": "어떤거 즐겨드세요?",
          "speaker_id": "2",
          "time": "20210827 08:51"
        },
        {
          "id": "MDRW2100018579.1.11",
          "form": "저는 육류 좋아해요",
          "original_form": "저는 육류 좋아해요",
          "speaker_id": "1",
          "time": "20210827 08:51"
        },
        {
          "id": "MDRW2100018579.1.12",
          "form": "해산물은 알러지가있어서",
          "original_form": "해산물은 알러지가있어서",
          "speaker_id": "1",
          "time": "20210827 08:51"
        },
        {
          "id": "MDRW2100018579.1.13",
          "form": "헉 ㅠ",
          "original_form": "헉 ㅠ",
          "speaker_id": "2",
          "time": "20210827 08:52"
        },
        {
          "id": "MDRW2100018579.1.14",
          "form": "초밥은 어떤거 즐겨드세요",
          "original_form": "초밥은 어떤거 즐겨드세요",
          "speaker_id": "1",
          "time": "20210827 08:52"
        },
        {
          "id": "MDRW2100018579.1.15",
          "form": "음식알러지는 대체 오ㅐ 있는걸까요 ....... 속상",
          "original_form": "음식알러지는 대체 오ㅐ 있는걸까요 ....... 속상",
          "speaker_id": "2",
          "time": "20210827 08:52"
        },
        {
          "id": "MDRW2100018579.1.16",
          "form": "그러니까요..ㅎ 원래 없었는데 13살쯤 생기더라구요",
          "original_form": "그러니까요..ㅎ 원래 없었는데 13살쯤 생기더라구요",
          "speaker_id": "1",
          "time": "20210827 08:52"
        },
        {
          "id": "MDRW2100018579.1.17",
          "form": "저는 연어초밥이나 간장새우 좋아해요!!",
          "original_form": "저는 연어초밥이나 간장새우 좋아해요!!",
          "speaker_id": "2",
          "time": "20210827 08:52"
        },
        {
          "id": "MDRW2100018579.1.18",
          "form": "맞아요 갑자기 생긴다고들 ㅠㅠ",
          "original_form": "맞아요 갑자기 생긴다고들 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 08:52"
        },
        {
          "id": "MDRW2100018579.1.19",
          "form": "저희집에도 새우 좋아하는 사람 한명 있는데ㅋㅋㅋ 그분도 새우만 주구장창 먹더라구요",
          "original_form": "저희집에도 새우 좋아하는 사람 한명 있는데ㅋㅋㅋ 그분도 새우만 주구장창 먹더라구요",
