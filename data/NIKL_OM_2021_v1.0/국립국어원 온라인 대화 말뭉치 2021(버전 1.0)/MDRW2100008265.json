          "speaker_id": "1",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100008265.1.3",
          "form": "전 주부랍니다",
          "original_form": "전 주부랍니다",
          "speaker_id": "2",
          "time": "20210822 15:01"
        },
        {
          "id": "MDRW2100008265.1.4",
          "form": "ㅎㅎ 남친보단 남편!!",
          "original_form": "ㅎㅎ 남친보단 남편!!",
          "speaker_id": "1",
          "time": "20210822 16:59"
        },
        {
          "id": "MDRW2100008265.1.5",
          "form": "네 애둘맘 입니다",
          "original_form": "네 애둘맘 입니다",
          "speaker_id": "2",
          "time": "20210822 17:12"
        },
        {
          "id": "MDRW2100008265.1.6",
          "form": "와 둘이나~~~ 존경스럾",
          "original_form": "와 둘이나~~~ 존경스럾",
          "speaker_id": "1",
          "time": "20210822 18:01"
        },
        {
          "id": "MDRW2100008265.1.7",
          "form": "아직 싱글이신 가요",
          "original_form": "아직 싱글이신 가요",
          "speaker_id": "2",
          "time": "20210822 18:04"
        },
        {
          "id": "MDRW2100008265.1.8",
          "form": "아뇨 기혼인데 아직 애는 없어요ㅎㅎ",
          "original_form": "아뇨 기혼인데 아직 애는 없어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 18:07"
        },
        {
          "id": "MDRW2100008265.1.9",
          "form": "네 직장은 다니시나요",
          "original_form": "네 직장은 다니시나요",
          "speaker_id": "2",
          "time": "20210822 19:29"
        },
        {
          "id": "MDRW2100008265.1.10",
          "form": "네 ㅎㅎ 맞벌이에요",
          "original_form": "네 ㅎㅎ 맞벌이에요",
          "speaker_id": "1",
          "time": "20210822 20:50"
        },
        {
          "id": "MDRW2100008265.1.11",
          "form": "저도 직장 다닌답니다",
          "original_form": "저도 직장 다닌답니다",
          "speaker_id": "2",
          "time": "20210822 20:58"
        },
        {
          "id": "MDRW2100008265.1.12",
          "form": "오잉 주부셨는데",
          "original_form": "오잉 주부셨는데",
