      "utterance": [
        {
          "id": "MDRW2100040251.1.1",
          "form": "오 연애하시나용",
          "original_form": "오 연애하시나용",
          "speaker_id": "2",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100040251.1.2",
          "form": "ㅎㅎ네네",
          "original_form": "ㅎㅎ네네",
          "speaker_id": "1",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100040251.1.3",
          "form": "얼마나 되셨나요",
          "original_form": "얼마나 되셨나요",
          "speaker_id": "2",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100040251.1.4",
          "form": "300일 조금 덜됐어요",
          "original_form": "300일 조금 덜됐어요",
          "speaker_id": "1",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100040251.1.5",
          "form": "크크 좋을때시군영",
          "original_form": "크크 좋을때시군영",
          "speaker_id": "2",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100040251.1.6",
          "form": "주말에 데이트 하시나요",
          "original_form": "주말에 데이트 하시나요",
          "speaker_id": "2",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100040251.1.7",
          "form": "ㅎㅎ그런가요?",
          "original_form": "ㅎㅎ그런가요?",
          "speaker_id": "1",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100040251.1.8",
          "form": "백신 맞고 누워있어서... 이번 주말은 집콕하려고요",
          "original_form": "백신 맞고 누워있어서... 이번 주말은 집콕하려고요",
          "speaker_id": "1",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100040251.1.9",
          "form": "name2님은 연애중이신가요?",
          "original_form": "&name2&님은 연애중이신가요?",
          "speaker_id": "1",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100040251.1.10",
          "form": "저는 공백을 길게 갖다가 얼마전에! 소개팅으로 만나서",
          "original_form": "저는 공백을 길게 갖다가 얼마전에! 소개팅으로 만나서",
          "speaker_id": "2",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100040251.1.11",
          "form": "이제 2주 됐어요",
          "original_form": "이제 2주 됐어요",
          "speaker_id": "2",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100040251.1.12",
          "form": "엄청 파릇파릇한 새내기",
          "original_form": "엄청 파릇파릇한 새내기",
          "speaker_id": "2",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100040251.1.13",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100040251.1.14",
          "form": "오호 설렘 가득한 때네요!!",
          "original_form": "오호 설렘 가득한 때네요!!",
          "speaker_id": "1",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100040251.1.15",
          "form": "좋으시겠다~",
          "original_form": "좋으시겠다~",
          "speaker_id": "1",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100040251.1.16",
          "form": "넹 오랜만에 설레네여",
          "original_form": "넹 오랜만에 설레네여",
          "speaker_id": "2",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100040251.1.17",
          "form": "저도 이번 주말은 집콕해여",
          "original_form": "저도 이번 주말은 집콕해여",
          "speaker_id": "2",
          "time": "20210904 15:09"
        },
        {
          "id": "MDRW2100040251.1.18",
          "form": "백신 맞고 데이트는 좀 무리긴 하죠",
          "original_form": "백신 맞고 데이트는 좀 무리긴 하죠",
          "speaker_id": "1",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100040251.1.19",
          "form": "마자요ㅋㅋ 보통 뭐하시나용",
          "original_form": "마자요ㅋㅋ 보통 뭐하시나용",
          "speaker_id": "2",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100040251.1.20",
          "form": "그때그때 다른데",
          "original_form": "그때그때 다른데",
          "speaker_id": "1",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100040251.1.21",
          "form": "요즘은 코로나 때문에 어디 많이 가지는 못하고",
          "original_form": "요즘은 코로나 때문에 어디 많이 가지는 못하고",
          "speaker_id": "1",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100040251.1.22",
          "form": "사람 많지 않은 곳으로 다녀요",
          "original_form": "사람 많지 않은 곳으로 다녀요",
          "speaker_id": "1",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100040251.1.23",
          "form": "아 좋네요ㅋㅋ 어떻게 만나셨나요!",
          "original_form": "아 좋네요ㅋㅋ 어떻게 만나셨나요!",
          "speaker_id": "2",
          "time": "20210904 15:14"
        },
        {
          "id": "MDRW2100040251.1.24",
          "form": "저도 소개받았어요",
          "original_form": "저도 소개받았어요",
          "speaker_id": "1",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100040251.1.25",
          "form": "나이차는 어떻게 되어용",
          "original_form": "나이차는 어떻게 되어용",
          "speaker_id": "2",
          "time": "20210904 15:21"
        },
        {
          "id": "MDRW2100040251.1.26",
          "form": "2살이요",
          "original_form": "2살이요",
          "speaker_id": "1",
          "time": "20210904 15:22"
        },
        {
          "id": "MDRW2100040251.1.27",
          "form": "오 저랑 똑같네요!",
          "original_form": "오 저랑 똑같네요!",
          "speaker_id": "2",
          "time": "20210904 15:22"
        }
      ]
