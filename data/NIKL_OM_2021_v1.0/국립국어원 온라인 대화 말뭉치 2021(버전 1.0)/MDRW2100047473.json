      "utterance": [
        {
          "id": "MDRW2100047473.1.1",
          "form": "ㅎㅎ 원래 탄산수를 왜 마셔 이랬는데",
          "original_form": "ㅎㅎ 원래 탄산수를 왜 마셔 이랬는데",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100047473.1.2",
          "form": "혼술 즐긴 이후로는 박스째 구비해요",
          "original_form": "혼술 즐긴 이후로는 박스째 구비해요",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100047473.1.3",
          "form": "원래 술자리는 잦은 편이었나요?",
          "original_form": "원래 술자리는 잦은 편이었나요?",
          "speaker_id": "2",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100047473.1.4",
          "form": "혼술 말고요",
          "original_form": "혼술 말고요",
          "speaker_id": "2",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100047473.1.5",
          "form": "ㅋㅋㅋ 아니요 저는",
          "original_form": "ㅋㅋㅋ 아니요 저는",
          "speaker_id": "1",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100047473.1.6",
          "form": "알쓰라 술자리거의안가고",
          "original_form": "알쓰라 술자리거의안가고",
          "speaker_id": "1",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100047473.1.7",
          "form": "혼술을 마니하는것같긴하네요ㅋㅋㅋ",
          "original_form": "혼술을 마니하는것같긴하네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100047473.1.8",
          "form": "아ㅎ 안주발을 당기는 방법도 있는데",
          "original_form": "아ㅎ 안주발을 당기는 방법도 있는데",
          "speaker_id": "2",
          "time": "20210826 17:29"
        },
        {
          "id": "MDRW2100047473.1.9",
          "form": "취객들 사이에선 아무래도 괴롭죠ㅎ",
          "original_form": "취객들 사이에선 아무래도 괴롭죠ㅎ",
          "speaker_id": "2",
          "time": "20210826 17:29"
        },
        {
          "id": "MDRW2100047473.1.10",
          "form": "전 코로나 이후로 혼술하게 됐어요",
          "original_form": "전 코로나 이후로 혼술하게 됐어요",
          "speaker_id": "2",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100047473.1.11",
          "form": "그전엔 술자리가 잦은 직장이라 술이 지긋지긋했거든요ㅎㅎ",
          "original_form": "그전엔 술자리가 잦은 직장이라 술이 지긋지긋했거든요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100047473.1.12",
          "form": "ㅠ 헐 그럼 힘드셨겠어요",
          "original_form": "ㅠ 헐 그럼 힘드셨겠어요",
          "speaker_id": "1",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100047473.1.13",
          "form": "아무래도 코로나의 순기능으로",
          "original_form": "아무래도 코로나의 순기능으로",
          "speaker_id": "1",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100047473.1.14",
          "form": "회식자리가 확줄긴했네요..",
          "original_form": "회식자리가 확줄긴했네요..",
          "speaker_id": "1",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100047473.1.15",
          "form": "유지가되야될텐데말이죠",
          "original_form": "유지가되야될텐데말이죠",
          "speaker_id": "1",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100047473.1.16",
          "form": "ㅎㅎ 유일한 코로나의 순기능이죠",
          "original_form": "ㅎㅎ 유일한 코로나의 순기능이죠",
          "speaker_id": "2",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100047473.1.17",
          "form": "저녁 있는 삶이 됐으니까요",
          "original_form": "저녁 있는 삶이 됐으니까요",
          "speaker_id": "2",
          "time": "20210826 17:47"
        },
        {
          "id": "MDRW2100047473.1.18",
          "form": "맞아요ㅋㅋㅋ 직장인들은",
          "original_form": "맞아요ㅋㅋㅋ 직장인들은",
          "speaker_id": "1",
          "time": "20210826 17:54"
        },
        {
          "id": "MDRW2100047473.1.19",
          "form": "재택도하고 한편으론 좋은점이있네요",
          "original_form": "재택도하고 한편으론 좋은점이있네요",
          "speaker_id": "1",
          "time": "20210826 17:54"
        },
        {
          "id": "MDRW2100047473.1.20",
          "form": "ㅎㅎ 단비같은 재택ㅜ 몇번 안했지만 너무 좋더라고요",
          "original_form": "ㅎㅎ 단비같은 재택ㅜ 몇번 안했지만 너무 좋더라고요",
          "speaker_id": "2",
          "time": "20210826 17:55"
        },
        {
          "id": "MDRW2100047473.1.21",
          "form": "나태한 자세로 티타임 즐기면서 일하고",
          "original_form": "나태한 자세로 티타임 즐기면서 일하고",
          "speaker_id": "2",
          "time": "20210826 17:56"
        },
        {
          "id": "MDRW2100047473.1.22",
          "form": "ㅋㅋㅋ 오우좋네요",
          "original_form": "ㅋㅋㅋ 오우좋네요",
          "speaker_id": "1",
          "time": "20210826 17:59"
        },
        {
          "id": "MDRW2100047473.1.23",
          "form": "제친구는 주4가",
          "original_form": "제친구는 주4가",
          "speaker_id": "1",
          "time": "20210826 17:59"
        },
        {
          "id": "MDRW2100047473.1.24",
          "form": "재택이래요ㅋㅋㅋㅋㅋ",
          "original_form": "재택이래요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:59"
        },
        {
          "id": "MDRW2100047473.1.25",
          "form": "ㅎㅎ 너무 부러워요ㅜㅜㅜ 그 직장이 내 직장이었어야 해",
          "original_form": "ㅎㅎ 너무 부러워요ㅜㅜㅜ 그 직장이 내 직장이었어야 해",
          "speaker_id": "2",
          "time": "20210826 18:01"
        },
        {
          "id": "MDRW2100047473.1.26",
          "form": "근데 재택하니 하루종일 먹는 단점이ㅎㅎ",
          "original_form": "근데 재택하니 하루종일 먹는 단점이ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:02"
        },
        {
          "id": "MDRW2100047473.1.27",
          "form": "ㅋㅋㅋ 코로나로",
          "original_form": "ㅋㅋㅋ 코로나로",
          "speaker_id": "1",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100047473.1.28",
          "form": "살찌신분들이 꽤되더라고요ㅠ",
          "original_form": "살찌신분들이 꽤되더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100047473.1.29",
          "form": "코로나 이후 배달음식 주문이 최소 다섯배는 뛰었더라고요 ㅜ",
          "original_form": "코로나 이후 배달음식 주문이 최소 다섯배는 뛰었더라고요 ㅜ",
          "speaker_id": "2",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100047473.1.30",
          "form": "배민에 위자료 청구해야 함ㅜ",
          "original_form": "배민에 위자료 청구해야 함ㅜ",
          "speaker_id": "2",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100047473.1.31",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100047473.1.32",
          "form": "5배요??ㅈ",
          "original_form": "5배요??ㅈ",
          "speaker_id": "1",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100047473.1.33",
          "form": "진짜로요?",
          "original_form": "진짜로요?",
          "speaker_id": "1",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100047473.1.34",
          "form": "네. 일단 제 주문내역을 보면요ㅜ",
          "original_form": "네. 일단 제 주문내역을 보면요ㅜ",
