      "utterance": [
        {
          "id": "MDRW2100018649.1.1",
          "form": "저 최근에 가인 카니발 자주 들었어요",
          "original_form": "저 최근에 가인 카니발 자주 들었어요",
          "speaker_id": "2",
          "time": "20210827 09:53"
        },
        {
          "id": "MDRW2100018649.1.2",
          "form": "그노래 좋아요",
          "original_form": "그노래 좋아요",
          "speaker_id": "1",
          "time": "20210827 09:53"
        },
        {
          "id": "MDRW2100018649.1.3",
          "form": "퍼레이드 보는 기분",
          "original_form": "퍼레이드 보는 기분",
          "speaker_id": "1",
          "time": "20210827 09:53"
        },
        {
          "id": "MDRW2100018649.1.4",
          "form": ".카니발 문이 열리면~",
          "original_form": ".카니발 문이 열리면~",
          "speaker_id": "1",
          "time": "20210827 09:53"
        },
        {
          "id": "MDRW2100018649.1.5",
          "form": "맞아요 되게 영화 코코 생각나고",
          "original_form": "맞아요 되게 영화 코코 생각나고",
          "speaker_id": "2",
          "time": "20210827 09:53"
        },
        {
          "id": "MDRW2100018649.1.6",
          "form": "코코좋아해요?",
          "original_form": "코코좋아해요?",
          "speaker_id": "1",
          "time": "20210827 09:54"
        },
        {
          "id": "MDRW2100018649.1.7",
          "form": "저도 코코좋아해요",
          "original_form": "저도 코코좋아해요",
          "speaker_id": "1",
          "time": "20210827 09:54"
        },
        {
          "id": "MDRW2100018649.1.8",
          "form": "코코보고 울었어요",
          "original_form": "코코보고 울었어요",
          "speaker_id": "2",
          "time": "20210827 09:54"
        },
        {
          "id": "MDRW2100018649.1.9",
          "form": "리멤벌미~~",
          "original_form": "리멤벌미~~",
          "speaker_id": "1",
          "time": "20210827 09:54"
        },
        {
          "id": "MDRW2100018649.1.10",
          "form": "으이구 일부러 울라고 만든 장면이네 하면서 줄줄 울었어요",
          "original_form": "으이구 일부러 울라고 만든 장면이네 하면서 줄줄 울었어요",
          "speaker_id": "2",
          "time": "20210827 09:54"
        },
        {
          "id": "MDRW2100018649.1.11",
          "form": "리멤벌믜~~~%",
          "original_form": "리멤벌믜~~~%",
          "speaker_id": "2",
          "time": "20210827 09:54"
        },
        {
          "id": "MDRW2100018649.1.12",
          "form": "참 나 유치하네ㅎ 했는데",
          "original_form": "참 나 유치하네ㅎ 했는데",
          "speaker_id": "1",
          "time": "20210827 09:54"
        },
        {
          "id": "MDRW2100018649.1.13",
          "form": "줄줄 우는 나를 발견",
          "original_form": "줄줄 우는 나를 발견",
          "speaker_id": "1",
          "time": "20210827 09:54"
        },
        {
          "id": "MDRW2100018649.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ ㅇㅈㅋㅋㅋㅋㅋㅋㅋ아 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ ㅇㅈㅋㅋㅋㅋㅋㅋㅋ아 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 09:55"
        },
        {
          "id": "MDRW2100018649.1.15",
          "form": "그 영화가 좋았던건",
          "original_form": "그 영화가 좋았던건",
          "speaker_id": "1",
          "time": "20210827 09:55"
        },
        {
          "id": "MDRW2100018649.1.16",
          "form": "죽음이란건 꼭 그렇게 슬프지만은 않구나",
          "original_form": "죽음이란건 꼭 그렇게 슬프지만은 않구나",
          "speaker_id": "1",
          "time": "20210827 09:55"
        },
        {
          "id": "MDRW2100018649.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210827 09:55"
        },
        {
          "id": "MDRW2100018649.1.18",
          "form": "음 뭐랄까 한국은",
          "original_form": "음 뭐랄까 한국은",
          "speaker_id": "1",
          "time": "20210827 09:55"
        },
        {
          "id": "MDRW2100018649.1.19",
          "form": "죽으면 지옥간다고 저주하잖아요",
          "original_form": "죽으면 지옥간다고 저주하잖아요",
          "speaker_id": "1",
          "time": "20210827 09:55"
        },
        {
          "id": "MDRW2100018649.1.20",
          "form": "열심이안살으면 나태지옥..",
          "original_form": "열심이안살으면 나태지옥..",
          "speaker_id": "1",
          "time": "20210827 09:56"
        },
        {
          "id": "MDRW2100018649.1.21",
          "form": "죽음이 되게 슬프로 우울한 존재죠",
          "original_form": "죽음이 되게 슬프로 우울한 존재죠",
          "speaker_id": "2",
          "time": "20210827 09:56"
        },
        {
          "id": "MDRW2100018649.1.22",
          "form": "신과함께와 너무 비교되는..",
          "original_form": "신과함께와 너무 비교되는..",
          "speaker_id": "1",
          "time": "20210827 09:56"
        },
        {
          "id": "MDRW2100018649.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 09:56"
        },
        {
          "id": "MDRW2100018649.1.24",
          "form": "아시죠",
          "original_form": "아시죠",
          "speaker_id": "1",
          "time": "20210827 09:56"
        },
        {
          "id": "MDRW2100018649.1.25",
          "form": "저 신과함께도 보고 울었어요 망할",
          "original_form": "저 신과함께도 보고 울었어요 망할",
          "speaker_id": "2",
          "time": "20210827 09:56"
        },
        {
          "id": "MDRW2100018649.1.26",
          "form": "와 신과함께 가자마자 나태지옥",
          "original_form": "와 신과함께 가자마자 나태지옥",
          "speaker_id": "1",
          "time": "20210827 09:56"
        },
        {
          "id": "MDRW2100018649.1.27",
          "form": "으이구 신파!!! 울라고 만들엏네!! 하면서 울기.",
          "original_form": "으이구 신파!!! 울라고 만들엏네!! 하면서 울기.",
