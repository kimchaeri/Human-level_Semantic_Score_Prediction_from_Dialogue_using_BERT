      "utterance": [
        {
          "id": "MDRW2100007432.1.1",
          "form": "반려동물",
          "original_form": "반려동물",
          "speaker_id": "1",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007432.1.2",
          "form": "귀엽지",
          "original_form": "귀엽지",
          "speaker_id": "1",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007432.1.3",
          "form": "너 만약에 아무런 제약 없으면 뭐 키우고 싶어?",
          "original_form": "너 만약에 아무런 제약 없으면 뭐 키우고 싶어?",
          "speaker_id": "1",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007432.1.4",
          "form": "나 예전에 반려동물 키운 적 있어",
          "original_form": "나 예전에 반려동물 키운 적 있어",
          "speaker_id": "2",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007432.1.5",
          "form": "그래? 나 몰랐는데",
          "original_form": "그래? 나 몰랐는데",
          "speaker_id": "1",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007432.1.6",
          "form": "초등학생때 언니가 웬 햄스터를 주어와서 키웠어",
          "original_form": "초등학생때 언니가 웬 햄스터를 주어와서 키웠어",
          "speaker_id": "2",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007432.1.7",
          "form": "얼마나 오래 살았어?",
          "original_form": "얼마나 오래 살았어?",
          "speaker_id": "1",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007432.1.8",
          "form": "..일주일....",
          "original_form": "..일주일....",
          "speaker_id": "2",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007432.1.9",
          "form": "너는 반려동물의 경험이 0에 수렴하니?",
          "original_form": "너는 반려동물의 경험이 0에 수렴하니?",
          "speaker_id": "2",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007432.1.10",
          "form": "그것도 키운거라고 해도 돼?",
          "original_form": "그것도 키운거라고 해도 돼?",
          "speaker_id": "1",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007432.1.11",
          "form": "키운거야 이름은기억안나지만",
          "original_form": "키운거야 이름은기억안나지만",
          "speaker_id": "2",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007432.1.12",
          "form": "난 현재 구피와 함께 하고 있어",
          "original_form": "난 현재 구피와 함께 하고 있어",
          "speaker_id": "1",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007432.1.13",
          "form": "이름은 한마리한마리 세세하게 지어줬어?",
          "original_form": "이름은 한마리한마리 세세하게 지어줬어?",
          "speaker_id": "2",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007432.1.14",
          "form": "그리고 병아리와 달팽이와 이름 모를 붉은 곤충을 키웠었어",
          "original_form": "그리고 병아리와 달팽이와 이름 모를 붉은 곤충을 키웠었어",
          "speaker_id": "1",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007432.1.15",
          "form": "이름 다 있어",
          "original_form": "이름 다 있어",
          "speaker_id": "1",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007432.1.16",
          "form": "많다.",
          "original_form": "많다.",
          "speaker_id": "2",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007432.1.17",
          "form": "읊어봐",
          "original_form": "읊어봐",
          "speaker_id": "2",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007432.1.18",
          "form": "구분을 못해",
          "original_form": "구분을 못해",
          "speaker_id": "1",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007432.1.19",
          "form": "이름에 의미가 있어?",
          "original_form": "이름에 의미가 있어?",
          "speaker_id": "2",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007432.1.20",
          "form": "name3 name4 name5 뭐 이런 식으로",
          "original_form": "&name3& &name4& &name5& 뭐 이런 식으로",
          "speaker_id": "1",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007432.1.21",
          "form": "정말 대충지었다.",
          "original_form": "정말 대충지었다.",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007432.1.22",
          "form": "용품은 무엇무엇을쓰니",
          "original_form": "용품은 무엇무엇을쓰니",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007432.1.23",
          "form": "구분 가는 애들 루피랑 부피 정도 있긴 한데 나머지는 구분 못해",
          "original_form": "구분 가는 애들 루피랑 부피 정도 있긴 한데 나머지는 구분 못해",
          "speaker_id": "1",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007432.1.24",
          "form": "어항과 H2O",
          "original_form": "어항과 H2O",
          "speaker_id": "1",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007432.1.25",
          "form": "그 막 산소 뽀글뽀글올라오는건 안해줘?",
          "original_form": "그 막 산소 뽀글뽀글올라오는건 안해줘?",
          "speaker_id": "2",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007432.1.26",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007432.1.27",
          "form": "밥..안줘?",
          "original_form": "밥..안줘?",
          "speaker_id": "2",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007432.1.28",
          "form": "알아서 숨쉬겠지",
          "original_form": "알아서 숨쉬겠지",
          "speaker_id": "1",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007432.1.29",
          "form": "학대야이거",
          "original_form": "학대야이거",
          "speaker_id": "2",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007432.1.30",
          "form": "밥도 용품에 포함이야?",
          "original_form": "밥도 용품에 포함이야?",
          "speaker_id": "1",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007432.1.31",
          "form": "따지자면 포함 아닐까?",
          "original_form": "따지자면 포함 아닐까?",
          "speaker_id": "2",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007432.1.32",
          "form": "어? 아니 그",
          "original_form": "어? 아니 그",
          "speaker_id": "1",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007432.1.33",
          "form": "번식 잘 하던데",
          "original_form": "번식 잘 하던데",
          "speaker_id": "1",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007432.1.34",
          "form": "지들끼리 잡아먹는거아니야..?",
          "original_form": "지들끼리 잡아먹는거아니야..?",
          "speaker_id": "2",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007432.1.35",
          "form": "아기 물고기들이 조금씩 사라져",
          "original_form": "아기 물고기들이 조금씩 사라져",
          "speaker_id": "1",
          "time": "20210822 02:11"
        }
