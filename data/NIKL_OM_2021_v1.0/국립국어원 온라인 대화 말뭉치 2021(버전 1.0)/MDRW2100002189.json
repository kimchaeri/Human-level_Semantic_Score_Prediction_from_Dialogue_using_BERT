      "utterance": [
        {
          "id": "MDRW2100002189.1.1",
          "form": "너는 군대에 갈 생각이 있니?",
          "original_form": "너는 군대에 갈 생각이 있니?",
          "speaker_id": "2",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002189.1.2",
          "form": "너의 주위에 군인이 아직 있니?",
          "original_form": "너의 주위에 군인이 아직 있니?",
          "speaker_id": "2",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002189.1.3",
          "form": "내 주위에는 군인이 거의 없는 거 같아.",
          "original_form": "내 주위에는 군인이 거의 없는 거 같아.",
          "speaker_id": "2",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002189.1.4",
          "form": "아니 아무래도 나이를 먹어서",
          "original_form": "아니 아무래도 나이를 먹어서",
          "speaker_id": "1",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002189.1.5",
          "form": "이제 가는건 좀 무리지",
          "original_form": "이제 가는건 좀 무리지",
          "speaker_id": "1",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002189.1.6",
          "form": "아냐.",
          "original_form": "아냐.",
          "speaker_id": "2",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002189.1.7",
          "form": "서른살이 넘어도 가는 사람들이 꽤 있는걸.",
          "original_form": "서른살이 넘어도 가는 사람들이 꽤 있는걸.",
          "speaker_id": "2",
          "time": "20210820 00:23"
        },
        {
          "id": "MDRW2100002189.1.8",
          "form": "기초체력을 기르는 데 좋은 거 같아",
          "original_form": "기초체력을 기르는 데 좋은 거 같아",
          "speaker_id": "2",
          "time": "20210820 00:23"
        },
        {
          "id": "MDRW2100002189.1.9",
          "form": "식단도 건강하고. 그렇지 않니?",
          "original_form": "식단도 건강하고. 그렇지 않니?",
          "speaker_id": "2",
          "time": "20210820 00:23"
        },
        {
          "id": "MDRW2100002189.1.10",
          "form": "맞아 요즘은 밥이 진짜 잘나온대",
          "original_form": "맞아 요즘은 밥이 진짜 잘나온대",
          "speaker_id": "1",
          "time": "20210820 00:23"
        },
        {
          "id": "MDRW2100002189.1.11",
          "form": "샤인머스켓 먹는거봣어?",
          "original_form": "샤인머스켓 먹는거봣어?",
          "speaker_id": "1",
          "time": "20210820 00:23"
        },
        {
          "id": "MDRW2100002189.1.12",
          "form": "맞아 인터넷에서 사진을 봤는데 맛있어보이더라",
          "original_form": "맞아 인터넷에서 사진을 봤는데 맛있어보이더라",
          "speaker_id": "2",
          "time": "20210820 00:23"
        },
        {
          "id": "MDRW2100002189.1.13",
          "form": "나도 아직 두번밖에 안먹어봤는데",
          "original_form": "나도 아직 두번밖에 안먹어봤는데",
          "speaker_id": "1",
          "time": "20210820 00:23"
        },
        {
          "id": "MDRW2100002189.1.14",
          "form": "봤지.",
          "original_form": "봤지.",
          "speaker_id": "2",
          "time": "20210820 00:23"
        },
        {
          "id": "MDRW2100002189.1.15",
          "form": "어라 나도 두번 먹어봄",
          "original_form": "어라 나도 두번 먹어봄",
          "speaker_id": "2",
          "time": "20210820 00:23"
        },
        {
          "id": "MDRW2100002189.1.16",
          "form": "부럽더라",
          "original_form": "부럽더라",
          "speaker_id": "1",
          "time": "20210820 00:23"
        },
        {
          "id": "MDRW2100002189.1.17",
          "form": "그거 말고도 여럿이 같이 운동을 한다는 것도 좋은 거 같아.",
          "original_form": "그거 말고도 여럿이 같이 운동을 한다는 것도 좋은 거 같아.",
          "speaker_id": "2",
          "time": "20210820 00:23"
        },
        {
          "id": "MDRW2100002189.1.18",
          "form": "그리고 기간도 예전보다 훨씬 짧잖아",
          "original_form": "그리고 기간도 예전보다 훨씬 짧잖아",
          "speaker_id": "1",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002189.1.19",
          "form": "무엇보다 군대에 다녀오면 호봉 인정이 되잖아.",
          "original_form": "무엇보다 군대에 다녀오면 호봉 인정이 되잖아.",
          "speaker_id": "2",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002189.1.20",
          "form": "맞아 친구들도 많이 생기고",
          "original_form": "맞아 친구들도 많이 생기고",
          "speaker_id": "1",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002189.1.21",
          "form": "여군은 안되는 거 같지만...",
          "original_form": "여군은 안되는 거 같지만...",
          "speaker_id": "2",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002189.1.22",
          "form": "맞아. 군대에서 사귄 인연들이 오래가는 거 같더라.",
          "original_form": "맞아. 군대에서 사귄 인연들이 오래가는 거 같더라.",
          "speaker_id": "2",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002189.1.23",
          "form": "부럽다 ㅠㅠ 나도 호봉 인정해주면 좋겠어",
          "original_form": "부럽다 ㅠㅠ 나도 호봉 인정해주면 좋겠어",
          "speaker_id": "1",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002189.1.24",
          "form": "나도.",
          "original_form": "나도.",
          "speaker_id": "2",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002189.1.25",
          "form": "그리고 좀 각양각색의 인연들도 만나잖아",
          "original_form": "그리고 좀 각양각색의 인연들도 만나잖아",
          "speaker_id": "1",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002189.1.26",
          "form": "아 내 친구의 소식을 들었는데 군인이 되었다고 하더라.",
          "original_form": "아 내 친구의 소식을 들었는데 군인이 되었다고 하더라.",
          "speaker_id": "2",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002189.1.27",
          "form": "요즘은 가혹행위도 없고",
          "original_form": "요즘은 가혹행위도 없고",
          "speaker_id": "1",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002189.1.28",
          "form": "여군??",
          "original_form": "여군??",
          "speaker_id": "1",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002189.1.29",
          "form": "여자군인은 자청해서 가는거니까 정말 멋있는 거 같아.",
          "original_form": "여자군인은 자청해서 가는거니까 정말 멋있는 거 같아.",
          "speaker_id": "2",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002189.1.30",
          "form": "맞아.",
          "original_form": "맞아.",
          "speaker_id": "2",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002189.1.31",
          "form": "정말 너무 멋있어",
          "original_form": "정말 너무 멋있어",
          "speaker_id": "1",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002189.1.32",
          "form": "군대에서도 처우 개선 좀 해줫으면해",
          "original_form": "군대에서도 처우 개선 좀 해줫으면해",
          "speaker_id": "1",
          "time": "20210820 00:24"
        },
        {
          "id": "MDRW2100002189.1.33",
          "form": "학창시절 평범한 아이였는데 군인에 뜻이 있는 줄은 몰랐어.",
          "original_form": "학창시절 평범한 아이였는데 군인에 뜻이 있는 줄은 몰랐어.",
          "speaker_id": "2",
