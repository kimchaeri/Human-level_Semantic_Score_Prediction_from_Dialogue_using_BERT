      "utterance": [
        {
          "id": "MDRW2100012690.1.1",
          "form": "최근에 구매하신 거 뭔가요?",
          "original_form": "최근에 구매하신 거 뭔가요?",
          "speaker_id": "2",
          "time": "20210825 00:06"
        },
        {
          "id": "MDRW2100012690.1.2",
          "form": "무선이어폰 샀어요..",
          "original_form": "무선이어폰 샀어요..",
          "speaker_id": "1",
          "time": "20210825 00:06"
        },
        {
          "id": "MDRW2100012690.1.3",
          "form": "오 좋죠",
          "original_form": "오 좋죠",
          "speaker_id": "2",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012690.1.4",
          "form": "버즈요?",
          "original_form": "버즈요?",
          "speaker_id": "2",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012690.1.5",
          "form": "아니요.. 버즈는 비싸서.. \n저렴한 큐씨와이요..",
          "original_form": "아니요.. 버즈는 비싸서.. \n저렴한 큐씨와이요..",
          "speaker_id": "1",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012690.1.6",
          "form": "들어보셧나여? 가성비로 유명해요",
          "original_form": "들어보셧나여? 가성비로 유명해요",
          "speaker_id": "1",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012690.1.7",
          "form": "아 뭔지 알아요!!!",
          "original_form": "아 뭔지 알아요!!!",
          "speaker_id": "2",
          "time": "20210825 00:17"
        },
        {
          "id": "MDRW2100012690.1.8",
          "form": "1만몇천원인데.. 올해 뭐 산거중 최고만족... \n10번만쓰다버려도 안아까울것같아요",
          "original_form": "1만몇천원인데.. 올해 뭐 산거중 최고만족... \n10번만쓰다버려도 안아까울것같아요",
          "speaker_id": "1",
          "time": "20210825 13:08"
        },
        {
          "id": "MDRW2100012690.1.9",
          "form": "아 진짜여?",
          "original_form": "아 진짜여?",
          "speaker_id": "2",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100012690.1.10",
          "form": "엄청 가성비 좋군요ㅋㅋㅋㅋ",
          "original_form": "엄청 가성비 좋군요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:09"
        },
        {
          "id": "MDRW2100012690.1.11",
          "form": "버즈 살려고 엄청고민했었는데.. \n이어폰 모쓰세요?",
          "original_form": "버즈 살려고 엄청고민했었는데.. \n이어폰 모쓰세요?",
          "speaker_id": "1",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100012690.1.12",
          "form": "저 에어팟이요 !!!!",
          "original_form": "저 에어팟이요 !!!!",
          "speaker_id": "2",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100012690.1.13",
          "form": "그러다 지금은 그냥 줄 사용해요 ㅋㅋㅋㅋ",
          "original_form": "그러다 지금은 그냥 줄 사용해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100012690.1.14",
          "form": "오.. 왜 갑자기 유선으로?",
          "original_form": "오.. 왜 갑자기 유선으로?",
          "speaker_id": "1",
          "time": "20210825 13:13"
        },
        {
          "id": "MDRW2100012690.1.15",
          "form": "유선이 음질은 더 좋죠..",
          "original_form": "유선이 음질은 더 좋죠..",
          "speaker_id": "1",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100012690.1.16",
          "form": "ㅋㅋ 맞아요",
          "original_form": "ㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100012690.1.17",
          "form": "그래도 무선이",
          "original_form": "그래도 무선이",
          "speaker_id": "2",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100012690.1.18",
          "form": "편한겅 크죠",
          "original_form": "편한겅 크죠",
          "speaker_id": "2",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100012690.1.19",
          "form": "이젠 유선보기 힘든득해요",
          "original_form": "이젠 유선보기 힘든득해요",
          "speaker_id": "1",
