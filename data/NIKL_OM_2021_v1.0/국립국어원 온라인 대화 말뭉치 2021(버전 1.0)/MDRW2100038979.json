      "utterance": [
        {
          "id": "MDRW2100038979.1.1",
          "form": "답이 뭐지",
          "original_form": "답이 뭐지",
          "speaker_id": "2",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100038979.1.2",
          "form": "그러게 긴급조치 비판 유인물 배포로 처벌을 받았네",
          "original_form": "그러게 긴급조치 비판 유인물 배포로 처벌을 받았네",
          "speaker_id": "1",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100038979.1.3",
          "form": "응 이분이 2013년 재심에서 무죄",
          "original_form": "응 이분이 2013년 재심에서 무죄",
          "speaker_id": "2",
          "time": "20210903 00:05"
        },
        {
          "id": "MDRW2100038979.1.4",
          "form": "재판부가 사과하는 마음으로 한 행동이라?",
          "original_form": "재판부가 사과하는 마음으로 한 행동이라?",
          "speaker_id": "1",
          "time": "20210903 00:06"
        },
        {
          "id": "MDRW2100038979.1.5",
          "form": "변호사니 알겠다",
          "original_form": "변호사니 알겠다",
          "speaker_id": "2",
          "time": "20210903 00:06"
        },
        {
          "id": "MDRW2100038979.1.6",
          "form": "판결문에 존댓말을 썻다는데",
          "original_form": "판결문에 존댓말을 썻다는데",
          "speaker_id": "1",
          "time": "20210903 00:06"
        },
        {
          "id": "MDRW2100038979.1.7",
          "form": "원래 해야하는거아니야",
          "original_form": "원래 해야하는거아니야",
          "speaker_id": "2",
          "time": "20210903 00:06"
        },
        {
          "id": "MDRW2100038979.1.8",
          "form": "그러게 선고를 하는 거라서 존댓말이 힘든건가",
          "original_form": "그러게 선고를 하는 거라서 존댓말이 힘든건가",
          "speaker_id": "1",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100038979.1.9",
          "form": "변호사 저분 말씀 잘하신다",
          "original_form": "변호사 저분 말씀 잘하신다",
          "speaker_id": "2",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100038979.1.10",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100038979.1.11",
          "form": "판결문이 한글이 아니었나",
          "original_form": "판결문이 한글이 아니었나",
          "speaker_id": "2",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100038979.1.12",
          "form": "법에 원래 한자를 많이 쓰잖아",
          "original_form": "법에 원래 한자를 많이 쓰잖아",
          "speaker_id": "1",
          "time": "20210903 00:08"
        },
