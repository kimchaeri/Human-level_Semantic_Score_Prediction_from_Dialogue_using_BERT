      "utterance": [
        {
          "id": "MDRW2100004987.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210821 19:45"
        },
        {
          "id": "MDRW2100004987.1.2",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "2",
          "time": "20210821 19:45"
        },
        {
          "id": "MDRW2100004987.1.3",
          "form": "닉네임이 좋아요...",
          "original_form": "닉네임이 좋아요...",
          "speaker_id": "1",
          "time": "20210821 19:46"
        },
        {
          "id": "MDRW2100004987.1.4",
          "form": "그래서 제 행복과 관련된 주제로 바꿔봣어요",
          "original_form": "그래서 제 행복과 관련된 주제로 바꿔봣어요",
          "speaker_id": "1",
          "time": "20210821 19:46"
        },
        {
          "id": "MDRW2100004987.1.5",
          "form": "아 네 ㅋㅋ편하신대로 하시면되세요",
          "original_form": "아 네 ㅋㅋ편하신대로 하시면되세요",
          "speaker_id": "2",
          "time": "20210821 19:49"
        },
        {
          "id": "MDRW2100004987.1.6",
          "form": "ㅋㅋㅋㅋ넹 오늘 뭐 드셨어요??",
          "original_form": "ㅋㅋㅋㅋ넹 오늘 뭐 드셨어요??",
          "speaker_id": "1",
          "time": "20210821 19:50"
        },
        {
          "id": "MDRW2100004987.1.7",
          "form": "식음료 좋아하시는거 있으세요?",
          "original_form": "식음료 좋아하시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210821 19:51"
        },
        {
          "id": "MDRW2100004987.1.8",
          "form": "저는 제 닉처럼 간짜장 조아해요!",
          "original_form": "저는 제 닉처럼 간짜장 조아해요!",
          "speaker_id": "1",
          "time": "20210821 19:55"
        },
        {
          "id": "MDRW2100004987.1.9",
          "form": "오늘도 먹었답니다... 헤",
          "original_form": "오늘도 먹었답니다... 헤",
          "speaker_id": "1",
          "time": "20210821 19:55"
        },
        {
          "id": "MDRW2100004987.1.10",
          "form": "아아 ㅋㅋ 간짜장 저도 좋아해요",
          "original_form": "아아 ㅋㅋ 간짜장 저도 좋아해요",
          "speaker_id": "2",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100004987.1.11",
          "form": "많이 좋아하시나봐요. 닉네임까지ㅋㅋ",
          "original_form": "많이 좋아하시나봐요. 닉네임까지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100004987.1.12",
          "form": "ㅋㅋㅋㅋㅋ맞아요 생각나는 게 없기도 하고",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 생각나는 게 없기도 하고",
          "speaker_id": "1",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100004987.1.13",
          "form": "오늘도 먹어서 닉으로 지어봤어요",
          "original_form": "오늘도 먹어서 닉으로 지어봤어요",
          "speaker_id": "1",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100004987.1.14",
          "form": "ㅋㅋ 그러시구나 ^^  간짜장 얘기하니 먹고싶네요 히히",
          "original_form": "ㅋㅋ 그러시구나 ^^  간짜장 얘기하니 먹고싶네요 히히",
          "speaker_id": "2",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100004987.1.15",
          "form": "저는 오늘 대구탕먹었어요.",
          "original_form": "저는 오늘 대구탕먹었어요.",
          "speaker_id": "2",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100004987.1.16",
          "form": "맛있더라구요.",
          "original_form": "맛있더라구요.",
          "speaker_id": "2",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100004987.1.17",
          "form": "대구탕! 맛있었겠네요",
          "original_form": "대구탕! 맛있었겠네요",
          "speaker_id": "1",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100004987.1.18",
          "form": "생선 좋아하세요?",
          "original_form": "생선 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100004987.1.19",
          "form": "네 생선좋아해요.ㅋ",
          "original_form": "네 생선좋아해요.ㅋ",
          "speaker_id": "2",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100004987.1.20",
          "form": "다른 좋아하는 음식은 있으세요?",
          "original_form": "다른 좋아하는 음식은 있으세요?",
          "speaker_id": "2",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100004987.1.21",
          "form": "저는 패스트푸드 좋아해요",
          "original_form": "저는 패스트푸드 좋아해요",
          "speaker_id": "1",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100004987.1.22",
          "form": "몸에 안 좋긴 하지만...",
          "original_form": "몸에 안 좋긴 하지만...",
          "speaker_id": "1",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100004987.1.23",
          "form": "계속 먹고 싶더라고요",
          "original_form": "계속 먹고 싶더라고요",
          "speaker_id": "1",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100004987.1.24",
          "form": "초딩입맛이라 그런가ㅋㅋㅋ ㅠㅠ",
          "original_form": "초딩입맛이라 그런가ㅋㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100004987.1.25",
          "form": "ㅋㅋ저도 초딩입맛이요ㅋㅋ",
          "original_form": "ㅋㅋ저도 초딩입맛이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100004987.1.26",
          "form": "건강한맛보다는",
          "original_form": "건강한맛보다는",
          "speaker_id": "2",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100004987.1.27",
          "form": "단짠단짠좋아해요",
          "original_form": "단짠단짠좋아해요",
          "speaker_id": "2",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100004987.1.28",
          "form": "단짠단짠 최고죠",
          "original_form": "단짠단짠 최고죠",
          "speaker_id": "1",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100004987.1.29",
          "form": "찜닭이랑 볶음밥좋아하구요.ㅋㅋ",
          "original_form": "찜닭이랑 볶음밥좋아하구요.ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100004987.1.30",
          "form": "커피도 사랑합니다ㅋㅋ",
          "original_form": "커피도 사랑합니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:31"
        }
      ]
