      "utterance": [
        {
          "id": "MDRW2100030346.1.1",
          "form": "하이 name2",
          "original_form": "하이 &name2&",
          "speaker_id": "1",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100030346.1.2",
          "form": "아임 아이언맨",
          "original_form": "아임 아이언맨",
          "speaker_id": "1",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100030346.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ안녕하세요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ안녕하세요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030346.1.4",
          "form": "왓두유라이크 쇼핑 아이템?",
          "original_form": "왓두유라이크 쇼핑 아이템?",
          "speaker_id": "1",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030346.1.5",
          "form": "역시 먹을거죠 ㅋㅋㅋㅋㅋ",
          "original_form": "역시 먹을거죠 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100030346.1.6",
          "form": "님은?",
          "original_form": "님은?",
          "speaker_id": "2",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100030346.1.7",
          "form": "미투",
          "original_form": "미투",
          "speaker_id": "1",
          "time": "20210904 00:11"
        },
        {
          "id": "MDRW2100030346.1.8",
          "form": "라이크 치킨",
          "original_form": "라이크 치킨",
          "speaker_id": "1",
          "time": "20210904 00:11"
        },
        {
          "id": "MDRW2100030346.1.9",
          "form": "배고프네요",
          "original_form": "배고프네요",
          "speaker_id": "1",
          "time": "20210904 00:11"
        },
        {
          "id": "MDRW2100030346.1.10",
          "form": "맵슐랭 먹고싶네여",
          "original_form": "맵슐랭 먹고싶네여",
          "speaker_id": "1",
          "time": "20210904 00:12"
        },
        {
          "id": "MDRW2100030346.1.11",
          "form": "전 강경한 후라이드파입니다",
          "original_form": "전 강경한 후라이드파입니다",
          "speaker_id": "2",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100030346.1.12",
          "form": "비비큐나 후참잘 후라이드가 먹고싶어요 ㅋㅋㅋㅋㅋ",
          "original_form": "비비큐나 후참잘 후라이드가 먹고싶어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100030346.1.13",
          "form": "저도 휘라이드파긴한데",
          "original_form": "저도 휘라이드파긴한데",
          "speaker_id": "1",
          "time": "20210904 00:14"
        },
        {
          "id": "MDRW2100030346.1.14",
          "form": "맵슐랭 존맛입니다",
          "original_form": "맵슐랭 존맛입니다",
          "speaker_id": "1",
          "time": "20210904 00:14"
        },
        {
          "id": "MDRW2100030346.1.15",
          "form": "드셔보십셔",
          "original_form": "드셔보십셔",
          "speaker_id": "1",
          "time": "20210904 00:14"
        },
        {
          "id": "MDRW2100030346.1.16",
          "form": "맵슐랭이 자담치킨인가요?",
          "original_form": "맵슐랭이 자담치킨인가요?",
          "speaker_id": "2",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100030346.1.17",
          "form": "네 먹으면 화장실 잘갑니다",
          "original_form": "네 먹으면 화장실 잘갑니다",
          "speaker_id": "1",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100030346.1.18",
          "form": "추천 감사합니다 ㅋㅋㅋ",
          "original_form": "추천 감사합니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:22"
        },
        {
          "id": "MDRW2100030346.1.19",
          "form": "자담치킨에서는 후라이드만 먹어봐서 맵슐랭이 괜찮은줄 몰랏네요",
          "original_form": "자담치킨에서는 후라이드만 먹어봐서 맵슐랭이 괜찮은줄 몰랏네요",
          "speaker_id": "2",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100030346.1.20",
          "form": "가끔 한번씩 먹으면 맛있어요 저도 강경 후라이드파였어요ㅋㅋ",
          "original_form": "가끔 한번씩 먹으면 맛있어요 저도 강경 후라이드파였어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:23"
        },
        {
          "id": "MDRW2100030346.1.21",
          "form": "후라이드 최애는 어디?",
          "original_form": "후라이드 최애는 어디?",
          "speaker_id": "2",
          "time": "20210904 00:25"
        },
        {
          "id": "MDRW2100030346.1.22",
          "form": "전 네네랑 비비큐!",
          "original_form": "전 네네랑 비비큐!",
          "speaker_id": "1",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100030346.1.23",
          "form": "아 치킨먹고싶다",
          "original_form": "아 치킨먹고싶다",
          "speaker_id": "1",
          "time": "20210904 00:27"
        }
      ]
    }
