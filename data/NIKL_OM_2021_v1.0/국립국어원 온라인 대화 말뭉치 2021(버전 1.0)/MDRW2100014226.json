      "utterance": [
        {
          "id": "MDRW2100014226.1.1",
          "form": "쇼핑 좋아하시나요?",
          "original_form": "쇼핑 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014226.1.2",
          "form": "저는 쇼핑 안좋아해요",
          "original_form": "저는 쇼핑 안좋아해요",
          "speaker_id": "1",
          "time": "20210825 18:04"
        },
        {
          "id": "MDRW2100014226.1.3",
          "form": "체력이 빠져나가는 느낌이죠",
          "original_form": "체력이 빠져나가는 느낌이죠",
          "speaker_id": "1",
          "time": "20210825 18:04"
        },
        {
          "id": "MDRW2100014226.1.4",
          "form": "맞아요 살거 있으면 그날 다 사고 안나와야해요",
          "original_form": "맞아요 살거 있으면 그날 다 사고 안나와야해요",
          "speaker_id": "2",
          "time": "20210825 18:04"
        },
        {
          "id": "MDRW2100014226.1.5",
          "form": "하지만 요샌 인터넷이란게 있잖아요?",
          "original_form": "하지만 요샌 인터넷이란게 있잖아요?",
          "speaker_id": "2",
          "time": "20210825 18:05"
        },
        {
          "id": "MDRW2100014226.1.6",
          "form": "최근에 구매한 물건 있으신가요?",
          "original_form": "최근에 구매한 물건 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 18:05"
        },
        {
          "id": "MDRW2100014226.1.7",
          "form": "아뇨 쇼핑 안좋아해요.",
          "original_form": "아뇨 쇼핑 안좋아해요.",
          "speaker_id": "1",
          "time": "20210825 18:06"
        },
        {
          "id": "MDRW2100014226.1.8",
          "form": "그래서 구매한물건이 무엇인지도 모르겠어요",
          "original_form": "그래서 구매한물건이 무엇인지도 모르겠어요",
          "speaker_id": "1",
          "time": "20210825 18:06"
        },
        {
          "id": "MDRW2100014226.1.9",
          "form": "돈이 없으시군요..",
          "original_form": "돈이 없으시군요..",
          "speaker_id": "2",
          "time": "20210825 18:07"
        },
        {
          "id": "MDRW2100014226.1.10",
          "form": "있어도 안사게되요",
          "original_form": "있어도 안사게되요",
          "speaker_id": "1",
          "time": "20210825 18:07"
        },
        {
          "id": "MDRW2100014226.1.11",
          "form": "그렇군요 .. 그럼 사고싶은 물건은 있어요?",
          "original_form": "그렇군요 .. 그럼 사고싶은 물건은 있어요?",
          "speaker_id": "2",
          "time": "20210825 18:08"
        },
        {
          "id": "MDRW2100014226.1.12",
          "form": "아뇨.없어요.",
          "original_form": "아뇨.없어요.",
          "speaker_id": "1",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014226.1.13",
          "form": "단호하시네요",
          "original_form": "단호하시네요",
          "speaker_id": "2",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014226.1.14",
          "form": "name2은 최근 구매한 물건이",
          "original_form": "&name2&은 최근 구매한 물건이",
          "speaker_id": "1",
          "time": "20210825 18:11"
        },
        {
          "id": "MDRW2100014226.1.15",
          "form": "있나요?",
          "original_form": "있나요?",
          "speaker_id": "1",
          "time": "20210825 18:11"
        },
        {
          "id": "MDRW2100014226.1.16",
          "form": "네 선풍기요",
          "original_form": "네 선풍기요",
          "speaker_id": "2",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100014226.1.17",
          "form": "발로 차서 부서졌어요",
          "original_form": "발로 차서 부서졌어요",
          "speaker_id": "2",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100014226.1.18",
          "form": "아이고 어쩌다 그걸 부셨을까잉",
          "original_form": "아이고 어쩌다 그걸 부셨을까잉",
          "speaker_id": "1",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014226.1.19",
          "form": "아이고 글게 말이요...",
          "original_form": "아이고 글게 말이요...",
          "speaker_id": "2",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014226.1.20",
          "form": "요새 시대가 좋아서~~",
          "original_form": "요새 시대가 좋아서~~",
          "speaker_id": "2",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014226.1.21",
          "form": "인터넷으로 사부렸당께",
          "original_form": "인터넷으로 사부렸당께",
          "speaker_id": "2",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100014226.1.22",
          "form": "이야 좋은거 사셨나벼?",
          "original_form": "이야 좋은거 사셨나벼?",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014226.1.23",
          "form": "그 뭐시냐 요오즘 애들은",
          "original_form": "그 뭐시냐 요오즘 애들은",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014226.1.24",
          "form": "서",
          "original_form": "서",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014226.1.25",
          "form": "스",
          "original_form": "스",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014226.1.26",
          "form": "서쿨머시기 쓴다던디",
          "original_form": "서쿨머시기 쓴다던디",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014226.1.27",
          "form": "서스가 뭐시당가?",
          "original_form": "서스가 뭐시당가?",
          "speaker_id": "2",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014226.1.28",
          "form": "임자도 그랴?",
          "original_form": "임자도 그랴?",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014226.1.29",
          "form": "서큘인가뭔가",
          "original_form": "서큘인가뭔가",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014226.1.30",
          "form": "요즘 애들 말이 보통 어려워야지 에잉",
          "original_form": "요즘 애들 말이 보통 어려워야지 에잉",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100014226.1.31",
          "form": "아 서큘레이터",
          "original_form": "아 서큘레이터",
          "speaker_id": "2",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014226.1.32",
          "form": "자네 무식해서 못쓰겟구만",
          "original_form": "자네 무식해서 못쓰겟구만",
          "speaker_id": "2",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014226.1.33",
          "form": "에잉쯧",
          "original_form": "에잉쯧",
          "speaker_id": "2",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014226.1.34",
          "form": "세월이 야속해",
          "original_form": "세월이 야속해",
          "speaker_id": "1",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014226.1.35",
          "form": "이 여편네 물건도 다 직접가서 보고 사겠구만",
          "original_form": "이 여편네 물건도 다 직접가서 보고 사겠구만",
          "speaker_id": "2",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100014226.1.36",
          "form": "요샌 인터넷으로 다들 산다구",
          "original_form": "요샌 인터넷으로 다들 산다구",
          "speaker_id": "2",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100014226.1.37",
          "form": "아이 고럼",
          "original_form": "아이 고럼",
          "speaker_id": "1",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100014226.1.38",
          "form": "야~ 이놈이 단단허니",
          "original_form": "야~ 이놈이 단단허니",
          "speaker_id": "1",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014226.1.39",
          "form": "오래가겠구만",
          "original_form": "오래가겠구만",
          "speaker_id": "1",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014226.1.40",
          "form": "하는게지",
          "original_form": "하는게지",
          "speaker_id": "1",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014226.1.41",
          "form": "뭐가 단단하단거여??",
          "original_form": "뭐가 단단하단거여??",
          "speaker_id": "2",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014226.1.42",
          "form": "선풍ㅇ기",
          "original_form": "선풍ㅇ기",
          "speaker_id": "1",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014226.1.43",
          "form": "님아",
          "original_form": "님아",
          "speaker_id": "1",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014226.1.44",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아니 선풍기를 단단함을 보고사?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아니 선풍기를 단단함을 보고사?",
          "speaker_id": "2",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014226.1.45",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014226.1.46",
          "form": "보통 성능을 따지지 않니?",
          "original_form": "보통 성능을 따지지 않니?",
          "speaker_id": "2",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014226.1.47",
          "form": "머리가 단단한가",
          "original_form": "머리가 단단한가",
          "speaker_id": "1",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014226.1.48",
          "form": "봐야지",
          "original_form": "봐야지",
          "speaker_id": "1",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014226.1.49",
          "form": "자네 머리가 단단한건 내 잘 알겠네..",
          "original_form": "자네 머리가 단단한건 내 잘 알겠네..",
          "speaker_id": "2",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014226.1.50",
          "form": "홀홀홀",
          "original_form": "홀홀홀",
          "speaker_id": "1",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100014226.1.51",
          "form": "슬슬 날씨가 쌀쌀해지니",
          "original_form": "슬슬 날씨가 쌀쌀해지니",
          "speaker_id": "1",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100014226.1.52",
          "form": "코트 한벌 사야하지않겠나",
          "original_form": "코트 한벌 사야하지않겠나",
          "speaker_id": "1",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014226.1.53",
          "form": "자네 얼죽코구만?",
          "original_form": "자네 얼죽코구만?",
          "speaker_id": "2",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014226.1.54",
          "form": "난 패딩이 좋아",
          "original_form": "난 패딩이 좋아",
          "speaker_id": "2",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014226.1.55",
          "form": "계절 넘어간다고 패딩 할인을 하던데",
          "original_form": "계절 넘어간다고 패딩 할인을 하던데",
          "speaker_id": "2",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014226.1.56",
          "form": "하나 장만하도록해",
          "original_form": "하나 장만하도록해",
          "speaker_id": "2",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014226.1.57",
          "form": "아 당연히 패딩이지",
          "original_form": "아 당연히 패딩이지",
          "speaker_id": "1",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014226.1.58",
          "form": "하지만 가을에 패딩을 입기에는 너무 이른감이 있어",
          "original_form": "하지만 가을에 패딩을 입기에는 너무 이른감이 있어",
          "speaker_id": "1",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014226.1.59",
          "form": "맞아 난 가디건도 좋아해",
          "original_form": "맞아 난 가디건도 좋아해",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014226.1.60",
          "form": "살이쪄서 그런가 코트가 답답허이..",
          "original_form": "살이쪄서 그런가 코트가 답답허이..",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014226.1.61",
          "form": "자네 중고거래 해본적 있나?",
          "original_form": "자네 중고거래 해본적 있나?",
          "speaker_id": "2",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014226.1.62",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014226.1.63",
          "form": "한번도 해본적 없다네",
          "original_form": "한번도 해본적 없다네",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014226.1.64",
          "form": "사실 나도 없다네",
          "original_form": "사실 나도 없다네",
          "speaker_id": "2",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014226.1.65",
          "form": "아 생각난건데",
          "original_form": "아 생각난건데",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014226.1.66",
          "form": "요새 당근이라고 많이들 하더라구",
          "original_form": "요새 당근이라고 많이들 하더라구",
          "speaker_id": "2",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014226.1.67",
          "form": "최근 구매한 물건이 있어",
          "original_form": "최근 구매한 물건이 있어",
          "speaker_id": "1",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014226.1.68",
          "form": "마라탕 때문에 쪄버린 내살을",
          "original_form": "마라탕 때문에 쪄버린 내살을",
          "speaker_id": "1",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014226.1.69",
          "form": "빼게 도와줄",
          "original_form": "빼게 도와줄",
          "speaker_id": "1",
