        },
        {
          "id": "MDRW2100020324.1.70",
          "form": "이유는 몰라..",
          "original_form": "이유는 몰라..",
          "speaker_id": "1",
          "time": "20210828 01:33"
        },
        {
          "id": "MDRW2100020324.1.71",
          "form": "신기하다",
          "original_form": "신기하다",
          "speaker_id": "2",
          "time": "20210828 01:33"
        },
        {
          "id": "MDRW2100020324.1.72",
          "form": "글고 나 깨어나도 금방 잠깨서",
          "original_form": "글고 나 깨어나도 금방 잠깨서",
          "speaker_id": "1",
          "time": "20210828 01:33"
        },
        {
          "id": "MDRW2100020324.1.73",
          "form": "뭐 물어봐도 ㄱㅊ",
