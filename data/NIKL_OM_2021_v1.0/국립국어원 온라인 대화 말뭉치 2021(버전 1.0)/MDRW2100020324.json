          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210828 01:23"
        },
        {
          "id": "MDRW2100020324.1.6",
          "form": "ㄱㅊ네",
          "original_form": "ㄱㅊ네",
          "speaker_id": "1",
          "time": "20210828 01:23"
        },
        {
          "id": "MDRW2100020324.1.7",
          "form": "근데 그건 보통 그러지않나",
          "original_form": "근데 그건 보통 그러지않나",
          "speaker_id": "1",
          "time": "20210828 01:24"
        },
        {
          "id": "MDRW2100020324.1.8",
          "form": "걱정되잖아",
          "original_form": "걱정되잖아",
          "speaker_id": "1",
          "time": "20210828 01:24"
        },
        {
          "id": "MDRW2100020324.1.9",
          "form": "구래도",
          "original_form": "구래도",
          "speaker_id": "2",
          "time": "20210828 01:24"
        },
        {
          "id": "MDRW2100020324.1.10",
          "form": "근대 이것도 나나 잘해야항듯ㅋ",
          "original_form": "근대 이것도 나나 잘해야항듯ㅋ",
          "speaker_id": "2",
          "time": "20210828 01:24"
        },
        {
          "id": "MDRW2100020324.1.11",
          "form": "나는..저런 연락도 개잘안해서",
          "original_form": "나는..저런 연락도 개잘안해서",
          "speaker_id": "2",
          "time": "20210828 01:25"
        },
        {
          "id": "MDRW2100020324.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:25"
        },
        {
          "id": "MDRW2100020324.1.13",
          "form": "그래!!",
          "original_form": "그래!!",
          "speaker_id": "1",
          "time": "20210828 01:25"
        },
        {
          "id": "MDRW2100020324.1.14",
          "form": "저런걸로 연락하는 습관이 너무 안되있어요",
          "original_form": "저런걸로 연락하는 습관이 너무 안되있어요",
          "speaker_id": "2",
          "time": "20210828 01:25"
        },
        {
          "id": "MDRW2100020324.1.15",
          "form": "오 그렇구나",
          "original_form": "오 그렇구나",
          "speaker_id": "1",
          "time": "20210828 01:25"
        },
        {
          "id": "MDRW2100020324.1.16",
          "form": "나는.  항상..",
          "original_form": "나는.  항상..",
          "speaker_id": "1",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100020324.1.17",
          "form": "다른 가족이 먼저",
          "original_form": "다른 가족이 먼저",
          "speaker_id": "1",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100020324.1.18",
          "form": "물어봐서",
          "original_form": "물어봐서",
          "speaker_id": "1",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100020324.1.19",
          "form": "얼결에 알려주게되는..",
          "original_form": "얼결에 알려주게되는..",
          "speaker_id": "1",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100020324.1.20",
          "form": "그래서 습괌이 됏..을지도???",
          "original_form": "그래서 습괌이 됏..을지도???",
          "speaker_id": "1",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100020324.1.21",
          "form": "근데 늦게들어가는건 아직",
          "original_form": "근데 늦게들어가는건 아직",
          "speaker_id": "1",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100020324.1.22",
          "form": "습관이 덜더ㅣㅁ",
          "original_form": "습관이 덜더ㅣㅁ",
          "speaker_id": "1",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100020324.1.23",
          "form": "외박으뉴몰라도",
          "original_form": "외박으뉴몰라도",
          "speaker_id": "1",
          "time": "20210828 01:26"
