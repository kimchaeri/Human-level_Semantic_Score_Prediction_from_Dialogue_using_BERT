          "speaker_id": "1",
          "time": "20210828 01:29"
        },
        {
          "id": "MDRW2100020324.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:29"
        },
        {
          "id": "MDRW2100020324.1.35",
          "form": "여름겨울에",
          "original_form": "여름겨울에",
          "speaker_id": "2",
          "time": "20210828 01:29"
        },
        {
          "id": "MDRW2100020324.1.36",
          "form": "엄마랑 님되는거아밈?;",
          "original_form": "엄마랑 님되는거아밈?;",
          "speaker_id": "2",
          "time": "20210828 01:29"
        },
        {
          "id": "MDRW2100020324.1.37",
          "form": "나 그래도 에어컨은",
          "original_form": "나 그래도 에어컨은",
          "speaker_id": "2",
          "time": "20210828 01:29"
        },
        {
          "id": "MDRW2100020324.1.38",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:29"
        },
        {
          "id": "MDRW2100020324.1.39",
          "form": "내가 이불덮으면돼",
          "original_form": "내가 이불덮으면돼",
          "speaker_id": "2",
          "time": "20210828 01:30"
        },
        {
          "id": "MDRW2100020324.1.40",
          "form": "나 추위는 양보가능",
          "original_form": "나 추위는 양보가능",
          "speaker_id": "1",
          "time": "20210828 01:30"
        },
        {
          "id": "MDRW2100020324.1.41",
          "form": "더위는 안돼",
          "original_form": "더위는 안돼",
          "speaker_id": "1",
          "time": "20210828 01:30"
        },
        {
          "id": "MDRW2100020324.1.42",
          "form": "더우면 성격조짐",
          "original_form": "더우면 성격조짐",
          "speaker_id": "1",
          "time": "20210828 01:30"
        },
        {
          "id": "MDRW2100020324.1.43",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아웃겨",
