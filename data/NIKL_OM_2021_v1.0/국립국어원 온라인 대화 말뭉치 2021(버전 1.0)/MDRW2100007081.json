      "utterance": [
        {
          "id": "MDRW2100007081.1.1",
          "form": "자빱tv가 나에겐 영화고 드라마고 뮤지컬이고 음원 사이트다..",
          "original_form": "자빱tv가 나에겐 영화고 드라마고 뮤지컬이고 음원 사이트다..",
          "speaker_id": "1",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007081.1.2",
          "form": "ㄱㄱ ㄱㄱㄱ ㄱㄱ당연하 소리를 하니",
          "original_form": "ㄱㄱ ㄱㄱㄱ ㄱㄱ당연하 소리를 하니",
          "speaker_id": "2",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007081.1.3",
          "form": "자빱이 내겐 하나의 티비여",
          "original_form": "자빱이 내겐 하나의 티비여",
          "speaker_id": "2",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007081.1.4",
          "form": "하..  우정리 보고싶다",
          "original_form": "하..  우정리 보고싶다",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007081.1.5",
          "form": "우정리 전원일기처럼 nnn회차해줬으면좋겠다",
          "original_form": "우정리 전원일기처럼 nnn회차해줬으면좋겠다",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007081.1.6",
          "form": "근데 우정리 말고도 다른 컨텐츠 보고싶으니까",
          "original_form": "근데 우정리 말고도 다른 컨텐츠 보고싶으니까",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007081.1.7",
          "form": "참아야지",
          "original_form": "참아야지",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007081.1.8",
          "form": "전원일기처럼하면",
          "original_form": "전원일기처럼하면",
          "speaker_id": "2",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007081.1.9",
          "form": "뭔데나언제나쁜말했는데",
          "original_form": "뭔데나언제나쁜말했는데",
          "speaker_id": "2",
          "time": "20210822 00:34"
        },
        {
          "id": "MDRW2100007081.1.10",
          "form": "가끔 나쁜말 인식하더라",
          "original_form": "가끔 나쁜말 인식하더라",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007081.1.11",
          "form": "암튼 전원일기처럼 하면 뭐?",
          "original_form": "암튼 전원일기처럼 하면 뭐?",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007081.1.12",
          "form": "우정리가 나쁜 말이여??진심으루???",
          "original_form": "우정리가 나쁜 말이여??진심으루???",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007081.1.13",
          "form": "무서워서",
          "original_form": "무서워서",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007081.1.14",
          "form": "내심장에 안 좋아",
          "original_form": "내심장에 안 좋아",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007081.1.15",
          "form": "ㅋㅋㄱㅋㅋㅋㅋㅋㄱㅋㅋㅋㄱㅋㅋㅋ",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋㅋㄱㅋㅋㅋㄱㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100007081.1.16",
          "form": "쫌 쫄리긴 하지",
          "original_form": "쫌 쫄리긴 하지",
          "speaker_id": "1",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100007081.1.17",
          "form": "하지만 다른 컨텐츠도 다 쫄리잖아",
          "original_form": "하지만 다른 컨텐츠도 다 쫄리잖아",
          "speaker_id": "1",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100007081.1.18",
          "form": "조용히해",
          "original_form": "조용히해",
          "speaker_id": "2",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007081.1.19",
          "form": "쫄리는 감각을 즐겨",
          "original_form": "쫄리는 감각을 즐겨",
          "speaker_id": "1",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007081.1.20",
          "form": "그 맛에 스릴러 보는거지",
          "original_form": "그 맛에 스릴러 보는거지",
          "speaker_id": "1",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007081.1.21",
          "form": "그럼나와오늘새벽을달리는건?",
          "original_form": "그럼나와오늘새벽을달리는건?",
          "speaker_id": "2",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007081.1.22",
          "form": "뭐 보게?",
          "original_form": "뭐 보게?",
          "speaker_id": "1",
          "time": "20210822 00:41"
        },
        {
          "id": "MDRW2100007081.1.23",
          "form": "몰라",
          "original_form": "몰라",
          "speaker_id": "2",
          "time": "20210822 00:42"
        },
        {
          "id": "MDRW2100007081.1.24",
          "form": "뭐여",
          "original_form": "뭐여",
          "speaker_id": "1",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007081.1.25",
          "form": "보고싶은 영화 생기면 연락해유",
