      "utterance": [
        {
          "id": "MDRW2100025457.1.1",
          "form": "유제품 안들어간 아이스크림ㅋㅋㅋㅋ",
          "original_form": "유제품 안들어간 아이스크림ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:42"
        },
        {
          "id": "MDRW2100025457.1.2",
          "form": "난 그 까만레몬샤베트머지",
          "original_form": "난 그 까만레몬샤베트머지",
          "speaker_id": "1",
          "time": "20210901 11:42"
        },
        {
          "id": "MDRW2100025457.1.3",
          "form": "아 그 블랙 소브레인가",
          "original_form": "아 그 블랙 소브레인가",
          "speaker_id": "2",
          "time": "20210901 11:42"
        },
        {
          "id": "MDRW2100025457.1.4",
          "form": "그거 먹어보고싶었는데",
          "original_form": "그거 먹어보고싶었는데",
          "speaker_id": "2",
          "time": "20210901 11:42"
        },
        {
          "id": "MDRW2100025457.1.5",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100025457.1.6",
          "form": "그거근데",
          "original_form": "그거근데",
          "speaker_id": "1",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100025457.1.7",
          "form": "손에묻으면",
          "original_form": "손에묻으면",
          "speaker_id": "1",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100025457.1.8",
          "form": "거의호러급이야",
          "original_form": "거의호러급이야",
          "speaker_id": "1",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100025457.1.9",
          "form": "예전에 버스에서 내릴때 그거 밟고",
          "original_form": "예전에 버스에서 내릴때 그거 밟고",
          "speaker_id": "2",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100025457.1.10",
          "form": "끄아아앙",
          "original_form": "끄아아앙",
          "speaker_id": "1",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100025457.1.11",
          "form": "다리에 검은물 범벅되서",
          "original_form": "다리에 검은물 범벅되서",
          "speaker_id": "2",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100025457.1.12",
          "form": "싫어하게됨ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "싫어하게됨ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100025457.1.13",
          "form": "녹는거호러급",
          "original_form": "녹는거호러급",
          "speaker_id": "1",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100025457.1.14",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "1",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100025457.1.15",
          "form": "왜그렇게녹지",
          "original_form": "왜그렇게녹지",
          "speaker_id": "1",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100025457.1.16",
          "form": "너무 끔찍함",
          "original_form": "너무 끔찍함",
          "speaker_id": "2",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100025457.1.17",
          "form": "맛은있는데",
          "original_form": "맛은있는데",
          "speaker_id": "1",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100025457.1.18",
          "form": "진짜ㅜ깜짝놀랐잖아",
          "original_form": "진짜ㅜ깜짝놀랐잖아",
          "speaker_id": "2",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100025457.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100025457.1.20",
          "form": "ㅋㅋㅋㅋ맛은내서타일이야상큼",
          "original_form": "ㅋㅋㅋㅋ맛은내서타일이야상큼",
          "speaker_id": "1",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100025457.1.21",
          "form": "마자 맛있다는데",
          "original_form": "마자 맛있다는데",
          "speaker_id": "2",
          "time": "20210901 11:43"
        },
        {
          "id": "MDRW2100025457.1.22",
          "form": "그거 먹으면 이빨 검게 되는거 아니여?",
          "original_form": "그거 먹으면 이빨 검게 되는거 아니여?",
          "speaker_id": "2",
          "time": "20210901 11:44"
        },
        {
          "id": "MDRW2100025457.1.23",
          "form": "ㅇㅇㅇ 맛있어",
          "original_form": "ㅇㅇㅇ 맛있어",
          "speaker_id": "1",
          "time": "20210901 11:44"
        },
        {
          "id": "MDRW2100025457.1.24",
