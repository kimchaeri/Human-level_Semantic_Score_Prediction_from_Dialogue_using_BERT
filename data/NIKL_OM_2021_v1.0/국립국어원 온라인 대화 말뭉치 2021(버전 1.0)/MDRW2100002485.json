        {
          "id": "MDRW2100002485.1.1",
          "form": "여행 좋아해요?",
          "original_form": "여행 좋아해요?",
          "speaker_id": "2",
          "time": "20210820 12:51"
        },
        {
          "id": "MDRW2100002485.1.2",
          "form": "저는 좋아해요!",
          "original_form": "저는 좋아해요!",
          "speaker_id": "1",
          "time": "20210820 12:53"
        },
        {
          "id": "MDRW2100002485.1.3",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "1",
          "time": "20210820 12:53"
        },
        {
          "id": "MDRW2100002485.1.4",
          "form": "좋아하죠~",
          "original_form": "좋아하죠~",
          "speaker_id": "2",
          "time": "20210820 12:54"
        },
        {
          "id": "MDRW2100002485.1.5",
          "form": "어디가보셨나요?",
          "original_form": "어디가보셨나요?",
          "speaker_id": "1",
          "time": "20210820 12:54"
        },
        {
          "id": "MDRW2100002485.1.6",
          "form": "좋아는 하는데 많이는 못가봤어요ㅠㅠ",
          "original_form": "좋아는 하는데 많이는 못가봤어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 14:43"
        },
        {
          "id": "MDRW2100002485.1.7",
          "form": "저도요ㅠㅠ",
          "original_form": "저도요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002485.1.8",
          "form": "국내라도 가고싶네요",
          "original_form": "국내라도 가고싶네요",
          "speaker_id": "1",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002485.1.9",
          "form": "국내여행도 좋죠~말 잘 통하고 음식 입에 잘 맞고",
          "original_form": "국내여행도 좋죠~말 잘 통하고 음식 입에 잘 맞고",
          "speaker_id": "2",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002485.1.10",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:05"
        },
        {
          "id": "MDRW2100002485.1.11",
          "form": "저는 강릉이랑 전주 가고싶어요ㅠㅠ",
          "original_form": "저는 강릉이랑 전주 가고싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 18:05"
        },
        {
          "id": "MDRW2100002485.1.12",
          "form": "전주!",
          "original_form": "전주!",
          "speaker_id": "2",
          "time": "20210822 22:04"
        },
        {
          "id": "MDRW2100002485.1.13",
          "form": "전주 한옥마을가서 한복입고 놀았던거 생각나네요ㅋㅋ",
          "original_form": "전주 한옥마을가서 한복입고 놀았던거 생각나네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:04"
        },
        {
          "id": "MDRW2100002485.1.14",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:56"
        },
        {
          "id": "MDRW2100002485.1.15",
          "form": "한복이 참 이쁜거같아요",
          "original_form": "한복이 참 이쁜거같아요",
          "speaker_id": "1",
          "time": "20210823 15:56"
        },
        {
          "id": "MDRW2100002485.1.16",
          "form": "전주에 너무잘어울리죠",
          "original_form": "전주에 너무잘어울리죠",
          "speaker_id": "1",
          "time": "20210823 15:56"
        },
        {
          "id": "MDRW2100002485.1.17",
          "form": "맛있는건 별로 못먹었던것같아요ㅠ",
          "original_form": "맛있는건 별로 못먹었던것같아요ㅠ",
          "speaker_id": "2",
          "time": "20210823 18:52"
        },
        {
          "id": "MDRW2100002485.1.18",
          "form": "아 그런가요?",
          "original_form": "아 그런가요?",
          "speaker_id": "1",
          "time": "20210824 10:43"
        },
        {
          "id": "MDRW2100002485.1.19",
          "form": "전주에 맛있는거많던데ㅠ",
          "original_form": "전주에 맛있는거많던데ㅠ",
