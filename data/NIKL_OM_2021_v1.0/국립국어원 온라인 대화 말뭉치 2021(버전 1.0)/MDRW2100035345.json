      "utterance": [
        {
          "id": "MDRW2100035345.1.1",
          "form": "몇시간동안 뭐하십니꽝",
          "original_form": "몇시간동안 뭐하십니꽝",
          "speaker_id": "2",
          "time": "20210829 20:25"
        },
        {
          "id": "MDRW2100035345.1.2",
          "form": "아하 ㅋㅋ닭볶음탕 ㅋㅋㅋ",
          "original_form": "아하 ㅋㅋ닭볶음탕 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:37"
        },
        {
          "id": "MDRW2100035345.1.3",
          "form": "유명한가보다 ㅋㅋㅋ",
          "original_form": "유명한가보다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:37"
        },
        {
          "id": "MDRW2100035345.1.4",
          "form": "뭐 그냥 ㅋㅋㅋ 이동네에도 생겼능데",
          "original_form": "뭐 그냥 ㅋㅋㅋ 이동네에도 생겼능데",
          "speaker_id": "2",
          "time": "20210830 11:37"
        },
        {
          "id": "MDRW2100035345.1.5",
          "form": "저녁애 칼칼하게 먹어보려고 궁물넉넉한 닭볶음탕 ㅋㅋ",
          "original_form": "저녁애 칼칼하게 먹어보려고 궁물넉넉한 닭볶음탕 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:37"
        },
        {
          "id": "MDRW2100035345.1.6",
          "form": "헐 수제비도 들었어 ㅠㅠ",
          "original_form": "헐 수제비도 들었어 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 11:38"
        },
        {
          "id": "MDRW2100035345.1.7",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "2",
          "time": "20210830 11:38"
        },
        {
          "id": "MDRW2100035345.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 배고프다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 배고프다",
          "speaker_id": "2",
          "time": "20210830 11:38"
        },
        {
          "id": "MDRW2100035345.1.9",
          "form": "ㅋㅋㅋ나도 엄마 언니가 준 투썸쿠폰쓰러나감",
          "original_form": "ㅋㅋㅋ나도 엄마 언니가 준 투썸쿠폰쓰러나감",
          "speaker_id": "1",
          "time": "20210830 11:38"
        },
        {
          "id": "MDRW2100035345.1.10",
          "form": "너두?",
          "original_form": "너두?",
          "speaker_id": "2",
          "time": "20210830 11:41"
        },
        {
          "id": "MDRW2100035345.1.11",
          "form": "난 아마1+1잇아서 저번에 겸사겸사 투썸마심",
          "original_form": "난 아마1+1잇아서 저번에 겸사겸사 투썸마심",
          "speaker_id": "2",
          "time": "20210830 11:42"
        },
        {
          "id": "MDRW2100035345.1.12",
          "form": "아 문자옴 ㅋㅋㅋ",
          "original_form": "아 문자옴 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:42"
        },
        {
          "id": "MDRW2100035345.1.13",
