        },
        {
          "id": "MDRW2100036586.1.2",
          "form": "어떤 죽을 좋아하니",
          "original_form": "어떤 죽을 좋아하니",
          "speaker_id": "1",
          "time": "20210831 01:11"
        },
        {
          "id": "MDRW2100036586.1.3",
          "form": "나는 김치 낙지 쪽",
          "original_form": "나는 김치 낙지 쪽",
          "speaker_id": "2",
          "time": "20210831 01:11"
        },
        {
          "id": "MDRW2100036586.1.4",
          "form": "호박죽은 어때",
          "original_form": "호박죽은 어때",
          "speaker_id": "1",
          "time": "20210831 01:11"
        },
        {
          "id": "MDRW2100036586.1.5",
          "form": "맛있지 호박죽",
          "original_form": "맛있지 호박죽",
          "speaker_id": "2",
          "time": "20210831 01:11"
        },
        {
          "id": "MDRW2100036586.1.6",
          "form": "나는 녹두죽을 싫어해",
          "original_form": "나는 녹두죽을 싫어해",
          "speaker_id": "1",
          "time": "20210831 01:11"
        },
        {
          "id": "MDRW2100036586.1.7",
          "form": "나도 좋아하진 않아",
          "original_form": "나도 좋아하진 않아",
          "speaker_id": "2",
          "time": "20210831 01:11"
        },
        {
          "id": "MDRW2100036586.1.8",
          "form": "삼계 죽은 어때",
          "original_form": "삼계 죽은 어때",
          "speaker_id": "1",
          "time": "20210831 01:12"
        },
        {
          "id": "MDRW2100036586.1.9",
          "form": "최악인 것 같아",
          "original_form": "최악인 것 같아",
          "speaker_id": "2",
          "time": "20210831 01:12"
        },
        {
          "id": "MDRW2100036586.1.10",
          "form": "아플 때는 역시 죽이 땡겨",
          "original_form": "아플 때는 역시 죽이 땡겨",
          "speaker_id": "1",
          "time": "20210831 01:12"
        },
        {
          "id": "MDRW2100036586.1.11",
          "form": "소화도 잘되고 좋지",
          "original_form": "소화도 잘되고 좋지",
          "speaker_id": "2",
          "time": "20210831 01:12"
        },
        {
          "id": "MDRW2100036586.1.12",
          "form": "근데 죽은 근 비싸",
          "original_form": "근데 죽은 근 비싸",
          "speaker_id": "1",
          "time": "20210831 01:12"
        },
        {
          "id": "MDRW2100036586.1.13",
          "form": "맞아 되게 비싸서 못 사먹어",
          "original_form": "맞아 되게 비싸서 못 사먹어",
          "speaker_id": "2",
          "time": "20210831 01:12"
        },
        {
          "id": "MDRW2100036586.1.14",
          "form": "그래도 먹고 싶다",
          "original_form": "그래도 먹고 싶다",
