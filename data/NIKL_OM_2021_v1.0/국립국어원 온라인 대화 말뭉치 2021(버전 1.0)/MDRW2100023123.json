      "utterance": [
        {
          "id": "MDRW2100023123.1.1",
          "form": "안녕하세요 저녁드셨나여?",
          "original_form": "안녕하세요 저녁드셨나여?",
          "speaker_id": "1",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100023123.1.2",
          "form": "네 저는 카레랑 해서 먹었습니다ㅎㅎ",
          "original_form": "네 저는 카레랑 해서 먹었습니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023123.1.3",
          "form": "그쪽은 저녁 드셨어요?",
          "original_form": "그쪽은 저녁 드셨어요?",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023123.1.4",
          "form": "아직 안 먹엇어요",
          "original_form": "아직 안 먹엇어요",
          "speaker_id": "1",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100023123.1.5",
          "form": "배고프네요ㅠㅠㅠ",
          "original_form": "배고프네요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100023123.1.6",
          "form": "헉 어쩌시다가 아직도 안 드셨어요ㅜㅜ",
          "original_form": "헉 어쩌시다가 아직도 안 드셨어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100023123.1.7",
          "form": "흑흑 귀찮아서 이렇게 됐네요..",
          "original_form": "흑흑 귀찮아서 이렇게 됐네요..",
          "speaker_id": "1",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100023123.1.8",
          "form": "카레는 직접 해드신건가요?",
          "original_form": "카레는 직접 해드신건가요?",
          "speaker_id": "1",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100023123.1.9",
          "form": "아뇨 부모님이 해주셨어요!",
          "original_form": "아뇨 부모님이 해주셨어요!",
          "speaker_id": "2",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100023123.1.10",
          "form": "지금이라도 배달시키는 건 어떠세요?",
          "original_form": "지금이라도 배달시키는 건 어떠세요?",
          "speaker_id": "2",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100023123.1.11",
          "form": "배달이라두ㅎㅎ",
          "original_form": "배달이라두ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100023123.1.12",
          "form": "지금 배달 안 되는 곳에 있어서",
          "original_form": "지금 배달 안 되는 곳에 있어서",
          "speaker_id": "1",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023123.1.13",
          "form": "안될것같아여 ㅠㅠㅠㅠㅠ",
          "original_form": "안될것같아여 ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023123.1.14",
          "form": "햄버거 있는거나 먹을까해영..",
          "original_form": "햄버거 있는거나 먹을까해영..",
          "speaker_id": "1",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100023123.1.15",
          "form": "악ㅜㅜㅜ제가 다 안타까워요",
          "original_form": "악ㅜㅜㅜ제가 다 안타까워요",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023123.1.16",
          "form": "햄버거는 무슨맛 햄버거예요?",
          "original_form": "햄버거는 무슨맛 햄버거예요?",
          "speaker_id": "2",
          "time": "20210829 22:21"
        },
        {
          "id": "MDRW2100023123.1.17",
          "form": "더블불고기영!!",
          "original_form": "더블불고기영!!",
          "speaker_id": "1",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100023123.1.18",
          "form": "너무 맛있지않나요 더불 ㅠㅠㅠㅠㅠ",
          "original_form": "너무 맛있지않나요 더불 ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:22"
        },
        {
          "id": "MDRW2100023123.1.19",
          "form": "헉 맥날껀가요???",
          "original_form": "헉 맥날껀가요???",
          "speaker_id": "2",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100023123.1.20",
          "form": "네넹 맥날이요!",
          "original_form": "네넹 맥날이요!",
          "speaker_id": "1",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100023123.1.21",
          "form": "저도 더불 좋아해요 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 더불 좋아해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:36"
        },
        {
          "id": "MDRW2100023123.1.22",
          "form": "젤 좋아하는 건 더블치즈버거지만ㅎㅎ",
          "original_form": "젤 좋아하는 건 더블치즈버거지만ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:36"
        },
        {
          "id": "MDRW2100023123.1.23",
          "form": "더블치즈버거는 어디 건가요?",
          "original_form": "더블치즈버거는 어디 건가요?",
          "speaker_id": "1",
          "time": "20210829 22:36"
        },
        {
          "id": "MDRW2100023123.1.24",
          "form": "여기도 맥날거예요!",
          "original_form": "여기도 맥날거예요!",
          "speaker_id": "2",
          "time": "20210829 22:36"
        },
        {
          "id": "MDRW2100023123.1.25",
          "form": "히든메뉴였는데 최근에 풀린걸로 알아요ㅎㅎ",
          "original_form": "히든메뉴였는데 최근에 풀린걸로 알아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:37"
        },
        {
          "id": "MDRW2100023123.1.26",
          "form": "오옹",
          "original_form": "오옹",
          "speaker_id": "1",
          "time": "20210829 22:40"
        },
        {
          "id": "MDRW2100023123.1.27",
          "form": "맛있겠네요",
          "original_form": "맛있겠네요",
          "speaker_id": "1",
          "time": "20210829 22:40"
        },
        {
          "id": "MDRW2100023123.1.28",
          "form": "맥윙 단종된 거 아시나요ㅠㅠ",
          "original_form": "맥윙 단종된 거 아시나요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:41"
        }
      ]
