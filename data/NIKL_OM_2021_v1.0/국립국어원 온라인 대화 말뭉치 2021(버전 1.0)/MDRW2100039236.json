        },
        {
          "id": "MDRW2100039236.1.2",
          "form": "어떤데?",
          "original_form": "어떤데?",
          "speaker_id": "1",
          "time": "20210903 15:03"
        },
        {
          "id": "MDRW2100039236.1.3",
          "form": "포장 잘 해서 왔더라",
          "original_form": "포장 잘 해서 왔더라",
          "speaker_id": "2",
          "time": "20210903 15:04"
        },
        {
          "id": "MDRW2100039236.1.4",
          "form": "오 진짜?",
          "original_form": "오 진짜?",
          "speaker_id": "1",
          "time": "20210903 15:04"
        },
        {
          "id": "MDRW2100039236.1.5",
          "form": "다행이다 인터넷으로 사서 걱정했는데",
          "original_form": "다행이다 인터넷으로 사서 걱정했는데",
          "speaker_id": "1",
          "time": "20210903 15:04"
        },
        {
          "id": "MDRW2100039236.1.6",
          "form": "납작한 박스에",
          "original_form": "납작한 박스에",
          "speaker_id": "2",
          "time": "20210903 15:04"
        },
        {
          "id": "MDRW2100039236.1.7",
          "form": "신문지깔고 양파 한줄로 해서 왔어",
          "original_form": "신문지깔고 양파 한줄로 해서 왔어",
          "speaker_id": "2",
          "time": "20210903 15:04"
        },
        {
          "id": "MDRW2100039236.1.8",
          "form": "오오오 신경써서 보냈네",
          "original_form": "오오오 신경써서 보냈네",
          "speaker_id": "1",
          "time": "20210903 15:04"
        },
        {
          "id": "MDRW2100039236.1.9",
          "form": "박스에 구멍도 있더라",
          "original_form": "박스에 구멍도 있더라",
          "speaker_id": "2",
          "time": "20210903 15:05"
        },
        {
          "id": "MDRW2100039236.1.10",
          "form": "대박 센스있네",
          "original_form": "대박 센스있네",
          "speaker_id": "1",
          "time": "20210903 15:05"
        },
        {
          "id": "MDRW2100039236.1.11",
          "form": "ㅇㅇ지금 박스 뚜껑 열어서 내놨다",
          "original_form": "ㅇㅇ지금 박스 뚜껑 열어서 내놨다",
          "speaker_id": "2",
          "time": "20210903 15:05"
        },
        {
          "id": "MDRW2100039236.1.12",
          "form": "이대로 보관함 될 거 같애",
          "original_form": "이대로 보관함 될 거 같애",
          "speaker_id": "2",
          "time": "20210903 15:05"
        },
        {
          "id": "MDRW2100039236.1.13",
          "form": "양파 상태는?",
          "original_form": "양파 상태는?",
          "speaker_id": "1",
          "time": "20210903 15:06"
        },
        {
          "id": "MDRW2100039236.1.14",
          "form": "단단하고 괜찮아 보이네",
          "original_form": "단단하고 괜찮아 보이네",
          "speaker_id": "2",
          "time": "20210903 15:06"
        },
        {
          "id": "MDRW2100039236.1.15",
          "form": "굿굿",
          "original_form": "굿굿",
          "speaker_id": "1",
          "time": "20210903 15:06"
        },
        {
          "id": "MDRW2100039236.1.16",
          "form": "양파 이제 많네ㅋㅋ",
          "original_form": "양파 이제 많네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:06"
        },
        {
          "id": "MDRW2100039236.1.17",
          "form": "상하기 전에 부지런히 먹어야지",
          "original_form": "상하기 전에 부지런히 먹어야지",
          "speaker_id": "2",
          "time": "20210903 15:06"
        },
        {
          "id": "MDRW2100039236.1.18",
          "form": "양파 많이 먹어야 돼",
          "original_form": "양파 많이 먹어야 돼",
          "speaker_id": "2",
          "time": "20210903 15:07"
