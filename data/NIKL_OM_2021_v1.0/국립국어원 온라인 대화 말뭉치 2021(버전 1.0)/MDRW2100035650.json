      "utterance": [
        {
          "id": "MDRW2100035650.1.1",
          "form": "레시피 있으세요?",
          "original_form": "레시피 있으세요?",
          "speaker_id": "1",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100035650.1.2",
          "form": "어떤 레시피요?",
          "original_form": "어떤 레시피요?",
          "speaker_id": "2",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100035650.1.3",
          "form": "음 나만의 레시피?",
          "original_form": "음 나만의 레시피?",
          "speaker_id": "1",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100035650.1.4",
          "form": "있으신가요?",
          "original_form": "있으신가요?",
          "speaker_id": "1",
          "time": "20210829 22:06"
        },
        {
          "id": "MDRW2100035650.1.5",
          "form": "따라하기도 벅차서 나만의 레시피는 없어요 ㅎㅎ",
          "original_form": "따라하기도 벅차서 나만의 레시피는 없어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100035650.1.6",
          "form": "레시피 있으세요?",
          "original_form": "레시피 있으세요?",
          "speaker_id": "2",
          "time": "20210829 22:20"
        },
        {
          "id": "MDRW2100035650.1.7",
          "form": "저도 없어요ㅎㄴㅅ",
          "original_form": "저도 없어요ㅎㄴㅅ",
          "speaker_id": "1",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100035650.1.8",
          "form": "만개의 레시피 깔았는데 좋더라구요ㅋㅋㅋ",
          "original_form": "만개의 레시피 깔았는데 좋더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100035650.1.9",
          "form": "레시피도 이것저것 많이 해봐야 생기는 것 같아요 ㅎㅎ",
          "original_form": "레시피도 이것저것 많이 해봐야 생기는 것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100035650.1.10",
          "form": "맞아요! 처음에 했을때보다 맘의 여유도 좀 있더라구요",
          "original_form": "맞아요! 처음에 했을때보다 맘의 여유도 좀 있더라구요",
          "speaker_id": "1",
          "time": "20210829 22:40"
        },
        {
          "id": "MDRW2100035650.1.11",
          "form": "저는 귀찮아서 그냥 대충 때우는 편이에요 ㅎㅎ",
          "original_form": "저는 귀찮아서 그냥 대충 때우는 편이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:44"
        },
        {
          "id": "MDRW2100035650.1.12",
          "form": "부지런하시네요 ㅎㅎ",
          "original_form": "부지런하시네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:44"
        },
        {
          "id": "MDRW2100035650.1.13",
          "form": "다이어트는 부지런해야하더라구요ㅠㅠ",
          "original_form": "다이어트는 부지런해야하더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:51"
        },
        {
          "id": "MDRW2100035650.1.14",
          "form": "게으른 성격이라 좀 힘에 부져욥..",
          "original_form": "게으른 성격이라 좀 힘에 부져욥..",
          "speaker_id": "1",
          "time": "20210829 22:51"
        },
        {
          "id": "MDRW2100035650.1.15",
          "form": "저는 그냥 피티에 맡겼어요 ㅎㅎ",
          "original_form": "저는 그냥 피티에 맡겼어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:13"
        },
        {
          "id": "MDRW2100035650.1.16",
          "form": "식단은 못하겠으니 운동을 더 많이 시켜라.. 마인드에요 지금은",
          "original_form": "식단은 못하겠으니 운동을 더 많이 시켜라.. 마인드에요 지금은",
          "speaker_id": "2",
          "time": "20210830 19:13"
        },
        {
          "id": "MDRW2100035650.1.17",
          "form": "피티라니요 그게 더 대단하신걸요",
          "original_form": "피티라니요 그게 더 대단하신걸요",
          "speaker_id": "1",
          "time": "20210830 19:22"
        },
        {
          "id": "MDRW2100035650.1.18",
          "form": "저 체험했다가 다리 후들거려서 등록 못했어요",
          "original_form": "저 체험했다가 다리 후들거려서 등록 못했어요",
          "speaker_id": "1",
          "time": "20210830 19:22"
        },
        {
          "id": "MDRW2100035650.1.19",
          "form": "저도 첫 주는 집에 오는 내내 갓 태어난 임팔라처럼 후들거리면서 왔어요 ㅋㅋㅋ",
          "original_form": "저도 첫 주는 집에 오는 내내 갓 태어난 임팔라처럼 후들거리면서 왔어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:23"
        },
        {
          "id": "MDRW2100035650.1.20",
          "form": "앗ㅋㅋㅋ 다 그런거군요",
          "original_form": "앗ㅋㅋㅋ 다 그런거군요",
