        {
          "id": "MDRW2100017237.1.1",
          "form": "직장다니세요?",
          "original_form": "직장다니세요?",
          "speaker_id": "1",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100017237.1.2",
          "form": "네 직장다니고있어용 name1님은요?",
          "original_form": "네 직장다니고있어용 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 18:13"
        },
        {
          "id": "MDRW2100017237.1.3",
          "form": "전 학생이요 직장 어때요?",
          "original_form": "전 학생이요 직장 어때요?",
          "speaker_id": "1",
          "time": "20210826 18:14"
        },
        {
          "id": "MDRW2100017237.1.4",
          "form": "그냥.. 돈 주니까 다녀요 ㅠ",
          "original_form": "그냥.. 돈 주니까 다녀요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017237.1.5",
          "form": "에구구 ㅜㅜ 월급은 어때요",
          "original_form": "에구구 ㅜㅜ 월급은 어때요",
          "speaker_id": "1",
          "time": "20210826 18:16"
        },
        {
          "id": "MDRW2100017237.1.6",
          "form": "입에 풀칠할만큼은 줘요 ㅠ 금칠은 못하구요 ㅠ",
          "original_form": "입에 풀칠할만큼은 줘요 ㅠ 금칠은 못하구요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 18:16"
        },
        {
          "id": "MDRW2100017237.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ 금칠 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 금칠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017237.1.8",
          "form": "돈이뭐라고 ㅜ 일하기싫네요",
          "original_form": "돈이뭐라고 ㅜ 일하기싫네요",
          "speaker_id": "1",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100017237.1.9",
          "form": "맞아요 왜 사람은 죽을때까지 일해야할까요..",
          "original_form": "맞아요 왜 사람은 죽을때까지 일해야할까요..",
          "speaker_id": "2",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100017237.1.10",
          "form": "전 노는게 좋은데 ㅠ 흑",
          "original_form": "전 노는게 좋은데 ㅠ 흑",
          "speaker_id": "2",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100017237.1.11",
          "form": "이직 생각은 없으시구요 ?",
          "original_form": "이직 생각은 없으시구요 ?",
          "speaker_id": "1",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100017237.1.12",
          "form": "이직 너무 귀찮아요 퇴근하면 누워있기도 바빠서 ㅋㅋ",
          "original_form": "이직 너무 귀찮아요 퇴근하면 누워있기도 바빠서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017237.1.13",
          "form": "name1님 좋은 회사 아시면 추천부탁해요~",
          "original_form": "&name1&님 좋은 회사 아시면 추천부탁해요~",
          "speaker_id": "2",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017237.1.14",
          "form": "휴 ..삼선전자어때요",
          "original_form": "휴 ..삼선전자어때요",
          "speaker_id": "1",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017237.1.15",
          "form": "그러게요.. 거기좋을까요... 전 돈은 많이주고 퇴근은 빠른곳이 좋은데 ㅋㅋ 가능할까요",
          "original_form": "그러게요.. 거기좋을까요... 전 돈은 많이주고 퇴근은 빠른곳이 좋은데 ㅋㅋ 가능할까요",
          "speaker_id": "2",
          "time": "20210826 18:53"
        },
        {
          "id": "MDRW2100017237.1.16",
          "form": "그런데가 잇나요",
