      "utterance": [
        {
          "id": "MDRW2100020701.1.1",
          "form": "ㅎㅎ도쿄 올림픽 보셨어요?",
          "original_form": "ㅎㅎ도쿄 올림픽 보셨어요?",
          "speaker_id": "2",
          "time": "20210828 11:20"
        },
        {
          "id": "MDRW2100020701.1.2",
          "form": "2주간 열심히 봤어요ㅎㅎ",
          "original_form": "2주간 열심히 봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 11:20"
        },
        {
          "id": "MDRW2100020701.1.3",
          "form": "어떤 종목 인상깊게 보셨어요?",
          "original_form": "어떤 종목 인상깊게 보셨어요?",
          "speaker_id": "2",
          "time": "20210828 11:22"
        },
        {
          "id": "MDRW2100020701.1.4",
          "form": "양궁이나 배구, 도마 재밌었어요!",
          "original_form": "양궁이나 배구, 도마 재밌었어요!",
          "speaker_id": "1",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100020701.1.5",
          "form": "양궁이랑 배구 진짜 대단했죠 ㅎㅎ",
          "original_form": "양궁이랑 배구 진짜 대단했죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 11:25"
        },
        {
          "id": "MDRW2100020701.1.6",
          "form": "셋 중에 어느게 젤 재밌었나요?",
          "original_form": "셋 중에 어느게 젤 재밌었나요?",
          "speaker_id": "2",
          "time": "20210828 11:25"
        },
        {
          "id": "MDRW2100020701.1.7",
          "form": "배구 한일전이 아무래도요ㅋㅋㅋㅋ",
          "original_form": "배구 한일전이 아무래도요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:15"
        },
        {
          "id": "MDRW2100020701.1.8",
          "form": "캬 배구 한일전 너무 통쾌했어요",
          "original_form": "캬 배구 한일전 너무 통쾌했어요",
          "speaker_id": "2",
          "time": "20210828 12:15"
        },
        {
          "id": "MDRW2100020701.1.9",
          "form": "메달을 못딸지언정 한일전은 지지말아야한다.",
          "original_form": "메달을 못딸지언정 한일전은 지지말아야한다.",
          "speaker_id": "2",
          "time": "20210828 12:16"
        },
        {
          "id": "MDRW2100020701.1.10",
          "form": "일본 매치포인트에서의 역전승은 정말ㅠㅜㅜ",
          "original_form": "일본 매치포인트에서의 역전승은 정말ㅠㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 12:17"
        },
        {
          "id": "MDRW2100020701.1.11",
          "form": "근데 일본 진짜 잘하긴 잘하더라구요",
          "original_form": "근데 일본 진짜 잘하긴 잘하더라구요",
          "speaker_id": "2",
          "time": "20210828 12:18"
        },
        {
          "id": "MDRW2100020701.1.12",
          "form": "원래 일본이 배구강국이에요?",
          "original_form": "원래 일본이 배구강국이에요?",
          "speaker_id": "2",
          "time": "20210828 12:18"
        },
        {
          "id": "MDRW2100020701.1.13",
          "form": "아마 우리나라 보다 못 하는 나라는 없었을걸요...ㅎ",
          "original_form": "아마 우리나라 보다 못 하는 나라는 없었을걸요...ㅎ",
          "speaker_id": "1",
          "time": "20210828 12:19"
        },
        {
          "id": "MDRW2100020701.1.14",
          "form": "와 진짜 도미니카도 소름돋게 잘하던데 ㅋㅋㅋ",
          "original_form": "와 진짜 도미니카도 소름돋게 잘하던데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:20"
        },
        {
          "id": "MDRW2100020701.1.15",
          "form": "근데 거기 입장할 때 흥 넘쳐서 너무 웃겼어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 거기 입장할 때 흥 넘쳐서 너무 웃겼어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:20"
        },
        {
          "id": "MDRW2100020701.1.16",
          "form": "맞아요ㅋㅋㅋㅋ 우리나라가 워낙 세계 순위가 낮아서ㅠ 다 강팀이었을거에요ㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ 우리나라가 워낙 세계 순위가 낮아서ㅠ 다 강팀이었을거에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:22"
        },
        {
          "id": "MDRW2100020701.1.17",
          "form": "근데도 강팀 상대로 4강까지 갔으면 진짜 대단한듯",
          "original_form": "근데도 강팀 상대로 4강까지 갔으면 진짜 대단한듯",
          "speaker_id": "2",
          "time": "20210828 12:23"
        }
      ]
