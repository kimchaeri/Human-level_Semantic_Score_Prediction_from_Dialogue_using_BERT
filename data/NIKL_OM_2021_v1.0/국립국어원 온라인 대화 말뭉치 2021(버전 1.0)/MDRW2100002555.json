      "utterance": [
        {
          "id": "MDRW2100002555.1.1",
          "form": "가고 싶은 여행지가 있으신가요",
          "original_form": "가고 싶은 여행지가 있으신가요",
          "speaker_id": "2",
          "time": "20210824 10:43"
        },
        {
          "id": "MDRW2100002555.1.2",
          "form": "저 강릉이요",
          "original_form": "저 강릉이요",
          "speaker_id": "1",
          "time": "20210824 10:57"
        },
        {
          "id": "MDRW2100002555.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210824 10:57"
        },
        {
          "id": "MDRW2100002555.1.4",
          "form": "저는 해외를 가본적이 없어서",
          "original_form": "저는 해외를 가본적이 없어서",
          "speaker_id": "2",
          "time": "20210824 10:58"
        },
        {
          "id": "MDRW2100002555.1.5",
          "form": "동남아 가고싶어요",
          "original_form": "동남아 가고싶어요",
          "speaker_id": "2",
          "time": "20210824 10:58"
        },
        {
          "id": "MDRW2100002555.1.6",
          "form": "강릉 가 보신 적 있으신가요 강릉 강릉 강릉 가면 주로 뭐하시나요",
          "original_form": "강릉 가 보신 적 있으신가요 강릉 강릉 강릉 가면 주로 뭐하시나요",
          "speaker_id": "2",
          "time": "20210824 10:59"
        },
        {
          "id": "MDRW2100002555.1.7",
          "form": "앗.. 핸드폰 오류 때문에 강릉이 여러번 쳐졌네요",
          "original_form": "앗.. 핸드폰 오류 때문에 강릉이 여러번 쳐졌네요",
          "speaker_id": "2",
          "time": "20210824 11:00"
        },
        {
          "id": "MDRW2100002555.1.8",
          "form": "동남아 좋을것같아요ㅋㅋㅋ",
          "original_form": "동남아 좋을것같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:06"
        },
        {
          "id": "MDRW2100002555.1.9",
          "form": "강릉가면 맛집이랑",
          "original_form": "강릉가면 맛집이랑",
          "speaker_id": "1",
          "time": "20210824 11:06"
        },
        {
          "id": "MDRW2100002555.1.10",
          "form": "바다요~~! 안목카페거리도요",
          "original_form": "바다요~~! 안목카페거리도요",
          "speaker_id": "1",
          "time": "20210824 11:06"
        },
        {
          "id": "MDRW2100002555.1.11",
          "form": "강릉 맛집 중에 어디가 제일 괜찮던가요",
          "original_form": "강릉 맛집 중에 어디가 제일 괜찮던가요",
          "speaker_id": "2",
          "time": "20210824 11:06"
        },
        {
          "id": "MDRW2100002555.1.12",
          "form": "전 해산물을 좋아해서요",
          "original_form": "전 해산물을 좋아해서요",
          "speaker_id": "1",
          "time": "20210824 11:12"
        },
        {
          "id": "MDRW2100002555.1.13",
          "form": "대게가 굿입니다",
          "original_form": "대게가 굿입니다",
          "speaker_id": "1",
          "time": "20210824 11:12"
        },
        {
          "id": "MDRW2100002555.1.14",
          "form": "대게 너무 맛있잖아요",
          "original_form": "대게 너무 맛있잖아요",
          "speaker_id": "2",
          "time": "20210824 11:12"
        },
        {
          "id": "MDRW2100002555.1.15",
          "form": "먹는 가면 꼭 먹어봐야 될 것 같아요",
          "original_form": "먹는 가면 꼭 먹어봐야 될 것 같아요",
          "speaker_id": "2",
          "time": "20210824 11:12"
        },
        {
          "id": "MDRW2100002555.1.16",
          "form": "다른 해산물 또 좋아는 거 있으신가요",
          "original_form": "다른 해산물 또 좋아는 거 있으신가요",
          "speaker_id": "2",
          "time": "20210824 11:13"
        },
        {
          "id": "MDRW2100002555.1.17",
          "form": "전 해산물은 다좋아해요ㅋㅋㅋ",
          "original_form": "전 해산물은 다좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:19"
        },
        {
          "id": "MDRW2100002555.1.18",
          "form": "강릉은 순두부도 유명하죠",
          "original_form": "강릉은 순두부도 유명하죠",
          "speaker_id": "1",
          "time": "20210824 11:20"
        },
        {
          "id": "MDRW2100002555.1.19",
          "form": "초당순두부 엄지네 젤라또",
          "original_form": "초당순두부 엄지네 젤라또",
          "speaker_id": "1",
          "time": "20210824 11:20"
        },
        {
          "id": "MDRW2100002555.1.20",
          "form": "혹시 초당 순두부 ?",
          "original_form": "혹시 초당 순두부 ?",
          "speaker_id": "2",
          "time": "20210824 11:20"
        },
        {
          "id": "MDRW2100002555.1.21",
          "form": "초당 순두부는 무슨 맛인가요 먹어본 적이 없어서",
          "original_form": "초당 순두부는 무슨 맛인가요 먹어본 적이 없어서",
          "speaker_id": "2",
          "time": "20210824 11:20"
        },
        {
          "id": "MDRW2100002555.1.22",
          "form": "ㅋㅋㅋ 얼큰한국물베이스인데",
          "original_form": "ㅋㅋㅋ 얼큰한국물베이스인데",
          "speaker_id": "1",
          "time": "20210824 11:23"
        },
        {
          "id": "MDRW2100002555.1.23",
          "form": "줄을 서서먹더라고요 강릉에선",
          "original_form": "줄을 서서먹더라고요 강릉에선",
          "speaker_id": "1",
          "time": "20210824 11:24"
        },
        {
          "id": "MDRW2100002555.1.24",
          "form": "얼큰한 국물이라니 너무 맛있겠어요",
          "original_form": "얼큰한 국물이라니 너무 맛있겠어요",
          "speaker_id": "2",
          "time": "20210824 11:24"
        },
        {
          "id": "MDRW2100002555.1.25",
          "form": "기회가 되면 한번 먹어보고 싶네요",
          "original_form": "기회가 되면 한번 먹어보고 싶네요",
          "speaker_id": "2",
          "time": "20210824 11:24"
        },
        {
          "id": "MDRW2100002555.1.26",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:29"
        },
        {
          "id": "MDRW2100002555.1.27",
          "form": "그앞에 초당순두부맛 젤라또도 같이팔아요",
          "original_form": "그앞에 초당순두부맛 젤라또도 같이팔아요",
          "speaker_id": "1",
          "time": "20210824 11:29"
        }
      ]
    }
