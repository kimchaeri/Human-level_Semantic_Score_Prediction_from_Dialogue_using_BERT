      "utterance": [
        {
          "id": "MDRW2100017398.1.1",
          "form": "역시 우린 게임이지",
          "original_form": "역시 우린 게임이지",
          "speaker_id": "1",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100017398.1.2",
          "form": "그래서 본론",
          "original_form": "그래서 본론",
          "speaker_id": "1",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017398.1.3",
          "form": "원신 찍먹",
          "original_form": "원신 찍먹",
          "speaker_id": "1",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017398.1.4",
          "form": "아 해보려고 하는데 왜 시간 안남 ㅠ?",
          "original_form": "아 해보려고 하는데 왜 시간 안남 ㅠ?",
          "speaker_id": "2",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017398.1.5",
          "form": "아니면 그 그 머냐",
          "original_form": "아니면 그 그 머냐",
          "speaker_id": "1",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017398.1.6",
          "form": "신도 야근을 하나요였나",
          "original_form": "신도 야근을 하나요였나",
          "speaker_id": "1",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017398.1.7",
          "form": "그거는?",
          "original_form": "그거는?",
          "speaker_id": "1",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017398.1.8",
          "form": "거기있는 먹음직스러워보이는 사약",
          "original_form": "거기있는 먹음직스러워보이는 사약",
          "speaker_id": "1",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100017398.1.9",
          "form": "데리취향이여보이던데",
          "original_form": "데리취향이여보이던데",
          "speaker_id": "1",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100017398.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 그래? 그것도 하려고는 하는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 그래? 그것도 하려고는 하는데",
          "speaker_id": "2",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100017398.1.11",
          "form": "ㅋ쿠ㅜ 게임..나도 게임..",
          "original_form": "ㅋ쿠ㅜ 게임..나도 게임..",
          "speaker_id": "2",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100017398.1.12",
          "form": "ㅇㅇ백발에 가면",
          "original_form": "ㅇㅇ백발에 가면",
          "speaker_id": "1",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100017398.1.13",
          "form": "걔",
          "original_form": "걔",
          "speaker_id": "1",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100017398.1.14",
          "form": "좀 데리취향 아닙닉가",
          "original_form": "좀 데리취향 아닙닉가",
          "speaker_id": "1",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100017398.1.15",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100017398.1.16",
          "form": "내가 데리취향이 백발금안 혹은 흑발금안 고양이과 상인것을",
          "original_form": "내가 데리취향이 백발금안 혹은 흑발금안 고양이과 상인것을",
          "speaker_id": "1",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100017398.1.17",
          "form": "알고잇다",
          "original_form": "알고잇다",
          "speaker_id": "1",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100017398.1.18",
          "form": "넘 취향인데",
          "original_form": "넘 취향인데",
          "speaker_id": "2",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100017398.1.19",
          "form": "그ㅡ놈 안나온다며",
          "original_form": "그ㅡ놈 안나온다며",
          "speaker_id": "2",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100017398.1.20",
          "form": "그치만 일단 맛은 있잖아",
          "original_form": "그치만 일단 맛은 있잖아",
          "speaker_id": "1",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100017398.1.21",
          "form": "에피타이져로 한수꾸락 주는거라도",
          "original_form": "에피타이져로 한수꾸락 주는거라도",
          "speaker_id": "1",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100017398.1.22",
          "form": "찍어먹어봐",
          "original_form": "찍어먹어봐",
          "speaker_id": "1",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100017398.1.23",
          "form": "그거는 한 5분씩  짧게 끊어서 해도 돼",
          "original_form": "그거는 한 5분씩  짧게 끊어서 해도 돼",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017398.1.24",
          "form": "게임이 가벼운편이라",
          "original_form": "게임이 가벼운편이라",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017398.1.25",
          "form": "ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017398.1.26",
          "form": "나도 하루에 2-3개씩이나 까고있지",
          "original_form": "나도 하루에 2-3개씩이나 까고있지",
          "speaker_id": "1",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017398.1.27",
          "form": "리얼타임 어쩌구 들은거 같지만..그래...이일단",
          "original_form": "리얼타임 어쩌구 들은거 같지만..그래...이일단",
          "speaker_id": "2",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017398.1.28",
          "form": "딱히 열심히 하진 않아",
          "original_form": "딱히 열심히 하진 않아",
          "speaker_id": "1",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017398.1.29",
          "form": "ㄴㄴ리얼타임 아니야",
          "original_form": "ㄴㄴ리얼타임 아니야",
          "speaker_id": "1",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017398.1.30",
          "form": "근데 앞부분에 나오고 안나오는 놈이 취향이면",
          "original_form": "근데 앞부분에 나오고 안나오는 놈이 취향이면",
          "speaker_id": "2",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017398.1.31",
          "form": "리얼타임이엇던 게임은 수메",
          "original_form": "리얼타임이엇던 게임은 수메",
          "speaker_id": "1",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017398.1.32",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017398.1.33",
          "form": "아냐 그래도 전반적으로",
          "original_form": "아냐 그래도 전반적으로",
          "speaker_id": "1",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017398.1.34",
          "form": "찍어먹어볼만 해",
          "original_form": "찍어먹어볼만 해",
          "speaker_id": "1",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017398.1.35",
          "form": "글쿤.. 저아 함 찍어먹어볼게",
          "original_form": "글쿤.. 저아 함 찍어먹어볼게",
          "speaker_id": "2",
          "time": "20210826 19:17"
        }
      ]
    }
