      "utterance": [
        {
          "id": "MDRW2100017232.1.1",
          "form": "날씨가 너무 습하네요",
          "original_form": "날씨가 너무 습하네요",
          "speaker_id": "1",
          "time": "20210826 18:16"
        },
        {
          "id": "MDRW2100017232.1.2",
          "form": "장마철이니까요",
          "original_form": "장마철이니까요",
          "speaker_id": "2",
          "time": "20210826 18:59"
        },
        {
          "id": "MDRW2100017232.1.3",
          "form": "제습기가 제기능을 하는지 의심이 가요",
          "original_form": "제습기가 제기능을 하는지 의심이 가요",
          "speaker_id": "2",
          "time": "20210826 18:59"
        },
        {
          "id": "MDRW2100017232.1.4",
          "form": "제습기 있으세요?",
          "original_form": "제습기 있으세요?",
          "speaker_id": "1",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017232.1.5",
          "form": "네 있기야하는데 어제 풀로 돌렸는데 65네요",
          "original_form": "네 있기야하는데 어제 풀로 돌렸는데 65네요",
          "speaker_id": "2",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017232.1.6",
          "form": "그래도 첨엔 85였어요",
          "original_form": "그래도 첨엔 85였어요",
          "speaker_id": "2",
          "time": "20210826 19:01"
        },
        {
          "id": "MDRW2100017232.1.7",
          "form": "숫자가 높을수록 습한거에요?",
          "original_form": "숫자가 높을수록 습한거에요?",
          "speaker_id": "1",
          "time": "20210826 19:02"
        },
        {
          "id": "MDRW2100017232.1.8",
          "form": "네 습도 니까요",
          "original_form": "네 습도 니까요",
          "speaker_id": "2",
          "time": "20210826 19:03"
        },
        {
          "id": "MDRW2100017232.1.9",
          "form": "아 저 수치가 습도 군요",
          "original_form": "아 저 수치가 습도 군요",
          "speaker_id": "1",
          "time": "20210826 19:04"
        },
        {
          "id": "MDRW2100017232.1.10",
          "form": "네 그래도 습하니까 피부는 좋아지는 것 같아요",
          "original_form": "네 그래도 습하니까 피부는 좋아지는 것 같아요",
          "speaker_id": "2",
          "time": "20210826 19:06"
        },
        {
          "id": "MDRW2100017232.1.11",
          "form": "ㅋ 그런 장점이 있네요",
          "original_form": "ㅋ 그런 장점이 있네요",
          "speaker_id": "1",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017232.1.12",
          "form": "최대 장점",
          "original_form": "최대 장점",
          "speaker_id": "2",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100017232.1.13",
          "form": "그리고는 없는것같아요ㅡ",
          "original_form": "그리고는 없는것같아요ㅡ",
          "speaker_id": "2",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100017232.1.14",
          "form": "저도 혹시 있나 생각해봤는데 없는것 같아요",
          "original_form": "저도 혹시 있나 생각해봤는데 없는것 같아요",
          "speaker_id": "1",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017232.1.15",
          "form": "비오는 날 좋아하세요?",
          "original_form": "비오는 날 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017232.1.16",
          "form": "아니요 완전 싫어해요 ㅎ",
          "original_form": "아니요 완전 싫어해요 ㅎ",
          "speaker_id": "1",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017232.1.17",
          "form": "저두 별로 안좋아했는데 요즘은 쫌 좋아졌어요",
          "original_form": "저두 별로 안좋아했는데 요즘은 쫌 좋아졌어요",
          "speaker_id": "2",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100017232.1.18",
          "form": "밤에 비오는거 좋아해요",
          "original_form": "밤에 비오는거 좋아해요",
          "speaker_id": "2",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100017232.1.19",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100017232.1.20",
          "form": "비오는 소리랑 운치?",
          "original_form": "비오는 소리랑 운치?",
          "speaker_id": "2",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100017232.1.21",
          "form": "비오는 밤 방에서 향초피우고 노래듣는거 좋아요",
          "original_form": "비오는 밤 방에서 향초피우고 노래듣는거 좋아요",
          "speaker_id": "2",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100017232.1.22",
          "form": "분위기 있겠네요",
          "original_form": "분위기 있겠네요",
          "speaker_id": "1",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100017232.1.23",
          "form": "어릴땐 비오면 학교도 안갔어요",
          "original_form": "어릴땐 비오면 학교도 안갔어요",
          "speaker_id": "2",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100017232.1.24",
          "form": "헉 저보다 더 싫어하셨군요 저는 학교는 갔는데",
          "original_form": "헉 저보다 더 싫어하셨군요 저는 학교는 갔는데",
          "speaker_id": "1",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100017232.1.25",
          "form": "기분쳐지고 버스타면 멀미하고",
          "original_form": "기분쳐지고 버스타면 멀미하고",
          "speaker_id": "2",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017232.1.26",
          "form": "헙 최악이네요",
          "original_form": "헙 최악이네요",
          "speaker_id": "1",
          "time": "20210826 19:16"
        },
        {
          "id": "MDRW2100017232.1.27",
          "form": "이번주에 비소식보다는 흐림이 많네요",
          "original_form": "이번주에 비소식보다는 흐림이 많네요",
          "speaker_id": "2",
          "time": "20210826 19:21"
        }
      ]
    }
  ]
