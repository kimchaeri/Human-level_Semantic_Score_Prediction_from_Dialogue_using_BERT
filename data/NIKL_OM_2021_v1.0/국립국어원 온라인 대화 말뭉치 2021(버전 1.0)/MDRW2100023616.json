      "utterance": [
        {
          "id": "MDRW2100023616.1.1",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 13:43"
        },
        {
          "id": "MDRW2100023616.1.2",
          "form": "저 방탈출 게임 좋아해용!",
          "original_form": "저 방탈출 게임 좋아해용!",
          "speaker_id": "2",
          "time": "20210830 13:43"
        },
        {
          "id": "MDRW2100023616.1.3",
          "form": "name1은 하시는 게임 있으세요?",
          "original_form": "&name1&은 하시는 게임 있으세요?",
          "speaker_id": "2",
          "time": "20210830 13:43"
        },
        {
          "id": "MDRW2100023616.1.4",
          "form": "저도 방탈출 좋아해요.",
          "original_form": "저도 방탈출 좋아해요.",
          "speaker_id": "1",
          "time": "20210830 13:44"
        },
        {
          "id": "MDRW2100023616.1.5",
          "form": "이스케이프룸이라는 어플 게임 자주해용",
          "original_form": "이스케이프룸이라는 어플 게임 자주해용",
          "speaker_id": "2",
          "time": "20210830 13:44"
        },
        {
          "id": "MDRW2100023616.1.6",
          "form": "오, 저도 해봤어요",
          "original_form": "오, 저도 해봤어요",
          "speaker_id": "1",
          "time": "20210830 13:44"
        },
        {
          "id": "MDRW2100023616.1.7",
          "form": "앗 해보셨구낭 !",
          "original_form": "앗 해보셨구낭 !",
          "speaker_id": "2",
          "time": "20210830 13:44"
        },
        {
          "id": "MDRW2100023616.1.8",
          "form": "이거 한판 깨는데 시간 얼마나 걸리세요?",
          "original_form": "이거 한판 깨는데 시간 얼마나 걸리세요?",
          "speaker_id": "2",
          "time": "20210830 13:45"
        },
        {
          "id": "MDRW2100023616.1.9",
          "form": "전 방탈출 게임중엔 러스티레이크를 좋아해요",
          "original_form": "전 방탈출 게임중엔 러스티레이크를 좋아해요",
          "speaker_id": "1",
          "time": "20210830 13:45"
        },
        {
          "id": "MDRW2100023616.1.10",
          "form": "한지 좀 오래되서 긴가민가한데",
          "original_form": "한지 좀 오래되서 긴가민가한데",
          "speaker_id": "1",
          "time": "20210830 13:45"
        },
        {
          "id": "MDRW2100023616.1.11",
          "form": "앗 추천 감사해요!",
          "original_form": "앗 추천 감사해요!",
          "speaker_id": "2",
          "time": "20210830 13:45"
        },
        {
          "id": "MDRW2100023616.1.12",
          "form": "제가 안해본 게임이네요ㅋㅋㅋ",
          "original_form": "제가 안해본 게임이네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:45"
        },
        {
          "id": "MDRW2100023616.1.13",
          "form": "그거 시리즈예요",
          "original_form": "그거 시리즈예요",
          "speaker_id": "1",
          "time": "20210830 13:46"
        },
        {
          "id": "MDRW2100023616.1.14",
          "form": "유료랑 무료가 섞여있으니",
          "original_form": "유료랑 무료가 섞여있으니",
          "speaker_id": "1",
          "time": "20210830 13:46"
        },
        {
          "id": "MDRW2100023616.1.15",
          "form": "오오 적절히 잘 소비해야겠네요ㅋㅋㅋㅋ",
          "original_form": "오오 적절히 잘 소비해야겠네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:47"
        },
        {
          "id": "MDRW2100023616.1.16",
          "form": "무료 해보시고 맘에들면 결제",
          "original_form": "무료 해보시고 맘에들면 결제",
          "speaker_id": "1",
          "time": "20210830 13:47"
        },
        {
          "id": "MDRW2100023616.1.17",
          "form": "게임은 과금이 무서워요",
          "original_form": "게임은 과금이 무서워요",
          "speaker_id": "2",
          "time": "20210830 13:47"
        },
        {
          "id": "MDRW2100023616.1.18",
          "form": "전 유료까지 다했어요",
          "original_form": "전 유료까지 다했어요",
          "speaker_id": "1",
          "time": "20210830 13:47"
        },
        {
          "id": "MDRW2100023616.1.19",
          "form": "아 이건 그런식의 과금은 아니고",
          "original_form": "아 이건 그런식의 과금은 아니고",
          "speaker_id": "1",
          "time": "20210830 13:47"
        },
        {
          "id": "MDRW2100023616.1.20",
          "form": "와 진짜 방탈출 게임좋아하시나보다!",
          "original_form": "와 진짜 방탈출 게임좋아하시나보다!",
          "speaker_id": "2",
          "time": "20210830 13:47"
        },
        {
          "id": "MDRW2100023616.1.21",
          "form": "이 회사가 내놓은 게임이 10개쯤 되면",
          "original_form": "이 회사가 내놓은 게임이 10개쯤 되면",
          "speaker_id": "1",
          "time": "20210830 13:47"
        },
        {
          "id": "MDRW2100023616.1.22",
          "form": "절반은 완전 무료고",
          "original_form": "절반은 완전 무료고",
          "speaker_id": "1",
          "time": "20210830 13:48"
        },
        {
          "id": "MDRW2100023616.1.23",
          "form": "한 절반은 처음부터 돈내고 구매하는 게임인거에요",
          "original_form": "한 절반은 처음부터 돈내고 구매하는 게임인거에요",
          "speaker_id": "1",
          "time": "20210830 13:48"
        },
        {
          "id": "MDRW2100023616.1.24",
          "form": "콘솔게임처럼요",
          "original_form": "콘솔게임처럼요",
          "speaker_id": "1",
          "time": "20210830 13:48"
        },
        {
          "id": "MDRW2100023616.1.25",
          "form": "아아 그래서",
          "original_form": "아아 그래서",
          "speaker_id": "2",
          "time": "20210830 13:48"
        },
        {
          "id": "MDRW2100023616.1.26",
          "form": "몇가지해보고 재밌으면 구매하는 식이구나",
          "original_form": "몇가지해보고 재밌으면 구매하는 식이구나",
          "speaker_id": "2",
          "time": "20210830 13:48"
        }
      ]
    }
  ]
