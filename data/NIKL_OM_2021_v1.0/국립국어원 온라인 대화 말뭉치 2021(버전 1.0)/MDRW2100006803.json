      "utterance": [
        {
          "id": "MDRW2100006803.1.1",
          "form": "아이돌 노래 들어요",
          "original_form": "아이돌 노래 들어요",
          "speaker_id": "2",
          "time": "20210908 09:48"
        },
        {
          "id": "MDRW2100006803.1.2",
          "form": "어디서 들으세요?",
          "original_form": "어디서 들으세요?",
          "speaker_id": "1",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100006803.1.3",
          "form": "그냥 유튜브에서 들어요",
          "original_form": "그냥 유튜브에서 들어요",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100006803.1.4",
          "form": "아 저두요 ㅎㅎ 멜론 순위로...ㅎㅎ",
          "original_form": "아 저두요 ㅎㅎ 멜론 순위로...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100006803.1.5",
          "form": "온앤오프 여름 쏙 들으시기로 한 거 들으셔야해요 ㅋㅋㅋㅋㅋ",
          "original_form": "온앤오프 여름 쏙 들으시기로 한 거 들으셔야해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100006803.1.6",
          "form": "기억력...ㅎㅎ 좋으시네요 ㅎㅎ",
          "original_form": "기억력...ㅎㅎ 좋으시네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100006803.1.7",
          "form": "ㅋㅋㅋㅋㅋ그치만 영업의 기회인걸용",
          "original_form": "ㅋㅋㅋㅋㅋ그치만 영업의 기회인걸용",
          "speaker_id": "2",
          "time": "20210908 10:01"
        },
        {
          "id": "MDRW2100006803.1.8",
          "form": "영업 성공률 좋으신가요?",
          "original_form": "영업 성공률 좋으신가요?",
          "speaker_id": "1",
          "time": "20210908 10:02"
        },
        {
          "id": "MDRW2100006803.1.9",
          "form": "아니오ㅠㅠㅠㅠ",
          "original_form": "아니오ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 10:04"
        },
        {
          "id": "MDRW2100006803.1.10",
          "form": "그러시군요 ㅎㅎ 최근 영화는 뭐 보셨나요?",
          "original_form": "그러시군요 ㅎㅎ 최근 영화는 뭐 보셨나요?",
          "speaker_id": "1",
          "time": "20210908 10:05"
        },
        {
          "id": "MDRW2100006803.1.11",
          "form": "나이브스 아웃이요",
          "original_form": "나이브스 아웃이요",
          "speaker_id": "2",
          "time": "20210908 10:07"
        },
        {
          "id": "MDRW2100006803.1.12",
          "form": "모르는영화네요..",
          "original_form": "모르는영화네요..",
          "speaker_id": "1",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100006803.1.13",
          "form": "추리물이에요 좀 정석적인",
          "original_form": "추리물이에요 좀 정석적인",
          "speaker_id": "2",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100006803.1.14",
          "form": "아 추리물 좋아하시나요?",
          "original_form": "아 추리물 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100006803.1.15",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100006803.1.16",
          "form": "드라마도 유명한게 많죠?",
          "original_form": "드라마도 유명한게 많죠?",
          "speaker_id": "1",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100006803.1.17",
          "form": "셜록 같은 거요?",
