      "utterance": [
        {
          "id": "MDRW2100045589.1.1",
          "form": "저는 치킨과자요!",
          "original_form": "저는 치킨과자요!",
          "speaker_id": "2",
          "time": "20210908 03:55"
        },
        {
          "id": "MDRW2100045589.1.2",
          "form": "뭐였지 이름이..",
          "original_form": "뭐였지 이름이..",
          "speaker_id": "2",
          "time": "20210908 03:55"
        },
        {
          "id": "MDRW2100045589.1.3",
          "form": "닭다리 너겟!",
          "original_form": "닭다리 너겟!",
          "speaker_id": "2",
          "time": "20210908 03:55"
        },
        {
          "id": "MDRW2100045589.1.4",
          "form": "와 닭다리 오랜만에 듣네요ㅋㅋㅋㅋㅋ",
          "original_form": "와 닭다리 오랜만에 듣네요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:57"
        },
        {
          "id": "MDRW2100045589.1.5",
          "form": "그거 어릴 때 왠지 모르지만 안에 봉지까서 꼭 상자에 부어서 먹었어욬ㅋㅋㅋ",
          "original_form": "그거 어릴 때 왠지 모르지만 안에 봉지까서 꼭 상자에 부어서 먹었어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:57"
        },
        {
          "id": "MDRW2100045589.1.6",
          "form": "그쵸! 저 옛날에 먹었던 과자였는데",
          "original_form": "그쵸! 저 옛날에 먹었던 과자였는데",
          "speaker_id": "2",
          "time": "20210908 03:57"
        },
        {
          "id": "MDRW2100045589.1.7",
          "form": "요새는 박스말고 크게 봉지로 나오더라구요!",
          "original_form": "요새는 박스말고 크게 봉지로 나오더라구요!",
          "speaker_id": "2",
          "time": "20210908 03:57"
        },
        {
          "id": "MDRW2100045589.1.8",
          "form": "그거 진짜 맛있어요",
          "original_form": "그거 진짜 맛있어요",
          "speaker_id": "2",
          "time": "20210908 03:57"
        },
        {
          "id": "MDRW2100045589.1.9",
          "form": "봉지과자에는 엄청 크게 많이 들어서",
          "original_form": "봉지과자에는 엄청 크게 많이 들어서",
          "speaker_id": "2",
          "time": "20210908 03:58"
        },
        {
          "id": "MDRW2100045589.1.10",
          "form": "많이먹을수있어 좋아요 ㅎㅎ",
          "original_form": "많이먹을수있어 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 03:58"
        },
        {
          "id": "MDRW2100045589.1.11",
          "form": "와 한입 가득 넣고 와작와작 먹으면 진짜 맛있을 거 같아요",
          "original_form": "와 한입 가득 넣고 와작와작 먹으면 진짜 맛있을 거 같아요",
          "speaker_id": "1",
          "time": "20210908 03:58"
        },
        {
          "id": "MDRW2100045589.1.12",
          "form": "왜 진작에 크게크게 안만든거죠?!ㅋㅋㅋㅋ",
          "original_form": "왜 진작에 크게크게 안만든거죠?!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:58"
        },
        {
          "id": "MDRW2100045589.1.13",
          "form": "그러니까요!",
          "original_form": "그러니까요!",
          "speaker_id": "2",
          "time": "20210908 04:00"
        },
        {
          "id": "MDRW2100045589.1.14",
          "form": "닭다리라는 것에 너무 치중한 나머지",
          "original_form": "닭다리라는 것에 너무 치중한 나머지",
          "speaker_id": "2",
          "time": "20210908 04:00"
        },
        {
          "id": "MDRW2100045589.1.15",
          "form": "크게 만들 생각을 못한거죠",
          "original_form": "크게 만들 생각을 못한거죠",
          "speaker_id": "2",
          "time": "20210908 04:00"
        },
        {
          "id": "MDRW2100045589.1.16",
          "form": "뭔가 작게 만들어서 작은 박스에 넣어주는 초기 패키지가",
          "original_form": "뭔가 작게 만들어서 작은 박스에 넣어주는 초기 패키지가",
          "speaker_id": "2",
          "time": "20210908 04:00"
        },
        {
          "id": "MDRW2100045589.1.17",
          "form": "인상에 남아버리니까 그대로 쭉 간게 아닌가..",
          "original_form": "인상에 남아버리니까 그대로 쭉 간게 아닌가..",
          "speaker_id": "2",
          "time": "20210908 04:00"
        },
        {
          "id": "MDRW2100045589.1.18",
          "form": "그치만 역시 크고 많이든게 짱이다!",
          "original_form": "그치만 역시 크고 많이든게 짱이다!",
          "speaker_id": "2",
          "time": "20210908 04:01"
        },
        {
          "id": "MDRW2100045589.1.19",
          "form": "맞아요! 이제 우린 돈도 버니까 큰 거 사먹을 수도 있고!",
          "original_form": "맞아요! 이제 우린 돈도 버니까 큰 거 사먹을 수도 있고!",
          "speaker_id": "1",
          "time": "20210908 04:02"
        },
        {
          "id": "MDRW2100045589.1.20",
          "form": "크면 왕좋다!",
          "original_form": "크면 왕좋다!",
          "speaker_id": "1",
          "time": "20210908 04:02"
        },
        {
          "id": "MDRW2100045589.1.21",
          "form": "맞아요!!!",
          "original_form": "맞아요!!!",
          "speaker_id": "2",
          "time": "20210908 04:03"
        },
        {
          "id": "MDRW2100045589.1.22",
          "form": "큰게 왕맛있다!!",
          "original_form": "큰게 왕맛있다!!",
          "speaker_id": "2",
          "time": "20210908 04:03"
        },
        {
          "id": "MDRW2100045589.1.23",
          "form": "조만간에 동네 마트 투어를 가야겠어요",
          "original_form": "조만간에 동네 마트 투어를 가야겠어요",
          "speaker_id": "1",
          "time": "20210908 04:04"
        },
        {
          "id": "MDRW2100045589.1.24",
          "form": "왕만한 닭다리 사냥하러...ㅋㅋㅋㅋㅋ",
          "original_form": "왕만한 닭다리 사냥하러...ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:04"
        },
        {
          "id": "MDRW2100045589.1.25",
          "form": "ㅋㅋㅋㅋㅋ 좋아요!",
          "original_form": "ㅋㅋㅋㅋㅋ 좋아요!",
          "speaker_id": "2",
          "time": "20210908 04:05"
        },
        {
          "id": "MDRW2100045589.1.26",
          "form": "저는 저번주에 이마트였나 두개 사면 할인하길래 두개나 사왔어요!",
          "original_form": "저는 저번주에 이마트였나 두개 사면 할인하길래 두개나 사왔어요!",
          "speaker_id": "2",
          "time": "20210908 04:06"
        },
        {
          "id": "MDRW2100045589.1.27",
          "form": "오랜만에 과자를 쌓아두고 먹는..!!",
          "original_form": "오랜만에 과자를 쌓아두고 먹는..!!",
          "speaker_id": "2",
          "time": "20210908 04:06"
        },
        {
          "id": "MDRW2100045589.1.28",
          "form": "헐 아닛 할인까지하면 참을 수가 없는데요 선생님.....!!!",
          "original_form": "헐 아닛 할인까지하면 참을 수가 없는데요 선생님.....!!!",
          "speaker_id": "1",
          "time": "20210908 04:06"
        },
        {
          "id": "MDRW2100045589.1.29",
          "form": "최고죠 !!",
          "original_form": "최고죠 !!",
          "speaker_id": "2",
          "time": "20210908 04:06"
        },
        {
          "id": "MDRW2100045589.1.30",
          "form": "저는 과자는 거의 할인해야 사먹는편이라",
          "original_form": "저는 과자는 거의 할인해야 사먹는편이라",
          "speaker_id": "2",
          "time": "20210908 04:07"
        },
        {
          "id": "MDRW2100045589.1.31",
          "form": "할인했을때 취향을 찾게되는걱 같아요",
          "original_form": "할인했을때 취향을 찾게되는걱 같아요",
          "speaker_id": "2",
          "time": "20210908 04:07"
        },
        {
          "id": "MDRW2100045589.1.32",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:07"
        },
        {
          "id": "MDRW2100045589.1.33",
          "form": "맞아요 편의점 정가는 비싸요....",
          "original_form": "맞아요 편의점 정가는 비싸요....",
          "speaker_id": "1",
          "time": "20210908 04:07"
        },
        {
          "id": "MDRW2100045589.1.34",
          "form": "홈플 이마트가 과자 연쇄 할인마라 좋아요",
          "original_form": "홈플 이마트가 과자 연쇄 할인마라 좋아요",
          "speaker_id": "1",
          "time": "20210908 04:07"
        },
        {
          "id": "MDRW2100045589.1.35",
          "form": "맞아요맞아요 ㅠㅠ",
          "original_form": "맞아요맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 04:09"
        },
        {
          "id": "MDRW2100045589.1.36",
          "form": "편의점에서 과자를 사먹는건...",
          "original_form": "편의점에서 과자를 사먹는건...",
          "speaker_id": "2",
          "time": "20210908 04:09"
        },
        {
          "id": "MDRW2100045589.1.37",
          "form": "사치죠...ㅠㅠㅠ",
          "original_form": "사치죠...ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 04:09"
        },
        {
          "id": "MDRW2100045589.1.38",
          "form": "돈아까워..ㅠㅠ",
          "original_form": "돈아까워..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 04:09"
        },
        {
          "id": "MDRW2100045589.1.39",
          "form": "근데 신상 나오면 편의점부터 풀어서",
          "original_form": "근데 신상 나오면 편의점부터 풀어서",
          "speaker_id": "1",
          "time": "20210908 04:10"
        },
        {
          "id": "MDRW2100045589.1.40",
          "form": "나빠요 과자회사들....",
          "original_form": "나빠요 과자회사들....",
          "speaker_id": "1",
          "time": "20210908 04:10"
        },
        {
          "id": "MDRW2100045589.1.41",
          "form": "아 그니까요",
          "original_form": "아 그니까요",
          "speaker_id": "2",
          "time": "20210908 04:10"
        },
        {
          "id": "MDRW2100045589.1.42",
          "form": "일부러 그런다니까요...ㅡㅡ",
          "original_form": "일부러 그런다니까요...ㅡㅡ",
