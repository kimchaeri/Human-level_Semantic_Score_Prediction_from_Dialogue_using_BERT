        {
          "id": "MDRW2100037666.1.1",
          "form": "저는 오리고기 먹엇아요",
          "original_form": "저는 오리고기 먹엇아요",
          "speaker_id": "2",
          "time": "20210901 14:50"
        },
        {
          "id": "MDRW2100037666.1.2",
          "form": "어쩧게요리햇는데요",
          "original_form": "어쩧게요리햇는데요",
          "speaker_id": "1",
          "time": "20210901 14:50"
        },
        {
          "id": "MDRW2100037666.1.3",
          "form": "국수랑 먹었어요",
          "original_form": "국수랑 먹었어요",
          "speaker_id": "2",
          "time": "20210901 14:51"
        },
        {
          "id": "MDRW2100037666.1.4",
          "form": "양념은요",
          "original_form": "양념은요",
          "speaker_id": "1",
          "time": "20210901 14:51"
        },
        {
          "id": "MDRW2100037666.1.5",
          "form": "피클 피클소스 마요네즈 설탕 소금 넣었어요",
          "original_form": "피클 피클소스 마요네즈 설탕 소금 넣었어요",
          "speaker_id": "2",
          "time": "20210901 14:51"
        },
        {
          "id": "MDRW2100037666.1.6",
          "form": "맛잇나요?",
