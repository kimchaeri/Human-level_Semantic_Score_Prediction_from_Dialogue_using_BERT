      "utterance": [
        {
          "id": "MDRW2100042395.1.1",
          "form": "어디로?",
          "original_form": "어디로?",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042395.1.2",
          "form": "올리브영같은데에서 시향지 쓸수있더라구요 ㅎㅎ 굿",
          "original_form": "올리브영같은데에서 시향지 쓸수있더라구요 ㅎㅎ 굿",
          "speaker_id": "1",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042395.1.3",
          "form": "근데 마스크 끼는데 향을 어떻게 맡아",
          "original_form": "근데 마스크 끼는데 향을 어떻게 맡아",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042395.1.4",
          "form": "엄청 높이 막 흔들면 괜찮다구욧",
          "original_form": "엄청 높이 막 흔들면 괜찮다구욧",
          "speaker_id": "1",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042395.1.5",
          "form": "빨리?",
          "original_form": "빨리?",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042395.1.6",
          "form": "네 빨리.. 글구 종이니까 걍 샥 뿌리고 마스크 안쪽으로 숙 집어넣어도",
          "original_form": "네 빨리.. 글구 종이니까 걍 샥 뿌리고 마스크 안쪽으로 숙 집어넣어도",
          "speaker_id": "1",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042395.1.7",
          "form": "괜찮은거같던데",
          "original_form": "괜찮은거같던데",
          "speaker_id": "1",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042395.1.8",
          "form": "위험하지 않냐 마스크 안에 넣으면",
          "original_form": "위험하지 않냐 마스크 안에 넣으면",
          "speaker_id": "2",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042395.1.9",
          "form": "금방 뿌려서 넣는거니까 괜찮지 않을까 하고",
          "original_form": "금방 뿌려서 넣는거니까 괜찮지 않을까 하고",
          "speaker_id": "1",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042395.1.10",
          "form": "사람들 다닐때 뿌리면 안되니까",
          "original_form": "사람들 다닐때 뿌리면 안되니까",
          "speaker_id": "2",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042395.1.11",
          "form": "당연 사람들 안다니는거 구석에서 뿌리죠 ㅋㅋ",
          "original_form": "당연 사람들 안다니는거 구석에서 뿌리죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042395.1.12",
          "form": "향수코너도 구석입니다",
          "original_form": "향수코너도 구석입니다",
          "speaker_id": "1",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042395.1.13",
          "form": "올리브영 너무 오랜만에 가봐서 모르겠다",
          "original_form": "올리브영 너무 오랜만에 가봐서 모르겠다",
          "speaker_id": "2",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042395.1.14",
          "form": "저랑 같이갈곳이 너무 많으시ㅔㄴ요",
          "original_form": "저랑 같이갈곳이 너무 많으시ㅔㄴ요",
          "speaker_id": "1",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042395.1.15",
          "form": "또 귀찮다고 안나갈거지",
          "original_form": "또 귀찮다고 안나갈거지",
          "speaker_id": "2",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042395.1.16",
          "form": "아니에요 ㅋㅋ 마스크만 안써도 되면 나갈거야",
          "original_form": "아니에요 ㅋㅋ 마스크만 안써도 되면 나갈거야",
          "speaker_id": "1",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100042395.1.17",
          "form": "안나가겠다는 소리 아냐?",
          "original_form": "안나가겠다는 소리 아냐?",
          "speaker_id": "2",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100042395.1.18",
          "form": "아니라구요 백신맞으면 그래두 나중에 괜찬하질테니까",
          "original_form": "아니라구요 백신맞으면 그래두 나중에 괜찬하질테니까",
          "speaker_id": "1",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100042395.1.19",
          "form": "꽤 오래 기다려야할텐데...",
          "original_form": "꽤 오래 기다려야할텐데...",
