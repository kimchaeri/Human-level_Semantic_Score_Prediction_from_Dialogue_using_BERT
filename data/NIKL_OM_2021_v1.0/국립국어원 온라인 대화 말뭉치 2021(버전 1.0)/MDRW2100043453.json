      "utterance": [
        {
          "id": "MDRW2100043453.1.1",
          "form": "집안일은 왜 해도해도 끝이 없을까",
          "original_form": "집안일은 왜 해도해도 끝이 없을까",
          "speaker_id": "2",
          "time": "20210907 19:06"
        },
        {
          "id": "MDRW2100043453.1.2",
          "form": "집안일이 원래 그렇지 뭐ㅋ",
          "original_form": "집안일이 원래 그렇지 뭐ㅋ",
          "speaker_id": "1",
          "time": "20210907 19:06"
        },
        {
          "id": "MDRW2100043453.1.3",
          "form": "해도 티가 잘 안나",
          "original_form": "해도 티가 잘 안나",
          "speaker_id": "2",
          "time": "20210907 19:06"
        },
        {
          "id": "MDRW2100043453.1.4",
          "form": "맞아 그리고 잠깐이면 또 더러워져있고..",
          "original_form": "맞아 그리고 잠깐이면 또 더러워져있고..",
          "speaker_id": "1",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100043453.1.5",
          "form": "그런의미에서 빨래는 좀 낫지",
          "original_form": "그런의미에서 빨래는 좀 낫지",
          "speaker_id": "2",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100043453.1.6",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100043453.1.7",
          "form": "빨래는 옷 입고 더러워질때까지 시간이 있으니까",
          "original_form": "빨래는 옷 입고 더러워질때까지 시간이 있으니까",
          "speaker_id": "2",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100043453.1.8",
          "form": "ㅋㅋ 인정",
          "original_form": "ㅋㅋ 인정",
          "speaker_id": "1",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100043453.1.9",
          "form": "설거지가 제일 싫어",
          "original_form": "설거지가 제일 싫어",
          "speaker_id": "2",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100043453.1.10",
          "form": "맞아 밥은 하루에 세번이나 먹으니까",
          "original_form": "맞아 밥은 하루에 세번이나 먹으니까",
          "speaker_id": "1",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100043453.1.11",
          "form": "간식도 먹고..",
          "original_form": "간식도 먹고..",
          "speaker_id": "2",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100043453.1.12",
          "form": "설거지가 하루에 한 일톤씩은 나오는듯ㅋㅋ",
          "original_form": "설거지가 하루에 한 일톤씩은 나오는듯ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100043453.1.13",
          "form": "청소기는 돌리니?",
          "original_form": "청소기는 돌리니?",
          "speaker_id": "2",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043453.1.14",
          "form": "응 한달에 한번",
          "original_form": "응 한달에 한번",
          "speaker_id": "1",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043453.1.15",
          "form": "너무한거 아니얔ㅋㅋ",
          "original_form": "너무한거 아니얔ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:09"
        }
