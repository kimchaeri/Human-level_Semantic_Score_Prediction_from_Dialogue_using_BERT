      "utterance": [
        {
          "id": "MDRW2100030689.1.1",
          "form": "김셩이 좋아하는 타로카드 구매",
          "original_form": "김셩이 좋아하는 타로카드 구매",
          "speaker_id": "1",
          "time": "20210904 11:28"
        },
        {
          "id": "MDRW2100030689.1.2",
          "form": "타로카드 사고싶어!!!!",
          "original_form": "타로카드 사고싶어!!!!",
          "speaker_id": "2",
          "time": "20210904 11:30"
        },
        {
          "id": "MDRW2100030689.1.3",
          "form": "헉헉 타로카드 사고싶어",
          "original_form": "헉헉 타로카드 사고싶어",
          "speaker_id": "2",
          "time": "20210904 11:30"
        },
        {
          "id": "MDRW2100030689.1.4",
          "form": "예쁜타로카드",
          "original_form": "예쁜타로카드",
          "speaker_id": "2",
          "time": "20210904 11:30"
        },
        {
          "id": "MDRW2100030689.1.5",
          "form": "앞으로 사고싶은 카드 종류",
          "original_form": "앞으로 사고싶은 카드 종류",
          "speaker_id": "1",
          "time": "20210904 11:30"
        },
        {
          "id": "MDRW2100030689.1.6",
          "form": "말해봐",
          "original_form": "말해봐",
          "speaker_id": "1",
          "time": "20210904 11:30"
        },
        {
          "id": "MDRW2100030689.1.7",
          "form": "절대 사버려",
          "original_form": "절대 사버려",
          "speaker_id": "2",
          "time": "20210904 11:30"
        },
        {
          "id": "MDRW2100030689.1.8",
          "form": "이제 뭐뭐 남았어?",
          "original_form": "이제 뭐뭐 남았어?",
          "speaker_id": "1",
          "time": "20210904 11:30"
        },
        {
          "id": "MDRW2100030689.1.9",
          "form": "음 로맨틱타로랑 성인덱이랑 앤젤앤써랑 오라클카드 유명한거 1개!",
          "original_form": "음 로맨틱타로랑 성인덱이랑 앤젤앤써랑 오라클카드 유명한거 1개!",
          "speaker_id": "2",
          "time": "20210904 11:31"
        },
        {
          "id": "MDRW2100030689.1.10",
          "form": "약 4개정도?",
          "original_form": "약 4개정도?",
          "speaker_id": "2",
          "time": "20210904 11:31"
        },
        {
          "id": "MDRW2100030689.1.11",
          "form": "엄청 많네 진짜루",
          "original_form": "엄청 많네 진짜루",
          "speaker_id": "1",
          "time": "20210904 11:32"
        },
        {
          "id": "MDRW2100030689.1.12",
          "form": "그중 뭐 먼저",
          "original_form": "그중 뭐 먼저",
          "speaker_id": "1",
          "time": "20210904 11:32"
        },
        {
          "id": "MDRW2100030689.1.13",
          "form": "장만할겨",
          "original_form": "장만할겨",
          "speaker_id": "1",
          "time": "20210904 11:32"
        },
        {
          "id": "MDRW2100030689.1.14",
          "form": "아무래도 오라클 아니면 앤젤앤써(이것도 오라클이긴함) 살것같아",
          "original_form": "아무래도 오라클 아니면 앤젤앤써(이것도 오라클이긴함) 살것같아",
          "speaker_id": "2",
          "time": "20210904 11:35"
        },
        {
          "id": "MDRW2100030689.1.15",
          "form": "오라클이 보조카드인데",
          "original_form": "오라클이 보조카드인데",
          "speaker_id": "2",
          "time": "20210904 11:35"
        },
        {
          "id": "MDRW2100030689.1.16",
          "form": "오라클 유명한건 좀...비싸구...",
          "original_form": "오라클 유명한건 좀...비싸구...",
          "speaker_id": "2",
          "time": "20210904 11:35"
        },
        {
          "id": "MDRW2100030689.1.17",
          "form": "앤젤앤써는 그것보다 싼데",
          "original_form": "앤젤앤써는 그것보다 싼데",
          "speaker_id": "2",
          "time": "20210904 11:37"
        },
        {
          "id": "MDRW2100030689.1.18",
          "form": "약간 그게 있어",
          "original_form": "약간 그게 있어",
          "speaker_id": "2",
          "time": "20210904 11:37"
        },
        {
          "id": "MDRW2100030689.1.19",
          "form": "결국 오라클이잖아",
          "original_form": "결국 오라클이잖아",
          "speaker_id": "1",
          "time": "20210904 11:37"
        },
        {
          "id": "MDRW2100030689.1.20",
          "form": "단답형으로 대답 내주는?",
          "original_form": "단답형으로 대답 내주는?",
          "speaker_id": "2",
          "time": "20210904 11:37"
        },
        {
          "id": "MDRW2100030689.1.21",
          "form": "예/아니오 로 대답 내주는편이라",
          "original_form": "예/아니오 로 대답 내주는편이라",
          "speaker_id": "2",
          "time": "20210904 11:37"
        },
        {
          "id": "MDRW2100030689.1.22",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210904 11:37"
        },
        {
          "id": "MDRW2100030689.1.23",
          "form": "실제로 카드에 yes/no 라고 써있어",
          "original_form": "실제로 카드에 yes/no 라고 써있어",
          "speaker_id": "2",
          "time": "20210904 11:38"
        },
        {
          "id": "MDRW2100030689.1.24",
          "form": "아 진쩌 신기하다",
          "original_form": "아 진쩌 신기하다",
          "speaker_id": "1",
          "time": "20210904 11:40"
        },
        {
          "id": "MDRW2100030689.1.25",
          "form": "빨리 사봐 궁금하니껜",
          "original_form": "빨리 사봐 궁금하니껜",
          "speaker_id": "1",
          "time": "20210904 11:40"
        }
      ]
    }
