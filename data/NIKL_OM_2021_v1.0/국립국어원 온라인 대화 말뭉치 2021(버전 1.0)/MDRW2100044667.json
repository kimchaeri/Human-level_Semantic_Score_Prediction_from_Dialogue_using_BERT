      "utterance": [
        {
          "id": "MDRW2100044667.1.1",
          "form": "왓츠업이 2011년도",
          "original_form": "왓츠업이 2011년도",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044667.1.2",
          "form": "본 기억이 없어요ㅋ",
          "original_form": "본 기억이 없어요ㅋ",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044667.1.3",
          "form": "건축학개론 조연이 2012년도 개봉햇으니 2011년도에 드라마도 영화도 시작했네",
          "original_form": "건축학개론 조연이 2012년도 개봉햇으니 2011년도에 드라마도 영화도 시작했네",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044667.1.4",
          "form": "영화와 드라마를 같이 시작했네요",
          "original_form": "영화와 드라마를 같이 시작했네요",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044667.1.5",
          "form": "아 대학교 뮤지컬학과 배경이래 이래서 주연을 했군",
          "original_form": "아 대학교 뮤지컬학과 배경이래 이래서 주연을 했군",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044667.1.6",
          "form": "청춘드라마 느낌인데요",
          "original_form": "청춘드라마 느낌인데요",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044667.1.7",
          "form": "학생역이었나요?",
          "original_form": "학생역이었나요?",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044667.1.8",
          "form": "mbn 0.7%  본사람이 거의 없겠다",
          "original_form": "mbn 0.7%  본사람이 거의 없겠다",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044667.1.9",
          "form": "안습이네ㅠ 그래도 영화가 흥행했으니 다행",
          "original_form": "안습이네ㅠ 그래도 영화가 흥행했으니 다행",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044667.1.10",
          "form": "그렇지 대학생역",
          "original_form": "그렇지 대학생역",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044667.1.11",
          "form": "여주는 누군가요?",
          "original_form": "여주는 누군가요?",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044667.1.12",
          "form": "임주환 대성 오주은 오만석 장희진 많이 나오네",
          "original_form": "임주환 대성 오주은 오만석 장희진 많이 나오네",
          "speaker_id": "2",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044667.1.13",
          "form": "대성도 나왔어요? ㅋㅋ",
          "original_form": "대성도 나왔어요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044667.1.14",
          "form": "그다음에 더킹 투하츠에 출연하고",
          "original_form": "그다음에 더킹 투하츠에 출연하고",
