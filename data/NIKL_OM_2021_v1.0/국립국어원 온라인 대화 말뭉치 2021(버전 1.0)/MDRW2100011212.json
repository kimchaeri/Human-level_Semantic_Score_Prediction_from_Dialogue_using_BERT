      "utterance": [
        {
          "id": "MDRW2100011212.1.1",
          "form": "애 안가질거면 결혼 굳이 할 필요가 있냐고 ㄷㄷ",
          "original_form": "애 안가질거면 결혼 굳이 할 필요가 있냐고 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210824 07:56"
        },
        {
          "id": "MDRW2100011212.1.2",
          "form": "아 그 생각은 저도 했어요",
          "original_form": "아 그 생각은 저도 했어요",
          "speaker_id": "1",
          "time": "20210824 07:56"
        },
        {
          "id": "MDRW2100011212.1.3",
          "form": "아이가 아니면 결혼이 필수가 아닌것 같아서",
          "original_form": "아이가 아니면 결혼이 필수가 아닌것 같아서",
          "speaker_id": "1",
          "time": "20210824 07:56"
        },
        {
          "id": "MDRW2100011212.1.4",
          "form": "음 근데 아플때 보호자는 가족만 해줄수 있잖아요 동거관계가 아니라 ㅎㅎ",
          "original_form": "음 근데 아플때 보호자는 가족만 해줄수 있잖아요 동거관계가 아니라 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 07:57"
        },
        {
          "id": "MDRW2100011212.1.5",
          "form": "그런가요?",
          "original_form": "그런가요?",
          "speaker_id": "1",
          "time": "20210824 07:58"
        },
        {
          "id": "MDRW2100011212.1.6",
          "form": "동거인은 그런권한이 없나요?",
          "original_form": "동거인은 그런권한이 없나요?",
          "speaker_id": "1",
          "time": "20210824 07:58"
        },
        {
          "id": "MDRW2100011212.1.7",
          "form": "법적책임때문에 그런가?",
          "original_form": "법적책임때문에 그런가?",
          "speaker_id": "1",
          "time": "20210824 07:58"
        },
        {
          "id": "MDRW2100011212.1.8",
          "form": "네ㅎ 흔히말하는 수술동의서는 사인못하죠",
          "original_form": "네ㅎ 흔히말하는 수술동의서는 사인못하죠",
          "speaker_id": "2",
          "time": "20210824 07:59"
        },
        {
          "id": "MDRW2100011212.1.9",
          "form": "또 상속문제도 있고",
          "original_form": "또 상속문제도 있고",
          "speaker_id": "2",
          "time": "20210824 07:59"
        },
        {
          "id": "MDRW2100011212.1.10",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210824 07:59"
        },
        {
          "id": "MDRW2100011212.1.11",
          "form": "아 그러고보니 저는 아버지께서 베트남에 계세요ㅜ",
          "original_form": "아 그러고보니 저는 아버지께서 베트남에 계세요ㅜ",
          "speaker_id": "2",
          "time": "20210824 08:00"
        },
        {
          "id": "MDRW2100011212.1.12",
          "form": "말했던가..",
          "original_form": "말했던가..",
          "speaker_id": "2",
          "time": "20210824 08:00"
        },
        {
          "id": "MDRW2100011212.1.13",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210824 08:00"
        },
        {
          "id": "MDRW2100011212.1.14",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210824 08:00"
        },
        {
          "id": "MDRW2100011212.1.15",
          "form": "그곳에서 일하시나요?",
          "original_form": "그곳에서 일하시나요?",
          "speaker_id": "1",
          "time": "20210824 08:00"
        },
        {
          "id": "MDRW2100011212.1.16",
          "form": "베트남에서 일하고계세요",
          "original_form": "베트남에서 일하고계세요",
          "speaker_id": "2",
          "time": "20210824 08:00"
        },
        {
          "id": "MDRW2100011212.1.17",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210824 08:00"
        },
        {
          "id": "MDRW2100011212.1.18",
          "form": "작은 한국 회사인데",
          "original_form": "작은 한국 회사인데",
          "speaker_id": "2",
          "time": "20210824 08:00"
        },
        {
          "id": "MDRW2100011212.1.19",
          "form": "네",
          "original_form": "네",
