          "speaker_id": "1",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007384.1.6",
          "form": "아 이번에",
          "original_form": "아 이번에",
          "speaker_id": "1",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007384.1.7",
          "form": "달러구트 꿈백화점 2권 나왔더라",
