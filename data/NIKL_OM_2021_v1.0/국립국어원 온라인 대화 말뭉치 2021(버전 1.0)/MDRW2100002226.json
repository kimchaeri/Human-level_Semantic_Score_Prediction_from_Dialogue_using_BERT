      "utterance": [
        {
          "id": "MDRW2100002226.1.1",
          "form": "요즘 민트초코 소주 나왓던데",
          "original_form": "요즘 민트초코 소주 나왓던데",
          "speaker_id": "1",
          "time": "20210820 00:53"
        },
        {
          "id": "MDRW2100002226.1.2",
          "form": "근데 빽다방에서 파는 민초는 너무 달어",
          "original_form": "근데 빽다방에서 파는 민초는 너무 달어",
          "speaker_id": "2",
          "time": "20210820 00:53"
        },
        {
          "id": "MDRW2100002226.1.3",
          "form": "먹어보고싶어?",
          "original_form": "먹어보고싶어?",
          "speaker_id": "1",
          "time": "20210820 00:53"
        },
        {
          "id": "MDRW2100002226.1.4",
          "form": "아 그거 궁금하긴 해",
          "original_form": "아 그거 궁금하긴 해",
          "speaker_id": "2",
          "time": "20210820 00:53"
        },
        {
          "id": "MDRW2100002226.1.5",
          "form": "근데 숙취가 심할 거 같아",
          "original_form": "근데 숙취가 심할 거 같아",
          "speaker_id": "2",
          "time": "20210820 00:53"
        },
        {
          "id": "MDRW2100002226.1.6",
          "form": "나는 약간 궁금하니까",
          "original_form": "나는 약간 궁금하니까",
          "speaker_id": "1",
          "time": "20210820 00:53"
        },
        {
          "id": "MDRW2100002226.1.7",
          "form": "그리고 그거 초코향이 많이 난대",
          "original_form": "그리고 그거 초코향이 많이 난대",
          "speaker_id": "2",
          "time": "20210820 00:53"
        },
        {
          "id": "MDRW2100002226.1.8",
          "form": "다음에 여러명 모이면 한번 먹어보자",
          "original_form": "다음에 여러명 모이면 한번 먹어보자",
          "speaker_id": "1",
          "time": "20210820 00:53"
        },
        {
          "id": "MDRW2100002226.1.9",
          "form": "나는 술에서 초코향이 나는건 머드쉐이크 ㅃ고는 좋아한지 않아.",
          "original_form": "나는 술에서 초코향이 나는건 머드쉐이크 ㅃ고는 좋아한지 않아.",
          "speaker_id": "2",
          "time": "20210820 00:53"
        },
        {
          "id": "MDRW2100002226.1.10",
          "form": "그니까 민트가 아주 조금이면 견딜 수 있어",
          "original_form": "그니까 민트가 아주 조금이면 견딜 수 있어",
          "speaker_id": "1",
          "time": "20210820 00:53"
        },
        {
          "id": "MDRW2100002226.1.11",
          "form": "좋아",
          "original_form": "좋아",
          "speaker_id": "2",
          "time": "20210820 00:53"
        },
        {
          "id": "MDRW2100002226.1.12",
          "form": "민트가 너 때린 적 있어?",
          "original_form": "민트가 너 때린 적 있어?",
          "speaker_id": "2",
          "time": "20210820 00:53"
        },
        {
          "id": "MDRW2100002226.1.13",
          "form": "한병을 네명이서 나눠먹으면",
          "original_form": "한병을 네명이서 나눠먹으면",
          "speaker_id": "1",
          "time": "20210820 00:53"
        },
        {
          "id": "MDRW2100002226.1.14",
          "form": "호기심 충족가능",
          "original_form": "호기심 충족가능",
          "speaker_id": "1",
          "time": "20210820 00:53"
        },
        {
          "id": "MDRW2100002226.1.15",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210820 00:53"
        },
        {
          "id": "MDRW2100002226.1.16",
          "form": "당연하지. 나도 호기심만 충족시키면 돼.",
          "original_form": "당연하지. 나도 호기심만 충족시키면 돼.",
          "speaker_id": "2",
          "time": "20210820 00:54"
        },
        {
          "id": "MDRW2100002226.1.17",
          "form": "예전에 뒤통수 맞았어",
          "original_form": "예전에 뒤통수 맞았어",
          "speaker_id": "1",
          "time": "20210820 00:54"
        },
        {
          "id": "MDRW2100002226.1.18",
          "form": "맛있을거라는 기대는 애초에 하지않아.",
          "original_form": "맛있을거라는 기대는 애초에 하지않아.",
          "speaker_id": "2",
          "time": "20210820 00:54"
        },
        {
          "id": "MDRW2100002226.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ예전에 서브웨이에소",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ예전에 서브웨이에소",
          "speaker_id": "1",
          "time": "20210820 00:54"
        },
        {
          "id": "MDRW2100002226.1.20",
          "form": "그냥 쿠키 시켰는데",
          "original_form": "그냥 쿠키 시켰는데",
          "speaker_id": "1",
          "time": "20210820 00:54"
        },
        {
          "id": "MDRW2100002226.1.21",
          "form": "최근에 민초 관련 상품들이 많이 나오더라",
          "original_form": "최근에 민초 관련 상품들이 많이 나오더라",
          "speaker_id": "2",
          "time": "20210820 00:54"
        }
      ]
