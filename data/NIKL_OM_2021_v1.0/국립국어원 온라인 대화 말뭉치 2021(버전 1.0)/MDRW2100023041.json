      "utterance": [
        {
          "id": "MDRW2100023041.1.1",
          "form": "김치 사먹다 보니 가격이 좀 비싸더라",
          "original_form": "김치 사먹다 보니 가격이 좀 비싸더라",
          "speaker_id": "2",
          "time": "20210829 21:03"
        },
        {
          "id": "MDRW2100023041.1.2",
          "form": "아무래도 사먹으니 그렇겠지",
          "original_form": "아무래도 사먹으니 그렇겠지",
          "speaker_id": "1",
          "time": "20210829 21:03"
        },
        {
          "id": "MDRW2100023041.1.3",
          "form": "우리 우유 먹듯이 노브랜드 꺼 한 번 사볼까?",
          "original_form": "우리 우유 먹듯이 노브랜드 꺼 한 번 사볼까?",
          "speaker_id": "2",
          "time": "20210829 21:04"
        },
        {
          "id": "MDRW2100023041.1.4",
          "form": "김치도 노브랜드가 있어?",
          "original_form": "김치도 노브랜드가 있어?",
          "speaker_id": "1",
          "time": "20210829 21:04"
        },
        {
          "id": "MDRW2100023041.1.5",
          "form": "ㅇㅇ 있더라고",
          "original_form": "ㅇㅇ 있더라고",
          "speaker_id": "2",
          "time": "20210829 21:04"
        },
        {
          "id": "MDRW2100023041.1.6",
          "form": "노브랜드 포기김치",
          "original_form": "노브랜드 포기김치",
          "speaker_id": "2",
          "time": "20210829 21:04"
        },
        {
          "id": "MDRW2100023041.1.7",
          "form": "흠.. 국산 아니지?",
          "original_form": "흠.. 국산 아니지?",
          "speaker_id": "1",
          "time": "20210829 21:04"
        },
        {
          "id": "MDRW2100023041.1.8",
          "form": "아냐 국산이라고 쓰여있던데",
          "original_form": "아냐 국산이라고 쓰여있던데",
          "speaker_id": "2",
          "time": "20210829 21:04"
        },
        {
          "id": "MDRW2100023041.1.9",
          "form": "오 그래?",
          "original_form": "오 그래?",
          "speaker_id": "1",
          "time": "20210829 21:05"
        },
        {
          "id": "MDRW2100023041.1.10",
          "form": "종가집 보다 낫다는 사람들도 있더라고",
          "original_form": "종가집 보다 낫다는 사람들도 있더라고",
          "speaker_id": "2",
          "time": "20210829 21:05"
        },
        {
          "id": "MDRW2100023041.1.11",
          "form": "우린 아직 종가집하고 조선 호텔만 먹어봐서 ㅎㅎ",
          "original_form": "우린 아직 종가집하고 조선 호텔만 먹어봐서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:05"
        },
        {
          "id": "MDRW2100023041.1.12",
          "form": "ㅇㅇ 비비고나 노브랜드도 한 번 먹어볼까봐",
          "original_form": "ㅇㅇ 비비고나 노브랜드도 한 번 먹어볼까봐",
          "speaker_id": "2",
          "time": "20210829 21:05"
        },
        {
          "id": "MDRW2100023041.1.13",
          "form": "그랴요~ 이것저것 먹어봐야 어떤 게 맛있는지 알지",
          "original_form": "그랴요~ 이것저것 먹어봐야 어떤 게 맛있는지 알지",
          "speaker_id": "1",
          "time": "20210829 21:06"
        },
        {
          "id": "MDRW2100023041.1.14",
          "form": "가격도 착하고 맛만 괜찮으면 계속 사먹어도 될 것 같아",
          "original_form": "가격도 착하고 맛만 괜찮으면 계속 사먹어도 될 것 같아",
          "speaker_id": "2",
          "time": "20210829 21:06"
        },
        {
          "id": "MDRW2100023041.1.15",
          "form": "ㅇㅇ 한 번 먹어보자",
          "original_form": "ㅇㅇ 한 번 먹어보자",
          "speaker_id": "1",
          "time": "20210829 21:06"
        }
      ]
