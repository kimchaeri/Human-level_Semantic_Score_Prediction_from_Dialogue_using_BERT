      "utterance": [
        {
          "id": "MDRW2100021292.1.1",
          "form": "저녁메뉴 힌트좀 주세요",
          "original_form": "저녁메뉴 힌트좀 주세요",
          "speaker_id": "2",
          "time": "20210828 17:21"
        },
        {
          "id": "MDRW2100021292.1.2",
          "form": "글쎄요?  카레? ㅋ",
          "original_form": "글쎄요?  카레? ㅋ",
          "speaker_id": "1",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100021292.1.3",
          "form": "카레 좋네요.",
          "original_form": "카레 좋네요.",
          "speaker_id": "2",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100021292.1.4",
          "form": "전 요리를 잘 못해요. 잘 하세요?",
          "original_form": "전 요리를 잘 못해요. 잘 하세요?",
          "speaker_id": "1",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100021292.1.5",
          "form": "양파 많이 들어간 백종원 스퇄 카레 맛있더라구요.",
          "original_form": "양파 많이 들어간 백종원 스퇄 카레 맛있더라구요.",
          "speaker_id": "2",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100021292.1.6",
          "form": "저도 먹어보던 음식만 하는편이예요.",
          "original_form": "저도 먹어보던 음식만 하는편이예요.",
          "speaker_id": "2",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100021292.1.7",
          "form": "물 대신 양파를 많이 넣는다고 하대요",
          "original_form": "물 대신 양파를 많이 넣는다고 하대요",
          "speaker_id": "1",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100021292.1.8",
          "form": "네.\r\n어제는 유튜브 보고 팽이버섯 볶음 했는데 맛있더라구요.",
          "original_form": "네.\r\n어제는 유튜브 보고 팽이버섯 볶음 했는데 맛있더라구요.",
          "speaker_id": "2",
          "time": "20210828 17:24"
        },
        {
          "id": "MDRW2100021292.1.9",
          "form": "저도 유튜브 보고 따라해요ㅋ",
          "original_form": "저도 유튜브 보고 따라해요ㅋ",
          "speaker_id": "1",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100021292.1.10",
          "form": "ㅎㅎ 저 결혼때 친구들이 요리책 선물로 줬었는데 요즘은 유튜브가 요리 선생님이예요.",
          "original_form": "ㅎㅎ 저 결혼때 친구들이 요리책 선물로 줬었는데 요즘은 유튜브가 요리 선생님이예요.",
          "speaker_id": "2",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100021292.1.11",
          "form": "채소값이 비싸서 장보기가 겁나요.",
          "original_form": "채소값이 비싸서 장보기가 겁나요.",
          "speaker_id": "2",
          "time": "20210828 17:27"
        },
        {
          "id": "MDRW2100021292.1.12",
          "form": "그나마 버섯류는 덜 올랐더라구요.",
          "original_form": "그나마 버섯류는 덜 올랐더라구요.",
          "speaker_id": "2",
          "time": "20210828 17:27"
        },
        {
          "id": "MDRW2100021292.1.13",
          "form": "그쵸. 채소는 시장이 싸요",
          "original_form": "그쵸. 채소는 시장이 싸요",
          "speaker_id": "1",
          "time": "20210828 17:27"
        },
        {
          "id": "MDRW2100021292.1.14",
          "form": "맞아요.",
          "original_form": "맞아요.",
          "speaker_id": "2",
          "time": "20210828 17:28"
        },
        {
          "id": "MDRW2100021292.1.15",
          "form": "대형마트 채소 너무 비싸요ㅜㅜ",
          "original_form": "대형마트 채소 너무 비싸요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 17:28"
        },
        {
          "id": "MDRW2100021292.1.16",
          "form": "몇개 사면 몇만원ㅠ",
          "original_form": "몇개 사면 몇만원ㅠ",
          "speaker_id": "1",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100021292.1.17",
          "form": "맞아요ㅜㅜ\r\n지난번 애호박 싸다고 우체국택배로 받아서 냉동했던거 요긴하게 먹고있어요.",
          "original_form": "맞아요ㅜㅜ\r\n지난번 애호박 싸다고 우체국택배로 받아서 냉동했던거 요긴하게 먹고있어요.",
          "speaker_id": "2",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100021292.1.18",
          "form": "알뜰하시네요",
          "original_form": "알뜰하시네요",
          "speaker_id": "1",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100021292.1.19",
          "form": "식재료 쌀때 무조건 냉동시켜요",
          "original_form": "식재료 쌀때 무조건 냉동시켜요",
          "speaker_id": "2",
          "time": "20210828 17:29"
        }
