      "utterance": [
        {
          "id": "MDRW2100018365.1.1",
          "form": "독서 자주 하시나요?",
          "original_form": "독서 자주 하시나요?",
          "speaker_id": "1",
          "time": "20210827 16:01"
        },
        {
          "id": "MDRW2100018365.1.2",
          "form": "요즘은 안하고 시간적으로 여유있을때 읽어요",
          "original_form": "요즘은 안하고 시간적으로 여유있을때 읽어요",
          "speaker_id": "2",
          "time": "20210827 16:03"
        },
        {
          "id": "MDRW2100018365.1.3",
          "form": "어떤 장르 주로 읽으세요?",
          "original_form": "어떤 장르 주로 읽으세요?",
          "speaker_id": "1",
          "time": "20210827 16:06"
        },
        {
          "id": "MDRW2100018365.1.4",
          "form": "음 인문학이나 심리관련 책들 읽었어요",
          "original_form": "음 인문학이나 심리관련 책들 읽었어요",
          "speaker_id": "2",
          "time": "20210827 16:07"
        },
        {
          "id": "MDRW2100018365.1.5",
          "form": "소설도 자주보고",
          "original_form": "소설도 자주보고",
          "speaker_id": "2",
          "time": "20210827 16:07"
        },
        {
          "id": "MDRW2100018365.1.6",
          "form": "저도 심리관련책 좋아해요!",
          "original_form": "저도 심리관련책 좋아해요!",
          "speaker_id": "1",
          "time": "20210827 16:08"
        },
        {
          "id": "MDRW2100018365.1.7",
          "form": "주로 읽는건 추리소설이랑 sf소설 자주보구요",
          "original_form": "주로 읽는건 추리소설이랑 sf소설 자주보구요",
          "speaker_id": "1",
          "time": "20210827 16:08"
        },
        {
          "id": "MDRW2100018365.1.8",
          "form": "소설책같은건 직접 구입하셔서 보시나요",
          "original_form": "소설책같은건 직접 구입하셔서 보시나요",
          "speaker_id": "2",
          "time": "20210827 16:09"
        },
        {
          "id": "MDRW2100018365.1.9",
          "form": "아니면 도서관을 이용하시나요",
          "original_form": "아니면 도서관을 이용하시나요",
          "speaker_id": "2",
          "time": "20210827 16:09"
        },
        {
          "id": "MDRW2100018365.1.10",
          "form": "e북으로 읽어요 가끔 종이책 구입하구요!",
          "original_form": "e북으로 읽어요 가끔 종이책 구입하구요!",
          "speaker_id": "1",
          "time": "20210827 16:11"
        },
        {
          "id": "MDRW2100018365.1.11",
          "form": "어떻게 보시나요?",
          "original_form": "어떻게 보시나요?",
          "speaker_id": "1",
          "time": "20210827 16:11"
        },
        {
          "id": "MDRW2100018365.1.12",
          "form": "저는 집근처에 도서관이 있어서",
          "original_form": "저는 집근처에 도서관이 있어서",
          "speaker_id": "2",
          "time": "20210827 16:11"
        },
        {
          "id": "MDRW2100018365.1.13",
          "form": "도서대출해서 보거나",
          "original_form": "도서대출해서 보거나",
          "speaker_id": "2",
          "time": "20210827 16:11"
        },
        {
          "id": "MDRW2100018365.1.14",
          "form": "직접가서 사는편이에요",
          "original_form": "직접가서 사는편이에요",
          "speaker_id": "2",
          "time": "20210827 16:12"
        },
        {
          "id": "MDRW2100018365.1.15",
          "form": "아하 그러시군요",
          "original_form": "아하 그러시군요",
          "speaker_id": "1",
          "time": "20210827 16:13"
        },
        {
          "id": "MDRW2100018365.1.16",
          "form": "재밌게 읽으셨던 책 추천 해주실 수 있나요?",
          "original_form": "재밌게 읽으셨던 책 추천 해주실 수 있나요?",
          "speaker_id": "1",
          "time": "20210827 16:13"
        },
        {
          "id": "MDRW2100018365.1.17",
          "form": "음 히가시노 게이고 라는 소설가의 책들이 가장 재밌었던거같아요",
          "original_form": "음 히가시노 게이고 라는 소설가의 책들이 가장 재밌었던거같아요",
          "speaker_id": "2",
          "time": "20210827 16:14"
        },
        {
          "id": "MDRW2100018365.1.18",
          "form": "라플라스의 마녀 라는 책 추천합니다",
          "original_form": "라플라스의 마녀 라는 책 추천합니다",
          "speaker_id": "2",
          "time": "20210827 16:15"
        },
        {
          "id": "MDRW2100018365.1.19",
          "form": "오 저도 추리소설 좋아해서 그 작가님 책 많이 읽었어요 !",
          "original_form": "오 저도 추리소설 좋아해서 그 작가님 책 많이 읽었어요 !",
          "speaker_id": "1",
          "time": "20210827 16:15"
        },
        {
          "id": "MDRW2100018365.1.20",
          "form": "추천해주신 책 꼭 읽어볼게요 ㅎㅎ",
          "original_form": "추천해주신 책 꼭 읽어볼게요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 16:16"
        },
        {
          "id": "MDRW2100018365.1.21",
          "form": "저도 추천해주실만한 책 있으신가요?",
          "original_form": "저도 추천해주실만한 책 있으신가요?",
          "speaker_id": "2",
          "time": "20210827 16:16"
        },
        {
          "id": "MDRW2100018365.1.22",
          "form": "sf소설인데 우리가 빛의속도로 갈수없다면 이라는 책 추천해요!",
          "original_form": "sf소설인데 우리가 빛의속도로 갈수없다면 이라는 책 추천해요!",
