      "utterance": [
        {
          "id": "MDRW2100014237.1.1",
          "form": "그냥 있는 반찬으로 먹을려고",
          "original_form": "그냥 있는 반찬으로 먹을려고",
          "speaker_id": "2",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014237.1.2",
          "form": "넌",
          "original_form": "넌",
          "speaker_id": "2",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014237.1.3",
          "form": "난 어제 우동먹어서 오늘은 밥으로 먹고 싶네",
          "original_form": "난 어제 우동먹어서 오늘은 밥으로 먹고 싶네",
          "speaker_id": "1",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014237.1.4",
          "form": "그래 골고루 먹어라",
          "original_form": "그래 골고루 먹어라",
          "speaker_id": "2",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014237.1.5",
          "form": "근데 냉장고에 뽈찜도 있고",
          "original_form": "근데 냉장고에 뽈찜도 있고",
          "speaker_id": "1",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014237.1.6",
          "form": "아직도 다 안먹었어",
          "original_form": "아직도 다 안먹었어",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014237.1.7",
          "form": "응 조금 남았어",
          "original_form": "응 조금 남았어",
          "speaker_id": "1",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014237.1.8",
          "form": "그냥 버려냐 너무 오래 있었다",
          "original_form": "그냥 버려냐 너무 오래 있었다",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014237.1.9",
          "form": "그런가?",
          "original_form": "그런가?",
          "speaker_id": "1",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014237.1.10",
          "form": "엉 아직 먹는거 조심해햐한다",
          "original_form": "엉 아직 먹는거 조심해햐한다",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014237.1.11",
          "form": "지금 비 너무 와서 배달 시켜 먹기 그런데",
          "original_form": "지금 비 너무 와서 배달 시켜 먹기 그런데",
          "speaker_id": "1",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014237.1.12",
          "form": "그렁 그냥 볶음밥 해먹어라",
          "original_form": "그렁 그냥 볶음밥 해먹어라",
          "speaker_id": "2",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014237.1.13",
          "form": "귀찮은데....아님 그냥 라면이라고 끓여 먹어야겠다",
          "original_form": "귀찮은데....아님 그냥 라면이라고 끓여 먹어야겠다",
          "speaker_id": "1",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014237.1.14",
          "form": "응 뽈찜은 먹지말고",
          "original_form": "응 뽈찜은 먹지말고",
          "speaker_id": "2",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100014237.1.15",
          "form": "알았어",
          "original_form": "알았어",
          "speaker_id": "1",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100014237.1.16",
          "form": "점심은 뭐 먹을껀데",
          "original_form": "점심은 뭐 먹을껀데",
