      "utterance": [
        {
          "id": "MDRW2100039380.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100039380.1.2",
          "form": "동물좋아하세요?",
          "original_form": "동물좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100039380.1.3",
          "form": "완전 좋아해요! 지금 강아지도 키우고 있어요ㅎㅎ",
          "original_form": "완전 좋아해요! 지금 강아지도 키우고 있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 16:56"
        },
        {
          "id": "MDRW2100039380.1.4",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210903 16:56"
        },
        {
          "id": "MDRW2100039380.1.5",
          "form": "저도 동물 좋아하는데ㅜ",
          "original_form": "저도 동물 좋아하는데ㅜ",
          "speaker_id": "1",
          "time": "20210903 16:58"
        },
        {
          "id": "MDRW2100039380.1.6",
          "form": "집이 요식업을 해서ㅜ",
          "original_form": "집이 요식업을 해서ㅜ",
          "speaker_id": "1",
          "time": "20210903 16:58"
        },
        {
          "id": "MDRW2100039380.1.7",
          "form": "못키우고 있습니다ㅜ",
          "original_form": "못키우고 있습니다ㅜ",
          "speaker_id": "1",
          "time": "20210903 16:58"
        },
        {
          "id": "MDRW2100039380.1.8",
          "form": "아하 아무래도 위생과 관련돼있으니ㅠㅠ",
          "original_form": "아하 아무래도 위생과 관련돼있으니ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100039380.1.9",
          "form": "만약 키우게되면 어떤 동물 키우고 싶으세요?",
          "original_form": "만약 키우게되면 어떤 동물 키우고 싶으세요?",
          "speaker_id": "2",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100039380.1.10",
          "form": "전 강아지 고양이 뭐든ㅎㅎㅎㅎ",
          "original_form": "전 강아지 고양이 뭐든ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 17:00"
        },
        {
          "id": "MDRW2100039380.1.11",
          "form": "둘다 너무 귀여워요ㅜ",
          "original_form": "둘다 너무 귀여워요ㅜ",
          "speaker_id": "1",
          "time": "20210903 17:00"
        },
        {
          "id": "MDRW2100039380.1.12",
          "form": "그쵸ㅠㅠ오늘 애견용품 가게 지나가는데 캣타워에 고양이 있길래 인형인줄 알았더니 고양이더라구요",
          "original_form": "그쵸ㅠㅠ오늘 애견용품 가게 지나가는데 캣타워에 고양이 있길래 인형인줄 알았더니 고양이더라구요",
          "speaker_id": "2",
          "time": "20210903 17:03"
        },
        {
          "id": "MDRW2100039380.1.13",
          "form": "누워서 자는데 너무 귀여웠어요ㅋㅋ",
          "original_form": "누워서 자는데 너무 귀여웠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:03"
        },
        {
          "id": "MDRW2100039380.1.14",
          "form": "고양이는 도도한 게 매력인거 같아요ㅜ",
          "original_form": "고양이는 도도한 게 매력인거 같아요ㅜ",
          "speaker_id": "1",
          "time": "20210903 17:07"
        },
        {
          "id": "MDRW2100039380.1.15",
          "form": "맞아요 강아지랑은 또 다른 매력ㅜㅠ근데 애교부릴때 부비부비하는건 또 귀엽고ㅠㅠ",
          "original_form": "맞아요 강아지랑은 또 다른 매력ㅜㅠ근데 애교부릴때 부비부비하는건 또 귀엽고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 17:11"
        },
        {
          "id": "MDRW2100039380.1.16",
          "form": "맞아요ㅠㅜ",
          "original_form": "맞아요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210903 18:26"
        },
        {
          "id": "MDRW2100039380.1.17",
          "form": "강아지는 뭔가 나만 바라보는게 너무 좋은거 같아요ㅜ",
          "original_form": "강아지는 뭔가 나만 바라보는게 너무 좋은거 같아요ㅜ",
          "speaker_id": "1",
          "time": "20210903 18:26"
        },
        {
          "id": "MDRW2100039380.1.18",
          "form": "진짜 집에 가면 한 몇 년은 떨어져지낸 것처럼 반겨줘요ㅋㅋ",
          "original_form": "진짜 집에 가면 한 몇 년은 떨어져지낸 것처럼 반겨줘요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:57"
        },
        {
          "id": "MDRW2100039380.1.19",
          "form": "크 부럽습니다ㅠㅜ",
          "original_form": "크 부럽습니다ㅠㅜ",
          "speaker_id": "1",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100039380.1.20",
          "form": "어떤의미론 대단하시구요!!",
          "original_form": "어떤의미론 대단하시구요!!",
          "speaker_id": "1",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100039380.1.21",
          "form": "가족들 다같이 케어하니까 괜찮아요ㅋㅋ",
          "original_form": "가족들 다같이 케어하니까 괜찮아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 19:56"
        },
        {
          "id": "MDRW2100039380.1.22",
          "form": "나중에 여건이 되신다면 키우게 되시면 좋겠어요",
          "original_form": "나중에 여건이 되신다면 키우게 되시면 좋겠어요",
          "speaker_id": "2",
          "time": "20210903 19:57"
        },
        {
          "id": "MDRW2100039380.1.23",
          "form": "진짜 여건만 되면 키우고 싶습니다!!!!!",
          "original_form": "진짜 여건만 되면 키우고 싶습니다!!!!!",
