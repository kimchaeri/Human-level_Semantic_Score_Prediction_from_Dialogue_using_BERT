      "utterance": [
        {
          "id": "MDRW2100044289.1.1",
          "form": "오늘머드셧나요",
          "original_form": "오늘머드셧나요",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044289.1.2",
          "form": "오늘은 두부조림과 낙곱새 먹었어요 님은 뭘 드셨나요?",
          "original_form": "오늘은 두부조림과 낙곱새 먹었어요 님은 뭘 드셨나요?",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044289.1.3",
          "form": "와 ㅁ대박",
          "original_form": "와 ㅁ대박",
          "speaker_id": "1",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044289.1.4",
          "form": "전 제육이요",
          "original_form": "전 제육이요",
          "speaker_id": "1",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044289.1.5",
          "form": "사드셧나요",
          "original_form": "사드셧나요",
          "speaker_id": "1",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044289.1.6",
          "form": "부모님과 같이 사는데 두부조림은 엄마가 만드시고 낙곱새는 반조리된거예요",
          "original_form": "부모님과 같이 사는데 두부조림은 엄마가 만드시고 낙곱새는 반조리된거예요",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044289.1.7",
          "form": "저녁에 드셨나요?",
          "original_form": "저녁에 드셨나요?",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044289.1.8",
          "form": "우와..낙곱새는 어디서사셧어요",
          "original_form": "우와..낙곱새는 어디서사셧어요",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044289.1.9",
          "form": "점저 다요",
          "original_form": "점저 다요",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044289.1.10",
          "form": "아마 집 근처 조방낙지?",
          "original_form": "아마 집 근처 조방낙지?",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044289.1.11",
          "form": "엄마가 사오셔서 모르겠어요",
          "original_form": "엄마가 사오셔서 모르겠어요",
          "speaker_id": "2",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044289.1.12",
          "form": "맛잇엇겟당",
          "original_form": "맛잇엇겟당",
          "speaker_id": "1",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044289.1.13",
          "form": "간식도 드셧나요",
          "original_form": "간식도 드셧나요",
          "speaker_id": "1",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044289.1.14",
          "form": "요플레 먹었어요",
          "original_form": "요플레 먹었어요",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044289.1.15",
          "form": "방금은 직접만든 라떼요",
          "original_form": "방금은 직접만든 라떼요",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044289.1.16",
          "form": "님은 간식 드셨나요?",
          "original_form": "님은 간식 드셨나요?",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044289.1.17",
          "form": "배라요",
          "original_form": "배라요",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044289.1.18",
          "form": "헐 라때",
          "original_form": "헐 라때",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044289.1.19",
          "form": "커피어케내려요",
          "original_form": "커피어케내려요",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044289.1.20",
          "form": "막 멋부리는 커피말고 카누랑 믹스커피로 해요!",
          "original_form": "막 멋부리는 커피말고 카누랑 믹스커피로 해요!",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044289.1.21",
          "form": "오오 마싯겟다 카페인 밤에 갠차느세요?",
          "original_form": "오오 마싯겟다 카페인 밤에 갠차느세요?",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044289.1.22",
          "form": "네 저는 아이러니하게도 카페인이 잘 맞더라구요",
          "original_form": "네 저는 아이러니하게도 카페인이 잘 맞더라구요",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044289.1.23",
          "form": "부러워요 전 잠이안와요",
          "original_form": "부러워요 전 잠이안와요",
          "speaker_id": "1",
          "time": "20210907 22:24"
        }
      ]
