      "utterance": [
        {
          "id": "MDRW2100006751.1.1",
          "form": "아침 드셨나요",
          "original_form": "아침 드셨나요",
          "speaker_id": "1",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100006751.1.2",
          "form": "저희는 아침 꼭 먹습니다",
          "original_form": "저희는 아침 꼭 먹습니다",
          "speaker_id": "2",
          "time": "20210908 08:32"
        },
        {
          "id": "MDRW2100006751.1.3",
          "form": "오늘도 먹었지요",
          "original_form": "오늘도 먹었지요",
          "speaker_id": "2",
          "time": "20210908 08:32"
        },
        {
          "id": "MDRW2100006751.1.4",
          "form": "name1님은 드셨나요?",
          "original_form": "&name1&님은 드셨나요?",
          "speaker_id": "2",
          "time": "20210908 08:32"
        },
        {
          "id": "MDRW2100006751.1.5",
          "form": "네 저도 대충이라도ㅎㅎ",
          "original_form": "네 저도 대충이라도ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 08:33"
        },
        {
          "id": "MDRW2100006751.1.6",
          "form": "아침엔 어떻게 드세요?",
          "original_form": "아침엔 어떻게 드세요?",
          "speaker_id": "1",
          "time": "20210908 08:33"
        },
        {
          "id": "MDRW2100006751.1.7",
          "form": "보통 아내가 밥국을 하는데",
          "original_form": "보통 아내가 밥국을 하는데",
          "speaker_id": "2",
          "time": "20210908 08:34"
        },
        {
          "id": "MDRW2100006751.1.8",
          "form": "요새는 간단히 요거트에 견과류 넣어서 과일과 채소를 곁들여 먹습니다",
          "original_form": "요새는 간단히 요거트에 견과류 넣어서 과일과 채소를 곁들여 먹습니다",
          "speaker_id": "2",
          "time": "20210908 08:34"
        },
        {
          "id": "MDRW2100006751.1.9",
          "form": "와 건강 상차림이네요",
          "original_form": "와 건강 상차림이네요",
          "speaker_id": "1",
          "time": "20210908 08:41"
        },
        {
          "id": "MDRW2100006751.1.10",
          "form": "요즘 그릭 요거트가 핫하더라고요",
          "original_form": "요즘 그릭 요거트가 핫하더라고요",
          "speaker_id": "1",
          "time": "20210908 08:41"
        },
        {
          "id": "MDRW2100006751.1.11",
          "form": "네 요거트를 먹어도 속이 편하더군요",
          "original_form": "네 요거트를 먹어도 속이 편하더군요",
          "speaker_id": "2",
          "time": "20210908 08:43"
        },
        {
          "id": "MDRW2100006751.1.12",
          "form": "일단 아침은 꼭 먹어야 하는 식구들이랍니다",
          "original_form": "일단 아침은 꼭 먹어야 하는 식구들이랍니다",
          "speaker_id": "2",
          "time": "20210908 08:43"
        },
        {
          "id": "MDRW2100006751.1.13",
          "form": "정말 좋은 습관이시네요.",
          "original_form": "정말 좋은 습관이시네요.",
          "speaker_id": "1",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100006751.1.14",
          "form": "저흰 저만 먹어요ㅡㄹ",
          "original_form": "저흰 저만 먹어요ㅡㄹ",
          "speaker_id": "1",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100006751.1.15",
          "form": "그래요? 아침 먹는 것도 습관이다 보니 안먹는 사람들은 먹는게  곤혹이라더군요",
          "original_form": "그래요? 아침 먹는 것도 습관이다 보니 안먹는 사람들은 먹는게  곤혹이라더군요",
          "speaker_id": "2",
          "time": "20210908 08:55"
        },
        {
          "id": "MDRW2100006751.1.16",
          "form": "저희는 부부가 아침을 꼭 먹어서 아이들도 아침 꼭 먹습니다",
          "original_form": "저희는 부부가 아침을 꼭 먹어서 아이들도 아침 꼭 먹습니다",
          "speaker_id": "2",
          "time": "20210908 08:55"
        },
        {
          "id": "MDRW2100006751.1.17",
          "form": "아침에 아이들과 함께라니",
          "original_form": "아침에 아이들과 함께라니",
          "speaker_id": "1",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100006751.1.18",
          "form": "정말 좋은 교육이시네요",
          "original_form": "정말 좋은 교육이시네요",
          "speaker_id": "1",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100006751.1.19",
          "form": "네 학교 다닐때도 지각하더라도 일단 아침은 먹어야 한다고 훈련시켜놨더니 다 잘 일어나서 먹습디다",
          "original_form": "네 학교 다닐때도 지각하더라도 일단 아침은 먹어야 한다고 훈련시켜놨더니 다 잘 일어나서 먹습디다",
          "speaker_id": "2",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100006751.1.20",
          "form": "요즘 아이들 같지 않네요",
          "original_form": "요즘 아이들 같지 않네요",
          "speaker_id": "1",
          "time": "20210908 09:34"
        },
        {
          "id": "MDRW2100006751.1.21",
          "form": "다행이 늦잠자는 아이들이 아니라서 잘 일어납니다",
          "original_form": "다행이 늦잠자는 아이들이 아니라서 잘 일어납니다",
          "speaker_id": "2",
          "time": "20210908 09:36"
        },
        {
          "id": "MDRW2100006751.1.22",
          "form": "저녁 식사도 같이 건강하게 많이 하시겠어요",
          "original_form": "저녁 식사도 같이 건강하게 많이 하시겠어요",
          "speaker_id": "1",
          "time": "20210908 09:39"
        },
        {
          "id": "MDRW2100006751.1.23",
          "form": "네 우리 식구는 하루에 한끼는 다 같이 먹기로 해서요",
          "original_form": "네 우리 식구는 하루에 한끼는 다 같이 먹기로 해서요",
          "speaker_id": "2",
          "time": "20210908 09:43"
        },
        {
          "id": "MDRW2100006751.1.24",
          "form": "아침을 혹시 따로 먹게 될 때 저녁을 같이 먹습니다",
          "original_form": "아침을 혹시 따로 먹게 될 때 저녁을 같이 먹습니다",
          "speaker_id": "2",
          "time": "20210908 09:43"
        }
