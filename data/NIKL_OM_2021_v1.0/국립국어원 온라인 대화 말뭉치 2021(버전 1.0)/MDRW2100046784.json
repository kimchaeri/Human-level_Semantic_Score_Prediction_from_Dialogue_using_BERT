        },
        {
          "id": "MDRW2100046784.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211104 18:56"
        },
        {
          "id": "MDRW2100046784.1.3",
          "form": "지금 뭐 하고 계시나요? ㅎㅎ",
          "original_form": "지금 뭐 하고 계시나요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 18:56"
        },
        {
          "id": "MDRW2100046784.1.4",
          "form": "롤이요",
          "original_form": "롤이요",
          "speaker_id": "1",
          "time": "20211104 18:56"
        },
        {
          "id": "MDRW2100046784.1.5",
          "form": "게임 좋아하시는군요!",
          "original_form": "게임 좋아하시는군요!",
          "speaker_id": "2",
          "time": "20211104 18:56"
        },
        {
          "id": "MDRW2100046784.1.6",
          "form": "롤만 하셔요?",
          "original_form": "롤만 하셔요?",
          "speaker_id": "2",
          "time": "20211104 18:57"
        },
        {
          "id": "MDRW2100046784.1.7",
          "form": "네 배그도요",
          "original_form": "네 배그도요",
          "speaker_id": "1",
          "time": "20211104 18:57"
        },
        {
          "id": "MDRW2100046784.1.8",
          "form": "어떤 게임을 더 좋아하세요?",
          "original_form": "어떤 게임을 더 좋아하세요?",
          "speaker_id": "2",
          "time": "20211104 18:57"
        },
        {
          "id": "MDRW2100046784.1.9",
          "form": "롤이요 ㅋㅋ",
          "original_form": "롤이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211104 18:57"
        },
        {
          "id": "MDRW2100046784.1.10",
          "form": "전 롤은 너무 어려워서",
          "original_form": "전 롤은 너무 어려워서",
          "speaker_id": "2",
          "time": "20211104 18:57"
        },
        {
          "id": "MDRW2100046784.1.11",
          "form": "배그가 더 재밌더라구요 ㅋㅋ",
          "original_form": "배그가 더 재밌더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 18:58"
        },
        {
          "id": "MDRW2100046784.1.12",
          "form": "배그 재밌죠 ㅋㅋ",
          "original_form": "배그 재밌죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211104 18:58"
        },
        {
          "id": "MDRW2100046784.1.13",
          "form": "네 ㅋㅋㅋ 처음 나왔을때 진짜 밤새도록 했어요",
          "original_form": "네 ㅋㅋㅋ 처음 나왔을때 진짜 밤새도록 했어요",
          "speaker_id": "2",
          "time": "20211104 18:58"
        },
        {
          "id": "MDRW2100046784.1.14",
          "form": "배그 유튜브도 많이 보고 그랬는데 ㅋㅋㅋ",
          "original_form": "배그 유튜브도 많이 보고 그랬는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 18:58"
        },
        {
          "id": "MDRW2100046784.1.15",
          "form": "배그 누구 봤어요 ?",
          "original_form": "배그 누구 봤어요 ?",
          "speaker_id": "1",
          "time": "20211104 18:58"
        },
        {
          "id": "MDRW2100046784.1.16",
          "form": "저 우주하마요!",
          "original_form": "저 우주하마요!",
          "speaker_id": "2",
          "time": "20211104 18:59"
        },
        {
          "id": "MDRW2100046784.1.17",
          "form": "아 ㅋㅋㅋ 저도 봐볼게요",
          "original_form": "아 ㅋㅋㅋ 저도 봐볼게요",
          "speaker_id": "1",
          "time": "20211104 18:59"
        },
        {
          "id": "MDRW2100046784.1.18",
          "form": "재밌어요 ㅋㅋㅋ 요즘엔 배그는 안하더라구요",
          "original_form": "재밌어요 ㅋㅋㅋ 요즘엔 배그는 안하더라구요",
