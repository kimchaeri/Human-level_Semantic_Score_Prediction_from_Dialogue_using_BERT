      "utterance": [
        {
          "id": "MDRW2100045992.1.1",
          "form": "최근 구매물건이 있으신가요",
          "original_form": "최근 구매물건이 있으신가요",
          "speaker_id": "2",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100045992.1.2",
          "form": "저 버즈2  샀어요",
          "original_form": "저 버즈2  샀어요",
          "speaker_id": "1",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100045992.1.3",
          "form": "구매한것 만족 하시나요",
          "original_form": "구매한것 만족 하시나요",
          "speaker_id": "2",
          "time": "20210908 09:33"
        },
        {
          "id": "MDRW2100045992.1.4",
          "form": "아직 2일되서 ㅋ",
          "original_form": "아직 2일되서 ㅋ",
          "speaker_id": "1",
          "time": "20210908 09:35"
        },
        {
          "id": "MDRW2100045992.1.5",
          "form": "버즈 쓰세요?",
          "original_form": "버즈 쓰세요?",
          "speaker_id": "1",
          "time": "20210908 09:36"
        },
        {
          "id": "MDRW2100045992.1.6",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210908 09:37"
        },
        {
          "id": "MDRW2100045992.1.7",
          "form": "가격은 비싸지 않나요",
          "original_form": "가격은 비싸지 않나요",
          "speaker_id": "2",
          "time": "20210908 09:37"
        },
        {
          "id": "MDRW2100045992.1.8",
          "form": "당근에서 샀어요",
          "original_form": "당근에서 샀어요",
          "speaker_id": "1",
          "time": "20210908 09:39"
        },
        {
          "id": "MDRW2100045992.1.9",
          "form": "당근에서도 판매하나요",
          "original_form": "당근에서도 판매하나요",
          "speaker_id": "2",
          "time": "20210908 09:40"
        },
        {
          "id": "MDRW2100045992.1.10",
          "form": "당근도 구경해봐야 겠어요",
          "original_form": "당근도 구경해봐야 겠어요",
          "speaker_id": "2",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100045992.1.11",
          "form": "중고죠 ㅎ",
          "original_form": "중고죠 ㅎ",
          "speaker_id": "1",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100045992.1.12",
          "form": "지금 미개봉으로 팔더라구요",
          "original_form": "지금 미개봉으로 팔더라구요",
          "speaker_id": "1",
          "time": "20210908 10:37"
        },
        {
          "id": "MDRW2100045992.1.13",
          "form": "개봉 안한 새것도 있군요",
          "original_form": "개봉 안한 새것도 있군요",
          "speaker_id": "2",
          "time": "20210908 10:39"
        },
        {
          "id": "MDRW2100045992.1.14",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210908 10:40"
        },
        {
          "id": "MDRW2100045992.1.15",
          "form": "출시한지얼마안되고",
          "original_form": "출시한지얼마안되고",
          "speaker_id": "1",
          "time": "20210908 10:40"
        },
        {
          "id": "MDRW2100045992.1.16",
          "form": "플립3 인가 사면서 사은품으로 받으신분들은",
          "original_form": "플립3 인가 사면서 사은품으로 받으신분들은",
          "speaker_id": "1",
          "time": "20210908 10:40"
        },
        {
          "id": "MDRW2100045992.1.17",
          "form": "필요없으면 내놓으시더라구요",
          "original_form": "필요없으면 내놓으시더라구요",
          "speaker_id": "1",
          "time": "20210908 10:40"
        },
        {
          "id": "MDRW2100045992.1.18",
          "form": "그런제품은 구매하면 좋겠네요",
          "original_form": "그런제품은 구매하면 좋겠네요",
          "speaker_id": "2",
          "time": "20210908 10:43"
        },
        {
          "id": "MDRW2100045992.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100045992.1.20",
          "form": "싸게 사니 좋아요",
          "original_form": "싸게 사니 좋아요",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100045992.1.21",
          "form": "새제품을",
          "original_form": "새제품을",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100045992.1.22",
