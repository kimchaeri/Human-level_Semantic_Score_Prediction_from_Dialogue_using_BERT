      "utterance": [
        {
          "id": "MDRW2100007394.1.1",
          "form": "청주 오늘 비 왔어?",
          "original_form": "청주 오늘 비 왔어?",
          "speaker_id": "1",
          "time": "20210822 01:47"
        },
        {
          "id": "MDRW2100007394.1.2",
          "form": "웅 왔어",
          "original_form": "웅 왔어",
          "speaker_id": "2",
          "time": "20210822 01:49"
        },
        {
          "id": "MDRW2100007394.1.3",
          "form": "여긴 거의 장마처럼 오던데",
          "original_form": "여긴 거의 장마처럼 오던데",
          "speaker_id": "1",
          "time": "20210822 01:51"
        },
        {
          "id": "MDRW2100007394.1.4",
          "form": "장마는 오래오는 비 말하는게 아니여?",
          "original_form": "장마는 오래오는 비 말하는게 아니여?",
          "speaker_id": "2",
          "time": "20210822 01:51"
        },
        {
          "id": "MDRW2100007394.1.5",
          "form": "장마 때 오래오기도 하고 많이 오기도 하잖아",
          "original_form": "장마 때 오래오기도 하고 많이 오기도 하잖아",
          "speaker_id": "1",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007394.1.6",
          "form": "그런가...?",
          "original_form": "그런가...?",
          "speaker_id": "2",
          "time": "20210822 02:02"
        },
        {
          "id": "MDRW2100007394.1.7",
          "form": "난 장마는 그냥 몇일이상 오는 비",
          "original_form": "난 장마는 그냥 몇일이상 오는 비",
          "speaker_id": "2",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007394.1.8",
          "form": "이렇게만 생각했어",
          "original_form": "이렇게만 생각했어",
          "speaker_id": "2",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007394.1.9",
          "form": "아닌가? 그럴수도 있지",
          "original_form": "아닌가? 그럴수도 있지",
          "speaker_id": "1",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007394.1.10",
          "form": "어쨌든 요즘 장마보다 비가 더 많이 내리더라",
          "original_form": "어쨌든 요즘 장마보다 비가 더 많이 내리더라",
          "speaker_id": "1",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007394.1.11",
          "form": "폭우야",
          "original_form": "폭우야",
          "speaker_id": "1",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007394.1.12",
          "form": "그니까 그래서 좀 추워",
          "original_form": "그니까 그래서 좀 추워",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007394.1.13",
          "form": "진짜 에어컨 키는 곳은 이해안갈정도",
          "original_form": "진짜 에어컨 키는 곳은 이해안갈정도",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007394.1.14",
          "form": "맞아 여기도 에어컨 안 켜도 되더라",
          "original_form": "맞아 여기도 에어컨 안 켜도 되더라",
          "speaker_id": "1",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007394.1.15",
          "form": "20도까지 내려가던데",
          "original_form": "20도까지 내려가던데",
          "speaker_id": "1",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007394.1.16",
          "form": "긴팔 입어야대 ㅋㅋㅋ",
          "original_form": "긴팔 입어야대 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 08:23"
        },
        {
          "id": "MDRW2100007394.1.17",
          "form": "ㅋㅋㅋ맞아 저녁이나 아침에 나가면",
          "original_form": "ㅋㅋㅋ맞아 저녁이나 아침에 나가면",
          "speaker_id": "1",
          "time": "20210822 08:41"
        },
        {
          "id": "MDRW2100007394.1.18",
          "form": "긴팔도 보이더라",
          "original_form": "긴팔도 보이더라",
          "speaker_id": "1",
          "time": "20210822 08:41"
        },
        {
          "id": "MDRW2100007394.1.19",
          "form": "여름 다 끝난듯?!",
          "original_form": "여름 다 끝난듯?!",
          "speaker_id": "2",
          "time": "20210822 09:20"
        },
        {
          "id": "MDRW2100007394.1.20",
          "form": "그래도 낮엔 더워...",
          "original_form": "그래도 낮엔 더워...",
          "speaker_id": "1",
          "time": "20210822 09:22"
        },
        {
          "id": "MDRW2100007394.1.21",
          "form": "원래 가을에도 낮엔 더워",
          "original_form": "원래 가을에도 낮엔 더워",
          "speaker_id": "2",
          "time": "20210822 09:28"
        },
        {
          "id": "MDRW2100007394.1.22",
          "form": "아침에 추워서 운동하려고 준비를했는데",
          "original_form": "아침에 추워서 운동하려고 준비를했는데",
          "speaker_id": "2",
          "time": "20210904 10:49"
        },
        {
          "id": "MDRW2100007394.1.23",
          "form": "더워졌어",
          "original_form": "더워졌어",
          "speaker_id": "2",
          "time": "20210904 10:49"
        },
