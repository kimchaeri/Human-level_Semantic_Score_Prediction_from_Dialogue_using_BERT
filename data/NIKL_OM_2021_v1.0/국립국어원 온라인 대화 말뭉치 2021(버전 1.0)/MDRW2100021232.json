      "utterance": [
        {
          "id": "MDRW2100021232.1.1",
          "form": "일본 영화는 종종 봐요 !",
          "original_form": "일본 영화는 종종 봐요 !",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021232.1.2",
          "form": "드라마는 본적이 없네요 ㅠ",
          "original_form": "드라마는 본적이 없네요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100021232.1.3",
          "form": "일존영화는 기억나는거 있어여?",
          "original_form": "일존영화는 기억나는거 있어여?",
          "speaker_id": "1",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021232.1.4",
          "form": "저는 애니쪽 좋아해서 하울 같은거 좋아해요 !",
          "original_form": "저는 애니쪽 좋아해서 하울 같은거 좋아해요 !",
          "speaker_id": "2",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100021232.1.5",
          "form": "지브리꺼 많이 보셧나봐요?",
          "original_form": "지브리꺼 많이 보셧나봐요?",
          "speaker_id": "1",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100021232.1.6",
          "form": "네네 ㅎㅎ",
          "original_form": "네네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 17:15"
        },
        {
          "id": "MDRW2100021232.1.7",
          "form": "지브리꺼 보시나요?",
          "original_form": "지브리꺼 보시나요?",
          "speaker_id": "2",
          "time": "20210828 17:15"
        },
        {
          "id": "MDRW2100021232.1.8",
          "form": "옛날꺼는 다봣는데... 포뇨 이후는 한개도 못본듯..",
          "original_form": "옛날꺼는 다봣는데... 포뇨 이후는 한개도 못본듯..",
          "speaker_id": "1",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100021232.1.9",
          "form": "최근 비행기만드는거? 바람 어쩌구인가 다운 받았어요.ㅡ",
          "original_form": "최근 비행기만드는거? 바람 어쩌구인가 다운 받았어요.ㅡ",
          "speaker_id": "1",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100021232.1.10",
          "form": "바람이 분다 맞죠 ??",
          "original_form": "바람이 분다 맞죠 ??",
          "speaker_id": "2",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100021232.1.11",
          "form": "아ㅡㅡ 그거.. 같아요... 그게 젤 최신꺼이죠?",
          "original_form": "아ㅡㅡ 그거.. 같아요... 그게 젤 최신꺼이죠?",
          "speaker_id": "1",
          "time": "20210828 17:24"
        },
        {
          "id": "MDRW2100021232.1.12",
          "form": "올해 또 뭐 나왔는데",
          "original_form": "올해 또 뭐 나왔는데",
          "speaker_id": "2",
          "time": "20210828 17:37"
        },
        {
          "id": "MDRW2100021232.1.13",
          "form": "별론거같더라고요 ㅠㅠ",
          "original_form": "별론거같더라고요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 17:37"
        },
        {
          "id": "MDRW2100021232.1.14",
          "form": "요즘은 너의 이름은? 그쪽이 더 떼깔이 좋은듯..",
          "original_form": "요즘은 너의 이름은? 그쪽이 더 떼깔이 좋은듯..",
          "speaker_id": "1",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100021232.1.15",
          "form": "신카이마코토? 던가..",
          "original_form": "신카이마코토? 던가..",
          "speaker_id": "1",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100021232.1.16",
          "form": "아 맞아요 ㅋㅋ 재밌죠 ㅎㅎ",
          "original_form": "아 맞아요 ㅋㅋ 재밌죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 17:50"
        },
        {
          "id": "MDRW2100021232.1.17",
          "form": "그런 색감?이 더 좋아요 ㅎㅎ",
          "original_form": "그런 색감?이 더 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 17:50"
        },
        {
          "id": "MDRW2100021232.1.18",
          "form": "지브리는 지금은 쫌 올드하긴해요",
          "original_form": "지브리는 지금은 쫌 올드하긴해요",
          "speaker_id": "1",
          "time": "20210828 17:51"
        },
        {
          "id": "MDRW2100021232.1.19",
          "form": "맞아요 .. 감을 잃었어요 ㅋㅋ",
          "original_form": "맞아요 .. 감을 잃었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:52"
        }
      ]
