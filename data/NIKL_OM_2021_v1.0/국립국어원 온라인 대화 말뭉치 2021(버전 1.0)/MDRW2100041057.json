        },
        {
          "id": "MDRW2100041057.1.3",
          "form": "다이어트하시나요?",
          "original_form": "다이어트하시나요?",
          "speaker_id": "1",
          "time": "20210905 16:15"
        },
        {
          "id": "MDRW2100041057.1.4",
          "form": "전 안 해요",
          "original_form": "전 안 해요",
          "speaker_id": "2",
          "time": "20210905 16:16"
        },
        {
          "id": "MDRW2100041057.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210905 16:16"
        },
        {
          "id": "MDRW2100041057.1.6",
          "form": "전 입다이어터예요ㅋㅋ",
          "original_form": "전 입다이어터예요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:17"
        },
        {
          "id": "MDRW2100041057.1.7",
          "form": "운동은 하세요?",
          "original_form": "운동은 하세요?",
          "speaker_id": "1",
          "time": "20210905 16:17"
        },
        {
          "id": "MDRW2100041057.1.8",
          "form": "근육 키우고 싶어서 피티 하고 있어요",
          "original_form": "근육 키우고 싶어서 피티 하고 있어요",
          "speaker_id": "2",
          "time": "20210905 16:18"
        },
        {
          "id": "MDRW2100041057.1.9",
          "form": "name1님은 운동 하세요?",
          "original_form": "&name1&님은 운동 하세요?",
          "speaker_id": "2",
          "time": "20210905 16:18"
        },
        {
          "id": "MDRW2100041057.1.10",
          "form": "와 멋지네요",
          "original_form": "와 멋지네요",
          "speaker_id": "1",
          "time": "20210905 16:18"
        },
        {
          "id": "MDRW2100041057.1.11",
          "form": "저는 빨리 걷기만요",
          "original_form": "저는 빨리 걷기만요",
          "speaker_id": "1",
          "time": "20210905 16:18"
        },
        {
          "id": "MDRW2100041057.1.12",
          "form": "일주일에 몇번하세요?",
          "original_form": "일주일에 몇번하세요?",
          "speaker_id": "1",
          "time": "20210905 16:18"
        },
        {
          "id": "MDRW2100041057.1.13",
          "form": "저는 일주일에 3번 정도 해요!",
          "original_form": "저는 일주일에 3번 정도 해요!",
          "speaker_id": "2",
          "time": "20210905 16:19"
        },
        {
          "id": "MDRW2100041057.1.14",
          "form": "name1님은 매일 걸으시는 건가요?",
          "original_form": "&name1&님은 매일 걸으시는 건가요?",
          "speaker_id": "2",
          "time": "20210905 16:19"
        },
        {
          "id": "MDRW2100041057.1.15",
          "form": "저도 일주일에 3-4번정도요",
          "original_form": "저도 일주일에 3-4번정도요",
          "speaker_id": "1",
          "time": "20210905 16:19"
        },
        {
          "id": "MDRW2100041057.1.16",
          "form": "한번할때 몇시간하세요?",
          "original_form": "한번할때 몇시간하세요?",
          "speaker_id": "1",
          "time": "20210905 16:19"
        },
        {
          "id": "MDRW2100041057.1.17",
          "form": "저는 2시간 정도 하는 것 같아요",
          "original_form": "저는 2시간 정도 하는 것 같아요",
          "speaker_id": "2",
          "time": "20210905 16:20"
        },
        {
          "id": "MDRW2100041057.1.18",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210905 16:20"
        },
        {
          "id": "MDRW2100041057.1.19",
          "form": "저는 한시간정도요",
          "original_form": "저는 한시간정도요",
          "speaker_id": "1",
          "time": "20210905 16:20"
        },
        {
          "id": "MDRW2100041057.1.20",
          "form": "운동 효과가 있나요?",
          "original_form": "운동 효과가 있나요?",
          "speaker_id": "1",
          "time": "20210905 16:20"
        },
        {
          "id": "MDRW2100041057.1.21",
          "form": "네 살이 엄청 쭉쭉 빠지고 있어요",
          "original_form": "네 살이 엄청 쭉쭉 빠지고 있어요",
          "speaker_id": "2",
          "time": "20210905 16:21"
        },
        {
          "id": "MDRW2100041057.1.22",
          "form": "name1님은 효과 보시고 계신가요?",
          "original_form": "&name1&님은 효과 보시고 계신가요?",
          "speaker_id": "2",
          "time": "20210905 16:21"
        },
        {
          "id": "MDRW2100041057.1.23",
          "form": "아뇨 저는 현상유지중이예요",
          "original_form": "아뇨 저는 현상유지중이예요",
          "speaker_id": "1",
          "time": "20210905 16:21"
        },
        {
          "id": "MDRW2100041057.1.24",
          "form": "그래도 안하는것보다는 낫겠지 싶어서 ㅎ",
