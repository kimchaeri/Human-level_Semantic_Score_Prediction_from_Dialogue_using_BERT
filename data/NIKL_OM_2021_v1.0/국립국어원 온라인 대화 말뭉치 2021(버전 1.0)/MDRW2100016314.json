      "utterance": [
        {
          "id": "MDRW2100016314.1.1",
          "form": "반려동물 키우시나요",
          "original_form": "반려동물 키우시나요",
          "speaker_id": "2",
          "time": "20210826 12:00"
        },
        {
          "id": "MDRW2100016314.1.2",
          "form": "저는 구피라는 물고기는 키우고있어요!ㅎㅎ",
          "original_form": "저는 구피라는 물고기는 키우고있어요!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 12:03"
        },
        {
          "id": "MDRW2100016314.1.3",
          "form": "오 물고기 신가하네요",
          "original_form": "오 물고기 신가하네요",
          "speaker_id": "2",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016314.1.4",
          "form": "작아서 움직일때 아주 귀여워요!ㅎㅎ",
          "original_form": "작아서 움직일때 아주 귀여워요!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 12:12"
        },
        {
          "id": "MDRW2100016314.1.5",
          "form": "name1은 키우는 동물 있으세요?",
          "original_form": "&name1&은 키우는 동물 있으세요?",
          "speaker_id": "1",
          "time": "20210826 12:12"
        },
        {
          "id": "MDRW2100016314.1.6",
          "form": "아뇨 고양이 키워보고싶어요",
          "original_form": "아뇨 고양이 키워보고싶어요",
          "speaker_id": "2",
          "time": "20210826 12:20"
        },
        {
          "id": "MDRW2100016314.1.7",
          "form": "고양이 좋죠ㅎㅎ",
          "original_form": "고양이 좋죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 12:27"
        },
        {
          "id": "MDRW2100016314.1.8",
          "form": "진짜 이쁜 고양이 많더라구요!",
          "original_form": "진짜 이쁜 고양이 많더라구요!",
          "speaker_id": "1",
          "time": "20210826 12:27"
        },
        {
          "id": "MDRW2100016314.1.9",
          "form": "눈이 넘 귀여워요",
          "original_form": "눈이 넘 귀여워요",
          "speaker_id": "2",
          "time": "20210826 12:36"
        },
        {
          "id": "MDRW2100016314.1.10",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210826 12:37"
        },
        {
          "id": "MDRW2100016314.1.11",
          "form": "저는 털 복실한 고양이들이 진짜 귀여운거같아요!",
          "original_form": "저는 털 복실한 고양이들이 진짜 귀여운거같아요!",
          "speaker_id": "1",
          "time": "20210826 12:37"
        },
        {
          "id": "MDRW2100016314.1.12",
          "form": "울음소리도 귀엽고ㅠ",
          "original_form": "울음소리도 귀엽고ㅠ",
          "speaker_id": "2",
          "time": "20210826 12:47"
        },
        {
          "id": "MDRW2100016314.1.13",
          "form": "맞아요ㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 12:51"
        },
        {
          "id": "MDRW2100016314.1.14",
          "form": "고양이 키우면 하루종일 보고만있어도 행복할거같아요",
          "original_form": "고양이 키우면 하루종일 보고만있어도 행복할거같아요",
          "speaker_id": "1",
          "time": "20210826 12:51"
        },
        {
          "id": "MDRW2100016314.1.15",
          "form": "마자요 언제 키워보지",
          "original_form": "마자요 언제 키워보지",
          "speaker_id": "2",
          "time": "20210826 13:25"
        },
        {
          "id": "MDRW2100016314.1.16",
          "form": "강아지도 좋아하세요?",
          "original_form": "강아지도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 13:31"
        },
        {
          "id": "MDRW2100016314.1.17",
          "form": "음 강아지는 생각이없네요ㅎㅎ",
          "original_form": "음 강아지는 생각이없네요ㅎㅎ",
