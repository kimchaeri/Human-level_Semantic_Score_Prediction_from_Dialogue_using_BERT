      "utterance": [
        {
          "id": "MDRW2100032418.1.1",
          "form": "봄은 좋아하시나요",
          "original_form": "봄은 좋아하시나요",
          "speaker_id": "2",
          "time": "20210905 17:11"
        },
        {
          "id": "MDRW2100032418.1.2",
          "form": "봄은 꽃이 피고 새로 시작하는 기분이지요",
          "original_form": "봄은 꽃이 피고 새로 시작하는 기분이지요",
          "speaker_id": "1",
          "time": "20210905 17:11"
        },
        {
          "id": "MDRW2100032418.1.3",
          "form": "싫어 할 이유가 없네요",
          "original_form": "싫어 할 이유가 없네요",
          "speaker_id": "2",
          "time": "20210905 17:11"
        },
        {
          "id": "MDRW2100032418.1.4",
          "form": "맞아요 봄은 새생명이 돋으니까",
          "original_form": "맞아요 봄은 새생명이 돋으니까",
          "speaker_id": "1",
          "time": "20210905 17:12"
        },
        {
          "id": "MDRW2100032418.1.5",
          "form": "새로 시작하기에 좋은데 출발 분위기",
          "original_form": "새로 시작하기에 좋은데 출발 분위기",
          "speaker_id": "2",
          "time": "20210905 17:12"
        },
        {
          "id": "MDRW2100032418.1.6",
          "form": "봄에는 계획을 세우게 되어요",
          "original_form": "봄에는 계획을 세우게 되어요",
          "speaker_id": "1",
          "time": "20210905 17:12"
        },
        {
          "id": "MDRW2100032418.1.7",
          "form": "기대감이 한껏 차오릅니다",
          "original_form": "기대감이 한껏 차오릅니다",
          "speaker_id": "2",
          "time": "20210905 17:12"
        },
        {
          "id": "MDRW2100032418.1.8",
          "form": "꽃도 종류별로 피어서 꽃을 사 놓기 좋은 계절이에요",
          "original_form": "꽃도 종류별로 피어서 꽃을 사 놓기 좋은 계절이에요",
          "speaker_id": "1",
          "time": "20210905 17:12"
        },
        {
          "id": "MDRW2100032418.1.9",
          "form": "여름엔 꽃을 삽니다",
          "original_form": "여름엔 꽃을 삽니다",
          "speaker_id": "2",
          "time": "20210905 17:12"
        },
        {
          "id": "MDRW2100032418.1.10",
          "form": "꽃가루 알러지는 힘들어요",
          "original_form": "꽃가루 알러지는 힘들어요",
          "speaker_id": "1",
          "time": "20210905 17:13"
        },
        {
          "id": "MDRW2100032418.1.11",
          "form": "봄에는 비염 때문에 힘들지요",
          "original_form": "봄에는 비염 때문에 힘들지요",
          "speaker_id": "2",
          "time": "20210905 17:13"
        },
        {
          "id": "MDRW2100032418.1.12",
          "form": "그래도 봄은 장점이 많아요",
          "original_form": "그래도 봄은 장점이 많아요",
          "speaker_id": "1",
          "time": "20210905 17:13"
        },
        {
          "id": "MDRW2100032418.1.13",
          "form": "새 학기도 시작되고",
          "original_form": "새 학기도 시작되고",
          "speaker_id": "2",
          "time": "20210905 17:13"
        },
        {
          "id": "MDRW2100032418.1.14",
          "form": "한해가 시작될뿐인데 정겨워요",
          "original_form": "한해가 시작될뿐인데 정겨워요",
