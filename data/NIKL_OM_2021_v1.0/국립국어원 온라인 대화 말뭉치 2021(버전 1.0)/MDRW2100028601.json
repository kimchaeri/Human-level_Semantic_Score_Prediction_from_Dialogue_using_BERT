      "utterance": [
        {
          "id": "MDRW2100028601.1.1",
          "form": "저는 웨이브쓰고있어요!",
          "original_form": "저는 웨이브쓰고있어요!",
          "speaker_id": "2",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028601.1.2",
          "form": "name1님은요?!",
          "original_form": "&name1&님은요?!",
          "speaker_id": "2",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028601.1.3",
          "form": "저는 넷플릭스만 이용중인데",
          "original_form": "저는 넷플릭스만 이용중인데",
          "speaker_id": "1",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028601.1.4",
          "form": "친구꺼 빌려서 웨이브랑 티빙 보고 있어요!",
          "original_form": "친구꺼 빌려서 웨이브랑 티빙 보고 있어요!",
          "speaker_id": "1",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028601.1.5",
          "form": "우와 다갖추신거에요 그3개면",
          "original_form": "우와 다갖추신거에요 그3개면",
          "speaker_id": "2",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028601.1.6",
          "form": "네 친구 덕분에 골고루 잘보고있어요!",
          "original_form": "네 친구 덕분에 골고루 잘보고있어요!",
          "speaker_id": "1",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100028601.1.7",
          "form": "근데 가끔은 왓챠도 보고싶더라구요ㅠㅠ",
          "original_form": "근데 가끔은 왓챠도 보고싶더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100028601.1.8",
          "form": "부러워요,,,",
          "original_form": "부러워요,,,",
          "speaker_id": "2",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100028601.1.9",
          "form": "티빙이랑 웨이브가 합쳤으면 좋겧어오",
          "original_form": "티빙이랑 웨이브가 합쳤으면 좋겧어오",
          "speaker_id": "2",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100028601.1.10",
          "form": "헉 그럼 완벽한데ㅠㅠ",
          "original_form": "헉 그럼 완벽한데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028601.1.11",
          "form": "넷플은 오리지널이 너무재밌죠ㅠ",
          "original_form": "넷플은 오리지널이 너무재밌죠ㅠ",
          "speaker_id": "2",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028601.1.12",
          "form": "네 보신 작품 있으신가요??",
          "original_form": "네 보신 작품 있으신가요??",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028601.1.13",
          "form": "브리저튼,,,!",
          "original_form": "브리저튼,,,!",
          "speaker_id": "2",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028601.1.14",
          "form": "이 넷플오리지널맞나요",
          "original_form": "이 넷플오리지널맞나요",
          "speaker_id": "2",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028601.1.15",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028601.1.16",
          "form": "몰라서 검색해봤는데 오리지널이네요!",
          "original_form": "몰라서 검색해봤는데 오리지널이네요!",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028601.1.17",
          "form": "브리저튼 재밌나요??!",
          "original_form": "브리저튼 재밌나요??!",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028601.1.18",
          "form": "야하다고해서",
          "original_form": "야하다고해서",
          "speaker_id": "2",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028601.1.19",
          "form": "신나게틀었는데",
          "original_form": "신나게틀었는데",
          "speaker_id": "2",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028601.1.20",
          "form": "그닥야하진않고",
          "original_form": "그닥야하진않고",
          "speaker_id": "2",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028601.1.21",
          "form": "재미는있었어요ㅋㅋ",
          "original_form": "재미는있었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028601.1.22",
          "form": "제 친구도 야하다고 추천해주더라구요ㅋㅋㅋㅋ",
          "original_form": "제 친구도 야하다고 추천해주더라구요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028601.1.23",
          "form": "나중에 한번 봐야겠네요!!!",
          "original_form": "나중에 한번 봐야겠네요!!!",
          "speaker_id": "1",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028601.1.24",
          "form": "웨이브로 영화도 보시나요??",
          "original_form": "웨이브로 영화도 보시나요??",
          "speaker_id": "1",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028601.1.25",
          "form": "아뇨! 거의 예능만봐요ㅋㅋ",
          "original_form": "아뇨! 거의 예능만봐요ㅋㅋ",
          "speaker_id": "2",
