      "utterance": [
        {
          "id": "MDRW2100011420.1.1",
          "form": "스포츠 좋아하시나요",
          "original_form": "스포츠 좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 12:56"
        },
        {
          "id": "MDRW2100011420.1.2",
          "form": "올림픽같은거 하면 좀보는데요",
          "original_form": "올림픽같은거 하면 좀보는데요",
          "speaker_id": "2",
          "time": "20210825 12:57"
        },
        {
          "id": "MDRW2100011420.1.3",
          "form": "평소에는 야구정도 봐요",
          "original_form": "평소에는 야구정도 봐요",
          "speaker_id": "2",
          "time": "20210825 12:57"
        },
        {
          "id": "MDRW2100011420.1.4",
          "form": "어느구단 응원하시나요",
          "original_form": "어느구단 응원하시나요",
          "speaker_id": "1",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100011420.1.5",
          "form": "기아요",
          "original_form": "기아요",
          "speaker_id": "2",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100011420.1.6",
          "form": "오 기아! 직관도 많이 가셨나요",
          "original_form": "오 기아! 직관도 많이 가셨나요",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100011420.1.7",
          "form": "직관은 잠실로 몇번가봤어요",
          "original_form": "직관은 잠실로 몇번가봤어요",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100011420.1.8",
          "form": "오 맥주 응원 재밌더라구요 코로나 전ㅠ",
          "original_form": "오 맥주 응원 재밌더라구요 코로나 전ㅠ",
          "speaker_id": "1",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100011420.1.9",
          "form": "네 직접보는 재미가 있었져",
          "original_form": "네 직접보는 재미가 있었져",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100011420.1.10",
          "form": "경기 다 챙겨보시나요",
          "original_form": "경기 다 챙겨보시나요",
          "speaker_id": "1",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100011420.1.11",
          "form": "아니요 요새는 코로나도 그렇고 비도오면 안하고 하니까 점점 안보게되더라고요",
          "original_form": "아니요 요새는 코로나도 그렇고 비도오면 안하고 하니까 점점 안보게되더라고요",
          "speaker_id": "2",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100011420.1.12",
          "form": "아 우천취소ㅠ",
          "original_form": "아 우천취소ㅠ",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100011420.1.13",
          "form": "그리고 기아가 못하니까 더보기싫더라고요",
          "original_form": "그리고 기아가 못하니까 더보기싫더라고요",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100011420.1.14",
          "form": "응원하는 팀 있으세여?",
          "original_form": "응원하는 팀 있으세여?",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100011420.1.15",
          "form": "저는 축구좋아해요ㅎㅎ",
          "original_form": "저는 축구좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:27"
        }
      ]
