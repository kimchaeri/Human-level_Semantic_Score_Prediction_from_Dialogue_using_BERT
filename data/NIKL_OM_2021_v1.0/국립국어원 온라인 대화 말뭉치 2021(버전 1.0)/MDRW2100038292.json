      "utterance": [
        {
          "id": "MDRW2100038292.1.1",
          "form": "오잉",
          "original_form": "오잉",
          "speaker_id": "1",
          "time": "20210902 09:58"
        },
        {
          "id": "MDRW2100038292.1.2",
          "form": "장마가 언제 끝날까요",
          "original_form": "장마가 언제 끝날까요",
          "speaker_id": "2",
          "time": "20210902 09:58"
        },
        {
          "id": "MDRW2100038292.1.3",
          "form": "그러게요ㅠ",
          "original_form": "그러게요ㅠ",
          "speaker_id": "1",
          "time": "20210902 09:59"
        },
        {
          "id": "MDRW2100038292.1.4",
          "form": "여름 다 지나가는데 장마라니",
          "original_form": "여름 다 지나가는데 장마라니",
          "speaker_id": "1",
          "time": "20210902 09:59"
        },
        {
          "id": "MDRW2100038292.1.5",
          "form": "오늘 계신곳 날씨는 어때요?",
          "original_form": "오늘 계신곳 날씨는 어때요?",
          "speaker_id": "1",
          "time": "20210902 09:59"
        },
        {
          "id": "MDRW2100038292.1.6",
          "form": "흐리기만 하고 비는 안와요",
          "original_form": "흐리기만 하고 비는 안와요",
          "speaker_id": "2",
          "time": "20210902 10:01"
        },
        {
          "id": "MDRW2100038292.1.7",
          "form": "그쪽은요?",
          "original_form": "그쪽은요?",
          "speaker_id": "2",
          "time": "20210902 10:01"
        },
        {
          "id": "MDRW2100038292.1.8",
          "form": "여기도 비는 안오는데\r\n하늘이 흐리네용",
          "original_form": "여기도 비는 안오는데\r\n하늘이 흐리네용",
          "speaker_id": "1",
          "time": "20210902 10:02"
        },
        {
          "id": "MDRW2100038292.1.9",
          "form": "장마가 쉬이 끝나질 않네요",
          "original_form": "장마가 쉬이 끝나질 않네요",
          "speaker_id": "2",
          "time": "20210902 10:03"
        },
        {
          "id": "MDRW2100038292.1.10",
          "form": "가을 엄청 기다렸는데...",
          "original_form": "가을 엄청 기다렸는데...",
          "speaker_id": "1",
          "time": "20210902 10:04"
        },
        {
          "id": "MDRW2100038292.1.11",
          "form": "올 가을도 짧게 지나가겠어요",
          "original_form": "올 가을도 짧게 지나가겠어요",
          "speaker_id": "1",
          "time": "20210902 10:04"
        },
        {
          "id": "MDRW2100038292.1.12",
          "form": "맞아요... 곧 단풍 지고 겨울이겠죠",
          "original_form": "맞아요... 곧 단풍 지고 겨울이겠죠",
          "speaker_id": "2",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100038292.1.13",
          "form": "어느 계절 좋아하세요?",
          "original_form": "어느 계절 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100038292.1.14",
          "form": "저는 겨울 좋아해요",
          "original_form": "저는 겨울 좋아해요",
          "speaker_id": "2",
          "time": "20210902 10:06"
        },
        {
          "id": "MDRW2100038292.1.15",
          "form": "지금 두근거립니다",
          "original_form": "지금 두근거립니다",
          "speaker_id": "2",
          "time": "20210902 10:06"
        },
        {
          "id": "MDRW2100038292.1.16",
          "form": "오 겨울을 좋아하시는군용",
          "original_form": "오 겨울을 좋아하시는군용",
          "speaker_id": "1",
          "time": "20210902 10:08"
        },
        {
          "id": "MDRW2100038292.1.17",
          "form": "눈을 좋아하시는거에요?",
          "original_form": "눈을 좋아하시는거에요?",
          "speaker_id": "1",
          "time": "20210902 10:08"
        },
        {
          "id": "MDRW2100038292.1.18",
          "form": "맞아요 눈오는거 엄청 좋아해요",
          "original_form": "맞아요 눈오는거 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210902 10:11"
        },
        {
          "id": "MDRW2100038292.1.19",
          "form": "저는 비랑 눈 다 별로 안 좋아해요ㅠ",
          "original_form": "저는 비랑 눈 다 별로 안 좋아해요ㅠ",
          "speaker_id": "1",
          "time": "20210902 10:21"
        },
        {
          "id": "MDRW2100038292.1.20",
          "form": "어릴땐 좋았는데 나이 먹어갈수록 ㅠㅋ",
          "original_form": "어릴땐 좋았는데 나이 먹어갈수록 ㅠㅋ",
          "speaker_id": "1",
          "time": "20210902 10:21"
        },
        {
          "id": "MDRW2100038292.1.21",
          "form": "나갈일이 있으면 힘들죠",
          "original_form": "나갈일이 있으면 힘들죠",
          "speaker_id": "2",
          "time": "20210902 10:23"
        },
        {
          "id": "MDRW2100038292.1.22",
          "form": "하지만 집에 있을 때 보는건 언제나 좋습니다",
          "original_form": "하지만 집에 있을 때 보는건 언제나 좋습니다",
          "speaker_id": "2",
          "time": "20210902 10:23"
        },
        {
          "id": "MDRW2100038292.1.23",
          "form": "네:) 저도 창밖으로\r\n보는건 좋아라합니다",
          "original_form": "네:) 저도 창밖으로\r\n보는건 좋아라합니다{emoji:♡}",
          "speaker_id": "1",
          "time": "20210902 10:27"
        }
      ]
