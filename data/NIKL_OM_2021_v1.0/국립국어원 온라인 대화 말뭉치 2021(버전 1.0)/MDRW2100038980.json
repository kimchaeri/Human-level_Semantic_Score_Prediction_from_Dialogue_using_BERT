        {
          "id": "MDRW2100038980.1.1",
          "form": "치킨 좋아하세요오?",
          "original_form": "치킨 좋아하세요오?",
          "speaker_id": "1",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100038980.1.2",
          "form": "치킨 넘 조아하죠",
          "original_form": "치킨 넘 조아하죠",
          "speaker_id": "2",
          "time": "20210903 16:03"
        },
        {
          "id": "MDRW2100038980.1.3",
          "form": "어제 저녁에두 통닭먹었어용",
          "original_form": "어제 저녁에두 통닭먹었어용",
          "speaker_id": "2",
          "time": "20210903 16:03"
        },
        {
          "id": "MDRW2100038980.1.4",
          "form": "어디거 드셨나여",
          "original_form": "어디거 드셨나여",
          "speaker_id": "1",
          "time": "20210903 16:06"
        },
        {
          "id": "MDRW2100038980.1.5",
          "form": "그 트럭에서 돌아가는 전기구이통닭 아세요?",
          "original_form": "그 트럭에서 돌아가는 전기구이통닭 아세요?",
          "speaker_id": "2",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100038980.1.6",
          "form": "ㅋㅋㅋㅋ그거먹었....",
          "original_form": "ㅋㅋㅋㅋ그거먹었....",
          "speaker_id": "2",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100038980.1.7",
          "form": "헐 미쳤다",
          "original_form": "헐 미쳤다",
          "speaker_id": "1",
          "time": "20210903 16:29"
        },
        {
          "id": "MDRW2100038980.1.8",
          "form": "그거 어때요?",
          "original_form": "그거 어때요?",
          "speaker_id": "1",
          "time": "20210903 16:29"
        },
        {
          "id": "MDRW2100038980.1.9",
          "form": "전 먹어보고만 싶었지 먹은 적은 없거등요",
          "original_form": "전 먹어보고만 싶었지 먹은 적은 없거등요",
          "speaker_id": "1",
          "time": "20210903 16:30"
        },
        {
          "id": "MDRW2100038980.1.10",
          "form": "ㅋㅋ맛있어요! 안에 찹쌀밥 들어있어서 그게 맛나요",
          "original_form": "ㅋㅋ맛있어요! 안에 찹쌀밥 들어있어서 그게 맛나요",
          "speaker_id": "2",
          "time": "20210903 16:33"
        },
        {
          "id": "MDRW2100038980.1.11",
          "form": "ㅋㅋㅋㅋㅋ헐 백숙 같은 느낌인가요..?",
          "original_form": "ㅋㅋㅋㅋㅋ헐 백숙 같은 느낌인가요..?",
          "speaker_id": "1",
          "time": "20210903 17:06"
        },
        {
          "id": "MDRW2100038980.1.12",
          "form": "네네 ㅋㅋㅋ겉이 바삭한 백숙...저는 백숙보다 맛나더라구여",
          "original_form": "네네 ㅋㅋㅋ겉이 바삭한 백숙...저는 백숙보다 맛나더라구여",
          "speaker_id": "2",
          "time": "20210903 17:54"
        },
        {
          "id": "MDRW2100038980.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ와 맛있을 거 같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ와 맛있을 거 같아요",
          "speaker_id": "1",
          "time": "20210903 18:17"
        },
        {
          "id": "MDRW2100038980.1.14",
          "form": "그거 짐짜 먹어보고싶었엉6",
          "original_form": "그거 짐짜 먹어보고싶었엉6",
          "speaker_id": "1",
          "time": "20210903 18:17"
        },
        {
          "id": "MDRW2100038980.1.15",
          "form": "통닭트럭을 만나면 놓치지마시길...",
          "original_form": "통닭트럭을 만나면 놓치지마시길...",
          "speaker_id": "2",
          "time": "20210903 18:32"
        },
        {
          "id": "MDRW2100038980.1.16",
          "form": "요즘은 잘 안보이더라구요 ㅠㅠ",
          "original_form": "요즘은 잘 안보이더라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100038980.1.17",
          "form": "마자여 저도 오랫만에 발견했어요...ㅜㅜ",
          "original_form": "마자여 저도 오랫만에 발견했어요...ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100038980.1.18",
          "form": "신호등통닭 이런 가게도 맛있대요",
          "original_form": "신호등통닭 이런 가게도 맛있대요",
          "speaker_id": "2",
          "time": "20210903 18:44"
        },
