      "utterance": [
        {
          "id": "MDRW2100041748.1.1",
          "form": "그래서 기프티콘은 교촌 많이 받아욬ㅋㅋㅋ",
          "original_form": "그래서 기프티콘은 교촌 많이 받아욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:19"
        },
        {
          "id": "MDRW2100041748.1.2",
          "form": "허니콤보도 맛있지만",
          "original_form": "허니콤보도 맛있지만",
          "speaker_id": "2",
          "time": "20210906 15:19"
        },
        {
          "id": "MDRW2100041748.1.3",
          "form": "저희동넨 암거도 없어서 무조건 배달이라ㄲㅋ",
          "original_form": "저희동넨 암거도 없어서 무조건 배달이라ㄲㅋ",
          "speaker_id": "1",
          "time": "20210906 15:19"
        },
        {
          "id": "MDRW2100041748.1.4",
          "form": "고.추바사삭 만세",
          "original_form": "고.추바사삭 만세",
          "speaker_id": "2",
          "time": "20210906 15:19"
        },
        {
          "id": "MDRW2100041748.1.5",
          "form": "배달비 은근 나오시겠어여",
          "original_form": "배달비 은근 나오시겠어여",
          "speaker_id": "2",
          "time": "20210906 15:20"
        },
        {
          "id": "MDRW2100041748.1.6",
          "form": "안먹어봣는데 이번에 열심히 해서 먹어봐야겠어요",
          "original_form": "안먹어봣는데 이번에 열심히 해서 먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210906 15:20"
        },
        {
          "id": "MDRW2100041748.1.7",
          "form": "ㅋㅋㅋㅋㅋ 네 추천합니다",
          "original_form": "ㅋㅋㅋㅋㅋ 네 추천합니다",
          "speaker_id": "2",
          "time": "20210906 15:20"
        },
        {
          "id": "MDRW2100041748.1.8",
          "form": "저 맵찔인데도 먹어요",
          "original_form": "저 맵찔인데도 먹어요",
          "speaker_id": "2",
          "time": "20210906 15:20"
        },
        {
          "id": "MDRW2100041748.1.9",
          "form": "먼지역이라고 막 배달비 더받져ㅋ",
          "original_form": "먼지역이라고 막 배달비 더받져ㅋ",
          "speaker_id": "1",
          "time": "20210906 15:20"
        },
        {
          "id": "MDRW2100041748.1.10",
          "form": "치사하다!!!!",
          "original_form": "치사하다!!!!",
          "speaker_id": "2",
          "time": "20210906 15:20"
        },
        {
          "id": "MDRW2100041748.1.11",
          "form": "치사하지만 배달의 노예라 시킵니다ㅠ",
          "original_form": "치사하지만 배달의 노예라 시킵니다ㅠ",
          "speaker_id": "1",
          "time": "20210906 15:21"
        },
        {
          "id": "MDRW2100041748.1.12",
          "form": "진짜 택배비 생각하면 배달비는 좀 오바에요ㅋㅋㅋㅋ",
          "original_form": "진짜 택배비 생각하면 배달비는 좀 오바에요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:21"
        },
        {
          "id": "MDRW2100041748.1.13",
          "form": "어쩔수 없긴 하죠 ㅠㅠ",
          "original_form": "어쩔수 없긴 하죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 15:21"
        },
        {
          "id": "MDRW2100041748.1.14",
          "form": "떡볶이는 좋아하세요?",
          "original_form": "떡볶이는 좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 15:21"
        },
        {
          "id": "MDRW2100041748.1.15",
          "form": "미미네 자주먹어요",
          "original_form": "미미네 자주먹어요",
          "speaker_id": "1",
          "time": "20210906 15:27"
        },
        {
          "id": "MDRW2100041748.1.16",
          "form": "미미네 가까우신가봐요 전 냉동 사먹다가 예전맛이 안나서 ㅠㅠ",
          "original_form": "미미네 가까우신가봐요 전 냉동 사먹다가 예전맛이 안나서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 15:27"
        },
        {
          "id": "MDRW2100041748.1.17",
          "form": "저도 냉동이요ㅋㅋ\r\n냉동실에 항상 사놓져",
          "original_form": "저도 냉동이요ㅋㅋ\r\n냉동실에 항상 사놓져",
          "speaker_id": "1",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100041748.1.18",
          "form": "이마트에서 아홉개 묶음이나 샀는데 먹느라 힘들었어요 ㅋㅋㅋ",
          "original_form": "이마트에서 아홉개 묶음이나 샀는데 먹느라 힘들었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100041748.1.19",
          "form": "저는 그 무슨 신월동할머니 였나",
          "original_form": "저는 그 무슨 신월동할머니 였나",
          "speaker_id": "2",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100041748.1.20",
          "form": "거기 떡볶이 맛있다고 사먹었었거든요",
          "original_form": "거기 떡볶이 맛있다고 사먹었었거든요",
          "speaker_id": "2",
          "time": "20210906 15:30"
        },
        {
          "id": "MDRW2100041748.1.21",
          "form": "거기도 맛이 바뀌었더라고요 ㅠㅠ",
          "original_form": "거기도 맛이 바뀌었더라고요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 15:30"
        },
        {
          "id": "MDRW2100041748.1.22",
          "form": "진짜 맛있어서 추천하고 다녔는데 아쉬운거 있죠",
          "original_form": "진짜 맛있어서 추천하고 다녔는데 아쉬운거 있죠",
          "speaker_id": "2",
          "time": "20210906 15:30"
        },
        {
          "id": "MDRW2100041748.1.23",
          "form": "전 이제 하나 남아서 빨리 쟁여야해요",
          "original_form": "전 이제 하나 남아서 빨리 쟁여야해요",
          "speaker_id": "1",
          "time": "20210906 15:33"
        },
        {
          "id": "MDRW2100041748.1.24",
          "form": "맛이 한결같은 곳이 없어서 아쉬워요",
          "original_form": "맛이 한결같은 곳이 없어서 아쉬워요",
          "speaker_id": "1",
          "time": "20210906 15:33"
        },
        {
          "id": "MDRW2100041748.1.25",
          "form": "ㅋㅋㅋ 빨리!! 냉동고가 비면 안돼요",
          "original_form": "ㅋㅋㅋ 빨리!! 냉동고가 비면 안돼요",
          "speaker_id": "2",
          "time": "20210906 15:34"
        },
        {
          "id": "MDRW2100041748.1.26",
          "form": "냉동고는 꽉찬게 에너지 효율이 좋댔어요",
          "original_form": "냉동고는 꽉찬게 에너지 효율이 좋댔어요",
          "speaker_id": "2",
          "time": "20210906 15:34"
        },
        {
          "id": "MDRW2100041748.1.27",
          "form": "이렇게 핑계를 대고 냉동식품을 삽니다",
          "original_form": "이렇게 핑계를 대고 냉동식품을 삽니다",
          "speaker_id": "2",
          "time": "20210906 15:34"
        },
        {
          "id": "MDRW2100041748.1.28",
          "form": "더열심히 쟁여야겠군요ㅋㅋ",
          "original_form": "더열심히 쟁여야겠군요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100041748.1.29",
          "form": "네 저 저얘기 듣고 마음 놨어욬ㅋㅋㅋ",
          "original_form": "네 저 저얘기 듣고 마음 놨어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100041748.1.30",
          "form": "근데 꽉차잇어야 갑작스러움에 대비할수잇어여ㅋㅋ",
          "original_form": "근데 꽉차잇어야 갑작스러움에 대비할수잇어여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:38"
        },
        {
          "id": "MDRW2100041748.1.31",
          "form": "급히 사다 더 비싸게 산다고요",
          "original_form": "급히 사다 더 비싸게 산다고요",
          "speaker_id": "1",
          "time": "20210906 15:38"
        }
      ]
