      "utterance": [
        {
          "id": "MDRW2100007382.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007382.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007382.1.3",
          "form": "흠........  연애 해본적 있으세요?",
          "original_form": "흠........  연애 해본적 있으세요?",
          "speaker_id": "1",
          "time": "20210822 01:50"
        },
        {
          "id": "MDRW2100007382.1.4",
          "form": "연애라...아주 오래 전이네요 ㅠ",
          "original_form": "연애라...아주 오래 전이네요 ㅠ",
          "speaker_id": "2",
          "time": "20210822 01:51"
        },
        {
          "id": "MDRW2100007382.1.5",
          "form": "언제 해봤는지 물어봐도 되나요?",
          "original_form": "언제 해봤는지 물어봐도 되나요?",
          "speaker_id": "1",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007382.1.6",
          "form": "한...3년, 4년 정도 전이요!!",
          "original_form": "한...3년, 4년 정도 전이요!!",
          "speaker_id": "2",
          "time": "20210822 01:53"
        },
        {
          "id": "MDRW2100007382.1.7",
          "form": "한창 대학 새내기였을 때가 마지막이에요",
          "original_form": "한창 대학 새내기였을 때가 마지막이에요",
          "speaker_id": "2",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007382.1.8",
          "form": "그 분이랑 진지하게 결혼에 대해서 생각 해본적은 없겠군요?",
          "original_form": "그 분이랑 진지하게 결혼에 대해서 생각 해본적은 없겠군요?",
          "speaker_id": "1",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100007382.1.9",
          "form": "새내기  때  였으면....",
          "original_form": "새내기  때  였으면....",
          "speaker_id": "1",
          "time": "20210822 01:55"
        },
        {
          "id": "MDRW2100007382.1.10",
          "form": "네네 그쵸",
          "original_form": "네네 그쵸",
          "speaker_id": "2",
          "time": "20210822 01:55"
        },
        {
          "id": "MDRW2100007382.1.11",
          "form": "결혼 생각해본 분이 있으신가요??",
          "original_form": "결혼 생각해본 분이 있으신가요??",
          "speaker_id": "2",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007382.1.12",
          "form": "아직이요",
          "original_form": "아직이요",
          "speaker_id": "1",
          "time": "20210822 01:56"
        },
        {
          "id": "MDRW2100007382.1.13",
          "form": "저는 22살이고 이제 복학생이 들어오는 학년인데 생각보다",
          "original_form": "저는 22살이고 이제 복학생이 들어오는 학년인데 생각보다",
          "speaker_id": "1",
          "time": "20210822 01:57"
        },
        {
          "id": "MDRW2100007382.1.14",
          "form": "주변에 결혼에 진심인 분을 보게 되서",
          "original_form": "주변에 결혼에 진심인 분을 보게 되서",
          "speaker_id": "1",
          "time": "20210822 01:57"
        },
        {
          "id": "MDRW2100007382.1.15",
          "form": "생각하게 되더라구요",
          "original_form": "생각하게 되더라구요",
          "speaker_id": "1",
          "time": "20210822 01:57"
        },
        {
          "id": "MDRW2100007382.1.16",
          "form": "22살인데요??",
          "original_form": "22살인데요??",
          "speaker_id": "2",
          "time": "20210822 01:59"
        },
        {
          "id": "MDRW2100007382.1.17",
          "form": "24살?",
          "original_form": "24살?",
          "speaker_id": "1",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007382.1.18",
          "form": "복학생 이시니까",
          "original_form": "복학생 이시니까",
          "speaker_id": "1",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007382.1.19",
          "form": "아하 그래도 흔치않네요. 요즘엔 결혼 생각하시는 분들이 거의 안 계시던데",
          "original_form": "아하 그래도 흔치않네요. 요즘엔 결혼 생각하시는 분들이 거의 안 계시던데",
          "speaker_id": "2",
          "time": "20210822 02:00"
        },
        {
          "id": "MDRW2100007382.1.20",
          "form": "결혼이 인생의 목표 이시더라구요",
          "original_form": "결혼이 인생의 목표 이시더라구요",
          "speaker_id": "1",
          "time": "20210822 02:01"
        },
        {
          "id": "MDRW2100007382.1.21",
          "form": "그것도 신기하고",
          "original_form": "그것도 신기하고",
          "speaker_id": "1",
          "time": "20210822 02:01"
        },
        {
          "id": "MDRW2100007382.1.22",
          "form": "20대 안에 결혼을 하고 싶어하시기도 하셔서",
          "original_form": "20대 안에 결혼을 하고 싶어하시기도 하셔서",
          "speaker_id": "1",
          "time": "20210822 02:01"
        }
      ]
