      "utterance": [
        {
          "id": "MDRW2100031092.1.1",
          "form": "도시락 싸봤니",
          "original_form": "도시락 싸봤니",
          "speaker_id": "2",
          "time": "20210904 16:41"
        },
        {
          "id": "MDRW2100031092.1.2",
          "form": "카라멜 팝콘 달달하니 맛있도다",
          "original_form": "카라멜 팝콘 달달하니 맛있도다",
          "speaker_id": "1",
          "time": "20210904 16:41"
        },
        {
          "id": "MDRW2100031092.1.3",
          "form": "도시락은 유부초밥이랑 김밥이랑 계란볶음밥밖에 안 만들어봤어",
          "original_form": "도시락은 유부초밥이랑 김밥이랑 계란볶음밥밖에 안 만들어봤어",
          "speaker_id": "1",
          "time": "20210904 16:41"
        },
        {
          "id": "MDRW2100031092.1.4",
          "form": "넌 뭐 싸봤니",
          "original_form": "넌 뭐 싸봤니",
          "speaker_id": "1",
          "time": "20210904 16:41"
        },
        {
          "id": "MDRW2100031092.1.5",
          "form": "그렇구나 난 최근에 산에갈때 싸봤어",
          "original_form": "그렇구나 난 최근에 산에갈때 싸봤어",
          "speaker_id": "2",
          "time": "20210904 16:41"
        },
        {
          "id": "MDRW2100031092.1.6",
          "form": "오 등산했니",
          "original_form": "오 등산했니",
          "speaker_id": "1",
          "time": "20210904 16:41"
        },
        {
          "id": "MDRW2100031092.1.7",
          "form": "뭐 만들었니",
          "original_form": "뭐 만들었니",
          "speaker_id": "1",
          "time": "20210904 16:42"
        },
        {
          "id": "MDRW2100031092.1.8",
          "form": "참깨주먹밥이랑",
          "original_form": "참깨주먹밥이랑",
          "speaker_id": "2",
          "time": "20210904 16:42"
        },
        {
          "id": "MDRW2100031092.1.9",
          "form": "소고기랑",
          "original_form": "소고기랑",
          "speaker_id": "2",
          "time": "20210904 16:42"
        },
        {
          "id": "MDRW2100031092.1.10",
          "form": "계란말이랑 김치볶음",
          "original_form": "계란말이랑 김치볶음",
          "speaker_id": "2",
          "time": "20210904 16:42"
        },
        {
          "id": "MDRW2100031092.1.11",
          "form": "맛나겠다 알차게 싸갔네",
          "original_form": "맛나겠다 알차게 싸갔네",
          "speaker_id": "1",
          "time": "20210904 16:42"
        },
        {
          "id": "MDRW2100031092.1.12",
          "form": "주먹밥 안에 뭐 넣었니?",
          "original_form": "주먹밥 안에 뭐 넣었니?",
          "speaker_id": "1",
          "time": "20210904 16:42"
        },
        {
          "id": "MDRW2100031092.1.13",
          "form": "도시락싸는거 은근 즐겁더라",
          "original_form": "도시락싸는거 은근 즐겁더라",
          "speaker_id": "2",
          "time": "20210904 16:42"
        },
        {
          "id": "MDRW2100031092.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ해피",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ해피",
          "speaker_id": "1",
          "time": "20210904 16:42"
        },
        {
          "id": "MDRW2100031092.1.15",
          "form": "주먹밥에 간만하고 안넣었어",
          "original_form": "주먹밥에 간만하고 안넣었어",
          "speaker_id": "2",
          "time": "20210904 16:42"
        },
        {
          "id": "MDRW2100031092.1.16",
          "form": "밥반찬을 많이 싸서",
          "original_form": "밥반찬을 많이 싸서",
          "speaker_id": "2",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100031092.1.17",
          "form": "소고기 왕창 가져갔니",
          "original_form": "소고기 왕창 가져갔니",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100031092.1.18",
          "form": "도시락 까먹는거 두근두근하지않냐",
          "original_form": "도시락 까먹는거 두근두근하지않냐",
          "speaker_id": "2",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100031092.1.19",
          "form": "소고기 일부러 1인분구워서",
          "original_form": "소고기 일부러 1인분구워서",
          "speaker_id": "2",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100031092.1.20",
          "form": "ㅋㅋㅋㅋㅌㅌ맞아 밖에서 먹으면 더 맛있어",
          "original_form": "ㅋㅋㅋㅋㅌㅌ맞아 밖에서 먹으면 더 맛있어",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100031092.1.21",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100031092.1.22",
          "form": "반은 먹고 반은 싸갔어",
          "original_form": "반은 먹고 반은 싸갔어",
          "speaker_id": "2",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100031092.1.23",
          "form": "두 번 먹고 싶었구나 ㅋㅋㅋㅋ",
          "original_form": "두 번 먹고 싶었구나 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:44"
        },
        {
