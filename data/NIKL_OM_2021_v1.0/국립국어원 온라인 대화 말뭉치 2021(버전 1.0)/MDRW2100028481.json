      "utterance": [
        {
          "id": "MDRW2100028481.1.1",
          "form": "그나저나",
          "original_form": "그나저나",
          "speaker_id": "1",
          "time": "20210902 20:16"
        },
        {
          "id": "MDRW2100028481.1.2",
          "form": "패션 후르츠를",
          "original_form": "패션 후르츠를",
          "speaker_id": "1",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028481.1.3",
          "form": "알아들으셨네",
          "original_form": "알아들으셨네",
          "speaker_id": "1",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028481.1.4",
          "form": "그럼요 알아듣죠 ㅋㅋ",
          "original_form": "그럼요 알아듣죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028481.1.5",
          "form": "뷔페에서 처음 먹고",
          "original_form": "뷔페에서 처음 먹고",
          "speaker_id": "1",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028481.1.6",
          "form": "이런 과일이 있나!",
          "original_form": "이런 과일이 있나!",
          "speaker_id": "1",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028481.1.7",
          "form": "했는데",
          "original_form": "했는데",
          "speaker_id": "1",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028481.1.8",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100028481.1.9",
          "form": "드마리스는 알긴 아는데",
          "original_form": "드마리스는 알긴 아는데",
          "speaker_id": "1",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100028481.1.10",
          "form": "안 가봤어요",
          "original_form": "안 가봤어요",
          "speaker_id": "1",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100028481.1.11",
          "form": "사실 이따금씩 놀리긴 하는데 대리님이랑 저랑 동년배지;;;",
          "original_form": "사실 이따금씩 놀리긴 하는데 대리님이랑 저랑 동년배지;;;",
          "speaker_id": "1",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100028481.1.12",
          "form": "추억의 뷔페이기 때문이지",
          "original_form": "추억의 뷔페이기 때문이지",
          "speaker_id": "2",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100028481.1.13",
          "form": "과장급 이상 소집해서",
          "original_form": "과장급 이상 소집해서",
          "speaker_id": "2",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028481.1.14",
          "form": "버르장머리없는",
          "original_form": "버르장머리없는",
          "speaker_id": "2",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028481.1.15",
          "form": "20대 직원을 타도해야지",
          "original_form": "20대 직원을 타도해야지",
          "speaker_id": "2",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028481.1.16",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028481.1.17",
          "form": "아니 근데",
          "original_form": "아니 근데",
          "speaker_id": "1",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028481.1.18",
          "form": "대리님도 20대로 뜰텐데",
          "original_form": "대리님도 20대로 뜰텐데",
          "speaker_id": "1",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028481.1.19",
          "form": "만나이 판정이라서",
          "original_form": "만나이 판정이라서",
          "speaker_id": "1",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028481.1.20",
          "form": "그런가?",
          "original_form": "그런가?",
          "speaker_id": "2",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028481.1.21",
          "form": "여튼",
          "original_form": "여튼",
          "speaker_id": "1",
          "time": "20210902 20:21"
        },
        {
          "id": "MDRW2100028481.1.22",
          "form": "드마리스는",
          "original_form": "드마리스는",
          "speaker_id": "1",
          "time": "20210902 20:21"
        },
        {
          "id": "MDRW2100028481.1.23",
          "form": "돈 벌면 가야지 했는데",
          "original_form": "돈 벌면 가야지 했는데",
          "speaker_id": "1",
          "time": "20210902 20:21"
        },
        {
          "id": "MDRW2100028481.1.24",
          "form": "정작 돈 버는 나이가 되니까",
          "original_form": "정작 돈 버는 나이가 되니까",
          "speaker_id": "1",
          "time": "20210902 20:21"
        },
        {
          "id": "MDRW2100028481.1.25",
          "form": "사라졌...",
          "original_form": "사라졌...",
          "speaker_id": "1",
          "time": "20210902 20:21"
        },
        {
          "id": "MDRW2100028481.1.26",
          "form": "더 비싼 뷔페 가세요",
          "original_form": "더 비싼 뷔페 가세요",
          "speaker_id": "2",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100028481.1.27",
          "form": "신라",
          "original_form": "신라",
          "speaker_id": "1",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100028481.1.28",
          "form": "호텔",
          "original_form": "호텔",
          "speaker_id": "1",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100028481.1.29",
          "form": "신라호텔이면",
          "original_form": "신라호텔이면",
          "speaker_id": "1",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028481.1.30",
          "form": "유명한 거 아닌가...",
          "original_form": "유명한 거 아닌가...",
          "speaker_id": "1",
