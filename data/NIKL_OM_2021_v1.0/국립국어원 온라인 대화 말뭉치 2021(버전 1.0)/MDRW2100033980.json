      "utterance": [
        {
          "id": "MDRW2100033980.1.1",
          "form": "저녁은 제대로 드세용~ 아까 라떼 드셨잖아요ㅎㅎ",
          "original_form": "저녁은 제대로 드세용~ 아까 라떼 드셨잖아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100033980.1.2",
          "form": "아악 감사합니다",
          "original_form": "아악 감사합니다{emoji:♥}{emoji:♥}",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100033980.1.3",
          "form": "저녁 덮밥 시켰어요",
          "original_form": "저녁 덮밥 시켰어요",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100033980.1.4",
          "form": "조금 성급하게",
          "original_form": "조금 성급하게",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100033980.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100033980.1.6",
          "form": "무슨 덮밥 시키셨나용",
          "original_form": "무슨 덮밥 시키셨나용",
          "speaker_id": "2",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100033980.1.7",
          "form": "저 숯불고기 덮밥",
          "original_form": "저 숯불고기 덮밥",
          "speaker_id": "1",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100033980.1.8",
          "form": "너무 기대되는데",
          "original_form": "너무 기대되는데",
          "speaker_id": "1",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100033980.1.9",
          "form": "왜케 안올까( 나문희 톤)",
          "original_form": "왜케 안올까( 나문희 톤)",
          "speaker_id": "1",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100033980.1.10",
          "form": "헉 고기는 뭐든 맛있죠ㅎㅎ\r\n멋진 저녁식사 하시겠는걸요ㅋㄱㅋ",
          "original_form": "헉 고기는 뭐든 맛있죠ㅎㅎ\r\n멋진 저녁식사 하시겠는걸요ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100033980.1.11",
          "form": "얼른 오면좋겠어요",
          "original_form": "얼른 오면좋겠어요",
          "speaker_id": "2",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100033980.1.12",
          "form": "감삼다 감삼다",
          "original_form": "감삼다 감삼다",
          "speaker_id": "1",
          "time": "20210825 18:06"
        },
        {
          "id": "MDRW2100033980.1.13",
          "form": "저녁 뭐드세요?",
          "original_form": "저녁 뭐드세요?",
          "speaker_id": "1",
          "time": "20210825 18:06"
        },
        {
          "id": "MDRW2100033980.1.14",
          "form": "저는 냉장고 털어서 비빔밥 해먹으려구요ㅋㅋㅋ",
          "original_form": "저는 냉장고 털어서 비빔밥 해먹으려구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:11"
        },
        {
          "id": "MDRW2100033980.1.15",
          "form": "냉장고 파먹기 해먹을때가 됐네욯ㅎ",
          "original_form": "냉장고 파먹기 해먹을때가 됐네욯ㅎ",
          "speaker_id": "2",
          "time": "20210825 18:11"
        },
        {
          "id": "MDRW2100033980.1.16",
          "form": "아 비빔밥 너무 맛있죠",
          "original_form": "아 비빔밥 너무 맛있죠",
          "speaker_id": "1",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100033980.1.17",
          "form": "남 저녁에 침고이네요",
          "original_form": "남 저녁에 침고이네요",
          "speaker_id": "1",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100033980.1.18",
          "form": "비빔밥에 뭘 젤 중요하게 생각하시나요",
          "original_form": "비빔밥에 뭘 젤 중요하게 생각하시나요",
          "speaker_id": "1",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100033980.1.19",
          "form": "오 처음 받아보는 질문이에요ㅎㅎ\r\n전 무조건 양념이요ㅎㅎㅎ",
          "original_form": "오 처음 받아보는 질문이에요ㅎㅎ\r\n전 무조건 양념이요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100033980.1.20",
          "form": "속재료도 중요하지만 전 음식 양념맛으로 먹는 편이거든요!",
          "original_form": "속재료도 중요하지만 전 음식 양념맛으로 먹는 편이거든요!",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100033980.1.21",
          "form": "맞아요 저도요",
          "original_form": "맞아요 저도요",
          "speaker_id": "1",
          "time": "20210825 19:18"
        },
        {
          "id": "MDRW2100033980.1.22",
          "form": "글고 나물의 양(?)과 질",
          "original_form": "글고 나물의 양(?)과 질",
          "speaker_id": "1",
          "time": "20210825 19:18"
        },
        {
          "id": "MDRW2100033980.1.23",
          "form": "고추장 vs 참기름 중에 이건 없으면 안된다 하는 거 있으세요?",
          "original_form": "고추장 vs 참기름 중에 이건 없으면 안된다 하는 거 있으세요?",
          "speaker_id": "1",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100033980.1.24",
          "form": "앜ㅋㅋ맞아요 나물도 중요",
          "original_form": "앜ㅋㅋ맞아요 나물도 중요",
          "speaker_id": "2",
          "time": "20210825 19:22"
        },
        {
          "id": "MDRW2100033980.1.25",
          "form": "저는 고추장,,,",
          "original_form": "저는 고추장,,,",
          "speaker_id": "2",
          "time": "20210825 19:22"
        },
        {
          "id": "MDRW2100033980.1.26",
          "form": "오오오 참기름보다는 고추장이군요???",
          "original_form": "오오오 참기름보다는 고추장이군요???",
          "speaker_id": "1",
          "time": "20210825 19:22"
        },
        {
          "id": "MDRW2100033980.1.27",
          "form": "쓰읍..저는 참기름",
          "original_form": "쓰읍..저는 참기름",
          "speaker_id": "1",
          "time": "20210825 19:22"
        },
        {
          "id": "MDRW2100033980.1.28",
          "form": "나물 뭐 좋아하세여?",
          "original_form": "나물 뭐 좋아하세여?",
          "speaker_id": "1",
          "time": "20210825 19:23"
        },
        {
          "id": "MDRW2100033980.1.29",
          "form": "아하!! 참기름의 고소함이랑 향은 대체재가 없긴하죠ㅜㅠ",
          "original_form": "아하!! 참기름의 고소함이랑 향은 대체재가 없긴하죠ㅜㅠ",
          "speaker_id": "2",
          "time": "20210825 19:23"
        },
        {
          "id": "MDRW2100033980.1.30",
          "form": "전 콩나물 들어간것도 좋고~~",
          "original_form": "전 콩나물 들어간것도 좋고~~",
          "speaker_id": "2",
          "time": "20210825 19:23"
        },
        {
          "id": "MDRW2100033980.1.31",
          "form": "거의 그때그때 있는거 넣는편이긴한데",
          "original_form": "거의 그때그때 있는거 넣는편이긴한데",
          "speaker_id": "2",
          "time": "20210825 19:23"
        },
        {
          "id": "MDRW2100033980.1.32",
          "form": "오오 콩나물 넘 맛나죠",
          "original_form": "오오 콩나물 넘 맛나죠",
          "speaker_id": "1",
          "time": "20210825 19:24"
        },
        {
          "id": "MDRW2100033980.1.33",
          "form": "ㅋㅋㅋㅋ맞아요~",
          "original_form": "ㅋㅋㅋㅋ맞아요~",
