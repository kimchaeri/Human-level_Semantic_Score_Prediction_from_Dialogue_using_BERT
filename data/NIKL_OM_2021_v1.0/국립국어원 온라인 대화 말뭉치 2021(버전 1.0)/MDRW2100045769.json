      "utterance": [
        {
          "id": "MDRW2100045769.1.1",
          "form": "좋아하는 음악 있으신가요?",
          "original_form": "좋아하는 음악 있으신가요?",
          "speaker_id": "2",
          "time": "20210908 06:50"
        },
        {
          "id": "MDRW2100045769.1.2",
          "form": "저는 해외인디 좋아해요",
          "original_form": "저는 해외인디 좋아해요",
          "speaker_id": "1",
          "time": "20210908 06:50"
        },
        {
          "id": "MDRW2100045769.1.3",
          "form": "name2님은 어떤장르 좋아하시나요?",
          "original_form": "&name2&님은 어떤장르 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210908 06:50"
        },
        {
          "id": "MDRW2100045769.1.4",
          "form": "음 저는 음악을 잘 몰라서요ㅋㅋㅠㅠ",
          "original_form": "음 저는 음악을 잘 몰라서요ㅋㅋㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:51"
        },
        {
          "id": "MDRW2100045769.1.5",
          "form": "그냥 차트에 있는 거 들어요!",
          "original_form": "그냥 차트에 있는 거 들어요!",
          "speaker_id": "2",
          "time": "20210908 06:51"
        },
        {
          "id": "MDRW2100045769.1.6",
          "form": "탑백귀이신가요?",
          "original_form": "탑백귀이신가요?",
          "speaker_id": "1",
          "time": "20210908 06:51"
        },
        {
          "id": "MDRW2100045769.1.7",
          "form": "앗ㅋㅋㅋㅋ네 유재석씨 귀입니다",
          "original_form": "앗ㅋㅋㅋㅋ네 유재석씨 귀입니다",
          "speaker_id": "2",
          "time": "20210908 06:52"
        },
        {
          "id": "MDRW2100045769.1.8",
          "form": "영화 좋아하는 거 있으신가요?",
          "original_form": "영화 좋아하는 거 있으신가요?",
          "speaker_id": "2",
          "time": "20210908 06:52"
        },
        {
          "id": "MDRW2100045769.1.9",
          "form": "영화는 안가리고 다 좋아해요",
          "original_form": "영화는 안가리고 다 좋아해요",
          "speaker_id": "1",
          "time": "20210908 06:53"
        },
        {
          "id": "MDRW2100045769.1.10",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210908 06:53"
        },
        {
          "id": "MDRW2100045769.1.11",
          "form": "저도 상영하면 거의 보러가요",
          "original_form": "저도 상영하면 거의 보러가요",
          "speaker_id": "2",
          "time": "20210908 06:53"
        },
        {
          "id": "MDRW2100045769.1.12",
          "form": "작년에 본 것 중엔 소리도 없이가 괜찮더라고요",
          "original_form": "작년에 본 것 중엔 소리도 없이가 괜찮더라고요",
          "speaker_id": "2",
          "time": "20210908 06:54"
        },
        {
          "id": "MDRW2100045769.1.13",
          "form": "강추합니다",
          "original_form": "강추합니다",
          "speaker_id": "2",
          "time": "20210908 06:54"
        },
        {
          "id": "MDRW2100045769.1.14",
          "form": "호불호가 갈리긴 하지만...",
          "original_form": "호불호가 갈리긴 하지만...",
          "speaker_id": "2",
          "time": "20210908 06:54"
        },
        {
          "id": "MDRW2100045769.1.15",
          "form": "한국영화인가요?",
          "original_form": "한국영화인가요?",
          "speaker_id": "1",
          "time": "20210908 06:55"
        },
        {
          "id": "MDRW2100045769.1.16",
          "form": "네네! 감독 입봉작이에요",
          "original_form": "네네! 감독 입봉작이에요",
          "speaker_id": "2",
          "time": "20210908 06:55"
        },
        {
          "id": "MDRW2100045769.1.17",
          "form": "유아인씨 나오는데 연기 정말 좋았습니다",
          "original_form": "유아인씨 나오는데 연기 정말 좋았습니다",
          "speaker_id": "2",
          "time": "20210908 06:55"
        },
        {
          "id": "MDRW2100045769.1.18",
          "form": "오 그렇군요 코로나 터지기 전까지는 개봉영화 거의 다 알았는데 코로나 이후로는 영화에 대한 관심이 많이 떨어졌어요 ㅠ",
          "original_form": "오 그렇군요 코로나 터지기 전까지는 개봉영화 거의 다 알았는데 코로나 이후로는 영화에 대한 관심이 많이 떨어졌어요 ㅠ",
          "speaker_id": "1",
          "time": "20210908 06:56"
        },
        {
          "id": "MDRW2100045769.1.19",
          "form": "최근엔 어떤영화보셨어요?",
          "original_form": "최근엔 어떤영화보셨어요?",
          "speaker_id": "1",
          "time": "20210908 06:57"
        },
        {
          "id": "MDRW2100045769.1.20",
          "form": "모가디슈요!",
          "original_form": "모가디슈요!",
          "speaker_id": "2",
          "time": "20210908 06:57"
        },
        {
          "id": "MDRW2100045769.1.21",
          "form": "그것도 재미있었어요",
          "original_form": "그것도 재미있었어요",
          "speaker_id": "2",
          "time": "20210908 06:57"
        },
        {
          "id": "MDRW2100045769.1.22",
          "form": "저도 모가디슈 봤어요 재밌더라구요",
          "original_form": "저도 모가디슈 봤어요 재밌더라구요",
          "speaker_id": "1",
          "time": "20210908 06:58"
        },
        {
          "id": "MDRW2100045769.1.23",
          "form": "누구랑 보셨어요?",
          "original_form": "누구랑 보셨어요?",
          "speaker_id": "1",
          "time": "20210908 06:58"
        },
        {
          "id": "MDRW2100045769.1.24",
          "form": "영화는 무조건 혼영만 해요",
          "original_form": "영화는 무조건 혼영만 해요",
          "speaker_id": "2",
          "time": "20210908 06:59"
        },
        {
          "id": "MDRW2100045769.1.25",
          "form": "앗 저도요 혼자봤습니다",
          "original_form": "앗 저도요 혼자봤습니다",
          "speaker_id": "1",
          "time": "20210908 06:59"
        }
      ]
    }
  ]
