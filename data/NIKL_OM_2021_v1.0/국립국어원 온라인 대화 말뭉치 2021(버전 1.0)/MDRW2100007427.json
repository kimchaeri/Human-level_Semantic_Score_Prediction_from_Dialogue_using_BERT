      "utterance": [
        {
          "id": "MDRW2100007427.1.1",
          "form": "티비 뭐 보세요",
          "original_form": "티비 뭐 보세요",
          "speaker_id": "1",
          "time": "20210822 02:02"
        },
        {
          "id": "MDRW2100007427.1.2",
          "form": "전 펜트하우스랑 놀토",
          "original_form": "전 펜트하우스랑 놀토",
          "speaker_id": "1",
          "time": "20210822 02:02"
        },
        {
          "id": "MDRW2100007427.1.3",
          "form": "저는 아형이랑 야구요...",
          "original_form": "저는 아형이랑 야구요...",
          "speaker_id": "2",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007427.1.4",
          "form": "이럴거면 이름을 야구로 할걸",
          "original_form": "이럴거면 이름을 야구로 할걸",
          "speaker_id": "2",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007427.1.5",
          "form": "아 야구 정말 좋아하시는군요",
          "original_form": "아 야구 정말 좋아하시는군요",
          "speaker_id": "1",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007427.1.6",
          "form": "거의 매일하니깐 좋으시겠어요 주 4회 하나요?",
          "original_form": "거의 매일하니깐 좋으시겠어요 주 4회 하나요?",
          "speaker_id": "1",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007427.1.7",
          "form": "거의 주 5~6회는 하는거 같아요",
          "original_form": "거의 주 5~6회는 하는거 같아요",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007427.1.8",
          "form": "오늘은 우천취소라 하나 날아갔어요!",
          "original_form": "오늘은 우천취소라 하나 날아갔어요!",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007427.1.9",
          "form": "원래는 드라마도 챙겨보고 했는데",
          "original_form": "원래는 드라마도 챙겨보고 했는데",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007427.1.10",
          "form": "한번 끊기면 흐름 잡기가 힘들어서",
          "original_form": "한번 끊기면 흐름 잡기가 힘들어서",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007427.1.11",
          "form": "예능 위주로 보게 되는거 같아요",
          "original_form": "예능 위주로 보게 되는거 같아요",
          "speaker_id": "2",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007427.1.12",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007427.1.13",
          "form": "요즘엔 진짜 뭐 하나 이어서 보는 것도 힘들죠",
          "original_form": "요즘엔 진짜 뭐 하나 이어서 보는 것도 힘들죠",
          "speaker_id": "1",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007427.1.14",
          "form": "그래서 유튜브 보는 게 더 편할때도 있어요",
          "original_form": "그래서 유튜브 보는 게 더 편할때도 있어요",
          "speaker_id": "1",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007427.1.15",
          "form": "클립으로 짧게 짧게 보는것도",
          "original_form": "클립으로 짧게 짧게 보는것도",
          "speaker_id": "2",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007427.1.16",
          "form": "좋은거 같아요",
          "original_form": "좋은거 같아요",
          "speaker_id": "2",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007427.1.17",
          "form": "맞아요 요점만 집어주니까",
          "original_form": "맞아요 요점만 집어주니까",
          "speaker_id": "1",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007427.1.18",
          "form": "아형은",
          "original_form": "아형은",
          "speaker_id": "1",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007427.1.19",
          "form": "누구 좋아하는 멤버 있으세요?",
          "original_form": "누구 좋아하는 멤버 있으세요?",
          "speaker_id": "1",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007427.1.20",
          "form": "저는 슈퍼주니어를 좋아해서 김희철 때문에 봐요!",
          "original_form": "저는 슈퍼주니어를 좋아해서 김희철 때문에 봐요!",
          "speaker_id": "2",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007427.1.21",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "1",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007427.1.22",
          "form": "슈주는 다 웃기죠ㅋㅋㅋㅋ",
          "original_form": "슈주는 다 웃기죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007427.1.23",
          "form": "저도 에스엠 다 좋아해요",
          "original_form": "저도 에스엠 다 좋아해요",
          "speaker_id": "1",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007427.1.24",
          "form": "아앗 감사합니다",
          "original_form": "아앗 감사합니다",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007427.1.25",
          "form": "에스엠이 확실히 예능쪽에서",
          "original_form": "에스엠이 확실히 예능쪽에서",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007427.1.26",
          "form": "잘 밀어주기도 하고 잘 받는 사람이 있으니까",
          "original_form": "잘 밀어주기도 하고 잘 받는 사람이 있으니까",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007427.1.27",
          "form": "많은거 같아요",
          "original_form": "많은거 같아요",
          "speaker_id": "2",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007427.1.28",
          "form": "맞아요 키도 놀토보면 되게 잘하더라구요",
          "original_form": "맞아요 키도 놀토보면 되게 잘하더라구요",
          "speaker_id": "1",
          "time": "20210822 02:09"
        },
        {
          "id": "MDRW2100007427.1.29",
          "form": "그런 캐릭터인줄 몰랐어요",
          "original_form": "그런 캐릭터인줄 몰랐어요",
          "speaker_id": "1",
          "time": "20210822 02:10"
        },
        {
          "id": "MDRW2100007427.1.30",
          "form": "맞아요ㅋㅋㅋ 키도 킨데 태연이 진짜 의외더라고요",
          "original_form": "맞아요ㅋㅋㅋ 키도 킨데 태연이 진짜 의외더라고요",
          "speaker_id": "2",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007427.1.31",
          "form": "소시도 다 예능 잘하는 거 같아요ㅋㅋㅋㅋ",
          "original_form": "소시도 다 예능 잘하는 거 같아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:11"
        }
      ]
    }
