      "utterance": [
        {
          "id": "MDRW2100020351.1.1",
          "form": "마지막에머라고?",
          "original_form": "마지막에머라고?",
          "speaker_id": "2",
          "time": "20210828 01:54"
        },
        {
          "id": "MDRW2100020351.1.2",
          "form": "까먹었다",
          "original_form": "까먹었다",
          "speaker_id": "1",
          "time": "20210828 01:54"
        },
        {
          "id": "MDRW2100020351.1.3",
          "form": "토스트 만든다고",
          "original_form": "토스트 만든다고",
          "speaker_id": "2",
          "time": "20210828 01:54"
        },
        {
          "id": "MDRW2100020351.1.4",
          "form": "못봣다",
          "original_form": "못봣다",
          "speaker_id": "2",
          "time": "20210828 01:54"
        },
        {
          "id": "MDRW2100020351.1.5",
          "form": "와맛잇겟다",
          "original_form": "와맛잇겟다",
          "speaker_id": "1",
          "time": "20210828 01:54"
        },
        {
          "id": "MDRW2100020351.1.6",
          "form": "말걸지마 안놔",
          "original_form": "말걸지마 안놔",
          "speaker_id": "1",
          "time": "20210828 01:55"
        },
        {
          "id": "MDRW2100020351.1.7",
          "form": "개웃겨",
          "original_form": "개웃겨",
          "speaker_id": "2",
          "time": "20210828 01:55"
        },
        {
          "id": "MDRW2100020351.1.8",
          "form": "난 name3이라는 사람이",
          "original_form": "난 &name3&이라는 사람이",
          "speaker_id": "2",
          "time": "20210828 01:55"
        },
        {
          "id": "MDRW2100020351.1.9",
          "form": "스포츠 좋아하냐고 묻는데",
          "original_form": "스포츠 좋아하냐고 묻는데",
          "speaker_id": "2",
          "time": "20210828 01:55"
        },
        {
          "id": "MDRW2100020351.1.10",
          "form": "닉부터 바사삭 같아서",
          "original_form": "닉부터 바사삭 같아서",
          "speaker_id": "2",
          "time": "20210828 01:55"
        },
        {
          "id": "MDRW2100020351.1.11",
          "form": "대답안하는 중",
          "original_form": "대답안하는 중",
          "speaker_id": "2",
          "time": "20210828 01:55"
        },
        {
          "id": "MDRW2100020351.1.12",
          "form": "저는 도쿄올림픽 봤녜요",
          "original_form": "저는 도쿄올림픽 봤녜요",
          "speaker_id": "1",
          "time": "20210828 01:55"
        },
        {
          "id": "MDRW2100020351.1.13",
          "form": "아웃겨",
          "original_form": "아웃겨",
          "speaker_id": "1",
          "time": "20210828 01:55"
        },
        {
          "id": "MDRW2100020351.1.14",
          "form": "봣다고말하고싳은데",
          "original_form": "봣다고말하고싳은데",
          "speaker_id": "1",
          "time": "20210828 01:55"
        },
        {
          "id": "MDRW2100020351.1.15",
          "form": "오 대박",
          "original_form": "오 대박",
          "speaker_id": "2",
          "time": "20210828 01:55"
        },
        {
          "id": "MDRW2100020351.1.16",
          "form": "대화잇기귀찮아서",
          "original_form": "대화잇기귀찮아서",
          "speaker_id": "1",
          "time": "20210828 01:55"
        },
        {
          "id": "MDRW2100020351.1.17",
          "form": "씹는중",
          "original_form": "씹는중",
          "speaker_id": "1",
          "time": "20210828 01:55"
        },
        {
          "id": "MDRW2100020351.1.18",
          "form": "다른사람은",
          "original_form": "다른사람은",
          "speaker_id": "1",
          "time": "20210828 01:56"
        },
        {
          "id": "MDRW2100020351.1.19",
          "form": "학교다니고있녜요",
          "original_form": "학교다니고있녜요",
          "speaker_id": "1",
          "time": "20210828 01:56"
        },
        {
          "id": "MDRW2100020351.1.20",
          "form": "니눈데 ㅡㅡ",
          "original_form": "니눈데 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210828 01:56"
        },
        {
          "id": "MDRW2100020351.1.21",
          "form": "나또 고민인거",
          "original_form": "나또 고민인거",
          "speaker_id": "1",
          "time": "20210828 01:56"
        },
        {
          "id": "MDRW2100020351.1.22",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210828 01:56"
        },
        {
          "id": "MDRW2100020351.1.23",
          "form": "건방지네",
          "original_form": "건방지네",
          "speaker_id": "2",
          "time": "20210828 01:57"
        },
        {
          "id": "MDRW2100020351.1.24",
          "form": "이분은 ㅇㅇ대 ㅇㅇ학과를 ㅇㅇ년전에 졸업한 분이라고",
          "original_form": "이분은 ㅇㅇ대 ㅇㅇ학과를 ㅇㅇ년전에 졸업한 분이라고",
          "speaker_id": "2",
          "time": "20210828 01:57"
        },
        {
          "id": "MDRW2100020351.1.25",
          "form": "물을 정수기를 들일지 생수 꼬박꼬박 사먹는게나을지",
          "original_form": "물을 정수기를 들일지 생수 꼬박꼬박 사먹는게나을지",
          "speaker_id": "1",
          "time": "20210828 01:57"
        },
        {
          "id": "MDRW2100020351.1.26",
          "form": "어케생각함",
          "original_form": "어케생각함",
          "speaker_id": "1",
          "time": "20210828 01:57"
        },
        {
          "id": "MDRW2100020351.1.27",
          "form": "그건 그때가서 조건 따져보고",
          "original_form": "그건 그때가서 조건 따져보고",
          "speaker_id": "2",
          "time": "20210828 01:57"
        },
        {
          "id": "MDRW2100020351.1.28",
          "form": "결정.",
          "original_form": "결정.",
          "speaker_id": "2",
          "time": "20210828 01:57"
        },
        {
          "id": "MDRW2100020351.1.29",
          "form": "정수기가 더 편하긴 한데",
          "original_form": "정수기가 더 편하긴 한데",
          "speaker_id": "2",
          "time": "20210828 01:57"
        },
        {
          "id": "MDRW2100020351.1.30",
          "form": "오 좋다",
          "original_form": "오 좋다",
          "speaker_id": "1",
          "time": "20210828 01:57"
        },
        {
          "id": "MDRW2100020351.1.31",
          "form": "생수생활도 익숙해서",
          "original_form": "생수생활도 익숙해서",
          "speaker_id": "2",
          "time": "20210828 01:57"
        },
        {
          "id": "MDRW2100020351.1.32",
          "form": "둘 다 좋음 근데 정수기가 더 좋음",
          "original_form": "둘 다 좋음 근데 정수기가 더 좋음",
          "speaker_id": "2",
          "time": "20210828 01:57"
        },
        {
          "id": "MDRW2100020351.1.33",
          "form": "정수기 단점이",
          "original_form": "정수기 단점이",
          "speaker_id": "1",
          "time": "20210828 01:57"
        },
        {
          "id": "MDRW2100020351.1.34",
          "form": "한두달에한번씩",
          "original_form": "한두달에한번씩",
          "speaker_id": "1",
          "time": "20210828 01:57"
        },
        {
          "id": "MDRW2100020351.1.35",
          "form": "관리해주는사람 찾아옴",
          "original_form": "관리해주는사람 찾아옴",
          "speaker_id": "1",
          "time": "20210828 01:58"
        },
        {
          "id": "MDRW2100020351.1.36",
          "form": "스트레스겟죠",
          "original_form": "스트레스겟죠",
          "speaker_id": "1",
          "time": "20210828 01:58"
        },
        {
          "id": "MDRW2100020351.1.37",
          "form": "헐 그건 몰랏네\r\n\r\n..",
          "original_form": "헐 그건 몰랏네\r\n\r\n..",
          "speaker_id": "2",
          "time": "20210828 01:58"
        },
        {
          "id": "MDRW2100020351.1.38",
          "form": "괜찬",
          "original_form": "괜찬",
          "speaker_id": "2",
          "time": "20210828 01:58"
        },
        {
          "id": "MDRW2100020351.1.39",
          "form": "ㄴ제가",
          "original_form": "ㄴ제가",
          "speaker_id": "2",
          "time": "20210828 01:58"
        },
        {
          "id": "MDRW2100020351.1.40",
          "form": "사람 받겟음",
          "original_form": "사람 받겟음",
          "speaker_id": "2",
          "time": "20210828 01:58"
        },
        {
          "id": "MDRW2100020351.1.41",
          "form": "어세요오세요",
          "original_form": "어세요오세요",
          "speaker_id": "2",
          "time": "20210828 01:58"
        },
