      "utterance": [
        {
          "id": "MDRW2100008247.1.1",
          "form": "집안일 하시나여",
          "original_form": "집안일 하시나여",
          "speaker_id": "1",
          "time": "20210822 14:27"
        },
        {
          "id": "MDRW2100008247.1.2",
          "form": "아뇨 집에서 그냥 놀지요",
          "original_form": "아뇨 집에서 그냥 놀지요",
          "speaker_id": "2",
          "time": "20210822 14:36"
        },
        {
          "id": "MDRW2100008247.1.3",
          "form": "저는 청소랑 제가먹은 설거지정도..ㅋㅋ",
          "original_form": "저는 청소랑 제가먹은 설거지정도..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 01:18"
        },
        {
          "id": "MDRW2100008247.1.4",
          "form": "저두 설거지랑 정리까진 하는데 그외는 잘 안하게 되네여ㅋㅋ",
          "original_form": "저두 설거지랑 정리까진 하는데 그외는 잘 안하게 되네여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 09:59"
        },
        {
          "id": "MDRW2100008247.1.5",
          "form": "맞아요 귀찮아죽겠어요ㅋㅋ",
          "original_form": "맞아요 귀찮아죽겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:35"
        },
        {
          "id": "MDRW2100008247.1.6",
          "form": "누군가 해줄때가 제일 좋아요",
          "original_form": "누군가 해줄때가 제일 좋아요",
          "speaker_id": "2",
          "time": "20210823 21:38"
        },
        {
          "id": "MDRW2100008247.1.7",
          "form": "평생 누군가 대신해주면좋겠어요ㅋㅋ",
          "original_form": "평생 누군가 대신해주면좋겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:39"
        },
        {
          "id": "MDRW2100008247.1.8",
          "form": "그럴수가없지만",
          "original_form": "그럴수가없지만",
          "speaker_id": "1",
          "time": "20210823 21:39"
        },
        {
          "id": "MDRW2100008247.1.9",
          "form": "청소도 한번 하면 끝나질 않아요",
          "original_form": "청소도 한번 하면 끝나질 않아요",
          "speaker_id": "2",
          "time": "20210823 21:46"
        },
        {
          "id": "MDRW2100008247.1.10",
          "form": "맞아요 끊임이없어요! 더럽기도더럽고..",
          "original_form": "맞아요 끊임이없어요! 더럽기도더럽고..",
          "speaker_id": "1",
          "time": "20210823 21:47"
        },
        {
          "id": "MDRW2100008247.1.11",
          "form": "더러운거 한번 보이면 계속 신경쓰이구요",
          "original_form": "더러운거 한번 보이면 계속 신경쓰이구요",
          "speaker_id": "2",
          "time": "20210823 21:48"
        },
        {
          "id": "MDRW2100008247.1.12",
          "form": "맞아요 근데 또 제가 비위가좋은편이아니라 청소할때 힘들어요ㅠㅋㅋ",
          "original_form": "맞아요 근데 또 제가 비위가좋은편이아니라 청소할때 힘들어요ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:12"
        },
        {
          "id": "MDRW2100008247.1.13",
          "form": "깨끗하게 유지하는게 좋겠네요",
          "original_form": "깨끗하게 유지하는게 좋겠네요",
          "speaker_id": "2",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100008247.1.14",
          "form": "누가대신해주면 굿인데 그럴수가없죠ㅋㅋ",
          "original_form": "누가대신해주면 굿인데 그럴수가없죠ㅋㅋ",
          "speaker_id": "1",
