      "utterance": [
        {
          "id": "MDRW2100019180.1.1",
          "form": "경쟁전하시나요?",
          "original_form": "경쟁전하시나요?",
          "speaker_id": "1",
          "time": "20210827 15:59"
        },
        {
          "id": "MDRW2100019180.1.2",
          "form": "요즘엔 아예 안해서요ㅋㅋ",
          "original_form": "요즘엔 아예 안해서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:09"
        },
        {
          "id": "MDRW2100019180.1.3",
          "form": "그땐 경쟁전했죠ㅋㅋ",
          "original_form": "그땐 경쟁전했죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:09"
        },
        {
          "id": "MDRW2100019180.1.4",
          "form": "저도 경쟁안돌린지 꽤되었어요ㅋㅋㅋ",
          "original_form": "저도 경쟁안돌린지 꽤되었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:12"
        },
        {
          "id": "MDRW2100019180.1.5",
          "form": "솔플하세요?",
          "original_form": "솔플하세요?",
          "speaker_id": "1",
          "time": "20210827 16:12"
        },
        {
          "id": "MDRW2100019180.1.6",
          "form": "아뇨ㅋㅋ",
          "original_form": "아뇨ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:14"
        },
        {
          "id": "MDRW2100019180.1.7",
          "form": "같이 하는 친구가있었어요ㅋㅋ",
          "original_form": "같이 하는 친구가있었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:14"
        },
        {
          "id": "MDRW2100019180.1.8",
          "form": "몇인팟하시나요?",
          "original_form": "몇인팟하시나요?",
          "speaker_id": "1",
          "time": "20210827 16:14"
        },
        {
          "id": "MDRW2100019180.1.9",
          "form": "전 탱 친구는 딜",
          "original_form": "전 탱 친구는 딜",
          "speaker_id": "2",
          "time": "20210827 16:14"
        },
        {
          "id": "MDRW2100019180.1.10",
          "form": "탱딜듀오 좋네요",
          "original_form": "탱딜듀오 좋네요",
          "speaker_id": "1",
          "time": "20210827 16:14"
        },
        {
          "id": "MDRW2100019180.1.11",
          "form": "ㅋㅋ넵 가끔은 바꿔서하고요",
          "original_form": "ㅋㅋ넵 가끔은 바꿔서하고요",
          "speaker_id": "2",
          "time": "20210827 16:15"
        },
        {
          "id": "MDRW2100019180.1.12",
          "form": "재밌었는데 언제부턴가 질려서요ㅋㅋ",
          "original_form": "재밌었는데 언제부턴가 질려서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:15"
        },
        {
          "id": "MDRW2100019180.1.13",
          "form": "팀원중 꼭 힐러잡는 애가 있어야겠네요ㅋㅋㅋ",
          "original_form": "팀원중 꼭 힐러잡는 애가 있어야겠네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:15"
        },
        {
          "id": "MDRW2100019180.1.14",
          "form": "그죠 요즘 옵치 업뎃도없고...",
          "original_form": "그죠 요즘 옵치 업뎃도없고...",
          "speaker_id": "1",
          "time": "20210827 16:15"
        },
        {
          "id": "MDRW2100019180.1.15",
          "form": "질려요",
          "original_form": "질려요",
          "speaker_id": "1",
          "time": "20210827 16:15"
        },
        {
          "id": "MDRW2100019180.1.16",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "2",
          "time": "20210827 16:15"
        },
        {
          "id": "MDRW2100019180.1.17",
          "form": "신캐는 좀 생겼나요?",
          "original_form": "신캐는 좀 생겼나요?",
          "speaker_id": "2",
          "time": "20210827 16:16"
        },
        {
          "id": "MDRW2100019180.1.18",
          "form": "저 솜브라나올때 관뒀어요ㅋㅋ",
          "original_form": "저 솜브라나올때 관뒀어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:16"
        },
        {
          "id": "MDRW2100019180.1.19",
          "form": "오래전인뎅",
          "original_form": "오래전인뎅",
          "speaker_id": "2",
          "time": "20210827 16:16"
        },
        {
          "id": "MDRW2100019180.1.20",
          "form": "솜브라뒤로 아마...",
          "original_form": "솜브라뒤로 아마...",
          "speaker_id": "1",
          "time": "20210827 16:16"
        },
        {
          "id": "MDRW2100019180.1.21",
          "form": "둠피스트랑 에코",
          "original_form": "둠피스트랑 에코",
          "speaker_id": "1",
          "time": "20210827 16:16"
        },
        {
          "id": "MDRW2100019180.1.22",
          "form": "브리기테도 그뒤에나왔나요?",
          "original_form": "브리기테도 그뒤에나왔나요?",
          "speaker_id": "1",
          "time": "20210827 16:16"
        },
        {
          "id": "MDRW2100019180.1.23",
          "form": "래킹볼 나왔어요",
          "original_form": "래킹볼 나왔어요",
          "speaker_id": "1",
          "time": "20210827 16:16"
        },
        {
          "id": "MDRW2100019180.1.24",
          "form": "많이 나왔네요ㅋㅋ",
          "original_form": "많이 나왔네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:31"
        },
        {
          "id": "MDRW2100019180.1.25",
          "form": "디바도 그동안 너프 많이 먹었죠?",
          "original_form": "디바도 그동안 너프 많이 먹었죠?",
          "speaker_id": "2",
          "time": "20210827 16:31"
        },
        {
          "id": "MDRW2100019180.1.26",
          "form": "네 너프진짜 많이 먹어서 잘안써요",
          "original_form": "네 너프진짜 많이 먹어서 잘안써요",
          "speaker_id": "1",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019180.1.27",
          "form": "요즘엔 필수픽은 어떤 캐인지 아시나요ㅋㄱㅋ",
          "original_form": "요즘엔 필수픽은 어떤 캐인지 아시나요ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019180.1.28",
          "form": "저때는 자리야였는데",
          "original_form": "저때는 자리야였는데",
          "speaker_id": "2",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019180.1.29",
          "form": "지금은 위도우요...",
          "original_form": "지금은 위도우요...",
          "speaker_id": "1",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019180.1.30",
          "form": "적폐 위도우",
          "original_form": "적폐 위도우",
          "speaker_id": "1",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019180.1.31",
          "form": "그리고 방벽캐좋아해요 시그마나 오리사요",
          "original_form": "그리고 방벽캐좋아해요 시그마나 오리사요",
          "speaker_id": "1",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019180.1.32",
          "form": "둘다 솜브라뒤에나온걸 깜박했네요",
          "original_form": "둘다 솜브라뒤에나온걸 깜박했네요",
          "speaker_id": "1",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019180.1.33",
          "form": "오리사",
          "original_form": "오리사",
          "speaker_id": "2",
          "time": "20210827 16:49"
        },
        {
          "id": "MDRW2100019180.1.34",
          "form": "그 눈 ㅡㅡ같이 생긴 캐릭터죠?",
          "original_form": "그 눈 ㅡㅡ같이 생긴 캐릭터죠?",
          "speaker_id": "2",
          "time": "20210827 16:49"
        },
        {
          "id": "MDRW2100019180.1.35",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210827 16:49"
        },
        {
          "id": "MDRW2100019180.1.36",
          "form": "정확하시네",
          "original_form": "정확하시네",
          "speaker_id": "1",
          "time": "20210827 16:49"
        },
        {
          "id": "MDRW2100019180.1.37",
          "form": "눈앞에서 본거 같아요",
          "original_form": "눈앞에서 본거 같아요",
          "speaker_id": "1",
          "time": "20210827 16:50"
        }
      ]
    }
  ]
