      "utterance": [
        {
          "id": "MDRW2100032399.1.1",
          "form": "가족끼리 가볍게 명절음식 먹지",
          "original_form": "가족끼리 가볍게 명절음식 먹지",
          "speaker_id": "2",
          "time": "20210905 16:55"
        },
        {
          "id": "MDRW2100032399.1.2",
          "form": "고향에 가는 거야?",
          "original_form": "고향에 가는 거야?",
          "speaker_id": "1",
          "time": "20210905 16:55"
        },
        {
          "id": "MDRW2100032399.1.3",
          "form": "아니 우리 집에서 모여",
          "original_form": "아니 우리 집에서 모여",
          "speaker_id": "2",
          "time": "20210905 16:55"
        },
        {
          "id": "MDRW2100032399.1.4",
          "form": "어떤 음식 차릴 거야?",
          "original_form": "어떤 음식 차릴 거야?",
          "speaker_id": "1",
          "time": "20210905 16:56"
        },
        {
          "id": "MDRW2100032399.1.5",
          "form": "송편하고 갈비",
          "original_form": "송편하고 갈비",
          "speaker_id": "2",
          "time": "20210905 16:56"
        },
        {
          "id": "MDRW2100032399.1.6",
          "form": "새우찜!",
          "original_form": "새우찜!",
          "speaker_id": "2",
          "time": "20210905 16:56"
        },
        {
          "id": "MDRW2100032399.1.7",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "1",
          "time": "20210905 16:56"
        },
        {
          "id": "MDRW2100032399.1.8",
          "form": "기타 등등",
          "original_form": "기타 등등",
          "speaker_id": "2",
          "time": "20210905 16:56"
        },
        {
          "id": "MDRW2100032399.1.9",
          "form": "떡국은 설날이지",
          "original_form": "떡국은 설날이지",
          "speaker_id": "1",
          "time": "20210905 16:56"
        },
        {
          "id": "MDRW2100032399.1.10",
          "form": "응 추석은 송편!",
          "original_form": "응 추석은 송편!",
          "speaker_id": "2",
          "time": "20210905 16:57"
        },
        {
          "id": "MDRW2100032399.1.11",
          "form": "추석엔 맛있는 게 많아서 좋아",
          "original_form": "추석엔 맛있는 게 많아서 좋아",
          "speaker_id": "1",
          "time": "20210905 16:57"
        },
        {
          "id": "MDRW2100032399.1.12",
          "form": "맞아 음식 차리긴 힘들지만 ㅋㅋㅋ",
          "original_form": "맞아 음식 차리긴 힘들지만 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100032399.1.13",
          "form": "난 올해 요리 안할 거야",
          "original_form": "난 올해 요리 안할 거야",
          "speaker_id": "1",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100032399.1.14",
          "form": "응? 왜?",
          "original_form": "응? 왜?",
          "speaker_id": "2",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100032399.1.15",
          "form": "힘드니까 그냥 사먹으려고",
          "original_form": "힘드니까 그냥 사먹으려고",
          "speaker_id": "1",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100032399.1.16",
          "form": "요즘엔 많이 사먹지",
          "original_form": "요즘엔 많이 사먹지",
          "speaker_id": "2",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100032399.1.17",
          "form": "우리집은 차례상도 안차려서",
          "original_form": "우리집은 차례상도 안차려서",
          "speaker_id": "1",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100032399.1.18",
          "form": "먹고 싶은 것 사먹으면 괜찮지",
          "original_form": "먹고 싶은 것 사먹으면 괜찮지",
          "speaker_id": "2",
          "time": "20210905 17:00"
        },
        {
          "id": "MDRW2100032399.1.19",
          "form": "게장 새우장 전복장 주문했어",
          "original_form": "게장 새우장 전복장 주문했어",
          "speaker_id": "1",
          "time": "20210905 17:00"
        }
      ]
