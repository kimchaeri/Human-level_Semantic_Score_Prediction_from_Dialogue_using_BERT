      "utterance": [
        {
          "id": "MDRW2100017600.1.1",
          "form": "날씨가 선선하군요",
          "original_form": "날씨가 선선하군요",
          "speaker_id": "1",
          "time": "20210826 22:10"
        },
        {
          "id": "MDRW2100017600.1.2",
          "form": "그러네요 바람이 시원헙니다",
          "original_form": "그러네요 바람이 시원헙니다",
          "speaker_id": "2",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100017600.1.3",
          "form": "근데 바람없으면 더움",
          "original_form": "근데 바람없으면 더움",
          "speaker_id": "2",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100017600.1.4",
          "form": "네 약간 습하기도 하고요",
          "original_form": "네 약간 습하기도 하고요",
          "speaker_id": "1",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100017600.1.5",
          "form": "끈적끈적하네요",
          "original_form": "끈적끈적하네요",
          "speaker_id": "2",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100017600.1.6",
          "form": "더 덥게 느껴지는",
          "original_form": "더 덥게 느껴지는",
          "speaker_id": "2",
          "time": "20210826 22:38"
        },
        {
          "id": "MDRW2100017600.1.7",
          "form": "오늘 자면서 선풍기 켜야하나",
          "original_form": "오늘 자면서 선풍기 켜야하나",
          "speaker_id": "1",
          "time": "20210826 22:39"
        },
        {
          "id": "MDRW2100017600.1.8",
          "form": "내일부터 비온다더니 습하네",
          "original_form": "내일부터 비온다더니 습하네",
          "speaker_id": "1",
          "time": "20210826 22:39"
        },
        {
          "id": "MDRW2100017600.1.9",
          "form": "선풍기 틀어야겠다",
          "original_form": "선풍기 틀어야겠다",
          "speaker_id": "2",
          "time": "20210826 22:43"
        },
        {
          "id": "MDRW2100017600.1.10",
          "form": "응 오늘의 날씨에 맞게",
          "original_form": "응 오늘의 날씨에 맞게",
          "speaker_id": "1",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100017600.1.11",
          "form": "오늘은 이불덮고 자면 덥겠어",
          "original_form": "오늘은 이불덮고 자면 덥겠어",
          "speaker_id": "2",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100017600.1.12",
          "form": "네 선풍기 거실에있다",
          "original_form": "네 선풍기 거실에있다",
          "speaker_id": "2",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100017600.1.13",
          "form": "응 창문 열고 선선한 바람 좀 부나 보다가",
          "original_form": "응 창문 열고 선선한 바람 좀 부나 보다가",
          "speaker_id": "1",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100017600.1.14",
          "form": "필요하면 거실서 선풍기 가져다 틀고 자려고",
          "original_form": "필요하면 거실서 선풍기 가져다 틀고 자려고",
          "speaker_id": "1",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100017600.1.15",
          "form": "엉 그래 가을 오는가 싶더니 아직 여름이네",
          "original_form": "엉 그래 가을 오는가 싶더니 아직 여름이네",
          "speaker_id": "2",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100017600.1.16",
          "form": "응 아직은 덥다",
          "original_form": "응 아직은 덥다",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100017600.1.17",
          "form": "오늘 아침에도 날씨가 흐리네",
          "original_form": "오늘 아침에도 날씨가 흐리네",
          "speaker_id": "2",
          "time": "20210827 07:59"
        },
        {
          "id": "MDRW2100017600.1.18",
          "form": "몸이 무겁고만",
          "original_form": "몸이 무겁고만",
          "speaker_id": "2",
          "time": "20210827 07:59"
        },
        {
          "id": "MDRW2100017600.1.19",
          "form": "응 비도 내려",
          "original_form": "응 비도 내려",
          "speaker_id": "1",
          "time": "20210827 08:05"
        },
        {
          "id": "MDRW2100017600.1.20",
          "form": "비오는데 출근하는거 고역이겠어 고생해",
          "original_form": "비오는데 출근하는거 고역이겠어 고생해",
          "speaker_id": "2",
          "time": "20210827 08:07"
        },
        {
          "id": "MDRW2100017600.1.21",
          "form": "우산 챙기고 계절에 맞게 옷도 입어서 괜찮아~",
          "original_form": "우산 챙기고 계절에 맞게 옷도 입어서 괜찮아~",
          "speaker_id": "1",
          "time": "20210827 08:07"
        },
        {
          "id": "MDRW2100017600.1.22",
          "form": "아직 낮엔 덥네",
          "original_form": "아직 낮엔 덥네",
          "speaker_id": "1",
          "time": "20210828 13:35"
        }
