      "utterance": [
        {
          "id": "MDRW2100020002.1.1",
          "form": "ㅎㅎ 맞아요ㅎㅎ",
          "original_form": "ㅎㅎ 맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100020002.1.2",
          "form": "다 유툽에서 동영상보고 따라하고 그러거든요 ㅋㅋ",
          "original_form": "다 유툽에서 동영상보고 따라하고 그러거든요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100020002.1.3",
          "form": "특별한거 없는데ㅎ 다 맛있죠ㅎㅎㅎ",
          "original_form": "특별한거 없는데ㅎ 다 맛있죠ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100020002.1.4",
          "form": "전 특별한게 있어요ㅎ 대기업의 힘을 믿습니다ㅎ 다시마가 제 특별레시피에요ㅎ",
          "original_form": "전 특별한게 있어요ㅎ 대기업의 힘을 믿습니다ㅎ 다시마가 제 특별레시피에요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100020002.1.5",
          "form": "다시다에 저도 편견없어요~ ㅋㅋ 먹는다고 죽는것도아닌데 맛이 최고중요하죠!",
          "original_form": "다시다에 저도 편견없어요~ ㅋㅋ 먹는다고 죽는것도아닌데 맛이 최고중요하죠!",
          "speaker_id": "1",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100020002.1.6",
          "form": "다시다 엄청 좋은 재료로 만드는거에요ㅎ 사탕수수ㅎ",
          "original_form": "다시다 엄청 좋은 재료로 만드는거에요ㅎ 사탕수수ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100020002.1.7",
          "form": "그만한 천연재료가 어딨어요ㅎㅎㅎ",
          "original_form": "그만한 천연재료가 어딨어요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100020002.1.8",
          "form": "맞아여맞아여 ㅋㅋㅋㅋ",
          "original_form": "맞아여맞아여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100020002.1.9",
          "form": "근데저희할머니는 절대 다시다 안쓰겠다며 ㅠ",
          "original_form": "근데저희할머니는 절대 다시다 안쓰겠다며 ㅠ",
          "speaker_id": "1",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100020002.1.10",
          "form": "에이 편견이시네요ㅎ",
          "original_form": "에이 편견이시네요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100020002.1.11",
          "form": "다시다 안쓰시고 미원쓰신거아니셔요?ㅎ",
          "original_form": "다시다 안쓰시고 미원쓰신거아니셔요?ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100020002.1.12",
          "form": "ㅋㅋㅋㅋㅋ 안본새 그럴지도몰라요",
          "original_form": "ㅋㅋㅋㅋㅋ 안본새 그럴지도몰라요",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100020002.1.13",
          "form": "ㅎㅎ 다 넣으셔요ㅎㅎ",
          "original_form": "ㅎㅎ 다 넣으셔요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100020002.1.14",
          "form": "ㅋㅋㅋㅋㅋ 역시...",
          "original_form": "ㅋㅋㅋㅋㅋ 역시...",
          "speaker_id": "1",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100020002.1.15",
          "form": "에고",
          "original_form": "에고",
          "speaker_id": "1",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100020002.1.16",
          "form": "ㅎㅎㅎ요리를 잘하는 방법은 없을까요?ㅎ",
          "original_form": "ㅎㅎㅎ요리를 잘하는 방법은 없을까요?ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100020002.1.17",
          "form": "요리잘하세요?",
          "original_form": "요리잘하세요?",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100020002.1.18",
          "form": "요리잘하는진모르겠지만 좋아해요~",
          "original_form": "요리잘하는진모르겠지만 좋아해요~",
          "speaker_id": "1",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100020002.1.19",
          "form": "잘하시니깐 좋아하는거아닐까요?ㅎ",
          "original_form": "잘하시니깐 좋아하는거아닐까요?ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020002.1.20",
          "form": "자신있는요리는요?",
          "original_form": "자신있는요리는요?",
          "speaker_id": "2",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020002.1.21",
          "form": "앜ㅋㅋㅋㅋ 저는 파스타랑 닭봉조림에대해 은근한 자부심이있어요 ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ 저는 파스타랑 닭봉조림에대해 은근한 자부심이있어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020002.1.22",
          "form": "오오ㅎ 잘하시네여ㅎ",
          "original_form": "오오ㅎ 잘하시네여ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020002.1.23",
          "form": "파스타도 종류가 많은데",
          "original_form": "파스타도 종류가 많은데",
          "speaker_id": "2",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020002.1.24",
          "form": "어렵던데ㅎ 대단하시네요ㅎ",
          "original_form": "어렵던데ㅎ 대단하시네요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020002.1.25",
          "form": "아녜요~ 생각보다 되게 간단해요ㅎㅎ 레시피는 유투브 참조!",
          "original_form": "아녜요~ 생각보다 되게 간단해요ㅎㅎ 레시피는 유투브 참조!",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020002.1.26",
          "form": "유튜브가 요리스승님이시네요ㅎㅎ",
          "original_form": "유튜브가 요리스승님이시네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020002.1.27",
          "form": "예전에저는 만개의 레시피 했었는데 어플이요",
          "original_form": "예전에저는 만개의 레시피 했었는데 어플이요",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020002.1.28",
          "form": "아하! 그거 유투브채널도있어요~ 유툽은 만능",
          "original_form": "아하! 그거 유투브채널도있어요~ 유툽은 만능",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020002.1.29",
          "form": "만개의레시피 유툽채널도있어요~",
          "original_form": "만개의레시피 유툽채널도있어요~",
          "speaker_id": "1",
          "time": "20210827 23:44"
        }
      ]
    }
