      "utterance": [
        {
          "id": "MDRW2100038113.1.1",
          "form": "name1님 야식안드세요?",
          "original_form": "&name1&님 야식안드세요?",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038113.1.2",
          "form": "오 야식",
          "original_form": "오 야식",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100038113.1.3",
          "form": "냉장고 열어볼까요",
          "original_form": "냉장고 열어볼까요",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100038113.1.4",
          "form": "전 멸치먹으니까",
          "original_form": "전 멸치먹으니까",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100038113.1.5",
          "form": "더 맛없는거드세요",
          "original_form": "더 맛없는거드세요",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100038113.1.6",
          "form": "연두부?",
          "original_form": "연두부?",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100038113.1.7",
          "form": "충분히 맛없죠",
          "original_form": "충분히 맛없죠",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100038113.1.8",
          "form": "인정?",
          "original_form": "인정?",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100038113.1.9",
          "form": "맛있겠당",
          "original_form": "맛있겠당",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100038113.1.10",
          "form": "맛난거있네요",
          "original_form": "맛난거있네요",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100038113.1.11",
          "form": "야식은 치킨이나",
          "original_form": "야식은 치킨이나",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038113.1.12",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038113.1.13",
          "form": "뭔가 기름진 이미지 아닙미까",
          "original_form": "뭔가 기름진 이미지 아닙미까",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100038113.1.14",
          "form": "멸치가더모에좋음",
          "original_form": "멸치가더모에좋음",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100038113.1.15",
          "form": "라면은 어떤데",
          "original_form": "라면은 어떤데",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100038113.1.16",
          "form": "안좋지안ㅅ을까여",
          "original_form": "안좋지안ㅅ을까여",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100038113.1.17",
          "form": "하지만 맛있자나여",
          "original_form": "하지만 맛있자나여",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100038113.1.18",
          "form": "맛있으면 다예엿",
          "original_form": "맛있으면 다예엿",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100038113.1.19",
          "form": "담날 후회함",
          "original_form": "담날 후회함",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100038113.1.20",
          "form": "칼로리가 높아야 맛이 있어요",
          "original_form": "칼로리가 높아야 맛이 있어요",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038113.1.21",
          "form": "인생은 한번뿐이구여",
          "original_form": "인생은 한번뿐이구여",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038113.1.22",
          "form": "유병장수하는수가 있어요 님",
          "original_form": "유병장수하는수가 있어요 님",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038113.1.23",
          "form": "맛있는건 영칼로리예여",
          "original_form": "맛있는건 영칼로리예여",
          "speaker_id": "1",
          "time": "20210901 23:30"
        }
      ]
    }
