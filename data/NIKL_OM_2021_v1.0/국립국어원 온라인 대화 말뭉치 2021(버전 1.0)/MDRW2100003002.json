      "utterance": [
        {
          "id": "MDRW2100003002.1.1",
          "form": "잡지 보시나요?",
          "original_form": "잡지 보시나요?",
          "speaker_id": "1",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100003002.1.2",
          "form": "음 치과나 미용실갈때 종종?",
          "original_form": "음 치과나 미용실갈때 종종?",
          "speaker_id": "2",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100003002.1.3",
          "form": "저도 그럴때 잘봐여 ㅋㅋ",
          "original_form": "저도 그럴때 잘봐여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:26"
        },
        {
          "id": "MDRW2100003002.1.4",
          "form": "가끔 보면 재믺더라고요",
          "original_form": "가끔 보면 재믺더라고요",
          "speaker_id": "1",
          "time": "20210820 17:27"
        },
        {
          "id": "MDRW2100003002.1.5",
          "form": "약간 시간떼우기 좋은거같아요",
          "original_form": "약간 시간떼우기 좋은거같아요",
          "speaker_id": "2",
          "time": "20210820 17:27"
        },
        {
          "id": "MDRW2100003002.1.6",
          "form": "맞아용",
          "original_form": "맞아용",
          "speaker_id": "1",
          "time": "20210820 17:27"
        },
        {
          "id": "MDRW2100003002.1.7",
          "form": "평소에는 안보다 보면 재밌더라구요",
          "original_form": "평소에는 안보다 보면 재밌더라구요",
          "speaker_id": "2",
          "time": "20210820 17:27"
        },
        {
          "id": "MDRW2100003002.1.8",
          "form": "책은 읽으세여?",
          "original_form": "책은 읽으세여?",
          "speaker_id": "1",
          "time": "20210820 17:27"
        },
        {
          "id": "MDRW2100003002.1.9",
          "form": "종이책 주로 읽었는데 이북 나오고선 이북봐요",
          "original_form": "종이책 주로 읽었는데 이북 나오고선 이북봐요",
          "speaker_id": "2",
          "time": "20210820 17:27"
        },
        {
          "id": "MDRW2100003002.1.10",
          "form": "오 전 폰으로는 웹툰만 봐요 ..",
          "original_form": "오 전 폰으로는 웹툰만 봐요 ..",
          "speaker_id": "1",
          "time": "20210820 17:28"
        },
        {
          "id": "MDRW2100003002.1.11",
          "form": "책장 넘기는게 더끌리는거같아요",
          "original_form": "책장 넘기는게 더끌리는거같아요",
          "speaker_id": "1",
          "time": "20210820 17:28"
        },
        {
          "id": "MDRW2100003002.1.12",
          "form": "웹툰어떤거 보세요?",
          "original_form": "웹툰어떤거 보세요?",
          "speaker_id": "2",
          "time": "20210820 17:28"
        },
        {
          "id": "MDRW2100003002.1.13",
          "form": "네이버웹툰중에 상위랭킹은 거의다보는거같아요 ㅋㅋㅋ",
          "original_form": "네이버웹툰중에 상위랭킹은 거의다보는거같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100003002.1.14",
          "form": "하루일과에 웹툰보는게 포함이라는..",
          "original_form": "하루일과에 웹툰보는게 포함이라는..",
          "speaker_id": "1",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100003002.1.15",
          "form": "오 그 중 가장 재밌는건 어떤거에요?",
          "original_form": "오 그 중 가장 재밌는건 어떤거에요?",
          "speaker_id": "2",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100003002.1.16",
          "form": "여주실격 재밌어요 추천이뇨",
          "original_form": "여주실격 재밌어요 추천이뇨",
          "speaker_id": "1",
          "time": "20210820 17:30"
        },
        {
          "id": "MDRW2100003002.1.17",
          "form": "맞아요 그거 재밌더라구요",
          "original_form": "맞아요 그거 재밌더라구요",
          "speaker_id": "2",
          "time": "20210820 17:30"
        }
