      "utterance": [
        {
          "id": "MDRW2100005329.1.1",
          "form": "넌 어떤 아르바이트를 해왔었니",
          "original_form": "넌 어떤 아르바이트를 해왔었니",
          "speaker_id": "1",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005329.1.2",
          "form": "이거 할 말 많을거같은데",
          "original_form": "이거 할 말 많을거같은데",
          "speaker_id": "1",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005329.1.3",
          "form": "그럼 나의 일대기에 적당히 호응해주시구려",
          "original_form": "그럼 나의 일대기에 적당히 호응해주시구려",
          "speaker_id": "2",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005329.1.4",
          "form": "일단 스타트는...맘스터치였지",
          "original_form": "일단 스타트는...맘스터치였지",
          "speaker_id": "2",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005329.1.5",
          "form": "맘스터치라 하면 자네도 잘 알고 있을게야",
          "original_form": "맘스터치라 하면 자네도 잘 알고 있을게야",
          "speaker_id": "2",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005329.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맘스터치... 잘 알고있지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맘스터치... 잘 알고있지",
          "speaker_id": "1",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005329.1.7",
          "form": "냅킨을 좀 꺼낸다고",
          "original_form": "냅킨을 좀 꺼낸다고",
          "speaker_id": "1",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005329.1.8",
          "form": "손을 팍 치는",
          "original_form": "손을 팍 치는",
          "speaker_id": "1",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005329.1.9",
          "form": "악질 사장을 만났더랬지...",
          "original_form": "악질 사장을 만났더랬지...",
          "speaker_id": "1",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005329.1.10",
          "form": "맞아, 지독한 사모님이었지",
          "original_form": "맞아, 지독한 사모님이었지",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005329.1.11",
          "form": "그래서 난 이틀인가만에",
          "original_form": "그래서 난 이틀인가만에",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005329.1.12",
          "form": "잠수를 탔어",
          "original_form": "잠수를 탔어",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005329.1.13",
          "form": "그다음이 아마 빨봉분식이었을 거야",
          "original_form": "그다음이 아마 빨봉분식이었을 거야",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005329.1.14",
          "form": "다소 엄하지만 좋은 사장님이었어",
          "original_form": "다소 엄하지만 좋은 사장님이었어",
          "speaker_id": "2",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005329.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ이틀이었구나?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ이틀이었구나?",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005329.1.16",
          "form": "난 하루인줄알았어",
          "original_form": "난 하루인줄알았어",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005329.1.17",
          "form": "하지만 그것도 3일",
          "original_form": "하지만 그것도 3일",
          "speaker_id": "2",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005329.1.18",
          "form": "아아 맞아!",
          "original_form": "아아 맞아!",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005329.1.19",
          "form": "정문에 있던 빨봉!",
          "original_form": "정문에 있던 빨봉!",
          "speaker_id": "1",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005329.1.20",
          "form": "나에겐 운명의 전화가 걸려왔어",
          "original_form": "나에겐 운명의 전화가 걸려왔어",
          "speaker_id": "2",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005329.1.21",
          "form": "맞아, 거기",
          "original_form": "맞아, 거기",
          "speaker_id": "2",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005329.1.22",
          "form": "컴퓨터를 두들길수 있는 일이라길래",
          "original_form": "컴퓨터를 두들길수 있는 일이라길래",
          "speaker_id": "2",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005329.1.23",
          "form": "일하고있던 빨봉과",
          "original_form": "일하고있던 빨봉과",
          "speaker_id": "2",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005329.1.24",
          "form": "일하기로 했던 편의점을 때려치우고",
          "original_form": "일하기로 했던 편의점을 때려치우고",
          "speaker_id": "2",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005329.1.25",
          "form": "탁송(차량운반) 업체에서",
          "original_form": "탁송(차량운반) 업체에서",
          "speaker_id": "2",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005329.1.26",
          "form": "경리업무를 봤지",
          "original_form": "경리업무를 봤지",
          "speaker_id": "2",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005329.1.27",
          "form": "그게 아마 4달인가",
          "original_form": "그게 아마 4달인가",
          "speaker_id": "2",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005329.1.28",
          "form": "아아 맞아맞아 ㅋㅋㅋㅋ",
          "original_form": "아아 맞아맞아 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005329.1.29",
          "form": "너가 말하니까 기억난다",
          "original_form": "너가 말하니까 기억난다",
          "speaker_id": "1",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005329.1.30",
          "form": "너 편의점 야간알바도",
          "original_form": "너 편의점 야간알바도",
          "speaker_id": "1",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005329.1.31",
          "form": "하루인가 이틀인가 했었지아마...??",
          "original_form": "하루인가 이틀인가 했었지아마...??",
          "speaker_id": "1",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005329.1.32",
          "form": "아 거기 4달밖에 안했어?",
          "original_form": "아 거기 4달밖에 안했어?",
          "speaker_id": "1",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005329.1.33",
          "form": "경리 알바 내 체감상",
          "original_form": "경리 알바 내 체감상",
          "speaker_id": "1",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005329.1.34",
          "form": "더 오래 한 기분인데",
          "original_form": "더 오래 한 기분인데",
          "speaker_id": "1",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005329.1.35",
          "form": "으음, 길어도 6개월은 안됐을 거야",
          "original_form": "으음, 길어도 6개월은 안됐을 거야",
          "speaker_id": "2",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005329.1.36",
          "form": "직장 사람이라고 하니 생각나는군",
          "original_form": "직장 사람이라고 하니 생각나는군",
          "speaker_id": "2",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005329.1.37",
          "form": "그 탁송업체 사장님에겐",
          "original_form": "그 탁송업체 사장님에겐",
          "speaker_id": "2",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005329.1.38",
          "form": "여자친구가 있었어",
          "original_form": "여자친구가 있었어",
          "speaker_id": "2",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005329.1.39",
          "form": "사모님도 있는데 말이지 ^^",
          "original_form": "사모님도 있는데 말이지 ^^",
          "speaker_id": "2",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005329.1.40",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005329.1.41",
          "form": "일과...연애...그리고 가정",
          "original_form": "일과...연애...그리고 가정",
          "speaker_id": "1",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005329.1.42",
          "form": "세가지를 다 잡은 사나이었구나",
          "original_form": "세가지를 다 잡은 사나이었구나",
          "speaker_id": "1",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005329.1.43",
          "form": "대단하네",
          "original_form": "대단하네",
          "speaker_id": "1",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005329.1.44",
          "form": "여튼 그 탁송 업체 일을 하다가",
          "original_form": "여튼 그 탁송 업체 일을 하다가",
          "speaker_id": "2",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005329.1.45",
          "form": "진짜 운명의 공고를 보게 됐지",
          "original_form": "진짜 운명의 공고를 보게 됐지",
          "speaker_id": "2",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005329.1.46",
          "form": "지금까지도 갱신되지 않은",
          "original_form": "지금까지도 갱신되지 않은",
          "speaker_id": "2",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005329.1.47",
          "form": "최장 알바 기록",
          "original_form": "최장 알바 기록",
          "speaker_id": "2",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005329.1.48",
          "form": "오...??",
          "original_form": "오...??",
          "speaker_id": "1",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005329.1.49",
          "form": "2년인가 3년인가",
          "original_form": "2년인가 3년인가",
          "speaker_id": "2",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005329.1.50",
          "form": "그 책 알바?재택?",
          "original_form": "그 책 알바?재택?",
          "speaker_id": "1",
          "time": "20210821 22:05"
        },
        {
          "id": "MDRW2100005329.1.51",
          "form": "딩동댕",
          "original_form": "딩동댕",
          "speaker_id": "2",
          "time": "20210821 22:05"
        }
      ]
