      "utterance": [
        {
          "id": "MDRW2100029406.1.1",
          "form": "샐러디에서 자주 사먹습니다 ㅎㅎ",
          "original_form": "샐러디에서 자주 사먹습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 07:59"
        },
        {
          "id": "MDRW2100029406.1.2",
          "form": "샐러디에서 어떤 거",
          "original_form": "샐러디에서 어떤 거",
          "speaker_id": "1",
          "time": "20210903 10:38"
        },
        {
          "id": "MDRW2100029406.1.3",
          "form": "자주 드세여?",
          "original_form": "자주 드세여?",
          "speaker_id": "1",
          "time": "20210903 10:38"
        },
        {
          "id": "MDRW2100029406.1.4",
          "form": "저 맥시칸랩!",
          "original_form": "저 맥시칸랩!",
          "speaker_id": "2",
          "time": "20210903 10:41"
        },
        {
          "id": "MDRW2100029406.1.5",
          "form": "헐 저도 그거 좋아해요ㅋㅋㅋㅋ",
          "original_form": "헐 저도 그거 좋아해요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 13:14"
        },
        {
          "id": "MDRW2100029406.1.6",
          "form": "베이컨칠리웜랩인가",
          "original_form": "베이컨칠리웜랩인가",
          "speaker_id": "1",
          "time": "20210903 13:14"
        },
        {
          "id": "MDRW2100029406.1.7",
          "form": "그걸도 맛있더라고요",
          "original_form": "그걸도 맛있더라고요",
          "speaker_id": "1",
          "time": "20210903 13:14"
        },
        {
          "id": "MDRW2100029406.1.8",
          "form": "아 그거는 2픽입니다",
          "original_form": "아 그거는 2픽입니다",
          "speaker_id": "2",
          "time": "20210903 14:16"
        },
        {
          "id": "MDRW2100029406.1.9",
          "form": "웜볼은 특히",
          "original_form": "웜볼은 특히",
          "speaker_id": "2",
          "time": "20210903 14:16"
        },
        {
          "id": "MDRW2100029406.1.10",
          "form": "멕시칸웜볼이 없어서",
          "original_form": "멕시칸웜볼이 없어서",
          "speaker_id": "2",
          "time": "20210903 14:16"
        },
        {
          "id": "MDRW2100029406.1.11",
          "form": "아니 그냥 넣으면 되는거 아닌거요 사실",
          "original_form": "아니 그냥 넣으면 되는거 아닌거요 사실",
          "speaker_id": "2",
          "time": "20210903 14:16"
        },
        {
          "id": "MDRW2100029406.1.12",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100029406.1.13",
          "form": "그냥 곡물추가하면 되지않나요?!",
          "original_form": "그냥 곡물추가하면 되지않나요?!",
          "speaker_id": "1",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100029406.1.14",
          "form": "근데 안해줘요ㅋㅋㅋㅋㅋ",
          "original_form": "근데 안해줘요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:32"
        },
        {
          "id": "MDRW2100029406.1.15",
          "form": "아니 볼에 멕시칸 재료",
          "original_form": "아니 볼에 멕시칸 재료",
          "speaker_id": "2",
          "time": "20210903 22:32"
        },
        {
          "id": "MDRW2100029406.1.16",
          "form": "넣으면 멕시칸웜볼 아닌가",
          "original_form": "넣으면 멕시칸웜볼 아닌가",
          "speaker_id": "2",
          "time": "20210903 22:32"
        },
        {
          "id": "MDRW2100029406.1.17",
          "form": "왜요????",
          "original_form": "왜요????",
          "speaker_id": "1",
          "time": "20210904 08:53"
        },
        {
          "id": "MDRW2100029406.1.18",
          "form": "매장가니까",
          "original_form": "매장가니까",
          "speaker_id": "1",
          "time": "20210904 08:53"
        },
        {
          "id": "MDRW2100029406.1.19",
          "form": "샐러드랑 웜볼 바꿀수있다던데???",
          "original_form": "샐러드랑 웜볼 바꿀수있다던데???",
          "speaker_id": "1",
          "time": "20210904 08:53"
        },
        {
          "id": "MDRW2100029406.1.20",
          "form": "오잉 그래요?",
          "original_form": "오잉 그래요?",
          "speaker_id": "2",
          "time": "20210904 08:59"
        },
        {
          "id": "MDRW2100029406.1.21",
          "form": "다음에 해봐야지....",
          "original_form": "다음에 해봐야지....",
          "speaker_id": "2",
          "time": "20210904 08:59"
        },
        {
          "id": "MDRW2100029406.1.22",
          "form": "저도 해보진 않았는데",
          "original_form": "저도 해보진 않았는데",
          "speaker_id": "1",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100029406.1.23",
          "form": "제가 가는 매장은 그렇게 써있었어요ㅋㅋ",
          "original_form": "제가 가는 매장은 그렇게 써있었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100029406.1.24",
          "form": "5만 넘게 남아있죠 훗",
          "original_form": "5만 넘게 남아있죠 훗",
          "speaker_id": "2",
          "time": "20210904 14:01"
        },
        {
          "id": "MDRW2100029406.1.25",
          "form": "꼭 다음주에 드셔보세요!!",
          "original_form": "꼭 다음주에 드셔보세요!!",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100029406.1.26",
          "form": "소스는 그냥 기본지정된거로 드세요?",
          "original_form": "소스는 그냥 기본지정된거로 드세요?",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100029406.1.27",
          "form": "네 따로 뭐 해본적이 ㅋㅋ",
          "original_form": "네 따로 뭐 해본적이 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100029406.1.28",
          "form": "걍 있는거 시켜먹어서",
          "original_form": "걍 있는거 시켜먹어서",
          "speaker_id": "2",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100029406.1.29",
          "form": "크리미칠리인가 멕시칸샐러드에 나오능거",
          "original_form": "크리미칠리인가 멕시칸샐러드에 나오능거",
          "speaker_id": "1",
          "time": "20210904 16:59"
        },
        {
          "id": "MDRW2100029406.1.30",
          "form": "맛있는데 뭔가 살짝 너무 느끼해여ㅜ",
          "original_form": "맛있는데 뭔가 살짝 너무 느끼해여ㅜ",
          "speaker_id": "1",
          "time": "20210904 17:00"
        },
        {
          "id": "MDRW2100029406.1.31",
          "form": "ㅋㅋ아 뭔지",
          "original_form": "ㅋㅋ아 뭔지",
          "speaker_id": "2",
          "time": "20210904 17:02"
        },
        {
          "id": "MDRW2100029406.1.32",
          "form": "알거 같군요",
          "original_form": "알거 같군요",
          "speaker_id": "2",
