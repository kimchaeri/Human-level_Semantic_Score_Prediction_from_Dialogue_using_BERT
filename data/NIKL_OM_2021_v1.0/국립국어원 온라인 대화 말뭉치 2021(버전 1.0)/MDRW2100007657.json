      "utterance": [
        {
          "id": "MDRW2100007657.1.1",
          "form": "게임해요?",
          "original_form": "게임해요?",
          "speaker_id": "1",
          "time": "20210822 09:09"
        },
        {
          "id": "MDRW2100007657.1.2",
          "form": "네 저 게임해요ㅋㅋㅋ 쿠키런하고 롤이요",
          "original_form": "네 저 게임해요ㅋㅋㅋ 쿠키런하고 롤이요",
          "speaker_id": "2",
          "time": "20210822 09:10"
        },
        {
          "id": "MDRW2100007657.1.3",
          "form": "롤 어렵죠?",
          "original_form": "롤 어렵죠?",
          "speaker_id": "1",
          "time": "20210822 09:10"
        },
        {
          "id": "MDRW2100007657.1.4",
          "form": "네 롤 엄청 어렵죠 캐릭터가 너무 많아요",
          "original_form": "네 롤 엄청 어렵죠 캐릭터가 너무 많아요",
          "speaker_id": "2",
          "time": "20210822 09:10"
        },
        {
          "id": "MDRW2100007657.1.5",
          "form": "요즘 인기네요",
          "original_form": "요즘 인기네요",
          "speaker_id": "1",
          "time": "20210822 09:11"
        },
        {
          "id": "MDRW2100007657.1.6",
          "form": "혹시 최근에 알파테스트로 나온 슈퍼피플 해보신적 있나요?",
          "original_form": "혹시 최근에 알파테스트로 나온 슈퍼피플 해보신적 있나요?",
          "speaker_id": "2",
          "time": "20210822 09:12"
        },
        {
          "id": "MDRW2100007657.1.7",
          "form": "첨 들어보네여 어떤거요?",
          "original_form": "첨 들어보네여 어떤거요?",
          "speaker_id": "1",
          "time": "20210822 09:12"
        },
        {
          "id": "MDRW2100007657.1.8",
          "form": "배그 같은 건데 레벨업도 있고 궁극기도 있대요",
          "original_form": "배그 같은 건데 레벨업도 있고 궁극기도 있대요",
          "speaker_id": "2",
          "time": "20210822 09:12"
        },
        {
          "id": "MDRW2100007657.1.9",
          "form": "오 잼나겠다",
          "original_form": "오 잼나겠다",
          "speaker_id": "1",
          "time": "20210822 09:13"
        },
        {
          "id": "MDRW2100007657.1.10",
          "form": "그니까요ㅋㅋㅋ 유튜버들 리뷰보는데 빨리 하고 싶네요",
          "original_form": "그니까요ㅋㅋㅋ 유튜버들 리뷰보는데 빨리 하고 싶네요",
          "speaker_id": "2",
          "time": "20210822 09:13"
        },
        {
          "id": "MDRW2100007657.1.11",
          "form": "밤에 하세요?",
          "original_form": "밤에 하세요?",
          "speaker_id": "1",
          "time": "20210822 09:14"
        },
        {
          "id": "MDRW2100007657.1.12",
          "form": "네 저는 주로 밤에 겜하죠 낮에는 수업들어야 하니까ㅎㅎ",
          "original_form": "네 저는 주로 밤에 겜하죠 낮에는 수업들어야 하니까ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 09:14"
        },
        {
          "id": "MDRW2100007657.1.13",
          "form": "ㅎㅎ 맞아요 밤이 한가롭죠",
          "original_form": "ㅎㅎ 맞아요 밤이 한가롭죠",
          "speaker_id": "1",
