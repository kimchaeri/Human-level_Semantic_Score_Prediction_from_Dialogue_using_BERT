          "form": "ㅇㄴ",
          "original_form": "ㅇㄴ",
          "speaker_id": "2",
          "time": "20210821 17:57"
        },
        {
          "id": "MDRW2100004685.1.12",
          "form": "나도 사카린이나 사볼까",
          "original_form": "나도 사카린이나 사볼까",
          "speaker_id": "2",
          "time": "20210821 17:57"
        },
        {
          "id": "MDRW2100004685.1.13",
          "form": "사카린 괜찮음",
          "original_form": "사카린 괜찮음",
          "speaker_id": "1",
          "time": "20210821 17:57"
        },
