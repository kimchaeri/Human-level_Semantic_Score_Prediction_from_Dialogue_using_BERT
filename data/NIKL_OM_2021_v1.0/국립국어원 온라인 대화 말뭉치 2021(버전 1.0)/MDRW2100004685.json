        },
        {
          "id": "MDRW2100004685.1.3",
          "form": "크리스탈라이트",
          "original_form": "크리스탈라이트",
          "speaker_id": "1",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004685.1.4",
          "form": "뭐가",
          "original_form": "뭐가",
          "speaker_id": "2",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004685.1.5",
          "form": "레몬맛 할인하길래",
          "original_form": "레몬맛 할인하길래",
          "speaker_id": "1",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004685.1.6",
          "form": "나도 하나만",
          "original_form": "나도 하나만",
          "speaker_id": "2",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004685.1.7",
          "form": "맛궁금",
          "original_form": "맛궁금",
          "speaker_id": "2",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004685.1.8",
          "form": "사봤는데 복숭아가 나아",
          "original_form": "사봤는데 복숭아가 나아",
          "speaker_id": "1",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004685.1.9",
          "form": "우리집은 엄마가 다 마셔서",
          "original_form": "우리집은 엄마가 다 마셔서",
          "speaker_id": "1",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004685.1.10",
          "form": "새로 사야해",
          "original_form": "새로 사야해",
          "speaker_id": "1",
          "time": "20210821 17:57"
        },
        {
