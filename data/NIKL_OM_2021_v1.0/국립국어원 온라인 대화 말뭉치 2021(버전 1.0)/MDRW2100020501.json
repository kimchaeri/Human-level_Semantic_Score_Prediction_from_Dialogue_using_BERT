      "utterance": [
        {
          "id": "MDRW2100020501.1.1",
          "form": "소고기 저도 좋아해요",
          "original_form": "소고기 저도 좋아해요",
          "speaker_id": "2",
          "time": "20210828 08:32"
        },
        {
          "id": "MDRW2100020501.1.2",
          "form": "어느부위 좋아하시나요",
          "original_form": "어느부위 좋아하시나요",
          "speaker_id": "2",
          "time": "20210828 08:32"
        },
        {
          "id": "MDRW2100020501.1.3",
          "form": "살치살이요",
          "original_form": "살치살이요",
          "speaker_id": "1",
          "time": "20210828 08:32"
        },
        {
          "id": "MDRW2100020501.1.4",
          "form": "어느부위 좋아하세여?",
          "original_form": "어느부위 좋아하세여?",
          "speaker_id": "1",
          "time": "20210828 08:32"
        },
        {
          "id": "MDRW2100020501.1.5",
          "form": "전 등심좋아해요",
          "original_form": "전 등심좋아해요",
          "speaker_id": "2",
          "time": "20210828 08:32"
        },
        {
          "id": "MDRW2100020501.1.6",
          "form": "고기에 기름장 찍어드시나요",
          "original_form": "고기에 기름장 찍어드시나요",
          "speaker_id": "2",
          "time": "20210828 08:32"
        },
        {
          "id": "MDRW2100020501.1.7",
          "form": "살치살은 기름져서 잘 안찍어먹어요",
          "original_form": "살치살은 기름져서 잘 안찍어먹어요",
          "speaker_id": "1",
          "time": "20210828 08:34"
        },
        {
          "id": "MDRW2100020501.1.8",
          "form": "어떤거 찍어먹으시나요",
          "original_form": "어떤거 찍어먹으시나요",
          "speaker_id": "1",
          "time": "20210828 08:34"
        },
        {
          "id": "MDRW2100020501.1.9",
          "form": "그냥 본연의 맛을 즐기시는구나",
          "original_form": "그냥 본연의 맛을 즐기시는구나",
          "speaker_id": "2",
          "time": "20210828 08:34"
        },
        {
          "id": "MDRW2100020501.1.10",
          "form": "전 기름장 찍어먹어요",
          "original_form": "전 기름장 찍어먹어요",
          "speaker_id": "2",
          "time": "20210828 08:34"
        },
        {
          "id": "MDRW2100020501.1.11",
          "form": "돼지고기도 좋아하시나요",
          "original_form": "돼지고기도 좋아하시나요",
          "speaker_id": "2",
          "time": "20210828 08:34"
        },
        {
          "id": "MDRW2100020501.1.12",
          "form": "네 고기류는 가리지 않고 잘먹어요",
          "original_form": "네 고기류는 가리지 않고 잘먹어요",
          "speaker_id": "1",
          "time": "20210828 08:35"
        },
        {
          "id": "MDRW2100020501.1.13",
          "form": "그럼 삼겹살도 좋아하시나요",
          "original_form": "그럼 삼겹살도 좋아하시나요",
          "speaker_id": "2",
          "time": "20210828 08:36"
        },
        {
          "id": "MDRW2100020501.1.14",
          "form": "네 오겹살도 좋아해요",
          "original_form": "네 오겹살도 좋아해요",
          "speaker_id": "1",
          "time": "20210828 08:36"
        },
        {
          "id": "MDRW2100020501.1.15",
          "form": "오겹살 매력있죠",
          "original_form": "오겹살 매력있죠",
          "speaker_id": "2",
          "time": "20210828 08:36"
        },
        {
          "id": "MDRW2100020501.1.16",
          "form": "위에 껍데기라그러나",
          "original_form": "위에 껍데기라그러나",
          "speaker_id": "2",
          "time": "20210828 08:36"
        },
        {
          "id": "MDRW2100020501.1.17",
          "form": "그거 먹는맛이 좋더라구요",
          "original_form": "그거 먹는맛이 좋더라구요",
          "speaker_id": "2",
          "time": "20210828 08:37"
        },
        {
          "id": "MDRW2100020501.1.18",
          "form": "네 전 껍데기만 따로 시켜먹기도 해요",
          "original_form": "네 전 껍데기만 따로 시켜먹기도 해요",
          "speaker_id": "1",
          "time": "20210828 08:37"
        },
        {
          "id": "MDRW2100020501.1.19",
          "form": "껍데기 좋아하시는구나",
          "original_form": "껍데기 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210828 08:37"
        },
        {
          "id": "MDRW2100020501.1.20",
          "form": "그 쫄깃한게 매력인거같아요",
          "original_form": "그 쫄깃한게 매력인거같아요",
          "speaker_id": "2",
          "time": "20210828 08:37"
        },
        {
          "id": "MDRW2100020501.1.21",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210828 08:38"
        },
        {
          "id": "MDRW2100020501.1.22",
          "form": "콩가루 찍어드세요?",
          "original_form": "콩가루 찍어드세요?",
          "speaker_id": "1",
          "time": "20210828 08:38"
        },
        {
          "id": "MDRW2100020501.1.23",
          "form": "네 원래는 그냥 먹었는데 찍어먹으니까 맛있더라구요",
          "original_form": "네 원래는 그냥 먹었는데 찍어먹으니까 맛있더라구요",
          "speaker_id": "2",
          "time": "20210828 08:39"
        }
