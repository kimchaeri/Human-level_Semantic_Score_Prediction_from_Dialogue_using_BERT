      "utterance": [
        {
          "id": "MDRW2100026667.1.1",
          "form": "싸이월드 아시나용?",
          "original_form": "싸이월드 아시나용?",
          "speaker_id": "1",
          "time": "20210902 11:22"
        },
        {
          "id": "MDRW2100026667.1.2",
          "form": "알죠! 싸이월드 오픈 기다리고 계신가요?",
          "original_form": "알죠! 싸이월드 오픈 기다리고 계신가요?",
          "speaker_id": "2",
          "time": "20210902 11:25"
        },
        {
          "id": "MDRW2100026667.1.3",
          "form": "아 오픈하나요!?",
          "original_form": "아 오픈하나요!?",
          "speaker_id": "1",
          "time": "20210902 12:18"
        },
        {
          "id": "MDRW2100026667.1.4",
          "form": "거기 흑역사 참 많았는데",
          "original_form": "거기 흑역사 참 많았는데",
          "speaker_id": "1",
          "time": "20210902 12:18"
        },
        {
          "id": "MDRW2100026667.1.5",
          "form": "한다고하는데 또 감감무소식이네요 ㅎㅎ",
          "original_form": "한다고하는데 또 감감무소식이네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 15:46"
        },
        {
          "id": "MDRW2100026667.1.6",
          "form": "저도 퍼가요 당한 사진들이 많아서 삭제를 목표로 기다리고있어요 ㅎㅎ",
          "original_form": "저도 퍼가요 당한 사진들이 많아서 삭제를 목표로 기다리고있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 15:46"
        },
        {
          "id": "MDRW2100026667.1.7",
          "form": "퍼가요~",
          "original_form": "퍼가요~{emoji:♡}",
          "speaker_id": "1",
          "time": "20210902 15:56"
        },
        {
          "id": "MDRW2100026667.1.8",
          "form": "ㅋㅋㅋㅋ진짜 많이한듯요",
          "original_form": "ㅋㅋㅋㅋ진짜 많이한듯요",
          "speaker_id": "1",
          "time": "20210902 15:57"
        },
        {
          "id": "MDRW2100026667.1.9",
          "form": "이 상한 축전 가져다가 메인올리고ㅋ",
          "original_form": "이 상한 축전 가져다가 메인올리고ㅋ",
          "speaker_id": "1",
          "time": "20210902 15:57"
        },
        {
          "id": "MDRW2100026667.1.10",
          "form": "맞아요 ㅋㅋㅋㅋ 스킨 브금 사느라 도토리 올인하고 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ 스킨 브금 사느라 도토리 올인하고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:03"
        },
        {
          "id": "MDRW2100026667.1.11",
          "form": "개인적으로 마지막 스킨 되게 마음에 들어서 가끔 생각났어요 ㅋㅋㅋㅋ",
          "original_form": "개인적으로 마지막 스킨 되게 마음에 들어서 가끔 생각났어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:03"
        },
        {
          "id": "MDRW2100026667.1.12",
          "form": "맞아요ㅋㅋ진짜ㅋㅋ스킨브금에 돈 쓰고ㅋㅋ쓸데없는짓을",
          "original_form": "맞아요ㅋㅋ진짜ㅋㅋ스킨브금에 돈 쓰고ㅋㅋ쓸데없는짓을",
          "speaker_id": "1",
          "time": "20210902 16:04"
        },
        {
          "id": "MDRW2100026667.1.13",
          "form": "와 그것도 기억하세요? 대단하시네요",
          "original_form": "와 그것도 기억하세요? 대단하시네요",
          "speaker_id": "1",
          "time": "20210902 16:04"
        },
        {
          "id": "MDRW2100026667.1.14",
          "form": "민속화가 디제잉하는 스킨이었는데 너무 마음에 들어서 기억나요 ㅋㅋㅋ",
          "original_form": "민속화가 디제잉하는 스킨이었는데 너무 마음에 들어서 기억나요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100026667.1.15",
          "form": "와우ㅋㅋ그런게 있었나요",
          "original_form": "와우ㅋㅋ그런게 있었나요",
          "speaker_id": "1",
          "time": "20210902 17:56"
        },
        {
          "id": "MDRW2100026667.1.16",
          "form": "저는 캐릭터였던거같아요",
          "original_form": "저는 캐릭터였던거같아요",
          "speaker_id": "1",
          "time": "20210902 17:56"
        },
        {
          "id": "MDRW2100026667.1.17",
          "form": "스킨도 다시 돌려주려나요 ㅎㅎ",
          "original_form": "스킨도 다시 돌려주려나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:02"
        },
        {
          "id": "MDRW2100026667.1.18",
          "form": "사진만 남기고 안 돌려줄 것 같아요 ㅎㅎ",
          "original_form": "사진만 남기고 안 돌려줄 것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 18:02"
        },
        {
          "id": "MDRW2100026667.1.19",
          "form": "그럴것같아요",
          "original_form": "그럴것같아요",
          "speaker_id": "1",
          "time": "20210902 19:35"
        },
        {
          "id": "MDRW2100026667.1.20",
          "form": "그때도 그게 유효기간이있었어서",
          "original_form": "그때도 그게 유효기간이있었어서",
          "speaker_id": "1",
          "time": "20210902 19:35"
        },
        {
          "id": "MDRW2100026667.1.21",
          "form": "짧은거만샀던",
          "original_form": "짧은거만샀던",
          "speaker_id": "1",
          "time": "20210902 19:35"
        },
        {
          "id": "MDRW2100026667.1.22",
          "form": "아 기간이 있었던 것도 같아요!",
          "original_form": "아 기간이 있었던 것도 같아요!",
          "speaker_id": "2",
          "time": "20210902 19:41"
        },
        {
          "id": "MDRW2100026667.1.23",
          "form": "오 기억력 좋으시네요 ㅎㅎ",
          "original_form": "오 기억력 좋으시네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 19:41"
        },
        {
          "id": "MDRW2100026667.1.24",
          "form": "ㅋㅋ아니 갑자기 생각하려니까 떠오르네요",
          "original_form": "ㅋㅋ아니 갑자기 생각하려니까 떠오르네요",
