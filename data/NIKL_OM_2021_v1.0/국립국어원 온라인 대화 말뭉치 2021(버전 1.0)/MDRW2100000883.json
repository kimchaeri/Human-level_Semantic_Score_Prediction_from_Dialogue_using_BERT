      "utterance": [
        {
          "id": "MDRW2100000883.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210819 20:19"
        },
        {
          "id": "MDRW2100000883.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210819 20:20"
        },
        {
          "id": "MDRW2100000883.1.3",
          "form": "아직도 더운거같아요 혹시 시원하신가요",
          "original_form": "아직도 더운거같아요 혹시 시원하신가요",
          "speaker_id": "2",
          "time": "20210819 20:20"
        },
        {
          "id": "MDRW2100000883.1.4",
          "form": "계속 선선하다가 오늘 되게 더웠어요.",
          "original_form": "계속 선선하다가 오늘 되게 더웠어요.",
          "speaker_id": "1",
          "time": "20210819 20:20"
        },
        {
          "id": "MDRW2100000883.1.5",
          "form": "아 혹시 그럼 최근 에어컨 안트셨나요",
          "original_form": "아 혹시 그럼 최근 에어컨 안트셨나요",
          "speaker_id": "2",
          "time": "20210819 20:20"
        },
        {
          "id": "MDRW2100000883.1.6",
          "form": "여태 저흰 켜놨어요",
          "original_form": "여태 저흰 켜놨어요",
          "speaker_id": "2",
          "time": "20210819 20:20"
        },
        {
          "id": "MDRW2100000883.1.7",
          "form": "네 한 일주일 전쯤까진 켰는데 고층이라 그런지 바람이 많이 불어서 요새는 끄고 있어요",
          "original_form": "네 한 일주일 전쯤까진 켰는데 고층이라 그런지 바람이 많이 불어서 요새는 끄고 있어요",
          "speaker_id": "1",
          "time": "20210819 20:21"
        },
        {
          "id": "MDRW2100000883.1.8",
          "form": "전 아직.덥던대",
          "original_form": "전 아직.덥던대",
          "speaker_id": "2",
          "time": "20210819 20:26"
        },
        {
          "id": "MDRW2100000883.1.9",
          "form": "제가 이상한건가요",
          "original_form": "제가 이상한건가요",
          "speaker_id": "2",
          "time": "20210819 20:26"
        },
        {
          "id": "MDRW2100000883.1.10",
          "form": "아뇨ㅋㅋ그럴 수 있죠!",
          "original_form": "아뇨ㅋㅋ그럴 수 있죠!",
          "speaker_id": "1",
          "time": "20210819 20:26"
        },
        {
          "id": "MDRW2100000883.1.11",
          "form": "지역따라서도 다르지 않나요?",
          "original_form": "지역따라서도 다르지 않나요?",
          "speaker_id": "1",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100000883.1.12",
          "form": "햇빛 때문에 덥더라구요",
          "original_form": "햇빛 때문에 덥더라구요",
          "speaker_id": "1",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100000883.1.13",
          "form": "조금만 움직여도 집에서",
          "original_form": "조금만 움직여도 집에서",
          "speaker_id": "2",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000883.1.14",
          "form": "땀이 주르륵 흘러요",
          "original_form": "땀이 주르륵 흘러요",
          "speaker_id": "2",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000883.1.15",
          "form": "9월되면 그럴일이 없겠죠...?",
          "original_form": "9월되면 그럴일이 없겠죠...?",
          "speaker_id": "2",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000883.1.16",
          "form": "그랬으면 좋겠어요!",
          "original_form": "그랬으면 좋겠어요!",
          "speaker_id": "1",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000883.1.17",
          "form": "요즘 홈트하고 싶어도 더우니까 못하겠더라구요",
          "original_form": "요즘 홈트하고 싶어도 더우니까 못하겠더라구요",
          "speaker_id": "1",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000883.1.18",
          "form": "다음주 26_27도던대",
          "original_form": "다음주 26_27도던대",
          "speaker_id": "2",
          "time": "20210819 20:36"
        },
        {
          "id": "MDRW2100000883.1.19",
          "form": "이 온도가 시원한편인가요 그래도 더운날씨인거맞죠",
          "original_form": "이 온도가 시원한편인가요 그래도 더운날씨인거맞죠",
          "speaker_id": "2",
          "time": "20210819 20:36"
        },
        {
          "id": "MDRW2100000883.1.20",
          "form": "솔직히 잘 안 믿겨요! 햇빛이 너무 쎄서 온도 많이 낮아졌는데도 덥네요",
          "original_form": "솔직히 잘 안 믿겨요! 햇빛이 너무 쎄서 온도 많이 낮아졌는데도 덥네요",
          "speaker_id": "1",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000883.1.21",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000883.1.22",
          "form": "아직은 덥죠",
          "original_form": "아직은 덥죠",
          "speaker_id": "2",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000883.1.23",
          "form": "조금만 움직여도 엄청나게 더워진거같아요",
          "original_form": "조금만 움직여도 엄청나게 더워진거같아요",
          "speaker_id": "2",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000883.1.24",
          "form": "이번 여름 진짜 너무너무 더워요",
          "original_form": "이번 여름 진짜 너무너무 더워요",
          "speaker_id": "1",
          "time": "20210819 20:40"
        }
      ]
