      "utterance": [
        {
          "id": "MDRW2100009895.1.1",
          "form": "점심 머드실껀가요",
          "original_form": "점심 머드실껀가요",
          "speaker_id": "2",
          "time": "20210823 11:25"
        },
        {
          "id": "MDRW2100009895.1.2",
          "form": "짬뽕 먹으려구요호 뭐드시나요?",
          "original_form": "짬뽕 먹으려구요호 뭐드시나요?",
          "speaker_id": "1",
          "time": "20210823 11:25"
        },
        {
          "id": "MDRW2100009895.1.3",
          "form": "저는.. 도시락 싸주시는데 안열어봤어요",
          "original_form": "저는.. 도시락 싸주시는데 안열어봤어요",
          "speaker_id": "2",
          "time": "20210823 11:26"
        },
        {
          "id": "MDRW2100009895.1.4",
          "form": "배고프네요",
          "original_form": "배고프네요",
          "speaker_id": "2",
          "time": "20210823 11:27"
        },
        {
          "id": "MDRW2100009895.1.5",
          "form": "저도 아침안먹어서 얼른 먹고싶네요ㅎㅎ",
          "original_form": "저도 아침안먹어서 얼른 먹고싶네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 11:27"
        },
        {
          "id": "MDRW2100009895.1.6",
          "form": "공유키친에서 돌아가면서 당번하거든요",
          "original_form": "공유키친에서 돌아가면서 당번하거든요",
          "speaker_id": "2",
          "time": "20210823 11:28"
        },
        {
          "id": "MDRW2100009895.1.7",
          "form": "머가 나오는지 몰라요 저도 아침걸러서 배고프네요",
          "original_form": "머가 나오는지 몰라요 저도 아침걸러서 배고프네요",
          "speaker_id": "2",
          "time": "20210823 11:29"
        },
        {
          "id": "MDRW2100009895.1.8",
          "form": "우와 신기하네요ㅎㅎ 그냥 서로 먹는건 봤는데 도시락은 신기해요",
          "original_form": "우와 신기하네요ㅎㅎ 그냥 서로 먹는건 봤는데 도시락은 신기해요",
          "speaker_id": "1",
          "time": "20210823 11:29"
        },
        {
          "id": "MDRW2100009895.1.9",
          "form": "코로나라서 요리하고",
          "original_form": "코로나라서 요리하고",
          "speaker_id": "2",
          "time": "20210823 11:30"
        },
        {
          "id": "MDRW2100009895.1.10",
          "form": "도시락에 담아줘요",
          "original_form": "도시락에 담아줘요",
          "speaker_id": "2",
          "time": "20210823 11:30"
        },
        {
          "id": "MDRW2100009895.1.11",
          "form": "따로먹지요",
          "original_form": "따로먹지요",
          "speaker_id": "2",
          "time": "20210823 11:30"
        },
        {
          "id": "MDRW2100009895.1.12",
          "form": "아 그렇구나ㅎㅎ재밌을거같아요 뭐먹을지 고민도 안되구",
          "original_form": "아 그렇구나ㅎㅎ재밌을거같아요 뭐먹을지 고민도 안되구",
          "speaker_id": "1",
          "time": "20210823 11:30"
        },
        {
          "id": "MDRW2100009895.1.13",
          "form": "돈도 적게들구 좋아요",
          "original_form": "돈도 적게들구 좋아요",
          "speaker_id": "2",
          "time": "20210823 11:31"
        },
        {
          "id": "MDRW2100009895.1.14",
          "form": "메뉴고민안하는게 제일 부럽네요ㅠㅠㅋㅋ",
          "original_form": "메뉴고민안하는게 제일 부럽네요ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 11:31"
        },
        {
          "id": "MDRW2100009895.1.15",
          "form": "건강하게 먹을수있는거같아요",
          "original_form": "건강하게 먹을수있는거같아요",
          "speaker_id": "2",
          "time": "20210823 11:32"
        },
        {
          "id": "MDRW2100009895.1.16",
          "form": "메뉴고민 힘들죠",
          "original_form": "메뉴고민 힘들죠",
          "speaker_id": "2",
          "time": "20210823 11:32"
        },
        {
          "id": "MDRW2100009895.1.17",
          "form": "맞아요...일이에요 완전ㅎㅎ",
          "original_form": "맞아요...일이에요 완전ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 11:32"
        },
        {
          "id": "MDRW2100009895.1.18",
          "form": "저는 매일 쌀국수만 먹게될거같은..ㅎ",
          "original_form": "저는 매일 쌀국수만 먹게될거같은..ㅎ",
          "speaker_id": "2",
          "time": "20210823 11:32"
        },
        {
          "id": "MDRW2100009895.1.19",
          "form": "급식이 부러운....ㅠㅠ",
          "original_form": "급식이 부러운....ㅠㅠ",
