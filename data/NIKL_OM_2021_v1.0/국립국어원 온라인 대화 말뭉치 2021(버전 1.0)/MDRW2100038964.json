        },
        {
          "id": "MDRW2100038964.1.2",
          "form": "아니오 ㅋㅋ",
          "original_form": "아니오 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100038964.1.3",
          "form": "name1님은 하고계신가요?",
          "original_form": "&name1&님은 하고계신가요?",
          "speaker_id": "2",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100038964.1.4",
          "form": "전 입다이어터예요",
          "original_form": "전 입다이어터예요",
          "speaker_id": "1",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100038964.1.5",
          "form": "맨날 음식을 못참아요",
          "original_form": "맨날 음식을 못참아요",
          "speaker_id": "1",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100038964.1.6",
          "form": "그럼 기준을 좀 낮추세요~ 너무 거듭된 실패의 경험은 좋지 않아요",
          "original_form": "그럼 기준을 좀 낮추세요~ 너무 거듭된 실패의 경험은 좋지 않아요",
          "speaker_id": "2",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100038964.1.7",
          "form": "네ㅜㅜ 운동은하세요?",
          "original_form": "네ㅜㅜ 운동은하세요?",
          "speaker_id": "1",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100038964.1.8",
          "form": "스트레칭만 간신히 해요 이게 참 애매하네요",
          "original_form": "스트레칭만 간신히 해요 이게 참 애매하네요",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100038964.1.9",
          "form": "스트레칭도 꾸준히하면 좋죠",
          "original_form": "스트레칭도 꾸준히하면 좋죠",
          "speaker_id": "1",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100038964.1.10",
          "form": "뭐든 안하는 것보단 좋으려니하구 조금씩 합니다 ㅠㅠㅋㅋ",
          "original_form": "뭐든 안하는 것보단 좋으려니하구 조금씩 합니다 ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100038964.1.11",
          "form": "저는 동네걷기만해요",
          "original_form": "저는 동네걷기만해요",
          "speaker_id": "1",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100038964.1.12",
          "form": "하루몇분 하세요?",
          "original_form": "하루몇분 하세요?",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100038964.1.13",
          "form": "사십분에서 한시간정도요",
          "original_form": "사십분에서 한시간정도요",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100038964.1.14",
          "form": "name2님은 스트레칭 얼마나하세요?",
          "original_form": "&name2&님은 스트레칭 얼마나하세요?",
          "speaker_id": "1",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100038964.1.15",
          "form": "하루에 20분정도요 건너듸는 날도 있어요",
          "original_form": "하루에 20분정도요 건너듸는 날도 있어요",
