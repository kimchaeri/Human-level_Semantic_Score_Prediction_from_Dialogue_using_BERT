      "utterance": [
        {
          "id": "MDRW2100021824.1.1",
          "form": "마스크쓰니 눈 성형하고 싶은 마음도 가끔 생기네요 ㅋ",
          "original_form": "마스크쓰니 눈 성형하고 싶은 마음도 가끔 생기네요 ㅋ",
          "speaker_id": "2",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100021824.1.2",
          "form": "아 진짜 요즘 많이 하는거같아요",
          "original_form": "아 진짜 요즘 많이 하는거같아요",
          "speaker_id": "1",
          "time": "20210828 22:57"
        },
        {
          "id": "MDRW2100021824.1.3",
          "form": "전 속눈썹펌할까해요",
          "original_form": "전 속눈썹펌할까해요",
          "speaker_id": "1",
          "time": "20210828 22:57"
        },
        {
          "id": "MDRW2100021824.1.4",
          "form": "저 속쌍커플인데.가끔 한쪽이 비율이 안맞아서  스트레스",
          "original_form": "저 속쌍커플인데.가끔 한쪽이 비율이 안맞아서  스트레스",
          "speaker_id": "2",
          "time": "20210828 22:57"
        },
        {
          "id": "MDRW2100021824.1.5",
          "form": "전 아예 무쌍ㅋㅋ",
          "original_form": "전 아예 무쌍ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:57"
        },
        {
          "id": "MDRW2100021824.1.6",
          "form": "마기꾼들 많아요",
          "original_form": "마기꾼들 많아요",
          "speaker_id": "1",
          "time": "20210828 22:58"
        },
        {
          "id": "MDRW2100021824.1.7",
          "form": "ㅎㅎㅎ 마기꾼 !!!!",
          "original_form": "ㅎㅎㅎ 마기꾼 !!!!",
          "speaker_id": "2",
          "time": "20210828 22:58"
        },
        {
          "id": "MDRW2100021824.1.8",
          "form": "눈썹펌 하면  예쁘던데",
          "original_form": "눈썹펌 하면  예쁘던데",
          "speaker_id": "2",
          "time": "20210828 22:58"
        },
        {
          "id": "MDRW2100021824.1.9",
          "form": "그러니까요 자동으로 쌍커풀도 생겨요ㅎㅎ",
          "original_form": "그러니까요 자동으로 쌍커풀도 생겨요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 23:02"
        },
        {
          "id": "MDRW2100021824.1.10",
          "form": "앜 저도 그럼 한번 해봐야 겠네요 ㅎㅎ",
          "original_form": "앜 저도 그럼 한번 해봐야 겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 23:02"
        },
        {
          "id": "MDRW2100021824.1.11",
          "form": "넵 부담없이ㅋㅋ",
          "original_form": "넵 부담없이ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:02"
        },
        {
          "id": "MDRW2100021824.1.12",
          "form": "셀프로도 많이 하더라고요",
          "original_form": "셀프로도 많이 하더라고요",
          "speaker_id": "1",
          "time": "20210828 23:03"
        },
        {
          "id": "MDRW2100021824.1.13",
          "form": "저는 똥손 ㅜㅜ",
          "original_form": "저는 똥손 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 23:03"
        },
        {
          "id": "MDRW2100021824.1.14",
          "form": "사실 저도!!! ㅋㅋ 돈주고 해야져ㅋㅋ",
          "original_form": "사실 저도!!! ㅋㅋ 돈주고 해야져ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:05"
        },
        {
          "id": "MDRW2100021824.1.15",
          "form": "ㅠㅠ 망하면  더손해라서 ㅋㅋ",
          "original_form": "ㅠㅠ 망하면  더손해라서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:06"
        },
        {
          "id": "MDRW2100021824.1.16",
          "form": "그렇네요ㅋㅋ 미련없이!!",
          "original_form": "그렇네요ㅋㅋ 미련없이!!",
          "speaker_id": "1",
          "time": "20210828 23:07"
        },
        {
          "id": "MDRW2100021824.1.17",
          "form": "회사동생은 몇번 샵가더니 자기가 셀프로",
          "original_form": "회사동생은 몇번 샵가더니 자기가 셀프로",
          "speaker_id": "1",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100021824.1.18",
          "form": "오홍 금손이네요",
          "original_form": "오홍 금손이네요",
          "speaker_id": "2",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100021824.1.19",
          "form": "제출 할게요 ㅎㅎ",
          "original_form": "제출 할게요 ㅎㅎ",
