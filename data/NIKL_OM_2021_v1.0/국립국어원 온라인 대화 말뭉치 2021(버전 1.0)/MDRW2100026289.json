      "utterance": [
        {
          "id": "MDRW2100026289.1.1",
          "form": "다이어트 해보신 거 있으세요?",
          "original_form": "다이어트 해보신 거 있으세요?",
          "speaker_id": "1",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026289.1.2",
          "form": "전 요즘 식욕이 감당이 안 돼서ㅠ",
          "original_form": "전 요즘 식욕이 감당이 안 돼서ㅠ",
          "speaker_id": "1",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100026289.1.3",
          "form": "전 요즘 회사에서 코로나로 무서워서 점심을 안먹고있어요",
          "original_form": "전 요즘 회사에서 코로나로 무서워서 점심을 안먹고있어요",
          "speaker_id": "2",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100026289.1.4",
          "form": "하루정도 굶으면 식욕이 조금 줄더라고요",
          "original_form": "하루정도 굶으면 식욕이 조금 줄더라고요",
          "speaker_id": "2",
          "time": "20210901 21:21"
        },
        {
          "id": "MDRW2100026289.1.5",
          "form": "점심 안 먹으면 힘 들지 않으세요??",
          "original_form": "점심 안 먹으면 힘 들지 않으세요??",
          "speaker_id": "1",
          "time": "20210901 21:21"
        },
        {
          "id": "MDRW2100026289.1.6",
          "form": "하루 굶기가ㅠㅠ",
          "original_form": "하루 굶기가ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 21:21"
        },
        {
          "id": "MDRW2100026289.1.7",
          "form": "처음엔 좀 그런데 계속 습관드니까 배안고프고\r\n점심시간도 여유로워서 좋더라고요",
          "original_form": "처음엔 좀 그런데 계속 습관드니까 배안고프고\r\n점심시간도 여유로워서 좋더라고요",
          "speaker_id": "2",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100026289.1.8",
          "form": "하루 굶기부터 해봐야겠네요 저는~",
          "original_form": "하루 굶기부터 해봐야겠네요 저는~",
          "speaker_id": "1",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100026289.1.9",
          "form": "엄청 날씬하실 거 같은데ㅋㅋ",
          "original_form": "엄청 날씬하실 거 같은데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100026289.1.10",
          "form": "그런 느낌이 드는데요ㅋㅋ",
          "original_form": "그런 느낌이 드는데요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100026289.1.11",
          "form": "아니예요ㅋㅋ 평균체중이예요",
          "original_form": "아니예요ㅋㅋ 평균체중이예요",
          "speaker_id": "2",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100026289.1.12",
          "form": "폭식과 배달음식으로 살쪘어요",
          "original_form": "폭식과 배달음식으로 살쪘어요",
          "speaker_id": "2",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100026289.1.13",
          "form": "근본 원인은 스트레스죠",
          "original_form": "근본 원인은 스트레스죠",
          "speaker_id": "2",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100026289.1.14",
          "form": "맞아요 스트레스가 만병의 근원이에요 진짜",
          "original_form": "맞아요 스트레스가 만병의 근원이에요 진짜",
          "speaker_id": "1",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100026289.1.15",
          "form": "배달음식은 늘 폭식을 유도하죠ㅠ",
          "original_form": "배달음식은 늘 폭식을 유도하죠ㅠ",
          "speaker_id": "1",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100026289.1.16",
          "form": "맞아요 스트레스는 모든 병을 불러일으켜요",
          "original_form": "맞아요 스트레스는 모든 병을 불러일으켜요",
          "speaker_id": "2",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100026289.1.17",
          "form": "스트레스 안 받고 사는 게 제일 좋을 텐데",
          "original_form": "스트레스 안 받고 사는 게 제일 좋을 텐데",
          "speaker_id": "1",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100026289.1.18",
          "form": "그런 사람 있을까요ㅠ",
          "original_form": "그런 사람 있을까요ㅠ",
          "speaker_id": "1",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100026289.1.19",
          "form": "그쵸 그런사람이 없죠ㅋㅋ\r\n스트레스를 적당히받아야하는데\r\n온몽으로 받고 있네요",
          "original_form": "그쵸 그런사람이 없죠ㅋㅋ\r\n스트레스를 적당히받아야하는데\r\n온몽으로 받고 있네요",
          "speaker_id": "2",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100026289.1.20",
          "form": "요즘은 정말 다 스트레스 덩어리에요",
          "original_form": "요즘은 정말 다 스트레스 덩어리에요",
          "speaker_id": "1",
          "time": "20210901 21:35"
        },
        {
          "id": "MDRW2100026289.1.21",
          "form": "정신 건강이 제일 걱정이에요ㅠ",
          "original_form": "정신 건강이 제일 걱정이에요ㅠ",
          "speaker_id": "1",
          "time": "20210901 21:35"
        },
        {
          "id": "MDRW2100026289.1.22",
          "form": "그니까요 눈에 보이지않는 스트레스가 정말 더 위험한 것 같아요",
          "original_form": "그니까요 눈에 보이지않는 스트레스가 정말 더 위험한 것 같아요",
          "speaker_id": "2",
          "time": "20210901 21:37"
        },
        {
          "id": "MDRW2100026289.1.23",
          "form": "스트레스 잘 푸는 것도 정말 중요한 것 같아요",
          "original_form": "스트레스 잘 푸는 것도 정말 중요한 것 같아요",
          "speaker_id": "1",
          "time": "20210901 21:37"
        }
      ]
    }
  ]
