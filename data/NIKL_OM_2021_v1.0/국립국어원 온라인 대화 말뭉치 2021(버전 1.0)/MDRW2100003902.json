        {
          "id": "MDRW2100003902.1.1",
          "form": "점심은 배달시키셨나요?",
          "original_form": "점심은 배달시키셨나요?",
          "speaker_id": "2",
          "time": "20210821 13:30"
        },
        {
          "id": "MDRW2100003902.1.2",
          "form": "아직 안먹었어요",
          "original_form": "아직 안먹었어요",
          "speaker_id": "1",
          "time": "20210821 13:31"
        },
        {
          "id": "MDRW2100003902.1.3",
          "form": "저도  아직 점심전이요",
          "original_form": "저도  아직 점심전이요",
          "speaker_id": "2",
          "time": "20210821 13:32"
        },
        {
          "id": "MDRW2100003902.1.4",
          "form": "뭐드실거예요?",
          "original_form": "뭐드실거예요?",
          "speaker_id": "2",
          "time": "20210821 13:32"
        },
        {
          "id": "MDRW2100003902.1.5",
          "form": "생각즈이요",
          "original_form": "생각즈이요",
          "speaker_id": "1",
          "time": "20210821 13:33"
        },
        {
          "id": "MDRW2100003902.1.6",
          "form": "다같이 드시는거예요? 집밥드시나요?",
          "original_form": "다같이 드시는거예요? 집밥드시나요?",
          "speaker_id": "2",
          "time": "20210821 13:33"
        },
        {
          "id": "MDRW2100003902.1.7",
          "form": "집밥이요",
          "original_form": "집밥이요",
          "speaker_id": "1",
          "time": "20210821 13:33"
        },
        {
          "id": "MDRW2100003902.1.8",
          "form": "전 제육볶음 먹을까 생각중이요",
          "original_form": "전 제육볶음 먹을까 생각중이요",
          "speaker_id": "2",
          "time": "20210821 13:34"
        },
        {
          "id": "MDRW2100003902.1.9",
          "form": "오 맛나겠다",
          "original_form": "오 맛나겠다",
          "speaker_id": "1",
          "time": "20210821 13:34"
        },
        {
