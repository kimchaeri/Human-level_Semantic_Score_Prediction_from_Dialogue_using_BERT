      "utterance": [
        {
          "id": "MDRW2100039440.1.1",
          "form": "ㅋㅋㅋㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:40"
        },
        {
          "id": "MDRW2100039440.1.2",
          "form": "그래도 자기 일에는 충실한 가보구만",
          "original_form": "그래도 자기 일에는 충실한 가보구만",
          "speaker_id": "2",
          "time": "20210903 17:40"
        },
        {
          "id": "MDRW2100039440.1.3",
          "form": "요즘 코로나땜에",
          "original_form": "요즘 코로나땜에",
          "speaker_id": "1",
          "time": "20210903 17:40"
        },
        {
          "id": "MDRW2100039440.1.4",
          "form": "마스크 쓰고 운동해서 그런가",
          "original_form": "마스크 쓰고 운동해서 그런가",
          "speaker_id": "1",
          "time": "20210903 17:40"
        },
        {
          "id": "MDRW2100039440.1.5",
          "form": "나 저번에 운동하다 토할뻔했어",
          "original_form": "나 저번에 운동하다 토할뻔했어",
          "speaker_id": "1",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100039440.1.6",
          "form": "헐 산소부족해서?",
          "original_form": "헐 산소부족해서?",
          "speaker_id": "2",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100039440.1.7",
          "form": "응 진짜 산소부족해서",
          "original_form": "응 진짜 산소부족해서",
          "speaker_id": "1",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100039440.1.8",
          "form": "멀미오듯이",
          "original_form": "멀미오듯이",
          "speaker_id": "1",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100039440.1.9",
          "form": "어질하더라",
          "original_form": "어질하더라",
          "speaker_id": "1",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100039440.1.10",
          "form": "으윽 마스크도 위험하네",
          "original_form": "으윽 마스크도 위험하네",
          "speaker_id": "2",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100039440.1.11",
          "form": "하체운동할때",
          "original_form": "하체운동할때",
          "speaker_id": "1",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100039440.1.12",
          "form": "나도 계단오를 때 마스크때문에 답답하긴 하더라",
          "original_form": "나도 계단오를 때 마스크때문에 답답하긴 하더라",
          "speaker_id": "2",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100039440.1.13",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039440.1.14",
          "form": "갑자기 뒷목이 뜨거워지면",
          "original_form": "갑자기 뒷목이 뜨거워지면",
          "speaker_id": "1",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039440.1.15",
          "form": "조심해라",
          "original_form": "조심해라",
          "speaker_id": "1",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039440.1.16",
          "form": "헐 왜",
          "original_form": "헐 왜",
          "speaker_id": "2",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039440.1.17",
          "form": "혈압오르는건가",
          "original_form": "혈압오르는건가",
          "speaker_id": "2",
          "time": "20210903 17:42"
        },
        {
          "id": "MDRW2100039440.1.18",
          "form": "하체가 근육이 커서",
          "original_form": "하체가 근육이 커서",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039440.1.19",
          "form": "과부화되면 피가 너무 빨리돌아서",
          "original_form": "과부화되면 피가 너무 빨리돌아서",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039440.1.20",
          "form": "산소가 부족해져",
          "original_form": "산소가 부족해져",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039440.1.21",
          "form": "그래서 어질어질~~웩 하는거지",
          "original_form": "그래서 어질어질~~웩 하는거지",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039440.1.22",
          "form": "헐 그렇구나.. 조심해야겠는걸",
          "original_form": "헐 그렇구나.. 조심해야겠는걸",
          "speaker_id": "2",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039440.1.23",
          "form": "다행히 아직까진 멀쩡한데",
          "original_form": "다행히 아직까진 멀쩡한데",
          "speaker_id": "2",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039440.1.24",
          "form": "무리하진 말아야겠어",
          "original_form": "무리하진 말아야겠어",
          "speaker_id": "2",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039440.1.25",
          "form": "산소부족은 아니고 둔근에 쥐난 적은 있는데 그것도 조심해야겠더라",
          "original_form": "산소부족은 아니고 둔근에 쥐난 적은 있는데 그것도 조심해야겠더라",
          "speaker_id": "2",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039440.1.26",
          "form": "헉 둔근에 쥐나면",
          "original_form": "헉 둔근에 쥐나면",
          "speaker_id": "1",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039440.1.27",
          "form": "어떻게 걷냐",
          "original_form": "어떻게 걷냐",
          "speaker_id": "1",
          "time": "20210903 17:44"
        },
        {
          "id": "MDRW2100039440.1.28",
          "form": "못 걸어ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "못 걸어ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100039440.1.29",
          "form": "다리는 금방 풀리는데",
          "original_form": "다리는 금방 풀리는데",
          "speaker_id": "2",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100039440.1.30",
          "form": "둔근은 근육이 커서 그런지 오래 가더라고",
          "original_form": "둔근은 근육이 커서 그런지 오래 가더라고",
          "speaker_id": "2",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100039440.1.31",
          "form": "둔근 풀릴때까지 강제 착석이네",
          "original_form": "둔근 풀릴때까지 강제 착석이네",
          "speaker_id": "1",
          "time": "20210903 17:46"
        }
      ]
    }
