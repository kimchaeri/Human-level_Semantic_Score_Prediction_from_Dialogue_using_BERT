      "utterance": [
        {
          "id": "MDRW2100041211.1.1",
          "form": "이제 육퇴",
          "original_form": "이제 육퇴",
          "speaker_id": "2",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100041211.1.2",
          "form": "신랑이 애들 재우러 들어갔당",
          "original_form": "신랑이 애들 재우러 들어갔당",
          "speaker_id": "2",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100041211.1.3",
          "form": "와 언니 좋겠다 ㅋㅋ",
          "original_form": "와 언니 좋겠다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100041211.1.4",
          "form": "나는 둘째는 잠들었는데 ㅋㅋㅋ 첫째가 또 깼네...",
          "original_form": "나는 둘째는 잠들었는데 ㅋㅋㅋ 첫째가 또 깼네...",
          "speaker_id": "1",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100041211.1.5",
          "form": "지금 과자 먹고 잇어 .",
          "original_form": "지금 과자 먹고 잇어 .",
          "speaker_id": "1",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100041211.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210905 20:58"
        },
        {
          "id": "MDRW2100041211.1.7",
          "form": "진짜 하루하루 피곤하네",
          "original_form": "진짜 하루하루 피곤하네",
          "speaker_id": "2",
          "time": "20210905 20:58"
        },
        {
          "id": "MDRW2100041211.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그러니깐 언니",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그러니깐 언니",
          "speaker_id": "1",
          "time": "20210905 20:59"
        },
        {
          "id": "MDRW2100041211.1.9",
          "form": "요즘 진짜 뭐가 안맞다 ㅋㅋ",
          "original_form": "요즘 진짜 뭐가 안맞다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:59"
        },
        {
          "id": "MDRW2100041211.1.10",
          "form": "첫째 자면 둘째가 안자고",
          "original_form": "첫째 자면 둘째가 안자고",
          "speaker_id": "1",
          "time": "20210905 20:59"
        },
        {
          "id": "MDRW2100041211.1.11",
          "form": "그래 편하게 저녁에 쉴 날이 없네",
          "original_form": "그래 편하게 저녁에 쉴 날이 없네",
          "speaker_id": "2",
          "time": "20210905 21:09"
        },
        {
          "id": "MDRW2100041211.1.12",
          "form": "디다 ㅋㅋㅋㄱ",
          "original_form": "디다 ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210905 21:10"
        },
        {
          "id": "MDRW2100041211.1.13",
          "form": "그라이ㅋㅋ",
          "original_form": "그라이ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:55"
        },
        {
          "id": "MDRW2100041211.1.14",
          "form": "언니도 고생했엉",
          "original_form": "언니도 고생했엉",
          "speaker_id": "1",
          "time": "20210905 22:56"
        },
        {
          "id": "MDRW2100041211.1.15",
          "form": "이제",
          "original_form": "이제",
          "speaker_id": "1",
          "time": "20210905 22:56"
        },
        {
          "id": "MDRW2100041211.1.16",
          "form": "재웠다ㅋ",
          "original_form": "재웠다ㅋ",
          "speaker_id": "1",
          "time": "20210905 22:56"
        },
        {
          "id": "MDRW2100041211.1.17",
          "form": "와ㅋㅋ 둘째 새벽3시에일어나서 곰국에 밥한그릇 말아먹고 놀다가잤음",
          "original_form": "와ㅋㅋ 둘째 새벽3시에일어나서 곰국에 밥한그릇 말아먹고 놀다가잤음",
          "speaker_id": "1",
          "time": "20210906 09:06"
        },
        {
          "id": "MDRW2100041211.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 미치겠다 진짜",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 미치겠다 진짜",
          "speaker_id": "2",
          "time": "20210906 10:06"
        },
        {
          "id": "MDRW2100041211.1.19",
          "form": "차라리 밤늦게 자는게 낫네 ㅋㅋㄱㄱㄱㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "차라리 밤늦게 자는게 낫네 ㅋㅋㄱㄱㄱㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 10:06"
        },
        {
          "id": "MDRW2100041211.1.20",
          "form": "3시에 일어나서 밥달라더나??",
          "original_form": "3시에 일어나서 밥달라더나??",
          "speaker_id": "2",
          "time": "20210906 10:06"
        },
        {
          "id": "MDRW2100041211.1.21",
          "form": "응 어제 낮잠없이 7시에잤거든",
          "original_form": "응 어제 낮잠없이 7시에잤거든",
          "speaker_id": "1",
          "time": "20210906 12:05"
        },
        {
          "id": "MDRW2100041211.1.22",
          "form": "저녁대충먹고 재웠는데",
          "original_form": "저녁대충먹고 재웠는데",
          "speaker_id": "1",
          "time": "20210906 12:06"
        },
        {
          "id": "MDRW2100041211.1.23",
          "form": "3시에 일어나서 밥달라고하더라",
          "original_form": "3시에 일어나서 밥달라고하더라",
          "speaker_id": "1",
          "time": "20210906 12:06"
        },
        {
          "id": "MDRW2100041211.1.24",
          "form": "와 대박 ㅋㄱㅋㅋㅋㄱ",
          "original_form": "와 대박 ㅋㄱㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210906 12:19"
        },
        {
          "id": "MDRW2100041211.1.25",
          "form": "그시간에 밥이라니",
          "original_form": "그시간에 밥이라니",
          "speaker_id": "2",
          "time": "20210906 12:19"
        },
        {
          "id": "MDRW2100041211.1.26",
          "form": "밥차려준 니도 대단하다",
          "original_form": "밥차려준 니도 대단하다",
          "speaker_id": "2",
          "time": "20210906 12:19"
        },
        {
          "id": "MDRW2100041211.1.27",
          "form": "나는 그냥 자라할듯 ㅋㅋㅋㅋㅋ",
          "original_form": "나는 그냥 자라할듯 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 12:19"
        },
        {
          "id": "MDRW2100041211.1.28",
          "form": "배고파해서 애가 잠을못자더라",
          "original_form": "배고파해서 애가 잠을못자더라",
          "speaker_id": "1",
          "time": "20210906 12:23"
        },
        {
          "id": "MDRW2100041211.1.29",
          "form": "곰국에말아줬지",
          "original_form": "곰국에말아줬지",
          "speaker_id": "1",
          "time": "20210906 12:23"
        },
        {
          "id": "MDRW2100041211.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㄱㅋ 배고파서 잠을 못자다니",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱㅋ 배고파서 잠을 못자다니",
          "speaker_id": "2",
          "time": "20210906 12:37"
        },
        {
          "id": "MDRW2100041211.1.31",
          "form": "우리집에서는 있을수없는 일이군",
          "original_form": "우리집에서는 있을수없는 일이군",
          "speaker_id": "2",
          "time": "20210906 12:37"
        },
        {
          "id": "MDRW2100041211.1.32",
          "form": "ㅋㅋㅋㅋㅋ 그러니깐",
          "original_form": "ㅋㅋㅋㅋㅋ 그러니깐",
          "speaker_id": "1",
          "time": "20210906 12:37"
        },
        {
          "id": "MDRW2100041211.1.33",
          "form": "아휴ㅋ",
          "original_form": "아휴ㅋ",
          "speaker_id": "1",
          "time": "20210906 12:37"
        },
        {
          "id": "MDRW2100041211.1.34",
          "form": "점심은?",
          "original_form": "점심은?",
          "speaker_id": "1",
          "time": "20210906 12:38"
        },
        {
          "id": "MDRW2100041211.1.35",
          "form": "오늘도 제육볶음 ㅎㅎ",
          "original_form": "오늘도 제육볶음 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 12:39"
        },
        {
          "id": "MDRW2100041211.1.36",
          "form": "금요일날 먹은거 맛있어서 오늘도 먹었다 ㅋㅋ",
          "original_form": "금요일날 먹은거 맛있어서 오늘도 먹었다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 12:39"
        },
        {
          "id": "MDRW2100041211.1.37",
          "form": "일끝났나??",
          "original_form": "일끝났나??",
          "speaker_id": "2",
          "time": "20210906 12:39"
        },
        {
          "id": "MDRW2100041211.1.38",
          "form": "ㅋㅋ응 제육볶음 맛있었구나ㅋㅋ좋다",
          "original_form": "ㅋㅋ응 제육볶음 맛있었구나ㅋㅋ좋다",
          "speaker_id": "1",
          "time": "20210906 12:39"
        }
      ]
    }
