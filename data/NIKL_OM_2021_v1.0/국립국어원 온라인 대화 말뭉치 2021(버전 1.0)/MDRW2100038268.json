      "utterance": [
        {
          "id": "MDRW2100038268.1.1",
          "form": "오눌 날씨어때요",
          "original_form": "오눌 날씨어때요",
          "speaker_id": "1",
          "time": "20210902 10:12"
        },
        {
          "id": "MDRW2100038268.1.2",
          "form": "여긴 선선해졌어요",
          "original_form": "여긴 선선해졌어요",
          "speaker_id": "1",
          "time": "20210902 10:12"
        },
        {
          "id": "MDRW2100038268.1.3",
          "form": "여기도 많이 시원해요",
          "original_form": "여기도 많이 시원해요",
          "speaker_id": "2",
          "time": "20210902 10:21"
        },
        {
          "id": "MDRW2100038268.1.4",
          "form": "좀 춥게도 느껴져서 창문을 좀만 열어뒀어요",
          "original_form": "좀 춥게도 느껴져서 창문을 좀만 열어뒀어요",
          "speaker_id": "2",
          "time": "20210902 10:21"
        },
        {
          "id": "MDRW2100038268.1.5",
          "form": "구쵸 선선해져서 다행이에요",
          "original_form": "구쵸 선선해져서 다행이에요",
          "speaker_id": "1",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100038268.1.6",
          "form": "올여름 너무 더웠어요",
          "original_form": "올여름 너무 더웠어요",
          "speaker_id": "1",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100038268.1.7",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 10:56"
        },
        {
          "id": "MDRW2100038268.1.8",
          "form": "비는 안 오시나요?",
          "original_form": "비는 안 오시나요?",
          "speaker_id": "2",
          "time": "20210902 10:56"
        },
        {
          "id": "MDRW2100038268.1.9",
          "form": "네네비안와요",
          "original_form": "네네비안와요",
          "speaker_id": "1",
          "time": "20210902 10:57"
        },
        {
          "id": "MDRW2100038268.1.10",
          "form": "이제내년에도 여름이길겠죠?ㅠㅠ",
          "original_form": "이제내년에도 여름이길겠죠?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 10:58"
        },
        {
          "id": "MDRW2100038268.1.11",
          "form": "네... 점점 길어지니ㅠㅠ",
          "original_form": "네... 점점 길어지니ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100038268.1.12",
          "form": "더 더워지지 않기만 바라고 있어요",
          "original_form": "더 더워지지 않기만 바라고 있어요",
          "speaker_id": "2",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100038268.1.13",
          "form": "맞습니다ㅜㅜ",
          "original_form": "맞습니다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 12:20"
        },
        {
          "id": "MDRW2100038268.1.14",
          "form": "점점시원해지길",
          "original_form": "점점시원해지길",
          "speaker_id": "1",
          "time": "20210902 12:20"
        },
        {
          "id": "MDRW2100038268.1.15",
          "form": "어떤 계절 제일 좋아하세요?",
          "original_form": "어떤 계절 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 12:29"
        },
        {
          "id": "MDRW2100038268.1.16",
          "form": "저는 가울 겨울 좋아해요 파르페는요?",
          "original_form": "저는 가울 겨울 좋아해요 파르페는요?",
          "speaker_id": "1",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100038268.1.17",
          "form": "저는 가을 좋아해요",
          "original_form": "저는 가을 좋아해요",
          "speaker_id": "2",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100038268.1.18",
          "form": "추위나 더위 타시나요?",
          "original_form": "추위나 더위 타시나요?",
          "speaker_id": "2",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100038268.1.19",
          "form": "저는 더위 심히 타요 ㅋㅋ",
          "original_form": "저는 더위 심히 타요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100038268.1.20",
          "form": "추위 강해요",
          "original_form": "추위 강해요",
          "speaker_id": "1",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100038268.1.21",
          "form": "저도 더위 더 많이 타요ㅠㅠ",
          "original_form": "저도 더위 더 많이 타요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100038268.1.22",
          "form": "이번 여름에 너무 힘들었어요",
          "original_form": "이번 여름에 너무 힘들었어요",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100038268.1.23",
          "form": "저도요ㅠㅠ 지금도 힘들어요 ㅋㅋ",
          "original_form": "저도요ㅠㅠ 지금도 힘들어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100038268.1.24",
          "form": "체질도 그렇대요 ㅋㅋ",
          "original_form": "체질도 그렇대요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100038268.1.25",
          "form": "체질은 어쩔 수 없죠ㅠㅠ",
          "original_form": "체질은 어쩔 수 없죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 15:08"
        }
