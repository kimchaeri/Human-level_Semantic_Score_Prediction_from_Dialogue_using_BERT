      "utterance": [
        {
          "id": "MDRW2100002077.1.1",
          "form": "동물 키워요?",
          "original_form": "동물 키워요?",
          "speaker_id": "1",
          "time": "20210819 23:22"
        },
        {
          "id": "MDRW2100002077.1.2",
          "form": "지금 아파트에 살아서 못키우는데",
          "original_form": "지금 아파트에 살아서 못키우는데",
          "speaker_id": "2",
          "time": "20210819 23:23"
        },
        {
          "id": "MDRW2100002077.1.3",
          "form": "부모님댁에는 리트리버 한마리 키우고있어요",
          "original_form": "부모님댁에는 리트리버 한마리 키우고있어요",
          "speaker_id": "2",
          "time": "20210819 23:24"
        },
        {
          "id": "MDRW2100002077.1.4",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210819 23:24"
        },
        {
          "id": "MDRW2100002077.1.5",
          "form": "일할때 혼자 두기가 미안해서 못키워요",
          "original_form": "일할때 혼자 두기가 미안해서 못키워요",
          "speaker_id": "1",
          "time": "20210819 23:24"
        },
        {
          "id": "MDRW2100002077.1.6",
          "form": "아 그쵸",
          "original_form": "아 그쵸",
          "speaker_id": "2",
          "time": "20210819 23:28"
        },
        {
          "id": "MDRW2100002077.1.7",
          "form": "그것도 문제죠",
          "original_form": "그것도 문제죠",
          "speaker_id": "2",
          "time": "20210819 23:28"
        },
        {
          "id": "MDRW2100002077.1.8",
          "form": "네 관리가 힘들거 같네여",
          "original_form": "네 관리가 힘들거 같네여",
          "speaker_id": "1",
          "time": "20210819 23:28"
        },
        {
          "id": "MDRW2100002077.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210819 23:28"
        },
        {
          "id": "MDRW2100002077.1.10",
          "form": "저는 개를 좋아하는데",
          "original_form": "저는 개를 좋아하는데",
          "speaker_id": "2",
          "time": "20210819 23:29"
        },
        {
          "id": "MDRW2100002077.1.11",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210819 23:29"
        },
        {
          "id": "MDRW2100002077.1.12",
          "form": "부모님댁에 있는 개가 레브라도리트리버인데",
          "original_form": "부모님댁에 있는 개가 레브라도리트리버인데",
          "speaker_id": "2",
          "time": "20210819 23:29"
        },
        {
          "id": "MDRW2100002077.1.13",
          "form": "너무 귀엽고 사랑스러워요",
          "original_form": "너무 귀엽고 사랑스러워요",
          "speaker_id": "2",
          "time": "20210819 23:29"
        },
        {
          "id": "MDRW2100002077.1.14",
          "form": "영리하고 순하고",
          "original_form": "영리하고 순하고",
          "speaker_id": "2",
          "time": "20210819 23:30"
        },
        {
          "id": "MDRW2100002077.1.15",
          "form": "그렇게라도 볼수 있어 부럽네요",
          "original_form": "그렇게라도 볼수 있어 부럽네요",
          "speaker_id": "1",
          "time": "20210819 23:30"
        },
        {
          "id": "MDRW2100002077.1.16",
          "form": "한번도 반려동물을 길러보신적 없으신가요?",
          "original_form": "한번도 반려동물을 길러보신적 없으신가요?",
          "speaker_id": "2",
          "time": "20210819 23:31"
        },
        {
          "id": "MDRW2100002077.1.17",
          "form": "어릴때 밖에 개정도요",
          "original_form": "어릴때 밖에 개정도요",
          "speaker_id": "1",
          "time": "20210819 23:31"
        },
        {
          "id": "MDRW2100002077.1.18",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "2",
          "time": "20210819 23:33"
        },
        {
          "id": "MDRW2100002077.1.19",
          "form": "그래도 강아지 관리 잘 못해줄것 같아서",
          "original_form": "그래도 강아지 관리 잘 못해줄것 같아서",
          "speaker_id": "2",
          "time": "20210819 23:34"
        },
        {
          "id": "MDRW2100002077.1.20",
          "form": "강아지 좋아하는데도 안키우시는거 보면 착하신분 같네요",
          "original_form": "강아지 좋아하는데도 안키우시는거 보면 착하신분 같네요",
          "speaker_id": "2",
          "time": "20210819 23:34"
        },
        {
          "id": "MDRW2100002077.1.21",
          "form": "그래도 넘  키우고 싶어요",
          "original_form": "그래도 넘  키우고 싶어요",
          "speaker_id": "1",
          "time": "20210819 23:35"
        }
      ]
