      "utterance": [
        {
          "id": "MDRW2100016084.1.1",
          "form": "연애는 하고 계신가요",
          "original_form": "연애는 하고 계신가요",
          "speaker_id": "1",
          "time": "20210826 09:19"
        },
        {
          "id": "MDRW2100016084.1.2",
          "form": "네ㅎㅎ 연애하세요?",
          "original_form": "네ㅎㅎ 연애하세요?",
          "speaker_id": "2",
          "time": "20210826 09:23"
        },
        {
          "id": "MDRW2100016084.1.3",
          "form": "네 저는 회사에서 만나 연애중입니다 하하",
          "original_form": "네 저는 회사에서 만나 연애중입니다 하하",
          "speaker_id": "1",
          "time": "20210826 09:24"
        },
        {
          "id": "MDRW2100016084.1.4",
          "form": "얼마나 연애 하셨어용?",
          "original_form": "얼마나 연애 하셨어용?",
          "speaker_id": "1",
          "time": "20210826 09:24"
        },
        {
          "id": "MDRW2100016084.1.5",
          "form": "아 재밌을거같아요 사내연애ㅎㅎ",
          "original_form": "아 재밌을거같아요 사내연애ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 09:24"
        },
        {
          "id": "MDRW2100016084.1.6",
          "form": "저는 2년정도ㅎㅎ",
          "original_form": "저는 2년정도ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 09:25"
        },
        {
          "id": "MDRW2100016084.1.7",
          "form": "하루종일 붙어 있으니 좋긴해요 ㅎㅎ",
          "original_form": "하루종일 붙어 있으니 좋긴해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 09:27"
        },
        {
          "id": "MDRW2100016084.1.8",
          "form": "특히 점심시간 ㅎㅎ",
          "original_form": "특히 점심시간 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 09:27"
        },
        {
          "id": "MDRW2100016084.1.9",
          "form": "오늘 점심은 뭐드실예정이신가용",
          "original_form": "오늘 점심은 뭐드실예정이신가용",
          "speaker_id": "1",
          "time": "20210826 09:27"
        },
        {
          "id": "MDRW2100016084.1.10",
          "form": "데이트하는 기분도 날거같아요ㅋㅋㅋ",
          "original_form": "데이트하는 기분도 날거같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 09:27"
        },
        {
          "id": "MDRW2100016084.1.11",
          "form": "저는 초밥먹으려구요ㅎㅎ",
          "original_form": "저는 초밥먹으려구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 09:28"
        },
        {
          "id": "MDRW2100016084.1.12",
          "form": "오 맛있겠다",
          "original_form": "오 맛있겠다",
          "speaker_id": "1",
          "time": "20210826 09:29"
        },
        {
          "id": "MDRW2100016084.1.13",
          "form": "오늘 외근이라 메뉴 고를수있네요ㅋㅌ",
          "original_form": "오늘 외근이라 메뉴 고를수있네요ㅋㅌ",
          "speaker_id": "2",
          "time": "20210826 09:30"
        },
        {
          "id": "MDRW2100016084.1.14",
          "form": "맛점하시고 힘내세요!!!",
          "original_form": "맛점하시고 힘내세요!!!",
          "speaker_id": "1",
          "time": "20210826 09:30"
        },
        {
          "id": "MDRW2100016084.1.15",
          "form": "오늘 외근이면",
          "original_form": "오늘 외근이면",
          "speaker_id": "1",
          "time": "20210826 09:30"
        },
        {
          "id": "MDRW2100016084.1.16",
          "form": "바쁘시겠네요?",
          "original_form": "바쁘시겠네요?",
          "speaker_id": "1",
          "time": "20210826 09:30"
        },
        {
          "id": "MDRW2100016084.1.17",
          "form": "그렇진않은데 돌아다녀서ㅎㅎ",
          "original_form": "그렇진않은데 돌아다녀서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 09:31"
        },
        {
          "id": "MDRW2100016084.1.18",
          "form": "그래도 오늘 좀 선선해서 일보러 가시기 괜찮겠어요 ㅎㅎ",
          "original_form": "그래도 오늘 좀 선선해서 일보러 가시기 괜찮겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 09:33"
        },
        {
          "id": "MDRW2100016084.1.19",
          "form": "네 날씨딱좋네요ㅎㅎ",
          "original_form": "네 날씨딱좋네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 09:34"
        },
        {
          "id": "MDRW2100016084.1.20",
          "form": "회사에만 있기 아쉬운 날씨네요",
