      "utterance": [
        {
          "id": "MDRW2100038652.1.1",
          "form": "저녁은 삼겹살인가용 ㅋ",
          "original_form": "저녁은 삼겹살인가용 ㅋ",
          "speaker_id": "1",
          "time": "20210902 20:09"
        },
        {
          "id": "MDRW2100038652.1.2",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210902 20:09"
        },
        {
          "id": "MDRW2100038652.1.3",
          "form": "요새 삼겹살 많이 먹네 ㅎㅎ",
          "original_form": "요새 삼겹살 많이 먹네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:09"
        },
        {
          "id": "MDRW2100038652.1.4",
          "form": "고기가 많이 싸서",
          "original_form": "고기가 많이 싸서",
          "speaker_id": "2",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100038652.1.5",
          "form": "오옷 좋구만",
          "original_form": "오옷 좋구만",
          "speaker_id": "1",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100038652.1.6",
          "form": "홈플 고기 맛이 좋더라",
          "original_form": "홈플 고기 맛이 좋더라",
          "speaker_id": "2",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100038652.1.7",
          "form": "마자마자 맛난당",
          "original_form": "마자마자 맛난당",
          "speaker_id": "1",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100038652.1.8",
          "form": "목살 보다 삼겹살이 낫지",
          "original_form": "목살 보다 삼겹살이 낫지",
          "speaker_id": "2",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100038652.1.9",
          "form": "웅웅",
          "original_form": "웅웅",
          "speaker_id": "1",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100038652.1.10",
          "form": "그래 삼겹살이 제일 맛있다",
          "original_form": "그래 삼겹살이 제일 맛있다",
          "speaker_id": "2",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100038652.1.11",
          "form": "삼겹살이 훨 맛남 ㅋㅋ",
          "original_form": "삼겹살이 훨 맛남 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100038652.1.12",
          "form": "그래서 더 비싸다",
          "original_form": "그래서 더 비싸다",
          "speaker_id": "2",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100038652.1.13",
          "form": "웅웅..ㅋㅋㅋ 비싼건 비싼 값을 하는군!",
