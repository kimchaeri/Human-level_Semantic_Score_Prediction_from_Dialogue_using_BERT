      "utterance": [
        {
          "id": "MDRW2100042984.1.1",
          "form": "한번 여쭤봐",
          "original_form": "한번 여쭤봐",
          "speaker_id": "2",
          "time": "20210907 12:29"
        },
        {
          "id": "MDRW2100042984.1.2",
          "form": "실수로 그랬을 수도 있으니까",
          "original_form": "실수로 그랬을 수도 있으니까",
          "speaker_id": "2",
          "time": "20210907 12:29"
        },
        {
          "id": "MDRW2100042984.1.3",
          "form": "이미 돌아서서 나왔는데...",
          "original_form": "이미 돌아서서 나왔는데...",
          "speaker_id": "1",
          "time": "20210907 12:29"
        },
        {
          "id": "MDRW2100042984.1.4",
          "form": "아앗...",
          "original_form": "아앗...",
          "speaker_id": "2",
          "time": "20210907 12:29"
        },
        {
          "id": "MDRW2100042984.1.5",
          "form": "보통 난",
          "original_form": "보통 난",
          "speaker_id": "1",
          "time": "20210907 12:29"
        },
        {
          "id": "MDRW2100042984.1.6",
          "form": "이제 아주머니가 메꿔야...",
          "original_form": "이제 아주머니가 메꿔야...",
          "speaker_id": "2",
          "time": "20210907 12:29"
        },
        {
          "id": "MDRW2100042984.1.7",
          "form": "금액 확인 안하니까",
          "original_form": "금액 확인 안하니까",
          "speaker_id": "1",
          "time": "20210907 12:29"
        },
        {
          "id": "MDRW2100042984.1.8",
          "form": "(아님",
          "original_form": "(아님",
          "speaker_id": "2",
          "time": "20210907 12:29"
        },
        {
          "id": "MDRW2100042984.1.9",
          "form": "걍 알아서 긁엇겟거니 하고",
          "original_form": "걍 알아서 긁엇겟거니 하고",
          "speaker_id": "1",
          "time": "20210907 12:29"
        },
        {
          "id": "MDRW2100042984.1.10",
          "form": "좀 지나서 내역 보다가 발견함...",
          "original_form": "좀 지나서 내역 보다가 발견함...",
          "speaker_id": "1",
          "time": "20210907 12:30"
        },
        {
          "id": "MDRW2100042984.1.11",
          "form": "그치 대부분은 금액 늦게 보거나 그렇지",
          "original_form": "그치 대부분은 금액 늦게 보거나 그렇지",
          "speaker_id": "2",
          "time": "20210907 12:32"
        },
        {
          "id": "MDRW2100042984.1.12",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "2",
          "time": "20210907 12:32"
        },
        {
          "id": "MDRW2100042984.1.13",
          "form": "찝찝하다",
          "original_form": "찝찝하다",
          "speaker_id": "2",
          "time": "20210907 12:32"
        },
        {
          "id": "MDRW2100042984.1.14",
          "form": "그니까;;;",
          "original_form": "그니까;;;",
          "speaker_id": "1",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100042984.1.15",
          "form": "아니 내가 뭐 먹고 튄것도 아니고",
          "original_form": "아니 내가 뭐 먹고 튄것도 아니고",
          "speaker_id": "1",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100042984.1.16",
          "form": "계산 다 했는데",
          "original_form": "계산 다 했는데",
          "speaker_id": "1",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100042984.1.17",
          "form": "왜 이 찝찝함을 느껴야하죠...",
          "original_form": "왜 이 찝찝함을 느껴야하죠...",
          "speaker_id": "1",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100042984.1.18",
          "form": "그니까...ㅋㅋㅋ",
          "original_form": "그니까...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:50"
        },
        {
          "id": "MDRW2100042984.1.19",
          "form": "분명 법은 지켰는데 괜히 찝찝해지기 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "분명 법은 지켰는데 괜히 찝찝해지기 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:50"
        },
        {
          "id": "MDRW2100042984.1.20",
          "form": "ㅎ ㅏ",
          "original_form": "ㅎ ㅏ",
          "speaker_id": "1",
          "time": "20210907 13:50"
        },
        {
          "id": "MDRW2100042984.1.21",
          "form": "가게가 너무 바빠서",
          "original_form": "가게가 너무 바빠서",
          "speaker_id": "1",
          "time": "20210907 13:50"
        },
        {
          "id": "MDRW2100042984.1.22",
          "form": "다들 정신이 없었나봐...",
          "original_form": "다들 정신이 없었나봐...",
          "speaker_id": "1",
          "time": "20210907 13:50"
        },
        {
          "id": "MDRW2100042984.1.23",
          "form": "ㅋ큐ㅠㅠ 나중에 마감할 때나 아실텐데",
          "original_form": "ㅋ큐ㅠㅠ 나중에 마감할 때나 아실텐데",
          "speaker_id": "2",
          "time": "20210907 13:56"
        }
