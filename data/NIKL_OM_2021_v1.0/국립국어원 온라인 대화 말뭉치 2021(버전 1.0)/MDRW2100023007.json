      "utterance": [
        {
          "id": "MDRW2100023007.1.1",
          "form": "첫구매이벤트는 엄청많이헤요ㅎ",
          "original_form": "첫구매이벤트는 엄청많이헤요ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:17"
        },
        {
          "id": "MDRW2100023007.1.2",
          "form": "꼭 하셔야합니당ㅎㅎ",
          "original_form": "꼭 하셔야합니당ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:17"
        },
        {
          "id": "MDRW2100023007.1.3",
          "form": "별거없어요ㅎ 네이버에 첫구매 치시면 쭈루루루루루루 나옴요ㅎ",
          "original_form": "별거없어요ㅎ 네이버에 첫구매 치시면 쭈루루루루루루 나옴요ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:17"
        },
        {
          "id": "MDRW2100023007.1.4",
          "form": "아 완전 꿀팁!!!!",
          "original_form": "아 완전 꿀팁!!!!",
          "speaker_id": "1",
          "time": "20210829 20:18"
        },
        {
          "id": "MDRW2100023007.1.5",
          "form": "이제 매일 검색하겠어요!!!",
          "original_form": "이제 매일 검색하겠어요!!!",
          "speaker_id": "1",
          "time": "20210829 20:18"
        },
        {
          "id": "MDRW2100023007.1.6",
          "form": "ㅎㅎ 매일검색하실필요없어요ㅎ",
          "original_form": "ㅎㅎ 매일검색하실필요없어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:19"
        },
        {
          "id": "MDRW2100023007.1.7",
          "form": "오픈채팅방이나 이런데 보시면 앱테크 엄청 많아요ㅎ",
          "original_form": "오픈채팅방이나 이런데 보시면 앱테크 엄청 많아요ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:20"
        },
        {
          "id": "MDRW2100023007.1.8",
          "form": "아라서 올려줌뇨ㅎ",
          "original_form": "아라서 올려줌뇨ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:20"
        },
        {
          "id": "MDRW2100023007.1.9",
          "form": "그리고 블로그중에 밍키언니 블로그가셔도되고 유튜브 보셔도 나와요ㅎ 저도 그거보고했아요ㅎ",
          "original_form": "그리고 블로그중에 밍키언니 블로그가셔도되고 유튜브 보셔도 나와요ㅎ 저도 그거보고했아요ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:20"
        },
        {
          "id": "MDRW2100023007.1.10",
          "form": "우와ㅋㅋ전 기껏해야 뽐뿌만 가봤었는데요",
          "original_form": "우와ㅋㅋ전 기껏해야 뽐뿌만 가봤었는데요",
          "speaker_id": "1",
          "time": "20210829 20:21"
        },
        {
          "id": "MDRW2100023007.1.11",
          "form": "완전 대박정보예요!",
          "original_form": "완전 대박정보예요!",
          "speaker_id": "1",
          "time": "20210829 20:22"
        },
        {
          "id": "MDRW2100023007.1.12",
          "form": "오오ㅎ 뽐뿌도 가고ㅎ",
          "original_form": "오오ㅎ 뽐뿌도 가고ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:22"
        },
        {
          "id": "MDRW2100023007.1.13",
          "form": "슈퍼투데이도 가셔요ㅎㅎ",
          "original_form": "슈퍼투데이도 가셔요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:22"
        },
        {
          "id": "MDRW2100023007.1.14",
          "form": "이건 좋은정보라기보단ㅎ 모르시니깐ㅎㅎ",
          "original_form": "이건 좋은정보라기보단ㅎ 모르시니깐ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:22"
        },
        {
          "id": "MDRW2100023007.1.15",
          "form": "알려드리는거에요ㅎㅎ 꼭 알뜰살뜰하게 구매하셔야죠ㅎㅎ",
          "original_form": "알려드리는거에요ㅎㅎ 꼭 알뜰살뜰하게 구매하셔야죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:22"
        },
        {
          "id": "MDRW2100023007.1.16",
          "form": "ㅋㅋ아 경품 은근 많이들 응모하더라고요",
          "original_form": "ㅋㅋ아 경품 은근 많이들 응모하더라고요",
          "speaker_id": "1",
          "time": "20210829 20:23"
        },
        {
          "id": "MDRW2100023007.1.17",
          "form": "맞아요ㅎ",
          "original_form": "맞아요ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:23"
        },
        {
          "id": "MDRW2100023007.1.18",
          "form": "안사면 백프로지만 살거라면 싸게 사는게 좋죠",
          "original_form": "안사면 백프로지만 살거라면 싸게 사는게 좋죠",
          "speaker_id": "1",
          "time": "20210829 20:23"
        },
        {
          "id": "MDRW2100023007.1.19",
          "form": "요즘 근데 뽐뿌 많이 안올라오지않아요?",
          "original_form": "요즘 근데 뽐뿌 많이 안올라오지않아요?",
