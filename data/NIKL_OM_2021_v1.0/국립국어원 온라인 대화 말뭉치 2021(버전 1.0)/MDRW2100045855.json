        {
          "id": "MDRW2100045855.1.1",
          "form": "나 오늘 코엑스 갈거야",
          "original_form": "나 오늘 코엑스 갈거야",
          "speaker_id": "2",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100045855.1.2",
          "form": "쇼핑하러?",
          "original_form": "쇼핑하러?",
          "speaker_id": "1",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100045855.1.3",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100045855.1.4",
          "form": "월급날이네",
          "original_form": "월급날이네",
          "speaker_id": "1",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100045855.1.5",
          "form": "맞아 ㅋㅋㅋㅋ 이제 쓰는 날이지",
          "original_form": "맞아 ㅋㅋㅋㅋ 이제 쓰는 날이지",
          "speaker_id": "2",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100045855.1.6",
          "form": "뭐살거?",
          "original_form": "뭐살거?",
          "speaker_id": "1",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100045855.1.7",
          "form": "옷을 사려고",
          "original_form": "옷을 사려고",
          "speaker_id": "2",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100045855.1.8",
          "form": "어떤옷",
          "original_form": "어떤옷",
          "speaker_id": "1",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100045855.1.9",
          "form": "가을 옷",
          "original_form": "가을 옷",
          "speaker_id": "2",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100045855.1.10",
          "form": "그럼 셔츠?",
          "original_form": "그럼 셔츠?",
          "speaker_id": "1",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100045855.1.11",
          "form": "맞아 아니면 블라우스",
          "original_form": "맞아 아니면 블라우스",
          "speaker_id": "2",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100045855.1.12",
          "form": "넌 밝은색이 어울려",
          "original_form": "넌 밝은색이 어울려",
          "speaker_id": "1",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100045855.1.13",
          "form": "그럼 좀 그런 것도 사봐야겠다",
          "original_form": "그럼 좀 그런 것도 사봐야겠다",
          "speaker_id": "2",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100045855.1.14",
          "form": "너 맨날 검정사면서",
          "original_form": "너 맨날 검정사면서",
