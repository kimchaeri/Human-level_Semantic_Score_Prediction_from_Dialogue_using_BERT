      "utterance": [
        {
          "id": "MDRW2100003321.1.1",
          "form": "요새 넷플릭스에서 재밌는거 많이 한다며?",
          "original_form": "요새 넷플릭스에서 재밌는거 많이 한다며?",
          "speaker_id": "2",
          "time": "20210820 21:45"
        },
        {
          "id": "MDRW2100003321.1.2",
          "form": "오늘 금요일인데 나혼자봐야지",
          "original_form": "오늘 금요일인데 나혼자봐야지",
          "speaker_id": "1",
          "time": "20210820 21:45"
        },
        {
          "id": "MDRW2100003321.1.3",
          "form": "요새 재미 없던데",
          "original_form": "요새 재미 없던데",
          "speaker_id": "2",
          "time": "20210820 21:45"
        },
        {
          "id": "MDRW2100003321.1.4",
          "form": "넷플릭스 아이디가 없어 모르겠다 뭐 있는데",
          "original_form": "넷플릭스 아이디가 없어 모르겠다 뭐 있는데",
          "speaker_id": "1",
          "time": "20210820 21:45"
        },
        {
          "id": "MDRW2100003321.1.5",
          "form": "나도 없어ㅋ 근데 유튜브에 예고편 올라오는데 재밌어 보였어",
          "original_form": "나도 없어ㅋ 근데 유튜브에 예고편 올라오는데 재밌어 보였어",
          "speaker_id": "2",
          "time": "20210820 21:46"
        },
        {
          "id": "MDRW2100003321.1.6",
          "form": "지난주 10% 시청률 웃겼다",
          "original_form": "지난주 10% 시청률 웃겼다",
          "speaker_id": "1",
          "time": "20210820 21:46"
        },
        {
          "id": "MDRW2100003321.1.7",
          "form": "울동생은 박나래 너무 꼴보기 싫대ㅋ",
          "original_form": "울동생은 박나래 너무 꼴보기 싫대ㅋ",
          "speaker_id": "2",
          "time": "20210820 21:47"
        },
        {
          "id": "MDRW2100003321.1.8",
          "form": "감안하고 봐야지ㅋㅋ 그려려니하고",
          "original_form": "감안하고 봐야지ㅋㅋ 그려려니하고",
          "speaker_id": "1",
          "time": "20210820 21:47"
        },
        {
          "id": "MDRW2100003321.1.9",
          "form": "너무 싫어해서 채널 돌려야 돼ㅋ",
          "original_form": "너무 싫어해서 채널 돌려야 돼ㅋ",
          "speaker_id": "2",
          "time": "20210820 21:48"
        },
        {
          "id": "MDRW2100003321.1.10",
          "form": "전현무랑 기안 여름단합 갔는데 엄청 웃겼다",
          "original_form": "전현무랑 기안 여름단합 갔는데 엄청 웃겼다",
          "speaker_id": "1",
          "time": "20210820 21:49"
        },
        {
          "id": "MDRW2100003321.1.11",
          "form": "난 뭐 채널권이 없어 지금도 조선티비 보고 있다",
          "original_form": "난 뭐 채널권이 없어 지금도 조선티비 보고 있다",
          "speaker_id": "2",
          "time": "20210820 21:50"
        },
        {
          "id": "MDRW2100003321.1.12",
          "form": "안타깝다 나같아서 ㅋㅋ",
          "original_form": "안타깝다 나같아서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 21:51"
        },
        {
          "id": "MDRW2100003321.1.13",
          "form": "그래서 유튜브를 많이 볼수 밖에",
          "original_form": "그래서 유튜브를 많이 볼수 밖에",
