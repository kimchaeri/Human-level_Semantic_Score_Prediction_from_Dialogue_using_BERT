      "utterance": [
        {
          "id": "MDRW2100010924.1.1",
          "form": "네 키우죠!",
          "original_form": "네 키우죠!",
          "speaker_id": "2",
          "time": "20210823 23:00"
        },
        {
          "id": "MDRW2100010924.1.2",
          "form": "키우는 더ㅜ힘들듯",
          "original_form": "키우는 더ㅜ힘들듯",
          "speaker_id": "1",
          "time": "20210823 23:07"
        },
        {
          "id": "MDRW2100010924.1.3",
          "form": "네 키우는거 참 힘들긴하죠",
          "original_form": "네 키우는거 참 힘들긴하죠",
          "speaker_id": "2",
          "time": "20210823 23:08"
        },
        {
          "id": "MDRW2100010924.1.4",
          "form": "저도 케어가 걱정이에요",
          "original_form": "저도 케어가 걱정이에요",
          "speaker_id": "1",
          "time": "20210823 23:10"
        },
        {
          "id": "MDRW2100010924.1.5",
          "form": "맞아요 케어가 힘들죠 ㅠㅠ",
          "original_form": "맞아요 케어가 힘들죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 23:48"
        },
        {
          "id": "MDRW2100010924.1.6",
          "form": "시간도 없잖아요",
          "original_form": "시간도 없잖아요",
          "speaker_id": "1",
          "time": "20210824 12:01"
        },
        {
          "id": "MDRW2100010924.1.7",
          "form": "시간이 없는건 아니지만",
          "original_form": "시간이 없는건 아니지만",
          "speaker_id": "2",
          "time": "20210824 12:02"
        },
        {
          "id": "MDRW2100010924.1.8",
          "form": "많이들긴하죠",
          "original_form": "많이들긴하죠",
          "speaker_id": "2",
          "time": "20210824 12:02"
        },
        {
          "id": "MDRW2100010924.1.9",
          "form": "네 관심이",
          "original_form": "네 관심이",
          "speaker_id": "1",
          "time": "20210824 12:37"
        },
        {
          "id": "MDRW2100010924.1.10",
          "form": "관심도 줘야하구요 맞아용",
          "original_form": "관심도 줘야하구요 맞아용",
          "speaker_id": "2",
          "time": "20210824 12:48"
        },
        {
          "id": "MDRW2100010924.1.11",
          "form": "나만 바라보죠",
          "original_form": "나만 바라보죠",
          "speaker_id": "1",
          "time": "20210824 12:49"
        },
        {
          "id": "MDRW2100010924.1.12",
          "form": "고양이가 요세 간식달라할때 저만",
          "original_form": "고양이가 요세 간식달라할때 저만",
          "speaker_id": "2",
          "time": "20210824 12:52"
        },
        {
          "id": "MDRW2100010924.1.13",
          "form": "뚫어지게 보는게 생각나네요 ㅎㅎ",
          "original_form": "뚫어지게 보는게 생각나네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 12:52"
        },
        {
          "id": "MDRW2100010924.1.14",
          "form": "ㅋㅋ 귀요미",
          "original_form": "ㅋㅋ 귀요미",
          "speaker_id": "1",
          "time": "20210824 12:52"
        },
        {
          "id": "MDRW2100010924.1.15",
          "form": "맞아요 ㅠㅠ 귀여워서",
          "original_form": "맞아요 ㅠㅠ 귀여워서",
          "speaker_id": "2",
          "time": "20210824 12:53"
        },
        {
          "id": "MDRW2100010924.1.16",
          "form": "안줄수가없어요",
          "original_form": "안줄수가없어요",
          "speaker_id": "2",
          "time": "20210824 12:54"
        },
        {
          "id": "MDRW2100010924.1.17",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "1",
          "time": "20210824 12:54"
        }
      ]
