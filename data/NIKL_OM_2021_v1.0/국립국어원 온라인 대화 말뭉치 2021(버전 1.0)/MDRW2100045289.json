      "utterance": [
        {
          "id": "MDRW2100045289.1.1",
          "form": "식사류보다 과일이 더 어려운 것 같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "식사류보다 과일이 더 어려운 것 같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045289.1.2",
          "form": "다시 과일 맞춰보실래요? ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "다시 과일 맞춰보실래요? ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045289.1.3",
          "form": "넵 좋아요 ㅋㅋㅋ",
          "original_form": "넵 좋아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045289.1.4",
          "form": "과일 생각했습니다 후후",
          "original_form": "과일 생각했습니다 후후",
          "speaker_id": "2",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045289.1.5",
          "form": "마트에서 흔하게 볼 수 있나요?",
          "original_form": "마트에서 흔하게 볼 수 있나요?",
          "speaker_id": "1",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045289.1.6",
          "form": "네! 흔해요",
          "original_form": "네! 흔해요",
          "speaker_id": "2",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045289.1.7",
          "form": "박스로 팔아요",
          "original_form": "박스로 팔아요",
          "speaker_id": "2",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045289.1.8",
          "form": "나무에서 열리나요?",
          "original_form": "나무에서 열리나요?",
          "speaker_id": "1",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045289.1.9",
          "form": "네! 나무에서 열립니다",
          "original_form": "네! 나무에서 열립니다",
          "speaker_id": "2",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045289.1.10",
          "form": "씨앗이 한개인가요?",
          "original_form": "씨앗이 한개인가요?",
          "speaker_id": "1",
          "time": "20210908 01:43"
        },
        {
          "id": "MDRW2100045289.1.11",
          "form": "아니요 씨앗이 많은 과일이예요",
          "original_form": "아니요 씨앗이 많은 과일이예요",
          "speaker_id": "2",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045289.1.12",
          "form": "노란색인가요?",
          "original_form": "노란색인가요?",
          "speaker_id": "1",
          "time": "20210908 01:44"
        },
        {
          "id": "MDRW2100045289.1.13",
          "form": "아니요 안 노래요!",
          "original_form": "아니요 안 노래요!",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045289.1.14",
          "form": "노란 순간이 한 번도 없는 과일이예요",
          "original_form": "노란 순간이 한 번도 없는 과일이예요",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045289.1.15",
          "form": "빨간색인가요?",
          "original_form": "빨간색인가요?",
          "speaker_id": "1",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045289.1.16",
          "form": "아니요! 빨간 순간도 전혀 없어요",
          "original_form": "아니요! 빨간 순간도 전혀 없어요",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045289.1.17",
          "form": "오잉 ㅋㅋㅋㅋㅋ",
          "original_form": "오잉 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045289.1.18",
          "form": "초록색인가요?",
          "original_form": "초록색인가요?",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045289.1.19",
          "form": "아 초록빛인 시기는 있어요",
          "original_form": "아 초록빛인 시기는 있어요",
          "speaker_id": "2",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045289.1.20",
          "form": "빨간색 노란색과는 거리가 먼~~~ 과일",
          "original_form": "빨간색 노란색과는 거리가 먼~~~ 과일",
          "speaker_id": "2",
          "time": "20210908 01:47"
        },
        {
          "id": "MDRW2100045289.1.21",
          "form": "덜 익으면 초록색이예요",
          "original_form": "덜 익으면 초록색이예요",
          "speaker_id": "2",
          "time": "20210908 01:47"
        }
