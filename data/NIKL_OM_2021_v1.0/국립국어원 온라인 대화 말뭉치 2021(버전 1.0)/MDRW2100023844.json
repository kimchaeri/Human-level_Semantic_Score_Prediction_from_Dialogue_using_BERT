      "utterance": [
        {
          "id": "MDRW2100023844.1.1",
          "form": "나 아직",
          "original_form": "나 아직",
          "speaker_id": "2",
          "time": "20210830 18:44"
        },
        {
          "id": "MDRW2100023844.1.2",
          "form": "근데 컵라면 먹을듯",
          "original_form": "근데 컵라면 먹을듯",
          "speaker_id": "2",
          "time": "20210830 18:44"
        },
        {
          "id": "MDRW2100023844.1.3",
          "form": "컵라면 어떤거?",
          "original_form": "컵라면 어떤거?",
          "speaker_id": "1",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100023844.1.4",
          "form": "나 그 스파게티 좋아해",
          "original_form": "나 그 스파게티 좋아해",
          "speaker_id": "2",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100023844.1.5",
          "form": "콕콕콕 스파게티",
          "original_form": "콕콕콕 스파게티",
          "speaker_id": "2",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100023844.1.6",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100023844.1.7",
          "form": "스파게티 범벅인가?",
          "original_form": "스파게티 범벅인가?",
          "speaker_id": "1",
          "time": "20210830 18:46"
        },
        {
          "id": "MDRW2100023844.1.8",
          "form": "이름은 그냥 스파게티일걸?",
          "original_form": "이름은 그냥 스파게티일걸?",
          "speaker_id": "2",
          "time": "20210830 18:47"
        },
        {
          "id": "MDRW2100023844.1.9",
          "form": "근데 반찬은 해야돼ㅎ",
          "original_form": "근데 반찬은 해야돼ㅎ",
          "speaker_id": "2",
          "time": "20210830 18:47"
        },
        {
          "id": "MDRW2100023844.1.10",
          "form": "아 그래?",
          "original_form": "아 그래?",
          "speaker_id": "1",
          "time": "20210830 18:47"
        },
        {
          "id": "MDRW2100023844.1.11",
          "form": "짜장만 짜장범벅인가보네",
          "original_form": "짜장만 짜장범벅인가보네",
          "speaker_id": "1",
          "time": "20210830 18:47"
        },
        {
          "id": "MDRW2100023844.1.12",
          "form": "짜장은 안먹어봐서",
          "original_form": "짜장은 안먹어봐서",
          "speaker_id": "2",
          "time": "20210830 18:47"
        },
        {
          "id": "MDRW2100023844.1.13",
          "form": "짜장은 짜파게티만 먹어",
          "original_form": "짜장은 짜파게티만 먹어",
          "speaker_id": "2",
          "time": "20210830 18:47"
        },
        {
          "id": "MDRW2100023844.1.14",
          "form": "여튼 반찬을ᆢ",
          "original_form": "여튼 반찬을ᆢ",
          "speaker_id": "2",
          "time": "20210830 18:48"
        },
        {
          "id": "MDRW2100023844.1.15",
          "form": "나도 짜장 안먹어봤는데",
          "original_form": "나도 짜장 안먹어봤는데",
          "speaker_id": "1",
          "time": "20210830 18:48"
        },
        {
          "id": "MDRW2100023844.1.16",
          "form": "짜장범벅 팬 많더라구",
          "original_form": "짜장범벅 팬 많더라구",
          "speaker_id": "1",
          "time": "20210830 18:48"
        },
        {
          "id": "MDRW2100023844.1.17",
          "form": "꽈리고투",
          "original_form": "꽈리고투",
          "speaker_id": "2",
          "time": "20210830 18:48"
        },
        {
          "id": "MDRW2100023844.1.18",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "2",
          "time": "20210830 18:48"
        },
        {
          "id": "MDRW2100023844.1.19",
          "form": "꽈리고chㅜ돼지고기 볶아서",
          "original_form": "꽈리고chㅜ돼지고기 볶아서",
          "speaker_id": "2",
          "time": "20210830 18:48"
        },
        {
          "id": "MDRW2100023844.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:48"
        },
        {
          "id": "MDRW2100023844.1.21",
          "form": "반찬을 왜??",
          "original_form": "반찬을 왜??",
          "speaker_id": "1",
          "time": "20210830 18:48"
        },
        {
          "id": "MDRW2100023844.1.22",
          "form": "성냥개비가 오면",
          "original_form": "성냥개비가 오면",
          "speaker_id": "2",
          "time": "20210830 18:49"
        },
        {
          "id": "MDRW2100023844.1.23",
          "form": "밥달라그래",
          "original_form": "밥달라그래",
          "speaker_id": "2",
          "time": "20210830 18:49"
        },
        {
          "id": "MDRW2100023844.1.24",
          "form": "저런.....",
          "original_form": "저런.....",
          "speaker_id": "1",
          "time": "20210830 18:49"
        },
        {
          "id": "MDRW2100023844.1.25",
          "form": "요새 밥을 안먹고와",
          "original_form": "요새 밥을 안먹고와",
          "speaker_id": "2",
          "time": "20210830 18:49"
        },
        {
          "id": "MDRW2100023844.1.26",
          "form": "같이 먹을사람이 업대",
          "original_form": "같이 먹을사람이 업대",
          "speaker_id": "2",
          "time": "20210830 18:49"
        },
        {
          "id": "MDRW2100023844.1.27",
          "form": "ㅇ ㅏ 정말",
          "original_form": "ㅇ ㅏ 정말",
          "speaker_id": "1",
          "time": "20210830 18:49"
        },
        {
          "id": "MDRW2100023844.1.28",
          "form": "젤 싫다...",
          "original_form": "젤 싫다...",
          "speaker_id": "1",
          "time": "20210830 18:49"
        },
        {
          "id": "MDRW2100023844.1.29",
          "form": "여튼 그래서",
          "original_form": "여튼 그래서",
          "speaker_id": "2",
          "time": "20210830 18:49"
        },
        {
          "id": "MDRW2100023844.1.30",
          "form": "꽈리에 돼지고기 굴소스 팽이버섯 해서",
          "original_form": "꽈리에 돼지고기 굴소스 팽이버섯 해서",
          "speaker_id": "2",
          "time": "20210830 18:49"
        },
        {
          "id": "MDRW2100023844.1.31",
          "form": "볶을거야",
          "original_form": "볶을거야",
          "speaker_id": "2",
          "time": "20210830 18:49"
        },
        {
          "id": "MDRW2100023844.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 점점",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 점점",
          "speaker_id": "1",
          "time": "20210830 18:50"
        },
        {
          "id": "MDRW2100023844.1.33",
          "form": "할 수있는 요리가 늘어나는듯",
          "original_form": "할 수있는 요리가 늘어나는듯",
          "speaker_id": "1",
          "time": "20210830 18:50"
        },
        {
          "id": "MDRW2100023844.1.34",
          "form": "아니 이건 걍",
          "original_form": "아니 이건 걍",
          "speaker_id": "2",
          "time": "20210830 18:50"
        },
        {
          "id": "MDRW2100023844.1.35",
          "form": "잇는 재료 냅다 팬에 들이붓고 볶기잖아ㅋㅋㅋㅋㅋㅋ",
          "original_form": "잇는 재료 냅다 팬에 들이붓고 볶기잖아ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:50"
        },
        {
          "id": "MDRW2100023844.1.36",
          "form": "그래도 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그래도 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:50"
        },
        {
          "id": "MDRW2100023844.1.37",
          "form": "난 걍",
          "original_form": "난 걍",
          "speaker_id": "1",
          "time": "20210830 18:50"
        },
        {
          "id": "MDRW2100023844.1.38",
          "form": "만두를 사먹기로 함....",
          "original_form": "만두를 사먹기로 함....",
          "speaker_id": "1",
          "time": "20210830 18:50"
        },
        {
          "id": "MDRW2100023844.1.39",
          "form": "오 만두",
          "original_form": "오 만두",
          "speaker_id": "2",
          "time": "20210830 18:50"
        },
        {
          "id": "MDRW2100023844.1.40",
          "form": "만두 맛있겠다",
          "original_form": "만두 맛있겠다",
          "speaker_id": "2",
          "time": "20210830 18:51"
        },
        {
          "id": "MDRW2100023844.1.41",
          "form": "ㅇㅇ 좀 기대중이야",
          "original_form": "ㅇㅇ 좀 기대중이야",
          "speaker_id": "1",
          "time": "20210830 18:51"
        },
        {
          "id": "MDRW2100023844.1.42",
          "form": "나도 만두먹고싶다.....",
          "original_form": "나도 만두먹고싶다.....",
          "speaker_id": "2",
          "time": "20210830 18:51"
        },
        {
          "id": "MDRW2100023844.1.43",
          "form": "찾아보니까 평이 괜찮아서",
          "original_form": "찾아보니까 평이 괜찮아서",
          "speaker_id": "1",
          "time": "20210830 18:51"
        },
        {
          "id": "MDRW2100023844.1.44",
          "form": "무슨만두 먹어?",
          "original_form": "무슨만두 먹어?",
          "speaker_id": "2",
          "time": "20210830 18:51"
        },
        {
          "id": "MDRW2100023844.1.45",
          "form": "고기만두 시킴",
          "original_form": "고기만두 시킴",
          "speaker_id": "1",
          "time": "20210830 18:51"
        },
        {
          "id": "MDRW2100023844.1.46",
          "form": "맛있겠다.......",
          "original_form": "맛있겠다.......",
          "speaker_id": "2",
          "time": "20210830 18:51"
        }
      ]
    }
