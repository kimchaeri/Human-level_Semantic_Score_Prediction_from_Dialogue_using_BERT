          "time": "20210901 17:56"
        },
        {
          "id": "MDRW2100025912.1.3",
          "form": "빨리퇴근하고싶은디왜아직",
          "original_form": "빨리퇴근하고싶은디왜아직",
          "speaker_id": "2",
          "time": "20210901 17:56"
        },
        {
          "id": "MDRW2100025912.1.4",
          "form": "시간이안댔쪄?",
          "original_form": "시간이안댔쪄?",
          "speaker_id": "2",
          "time": "20210901 17:56"
        },
        {
          "id": "MDRW2100025912.1.5",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100025912.1.6",
          "form": "밥 존맛탱",
          "original_form": "밥 존맛탱",
          "speaker_id": "1",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100025912.1.7",
          "form": "내일도 출근이라니 끔찍하네요",
          "original_form": "내일도 출근이라니 끔찍하네요",
          "speaker_id": "1",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100025912.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ 그러니까요.. 벌써출근을해버림요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그러니까요.. 벌써출근을해버림요",
          "speaker_id": "2",
          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100025912.1.9",
          "form": "집가고시품",
          "original_form": "집가고시품",
          "speaker_id": "2",
          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100025912.1.10",
          "form": "왕 가고시픔",
          "original_form": "왕 가고시픔",
          "speaker_id": "1",
          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100025912.1.11",
          "form": "오늘 교육해야해요",
          "original_form": "오늘 교육해야해요",
          "speaker_id": "1",
          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100025912.1.12",
          "form": "교육진짜 짜증나져 저는가만히 앉아서 있는것을못하겠어여",
          "original_form": "교육진짜 짜증나져 저는가만히 앉아서 있는것을못하겠어여",
          "speaker_id": "2",
          "time": "20210902 10:55"
        },
        {
          "id": "MDRW2100025912.1.13",
          "form": "저는 그냥 켜놓고 왔다갔다 할거에요",
          "original_form": "저는 그냥 켜놓고 왔다갔다 할거에요",
          "speaker_id": "1",
          "time": "20210902 10:57"
        },
        {
          "id": "MDRW2100025912.1.14",
          "form": "ㅋㅋㅋ 뭐있나요 대충하면대지",
          "original_form": "ㅋㅋㅋ 뭐있나요 대충하면대지",
          "speaker_id": "2",
          "time": "20210902 11:01"
        },
        {
          "id": "MDRW2100025912.1.15",
          "form": "급여는 언제 나올까요",
          "original_form": "급여는 언제 나올까요",
          "speaker_id": "1",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100025912.1.16",
          "form": "중간에 상여금이 먼저 나오겠쪄",
          "original_form": "중간에 상여금이 먼저 나오겠쪄",
          "speaker_id": "2",
          "time": "20210902 11:05"
        },
        {
          "id": "MDRW2100025912.1.17",
          "form": "벌써 즐거워여ㅋㅋㅋ",
          "original_form": "벌써 즐거워여ㅋㅋㅋ",
