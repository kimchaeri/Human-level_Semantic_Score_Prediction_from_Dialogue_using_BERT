      "utterance": [
        {
          "id": "MDRW2100046192.1.1",
          "form": "맞다이뜨자는대산ㄴㅔ",
          "original_form": "맞다이뜨자는대산ㄴㅔ",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100046192.1.2",
          "form": "지금이순간 행복하자 영원하자가 아니라구요",
          "original_form": "지금이순간 행복하자 영원하자가 아니라구요",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046192.1.3",
          "form": "왜 다들",
          "original_form": "왜 다들",
          "speaker_id": "1",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046192.1.4",
          "form": "부르지말라고요",
          "original_form": "부르지말라고요",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046192.1.5",
          "form": "결혼식 축가로 그걸..",
          "original_form": "결혼식 축가로 그걸..",
          "speaker_id": "1",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046192.1.6",
          "form": "이거하면결혼파탄나라는거야뭐야",
          "original_form": "이거하면결혼파탄나라는거야뭐야",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046192.1.7",
          "form": "ㅋㄱㅋㄱㅋㅋㅋㅋㄱㅋㄱㅋㄱㄱ",
          "original_form": "ㅋㄱㅋㄱㅋㅋㅋㅋㄱㅋㄱㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046192.1.8",
          "form": "결국자기가 만든인격이",
          "original_form": "결국자기가 만든인격이",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046192.1.9",
          "form": "난 네안에영원히산다며",
          "original_form": "난 네안에영원히산다며",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046192.1.10",
          "form": "괴롭히는건데",
          "original_form": "괴롭히는건데",
          "speaker_id": "2",
          "time": "20210908 11:04"
        },
        {
          "id": "MDRW2100046192.1.11",
          "form": "끔-찍",
          "original_form": "끔-찍",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046192.1.12",
          "form": "대놓고",
          "original_form": "대놓고",
          "speaker_id": "1",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046192.1.13",
          "form": "지옥 언급하자나",
          "original_form": "지옥 언급하자나",
          "speaker_id": "1",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046192.1.14",
          "form": "지킬 불쌍..",
          "original_form": "지킬 불쌍..",
          "speaker_id": "1",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046192.1.15",
          "form": "뭐 업보긴 하지만",
          "original_form": "뭐 업보긴 하지만",
          "speaker_id": "1",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046192.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046192.1.17",
          "form": "널죽이고",
          "original_form": "널죽이고",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046192.1.18",
          "form": "나도죽겠다",
          "original_form": "나도죽겠다",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046192.1.19",
          "form": "그럼지옥에서만나자 지킬",
          "original_form": "그럼지옥에서만나자 지킬",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046192.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:05"
        },
        {
          "id": "MDRW2100046192.1.21",
          "form": "아니글게 누ㄱ지몸에",
          "original_form": "아니글게 누ㄱ지몸에",
