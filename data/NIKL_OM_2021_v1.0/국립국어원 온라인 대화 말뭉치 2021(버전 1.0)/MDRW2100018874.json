      "utterance": [
        {
          "id": "MDRW2100018874.1.1",
          "form": "최근 중고거래 하신적있나요?",
          "original_form": "최근 중고거래 하신적있나요?",
          "speaker_id": "2",
          "time": "20210827 13:13"
        },
        {
          "id": "MDRW2100018874.1.2",
          "form": "네 저 당근 좋아해요",
          "original_form": "네 저 당근 좋아해요",
          "speaker_id": "1",
          "time": "20210827 13:14"
        },
        {
          "id": "MDRW2100018874.1.3",
          "form": "얼마전엔 고양이 물품을 사왔답니다ㅎㅎ",
          "original_form": "얼마전엔 고양이 물품을 사왔답니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 13:15"
        },
        {
          "id": "MDRW2100018874.1.4",
          "form": "오~ 저두 당근으로 얼마전에 에그팬 구매했답니다 ㅋㅋㅋ",
          "original_form": "오~ 저두 당근으로 얼마전에 에그팬 구매했답니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:33"
        },
        {
          "id": "MDRW2100018874.1.5",
          "form": "오오 당근 좋죠ㅋㅋㅋ 중고나라보다 편한거같아요",
          "original_form": "오오 당근 좋죠ㅋㅋㅋ 중고나라보다 편한거같아요",
          "speaker_id": "1",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100018874.1.6",
          "form": "에그팬은 써보셨나요?",
          "original_form": "에그팬은 써보셨나요?",
          "speaker_id": "1",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100018874.1.7",
          "form": "네 ㅋㅋ 오늘썼어요! 새제품마냥 좋더라구요~",
          "original_form": "네 ㅋㅋ 오늘썼어요! 새제품마냥 좋더라구요~",
          "speaker_id": "2",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100018874.1.8",
          "form": "프라이는 이쁘게 만들어지던가요?",
          "original_form": "프라이는 이쁘게 만들어지던가요?",
          "speaker_id": "1",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100018874.1.9",
          "form": "저도 급 뽐뿌오네요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 급 뽐뿌오네요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100018874.1.10",
          "form": "네 ㅋㅋㅋ 딱 프라이하기 좋은사이즈라 편하고 좋았어요",
          "original_form": "네 ㅋㅋㅋ 딱 프라이하기 좋은사이즈라 편하고 좋았어요",
          "speaker_id": "2",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100018874.1.11",
          "form": "저는 당근에 샴푸 파는 중인데 아무도 안사시네요",
          "original_form": "저는 당근에 샴푸 파는 중인데 아무도 안사시네요",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100018874.1.12",
          "form": "앜ㅋㅋㅋㅋ이런.. 샴푸살생각은 안해봣어요 ㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ이런.. 샴푸살생각은 안해봣어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100018874.1.13",
          "form": "추석되기 전에 팔고 싶네요",
          "original_form": "추석되기 전에 팔고 싶네요",
          "speaker_id": "1",
          "time": "20210827 15:00"
        },
        {
          "id": "MDRW2100018874.1.14",
          "form": "ㅋㅋㅋㅋ 명절선물로 사가는분이 있길 바래요 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 명절선물로 사가는분이 있길 바래요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:01"
        },
        {
          "id": "MDRW2100018874.1.15",
          "form": "맞아요 명절선물로 딱인데 사가시길..ㅠㅠ",
