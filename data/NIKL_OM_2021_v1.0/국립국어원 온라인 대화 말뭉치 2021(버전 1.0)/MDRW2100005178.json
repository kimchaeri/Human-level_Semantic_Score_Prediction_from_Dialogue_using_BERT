      "utterance": [
        {
          "id": "MDRW2100005178.1.1",
          "form": "잠깐 학교 다니세요...?",
          "original_form": "잠깐 학교 다니세요...?",
          "speaker_id": "2",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005178.1.2",
          "form": "네 학점은 다채우고 졸업 예정자랍니다",
          "original_form": "네 학점은 다채우고 졸업 예정자랍니다",
          "speaker_id": "1",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005178.1.3",
          "form": "휴.. 졸업하고싶어요",
          "original_form": "휴.. 졸업하고싶어요",
          "speaker_id": "1",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005178.1.4",
          "form": "지긋지긋한 학교!",
          "original_form": "지긋지긋한 학교!",
          "speaker_id": "1",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005178.1.5",
          "form": "댑악 전 졸업한지 어느덧 1년",
          "original_form": "댑악 전 졸업한지 어느덧 1년",
          "speaker_id": "2",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005178.1.6",
          "form": "학교압 연어덥밥집이 그리워요",
          "original_form": "학교압 연어덥밥집이 그리워요",
          "speaker_id": "2",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100005178.1.7",
          "form": "연어초밥집도",
          "original_form": "연어초밥집도",
          "speaker_id": "2",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100005178.1.8",
          "form": "연어 좋아하시는군요 학교근처에 맛집이 많았나봐요",
          "original_form": "연어 좋아하시는군요 학교근처에 맛집이 많았나봐요",
          "speaker_id": "1",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100005178.1.9",
          "form": "연어를사랑해요....맛집.....하....학생돈으로는 조금 부담되는 맛집이라... 자주 못 갔어요....",
          "original_form": "연어를사랑해요....맛집.....하....학생돈으로는 조금 부담되는 맛집이라... 자주 못 갔어요....",
          "speaker_id": "2",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005178.1.10",
          "form": "그게 너무 슬퍼요...",
          "original_form": "그게 너무 슬퍼요...",
          "speaker_id": "2",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005178.1.11",
          "form": "자주갈걸....",
          "original_form": "자주갈걸....",
          "speaker_id": "2",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005178.1.12",
          "form": "ㅜㅜㅡㅠ 저도 학교주변에서 자취할때",
          "original_form": "ㅜㅜㅡㅠ 저도 학교주변에서 자취할때",
          "speaker_id": "1",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100005178.1.13",
          "form": "맛있는 맛집들과 동네빵집이 있었는데",
          "original_form": "맛있는 맛집들과 동네빵집이 있었는데",
          "speaker_id": "1",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100005178.1.14",
          "form": "못먹게되어서 아쉬워요..",
          "original_form": "못먹게되어서 아쉬워요..",
          "speaker_id": "1",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100005178.1.15",
          "form": "name1님은 동아리 활동하셨나요??",
          "original_form": "&name1&님은 동아리 활동하셨나요??",
          "speaker_id": "2",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005178.1.16",
          "form": "아니요! 동아리같은건 사람들하고 엮이니깝",
          "original_form": "아니요! 동아리같은건 사람들하고 엮이니깝",
          "speaker_id": "1",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005178.1.17",
          "form": "까 피곤해서 안했어요",
          "original_form": "까 피곤해서 안했어요",
          "speaker_id": "1",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005178.1.18",
          "form": "ㅋㅋㅋㅋ현명하시내요....저도에요...",
          "original_form": "ㅋㅋㅋㅋ현명하시내요....저도에요...",
          "speaker_id": "2",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005178.1.19",
          "form": "아무랴도 사람이엮이면..피곤하죠...",
          "original_form": "아무랴도 사람이엮이면..피곤하죠...",
          "speaker_id": "2",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005178.1.20",
          "form": "그쵸 귀찮아요.. 학교끝나면 집에가서 하고싶은것도 많은데",
          "original_form": "그쵸 귀찮아요.. 학교끝나면 집에가서 하고싶은것도 많은데",
          "speaker_id": "1",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005178.1.21",
          "form": "그리고 학교끝나면 피곤해서 뭐하기 싫다구요",
          "original_form": "그리고 학교끝나면 피곤해서 뭐하기 싫다구요",
          "speaker_id": "1",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005178.1.22",
          "form": "마자요마자 학교끝나면...영혼이..빨려...",
          "original_form": "마자요마자 학교끝나면...영혼이..빨려...",
          "speaker_id": "2",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005178.1.23",
          "form": "집에와서 쉬어야만....",
          "original_form": "집에와서 쉬어야만....",
          "speaker_id": "1",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005178.1.24",
          "form": "학교에 고양이 많았나요",
          "original_form": "학교에 고양이 많았나요",
          "speaker_id": "1",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005178.1.25",
          "form": "고영 제 건물쪽에는 없었어요 근데 중앙으로가면",
          "original_form": "고영 제 건물쪽에는 없었어요 근데 중앙으로가면",
          "speaker_id": "2",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005178.1.26",
          "form": "너구리?수달??",
          "original_form": "너구리?수달??",
          "speaker_id": "2",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005178.1.27",
          "form": "아무튼 뭐자주나왔다더라고요",
          "original_form": "아무튼 뭐자주나왔다더라고요",
          "speaker_id": "2",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005178.1.28",
          "form": "헐 너무 귀엽네요... 부러워요ㅜㅠ",
          "original_form": "헐 너무 귀엽네요... 부러워요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005178.1.29",
          "form": "너굴맨 수달...",
          "original_form": "너굴맨 수달...",
          "speaker_id": "1",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005178.1.30",
          "form": "너굴맨!가족이!!!",
          "original_form": "너굴맨!가족이!!!",
          "speaker_id": "2",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005178.1.31",
          "form": "학교에 있닥우요",
          "original_form": "학교에 있닥우요",
          "speaker_id": "1",
          "time": "20210821 22:55"
        }
      ]
