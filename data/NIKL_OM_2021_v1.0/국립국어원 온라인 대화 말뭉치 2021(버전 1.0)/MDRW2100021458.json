      "utterance": [
        {
          "id": "MDRW2100021458.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 19:14"
        },
        {
          "id": "MDRW2100021458.1.2",
          "form": "반가워요",
          "original_form": "반가워요",
          "speaker_id": "2",
          "time": "20210828 19:17"
        },
        {
          "id": "MDRW2100021458.1.3",
          "form": "식사하셨나요??",
          "original_form": "식사하셨나요??",
          "speaker_id": "1",
          "time": "20210828 19:17"
        },
        {
          "id": "MDRW2100021458.1.4",
          "form": "네 간단히 과일이랑 귀리쉐이크 먹었어요",
          "original_form": "네 간단히 과일이랑 귀리쉐이크 먹었어요",
          "speaker_id": "2",
          "time": "20210828 19:18"
        },
        {
          "id": "MDRW2100021458.1.5",
          "form": "건강식으로 드셨네요!",
          "original_form": "건강식으로 드셨네요!",
          "speaker_id": "1",
          "time": "20210828 19:19"
        },
        {
          "id": "MDRW2100021458.1.6",
          "form": "아 건강식인가요 ㅎㅎ",
          "original_form": "아 건강식인가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100021458.1.7",
          "form": "네 ㅋㅋㅋ 전 귀리 안 먹어봤어요",
          "original_form": "네 ㅋㅋㅋ 전 귀리 안 먹어봤어요",
          "speaker_id": "1",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100021458.1.8",
          "form": "어떤가요?",
          "original_form": "어떤가요?",
          "speaker_id": "1",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100021458.1.9",
          "form": "오트밀은 먹지 마세요 종이를 물 말아서 먹는 맛이에요",
          "original_form": "오트밀은 먹지 마세요 종이를 물 말아서 먹는 맛이에요",
          "speaker_id": "2",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100021458.1.10",
          "form": "헉 그래요? ㅋㅋㅋㅋㅋ",
          "original_form": "헉 그래요? ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100021458.1.11",
          "form": "아니대체 어떻게하면 그런 맛이....",
          "original_form": "아니대체 어떻게하면 그런 맛이....",
          "speaker_id": "1",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100021458.1.12",
          "form": "드시려면 우유 두유 요거트 과일 토핑 등이 있어야 해요",
          "original_form": "드시려면 우유 두유 요거트 과일 토핑 등이 있어야 해요",
          "speaker_id": "2",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100021458.1.13",
          "form": "아 그런게 필수군요",
          "original_form": "아 그런게 필수군요",
          "speaker_id": "1",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100021458.1.14",
          "form": "건강위해서 드시는 건가요?",
          "original_form": "건강위해서 드시는 건가요?",
          "speaker_id": "1",
          "time": "20210828 19:28"
        },
        {
          "id": "MDRW2100021458.1.15",
          "form": "네 귀리가 10대 푸드 중 유일한 곡물이에요",
          "original_form": "네 귀리가 10대 푸드 중 유일한 곡물이에요",
          "speaker_id": "2",
          "time": "20210828 19:29"
        },
        {
          "id": "MDRW2100021458.1.16",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "1",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100021458.1.17",
          "form": "다이어트할때 많이 드시더라구요",
          "original_form": "다이어트할때 많이 드시더라구요",
          "speaker_id": "1",
          "time": "20210828 19:30"
        }
