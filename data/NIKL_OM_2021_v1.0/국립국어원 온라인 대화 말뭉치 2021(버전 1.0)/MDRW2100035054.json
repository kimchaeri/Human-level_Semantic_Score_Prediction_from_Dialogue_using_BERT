        {
          "id": "MDRW2100035054.1.1",
          "form": "코로나가 끝나면 여행을 가고 싶어",
          "original_form": "코로나가 끝나면 여행을 가고 싶어",
          "speaker_id": "2",
          "time": "20210829 01:16"
        },
        {
          "id": "MDRW2100035054.1.2",
          "form": "어디가 가고 싶니",
          "original_form": "어디가 가고 싶니",
          "speaker_id": "1",
          "time": "20210829 01:16"
        },
        {
          "id": "MDRW2100035054.1.3",
          "form": "중국을 가고 싶어",
          "original_form": "중국을 가고 싶어",
          "speaker_id": "2",
          "time": "20210829 01:16"
        },
        {
          "id": "MDRW2100035054.1.4",
          "form": "의외인데",
          "original_form": "의외인데",
          "speaker_id": "1",
          "time": "20210829 01:16"
        },
        {
          "id": "MDRW2100035054.1.5",
          "form": "나도 예전에는 중국을 좋아하지 않았어",
          "original_form": "나도 예전에는 중국을 좋아하지 않았어",
          "speaker_id": "2",
          "time": "20210829 01:16"
        },
        {
          "id": "MDRW2100035054.1.6",
          "form": "지금은 왜 가고 싶은데",
          "original_form": "지금은 왜 가고 싶은데",
          "speaker_id": "1",
          "time": "20210829 01:16"
        },
        {
          "id": "MDRW2100035054.1.7",
          "form": "팬더를 보고 싶어",
          "original_form": "팬더를 보고 싶어",
          "speaker_id": "2",
          "time": "20210829 01:17"
        },
        {
          "id": "MDRW2100035054.1.8",
          "form": "자이언트팬더 말하는 거니",
          "original_form": "자이언트팬더 말하는 거니",
          "speaker_id": "1",
          "time": "20210829 01:17"
        },
        {
          "id": "MDRW2100035054.1.9",
          "form": "푸 바오를 보면서 팬더에 매력을 깨달았어",
          "original_form": "푸 바오를 보면서 팬더에 매력을 깨달았어",
          "speaker_id": "2",
          "time": "20210829 01:17"
        },
        {
          "id": "MDRW2100035054.1.10",
          "form": "팬더가 진짜 귀엽긴 해",
          "original_form": "팬더가 진짜 귀엽긴 해",
          "speaker_id": "1",
          "time": "20210829 01:17"
        },
        {
          "id": "MDRW2100035054.1.11",
          "form": "팬더는 중국에 아주 많이 있어",
          "original_form": "팬더는 중국에 아주 많이 있어",
          "speaker_id": "2",
          "time": "20210829 01:17"
        },
        {
          "id": "MDRW2100035054.1.12",
          "form": "레서 팬더도 귀엽던데",
          "original_form": "레서 팬더도 귀엽던데",
          "speaker_id": "1",
          "time": "20210829 01:17"
        },
        {
          "id": "MDRW2100035054.1.13",
          "form": "레서팬더는 일본에 많아",
          "original_form": "레서팬더는 일본에 많아",
          "speaker_id": "2",
