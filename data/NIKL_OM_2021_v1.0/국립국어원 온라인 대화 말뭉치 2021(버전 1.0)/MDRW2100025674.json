      "utterance": [
        {
          "id": "MDRW2100025674.1.1",
          "form": "스포츠 경기 뭐 좋아하세요",
          "original_form": "스포츠 경기 뭐 좋아하세요",
          "speaker_id": "2",
          "time": "20210901 15:46"
        },
        {
          "id": "MDRW2100025674.1.2",
          "form": "저요즘 양궁이랑펜싱 보는게 재미있더라구요 ㅠ",
          "original_form": "저요즘 양궁이랑펜싱 보는게 재미있더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 15:47"
        },
        {
          "id": "MDRW2100025674.1.3",
          "form": "올림픽 보셨너여?",
          "original_form": "올림픽 보셨너여?",
          "speaker_id": "1",
          "time": "20210901 15:47"
        },
        {
          "id": "MDRW2100025674.1.4",
          "form": "당연하죠",
          "original_form": "당연하죠",
          "speaker_id": "2",
          "time": "20210901 15:47"
        },
        {
          "id": "MDRW2100025674.1.5",
          "form": "보는 재미로 살았는데",
          "original_form": "보는 재미로 살았는데",
          "speaker_id": "2",
          "time": "20210901 15:47"
        },
        {
          "id": "MDRW2100025674.1.6",
          "form": "높이뛰기 스릴 있더라구요",
          "original_form": "높이뛰기 스릴 있더라구요",
          "speaker_id": "2",
          "time": "20210901 15:48"
        },
        {
          "id": "MDRW2100025674.1.7",
          "form": "전그거 보는데 저가 다 넘어질거같던데",
          "original_form": "전그거 보는데 저가 다 넘어질거같던데",
          "speaker_id": "1",
          "time": "20210901 20:10"
        },
        {
          "id": "MDRW2100025674.1.8",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:10"
        },
        {
          "id": "MDRW2100025674.1.9",
          "form": "맨바닥에서 어찌 그리 점프를 잘할까요 ㅋㅋ",
          "original_form": "맨바닥에서 어찌 그리 점프를 잘할까요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:18"
        },
        {
          "id": "MDRW2100025674.1.10",
          "form": "그러니까요 진짜 운동실력이 남다른가바요",
          "original_form": "그러니까요 진짜 운동실력이 남다른가바요",
          "speaker_id": "1",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100025674.1.11",
          "form": "배구는 일본 이겨서 너무 통쾌했어요",
          "original_form": "배구는 일본 이겨서 너무 통쾌했어요",
          "speaker_id": "2",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100025674.1.12",
          "form": "맞아요 메달못따도 이긴게 더 좋터라구요양궁도 진짜 대박이더라구요",
          "original_form": "맞아요 메달못따도 이긴게 더 좋터라구요양궁도 진짜 대박이더라구요",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100025674.1.13",
          "form": "능력이 너무 부러워욤 안산님",
          "original_form": "능력이 너무 부러워욤 안산님",
          "speaker_id": "2",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100025674.1.14",
          "form": "금메달 많이 따셔서",
          "original_form": "금메달 많이 따셔서",
          "speaker_id": "2",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100025674.1.15",
          "form": "맞아요 차도 받았자나요 ㅠㅠ",
          "original_form": "맞아요 차도 받았자나요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100025674.1.16",
          "form": "그러게요  ㅠㅠ 역시 어렸을때부터 한우물 파야하는가바요",
          "original_form": "그러게요  ㅠㅠ 역시 어렸을때부터 한우물 파야하는가바요",
          "speaker_id": "2",
          "time": "20210901 20:56"
        },
        {
          "id": "MDRW2100025674.1.17",
          "form": "ㅋㅋㅋ그러켔죠?? 저는 무서워서 못팔거같어요 ㅋㅋ",
          "original_form": "ㅋㅋㅋ그러켔죠?? 저는 무서워서 못팔거같어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100025674.1.18",
          "form": "마져요 ㅋㅋㅋ 잘하는 것두 딱히 없고  ㅠㅜ",
          "original_form": "마져요 ㅋㅋㅋ 잘하는 것두 딱히 없고  ㅠㅜ",
          "speaker_id": "2",
