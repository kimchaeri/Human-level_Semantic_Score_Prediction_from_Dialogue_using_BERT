        },
        {
          "id": "MDRW2100011193.1.3",
          "form": "아르바이트 해보신적 있으신가요?",
          "original_form": "아르바이트 해보신적 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 02:20"
        },
        {
          "id": "MDRW2100011193.1.4",
          "form": "네 지금 하는 중이에요",
          "original_form": "네 지금 하는 중이에요",
          "speaker_id": "2",
          "time": "20210825 02:21"
        },
        {
          "id": "MDRW2100011193.1.5",
          "form": "어떤 아르바이트예요?",
          "original_form": "어떤 아르바이트예요?",
          "speaker_id": "1",
          "time": "20210825 02:21"
        },
        {
          "id": "MDRW2100011193.1.6",
          "form": "학원알바요!",
          "original_form": "학원알바요!",
          "speaker_id": "2",
          "time": "20210825 02:22"
        },
        {
          "id": "MDRW2100011193.1.7",
          "form": "적성에 맞으세요?",
          "original_form": "적성에 맞으세요?",
