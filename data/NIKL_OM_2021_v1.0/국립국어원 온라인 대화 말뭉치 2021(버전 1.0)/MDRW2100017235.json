      "utterance": [
        {
          "id": "MDRW2100017235.1.1",
          "form": "요즘 보시는 프로는 있으신가용",
          "original_form": "요즘 보시는 프로는 있으신가용",
          "speaker_id": "1",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100017235.1.2",
          "form": "저 일박이일하고 유튜브봐요",
          "original_form": "저 일박이일하고 유튜브봐요",
          "speaker_id": "2",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100017235.1.3",
          "form": "즐기는 프로있으세요?",
          "original_form": "즐기는 프로있으세요?",
          "speaker_id": "2",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100017235.1.4",
          "form": "요즘 넷플에 체인비데이즈랑 돌싱글즈라는 연애프로봐요 ㅋㅋ",
          "original_form": "요즘 넷플에 체인비데이즈랑 돌싱글즈라는 연애프로봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:13"
        },
        {
          "id": "MDRW2100017235.1.5",
          "form": "일박이일 아직 하는구나 몰랐어요ㅋㅋ",
          "original_form": "일박이일 아직 하는구나 몰랐어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:14"
        },
        {
          "id": "MDRW2100017235.1.6",
          "form": "저도 안보다가 집콕하면서 비지엠처럼 켜놔요",
          "original_form": "저도 안보다가 집콕하면서 비지엠처럼 켜놔요",
          "speaker_id": "2",
          "time": "20210826 18:17"
        },
        {
          "id": "MDRW2100017235.1.7",
          "form": "돌싱 요즘 유명하더라고요",
          "original_form": "돌싱 요즘 유명하더라고요",
          "speaker_id": "2",
          "time": "20210826 18:17"
        },
        {
          "id": "MDRW2100017235.1.8",
          "form": "은근 웃기더라구요 ㅋㅋㅋ",
          "original_form": "은근 웃기더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:47"
        },
        {
          "id": "MDRW2100017235.1.9",
          "form": "넷플로 보면 시간 구애안받으니까 좋은거같아요",
          "original_form": "넷플로 보면 시간 구애안받으니까 좋은거같아요",
          "speaker_id": "2",
          "time": "20210826 18:53"
        },
        {
          "id": "MDRW2100017235.1.10",
          "form": "맞아요 요즘 티비를 거의 안보게 되더라구요",
          "original_form": "맞아요 요즘 티비를 거의 안보게 되더라구요",
          "speaker_id": "1",
          "time": "20210826 19:03"
        },
        {
          "id": "MDRW2100017235.1.11",
          "form": "넷플같은거 쓰세요?",
          "original_form": "넷플같은거 쓰세요?",
          "speaker_id": "1",
          "time": "20210826 19:03"
        },
        {
          "id": "MDRW2100017235.1.12",
          "form": "전 올레티비만 쭉 봐서... 그런데 지인들보니 좀 좋은거같고 그러네요?",
          "original_form": "전 올레티비만 쭉 봐서... 그런데 지인들보니 좀 좋은거같고 그러네요?",
          "speaker_id": "2",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017235.1.13",
          "form": "추천하시나요?",
          "original_form": "추천하시나요?",
          "speaker_id": "2",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017235.1.14",
          "form": "아항 네네 ㅋㅋㅋㅋ 계정 나눠쓰면 좋은거같아요",
          "original_form": "아항 네네 ㅋㅋㅋㅋ 계정 나눠쓰면 좋은거같아요",
          "speaker_id": "1",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017235.1.15",
          "form": "그냥..핸드폰으로도 보고...배속도되고",
          "original_form": "그냥..핸드폰으로도 보고...배속도되고",
          "speaker_id": "1",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100017235.1.16",
          "form": "아 진짜 그렇겠네요 너무 많은 컨텐츠라 선택하기 어려울거 같아요",
          "original_form": "아 진짜 그렇겠네요 너무 많은 컨텐츠라 선택하기 어려울거 같아요",
          "speaker_id": "2",
          "time": "20210826 19:06"
        },
        {
          "id": "MDRW2100017235.1.17",
          "form": "ㅋㅋㅋ 맞아요 그런건 있어요",
          "original_form": "ㅋㅋㅋ 맞아요 그런건 있어요",
          "speaker_id": "1",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017235.1.18",
          "form": "다들 선택하느라 시간다간다잖아요 ㅋㅋ",
          "original_form": "다들 선택하느라 시간다간다잖아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017235.1.19",
          "form": "그 썸네일이라고해야할까요? 그 이미지만 보다 잔다고 하는거 봤어요ㅎㅎ",
          "original_form": "그 썸네일이라고해야할까요? 그 이미지만 보다 잔다고 하는거 봤어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017235.1.20",
          "form": "ㅋㅋㅋㅋ진짜 ㅋㅋㅋㅋ그래도 없으면 아쉽고그래요",
          "original_form": "ㅋㅋㅋㅋ진짜 ㅋㅋㅋㅋ그래도 없으면 아쉽고그래요",
          "speaker_id": "1",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100017235.1.21",
          "form": "그러게요 막상 이용하다 없음ㅎㅎ",
          "original_form": "그러게요 막상 이용하다 없음ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100017235.1.22",
          "form": "유튜브는 잘안보세요? 전 요리랑 음악 컨테츠 잘 이용해요",
          "original_form": "유튜브는 잘안보세요? 전 요리랑 음악 컨테츠 잘 이용해요",
          "speaker_id": "2",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100017235.1.23",
          "form": "아 유투브 봐요!  이거도프리미엄 고민중 ㅋㅋ",
          "original_form": "아 유투브 봐요!  이거도프리미엄 고민중 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100017235.1.24",
          "form": "넘비싸서ㅠ",
          "original_form": "넘비싸서ㅠ",
          "speaker_id": "1",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100017235.1.25",
          "form": "요리 어떤거보세용",
          "original_form": "요리 어떤거보세용",
          "speaker_id": "1",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100017235.1.26",
          "form": "ㅋㅋ 저 베이킹영상 좋아해요!",
          "original_form": "ㅋㅋ 저 베이킹영상 좋아해요!",
          "speaker_id": "2",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100017235.1.27",
          "form": "유튜브 프리미엄 진짜 한번 쓰면 절대 못 끊는다고 하더라고요",
          "original_form": "유튜브 프리미엄 진짜 한번 쓰면 절대 못 끊는다고 하더라고요",
          "speaker_id": "2",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017235.1.28",
          "form": "헐 베이킹 하시는구나 채널 뭐보세요",
          "original_form": "헐 베이킹 하시는구나 채널 뭐보세요",
          "speaker_id": "1",
          "time": "20210826 19:15"
        },
        {
          "id": "MDRW2100017235.1.29",
          "form": "저 자도르 많이 본거같은데 ㅋㅋ",
          "original_form": "저 자도르 많이 본거같은데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:16"
        }
      ]
    }
