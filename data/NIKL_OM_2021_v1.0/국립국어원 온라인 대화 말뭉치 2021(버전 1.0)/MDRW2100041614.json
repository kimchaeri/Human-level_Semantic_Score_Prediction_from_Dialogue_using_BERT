      "utterance": [
        {
          "id": "MDRW2100041614.1.1",
          "form": "아니 본업들이 다",
          "original_form": "아니 본업들이 다",
          "speaker_id": "2",
          "time": "20210906 12:44"
        },
        {
          "id": "MDRW2100041614.1.2",
          "form": "방송 연극 드라마 이러잖아",
          "original_form": "방송 연극 드라마 이러잖아",
          "speaker_id": "2",
          "time": "20210906 12:44"
        },
        {
          "id": "MDRW2100041614.1.3",
          "form": "그거 갠플 안되거든",
          "original_form": "그거 갠플 안되거든",
          "speaker_id": "2",
          "time": "20210906 12:44"
        },
        {
          "id": "MDRW2100041614.1.4",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210906 12:44"
        },
        {
          "id": "MDRW2100041614.1.5",
          "form": "그래서 하라고 하는 건가?",
          "original_form": "그래서 하라고 하는 건가?",
          "speaker_id": "2",
          "time": "20210906 12:44"
        },
        {
          "id": "MDRW2100041614.1.6",
          "form": "다같이쓰는거임?",
          "original_form": "다같이쓰는거임?",
          "speaker_id": "1",
          "time": "20210906 12:44"
        },
        {
          "id": "MDRW2100041614.1.7",
          "form": "연극도 연출팀이랑 배우랑 팀플하고",
          "original_form": "연극도 연출팀이랑 배우랑 팀플하고",
          "speaker_id": "2",
          "time": "20210906 12:45"
        },
        {
          "id": "MDRW2100041614.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ그럴수도",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그럴수도",
          "speaker_id": "1",
          "time": "20210906 12:45"
        },
        {
          "id": "MDRW2100041614.1.9",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210906 12:45"
        },
        {
          "id": "MDRW2100041614.1.10",
          "form": "드라마나 영화는 당연히 글쓰는 것만 혼자하지 다하고",
          "original_form": "드라마나 영화는 당연히 글쓰는 것만 혼자하지 다하고",
          "speaker_id": "2",
          "time": "20210906 12:45"
        },
        {
          "id": "MDRW2100041614.1.11",
          "form": "ㄱ그래서",
          "original_form": "ㄱ그래서",
          "speaker_id": "1",
          "time": "20210906 12:45"
        },
        {
          "id": "MDRW2100041614.1.12",
          "form": "팀플을",
          "original_form": "팀플을",
          "speaker_id": "1",
          "time": "20210906 12:45"
        },
        {
          "id": "MDRW2100041614.1.13",
          "form": "더사랑하시는듯",
          "original_form": "더사랑하시는듯",
          "speaker_id": "1",
          "time": "20210906 12:45"
        },
        {
          "id": "MDRW2100041614.1.14",
          "form": "아 에반디요 진짜",
          "original_form": "아 에반디요 진짜",
          "speaker_id": "2",
          "time": "20210906 12:46"
        },
        {
          "id": "MDRW2100041614.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
