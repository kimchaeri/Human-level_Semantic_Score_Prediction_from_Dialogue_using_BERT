      "utterance": [
        {
          "id": "MDRW2100046942.1.1",
          "form": "안 녕하세요",
          "original_form": "안 녕하세요",
          "speaker_id": "1",
          "time": "20211105 11:57"
        },
        {
          "id": "MDRW2100046942.1.2",
          "form": "똑똑",
          "original_form": "똑똑",
          "speaker_id": "1",
          "time": "20211105 13:18"
        },
        {
          "id": "MDRW2100046942.1.3",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211105 13:32"
        },
        {
          "id": "MDRW2100046942.1.4",
          "form": "게장 좋아하세요?",
          "original_form": "게장 좋아하세요?",
          "speaker_id": "2",
          "time": "20211105 13:32"
        },
        {
          "id": "MDRW2100046942.1.5",
          "form": "죄송합니다 바빠서",
          "original_form": "죄송합니다 바빠서",
          "speaker_id": "1",
          "time": "20211105 17:02"
        },
        {
          "id": "MDRW2100046942.1.6",
          "form": "게장 진짜좋아해요",
          "original_form": "게장 진짜좋아해요",
          "speaker_id": "1",
          "time": "20211105 17:02"
        },
        {
          "id": "MDRW2100046942.1.7",
          "form": "저도 게장 진짜 좋아하거든요",
          "original_form": "저도 게장 진짜 좋아하거든요",
          "speaker_id": "2",
          "time": "20211105 17:02"
        },
        {
          "id": "MDRW2100046942.1.8",
          "form": "이번에 게장 샀어요 ㅎㅎㅎㅎ",
          "original_form": "이번에 게장 샀어요 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211105 17:03"
        },
        {
          "id": "MDRW2100046942.1.9",
          "form": "네넹 ㅎ",
          "original_form": "네넹 ㅎ",
          "speaker_id": "1",
          "time": "20211105 17:30"
        },
        {
          "id": "MDRW2100046942.1.10",
          "form": "짐짜 맛있죠",
          "original_form": "짐짜 맛있죠",
          "speaker_id": "1",
          "time": "20211105 17:30"
        },
        {
          "id": "MDRW2100046942.1.11",
          "form": "간장게장 파신가요",
          "original_form": "간장게장 파신가요",
          "speaker_id": "2",
          "time": "20211105 17:30"
        },
        {
          "id": "MDRW2100046942.1.12",
          "form": "양념게장 파신가요 ㅎㅎ",
          "original_form": "양념게장 파신가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211105 17:30"
        },
        {
          "id": "MDRW2100046942.1.13",
          "form": "아뇨 간장게장을 더좋아해요 ㅋㅋㅋ",
          "original_form": "아뇨 간장게장을 더좋아해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211105 17:48"
        },
        {
          "id": "MDRW2100046942.1.14",
          "form": "맛있으니깐욤 ㅎ",
          "original_form": "맛있으니깐욤 ㅎ",
          "speaker_id": "1",
          "time": "20211105 17:48"
        },
        {
          "id": "MDRW2100046942.1.15",
          "form": "간장게장파신군요 ㅎㅎ",
          "original_form": "간장게장파신군요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211105 17:50"
        },
        {
          "id": "MDRW2100046942.1.16",
          "form": "저는 어렸을땐 양념게장파였는데",
          "original_form": "저는 어렸을땐 양념게장파였는데",
          "speaker_id": "2",
          "time": "20211105 17:50"
        },
        {
          "id": "MDRW2100046942.1.17",
          "form": "이젠 간장게장이 더 맛있어요",
          "original_form": "이젠 간장게장이 더 맛있어요",
          "speaker_id": "2",
          "time": "20211105 17:51"
        },
        {
          "id": "MDRW2100046942.1.18",
          "form": "게장은 다 맛있지만요 ㅋㅋㅋ",
          "original_form": "게장은 다 맛있지만요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211105 17:51"
        },
        {
          "id": "MDRW2100046942.1.19",
          "form": "네 살이 아주 쫀득하죠",
          "original_form": "네 살이 아주 쫀득하죠",
          "speaker_id": "1",
          "time": "20211105 17:58"
        },
        {
          "id": "MDRW2100046942.1.20",
          "form": "이제퇴근하네요 ㅠ",
          "original_form": "이제퇴근하네요 ㅠ",
          "speaker_id": "1",
          "time": "20211105 17:58"
        },
        {
          "id": "MDRW2100046942.1.21",
          "form": "고생 많으셨어요 ㅎㅎ 그래도 금욜 칼퇴!",
          "original_form": "고생 많으셨어요 ㅎㅎ 그래도 금욜 칼퇴!",
          "speaker_id": "2",
          "time": "20211105 17:59"
        },
        {
          "id": "MDRW2100046942.1.22",
          "form": "간장게장 딱지에 밥 비벼먹고 싶은데",
          "original_form": "간장게장 딱지에 밥 비벼먹고 싶은데",
          "speaker_id": "2",
          "time": "20211105 17:59"
        },
        {
          "id": "MDRW2100046942.1.23",
          "form": "오늘건 딱지가 좀 약했어요 ..",
          "original_form": "오늘건 딱지가 좀 약했어요 ..",
          "speaker_id": "2",
          "time": "20211105 17:59"
        },
        {
          "id": "MDRW2100046942.1.24",
          "form": "아 먹고싶네요 ㅎㅎ",
          "original_form": "아 먹고싶네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211105 18:02"
        },
        {
          "id": "MDRW2100046942.1.25",
          "form": "혼자밥해먹어야하니",
          "original_form": "혼자밥해먹어야하니",
          "speaker_id": "1",
          "time": "20211105 18:02"
        },
        {
          "id": "MDRW2100046942.1.26",
          "form": "ㅎ귀찮",
          "original_form": "ㅎ귀찮",
          "speaker_id": "1",
          "time": "20211105 18:02"
        },
        {
          "id": "MDRW2100046942.1.27",
          "form": "오늘 저녁밥은 뭘 드시나요?",
          "original_form": "오늘 저녁밥은 뭘 드시나요?",
          "speaker_id": "2",
          "time": "20211105 18:02"
        },
        {
          "id": "MDRW2100046942.1.28",
          "form": "아직 미정이요",
          "original_form": "아직 미정이요",
          "speaker_id": "1",
          "time": "20211105 18:02"
        },
        {
          "id": "MDRW2100046942.1.29",
          "form": "집가서 생각을 ㅎ",
          "original_form": "집가서 생각을 ㅎ",
          "speaker_id": "1",
