      "utterance": [
        {
          "id": "MDRW2100018436.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210827 01:47"
        },
        {
          "id": "MDRW2100018436.1.2",
          "form": "저는 대학생이에요",
          "original_form": "저는 대학생이에요",
          "speaker_id": "2",
          "time": "20210827 01:47"
        },
        {
          "id": "MDRW2100018436.1.3",
          "form": "반가워요 전 직장인이에요",
          "original_form": "반가워요 전 직장인이에요",
          "speaker_id": "1",
          "time": "20210827 01:47"
        },
        {
          "id": "MDRW2100018436.1.4",
          "form": "대학생이시구나~ 학교 생활은 재밌어요?",
          "original_form": "대학생이시구나~ 학교 생활은 재밌어요?",
          "speaker_id": "1",
          "time": "20210827 01:47"
        },
        {
          "id": "MDRW2100018436.1.5",
          "form": "음... 저는 그냥 그러내요ㅠㅠ",
          "original_form": "음... 저는 그냥 그러내요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 01:48"
        },
        {
          "id": "MDRW2100018436.1.6",
          "form": "코로나라 학교도 제대로 못 가봤어요ㅋㅋㅋ",
          "original_form": "코로나라 학교도 제대로 못 가봤어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:48"
        },
        {
          "id": "MDRW2100018436.1.7",
          "form": "아하 요즘은 다 비대면 하는거 같더라구요",
          "original_form": "아하 요즘은 다 비대면 하는거 같더라구요",
          "speaker_id": "1",
          "time": "20210827 01:48"
        },
        {
          "id": "MDRW2100018436.1.8",
          "form": "그럼 동기들이랑 아직 안 친하겠어요...?",
          "original_form": "그럼 동기들이랑 아직 안 친하겠어요...?",
          "speaker_id": "1",
          "time": "20210827 01:49"
        },
        {
          "id": "MDRW2100018436.1.9",
          "form": "네 맞아요ㅠㅠ 진짜 잘 몰라요",
          "original_form": "네 맞아요ㅠㅠ 진짜 잘 몰라요",
          "speaker_id": "2",
          "time": "20210827 01:49"
        },
        {
          "id": "MDRW2100018436.1.10",
          "form": "아맞다 저 아재 수강신청 했어요 ㅎㅎ",
          "original_form": "아맞다 저 아재 수강신청 했어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 01:49"
        },
        {
          "id": "MDRW2100018436.1.11",
          "form": "어제",
          "original_form": "어제",
          "speaker_id": "2",
          "time": "20210827 01:49"
        },
        {
          "id": "MDRW2100018436.1.12",
          "form": "오오 수강신청이라는 단어 오랜만이네요ㅋㅋ",
          "original_form": "오오 수강신청이라는 단어 오랜만이네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:50"
        },
        {
          "id": "MDRW2100018436.1.13",
          "form": "성공하셨나요?",
          "original_form": "성공하셨나요?",
          "speaker_id": "1",
          "time": "20210827 01:50"
        },
        {
          "id": "MDRW2100018436.1.14",
          "form": "네 저 완벽성공입니다 ㅎㅎ",
          "original_form": "네 저 완벽성공입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 01:50"
        },
        {
          "id": "MDRW2100018436.1.15",
          "form": "예전에는 수강신청 어떻게 했어요?",
          "original_form": "예전에는 수강신청 어떻게 했어요?",
          "speaker_id": "2",
          "time": "20210827 01:50"
        },
        {
          "id": "MDRW2100018436.1.16",
          "form": "너무 오래 전이라 기억도 잘 안나네요ㅋㅋ",
          "original_form": "너무 오래 전이라 기억도 잘 안나네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:51"
        },
        {
          "id": "MDRW2100018436.1.17",
          "form": "예전에는 그냥 학교가서 했었어요",
          "original_form": "예전에는 그냥 학교가서 했었어요",
          "speaker_id": "1",
          "time": "20210827 01:51"
        },
        {
          "id": "MDRW2100018436.1.18",
          "form": "요즘은 컴퓨터로 하죠?",
          "original_form": "요즘은 컴퓨터로 하죠?",
          "speaker_id": "1",
          "time": "20210827 01:51"
        },
        {
          "id": "MDRW2100018436.1.19",
          "form": "네 맞아요 홈페이지에서 합니다",
          "original_form": "네 맞아요 홈페이지에서 합니다",
          "speaker_id": "2",
          "time": "20210827 01:52"
        },
        {
          "id": "MDRW2100018436.1.20",
          "form": "그렇구나 ㅎㅎ 개강은 언제 하나요?",
          "original_form": "그렇구나 ㅎㅎ 개강은 언제 하나요?",
          "speaker_id": "1",
          "time": "20210827 01:52"
        },
        {
          "id": "MDRW2100018436.1.21",
          "form": "개강 다음주 월요일이요...",
          "original_form": "개강 다음주 월요일이요...",
          "speaker_id": "2",
          "time": "20210827 01:52"
        },
        {
          "id": "MDRW2100018436.1.22",
          "form": "며칠 안 남았네요ㅠㅠ",
          "original_form": "며칠 안 남았네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 01:52"
        },
        {
          "id": "MDRW2100018436.1.23",
          "form": "떨리겠어요 그래도 학교생활할 때가 좋긴 해요",
          "original_form": "떨리겠어요 그래도 학교생활할 때가 좋긴 해요",
          "speaker_id": "1",
          "time": "20210827 01:53"
        },
        {
          "id": "MDRW2100018436.1.24",
          "form": "직장생활은... 절레절레 ㅎ",
          "original_form": "직장생활은... 절레절레 ㅎ",
          "speaker_id": "1",
          "time": "20210827 01:53"
        },
        {
          "id": "MDRW2100018436.1.25",
          "form": "ㅋㅋㅋ 맞어요 저도 그렇게 생각해요",
          "original_form": "ㅋㅋㅋ 맞어요 저도 그렇게 생각해요",
          "speaker_id": "2",
          "time": "20210827 01:54"
        },
        {
          "id": "MDRW2100018436.1.26",
          "form": "학생 때가 좋은거 같아요 시험이 좀 어렵긴 해도...",
          "original_form": "학생 때가 좋은거 같아요 시험이 좀 어렵긴 해도...",
          "speaker_id": "2",
          "time": "20210827 01:54"
        },
        {
          "id": "MDRW2100018436.1.27",
          "form": "맞아요 직장인 되면 방학이 없답니다 ㅠ",
          "original_form": "맞아요 직장인 되면 방학이 없답니다 ㅠ",
          "speaker_id": "1",
          "time": "20210827 01:55"
        },
        {
          "id": "MDRW2100018436.1.28",
          "form": "그러니 지금을 즐겨요~",
          "original_form": "그러니 지금을 즐겨요~",
          "speaker_id": "1",
          "time": "20210827 01:55"
        },
        {
          "id": "MDRW2100018436.1.29",
          "form": "넵 남은 방학 실컷 즐기겠습니다 ㅎㅎ",
          "original_form": "넵 남은 방학 실컷 즐기겠습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 01:57"
        }
      ]
    }
