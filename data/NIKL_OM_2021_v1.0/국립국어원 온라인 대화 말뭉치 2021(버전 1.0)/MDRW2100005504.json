        },
        {
          "id": "MDRW2100005504.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210821 22:45"
        },
        {
          "id": "MDRW2100005504.1.3",
          "form": "요즘에 맛있게 드신 음식이 있으신가요?",
          "original_form": "요즘에 맛있게 드신 음식이 있으신가요?",
          "speaker_id": "1",
          "time": "20210821 22:46"
        },
        {
          "id": "MDRW2100005504.1.4",
          "form": "오늘 간짜장 먹었어요!!",
          "original_form": "오늘 간짜장 먹었어요!!",
          "speaker_id": "2",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005504.1.5",
          "form": "아 그러셨군요ㅎㅎ",
          "original_form": "아 그러셨군요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 22:48"
        },
        {
          "id": "MDRW2100005504.1.6",
          "form": "저도 간짜장 좋아해요ㅎㅎㅎ",
          "original_form": "저도 간짜장 좋아해요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 22:48"
        },
        {
          "id": "MDRW2100005504.1.7",
          "form": "간짜장 맛있죠ㅎㅎ",
          "original_form": "간짜장 맛있죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005504.1.8",
          "form": "name1님은 오늘 뭐 드셨나요??",
          "original_form": "&name1&님은 오늘 뭐 드셨나요??",
          "speaker_id": "2",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005504.1.9",
          "form": "저는 오늘 엄마가 불고기해줘서 저녁엔 불고기 먹었어요",
          "original_form": "저는 오늘 엄마가 불고기해줘서 저녁엔 불고기 먹었어요",
          "speaker_id": "1",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005504.1.10",
          "form": "점심엔 라끼남보다가 라면 끓여먹었고요ㅎㅎ",
          "original_form": "점심엔 라끼남보다가 라면 끓여먹었고요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005504.1.11",
          "form": "name2님은 간짜장말고 또 뭐 드셨나요?",
          "original_form": "&name2&님은 간짜장말고 또 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005504.1.12",
          "form": "불고기 맛있겠네요",
          "original_form": "불고기 맛있겠네요",
          "speaker_id": "2",
          "time": "20210821 22:52"
        },
        {
          "id": "MDRW2100005504.1.13",
          "form": "저는 아점으로 피자 먹었어요!",
          "original_form": "저는 아점으로 피자 먹었어요!",
          "speaker_id": "2",
          "time": "20210821 22:52"
        },
        {
          "id": "MDRW2100005504.1.14",
          "form": "점저로 간짜장이랑 짬뽕 ㅎㅎ",
          "original_form": "점저로 간짜장이랑 짬뽕 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:52"
        },
        {
          "id": "MDRW2100005504.1.15",
          "form": "저도 피자 좋아하는데",
          "original_form": "저도 피자 좋아하는데",
          "speaker_id": "1",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005504.1.16",
          "form": "요즘에 먹은 피자 중에 잭슨피자가 제일 맛있더라고요",
          "original_form": "요즘에 먹은 피자 중에 잭슨피자가 제일 맛있더라고요",
          "speaker_id": "1",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005504.1.17",
          "form": "잭슨피자는 처음 들어보는데",
          "original_form": "잭슨피자는 처음 들어보는데",
          "speaker_id": "2",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005504.1.18",
          "form": "체인점? 브랜드인가요",
          "original_form": "체인점? 브랜드인가요",
          "speaker_id": "2",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005504.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005504.1.20",
          "form": "원래 압구정인가가 본점인데",
          "original_form": "원래 압구정인가가 본점인데",
          "speaker_id": "1",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005504.1.21",
          "form": "유명해져서 체인점을 많이 냈어요",
          "original_form": "유명해져서 체인점을 많이 냈어요",
          "speaker_id": "1",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005504.1.22",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "2",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005504.1.23",
          "form": "무슨 메뉴가 제일 맛있어요??",
          "original_form": "무슨 메뉴가 제일 맛있어요??",
          "speaker_id": "2",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005504.1.24",
          "form": "저는 바질피자가 제일 인상적이었어요",
          "original_form": "저는 바질피자가 제일 인상적이었어요",
          "speaker_id": "1",
          "time": "20210821 22:59"
        },
        {
          "id": "MDRW2100005504.1.25",
          "form": "원래 바질을 별로 안좋아하는데",
          "original_form": "원래 바질을 별로 안좋아하는데",
          "speaker_id": "1",
          "time": "20210821 22:59"
        },
        {
          "id": "MDRW2100005504.1.26",
          "form": "이 집에서 먹어보고 맛있어서",
          "original_form": "이 집에서 먹어보고 맛있어서",
          "speaker_id": "1",
          "time": "20210821 22:59"
        },
        {
          "id": "MDRW2100005504.1.27",
          "form": "바질도 좋아졌거든요ㅎㅎ",
          "original_form": "바질도 좋아졌거든요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 22:59"
        },
        {
          "id": "MDRW2100005504.1.28",
          "form": "바질피자 맛있죠",
          "original_form": "바질피자 맛있죠",
          "speaker_id": "2",
          "time": "20210821 23:02"
        },
        {
          "id": "MDRW2100005504.1.29",
          "form": "바질 하니까 파스타도 머곡 싶네오",
          "original_form": "바질 하니까 파스타도 머곡 싶네오",
          "speaker_id": "2",
          "time": "20210821 23:02"
        },
        {
          "id": "MDRW2100005504.1.30",
          "form": "바질이 파스타랑도 잘 어울리죠",
          "original_form": "바질이 파스타랑도 잘 어울리죠",
          "speaker_id": "1",
          "time": "20210821 23:03"
        },
        {
          "id": "MDRW2100005504.1.31",
          "form": "요새 바질을 직접 키우고 있는데",
          "original_form": "요새 바질을 직접 키우고 있는데",
          "speaker_id": "1",
          "time": "20210821 23:04"
        },
        {
          "id": "MDRW2100005504.1.32",
          "form": "얼른 자라서 파스타해먹고 싶어요",
          "original_form": "얼른 자라서 파스타해먹고 싶어요",
          "speaker_id": "1",
          "time": "20210821 23:04"
        },
        {
          "id": "MDRW2100005504.1.33",
          "form": "우와 바질을 직접 키우신다니",
          "original_form": "우와 바질을 직접 키우신다니",
          "speaker_id": "2",
          "time": "20210821 23:07"
        }
      ]
