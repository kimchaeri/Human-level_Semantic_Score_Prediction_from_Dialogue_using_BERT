      "utterance": [
        {
          "id": "MDRW2100004380.1.1",
          "form": "해방타운보세요?",
          "original_form": "해방타운보세요?",
          "speaker_id": "1",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004380.1.2",
          "form": "그게 머예요?",
          "original_form": "그게 머예요?",
          "speaker_id": "2",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004380.1.3",
          "form": "장윤정 허재 백지영 등 나와서",
          "original_form": "장윤정 허재 백지영 등 나와서",
          "speaker_id": "1",
          "time": "20210821 16:17"
        },
        {
          "id": "MDRW2100004380.1.4",
          "form": "오피스텔 하나씩  잡아줘서 혼자지내는프로요ㅎ",
          "original_form": "오피스텔 하나씩  잡아줘서 혼자지내는프로요ㅎ",
          "speaker_id": "1",
          "time": "20210821 16:17"
        },
        {
          "id": "MDRW2100004380.1.5",
          "form": "집나와서 해방ㅋ",
          "original_form": "집나와서 해방ㅋ",
          "speaker_id": "1",
          "time": "20210821 16:17"
        },
        {
          "id": "MDRW2100004380.1.6",
          "form": "오 진짜 저한테 필요한 프로그램같아요 ㅎ",
          "original_form": "오 진짜 저한테 필요한 프로그램같아요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 16:19"
        },
        {
          "id": "MDRW2100004380.1.7",
          "form": "시작한지 얼마안됐나요?",
          "original_form": "시작한지 얼마안됐나요?",
          "speaker_id": "2",
          "time": "20210821 16:19"
        },
        {
          "id": "MDRW2100004380.1.8",
          "form": "멤버교체도됐으니 한두달된듯요",
          "original_form": "멤버교체도됐으니 한두달된듯요",
          "speaker_id": "1",
          "time": "20210821 16:20"
        },
        {
          "id": "MDRW2100004380.1.9",
          "form": "장윤정이 유쾌하고 재밌어요",
          "original_form": "장윤정이 유쾌하고 재밌어요",
          "speaker_id": "1",
          "time": "20210821 16:20"
        },
        {
          "id": "MDRW2100004380.1.10",
          "form": "오래됐네요? 저는 왜 첨듣는건지?",
          "original_form": "오래됐네요? 저는 왜 첨듣는건지?",
          "speaker_id": "2",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004380.1.11",
          "form": "개인 성격이 다 나올것같아요",
          "original_form": "개인 성격이 다 나올것같아요",
          "speaker_id": "2",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004380.1.12",
          "form": "jtbc에서 해요ㅎ 아마바쁘셔서 못보셨을듯",
          "original_form": "jtbc에서 해요ㅎ 아마바쁘셔서 못보셨을듯",
          "speaker_id": "1",
          "time": "20210821 16:22"
        },
        {
          "id": "MDRW2100004380.1.13",
          "form": "재미있을것같아요 그런것도 대본있겠죠?",
          "original_form": "재미있을것같아요 그런것도 대본있겠죠?",
          "speaker_id": "2",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004380.1.14",
          "form": "컨셉이 있는거같긴해요",
          "original_form": "컨셉이 있는거같긴해요",
          "speaker_id": "1",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004380.1.15",
          "form": "친구들도 초대해서 놀고",
          "original_form": "친구들도 초대해서 놀고",
          "speaker_id": "1",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004380.1.16",
          "form": "제가 누리고싶은 컨셉이네요 ㅎ",
          "original_form": "제가 누리고싶은 컨셉이네요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004380.1.17",
          "form": "자유부인 ㅎㅎ",
          "original_form": "자유부인 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004380.1.18",
          "form": "너무 좋은가보더라구요ㅎ",
          "original_form": "너무 좋은가보더라구요ㅎ",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004380.1.19",
          "form": "돈벌고 자유부인하고 너무 좋겠는데요",
          "original_form": "돈벌고 자유부인하고 너무 좋겠는데요",
          "speaker_id": "2",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004380.1.20",
          "form": "윤혜진?",
          "original_form": "윤혜진?",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004380.1.21",
          "form": "엄태웅 와이프도 나와요",
          "original_form": "엄태웅 와이프도 나와요",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004380.1.22",
          "form": "발래리나아니예요?",
          "original_form": "발래리나아니예요?",
          "speaker_id": "2",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004380.1.23",
          "form": "맞아요 딸 지온이도 엄청컸더라구요",
          "original_form": "맞아요 딸 지온이도 엄청컸더라구요",
          "speaker_id": "1",
          "time": "20210821 16:30"
        },
        {
          "id": "MDRW2100004380.1.24",
          "form": "아...갑자기 엄태웅이 왜한동안 안보인건지 생각이 안나네요 ㅎ",
          "original_form": "아...갑자기 엄태웅이 왜한동안 안보인건지 생각이 안나네요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004380.1.25",
          "form": "아 유흥업소",
          "original_form": "아 유흥업소",
          "speaker_id": "1",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004380.1.26",
          "form": "그사건이 있었죠ㅎ",
          "original_form": "그사건이 있었죠ㅎ",
          "speaker_id": "1",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004380.1.27",
          "form": "아 그랬나요?",
          "original_form": "아 그랬나요?",
          "speaker_id": "2",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004380.1.28",
          "form": "네 그래서 와이프어쩌나 했는데 잘사나봐요ㅎ",
          "original_form": "네 그래서 와이프어쩌나 했는데 잘사나봐요ㅎ",
          "speaker_id": "1",
          "time": "20210821 16:33"
        }
      ]
