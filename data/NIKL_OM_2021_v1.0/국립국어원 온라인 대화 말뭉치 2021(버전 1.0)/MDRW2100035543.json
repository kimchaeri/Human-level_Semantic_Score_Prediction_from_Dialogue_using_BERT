        },
        {
          "id": "MDRW2100035543.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210829 20:13"
        },
        {
          "id": "MDRW2100035543.1.5",
          "form": "잔치국수 만들어서 먹었어요",
          "original_form": "잔치국수 만들어서 먹었어요",
          "speaker_id": "2",
          "time": "20210829 20:13"
        },
        {
          "id": "MDRW2100035543.1.6",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210829 20:13"
        },
        {
          "id": "MDRW2100035543.1.7",
          "form": "저는 아직이요 육수는 어떻게 내셨나요?",
          "original_form": "저는 아직이요 육수는 어떻게 내셨나요?",
          "speaker_id": "1",
          "time": "20210829 20:15"
        },
        {
          "id": "MDRW2100035543.1.8",
          "form": "아직 저녁식사 안하셨군요",
          "original_form": "아직 저녁식사 안하셨군요",
          "speaker_id": "2",
          "time": "20210829 20:19"
        },
        {
          "id": "MDRW2100035543.1.9",
          "form": "집에 육수팩이 있어서 그것 활용했어요",
          "original_form": "집에 육수팩이 있어서 그것 활용했어요",
          "speaker_id": "2",
          "time": "20210829 20:19"
        },
        {
          "id": "MDRW2100035543.1.10",
          "form": "오 육수팩은 비싼가요?",
          "original_form": "오 육수팩은 비싼가요?",
          "speaker_id": "1",
          "time": "20210829 20:21"
        },
        {
          "id": "MDRW2100035543.1.11",
          "form": "지난번에 사뒀는데",
          "original_form": "지난번에 사뒀는데",
          "speaker_id": "2",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100035543.1.12",
          "form": "많이 비싸진 않았던 것 같아요",
          "original_form": "많이 비싸진 않았던 것 같아요",
          "speaker_id": "2",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100035543.1.13",
          "form": "그렇군요 좋아하시는 음식 있으신가요?",
          "original_form": "그렇군요 좋아하시는 음식 있으신가요?",
          "speaker_id": "1",
          "time": "20210829 21:42"
        },
        {
          "id": "MDRW2100035543.1.14",
          "form": "저는 일식이나 태국음식 좋아해요",
          "original_form": "저는 일식이나 태국음식 좋아해요",
          "speaker_id": "2",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100035543.1.15",
          "form": "태국음식은 어떤 음식 좋아하시나요?",
          "original_form": "태국음식은 어떤 음식 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100035543.1.16",
          "form": "쌀국수 쏨땀 나시고랭.. 다 좋아해요",
          "original_form": "쌀국수 쏨땀 나시고랭.. 다 좋아해요",
          "speaker_id": "2",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100035543.1.17",
          "form": "똠얌꿍이나 푸팟퐁커리도",
          "original_form": "똠얌꿍이나 푸팟퐁커리도",
          "speaker_id": "2",
          "time": "20210829 22:48"
        },
        {
