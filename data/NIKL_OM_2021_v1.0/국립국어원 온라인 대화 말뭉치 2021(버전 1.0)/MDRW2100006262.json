      "utterance": [
        {
          "id": "MDRW2100006262.1.1",
          "form": "안녕하세요 맛집 탐방 많이 다니세요?ㅎㅎ",
          "original_form": "안녕하세요 맛집 탐방 많이 다니세요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100006262.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006262.1.3",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006262.1.4",
          "form": "작년 이래로 잘 못 다니고 있네요",
          "original_form": "작년 이래로 잘 못 다니고 있네요",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006262.1.5",
          "form": "코로나 때문에 그죠?",
          "original_form": "코로나 때문에 그죠?",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006262.1.6",
          "form": "원래는 많이 다니셨어요? ㅎㄹ",
          "original_form": "원래는 많이 다니셨어요? ㅎㄹ",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006262.1.7",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006262.1.8",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006262.1.9",
          "form": "놀랍게도",
          "original_form": "놀랍게도",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006262.1.10",
          "form": "그 이전에도 사실 잘 다니지는",
          "original_form": "그 이전에도 사실 잘 다니지는",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006262.1.11",
          "form": "흠흠",
          "original_form": "흠흠",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100006262.1.12",
          "form": "아ㅋㅋㅋㅋㅋ동네 맛집 위주인가요",
          "original_form": "아ㅋㅋㅋㅋㅋ동네 맛집 위주인가요",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006262.1.13",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006262.1.14",
          "form": "그럼 몇시간씩 줄 서고 이런데도 잘 안가셨겠네요",
          "original_form": "그럼 몇시간씩 줄 서고 이런데도 잘 안가셨겠네요",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006262.1.15",
          "form": "예 그러네요",
          "original_form": "예 그러네요",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006262.1.16",
          "form": "그런 데 자주 다니시나요?",
          "original_form": "그런 데 자주 다니시나요?",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006262.1.17",
          "form": "예전엔 그래도 많이 다녔어요 친구들이랑",
          "original_form": "예전엔 그래도 많이 다녔어요 친구들이랑",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100006262.1.18",
          "form": "지금은 사람 없는데만 골라가거나 식당을 안가서 다 옛날 일이지만..",
          "original_form": "지금은 사람 없는데만 골라가거나 식당을 안가서 다 옛날 일이지만..",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100006262.1.19",
          "form": "아앗...",
          "original_form": "아앗...",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006262.1.20",
          "form": "그러면 그런 맛집 정보는",
          "original_form": "그러면 그런 맛집 정보는",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006262.1.21",
          "form": "어디서 찾으셨었나요",
          "original_form": "어디서 찾으셨었나요",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006262.1.22",
          "form": "음 네이버 지도 평점이나 친구추천이 많았어요",
          "original_form": "음 네이버 지도 평점이나 친구추천이 많았어요",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100006262.1.23",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100006262.1.24",
          "form": "최근에는 인스타 깔았는게 거기도 맛집 정보 많더라구요ㅎㅎ",
          "original_form": "최근에는 인스타 깔았는게 거기도 맛집 정보 많더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 22:24"
        }
