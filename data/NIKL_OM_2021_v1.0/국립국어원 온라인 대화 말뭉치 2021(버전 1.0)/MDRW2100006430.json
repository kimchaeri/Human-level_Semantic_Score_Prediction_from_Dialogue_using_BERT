        },
        {
          "id": "MDRW2100006430.1.2",
          "form": "올만에 로맨스 잡으셨어요?",
          "original_form": "올만에 로맨스 잡으셨어요?",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100006430.1.3",
          "form": "아니네 로판이네",
          "original_form": "아니네 로판이네",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100006430.1.4",
          "form": "녜 로판이에요 ㅋㅋㅋㅋ",
          "original_form": "녜 로판이에요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100006430.1.5",
          "form": "판타지 요소는 없는것 같긴 한데",
          "original_form": "판타지 요소는 없는것 같긴 한데",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100006430.1.6",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100006430.1.7",
          "form": "일단 현대배경은 아니니 ㅋㅋㅋ",
          "original_form": "일단 현대배경은 아니니 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100006430.1.8",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100006430.1.9",
          "form": "중세 갓다고 판타진가봐",
          "original_form": "중세 갓다고 판타진가봐",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100006430.1.10",
          "form": "대부분 로판이 그렇긴 하지 ㅋㅋㅋ",
          "original_form": "대부분 로판이 그렇긴 하지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100006430.1.11",
          "form": "근데 저건 배경이 근대에 가까워",
          "original_form": "근데 저건 배경이 근대에 가까워",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100006430.1.12",
          "form": "자동차 타고 댕기거든",
          "original_form": "자동차 타고 댕기거든",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100006430.1.13",
          "form": "마차도 있긴 한데 남주는 차타고 댕긴다 ㅋㅋㅋ",
          "original_form": "마차도 있긴 한데 남주는 차타고 댕긴다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100006430.1.14",
          "form": "아..??",
          "original_form": "아..??",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100006430.1.15",
          "form": "차요?",
          "original_form": "차요?",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100006430.1.16",
          "form": "엔진달고 움직이는?",
          "original_form": "엔진달고 움직이는?",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100006430.1.17",
          "form": "넹 그 차요 ㅋㅋㅋㅋ",
          "original_form": "넹 그 차요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100006430.1.18",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100006430.1.19",
          "form": "그냥 현대물 하세욬ㅋㅋ",
          "original_form": "그냥 현대물 하세욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100006430.1.20",
          "form": "아냐 그래도 근대라고 해줘 ㅋㅋㅋㅋ",
          "original_form": "아냐 그래도 근대라고 해줘 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100006430.1.21",
          "form": "나름 신분제 사회도 있는 동네라궄ㅋㅋㅋ",
          "original_form": "나름 신분제 사회도 있는 동네라궄ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100006430.1.22",
          "form": "그래 근대까지 오자",
          "original_form": "그래 근대까지 오자",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100006430.1.23",
          "form": "아 그래도 판타지로 쳐야하나?",
          "original_form": "아 그래도 판타지로 쳐야하나?",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100006430.1.24",
          "form": "현대는 아니니 ㅋㅋㅋ",
          "original_form": "현대는 아니니 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100006430.1.25",
          "form": "일단은 가상의 세계관이긴 하니깐ㅋㅋㅋ",
          "original_form": "일단은 가상의 세계관이긴 하니깐ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100006430.1.26",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100006430.1.27",
          "form": "그치 현대가 아닌이상",
          "original_form": "그치 현대가 아닌이상",
          "speaker_id": "1",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100006430.1.28",
          "form": "역사물은 판타지가 될수밖에 없지",
          "original_form": "역사물은 판타지가 될수밖에 없지",
          "speaker_id": "1",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100006430.1.29",
          "form": "아무리 고증해돜",
          "original_form": "아무리 고증해돜",
          "speaker_id": "1",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100006430.1.30",
          "form": "찐역사를 배경으로 가져다 쓰는거 아닌이상에야 ㅋㅋㅋ",
          "original_form": "찐역사를 배경으로 가져다 쓰는거 아닌이상에야 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100006430.1.31",
          "form": "같은 역사도 역사가가 어떻게 쓰고 누구 입장에서 쓰느냐에 따라 달라지니까",
          "original_form": "같은 역사도 역사가가 어떻게 쓰고 누구 입장에서 쓰느냐에 따라 달라지니까",
          "speaker_id": "1",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100006430.1.32",
          "form": "선생도 마찬가지고",
          "original_form": "선생도 마찬가지고",
          "speaker_id": "1",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100006430.1.33",
          "form": "그러니 소설은 더 하겠지 ㅋㅋ",
          "original_form": "그러니 소설은 더 하겠지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100006430.1.34",
          "form": "것도 글치 ㅋㅋㅋ",
          "original_form": "것도 글치 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100006430.1.35",
          "form": "배경으로 갖다 쓰는것도 뭘 얼마나 가져다 쓰냐에 따라",
          "original_form": "배경으로 갖다 쓰는것도 뭘 얼마나 가져다 쓰냐에 따라",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100006430.1.36",
          "form": "분명 같은 왕이 시대인데 완전 다른 작품이 나오기도 하는것ㅋㅋㅋ",
          "original_form": "분명 같은 왕이 시대인데 완전 다른 작품이 나오기도 하는것ㅋㅋㅋ",
          "speaker_id": "2",
