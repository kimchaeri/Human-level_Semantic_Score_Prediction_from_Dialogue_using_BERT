      "utterance": [
        {
          "id": "MDRW2100010131.1.1",
          "form": "마스크 써서 더 그런것 같아",
          "original_form": "마스크 써서 더 그런것 같아",
          "speaker_id": "2",
          "time": "20210823 14:39"
        },
        {
          "id": "MDRW2100010131.1.2",
          "form": "맞아 나 마스크 접촉성 알레르기 있는 거 같아ㅠㅜ",
          "original_form": "맞아 나 마스크 접촉성 알레르기 있는 거 같아ㅠㅜ",
          "speaker_id": "1",
          "time": "20210823 14:40"
        },
        {
          "id": "MDRW2100010131.1.3",
          "form": "그게 머야?",
          "original_form": "그게 머야?",
          "speaker_id": "2",
          "time": "20210823 14:40"
        },
        {
          "id": "MDRW2100010131.1.4",
          "form": "마스크 쓸리는 거 때문에 피부에 뭐 올라오는 거",
          "original_form": "마스크 쓸리는 거 때문에 피부에 뭐 올라오는 거",
          "speaker_id": "1",
          "time": "20210823 14:40"
        },
        {
          "id": "MDRW2100010131.1.5",
          "form": "아 나도 약간 올라오는 것 같아",
          "original_form": "아 나도 약간 올라오는 것 같아",
          "speaker_id": "2",
          "time": "20210823 14:41"
        },
        {
          "id": "MDRW2100010131.1.6",
          "form": "그래? 몰랐네",
          "original_form": "그래? 몰랐네",
          "speaker_id": "1",
          "time": "20210823 14:41"
        },
        {
          "id": "MDRW2100010131.1.7",
          "form": "빨리 마스크 벗고 싶다",
          "original_form": "빨리 마스크 벗고 싶다",
          "speaker_id": "2",
          "time": "20210823 14:41"
        },
        {
          "id": "MDRW2100010131.1.8",
          "form": "그니까 이렇게 오래 쓸 줄 몰랐어",
          "original_form": "그니까 이렇게 오래 쓸 줄 몰랐어",
          "speaker_id": "1",
          "time": "20210823 14:41"
        },
        {
          "id": "MDRW2100010131.1.9",
          "form": "내년이면 2년 째인데",
          "original_form": "내년이면 2년 째인데",
          "speaker_id": "2",
          "time": "20210823 14:42"
        },
        {
          "id": "MDRW2100010131.1.10",
          "form": "어휴 지긋지긋하다",
          "original_form": "어휴 지긋지긋하다",
          "speaker_id": "1",
          "time": "20210823 14:42"
        },
        {
          "id": "MDRW2100010131.1.11",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210823 14:42"
        },
        {
          "id": "MDRW2100010131.1.12",
          "form": "백신 다 맞아도 써야할 거 같은데 사실",
          "original_form": "백신 다 맞아도 써야할 거 같은데 사실",
