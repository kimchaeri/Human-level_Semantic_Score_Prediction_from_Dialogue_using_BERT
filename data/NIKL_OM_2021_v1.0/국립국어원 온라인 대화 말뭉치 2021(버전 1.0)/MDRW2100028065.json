      "utterance": [
        {
          "id": "MDRW2100028065.1.1",
          "form": "안녕하세요 ㅎㅎㅎㅎ퇴근시간 거의다되가네요",
          "original_form": "안녕하세요 ㅎㅎㅎㅎ퇴근시간 거의다되가네요",
          "speaker_id": "1",
          "time": "20210902 17:21"
        },
        {
          "id": "MDRW2100028065.1.2",
          "form": "요즘 폰으로 무슨게임하세요?",
          "original_form": "요즘 폰으로 무슨게임하세요?",
          "speaker_id": "1",
          "time": "20210902 17:21"
        },
        {
          "id": "MDRW2100028065.1.3",
          "form": "칼퇴근 소취합니다ㅎ",
          "original_form": "칼퇴근 소취합니다ㅎ",
          "speaker_id": "2",
          "time": "20210902 17:22"
        },
        {
          "id": "MDRW2100028065.1.4",
          "form": "전 게임 하는것보다 보는거 좋아해요ㅎ",
          "original_form": "전 게임 하는것보다 보는거 좋아해요ㅎ",
          "speaker_id": "2",
          "time": "20210902 17:22"
        },
        {
          "id": "MDRW2100028065.1.5",
          "form": "님은 무슨 게임 하시나요",
          "original_form": "님은 무슨 게임 하시나요",
          "speaker_id": "2",
          "time": "20210902 17:22"
        },
        {
          "id": "MDRW2100028065.1.6",
          "form": "오 무슨게임 보는거 좋아하세요?",
          "original_form": "오 무슨게임 보는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100028065.1.7",
          "form": "전 솔라테어? 카드게임 말고는 아무것도 안한답니다 ㅎㅎ",
          "original_form": "전 솔라테어? 카드게임 말고는 아무것도 안한답니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100028065.1.8",
          "form": "보는건  다 재밌는ㅈ것 같아요ㅎ",
          "original_form": "보는건  다 재밌는ㅈ것 같아요ㅎ",
          "speaker_id": "2",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100028065.1.9",
          "form": "심지어 카트라이더도 흥미진진하게 관전해요ㅎㅅ",
          "original_form": "심지어 카트라이더도 흥미진진하게 관전해요ㅎㅅ",
          "speaker_id": "2",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100028065.1.10",
          "form": "솔라테어는 어떤 게임인가요!",
          "original_form": "솔라테어는 어떤 게임인가요!",
          "speaker_id": "2",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100028065.1.11",
          "form": "카트라이더 요즘도 있군요 몰랐어요 ㅎㅎ추억의 게임",
          "original_form": "카트라이더 요즘도 있군요 몰랐어요 ㅎㅎ추억의 게임",
          "speaker_id": "1",
          "time": "20210902 17:25"
        },
        {
          "id": "MDRW2100028065.1.12",
          "form": "그 카드 순서대로 맞춰서 없애는게임이요! ㅋㅋ한국어로 뭐지ㅜㅜ",
          "original_form": "그 카드 순서대로 맞춰서 없애는게임이요! ㅋㅋ한국어로 뭐지ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 17:25"
        },
        {
          "id": "MDRW2100028065.1.13",
          "form": "아아 그거군요!",
          "original_form": "아아 그거군요!",
          "speaker_id": "2",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100028065.1.14",
          "form": "그거야말로 고전게임 아닌가요ㅎ",
          "original_form": "그거야말로 고전게임 아닌가요ㅎ",
          "speaker_id": "2",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100028065.1.15",
          "form": "pc게임 지뢰찾기와 같이",
          "original_form": "pc게임 지뢰찾기와 같이",
          "speaker_id": "2",
          "time": "20210902 17:29"
        },
        {
          "id": "MDRW2100028065.1.16",
          "form": "프리셀 게임?",
          "original_form": "프리셀 게임?",
          "speaker_id": "2",
          "time": "20210902 17:29"
        },
        {
          "id": "MDRW2100028065.1.17",
          "form": "네네 컴터에 기본으로 있는 그 게임이요 ㅋㅋㅋ카드게임",
          "original_form": "네네 컴터에 기본으로 있는 그 게임이요 ㅋㅋㅋ카드게임",
          "speaker_id": "1",
          "time": "20210902 17:30"
        },
        {
          "id": "MDRW2100028065.1.18",
          "form": "아무생각없이 하기 좋아요ㅎㅎ",
          "original_form": "아무생각없이 하기 좋아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:30"
        },
        {
          "id": "MDRW2100028065.1.19",
          "form": "요즘 핫한 게임들 아세요?",
          "original_form": "요즘 핫한 게임들 아세요?",
          "speaker_id": "1",
          "time": "20210902 17:30"
        },
        {
          "id": "MDRW2100028065.1.20",
          "form": "알죠 쿠킹덤이라거나ㅎㅎ",
          "original_form": "알죠 쿠킹덤이라거나ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 17:30"
        },
        {
          "id": "MDRW2100028065.1.21",
          "form": "스테디 배그 오버워치도 알죵",
          "original_form": "스테디 배그 오버워치도 알죵",
          "speaker_id": "2",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100028065.1.22",
          "form": "님은 딴게임 해볼생각 안하셨나요?",
          "original_form": "님은 딴게임 해볼생각 안하셨나요?",
          "speaker_id": "2",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100028065.1.23",
          "form": "오버워치 이름은 들어봤네요! ㅋㅋ",
          "original_form": "오버워치 이름은 들어봤네요! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:05"
        },
        {
          "id": "MDRW2100028065.1.24",
          "form": "아  저 요즘 집에있는 시간이많아져서 캔디크러쉬 다운받아보려구요 ㅋㅋ",
          "original_form": "아  저 요즘 집에있는 시간이많아져서 캔디크러쉬 다운받아보려구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:05"
        },
        {
          "id": "MDRW2100028065.1.25",
          "form": "고전게임 좋아하시는군요!",
          "original_form": "고전게임 좋아하시는군요!",
          "speaker_id": "2",
          "time": "20210902 18:06"
        },
        {
          "id": "MDRW2100028065.1.26",
          "form": "그렇네요 ㅎㅎㅎㅎ",
          "original_form": "그렇네요 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 18:06"
        },
        {
          "id": "MDRW2100028065.1.27",
          "form": "쿠킹덤은 무슨게임인가요?요리하는건가?",
          "original_form": "쿠킹덤은 무슨게임인가요?요리하는건가?",
          "speaker_id": "1",
          "time": "20210902 18:07"
        },
        {
          "id": "MDRW2100028065.1.28",
          "form": "전 간만에 프렌즈팡 당기네요ㅋ",
          "original_form": "전 간만에 프렌즈팡 당기네요ㅋ",
          "speaker_id": "2",
          "time": "20210902 18:07"
        },
        {
          "id": "MDRW2100028065.1.29",
          "form": "쿠키들이 아기자기 노는게임?",
          "original_form": "쿠키들이 아기자기 노는게임?",
          "speaker_id": "2",
          "time": "20210902 18:07"
        },
        {
          "id": "MDRW2100028065.1.30",
          "form": "캐디가 이쁘더라고요!",
          "original_form": "캐디가 이쁘더라고요!",
          "speaker_id": "2",
          "time": "20210902 18:07"
        },
        {
          "id": "MDRW2100028065.1.31",
          "form": "ㅋㅋㅋㅋ프렌즈팡 처음들어보는데",
          "original_form": "ㅋㅋㅋㅋ프렌즈팡 처음들어보는데",
