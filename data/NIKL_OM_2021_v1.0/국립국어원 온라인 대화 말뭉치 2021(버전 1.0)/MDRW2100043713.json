      "utterance": [
        {
          "id": "MDRW2100043713.1.1",
          "form": "네네 샌드위치먹었습니다 ㅎㅎ 다이어트 중이라구",
          "original_form": "네네 샌드위치먹었습니다 ㅎㅎ 다이어트 중이라구",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043713.1.2",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043713.1.3",
          "form": "저는 육개장 먹었어요ㅎㅎ",
          "original_form": "저는 육개장 먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043713.1.4",
          "form": "샌드위치 직접 만드시나요?",
          "original_form": "샌드위치 직접 만드시나요?",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043713.1.5",
          "form": "아니요 시켜먹었어요 ㅋㅋㅋㅋ",
          "original_form": "아니요 시켜먹었어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043713.1.6",
          "form": "6500원..",
          "original_form": "6500원..",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043713.1.7",
          "form": "배달로 2개씩 시켜서 저녁 점심",
          "original_form": "배달로 2개씩 시켜서 저녁 점심",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043713.1.8",
          "form": "이렇게 먹어요 ㅎㅎ",
          "original_form": "이렇게 먹어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043713.1.9",
          "form": "우와 샌드위치 맛있는곳 있나요?!",
          "original_form": "우와 샌드위치 맛있는곳 있나요?!",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043713.1.10",
          "form": "저희 동네에 소스랑 야채가 실한 곳이 있어요!",
          "original_form": "저희 동네에 소스랑 야채가 실한 곳이 있어요!",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043713.1.11",
          "form": "정착해서 일주일에 몇번씩 먹는 것 같아요 ㅎㅎ",
          "original_form": "정착해서 일주일에 몇번씩 먹는 것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043713.1.12",
          "form": "프렌차이즈는 아니겠죠?ㅠㅠㅠ",
          "original_form": "프렌차이즈는 아니겠죠?ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043713.1.13",
          "form": "동네에 그런곳 하나라도 있으면 좋을거 같아요!",
          "original_form": "동네에 그런곳 하나라도 있으면 좋을거 같아요!",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043713.1.14",
          "form": "네 아닙니다 ㅎㅎ",
          "original_form": "네 아닙니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043713.1.15",
          "form": "프렌차이즈는 야채가 너무 없어요ㅠㅠ",
          "original_form": "프렌차이즈는 야채가 너무 없어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043713.1.16",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100043713.1.17",
          "form": "비싸기만하고 재료도 별로라 안먹게되더라구요..",
          "original_form": "비싸기만하고 재료도 별로라 안먹게되더라구요..",
          "speaker_id": "1",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100043713.1.18",
          "form": "샌드위치 자주 드시나보네요!",
          "original_form": "샌드위치 자주 드시나보네요!",
          "speaker_id": "1",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043713.1.19",
          "form": "네 맞아요!",
          "original_form": "네 맞아요!",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043713.1.20",
          "form": "맞아요ㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043713.1.21",
          "form": "샌드위치는 어떤거 좋아하세료",
          "original_form": "샌드위치는 어떤거 좋아하세료",
          "speaker_id": "2",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043713.1.22",
          "form": "저 그냥 가리는거 없이 다 먹어요ㅎㅎ",
          "original_form": "저 그냥 가리는거 없이 다 먹어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043713.1.23",
          "form": "좋아하는 샌드위치 있으신가요?",
          "original_form": "좋아하는 샌드위치 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043713.1.24",
          "form": "저는클럽샌드위치나",
          "original_form": "저는클럽샌드위치나",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100043713.1.25",
          "form": "다좋아요 ㅎㅎ",
          "original_form": "다좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100043713.1.26",
          "form": "샌드위치는 다 맛있는거 같아요ㅎㅎ",
          "original_form": "샌드위치는 다 맛있는거 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100043713.1.27",
          "form": "치즈도 좋아하시나요?",
          "original_form": "치즈도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100043713.1.28",
          "form": "넹 !!",
          "original_form": "넹 !!",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100043713.1.29",
          "form": "햄두요 ㅎㅎ",
          "original_form": "햄두요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100043713.1.30",
          "form": "치즈한장 먹어야곗네요 ㅎㅎ",
          "original_form": "치즈한장 먹어야곗네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100043713.1.31",
          "form": "오 저도 치즈 좋아해요ㅎㅎ",
          "original_form": "오 저도 치즈 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100043713.1.32",
          "form": "스트링치즈!!",
          "original_form": "스트링치즈!!",
          "speaker_id": "1",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100043713.1.33",
          "form": "헉 맛나죠!!",
          "original_form": "헉 맛나죠!!",
          "speaker_id": "2",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100043713.1.34",
          "form": "배가 고프네요 ㅠㅠ",
          "original_form": "배가 고프네요 ㅠㅠ",
