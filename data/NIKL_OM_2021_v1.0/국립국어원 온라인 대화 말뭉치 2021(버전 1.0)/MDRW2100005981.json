      "utterance": [
        {
          "id": "MDRW2100005981.1.1",
          "form": "저녁머먹어",
          "original_form": "저녁머먹어",
          "speaker_id": "1",
          "time": "20210907 18:16"
        },
        {
          "id": "MDRW2100005981.1.2",
          "form": "저녁 모목어",
          "original_form": "저녁 모목어",
          "speaker_id": "1",
          "time": "20210907 18:16"
        },
        {
          "id": "MDRW2100005981.1.3",
          "form": "음 오늘은 참치마요 주먹밥에 에어프라이어에 만두돌려먹으려고",
          "original_form": "음 오늘은 참치마요 주먹밥에 에어프라이어에 만두돌려먹으려고",
          "speaker_id": "2",
          "time": "20210907 18:17"
        },
        {
          "id": "MDRW2100005981.1.4",
          "form": "너는 뭐먹을거니",
          "original_form": "너는 뭐먹을거니",
          "speaker_id": "2",
          "time": "20210907 18:17"
        },
        {
          "id": "MDRW2100005981.1.5",
          "form": "우와 대박이다",
          "original_form": "우와 대박이다",
          "speaker_id": "1",
          "time": "20210907 18:17"
        },
        {
          "id": "MDRW2100005981.1.6",
          "form": "난 쌀국슈 시켜놖어",
          "original_form": "난 쌀국슈 시켜놖어",
          "speaker_id": "1",
          "time": "20210907 18:17"
        },
        {
          "id": "MDRW2100005981.1.7",
          "form": "아파트 상가에 있지만 힘들어서 배달했어",
          "original_form": "아파트 상가에 있지만 힘들어서 배달했어",
          "speaker_id": "1",
          "time": "20210907 18:17"
        },
        {
          "id": "MDRW2100005981.1.8",
          "form": "바보같지ㅠㅠ",
          "original_form": "바보같지ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 18:17"
        },
        {
          "id": "MDRW2100005981.1.9",
          "form": "엥 나가기 귀찮으면 배달시키는거지 그게 왜 바보같어~",
          "original_form": "엥 나가기 귀찮으면 배달시키는거지 그게 왜 바보같어~",
          "speaker_id": "2",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100005981.1.10",
          "form": "배달비로 귀찮음을커버할수있다면야",
          "original_form": "배달비로 귀찮음을커버할수있다면야",
          "speaker_id": "2",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100005981.1.11",
          "form": "쌀국수맛있겠네",
          "original_form": "쌀국수맛있겠네",
          "speaker_id": "2",
          "time": "20210907 18:20"
        },
        {
          "id": "MDRW2100005981.1.12",
          "form": "그러게 말이야 ㅋㅋㅋㅋ",
          "original_form": "그러게 말이야 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:20"
        },
        {
          "id": "MDRW2100005981.1.13",
          "form": "얼른 오면 조케따",
          "original_form": "얼른 오면 조케따",
          "speaker_id": "1",
          "time": "20210907 18:20"
        },
        {
          "id": "MDRW2100005981.1.14",
          "form": "고수듬뿍넣어먹니",
          "original_form": "고수듬뿍넣어먹니",
          "speaker_id": "2",
          "time": "20210907 18:20"
        },
        {
          "id": "MDRW2100005981.1.15",
          "form": "나 7시부터 대학원 수업이야",
          "original_form": "나 7시부터 대학원 수업이야",
          "speaker_id": "1",
          "time": "20210907 18:20"
        },
        {
          "id": "MDRW2100005981.1.16",
          "form": "고수 그냥 조금만 ㅋㅋㅋ",
          "original_form": "고수 그냥 조금만 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:20"
        },
        {
          "id": "MDRW2100005981.1.17",
          "form": "고수 조아해?",
          "original_form": "고수 조아해?",
          "speaker_id": "1",
          "time": "20210907 18:20"
        },
        {
          "id": "MDRW2100005981.1.18",
          "form": "많이는 못먹지만 쌀국수에는 넣어서먹어야맛있더라",
          "original_form": "많이는 못먹지만 쌀국수에는 넣어서먹어야맛있더라",
          "speaker_id": "2",
          "time": "20210907 18:21"
        },
        {
          "id": "MDRW2100005981.1.19",
          "form": "그치 맞아",
          "original_form": "그치 맞아",
          "speaker_id": "1",
          "time": "20210907 18:21"
        },
        {
          "id": "MDRW2100005981.1.20",
          "form": "수업전에 든든하게먹어야겠네",
          "original_form": "수업전에 든든하게먹어야겠네",
          "speaker_id": "2",
          "time": "20210907 18:21"
        },
        {
          "id": "MDRW2100005981.1.21",
          "form": "주멱밥 직접 만둘어?",
          "original_form": "주멱밥 직접 만둘어?",
          "speaker_id": "1",
          "time": "20210907 18:21"
        },
        {
          "id": "MDRW2100005981.1.22",
          "form": "주먹밥은 직접 만들지\r\n집에 재료도 다 있고 만들기도 쉽고~!",
          "original_form": "주먹밥은 직접 만들지\r\n집에 재료도 다 있고 만들기도 쉽고~!",
          "speaker_id": "2",
          "time": "20210907 18:24"
        },
        {
          "id": "MDRW2100005981.1.23",
          "form": "와 정말",
          "original_form": "와 정말",
          "speaker_id": "1",
          "time": "20210907 18:25"
        },
        {
          "id": "MDRW2100005981.1.24",
          "form": "레시피 뭐염",
          "original_form": "레시피 뭐염",
          "speaker_id": "1",
          "time": "20210907 18:25"
        },
        {
          "id": "MDRW2100005981.1.25",
          "form": "주먹밥 레시피 뭐야 알려죠",
          "original_form": "주먹밥 레시피 뭐야 알려죠",
          "speaker_id": "1",
          "time": "20210907 18:26"
        }
      ]
    }
