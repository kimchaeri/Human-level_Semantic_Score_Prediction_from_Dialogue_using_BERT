      "utterance": [
        {
          "id": "MDRW2100014139.1.1",
          "form": "요새 태풍 때문에 날씨 너무 별로이지않나요ㅜ",
          "original_form": "요새 태풍 때문에 날씨 너무 별로이지않나요ㅜ",
          "speaker_id": "1",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014139.1.2",
          "form": "너무 습해요ㅋㅋㅋ",
          "original_form": "너무 습해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014139.1.3",
          "form": "태풍너무싫어요ㅠㅠ",
          "original_form": "태풍너무싫어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014139.1.4",
          "form": "나갔다가 집에 오면 머리도 눅눅해져있어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "나갔다가 집에 오면 머리도 눅눅해져있어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014139.1.5",
          "form": "그기분너무싫어요ㅠㅠ",
          "original_form": "그기분너무싫어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100014139.1.6",
          "form": "전 그거때문에 앞머리 없앨까까지 고민중이에요ㅋㅋㅋ",
          "original_form": "전 그거때문에 앞머리 없앨까까지 고민중이에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100014139.1.7",
          "form": "그래도 여긴 지금 비는 안와요! 습하긴 마찬가지지만ㅋㅋㅋ",
          "original_form": "그래도 여긴 지금 비는 안와요! 습하긴 마찬가지지만ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014139.1.8",
          "form": "비랑 더위만 아니면 날씨 좋아요",
          "original_form": "비랑 더위만 아니면 날씨 좋아요",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014139.1.9",
          "form": "저두요ㅋㅋㅋ",
          "original_form": "저두요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014139.1.10",
          "form": "아 아니다 요샌 눈오는거 별로 안좋아해요ㅋㅋㅋ",
          "original_form": "아 아니다 요샌 눈오는거 별로 안좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014139.1.11",
          "form": "미끄러질까봐ㅋㅋㅋ",
          "original_form": "미끄러질까봐ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014139.1.12",
          "form": "앜ㅋㅋㅋㄱ",
          "original_form": "앜ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014139.1.13",
          "form": "그러고보니",
          "original_form": "그러고보니",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014139.1.14",
          "form": "빙판길에 넘어진적이있네오",
          "original_form": "빙판길에 넘어진적이있네오",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014139.1.15",
          "form": "그것만 아니면 딱 좋은데",
          "original_form": "그것만 아니면 딱 좋은데",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014139.1.16",
          "form": "근데 비오든 눈오든 집에만 있어도 되면 뭐. . 그건 좋을거같아요ㅋㅋㅋ",
          "original_form": "근데 비오든 눈오든 집에만 있어도 되면 뭐. . 그건 좋을거같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014139.1.17",
          "form": "근데 이제 나가야되는 순간.. .싫어요ㅋㅋㅋ",
          "original_form": "근데 이제 나가야되는 순간.. .싫어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014139.1.18",
          "form": "코로나때문에 강제 집에있어야되는ㅜㅜ",
          "original_form": "코로나때문에 강제 집에있어야되는ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014139.1.19",
          "form": "ㅠㅠ하긴 그건 그래요ㅠ 날씨 좋아도 주말에 못나가고ㅠ 일의 노예가 된 기분",
          "original_form": "ㅠㅠ하긴 그건 그래요ㅠ 날씨 좋아도 주말에 못나가고ㅠ 일의 노예가 된 기분",
          "speaker_id": "1",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014139.1.20",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014139.1.21",
          "form": "날씨가 오늘의하루를 결정해줘요",
          "original_form": "날씨가 오늘의하루를 결정해줘요",
          "speaker_id": "2",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014139.1.22",
          "form": "오맞아요ㅋㅋㅋ 집에서 딱 나갔는데 날씨좋으면 기분좋더라구요ㅋㅋㅋ",
          "original_form": "오맞아요ㅋㅋㅋ 집에서 딱 나갔는데 날씨좋으면 기분좋더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:37"
        }
      ]
    }
