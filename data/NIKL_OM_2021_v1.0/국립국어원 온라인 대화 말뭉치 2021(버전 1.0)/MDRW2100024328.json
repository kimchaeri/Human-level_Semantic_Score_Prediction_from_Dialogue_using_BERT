      "utterance": [
        {
          "id": "MDRW2100024328.1.1",
          "form": "너 또 뭐샀길래 택배가 이렇게 많이 와",
          "original_form": "너 또 뭐샀길래 택배가 이렇게 많이 와",
          "speaker_id": "2",
          "time": "20210831 00:58"
        },
        {
          "id": "MDRW2100024328.1.2",
          "form": "택배 더 올거야",
          "original_form": "택배 더 올거야",
          "speaker_id": "1",
          "time": "20210831 01:00"
        },
        {
          "id": "MDRW2100024328.1.3",
          "form": "뭐샀는데",
          "original_form": "뭐샀는데",
          "speaker_id": "2",
          "time": "20210831 01:00"
        },
        {
          "id": "MDRW2100024328.1.4",
          "form": "비오틴이랑 보드게임",
          "original_form": "비오틴이랑 보드게임",
          "speaker_id": "1",
          "time": "20210831 01:02"
        },
        {
          "id": "MDRW2100024328.1.5",
          "form": "보드게임은 왜 샀어",
          "original_form": "보드게임은 왜 샀어",
          "speaker_id": "2",
          "time": "20210831 01:02"
        },
        {
          "id": "MDRW2100024328.1.6",
          "form": "오늘이 네이버페이 멤버십데이라",
          "original_form": "오늘이 네이버페이 멤버십데이라",
          "speaker_id": "1",
          "time": "20210831 01:07"
        },
        {
          "id": "MDRW2100024328.1.7",
          "form": "9프로 적립해준다그래서 싸게 샀어",
          "original_form": "9프로 적립해준다그래서 싸게 샀어",
          "speaker_id": "1",
          "time": "20210831 01:07"
        },
        {
          "id": "MDRW2100024328.1.8",
          "form": "한번 하고 팔거야",
          "original_form": "한번 하고 팔거야",
          "speaker_id": "1",
          "time": "20210831 01:09"
        },
        {
          "id": "MDRW2100024328.1.9",
          "form": "어디에 팔건데",
          "original_form": "어디에 팔건데",
          "speaker_id": "2",
          "time": "20210831 01:10"
        },
        {
          "id": "MDRW2100024328.1.10",
          "form": "중고나라나 당근마켓에",
          "original_form": "중고나라나 당근마켓에",
          "speaker_id": "1",
          "time": "20210831 01:10"
        },
        {
          "id": "MDRW2100024328.1.11",
          "form": "한번했는데 그게 팔려??",
          "original_form": "한번했는데 그게 팔려??",
          "speaker_id": "2",
          "time": "20210831 01:26"
        },
        {
          "id": "MDRW2100024328.1.12",
          "form": "새거보다 싸긴하니까 팔리지",
          "original_form": "새거보다 싸긴하니까 팔리지",
          "speaker_id": "1",
          "time": "20210831 01:26"
        },
        {
          "id": "MDRW2100024328.1.13",
          "form": "얼마에 파는데",
          "original_form": "얼마에 파는데",
          "speaker_id": "2",
          "time": "20210831 12:11"
        },
        {
          "id": "MDRW2100024328.1.14",
          "form": "23000원 새거는 30000만원이야",
          "original_form": "23000원 새거는 30000만원이야",
          "speaker_id": "1",
