      "utterance": [
        {
          "id": "MDRW2100003015.1.1",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "1",
          "time": "20210820 17:32"
        },
        {
          "id": "MDRW2100003015.1.2",
          "form": "네 비빔밥",
          "original_form": "네 비빔밥",
          "speaker_id": "2",
          "time": "20210820 17:32"
        },
        {
          "id": "MDRW2100003015.1.3",
          "form": "오 비빔밥도 맛나겠네요",
          "original_form": "오 비빔밥도 맛나겠네요",
          "speaker_id": "1",
          "time": "20210820 17:33"
        },
        {
          "id": "MDRW2100003015.1.4",
          "form": "맛있었죠 김 많이넣고",
          "original_form": "맛있었죠 김 많이넣고",
          "speaker_id": "2",
          "time": "20210820 17:34"
        },
        {
          "id": "MDRW2100003015.1.5",
          "form": "오 역시 비빔밥엔 김들어가야 맛나죠",
          "original_form": "오 역시 비빔밥엔 김들어가야 맛나죠",
          "speaker_id": "1",
          "time": "20210820 17:38"
        },
        {
          "id": "MDRW2100003015.1.6",
          "form": "배달음식 자주드세요?",
          "original_form": "배달음식 자주드세요?",
          "speaker_id": "2",
          "time": "20210820 21:19"
        },
        {
          "id": "MDRW2100003015.1.7",
          "form": "배달음식 자주는 아니고 가끔 먹어요 자주먹다보니 약간 몸이 힘들더라구용ㅎㅎ 자주드세요?",
          "original_form": "배달음식 자주는 아니고 가끔 먹어요 자주먹다보니 약간 몸이 힘들더라구용ㅎㅎ 자주드세요?",
          "speaker_id": "1",
          "time": "20210820 21:21"
        },
        {
          "id": "MDRW2100003015.1.8",
          "form": "한 2주 한번은 먹어요",
          "original_form": "한 2주 한번은 먹어요",
          "speaker_id": "2",
          "time": "20210820 21:21"
        },
        {
          "id": "MDRW2100003015.1.9",
          "form": "오 그래두 자주안드시네요 요새 자주드시는분은 거의 매일 시켜드시더라구요",
          "original_form": "오 그래두 자주안드시네요 요새 자주드시는분은 거의 매일 시켜드시더라구요",
          "speaker_id": "1",
          "time": "20210820 21:23"
        },
        {
          "id": "MDRW2100003015.1.10",
          "form": "코로나라서",
          "original_form": "코로나라서",
          "speaker_id": "2",
          "time": "20210820 21:23"
        },
        {
          "id": "MDRW2100003015.1.11",
          "form": "자주 배달시키는음식은(?)",
          "original_form": "자주 배달시키는음식은(?)",
          "speaker_id": "2",
          "time": "20210820 21:24"
        },
        {
          "id": "MDRW2100003015.1.12",
          "form": "아 그쵸 외식을 못하고하니ㅜ",
          "original_form": "아 그쵸 외식을 못하고하니ㅜ",
          "speaker_id": "1",
          "time": "20210820 21:24"
        },
        {
          "id": "MDRW2100003015.1.13",
          "form": "어떤거 자주 배달시키셔용?",
          "original_form": "어떤거 자주 배달시키셔용?",
          "speaker_id": "1",
          "time": "20210820 21:24"
        },
        {
          "id": "MDRW2100003015.1.14",
          "form": "아쉽지만 어쩔수없죠",
          "original_form": "아쉽지만 어쩔수없죠",
          "speaker_id": "2",
          "time": "20210820 21:24"
        },
        {
          "id": "MDRW2100003015.1.15",
          "form": "마자요ㅜ",
          "original_form": "마자요ㅜ",
          "speaker_id": "1",
          "time": "20210820 21:24"
        },
        {
          "id": "MDRW2100003015.1.16",
          "form": "배달용기 처리 잘하는것도 중요",
          "original_form": "배달용기 처리 잘하는것도 중요",
          "speaker_id": "2",
          "time": "20210820 21:25"
        }
      ]
