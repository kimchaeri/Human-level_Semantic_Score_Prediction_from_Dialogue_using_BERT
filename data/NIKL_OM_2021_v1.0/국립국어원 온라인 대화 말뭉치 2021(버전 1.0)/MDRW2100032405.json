      "utterance": [
        {
          "id": "MDRW2100032405.1.1",
          "form": "겜 하시는 거 있으세용?",
          "original_form": "겜 하시는 거 있으세용?",
          "speaker_id": "1",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100032405.1.2",
          "form": "모바일게임만 해요",
          "original_form": "모바일게임만 해요",
          "speaker_id": "2",
          "time": "20210905 17:03"
        },
        {
          "id": "MDRW2100032405.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210905 17:03"
        },
        {
          "id": "MDRW2100032405.1.4",
          "form": "헉 저두요 ㅋㅋㅋ",
          "original_form": "헉 저두요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:04"
        },
        {
          "id": "MDRW2100032405.1.5",
          "form": "고양이자리라고 폰겜해용",
          "original_form": "고양이자리라고 폰겜해용",
          "speaker_id": "1",
          "time": "20210905 17:04"
        },
        {
          "id": "MDRW2100032405.1.6",
          "form": "뭔가 귀여울것 같네요",
          "original_form": "뭔가 귀여울것 같네요",
          "speaker_id": "2",
          "time": "20210905 17:04"
        },
        {
          "id": "MDRW2100032405.1.7",
          "form": "무슨게임이예요?",
          "original_form": "무슨게임이예요?",
          "speaker_id": "2",
          "time": "20210905 17:05"
        },
        {
          "id": "MDRW2100032405.1.8",
          "form": "고양이 키우는 방치형 게임이에요!!",
          "original_form": "고양이 키우는 방치형 게임이에요!!",
          "speaker_id": "1",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100032405.1.9",
          "form": "약간 망겜이긴 한데 재밌어요 ㅋㅋㅋ",
          "original_form": "약간 망겜이긴 한데 재밌어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100032405.1.10",
          "form": "아 귀엽겠네요",
          "original_form": "아 귀엽겠네요",
          "speaker_id": "2",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100032405.1.11",
          "form": "친구가 추천해준건데",
          "original_form": "친구가 추천해준건데",
          "speaker_id": "2",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100032405.1.12",
          "form": "진짜 귀여워요~",
          "original_form": "진짜 귀여워요~",
          "speaker_id": "1",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100032405.1.13",
          "form": "오 네네",
          "original_form": "오 네네",
          "speaker_id": "1",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100032405.1.14",
          "form": "길잃은 고양이라고",
          "original_form": "길잃은 고양이라고",
          "speaker_id": "2",
          "time": "20210905 17:07"
        },
        {
          "id": "MDRW2100032405.1.15",
          "form": "게임 귀여워요",
          "original_form": "게임 귀여워요",
          "speaker_id": "2",
          "time": "20210905 17:07"
        },
        {
          "id": "MDRW2100032405.1.16",
          "form": "우왕 그것도 비슷할거 같아용ㅋㅋㅋ",
          "original_form": "우왕 그것도 비슷할거 같아용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:07"
        },
        {
          "id": "MDRW2100032405.1.17",
          "form": "귀엽겠다앙",
          "original_form": "귀엽겠다앙",
          "speaker_id": "1",
          "time": "20210905 17:07"
        },
        {
          "id": "MDRW2100032405.1.18",
          "form": "방탈출게임이예요",
          "original_form": "방탈출게임이예요",
          "speaker_id": "2",
          "time": "20210905 17:07"
        },
        {
          "id": "MDRW2100032405.1.19",
          "form": "일본거드라구요",
          "original_form": "일본거드라구요",
          "speaker_id": "2",
          "time": "20210905 17:07"
        },
        {
          "id": "MDRW2100032405.1.20",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210905 17:07"
        },
        {
          "id": "MDRW2100032405.1.21",
          "form": "우와 일본이 그런건 잘만드나봐용",
          "original_form": "우와 일본이 그런건 잘만드나봐용",
          "speaker_id": "1",
          "time": "20210905 17:08"
        },
        {
          "id": "MDRW2100032405.1.22",
          "form": "그쵸 고양이 관련된건 ㅎㅎ",
          "original_form": "그쵸 고양이 관련된건 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 17:08"
        },
        {
          "id": "MDRW2100032405.1.23",
          "form": "저 예전에 일본 겜은 역전재판이라구",
          "original_form": "저 예전에 일본 겜은 역전재판이라구",
          "speaker_id": "1",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100032405.1.24",
          "form": "그것만 해봤던거 같아요!",
          "original_form": "그것만 해봤던거 같아요!",
          "speaker_id": "1",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100032405.1.25",
          "form": "아 저그거 게임 실황만봤어요",
          "original_form": "아 저그거 게임 실황만봤어요",
          "speaker_id": "2",
          "time": "20210905 17:10"
        }
      ]
    }
