        },
        {
          "id": "MDRW2100027209.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:28"
        },
        {
          "id": "MDRW2100027209.1.3",
          "form": "맞습니다. 나-먹는거=0",
          "original_form": "맞습니다. 나-먹는거=0",
          "speaker_id": "1",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100027209.1.4",
          "form": "먹는데 돈써야죠",
          "original_form": "먹는데 돈써야죠",
          "speaker_id": "2",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100027209.1.5",
          "form": "맛있는음식은",
          "original_form": "맛있는음식은",
          "speaker_id": "2",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100027209.1.6",
          "form": "세상에 먹을게 지천이지요",
          "original_form": "세상에 먹을게 지천이지요",
          "speaker_id": "1",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100027209.1.7",
          "form": "돈이아깝지 않아요",
          "original_form": "돈이아깝지 않아요",
          "speaker_id": "2",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100027209.1.8",
          "form": "며칠전에 옻삼계탕먹었어요",
          "original_form": "며칠전에 옻삼계탕먹었어요",
          "speaker_id": "2",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100027209.1.9",
          "form": "도넛도 좋아하나요",
          "original_form": "도넛도 좋아하나요",
          "speaker_id": "1",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100027209.1.10",
          "form": "옻 알러지없나요",
          "original_form": "옻 알러지없나요",
          "speaker_id": "1",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100027209.1.11",
          "form": "몸이 찬데 옻이 따뜻하게 해줘요",
          "original_form": "몸이 찬데 옻이 따뜻하게 해줘요",
          "speaker_id": "2",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100027209.1.12",
          "form": "없어서 마음껏먹어요",
          "original_form": "없어서 마음껏먹어요",
          "speaker_id": "2",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100027209.1.13",
          "form": "다행이에요",
          "original_form": "다행이에요",
          "speaker_id": "2",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100027209.1.14",
          "form": "도넛은 한두개먹어요",
          "original_form": "도넛은 한두개먹어요",
          "speaker_id": "2",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100027209.1.15",
          "form": "좋군요",
          "original_form": "좋군요",
          "speaker_id": "1",
          "time": "20210902 10:30"
        },
        {
          "id": "MDRW2100027209.1.16",
          "form": "많이달아서요",
          "original_form": "많이달아서요",
          "speaker_id": "2",
          "time": "20210902 10:30"
        },
        {
          "id": "MDRW2100027209.1.17",
          "form": "크리스피도넛 좋아합니다",
          "original_form": "크리스피도넛 좋아합니다",
          "speaker_id": "1",
          "time": "20210902 10:30"
        },
        {
          "id": "MDRW2100027209.1.18",
          "form": "커피랑 먹으면 많이 먹을수있죠",
          "original_form": "커피랑 먹으면 많이 먹을수있죠",
          "speaker_id": "2",
          "time": "20210902 10:30"
        },
        {
          "id": "MDRW2100027209.1.19",
          "form": "던킨도 맛나요",
          "original_form": "던킨도 맛나요",
          "speaker_id": "2",
          "time": "20210902 10:30"
        },
        {
          "id": "MDRW2100027209.1.20",
          "form": "제주도엔 없어서 던킨 먹습니다",
          "original_form": "제주도엔 없어서 던킨 먹습니다",
          "speaker_id": "1",
          "time": "20210902 10:30"
        },
        {
          "id": "MDRW2100027209.1.21",
          "form": "달쓴달쓴 좋은조합",
          "original_form": "달쓴달쓴 좋은조합",
          "speaker_id": "2",
          "time": "20210902 10:30"
        },
        {
          "id": "MDRW2100027209.1.22",
          "form": "던킨이 종류가 다양하죠",
          "original_form": "던킨이 종류가 다양하죠",
          "speaker_id": "1",
          "time": "20210902 10:30"
        },
        {
          "id": "MDRW2100027209.1.23",
          "form": "크리스피로 많이드세요",
          "original_form": "크리스피로 많이드세요",
          "speaker_id": "2",
          "time": "20210902 10:30"
        }
