      "utterance": [
        {
          "id": "MDRW2100006987.1.1",
          "form": "약간 논외로 저는 알파카도 좋아해요...",
          "original_form": "약간 논외로 저는 알파카도 좋아해요...",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006987.1.2",
          "form": "알파카!!",
          "original_form": "알파카!!",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006987.1.3",
          "form": "제주도에 알파카농장? 있다고 하더라고요",
          "original_form": "제주도에 알파카농장? 있다고 하더라고요",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006987.1.4",
          "form": "저도 가서 직접 알파카 한번 보고싶어요",
          "original_form": "저도 가서 직접 알파카 한번 보고싶어요",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006987.1.5",
          "form": "우와ㅠ 알파카농장이라니 천국이네요",
          "original_form": "우와ㅠ 알파카농장이라니 천국이네요",
          "speaker_id": "1",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006987.1.6",
          "form": "직접 체험 농장 간적 있으세요?",
          "original_form": "직접 체험 농장 간적 있으세요?",
          "speaker_id": "2",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006987.1.7",
          "form": "초등학교 때 고구마 캐기? 그런 거 말곤 안 가봤어요",
          "original_form": "초등학교 때 고구마 캐기? 그런 거 말곤 안 가봤어요",
          "speaker_id": "1",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006987.1.8",
          "form": "고구마캐기 ㅋㅋㅋㄱㅋㅋ 아 너무 귀엽네요 고구마",
          "original_form": "고구마캐기 ㅋㅋㅋㄱㅋㅋ 아 너무 귀엽네요 고구마",
          "speaker_id": "2",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100006987.1.9",
          "form": "요즘은 고양이카페도 많고 이색동물카페도 많더라고요",
          "original_form": "요즘은 고양이카페도 많고 이색동물카페도 많더라고요",
          "speaker_id": "2",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100006987.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ고구마 헷",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ고구마 헷",
          "speaker_id": "1",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100006987.1.11",
          "form": "맞아요 동물카페 넘 가고 싶어요",
          "original_form": "맞아요 동물카페 넘 가고 싶어요",
          "speaker_id": "1",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100006987.1.12",
          "form": "근데 사실 제가 동물 털 알러지가 있어서 엄두를 못 내고 있어요ㅠㅠ",
          "original_form": "근데 사실 제가 동물 털 알러지가 있어서 엄두를 못 내고 있어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100006987.1.13",
          "form": "아 그러면 속상하죠ㅠ",
          "original_form": "아 그러면 속상하죠ㅠ",
          "speaker_id": "2",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100006987.1.14",
          "form": "저는 키우지않는 이유가 책임질 자신이 없어서 못키우겠더라고요",
          "original_form": "저는 키우지않는 이유가 책임질 자신이 없어서 못키우겠더라고요",
          "speaker_id": "2",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100006987.1.15",
          "form": "그래도 다행히 취향이 파충류도 좋아하긴 해서 이구아나 같은 아이들도 키워봤어요",
          "original_form": "그래도 다행히 취향이 파충류도 좋아하긴 해서 이구아나 같은 아이들도 키워봤어요",
          "speaker_id": "1",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100006987.1.16",
          "form": "책임지는 거 어렵죠... 나 하나 책임지기도 힘든데ㅠㅠ",
          "original_form": "책임지는 거 어렵죠... 나 하나 책임지기도 힘든데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100006987.1.17",
          "form": "맞아요 ㅠ",
          "original_form": "맞아요 ㅠ",
          "speaker_id": "2",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100006987.1.18",
          "form": "헛 파충류",
          "original_form": "헛 파충류",
          "speaker_id": "2",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100006987.1.19",
          "form": "도마뱀 이런 아이인가요?",
          "original_form": "도마뱀 이런 아이인가요?",
          "speaker_id": "2",
          "time": "20210822 00:19"
        },
        {
          "id": "MDRW2100006987.1.20",
          "form": "네!! 이구아나 파충류",
          "original_form": "네!! 이구아나 파충류",
          "speaker_id": "1",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100006987.1.21",
          "form": "아니 도마뱀",
          "original_form": "아니 도마뱀",
          "speaker_id": "1",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100006987.1.22",
          "form": "둘 다 키우ㅜ봐ㅛ어요",
          "original_form": "둘 다 키우ㅜ봐ㅛ어요",
          "speaker_id": "1",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100006987.1.23",
          "form": "오 신기해요... 먹이는 어떤걸 주나요?",
          "original_form": "오 신기해요... 먹이는 어떤걸 주나요?",
          "speaker_id": "2",
          "time": "20210822 00:20"
        },
        {
          "id": "MDRW2100006987.1.24",
          "form": "동물 시체나 곤충 같은 거 안 주고",
          "original_form": "동물 시체나 곤충 같은 거 안 주고",
          "speaker_id": "1",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100006987.1.25",
          "form": "그냥 사료 같은 거 사서 줬었는데",
          "original_form": "그냥 사료 같은 거 사서 줬었는데",
          "speaker_id": "1",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100006987.1.26",
          "form": "잘 키우시는 분들은 곤충 ㅠㅠ 도 주시더라고요",
          "original_form": "잘 키우시는 분들은 곤충 ㅠㅠ 도 주시더라고요",
          "speaker_id": "1",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100006987.1.27",
          "form": "곤충... 전 아마 절대 못만질것같아요ㅠ",
          "original_form": "곤충... 전 아마 절대 못만질것같아요ㅠ",
          "speaker_id": "2",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100006987.1.28",
          "form": "저도요ㅠ",
          "original_form": "저도요ㅠ",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100006987.1.29",
          "form": "파충류 뱀 키우시는분들이 은근 많으시더라고요",
          "original_form": "파충류 뱀 키우시는분들이 은근 많으시더라고요",
          "speaker_id": "2",
          "time": "20210822 00:23"
        }
      ]
    }
