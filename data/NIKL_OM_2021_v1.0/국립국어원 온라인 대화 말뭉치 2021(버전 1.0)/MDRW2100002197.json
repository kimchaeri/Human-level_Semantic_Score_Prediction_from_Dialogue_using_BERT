      "utterance": [
        {
          "id": "MDRW2100002197.1.1",
          "form": "요즘 운동 하시는 거 있으세요?",
          "original_form": "요즘 운동 하시는 거 있으세요?",
          "speaker_id": "2",
          "time": "20210820 00:27"
        },
        {
          "id": "MDRW2100002197.1.2",
          "form": "운동은 딱히 안하는 편이에요. 하시는 운동 있으세요?",
          "original_form": "운동은 딱히 안하는 편이에요. 하시는 운동 있으세요?",
          "speaker_id": "1",
          "time": "20210820 00:28"
        },
        {
          "id": "MDRW2100002197.1.3",
          "form": "전 주말에 홈트 하려고 노력하고 있어요",
          "original_form": "전 주말에 홈트 하려고 노력하고 있어요",
          "speaker_id": "2",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002197.1.4",
          "form": "곧 운동도 생각중이구요ㅋㅋㅋㅋ!",
          "original_form": "곧 운동도 생각중이구요ㅋㅋㅋㅋ!",
          "speaker_id": "2",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002197.1.5",
          "form": "홈트 좋죠",
          "original_form": "홈트 좋죠",
          "speaker_id": "1",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002197.1.6",
          "form": "집에서 틈틈히 운동 해줘야해요",
          "original_form": "집에서 틈틈히 운동 해줘야해요",
          "speaker_id": "1",
          "time": "20210820 00:30"
        },
        {
          "id": "MDRW2100002197.1.7",
          "form": "평소 좋아하는 운동있나요?",
          "original_form": "평소 좋아하는 운동있나요?",
          "speaker_id": "1",
          "time": "20210820 00:30"
        },
        {
          "id": "MDRW2100002197.1.8",
          "form": "저는 운동을 하는것 보다 보는 걸 좋아해서ㅎㅎ",
          "original_form": "저는 운동을 하는것 보다 보는 걸 좋아해서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 00:30"
        },
        {
          "id": "MDRW2100002197.1.9",
          "form": "귀차니즘으로 인해 운동을 억지로 하고 있어요 흑",
          "original_form": "귀차니즘으로 인해 운동을 억지로 하고 있어요 흑",
          "speaker_id": "2",
          "time": "20210820 00:30"
        },
        {
          "id": "MDRW2100002197.1.10",
          "form": "저도 보는게 더 좋답니다! 배워 본 운동은 있으신가요??!",
          "original_form": "저도 보는게 더 좋답니다! 배워 본 운동은 있으신가요??!",
          "speaker_id": "2",
          "time": "20210820 00:31"
        },
        {
          "id": "MDRW2100002197.1.11",
          "form": "아니요ㅎㅎ.. 뭐랄까 그냥 무작정 걷는건 많이 해봤어요",
          "original_form": "아니요ㅎㅎ.. 뭐랄까 그냥 무작정 걷는건 많이 해봤어요",
          "speaker_id": "1",
          "time": "20210820 00:31"
        },
        {
          "id": "MDRW2100002197.1.12",
          "form": "오 산책도 좋죠",
          "original_form": "오 산책도 좋죠",
          "speaker_id": "2",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002197.1.13",
          "form": "요즘 러닝도 많이 하잖아요!!",
          "original_form": "요즘 러닝도 많이 하잖아요!!",
          "speaker_id": "2",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002197.1.14",
          "form": "근데 코로나로 산책도 조심조심하게되더라구요",
          "original_form": "근데 코로나로 산책도 조심조심하게되더라구요",
          "speaker_id": "1",
          "time": "20210820 00:33"
        },
        {
          "id": "MDRW2100002197.1.15",
          "form": "괜히 나갔다가 코로나걸릴까봐 무서워요 그래서 다들 홈트하나봐요",
          "original_form": "괜히 나갔다가 코로나걸릴까봐 무서워요 그래서 다들 홈트하나봐요",
          "speaker_id": "1",
          "time": "20210820 00:33"
        },
        {
          "id": "MDRW2100002197.1.16",
          "form": "홈트의 매력은 무엇인가요",
          "original_form": "홈트의 매력은 무엇인가요",
          "speaker_id": "1",
          "time": "20210820 00:35"
        },
        {
          "id": "MDRW2100002197.1.17",
          "form": "ㅋㅋ ㅋㅋ 마스크로 무장하고 가야하는데 힘드니까요 ㅠㅠ",
          "original_form": "ㅋㅋ ㅋㅋ 마스크로 무장하고 가야하는데 힘드니까요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 00:36"
        },
        {
          "id": "MDRW2100002197.1.18",
          "form": "홈트는 그냥 ... 집에서 간편하게 할 수 있는 운동이랄까",
          "original_form": "홈트는 그냥 ... 집에서 간편하게 할 수 있는 운동이랄까",
          "speaker_id": "2",
          "time": "20210820 00:36"
        },
        {
          "id": "MDRW2100002197.1.19",
          "form": "뭔가 눈치도 안보고 막하게되서 좋아요",
          "original_form": "뭔가 눈치도 안보고 막하게되서 좋아요",
          "speaker_id": "2",
          "time": "20210820 00:36"
        },
        {
          "id": "MDRW2100002197.1.20",
          "form": "그러네요! 마스크 없이 눈치볼것없이 되게 좋은데요!",
          "original_form": "그러네요! 마스크 없이 눈치볼것없이 되게 좋은데요!",
          "speaker_id": "1",
          "time": "20210820 00:36"
        },
        {
          "id": "MDRW2100002197.1.21",
          "form": "그렇죠 마스크 쓰고 운동하면 진짜 기절이에요",
          "original_form": "그렇죠 마스크 쓰고 운동하면 진짜 기절이에요",
          "speaker_id": "2",
          "time": "20210820 00:37"
        },
        {
          "id": "MDRW2100002197.1.22",
          "form": "숨막혀서 흑",
          "original_form": "숨막혀서 흑",
          "speaker_id": "2",
          "time": "20210820 00:37"
        },
        {
          "id": "MDRW2100002197.1.23",
          "form": "산소부족으로 그쵸",
          "original_form": "산소부족으로 그쵸",
          "speaker_id": "1",
          "time": "20210820 00:37"
        },
        {
          "id": "MDRW2100002197.1.24",
          "form": "홈트 좋은거같아요!",
          "original_form": "홈트 좋은거같아요!",
          "speaker_id": "1",
          "time": "20210820 00:38"
        },
        {
          "id": "MDRW2100002197.1.25",
          "form": "저도 해볼까봐요",
          "original_form": "저도 해볼까봐요",
          "speaker_id": "1",
          "time": "20210820 00:38"
        },
        {
          "id": "MDRW2100002197.1.26",
          "form": "해보는 거 추천해요 !!!",
          "original_form": "해보는 거 추천해요 !!!",
          "speaker_id": "2",
          "time": "20210820 00:39"
        },
        {
          "id": "MDRW2100002197.1.27",
          "form": "운동은 정말 필수더라구요ㅋㅋㅋㅋ",
          "original_form": "운동은 정말 필수더라구요ㅋㅋㅋㅋ",
