      "utterance": [
        {
          "id": "MDRW2100009535.1.1",
          "form": "전 바닷가랑 호수랑 강이랑 동네여기저기 걸어요 ㅋ",
          "original_form": "전 바닷가랑 호수랑 강이랑 동네여기저기 걸어요 ㅋ",
          "speaker_id": "1",
          "time": "20210823 00:17"
        },
        {
          "id": "MDRW2100009535.1.2",
          "form": "name1님은 아직 미혼이신가요?",
          "original_form": "&name1&님은 아직 미혼이신가요?",
          "speaker_id": "2",
          "time": "20210823 00:50"
        },
        {
          "id": "MDRW2100009535.1.3",
          "form": "네 엄마랑 둘이 지지고볶고 살아여",
          "original_form": "네 엄마랑 둘이 지지고볶고 살아여",
          "speaker_id": "1",
          "time": "20210823 00:51"
        },
        {
          "id": "MDRW2100009535.1.4",
          "form": "제일 부럽네요",
          "original_form": "제일 부럽네요{emoji:♡}",
          "speaker_id": "2",
          "time": "20210823 00:51"
        },
        {
          "id": "MDRW2100009535.1.5",
          "form": "아니 엄만 절 이제 좀치워버리고싶어하셍ㆍ",
          "original_form": "아니 엄만 절 이제 좀치워버리고싶어하셍ㆍ",
          "speaker_id": "1",
          "time": "20210823 00:52"
        },
        {
          "id": "MDRW2100009535.1.6",
          "form": "하세요",
          "original_form": "하세요",
          "speaker_id": "1",
          "time": "20210823 00:52"
        },
        {
          "id": "MDRW2100009535.1.7",
          "form": "상하기 직전 생선인가",
          "original_form": "상하기 직전 생선인가",
          "speaker_id": "1",
          "time": "20210823 00:52"
        },
        {
          "id": "MDRW2100009535.1.8",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:52"
        },
        {
          "id": "MDRW2100009535.1.9",
          "form": "유통기한 넉넉하다\r\n말씀 드리세요ㅋㅋㅋㅋㅋ",
          "original_form": "유통기한 넉넉하다\r\n말씀 드리세요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:54"
        },
        {
          "id": "MDRW2100009535.1.10",
          "form": "가사일은 음 저는 제가 그럭저럭하는거 같은데",
          "original_form": "가사일은 음 저는 제가 그럭저럭하는거 같은데",
          "speaker_id": "1",
          "time": "20210823 00:56"
        },
        {
          "id": "MDRW2100009535.1.11",
          "form": "늘 마음에 안 들어 하셔서 ㅋㅋ",
          "original_form": "늘 마음에 안 들어 하셔서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:57"
        },
        {
          "id": "MDRW2100009535.1.12",
          "form": "엄마 눈에는 찰 수가 없어요ㅋㅋ",
          "original_form": "엄마 눈에는 찰 수가 없어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 01:01"
        },
        {
          "id": "MDRW2100009535.1.13",
          "form": "저 결혼 7년차인데도\r\n엄마는 아직도 저보면\r\n소꿉장난하냐 하세여ㅜㅜ",
          "original_form": "저 결혼 7년차인데도\r\n엄마는 아직도 저보면\r\n소꿉장난하냐 하세여ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 01:01"
        },
        {
          "id": "MDRW2100009535.1.14",
          "form": "근데 집안일은 정말 해도 티도 안나고",
          "original_form": "근데 집안일은 정말 해도 티도 안나고",
          "speaker_id": "1",
          "time": "20210823 09:40"
        },
        {
          "id": "MDRW2100009535.1.15",
          "form": "손빨래 진짜 너무 싫엉ㆍ",
          "original_form": "손빨래 진짜 너무 싫엉ㆍ",
          "speaker_id": "1",
          "time": "20210823 09:40"
        },
        {
          "id": "MDRW2100009535.1.16",
          "form": "어요",
          "original_form": "어요",
          "speaker_id": "1",
          "time": "20210823 09:40"
        },
        {
          "id": "MDRW2100009535.1.17",
          "form": "맞아요ㅋㅋ 안하면 바로 티나면서",
          "original_form": "맞아요ㅋㅋ 안하면 바로 티나면서",
          "speaker_id": "2",
          "time": "20210823 09:48"
        },
        {
          "id": "MDRW2100009535.1.18",
          "form": "하는건 티도 안나는거 집안일이에여ㅜ",
          "original_form": "하는건 티도 안나는거 집안일이에여ㅜ",
          "speaker_id": "2",
          "time": "20210823 09:48"
        },
        {
          "id": "MDRW2100009535.1.19",
          "form": "손빨래까지 하시고 대단하세요:)",
          "original_form": "손빨래까지 하시고 대단하세요:)",
          "speaker_id": "2",
          "time": "20210823 09:48"
        },
        {
          "id": "MDRW2100009535.1.20",
          "form": "아니 사실 안하고싶어요ㅜ",
          "original_form": "아니 사실 안하고싶어요ㅜ",
          "speaker_id": "1",
          "time": "20210823 09:51"
        },
        {
          "id": "MDRW2100009535.1.21",
          "form": "혼자 살땐 속옷도 그냥 세탁기에 맡김ㅜ",
          "original_form": "혼자 살땐 속옷도 그냥 세탁기에 맡김ㅜ",
          "speaker_id": "1",
          "time": "20210823 09:51"
        },
        {
          "id": "MDRW2100009535.1.22",
          "form": "하 근데 엄마가 그꼴을 못보세요",
          "original_form": "하 근데 엄마가 그꼴을 못보세요",
          "speaker_id": "1",
          "time": "20210823 09:51"
        },
        {
          "id": "MDRW2100009535.1.23",
          "form": "특히 여름옷은 무조건 손빨래  해야한다 주의세요",
          "original_form": "특히 여름옷은 무조건 손빨래  해야한다 주의세요",
          "speaker_id": "1",
          "time": "20210823 09:51"
        },
        {
          "id": "MDRW2100009535.1.24",
          "form": "저희 엄마도 그러세요ㅠ",
          "original_form": "저희 엄마도 그러세요ㅠ",
          "speaker_id": "2",
          "time": "20210823 09:53"
        },
        {
          "id": "MDRW2100009535.1.25",
          "form": "하지만 저는 다 세탁기행입니다ㅋㅋ",
          "original_form": "하지만 저는 다 세탁기행입니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 09:53"
        },
        {
          "id": "MDRW2100009535.1.26",
          "form": "엄마 이사하실때 식기세척기도\r\n사드렸는데 맨날 손설거지 하세요",
          "original_form": "엄마 이사하실때 식기세척기도\r\n사드렸는데 맨날 손설거지 하세요",
          "speaker_id": "2",
          "time": "20210823 09:54"
        },
        {
          "id": "MDRW2100009535.1.27",
          "form": "전 근데 그게 너무 싫어요",
          "original_form": "전 근데 그게 너무 싫어요",
          "speaker_id": "1",
          "time": "20210823 09:54"
        },
        {
          "id": "MDRW2100009535.1.28",
          "form": "근데 또 맞는 말이긴해요",
