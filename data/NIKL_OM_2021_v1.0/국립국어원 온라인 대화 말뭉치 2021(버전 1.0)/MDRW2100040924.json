      "utterance": [
        {
          "id": "MDRW2100040924.1.1",
          "form": "저희집에 전신거울이 두개에요",
          "original_form": "저희집에 전신거울이 두개에요",
          "speaker_id": "2",
          "time": "20210905 12:59"
        },
        {
          "id": "MDRW2100040924.1.2",
          "form": "진짜요?!?!",
          "original_form": "진짜요?!?!",
          "speaker_id": "1",
          "time": "20210905 12:59"
        },
        {
          "id": "MDRW2100040924.1.3",
          "form": "작은거 큰거 2개?",
          "original_form": "작은거 큰거 2개?",
          "speaker_id": "1",
          "time": "20210905 12:59"
        },
        {
          "id": "MDRW2100040924.1.4",
          "form": "네 궁금하세요?",
          "original_form": "네 궁금하세요?",
          "speaker_id": "2",
          "time": "20210905 12:59"
        },
        {
          "id": "MDRW2100040924.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210905 12:59"
        },
        {
          "id": "MDRW2100040924.1.6",
          "form": "옷방 1 화장실앞 1",
          "original_form": "옷방 1 화장실앞 1",
          "speaker_id": "2",
          "time": "20210905 13:00"
        },
        {
          "id": "MDRW2100040924.1.7",
          "form": "아하 옷방에 있는게 작은거에요?",
          "original_form": "아하 옷방에 있는게 작은거에요?",
          "speaker_id": "1",
          "time": "20210905 13:00"
        },
        {
          "id": "MDRW2100040924.1.8",
          "form": "옷방이크죠",
          "original_form": "옷방이크죠",
          "speaker_id": "2",
          "time": "20210905 13:00"
        },
        {
          "id": "MDRW2100040924.1.9",
          "form": "사실 작은걸 더마니써요",
          "original_form": "사실 작은걸 더마니써요",
          "speaker_id": "2",
          "time": "20210905 13:01"
        },
        {
          "id": "MDRW2100040924.1.10",
          "form": "아 뭐에요 ㅡㅡ 큰거 추천하신다면서요",
          "original_form": "아 뭐에요 ㅡㅡ 큰거 추천하신다면서요",
          "speaker_id": "1",
          "time": "20210905 13:02"
        },
        {
          "id": "MDRW2100040924.1.11",
          "form": "큰게좋죠",
          "original_form": "큰게좋죠",
          "speaker_id": "2",
          "time": "20210905 13:02"
        },
        {
          "id": "MDRW2100040924.1.12",
          "form": "옷방이더멀어서요",
          "original_form": "옷방이더멀어서요",
          "speaker_id": "2",
          "time": "20210905 13:02"
        },
        {
          "id": "MDRW2100040924.1.13",
          "form": "집이좀커서",
          "original_form": "집이좀커서",
          "speaker_id": "2",
          "time": "20210905 13:02"
        },
        {
          "id": "MDRW2100040924.1.14",
          "form": "작은거사세요",
          "original_form": "작은거사세요",
          "speaker_id": "2",
          "time": "20210905 13:03"
        },
        {
          "id": "MDRW2100040924.1.15",
          "form": "큰거사면 엄마한테등짝맞을거같음",
          "original_form": "큰거사면 엄마한테등짝맞을거같음",
          "speaker_id": "2",
          "time": "20210905 13:03"
        },
        {
          "id": "MDRW2100040924.1.16",
          "form": "넹 작는거라면 폭이 몇cm정도죠?",
          "original_form": "넹 작는거라면 폭이 몇cm정도죠?",
          "speaker_id": "1",
          "time": "20210905 13:04"
        },
        {
          "id": "MDRW2100040924.1.17",
          "form": "40??",
          "original_form": "40??",
          "speaker_id": "2",
          "time": "20210905 13:04"
        },
        {
          "id": "MDRW2100040924.1.18",
          "form": "45?",
          "original_form": "45?",
          "speaker_id": "2",
          "time": "20210905 13:04"
        },
        {
          "id": "MDRW2100040924.1.19",
          "form": "젤 흔한거중에 와이드로요",
          "original_form": "젤 흔한거중에 와이드로요",
          "speaker_id": "2",
          "time": "20210905 13:05"
        },
        {
          "id": "MDRW2100040924.1.20",
          "form": "가격은 어느정도가 적당한건가여",
          "original_form": "가격은 어느정도가 적당한건가여",
          "speaker_id": "1",
          "time": "20210905 13:05"
        },
        {
          "id": "MDRW2100040924.1.21",
          "form": "3마넌햇던가 ???  ....",
          "original_form": "3마넌햇던가 ???  ....",
          "speaker_id": "2",
          "time": "20210905 13:06"
        },
        {
          "id": "MDRW2100040924.1.22",
          "form": "오 싸네요 근데 프레임있으면 더 비싸죠?",
          "original_form": "오 싸네요 근데 프레임있으면 더 비싸죠?",
          "speaker_id": "1",
          "time": "20210905 13:06"
        },
        {
          "id": "MDRW2100040924.1.23",
          "form": "그죵",
          "original_form": "그죵",
          "speaker_id": "2",
          "time": "20210905 13:06"
        },
        {
          "id": "MDRW2100040924.1.24",
          "form": "근데거울 생각보다 공간차지가 크요",
          "original_form": "근데거울 생각보다 공간차지가 크요",
          "speaker_id": "2",
          "time": "20210905 13:06"
        },
        {
          "id": "MDRW2100040924.1.25",
