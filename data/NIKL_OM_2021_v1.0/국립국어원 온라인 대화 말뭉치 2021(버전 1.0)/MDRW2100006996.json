      "utterance": [
        {
          "id": "MDRW2100006996.1.1",
          "form": "꽃매미 ㅋㅋㅋㅋㅋ근데 예전엔 학교가는길에",
          "original_form": "꽃매미 ㅋㅋㅋㅋㅋ근데 예전엔 학교가는길에",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006996.1.2",
          "form": "엄청많았는데",
          "original_form": "엄청많았는데",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006996.1.3",
          "form": "이젠안보이는거같아 잘...",
          "original_form": "이젠안보이는거같아 잘...",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006996.1.4",
          "form": "우리 중고딩때가",
          "original_form": "우리 중고딩때가",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006996.1.5",
          "form": "피크였지",
          "original_form": "피크였지",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006996.1.6",
          "form": "맞아ㅋㅋㅋㅋ",
          "original_form": "맞아ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100006996.1.7",
          "form": "못본지 진짜 오래된듯",
          "original_form": "못본지 진짜 오래된듯",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100006996.1.8",
          "form": "우리 고딩때는 걍 구름다리만 가면",
          "original_form": "우리 고딩때는 걍 구름다리만 가면",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100006996.1.9",
          "form": "진짜 많이 있었는데",
          "original_form": "진짜 많이 있었는데",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100006996.1.10",
          "form": "등하굣길 메이트였지...꽃매미..",
          "original_form": "등하굣길 메이트였지...꽃매미..",
          "speaker_id": "2",
          "time": "20210822 00:16"
        },
        {
          "id": "MDRW2100006996.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ진심 ㅇㅈ...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ진심 ㅇㅈ...",
          "speaker_id": "1",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100006996.1.12",
          "form": "거기가 산이 위에 있어서",
          "original_form": "거기가 산이 위에 있어서",
          "speaker_id": "1",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100006996.1.13",
          "form": "구름다리 위에",
          "original_form": "구름다리 위에",
          "speaker_id": "1",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100006996.1.14",
          "form": "그렇게 많았나...?",
          "original_form": "그렇게 많았나...?",
          "speaker_id": "1",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100006996.1.15",
          "form": "맞아 .... 우리 산 옆이라서",
          "original_form": "맞아 .... 우리 산 옆이라서",
          "speaker_id": "2",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100006996.1.16",
          "form": "벌레가 많았어 ...",
          "original_form": "벌레가 많았어 ...",
          "speaker_id": "2",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100006996.1.17",
          "form": "고라니도 있고...",
          "original_form": "고라니도 있고...",
          "speaker_id": "2",
          "time": "20210822 00:18"
        },
        {
          "id": "MDRW2100006996.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100006996.1.19",
          "form": "고라니한테",
          "original_form": "고라니한테",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100006996.1.20",
          "form": "한대맞은 애도 있었잖앜ㅋㅋㅋㅋㅋ",
          "original_form": "한대맞은 애도 있었잖앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100006996.1.21",
          "form": "그거 우리반이었잖아",
          "original_form": "그거 우리반이었잖아",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100006996.1.22",
          "form": "나 그때 고라니 발로 찼었어 ...",
          "original_form": "나 그때 고라니 발로 찼었어 ...",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100006996.1.23",
          "form": "발로 찼다기보다 내가 발을 들고있었는데",
          "original_form": "발로 찼다기보다 내가 발을 들고있었는데",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100006996.1.24",
          "form": "걔가 와서 박은거지만",
          "original_form": "걔가 와서 박은거지만",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100006996.1.25",
          "form": "??",
          "original_form": "??",
          "speaker_id": "1",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100006996.1.26",
          "form": "고라니를 발로 찼다고 너가??",
          "original_form": "고라니를 발로 찼다고 너가??",
          "speaker_id": "1",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100006996.1.27",
          "form": "ㅇㅇ 그때 고라니가 나 포함 3명을 쳤는데",
          "original_form": "ㅇㅇ 그때 고라니가 나 포함 3명을 쳤는데",
          "speaker_id": "2",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100006996.1.28",
          "form": "첫번째 애 두번째 애는",
          "original_form": "첫번째 애 두번째 애는",
          "speaker_id": "2",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100006996.1.29",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100006996.1.30",
          "form": "너도 맞았었나? 왜 몰랐지",
          "original_form": "너도 맞았었나? 왜 몰랐지",
          "speaker_id": "1",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100006996.1.31",
          "form": "나는 맞았다기보다",
          "original_form": "나는 맞았다기보다",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100006996.1.32",
          "form": "난 한명 패고 고라니가 사라진줄알았어",
          "original_form": "난 한명 패고 고라니가 사라진줄알았어",
          "speaker_id": "1",
          "time": "20210822 00:29"
