      "utterance": [
        {
          "id": "MDRW2100020177.1.1",
          "form": "지금은 관리해야해서ㅎㅎ",
          "original_form": "지금은 관리해야해서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020177.1.2",
          "form": "닭 가 슴 살 만 먹어요ㅎ",
          "original_form": "닭 가 슴 살 만 먹어요ㅎ",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020177.1.3",
          "form": "헛!!! 그러다 병나여 ㅡㅅ ㅜ",
          "original_form": "헛!!! 그러다 병나여 ㅡㅅ ㅜ",
          "speaker_id": "1",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020177.1.4",
          "form": "점심 거르고 저녁만ㅎㅎ",
          "original_form": "점심 거르고 저녁만ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020177.1.5",
          "form": "골고루 먹어요",
          "original_form": "골고루 먹어요",
          "speaker_id": "1",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020177.1.6",
          "form": "저녁에 아내가 먹자하면 가끔 시켜먹고",
          "original_form": "저녁에 아내가 먹자하면 가끔 시켜먹고",
          "speaker_id": "2",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020177.1.7",
          "form": "ㅎㅎ 일상이 되버렸으니ㅎ 이제부터 관리해야죠ㅎㅎ",
          "original_form": "ㅎㅎ 일상이 되버렸으니ㅎ 이제부터 관리해야죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020177.1.8",
          "form": "저는 스타일이",
          "original_form": "저는 스타일이",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020177.1.9",
          "form": "한식.좋아헤서",
          "original_form": "한식.좋아헤서",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020177.1.10",
          "form": "국은 꼭 잇어야 해요 밥먹을때 ㅎㅎ",
          "original_form": "국은 꼭 잇어야 해요 밥먹을때 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020177.1.11",
          "form": "저도 좋아하는데ㅎㅎ",
          "original_form": "저도 좋아하는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020177.1.12",
          "form": "전 국물 킬러요ㅎㅎㅎㅎ",
          "original_form": "전 국물 킬러요ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020177.1.13",
          "form": "전 김찌. 된찌. 청국장 사랑합니다 ㅎㅎ",
          "original_form": "전 김찌. 된찌. 청국장 사랑합니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020177.1.14",
          "form": "건더기는 많이 안먹고 국물에 밥말아먹는게 좋아해요",
          "original_form": "건더기는 많이 안먹고 국물에 밥말아먹는게 좋아해요",
          "speaker_id": "2",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020177.1.15",
          "form": "저두욘ㅎㅎ",
          "original_form": "저두욘ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020177.1.16",
          "form": "국물파ㅎㅎ",
          "original_form": "국물파ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020177.1.17",
          "form": "건더기는 저리가라ㅎㅎㅎ",
          "original_form": "건더기는 저리가라ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020177.1.18",
          "form": "닭 가/슴살만 먹지망고 한끼는 그래도 골고루 드세요",
          "original_form": "닭 가/슴살만 먹지망고 한끼는 그래도 골고루 드세요",
          "speaker_id": "1",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020177.1.19",
          "form": "일주일에 몇번은 점심먹어요ㅎㅎㅎ",
          "original_form": "일주일에 몇번은 점심먹어요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020177.1.20",
          "form": "몸아프면 답도 없어용",
          "original_form": "몸아프면 답도 없어용",
