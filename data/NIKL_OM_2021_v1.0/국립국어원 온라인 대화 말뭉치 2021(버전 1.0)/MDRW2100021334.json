      "utterance": [
        {
          "id": "MDRW2100021334.1.1",
          "form": "요즘 다이어트 때문에 곤약 볶음밥 먹음",
          "original_form": "요즘 다이어트 때문에 곤약 볶음밥 먹음",
          "speaker_id": "1",
          "time": "20210828 17:43"
        },
        {
          "id": "MDRW2100021334.1.2",
          "form": "곤약볶음밥 뭐가 맛있어 요새?",
          "original_form": "곤약볶음밥 뭐가 맛있어 요새?",
          "speaker_id": "2",
          "time": "20210828 17:43"
        },
        {
          "id": "MDRW2100021334.1.3",
          "form": "허닭 볶음밥이 제일 맛있더라",
          "original_form": "허닭 볶음밥이 제일 맛있더라",
          "speaker_id": "1",
          "time": "20210828 17:43"
        },
        {
          "id": "MDRW2100021334.1.4",
          "form": "가격은 어때?",
          "original_form": "가격은 어때?",
          "speaker_id": "2",
          "time": "20210828 17:43"
        },
        {
          "id": "MDRW2100021334.1.5",
          "form": "소불고기도 맛있구",
          "original_form": "소불고기도 맛있구",
          "speaker_id": "1",
          "time": "20210828 17:43"
        },
        {
          "id": "MDRW2100021334.1.6",
          "form": "아 말만들어도 맛있겠다 또?",
          "original_form": "아 말만들어도 맛있겠다 또?",
          "speaker_id": "2",
          "time": "20210828 17:44"
        },
        {
          "id": "MDRW2100021334.1.7",
          "form": "가격도 싼편이야! 곤약 볶음밥 중에선",
          "original_form": "가격도 싼편이야! 곤약 볶음밥 중에선",
          "speaker_id": "1",
          "time": "20210828 17:44"
        },
        {
          "id": "MDRW2100021334.1.8",
          "form": "음 깍두기랑 김치 닭갈비 추천",
          "original_form": "음 깍두기랑 김치 닭갈비 추천",
          "speaker_id": "1",
          "time": "20210828 17:44"
        },
        {
          "id": "MDRW2100021334.1.9",
          "form": "아 나도 시켜먹을래 단백질 함량은 괜찮아?",
          "original_form": "아 나도 시켜먹을래 단백질 함량은 괜찮아?",
          "speaker_id": "2",
          "time": "20210828 17:44"
        },
        {
          "id": "MDRW2100021334.1.10",
          "form": "하나 아쉬운게 낙지맛이 없어..ㅠㅠ",
          "original_form": "하나 아쉬운게 낙지맛이 없어..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 17:44"
        },
        {
          "id": "MDRW2100021334.1.11",
          "form": "아니 낙지맛이 없다니 허닭 실망",
          "original_form": "아니 낙지맛이 없다니 허닭 실망",
          "speaker_id": "2",
          "time": "20210828 17:44"
        },
        {
          "id": "MDRW2100021334.1.12",
          "form": "나트륨이 좀 높기는 한데.. 맛을보면 속세의 맛이라 이해가능",
          "original_form": "나트륨이 좀 높기는 한데.. 맛을보면 속세의 맛이라 이해가능",
          "speaker_id": "1",
          "time": "20210828 17:44"
        },
        {
          "id": "MDRW2100021334.1.13",
          "form": "단백질은 어때?",
          "original_form": "단백질은 어때?",
          "speaker_id": "2",
          "time": "20210828 17:45"
        },
        {
          "id": "MDRW2100021334.1.14",
          "form": "그치 낙지가 왜 아직도 없냐구....",
          "original_form": "그치 낙지가 왜 아직도 없냐구....",
          "speaker_id": "1",
          "time": "20210828 17:45"
        },
        {
          "id": "MDRW2100021334.1.15",
          "form": "단백질은 신경잘안써서 모르겠는데 저거 먹으면서 맨날 닭가슴살 소세지 하나 챙겨먹음ㅋㅋㅋ",
          "original_form": "단백질은 신경잘안써서 모르겠는데 저거 먹으면서 맨날 닭가슴살 소세지 하나 챙겨먹음ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:45"
        },
        {
          "id": "MDRW2100021334.1.16",
          "form": "낙지가 젤 맛있는데..흠냐뤼..",
          "original_form": "낙지가 젤 맛있는데..흠냐뤼..",
          "speaker_id": "2",
          "time": "20210828 17:45"
        },
        {
          "id": "MDRW2100021334.1.17",
          "form": "요즘 머리자르고 싶어",
          "original_form": "요즘 머리자르고 싶어",
          "speaker_id": "1",
          "time": "20210828 17:49"
        }
      ]
