      "utterance": [
        {
          "id": "MDRW2100043126.1.1",
          "form": "안녕하세욯ㅎ",
          "original_form": "안녕하세욯ㅎ",
          "speaker_id": "1",
          "time": "20210907 14:37"
        },
        {
          "id": "MDRW2100043126.1.2",
          "form": "좋아하는 게임있으신가요?",
          "original_form": "좋아하는 게임있으신가요?",
          "speaker_id": "1",
          "time": "20210907 14:37"
        },
        {
          "id": "MDRW2100043126.1.3",
          "form": "안녕하세요 저는 젤다의전설? 한동안 플레이 한적이 있습니다",
          "original_form": "안녕하세요 저는 젤다의전설? 한동안 플레이 한적이 있습니다",
          "speaker_id": "2",
          "time": "20210907 14:41"
        },
        {
          "id": "MDRW2100043126.1.4",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 14:41"
        },
        {
          "id": "MDRW2100043126.1.5",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 14:41"
        },
        {
          "id": "MDRW2100043126.1.6",
          "form": "오 젤다의 전설! 저도 닌텐도로 했어요 ㅎㅎ",
          "original_form": "오 젤다의 전설! 저도 닌텐도로 했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 14:45"
        },
        {
          "id": "MDRW2100043126.1.7",
          "form": "저는 롤이랑 오버워치 좋아합니다",
          "original_form": "저는 롤이랑 오버워치 좋아합니다",
          "speaker_id": "1",
          "time": "20210907 14:45"
        },
        {
          "id": "MDRW2100043126.1.8",
          "form": "오 보통 집에서 게임 하시나요?",
          "original_form": "오 보통 집에서 게임 하시나요?",
          "speaker_id": "2",
          "time": "20210907 14:47"
        },
        {
          "id": "MDRW2100043126.1.9",
          "form": "요즘은요ㅠㅠ",
          "original_form": "요즘은요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 14:47"
        },
        {
          "id": "MDRW2100043126.1.10",
          "form": "pc방이 10시에 문을닫아서요..",
          "original_form": "pc방이 10시에 문을닫아서요..",
          "speaker_id": "1",
          "time": "20210907 14:47"
        },
        {
          "id": "MDRW2100043126.1.11",
          "form": "아 그쵸ㅠㅠ 아무래도",
          "original_form": "아 그쵸ㅠㅠ 아무래도",
          "speaker_id": "2",
          "time": "20210907 14:47"
        },
        {
          "id": "MDRW2100043126.1.12",
          "form": "다른 게임 하시나용?",
          "original_form": "다른 게임 하시나용?",
          "speaker_id": "1",
          "time": "20210907 14:47"
        },
        {
          "id": "MDRW2100043126.1.13",
          "form": "요새 pc방은 좀 힘들죠 ㅎㅎ",
          "original_form": "요새 pc방은 좀 힘들죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 14:48"
        },
        {
          "id": "MDRW2100043126.1.14",
          "form": "음 저는 pc게임은 배틀그라운드를 마지막으로 ㅋㅋ안한지 꽤 된것같아요",
          "original_form": "음 저는 pc게임은 배틀그라운드를 마지막으로 ㅋㅋ안한지 꽤 된것같아요",
          "speaker_id": "2",
          "time": "20210907 14:48"
        },
        {
          "id": "MDRW2100043126.1.15",
          "form": "주로 체스나 오목 방탈출? 이런거 합니다 ㅎㅎ",
          "original_form": "주로 체스나 오목 방탈출? 이런거 합니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 14:48"
        },
        {
          "id": "MDRW2100043126.1.16",
          "form": "오 배틀그라운드면 그래도 최근아닌가요!?",
          "original_form": "오 배틀그라운드면 그래도 최근아닌가요!?",
          "speaker_id": "1",
          "time": "20210907 14:48"
        },
        {
          "id": "MDRW2100043126.1.17",
          "form": "어렵지않아요??",
          "original_form": "어렵지않아요??",
          "speaker_id": "1",
          "time": "20210907 14:48"
        },
        {
          "id": "MDRW2100043126.1.18",
          "form": "오오 체스도 재밌죠 ㅎㅎ",
          "original_form": "오오 체스도 재밌죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 14:49"
        },
        {
          "id": "MDRW2100043126.1.19",
          "form": "ㅎㅎ네네 그 배틀그라운드 도전해봤는데",
          "original_form": "ㅎㅎ네네 그 배틀그라운드 도전해봤는데",
          "speaker_id": "2",
          "time": "20210907 14:49"
        },
        {
          "id": "MDRW2100043126.1.20",
          "form": "모니터가 커브드여서 그랬는지 ㅋㅋㅋ멀미했어요 ㄷㄷ",
          "original_form": "모니터가 커브드여서 그랬는지 ㅋㅋㅋ멀미했어요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210907 14:49"
        },
        {
          "id": "MDRW2100043126.1.21",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210907 14:49"
        },
        {
          "id": "MDRW2100043126.1.22",
          "form": "저도 멀미나더라구요 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 멀미나더라구요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100043126.1.23",
          "form": "모바일에서는 괜찮았는데",
          "original_form": "모바일에서는 괜찮았는데",
          "speaker_id": "1",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100043126.1.24",
          "form": "오 저두요 ㅎㅎ 모바일은 그런거 못느꼈는데",
          "original_form": "오 저두요 ㅎㅎ 모바일은 그런거 못느꼈는데",
          "speaker_id": "2",
          "time": "20210907 14:50"
        },
        {
          "id": "MDRW2100043126.1.25",
          "form": "롤이랑 오버워치면",
          "original_form": "롤이랑 오버워치면",
          "speaker_id": "2",
          "time": "20210907 14:51"
        },
        {
          "id": "MDRW2100043126.1.26",
          "form": "항상 하시는 포지션도 있나요? 막 힐러같은?",
          "original_form": "항상 하시는 포지션도 있나요? 막 힐러같은?",
          "speaker_id": "2",
          "time": "20210907 14:51"
        },
        {
          "id": "MDRW2100043126.1.27",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "1",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100043126.1.28",
          "form": "헉 제 아이디아시나요",
          "original_form": "헉 제 아이디아시나요",
          "speaker_id": "1",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100043126.1.29",
          "form": "힐러만 합니다 ㅋㅋㅋㅋ",
          "original_form": "힐러만 합니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100043126.1.30",
          "form": "앗 ㅋㅋㅋ타율이 좋았네요",
          "original_form": "앗 ㅋㅋㅋ타율이 좋았네요",
          "speaker_id": "2",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100043126.1.31",
          "form": "잘찍었어요",
          "original_form": "잘찍었어요",
          "speaker_id": "2",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100043126.1.32",
          "form": "롤이랑 오버워치 전에는 어떤 게임 허셧나요?",
          "original_form": "롤이랑 오버워치 전에는 어떤 게임 허셧나요?",
          "speaker_id": "2",
          "time": "20210907 14:56"
        },
        {
          "id": "MDRW2100043126.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:56"
        },
        {
          "id": "MDRW2100043126.1.34",
          "form": "크아같은 게임빼고는",
          "original_form": "크아같은 게임빼고는",
          "speaker_id": "1",
          "time": "20210907 14:56"
        },
        {
          "id": "MDRW2100043126.1.35",
          "form": "제대로 빠진 게임은 오버워치가 처음이에요!",
          "original_form": "제대로 빠진 게임은 오버워치가 처음이에요!",
          "speaker_id": "1",
          "time": "20210907 14:57"
        },
        {
          "id": "MDRW2100043126.1.36",
          "form": "천시간 넘게했덥니다..ㅎ",
          "original_form": "천시간 넘게했덥니다..ㅎ",
          "speaker_id": "1",
          "time": "20210907 14:57"
        },
        {
          "id": "MDRW2100043126.1.37",
          "form": "와 ㅎㅎ 집중력이 대단하시네요",
          "original_form": "와 ㅎㅎ 집중력이 대단하시네요",
          "speaker_id": "2",
          "time": "20210907 14:58"
        },
        {
          "id": "MDRW2100043126.1.38",
          "form": "어떻게 입문하신거에요?",
          "original_form": "어떻게 입문하신거에요?",
          "speaker_id": "2",
          "time": "20210907 14:58"
        },
        {
          "id": "MDRW2100043126.1.39",
          "form": "친구가 데려갔어요 ㅎㅎ..",
          "original_form": "친구가 데려갔어요 ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210907 14:58"
        },
        {
          "id": "MDRW2100043126.1.40",
          "form": "고1때 시작했답니다",
          "original_form": "고1때 시작했답니다",
          "speaker_id": "1",
          "time": "20210907 14:58"
        },
        {
          "id": "MDRW2100043126.1.41",
          "form": "ㅋㅋㅋ아 그러셨군요",
          "original_form": "ㅋㅋㅋ아 그러셨군요",
          "speaker_id": "2",
          "time": "20210907 14:59"
        },
        {
          "id": "MDRW2100043126.1.42",
          "form": "그때도 셧다운제가 있었나요?",
          "original_form": "그때도 셧다운제가 있었나요?",
          "speaker_id": "2",
          "time": "20210907 14:59"
        },
        {
          "id": "MDRW2100043126.1.43",
          "form": "네네 ㅎㅎㅎ",
          "original_form": "네네 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 15:02"
        }
      ]
