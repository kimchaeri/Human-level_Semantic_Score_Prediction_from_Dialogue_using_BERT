      "utterance": [
        {
          "id": "MDRW2100042385.1.1",
          "form": "네 엄마가ㅋㅋ ㅜㅠ 아침마다",
          "original_form": "네 엄마가ㅋㅋ ㅜㅠ 아침마다",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042385.1.2",
          "form": "아침 드시나요?",
          "original_form": "아침 드시나요?",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042385.1.3",
          "form": "저도 엄마라서 꼭 먹입니다  ㅎㅎ",
          "original_form": "저도 엄마라서 꼭 먹입니다  ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042385.1.4",
          "form": "다행이 식구들이 일찍 일어나서 제가 밥먹으라고 깨울 필요는 없네요",
          "original_form": "다행이 식구들이 일찍 일어나서 제가 밥먹으라고 깨울 필요는 없네요",
          "speaker_id": "1",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042385.1.5",
          "form": "ㅋㅋ부모님의 사랑ㅜㅠ",
          "original_form": "ㅋㅋ부모님의 사랑ㅜㅠ",
          "speaker_id": "2",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042385.1.6",
          "form": "진짜 너무 감사해요 주말에는 저희가 해요",
          "original_form": "진짜 너무 감사해요 주말에는 저희가 해요",
          "speaker_id": "2",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042385.1.7",
          "form": "좋은 자녀분들이네요",
          "original_form": "좋은 자녀분들이네요",
          "speaker_id": "1",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100042385.1.8",
          "form": "저도 요새 딸에게 요리를 좀 가르치고 있어요",
          "original_form": "저도 요새 딸에게 요리를 좀 가르치고 있어요",
          "speaker_id": "1",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100042385.1.9",
          "form": "제가 해야 빠르지만 배우라고 기회를 주는거죠",
          "original_form": "제가 해야 빠르지만 배우라고 기회를 주는거죠",
          "speaker_id": "1",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100042385.1.10",
          "form": "ㅋㅋ저희엄마도 김장 가르쳐주시겠다고",
          "original_form": "ㅋㅋ저희엄마도 김장 가르쳐주시겠다고",
          "speaker_id": "2",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042385.1.11",
          "form": "근데 레시피가 너무 복잡해요",
          "original_form": "근데 레시피가 너무 복잡해요",
          "speaker_id": "2",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042385.1.12",
          "form": "김장 쉽지 않죠",
          "original_form": "김장 쉽지 않죠",
          "speaker_id": "1",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042385.1.13",
          "form": "난이도 높은 음식이에요",
          "original_form": "난이도 높은 음식이에요",
          "speaker_id": "1",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042385.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042385.1.15",
          "form": "그리고 좋다는걸 다 넣다보니 점점 복잡ㅜ",
          "original_form": "그리고 좋다는걸 다 넣다보니 점점 복잡ㅜ",
          "speaker_id": "2",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042385.1.16",
          "form": "그렇죠.. 뭐가 많아지죠",
          "original_form": "그렇죠.. 뭐가 많아지죠",
          "speaker_id": "1",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100042385.1.17",
          "form": "기본기를 배워야하는데 갑자기 심화과정\r\n들어가니까요",
          "original_form": "기본기를 배워야하는데 갑자기 심화과정\r\n들어가니까요",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042385.1.18",
          "form": "김장은 심화과정 맞죠 어려워요",
          "original_form": "김장은 심화과정 맞죠 어려워요",
          "speaker_id": "1",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042385.1.19",
          "form": "어떤 요리 전수중이세요?",
          "original_form": "어떤 요리 전수중이세요?",
          "speaker_id": "2",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100042385.1.20",
          "form": "특정 요리라기 보다 처음엔 기초를 배워야 하니까",
          "original_form": "특정 요리라기 보다 처음엔 기초를 배워야 하니까",
          "speaker_id": "1",
          "time": "20210907 06:24"
        },
        {
          "id": "MDRW2100042385.1.21",
          "form": "제가 요리할때 옆에서 보면서 시키는 거 하는 거예요",
          "original_form": "제가 요리할때 옆에서 보면서 시키는 거 하는 거예요",
          "speaker_id": "1",
          "time": "20210907 06:24"
        },
        {
          "id": "MDRW2100042385.1.22",
          "form": "재료 가져오고 제가 썰면 딸이 볶고",
          "original_form": "재료 가져오고 제가 썰면 딸이 볶고",
          "speaker_id": "1",
          "time": "20210907 06:25"
        },
        {
          "id": "MDRW2100042385.1.23",
          "form": "음식 점시로 옮기고",
          "original_form": "음식 점시로 옮기고",
          "speaker_id": "1",
          "time": "20210907 06:25"
        },
        {
          "id": "MDRW2100042385.1.24",
          "form": "딱 시다하는 거죠",
          "original_form": "딱 시다하는 거죠",
          "speaker_id": "1",
          "time": "20210907 06:25"
        },
        {
          "id": "MDRW2100042385.1.25",
          "form": "ㅋㅋ 아 제가 매일하는거네요ㅋㅋ",
          "original_form": "ㅋㅋ 아 제가 매일하는거네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 07:01"
        }
      ]
    }
