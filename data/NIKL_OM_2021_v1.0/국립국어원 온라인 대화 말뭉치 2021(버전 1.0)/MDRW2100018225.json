      "utterance": [
        {
          "id": "MDRW2100018225.1.1",
          "form": "여행 요즘에 못가서 아쉬워요 ㅠㅠ 어디 좋은 여행지 아세요?",
          "original_form": "여행 요즘에 못가서 아쉬워요 ㅠㅠ 어디 좋은 여행지 아세요?",
          "speaker_id": "2",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100018225.1.2",
          "form": "국내요?",
          "original_form": "국내요?",
          "speaker_id": "1",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100018225.1.3",
          "form": "코로나 확진자가 넘  많아서",
          "original_form": "코로나 확진자가 넘  많아서",
          "speaker_id": "1",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100018225.1.4",
          "form": "전 여름에 양양가서 서핑하고 싶었는데 참았어요",
          "original_form": "전 여름에 양양가서 서핑하고 싶었는데 참았어요",
          "speaker_id": "1",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100018225.1.5",
          "form": "아무곳이나 추천해주시면 나중에 가볼수 있을때 가볼려고욬ㅋㅋㅋ",
          "original_form": "아무곳이나 추천해주시면 나중에 가볼수 있을때 가볼려고욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100018225.1.6",
          "form": "제 마지막 여행이 양양이었어요",
          "original_form": "제 마지막 여행이 양양이었어요",
          "speaker_id": "2",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100018225.1.7",
          "form": "해보셨나요???",
          "original_form": "해보셨나요???",
          "speaker_id": "1",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100018225.1.8",
          "form": "친구들이랑 19년도에 양양 놀러간게 마지막 ㅠㅠ",
          "original_form": "친구들이랑 19년도에 양양 놀러간게 마지막 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100018225.1.9",
          "form": "서핑이요",
          "original_form": "서핑이요",
          "speaker_id": "1",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100018225.1.10",
          "form": "코로나 전이네요",
          "original_form": "코로나 전이네요",
          "speaker_id": "1",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100018225.1.11",
          "form": "서핑은 못해봤어요 ㅠㅠ",
          "original_form": "서핑은 못해봤어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100018225.1.12",
          "form": "저 몸치라 ㅋㅋㅋㅋ 운동 좋아하시나봐요",
          "original_form": "저 몸치라 ㅋㅋㅋㅋ 운동 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100018225.1.13",
          "form": "아뇨 저는 서지도 못할거 같은데",
          "original_form": "아뇨 저는 서지도 못할거 같은데",
          "speaker_id": "1",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100018225.1.14",
          "form": "그냥 해보고싶어요ㅋㅋ",
          "original_form": "그냥 해보고싶어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100018225.1.15",
          "form": "강원도는 고성이 좋져",
          "original_form": "강원도는 고성이 좋져",
          "speaker_id": "1",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100018225.1.16",
          "form": "차없으면 속초",
          "original_form": "차없으면 속초",
          "speaker_id": "1",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100018225.1.17",
          "form": "고성 양양 속초 진짜 좋더라고요",
          "original_form": "고성 양양 속초 진짜 좋더라고요",
          "speaker_id": "2",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018225.1.18",
          "form": "강원도 좋아하시는군요",
          "original_form": "강원도 좋아하시는군요",
          "speaker_id": "1",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018225.1.19",
          "form": "바다도 예쁘고 밥도 맛있고",
          "original_form": "바다도 예쁘고 밥도 맛있고",
          "speaker_id": "2",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018225.1.20",
          "form": "몇번 안가봤는데 좋더라고요",
          "original_form": "몇번 안가봤는데 좋더라고요",
          "speaker_id": "2",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018225.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018225.1.22",
          "form": "저는 마지막으로 간게 제주도에요",
          "original_form": "저는 마지막으로 간게 제주도에요",
          "speaker_id": "1",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018225.1.23",
          "form": "좋으셨겠다",
          "original_form": "좋으셨겠다",
          "speaker_id": "2",
          "time": "20210826 23:20"
        }
      ]
    }
