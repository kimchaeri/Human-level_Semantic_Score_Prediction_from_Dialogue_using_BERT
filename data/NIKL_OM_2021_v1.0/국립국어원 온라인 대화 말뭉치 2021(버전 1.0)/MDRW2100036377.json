      "utterance": [
        {
          "id": "MDRW2100036377.1.1",
          "form": "이디야 연해여ㅠㅠ",
          "original_form": "이디야 연해여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 22:08"
        },
        {
          "id": "MDRW2100036377.1.2",
          "form": "그쵸???",
          "original_form": "그쵸???",
          "speaker_id": "1",
          "time": "20210830 22:08"
        },
        {
          "id": "MDRW2100036377.1.3",
          "form": "네ㅠㅠ",
          "original_form": "네ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 22:08"
        },
        {
          "id": "MDRW2100036377.1.4",
          "form": "저 연한 커피 좋아해서 텀블러에다가 물 왕창 섞어먹는데",
          "original_form": "저 연한 커피 좋아해서 텀블러에다가 물 왕창 섞어먹는데",
          "speaker_id": "1",
          "time": "20210830 22:08"
        },
        {
          "id": "MDRW2100036377.1.5",
          "form": "동네에 커피숍다양하게 잇어서 돌아다니면서 마시는데",
          "original_form": "동네에 커피숍다양하게 잇어서 돌아다니면서 마시는데",
          "speaker_id": "2",
          "time": "20210830 22:08"
        },
        {
          "id": "MDRW2100036377.1.6",
          "form": "스타벅스는 그러면 농도가 맞거든요??",
          "original_form": "스타벅스는 그러면 농도가 맞거든요??",
          "speaker_id": "1",
          "time": "20210830 22:09"
        },
        {
          "id": "MDRW2100036377.1.7",
          "form": "난 빽다방이나 스벅이 나은거 같아요",
          "original_form": "난 빽다방이나 스벅이 나은거 같아요",
          "speaker_id": "2",
          "time": "20210830 22:09"
        },
        {
          "id": "MDRW2100036377.1.8",
          "form": "근데 이디야는 준대로 마시면 딱 좋아요ㅋㅋㅋ",
          "original_form": "근데 이디야는 준대로 마시면 딱 좋아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:09"
        },
        {
          "id": "MDRW2100036377.1.9",
          "form": "오!",
          "original_form": "오!",
          "speaker_id": "2",
          "time": "20210830 22:09"
        },
        {
          "id": "MDRW2100036377.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:09"
        },
        {
          "id": "MDRW2100036377.1.11",
          "form": "빽다방",
          "original_form": "빽다방",
          "speaker_id": "1",
          "time": "20210830 22:09"
        },
        {
          "id": "MDRW2100036377.1.12",
          "form": "왠지 손해본느낌",
          "original_form": "왠지 손해본느낌",
          "speaker_id": "2",
          "time": "20210830 22:09"
        },
        {
          "id": "MDRW2100036377.1.13",
          "form": "녹차프라푸치노 맛있어요",
          "original_form": "녹차프라푸치노 맛있어요",
          "speaker_id": "1",
          "time": "20210830 22:09"
        },
        {
          "id": "MDRW2100036377.1.14",
          "form": "네네ㅠㅠ",
          "original_form": "네네ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 22:09"
        },
        {
          "id": "MDRW2100036377.1.15",
          "form": "그거 드실때 꼭 소프트 아이스크림",
          "original_form": "그거 드실때 꼭 소프트 아이스크림",
          "speaker_id": "1",
          "time": "20210830 22:09"
        },
        {
          "id": "MDRW2100036377.1.16",
          "form": "추가하셔서 드세요!",
          "original_form": "추가하셔서 드세요!",
          "speaker_id": "1",
          "time": "20210830 22:09"
        },
        {
          "id": "MDRW2100036377.1.17",
          "form": "아 !",
          "original_form": "아 !",
          "speaker_id": "2",
          "time": "20210830 22:09"
        },
        {
          "id": "MDRW2100036377.1.18",
          "form": "빽다방 아스크림맛잇나요?!",
          "original_form": "빽다방 아스크림맛잇나요?!",
          "speaker_id": "2",
          "time": "20210830 22:10"
        },
        {
          "id": "MDRW2100036377.1.19",
          "form": "저 안먹어봣는데!!",
          "original_form": "저 안먹어봣는데!!",
          "speaker_id": "2",
          "time": "20210830 22:10"
        },
        {
          "id": "MDRW2100036377.1.20",
          "form": "넹넹 맛있어요!",
          "original_form": "넹넹 맛있어요!",
          "speaker_id": "1",
          "time": "20210830 22:10"
        },
        {
          "id": "MDRW2100036377.1.21",
          "form": "우왑 ㅋㅋㅋ 조만간 먹어야겟어요 ㅋㅋ",
