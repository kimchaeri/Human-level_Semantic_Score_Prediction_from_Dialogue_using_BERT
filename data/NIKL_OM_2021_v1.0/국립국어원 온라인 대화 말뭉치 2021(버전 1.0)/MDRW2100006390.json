      "utterance": [
        {
          "id": "MDRW2100006390.1.1",
          "form": "아이스크림 한숟가락 먹긴했어요",
          "original_form": "아이스크림 한숟가락 먹긴했어요",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100006390.1.2",
          "form": "밤만되면 출출해요",
          "original_form": "밤만되면 출출해요",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100006390.1.3",
          "form": "컵누들 쟁여놓고 먹고있네요",
          "original_form": "컵누들 쟁여놓고 먹고있네요",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100006390.1.4",
          "form": "아까 잠올때 잤어야했는데 잘시간 지나니  넘 정신이 맑네요",
          "original_form": "아까 잠올때 잤어야했는데 잘시간 지나니  넘 정신이 맑네요",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100006390.1.5",
          "form": "컵누들은 칼로리 적죠? 아직 안먹어봤어요",
          "original_form": "컵누들은 칼로리 적죠? 아직 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100006390.1.6",
          "form": "두젓가락이면 끝나는 양이예요ㅎ",
          "original_form": "두젓가락이면 끝나는 양이예요ㅎ",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100006390.1.7",
          "form": "당면좋아해서 전 맛있더라구요",
          "original_form": "당면좋아해서 전 맛있더라구요",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100006390.1.8",
          "form": "엄청 양이 적나보네요",
          "original_form": "엄청 양이 적나보네요",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100006390.1.9",
          "form": "나중에마트가면 사봐야겠어요",
          "original_form": "나중에마트가면 사봐야겠어요",
          "speaker_id": "2",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100006390.1.10",
          "form": "칼로리가 적은거라 ㅎ",
          "original_form": "칼로리가 적은거라 ㅎ",
          "speaker_id": "1",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100006390.1.11",
          "form": "찜닭맛나와서 그거 먹어봤는데 괜찮더라구요",
          "original_form": "찜닭맛나와서 그거 먹어봤는데 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100006390.1.12",
          "form": "찜닭맛 잘생각했다 사야겠네요",
          "original_form": "찜닭맛 잘생각했다 사야겠네요",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100006390.1.13",
          "form": "매콤한맛이 젤 괜찮고 우동도 쏘쏘",
          "original_form": "매콤한맛이 젤 괜찮고 우동도 쏘쏘",
          "speaker_id": "1",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100006390.1.14",
          "form": "꼭 사봐야겠네요.",
          "original_form": "꼭 사봐야겠네요.",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100006390.1.15",
          "form": "당면은 아닌데 쌀국수도 맛나요ㅎ",
          "original_form": "당면은 아닌데 쌀국수도 맛나요ㅎ",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100006390.1.16",
          "form": "똠양꿍은 호불호갈릴듯",
          "original_form": "똠양꿍은 호불호갈릴듯",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100006390.1.17",
          "form": "낼은 쌀국수먹으러가야겠어요",
          "original_form": "낼은 쌀국수먹으러가야겠어요",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100006390.1.18",
          "form": "그건 안먹어봤어요",
          "original_form": "그건 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100006390.1.19",
          "form": "음식 가려서 안먹어볼듯해요. ㅎㅎ",
          "original_form": "음식 가려서 안먹어볼듯해요. ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100006390.1.20",
          "form": "쌀국수는 괜찮아요 골고루사보셔요ㅎ",
          "original_form": "쌀국수는 괜찮아요 골고루사보셔요ㅎ",
          "speaker_id": "1",
          "time": "20210907 23:42"
