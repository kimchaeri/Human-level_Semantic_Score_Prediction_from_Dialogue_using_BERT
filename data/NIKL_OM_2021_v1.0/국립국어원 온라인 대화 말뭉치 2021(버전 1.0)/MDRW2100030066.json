      "utterance": [
        {
          "id": "MDRW2100030066.1.1",
          "form": "코로나 끝나면 어디가고싶으세용?",
          "original_form": "코로나 끝나면 어디가고싶으세용?",
          "speaker_id": "1",
          "time": "20210903 20:34"
        },
        {
          "id": "MDRW2100030066.1.2",
          "form": "가족들과 남해쪽 가고 싶군요",
          "original_form": "가족들과 남해쪽 가고 싶군요",
          "speaker_id": "2",
          "time": "20210903 20:44"
        },
        {
          "id": "MDRW2100030066.1.3",
          "form": "남해는 팬션이 유명하죠!",
          "original_form": "남해는 팬션이 유명하죠!",
          "speaker_id": "1",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100030066.1.4",
          "form": "저도 독일마을 가보고싶었어요",
          "original_form": "저도 독일마을 가보고싶었어요",
          "speaker_id": "1",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100030066.1.5",
          "form": "재작년에 가봤습니다",
          "original_form": "재작년에 가봤습니다",
          "speaker_id": "2",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100030066.1.6",
          "form": "좋았어요",
          "original_form": "좋았어요",
          "speaker_id": "2",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100030066.1.7",
          "form": "아!! 두번 가고싶으신거였구나 ㅎㅎ",
          "original_form": "아!! 두번 가고싶으신거였구나 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100030066.1.8",
          "form": "남해가 엄청 좋으셨나봐요 ㅎㅎ",
          "original_form": "남해가 엄청 좋으셨나봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100030066.1.9",
          "form": "좋았어요",
          "original_form": "좋았어요",
          "speaker_id": "2",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100030066.1.10",
          "form": "여수도 좋았고",
          "original_form": "여수도 좋았고",
          "speaker_id": "2",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100030066.1.11",
          "form": "오우 저도 여수 가고싶어요!!",
          "original_form": "오우 저도 여수 가고싶어요!!",
          "speaker_id": "1",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100030066.1.12",
          "form": "여수 돌고래!!",
          "original_form": "여수 돌고래!!",
          "speaker_id": "1",
          "time": "20210903 20:55"
        },
        {
          "id": "MDRW2100030066.1.13",
          "form": "여수 밤바다 케이블카도 좋습니다",
          "original_form": "여수 밤바다 케이블카도 좋습니다",
          "speaker_id": "2",
          "time": "20210903 20:56"
        },
        {
          "id": "MDRW2100030066.1.14",
          "form": "우와아 밤에도 케이블카 운영해요?!",
          "original_form": "우와아 밤에도 케이블카 운영해요?!",
          "speaker_id": "1",
          "time": "20210903 20:57"
        },
        {
          "id": "MDRW2100030066.1.15",
          "form": "대박 매력적인데요?",
          "original_form": "대박 매력적인데요?",
          "speaker_id": "1",
          "time": "20210903 20:57"
        },
        {
          "id": "MDRW2100030066.1.16",
          "form": "네 아래 유람선이 지나가면서 폭죽도 터트렸었지요",
          "original_form": "네 아래 유람선이 지나가면서 폭죽도 터트렸었지요",
          "speaker_id": "2",
          "time": "20210903 20:58"
        },
        {
          "id": "MDRW2100030066.1.17",
          "form": "유람선도 있어요???",
          "original_form": "유람선도 있어요???",
          "speaker_id": "1",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100030066.1.18",
          "form": "돌고래말곤 정말 하나도 몰랐네요 ㅋㅋㅋㅋㅋ",
          "original_form": "돌고래말곤 정말 하나도 몰랐네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100030066.1.19",
          "form": "디게 이쁠거같아요",
          "original_form": "디게 이쁠거같아요",
          "speaker_id": "1",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100030066.1.20",
          "form": "가족들이 좋아해서 뿌듯했었지요",
          "original_form": "가족들이 좋아해서 뿌듯했었지요",
          "speaker_id": "2",
          "time": "20210903 21:03"
        },
        {
          "id": "MDRW2100030066.1.21",
          "form": "좋아요 ㅎㅎ 행복한 일이에요!",
          "original_form": "좋아요 ㅎㅎ 행복한 일이에요!",
          "speaker_id": "1",
          "time": "20210903 21:03"
        }
