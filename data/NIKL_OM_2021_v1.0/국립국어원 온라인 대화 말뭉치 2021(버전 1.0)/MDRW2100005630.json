      "utterance": [
        {
          "id": "MDRW2100005630.1.1",
          "form": "오늘 뭐 드셨나요??",
          "original_form": "오늘 뭐 드셨나요??",
          "speaker_id": "1",
          "time": "20210821 23:26"
        },
        {
          "id": "MDRW2100005630.1.2",
          "form": "오늘 간짜장 먹었어요!",
          "original_form": "오늘 간짜장 먹었어요!",
          "speaker_id": "2",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005630.1.3",
          "form": "닉도 ㅎㅎ",
          "original_form": "닉도 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005630.1.4",
          "form": "오 중국집 좋아하세요??",
          "original_form": "오 중국집 좋아하세요??",
          "speaker_id": "1",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005630.1.5",
          "form": "중국집 알바하고 있어서",
          "original_form": "중국집 알바하고 있어서",
          "speaker_id": "2",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005630.1.6",
          "form": "하나씩 도전하다가 간짜장에 꽂혔어요",
          "original_form": "하나씩 도전하다가 간짜장에 꽂혔어요",
          "speaker_id": "2",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005630.1.7",
          "form": "오 그럼 직접 만드시는건가요??",
          "original_form": "오 그럼 직접 만드시는건가요??",
          "speaker_id": "1",
          "time": "20210821 23:34"
        },
        {
          "id": "MDRW2100005630.1.8",
          "form": "아뇨 저는 서빙이랑 잡일만 하고 있어요",
          "original_form": "아뇨 저는 서빙이랑 잡일만 하고 있어요",
          "speaker_id": "2",
          "time": "20210821 23:34"
        },
        {
          "id": "MDRW2100005630.1.9",
          "form": "요리를 못해서 주방 체험은 아예 못하겠더라고요",
          "original_form": "요리를 못해서 주방 체험은 아예 못하겠더라고요",
          "speaker_id": "2",
          "time": "20210821 23:34"
        },
        {
          "id": "MDRW2100005630.1.10",
          "form": "아하 저도 요리를 잘 못해요",
          "original_form": "아하 저도 요리를 잘 못해요",
          "speaker_id": "1",
          "time": "20210821 23:35"
        },
        {
          "id": "MDRW2100005630.1.11",
          "form": "그래서 자주 시켜먹어요",
          "original_form": "그래서 자주 시켜먹어요",
          "speaker_id": "1",
          "time": "20210821 23:35"
        },
        {
          "id": "MDRW2100005630.1.12",
          "form": "어떤 배달음식 좋아하세요?",
          "original_form": "어떤 배달음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 23:35"
        },
        {
          "id": "MDRW2100005630.1.13",
          "form": "저는 마라탕이랑",
          "original_form": "저는 마라탕이랑",
          "speaker_id": "2",
          "time": "20210821 23:35"
        },
        {
          "id": "MDRW2100005630.1.14",
          "form": "웬만한 패스트푸드 다 좋아해요",
          "original_form": "웬만한 패스트푸드 다 좋아해요",
          "speaker_id": "2",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005630.1.15",
          "form": "아 마라탕 맛있죠",
          "original_form": "아 마라탕 맛있죠",
          "speaker_id": "1",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005630.1.16",
          "form": "먹기전엔 무슨맛인지 정말 짐작을 못했는데",
          "original_form": "먹기전엔 무슨맛인지 정말 짐작을 못했는데",
          "speaker_id": "1",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005630.1.17",
          "form": "먹고나니까 계속 생각나는 맛이었어요",
          "original_form": "먹고나니까 계속 생각나는 맛이었어요",
          "speaker_id": "1",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005630.1.18",
          "form": "근데 한순간에 중독더ㅣ더라고요",
          "original_form": "근데 한순간에 중독더ㅣ더라고요",
          "speaker_id": "2",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005630.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005630.1.20",
          "form": "저는 찜닭 시켜먹는걸 좋아해요!!",
          "original_form": "저는 찜닭 시켜먹는걸 좋아해요!!",
          "speaker_id": "1",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100005630.1.21",
          "form": "와ㅠ 찜닭 맛있죠",
          "original_form": "와ㅠ 찜닭 맛있죠",
          "speaker_id": "2",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005630.1.22",
          "form": "저는 ㅜ두찜 애용해요",
          "original_form": "저는 ㅜ두찜 애용해요",
          "speaker_id": "2",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100005630.1.23",
          "form": "아 두찜 저도 좋아해요!",
          "original_form": "아 두찜 저도 좋아해요!",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005630.1.24",
          "form": "로제찜닭 먹어보셨나요?",
          "original_form": "로제찜닭 먹어보셨나요?",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005630.1.25",
          "form": "네+!!",
          "original_form": "네+!!",
          "speaker_id": "2",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005630.1.26",
          "form": "근데 저는 빨간찜닭이 더 맛있더라고요",
          "original_form": "근데 저는 빨간찜닭이 더 맛있더라고요",
          "speaker_id": "2",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100005630.1.27",
          "form": "저두요 역시 찜닭은 클래식이 더 좋은 것 같아요",
          "original_form": "저두요 역시 찜닭은 클래식이 더 좋은 것 같아요",
          "speaker_id": "1",
          "time": "20210822 00:39"
        }
      ]
    }
