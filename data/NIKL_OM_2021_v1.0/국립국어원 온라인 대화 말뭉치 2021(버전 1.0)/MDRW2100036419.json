      "utterance": [
        {
          "id": "MDRW2100036419.1.1",
          "form": "ㅠㅠ 여튼버리고나니 아깝네요",
          "original_form": "ㅠㅠ 여튼버리고나니 아깝네요",
          "speaker_id": "2",
          "time": "20210830 22:54"
        },
        {
          "id": "MDRW2100036419.1.2",
          "form": "어떤떡좋아하세요?",
          "original_form": "어떤떡좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036419.1.3",
          "form": "인절미 쑥떡 요런 고물있는거 좋아해요",
          "original_form": "인절미 쑥떡 요런 고물있는거 좋아해요",
          "speaker_id": "2",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100036419.1.4",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036419.1.5",
          "form": "오늘 절편 쑥 사려는데",
          "original_form": "오늘 절편 쑥 사려는데",
          "speaker_id": "1",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036419.1.6",
          "form": "품절되서 못샀어요",
          "original_form": "품절되서 못샀어요",
          "speaker_id": "1",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036419.1.7",
          "form": "오!",
          "original_form": "오!",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036419.1.8",
          "form": "이런 슬프네여ㅠㅠㅠ",
          "original_form": "이런 슬프네여ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036419.1.9",
          "form": "요즘은 동네 떡집도 몇개 없어서 사려면 멀리 나가야하고 해서",
          "original_form": "요즘은 동네 떡집도 몇개 없어서 사려면 멀리 나가야하고 해서",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036419.1.10",
          "form": "봄에 쑥떡 열씨미 먹었는데",
          "original_form": "봄에 쑥떡 열씨미 먹었는데",
          "speaker_id": "1",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036419.1.11",
          "form": "또 먹고싶어지네요",
          "original_form": "또 먹고싶어지네요",
          "speaker_id": "1",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036419.1.12",
          "form": "저희는 어머님이 쑥 뜯어서 해주셨는데",
          "original_form": "저희는 어머님이 쑥 뜯어서 해주셨는데",
          "speaker_id": "2",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100036419.1.13",
          "form": "그게 냉동실 들어가면 또 잘 안먹게 되고 그렇더라구요ㅠ",
          "original_form": "그게 냉동실 들어가면 또 잘 안먹게 되고 그렇더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100036419.1.14",
          "form": "저는 회사언니가 촌에가서 쑥뜯어서 ㅎㅎ",
          "original_form": "저는 회사언니가 촌에가서 쑥뜯어서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100036419.1.15",
          "form": "잘받아먹었거든요",
          "original_form": "잘받아먹었거든요",
          "speaker_id": "1",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100036419.1.16",
          "form": "요건 진짜 다 먹었어요 ㅎ",
          "original_form": "요건 진짜 다 먹었어요 ㅎ",
          "speaker_id": "1",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100036419.1.17",
          "form": "콩고물 묻혀서",
          "original_form": "콩고물 묻혀서",
          "speaker_id": "1",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100036419.1.18",
          "form": "그동네는 사이즈가 어때요?? 저희어머님은 맨날 해주시면 팔뚝만하게...해주셔서",
          "original_form": "그동네는 사이즈가 어때요?? 저희어머님은 맨날 해주시면 팔뚝만하게...해주셔서",
          "speaker_id": "2",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100036419.1.19",
          "form": "팔뚝 ㅋㅋㅋ",
          "original_form": "팔뚝 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100036419.1.20",
          "form": "여기도 아기팔뚝 ㅋㅋ",
          "original_form": "여기도 아기팔뚝 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100036419.1.21",
          "form": "근데 부산은 조막만하게 한다고 하더라구요",
          "original_form": "근데 부산은 조막만하게 한다고 하더라구요",
          "speaker_id": "1",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100036419.1.22",
          "form": "한입크기로",
          "original_form": "한입크기로",
          "speaker_id": "1",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100036419.1.23",
          "form": "아 맞죠 저는 그거보고 놀랬는데 전 부산은 아니지만 그 사이즈는 첨봤거든요 정말...ㅎㅎ",
          "original_form": "아 맞죠 저는 그거보고 놀랬는데 전 부산은 아니지만 그 사이즈는 첨봤거든요 정말...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100036419.1.24",
          "form": "이게 좋은데 이렇게하는데도 잘없고 비싼가봐요",
          "original_form": "이게 좋은데 이렇게하는데도 잘없고 비싼가봐요",
          "speaker_id": "1",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100036419.1.25",
          "form": "작은 크기요??",
          "original_form": "작은 크기요??",
          "speaker_id": "2",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100036419.1.26",
          "form": "네 한입크기로 하는거요",
          "original_form": "네 한입크기로 하는거요",
          "speaker_id": "1",
          "time": "20210830 23:58"
        }
      ]
    }
