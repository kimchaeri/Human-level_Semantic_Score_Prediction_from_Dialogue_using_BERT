        },
        {
          "id": "MDRW2100027236.1.2",
          "form": "계절감이없어서",
          "original_form": "계절감이없어서",
          "speaker_id": "1",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100027236.1.3",
          "form": "두루두루듣기좋다는것",
          "original_form": "두루두루듣기좋다는것",
          "speaker_id": "1",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100027236.1.4",
          "form": "ㅇㅇ좋아",
          "original_form": "ㅇㅇ좋아",
          "speaker_id": "2",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100027236.1.5",
          "form": "갠적으론오후가",
          "original_form": "갠적으론오후가",
          "speaker_id": "2",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100027236.1.6",
          "form": "계절상관없이듣기좋은거같음",
          "original_form": "계절상관없이듣기좋은거같음",
          "speaker_id": "2",
          "time": "20210902 10:42"
        },
        {
          "id": "MDRW2100027236.1.7",
          "form": "적당히산뜻해서",
          "original_form": "적당히산뜻해서",
          "speaker_id": "2",
          "time": "20210902 10:42"
        },
        {
          "id": "MDRW2100027236.1.8",
          "form": "버터플라이도좋고",
          "original_form": "버터플라이도좋고",
          "speaker_id": "2",
          "time": "20210902 10:42"
        },
        {
          "id": "MDRW2100027236.1.9",
          "form": "ㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 10:42"
        },
        {
          "id": "MDRW2100027236.1.10",
          "form": "그 갇자기제목이생각안나는데",
          "original_form": "그 갇자기제목이생각안나는데",
          "speaker_id": "1",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100027236.1.11",
          "form": "봄들어간그거말고는",
          "original_form": "봄들어간그거말고는",
          "speaker_id": "1",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100027236.1.12",
          "form": "다 언제들어ㄷ괜찮은느낌",
          "original_form": "다 언제들어ㄷ괜찮은느낌",
          "speaker_id": "1",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100027236.1.13",
          "form": "봄이너에게ㅇㅇ",
          "original_form": "봄이너에게ㅇㅇ",
          "speaker_id": "2",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027236.1.14",
          "form": "이건제목부터ㅋㅋㅋㅋㅋㅋ",
          "original_form": "이건제목부터ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027236.1.15",
          "form": "그리고그거",
          "original_form": "그리고그거",
          "speaker_id": "2",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027236.1.16",
          "form": "팬송이라ㅋㅋㅋㅋㅋ",
          "original_form": "팬송이라ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:44"
        },
        {
