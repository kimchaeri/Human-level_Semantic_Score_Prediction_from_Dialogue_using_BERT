      "utterance": [
        {
          "id": "MDRW2100010030.1.1",
          "form": "안녕하세요 점심 드셨나요?",
          "original_form": "안녕하세요 점심 드셨나요?",
          "speaker_id": "1",
          "time": "20210823 13:22"
        },
        {
          "id": "MDRW2100010030.1.2",
          "form": "네 방금 먹었어요",
          "original_form": "네 방금 먹었어요",
          "speaker_id": "2",
          "time": "20210823 13:22"
        },
        {
          "id": "MDRW2100010030.1.3",
          "form": "오 메뉴가 뭐였나요?",
          "original_form": "오 메뉴가 뭐였나요?",
          "speaker_id": "1",
          "time": "20210823 13:24"
        },
        {
          "id": "MDRW2100010030.1.4",
          "form": "초밥이랑 우동 먹었어요",
          "original_form": "초밥이랑 우동 먹었어요",
          "speaker_id": "2",
          "time": "20210823 13:24"
        },
        {
          "id": "MDRW2100010030.1.5",
          "form": "식사하셨나요",
          "original_form": "식사하셨나요",
          "speaker_id": "2",
          "time": "20210823 13:24"
        },
        {
          "id": "MDRW2100010030.1.6",
          "form": "와 초밥 맛있겠네요",
          "original_form": "와 초밥 맛있겠네요",
          "speaker_id": "1",
          "time": "20210823 13:24"
        },
        {
          "id": "MDRW2100010030.1.7",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "1",
          "time": "20210823 13:24"
        },
        {
          "id": "MDRW2100010030.1.8",
          "form": "아직 안드셨어요? 왜요?",
          "original_form": "아직 안드셨어요? 왜요?",
          "speaker_id": "2",
          "time": "20210823 13:24"
        },
        {
          "id": "MDRW2100010030.1.9",
          "form": "뭘 먹을 지 모르겠어서 고민중이었어요",
          "original_form": "뭘 먹을 지 모르겠어서 고민중이었어요",
          "speaker_id": "1",
          "time": "20210823 13:25"
        },
        {
          "id": "MDRW2100010030.1.10",
          "form": "드시고 싶으신 게 있나요",
          "original_form": "드시고 싶으신 게 있나요",
          "speaker_id": "2",
          "time": "20210823 13:26"
        },
        {
          "id": "MDRW2100010030.1.11",
          "form": "음... 파리바게트 샌드위치 아니면 라면 투움바 중에 고민 중이에요",
          "original_form": "음... 파리바게트 샌드위치 아니면 라면 투움바 중에 고민 중이에요",
          "speaker_id": "1",
          "time": "20210823 13:28"
        },
        {
          "id": "MDRW2100010030.1.12",
          "form": "음 저는 파리바게트 런치샌드위치를 추천드려오",
          "original_form": "음 저는 파리바게트 런치샌드위치를 추천드려오",
          "speaker_id": "2",
          "time": "20210823 13:29"
        },
        {
          "id": "MDRW2100010030.1.13",
          "form": "점심은 가볍게 드세요 마침 요기요에서 할인도 한다더라구요",
          "original_form": "점심은 가볍게 드세요 마침 요기요에서 할인도 한다더라구요",
          "speaker_id": "2",
          "time": "20210823 13:29"
        },
        {
          "id": "MDRW2100010030.1.14",
          "form": "오 진짜요! 요기요 할인은 무조건 받아야죠",
          "original_form": "오 진짜요! 요기요 할인은 무조건 받아야죠",
          "speaker_id": "1",
          "time": "20210823 13:29"
        },
        {
          "id": "MDRW2100010030.1.15",
          "form": "이왕 먹는거 할인받으면 맛이 두배ㅎ",
          "original_form": "이왕 먹는거 할인받으면 맛이 두배ㅎ",
          "speaker_id": "2",
          "time": "20210823 13:30"
        },
        {
          "id": "MDRW2100010030.1.16",
          "form": "그럼 오늘 점심은 파리바게트로 해결해야겠네요",
          "original_form": "그럼 오늘 점심은 파리바게트로 해결해야겠네요",
          "speaker_id": "1",
          "time": "20210823 13:32"
        },
        {
          "id": "MDRW2100010030.1.17",
          "form": "안녕하세요 곧 점심 시간이네요! 뭘 드실 건가요?",
          "original_form": "안녕하세요 곧 점심 시간이네요! 뭘 드실 건가요?",
          "speaker_id": "1",
          "time": "20210824 11:12"
        }
      ]
    }
