          "speaker_id": "1",
          "time": "20210828 02:12"
        },
        {
          "id": "MDRW2100020365.1.3",
          "form": "오늘은 어케했나요",
          "original_form": "오늘은 어케했나요",
          "speaker_id": "1",
          "time": "20210828 02:13"
        },
        {
          "id": "MDRW2100020365.1.4",
          "form": "오늘도 똑같이.",
          "original_form": "오늘도 똑같이.",
          "speaker_id": "2",
          "time": "20210828 02:13"
        },
        {
          "id": "MDRW2100020365.1.5",
          "form": "햄이 저범에 뜯어놓고",
          "original_form": "햄이 저범에 뜯어놓고",
          "speaker_id": "2",
          "time": "20210828 02:13"
        },
        {
          "id": "MDRW2100020365.1.6",
          "form": "만게잇어서",
          "original_form": "만게잇어서",
          "speaker_id": "2",
          "time": "20210828 02:13"
        },
        {
          "id": "MDRW2100020365.1.7",
          "form": "다 써버림",
          "original_form": "다 써버림",
          "speaker_id": "2",
          "time": "20210828 02:13"
        },
        {
          "id": "MDRW2100020365.1.8",
          "form": "맛나겟다..",
          "original_form": "맛나겟다..",
          "speaker_id": "1",
          "time": "20210828 02:13"
        },
        {
          "id": "MDRW2100020365.1.9",
          "form": ";",
          "original_form": ";",
          "speaker_id": "2",
          "time": "20210828 02:13"
        },
        {
          "id": "MDRW2100020365.1.10",
          "form": "하나더물어봐야함",
          "original_form": "하나더물어봐야함",
          "speaker_id": "2",
          "time": "20210828 02:14"
        },
        {
          "id": "MDRW2100020365.1.11",
          "form": "런천미트 못먹음?",
          "original_form": "런천미트 못먹음?",
          "speaker_id": "2",
          "time": "20210828 02:14"
        },
        {
          "id": "MDRW2100020365.1.12",
          "form": "머임",
          "original_form": "머임",
          "speaker_id": "1",
          "time": "20210828 02:14"
        },
        {
          "id": "MDRW2100020365.1.13",
          "form": "못목진않은데",
          "original_form": "못목진않은데",
          "speaker_id": "1",
          "time": "20210828 02:14"
        },
        {
          "id": "MDRW2100020365.1.14",
          "form": "스팸 극하타치",
          "original_form": "스팸 극하타치",
          "speaker_id": "1",
          "time": "20210828 02:14"
        },
        {
          "id": "MDRW2100020365.1.15",
          "form": "ㅇㅋ",
          "original_form": "ㅇㅋ",
          "speaker_id": "2",
          "time": "20210828 02:14"
        },
        {
          "id": "MDRW2100020365.1.16",
          "form": "ㅡ그럼",
          "original_form": "ㅡ그럼",
          "speaker_id": "2",
          "time": "20210828 02:14"
        },
        {
          "id": "MDRW2100020365.1.17",
          "form": "그건왜요",
          "original_form": "그건왜요",
          "speaker_id": "1",
          "time": "20210828 02:14"
        },
        {
          "id": "MDRW2100020365.1.18",
          "form": "분홍소시지 극혐해요?",
          "original_form": "분홍소시지 극혐해요?",
          "speaker_id": "2",
          "time": "20210828 02:14"
        },
        {
          "id": "MDRW2100020365.1.19",
          "form": "네 분혼소세지싱ㅎ어요",
          "original_form": "네 분혼소세지싱ㅎ어요",
          "speaker_id": "1",
          "time": "20210828 02:14"
        },
        {
          "id": "MDRW2100020365.1.20",
          "form": "나 오늘 갑자기",
          "original_form": "나 오늘 갑자기",
          "speaker_id": "2",
          "time": "20210828 02:14"
        },
        {
          "id": "MDRW2100020365.1.21",
          "form": "분홍소시지가 문득",
          "original_form": "분홍소시지가 문득",
          "speaker_id": "2",
          "time": "20210828 02:15"
        },
        {
          "id": "MDRW2100020365.1.22",
          "form": "먹고싶단 생각을 함",
          "original_form": "먹고싶단 생각을 함",
          "speaker_id": "2",
          "time": "20210828 02:15"
        },
        {
          "id": "MDRW2100020365.1.23",
          "form": "헐..",
          "original_form": "헐..",
          "speaker_id": "1",
          "time": "20210828 02:15"
        },
        {
          "id": "MDRW2100020365.1.24",
          "form": "세뇌아냐?",
          "original_form": "세뇌아냐?",
          "speaker_id": "1",
          "time": "20210828 02:15"
        },
        {
          "id": "MDRW2100020365.1.25",
          "form": "아니 분홍소시지를..",
          "original_form": "아니 분홍소시지를..",
          "speaker_id": "2",
          "time": "20210828 02:15"
        },
        {
          "id": "MDRW2100020365.1.26",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "2",
          "time": "20210828 02:15"
        },
        {
          "id": "MDRW2100020365.1.27",
          "form": "ㄱ그거 달걀 묻혀서",
          "original_form": "ㄱ그거 달걀 묻혀서",
          "speaker_id": "2",
          "time": "20210828 02:15"
        },
        {
          "id": "MDRW2100020365.1.28",
          "form": "케찹에 찍어머근ㄴ",
          "original_form": "케찹에 찍어머근ㄴ",
          "speaker_id": "2",
          "time": "20210828 02:15"
        },
        {
          "id": "MDRW2100020365.1.29",
          "form": "그게 갑자기 너무",
          "original_form": "그게 갑자기 너무",
          "speaker_id": "2",
          "time": "20210828 02:15"
        },
        {
          "id": "MDRW2100020365.1.30",
          "form": "생각이나는거에오",
          "original_form": "생각이나는거에오",
          "speaker_id": "2",
          "time": "20210828 02:15"
        },
        {
          "id": "MDRW2100020365.1.31",
          "form": "하 달걀묻힌거 싫어~~",
          "original_form": "하 달걀묻힌거 싫어~~",
          "speaker_id": "1",
          "time": "20210828 02:15"
        },
        {
          "id": "MDRW2100020365.1.32",
          "form": "아니 글고 런천미트는 왜 물어봣냐면",
          "original_form": "아니 글고 런천미트는 왜 물어봣냐면",
          "speaker_id": "2",
          "time": "20210828 02:15"
        },
        {
          "id": "MDRW2100020365.1.33",
          "form": "님만 먹우삼",
          "original_form": "님만 먹우삼",
          "speaker_id": "1",
          "time": "20210828 02:15"
        },
        {
          "id": "MDRW2100020365.1.34",
          "form": "런천미트 못먹는 사람도",
          "original_form": "런천미트 못먹는 사람도",
          "speaker_id": "2",
          "time": "20210828 02:15"
        },
        {
          "id": "MDRW2100020365.1.35",
          "form": "근데 하나는 줘야함",
          "original_form": "근데 하나는 줘야함",
          "speaker_id": "1",
          "time": "20210828 02:15"
        },
        {
          "id": "MDRW2100020365.1.36",
          "form": "ㅣ잇을수잇잔아",
          "original_form": "ㅣ잇을수잇잔아",
          "speaker_id": "2",
          "time": "20210828 02:15"
        },
        {
          "id": "MDRW2100020365.1.37",
          "form": "근데 모르고 사와",
          "original_form": "근데 모르고 사와",
          "speaker_id": "2",
          "time": "20210828 02:15"
        },
        {
          "id": "MDRW2100020365.1.38",
          "form": "그럼 안돼지",
          "original_form": "그럼 안돼지",
          "speaker_id": "2",
          "time": "20210828 02:15"
        },
        {
          "id": "MDRW2100020365.1.39",
          "form": "먹는거보면 맛잇어보여",
          "original_form": "먹는거보면 맛잇어보여",
          "speaker_id": "1",
          "time": "20210828 02:15"
        },
        {
          "id": "MDRW2100020365.1.40",
          "form": "그래 나는 그럼",
          "original_form": "그래 나는 그럼",
          "speaker_id": "2",
          "time": "20210828 02:15"
        },
        {
          "id": "MDRW2100020365.1.41",
