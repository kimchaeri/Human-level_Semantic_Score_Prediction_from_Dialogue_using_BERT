      "utterance": [
        {
          "id": "MDRW2100008258.1.1",
          "form": "여고추리반 보셨어요?",
          "original_form": "여고추리반 보셨어요?",
          "speaker_id": "1",
          "time": "20210822 14:37"
        },
        {
          "id": "MDRW2100008258.1.2",
          "form": "저번에 같이 보던곳에서",
          "original_form": "저번에 같이 보던곳에서",
          "speaker_id": "1",
          "time": "20210822 14:38"
        },
        {
          "id": "MDRW2100008258.1.3",
          "form": "올스탑했니?",
          "original_form": "올스탑했니?",
          "speaker_id": "1",
          "time": "20210822 14:38"
        },
        {
          "id": "MDRW2100008258.1.4",
          "form": "넹 거기가 끝이예요",
          "original_form": "넹 거기가 끝이예요",
          "speaker_id": "2",
          "time": "20210822 14:38"
        },
        {
          "id": "MDRW2100008258.1.5",
          "form": "2화쯤이었나??",
          "original_form": "2화쯤이었나??",
          "speaker_id": "2",
          "time": "20210822 14:38"
        },
        {
          "id": "MDRW2100008258.1.6",
          "form": "왜용 뒷부분 보실 의향있으신가요",
          "original_form": "왜용 뒷부분 보실 의향있으신가요",
          "speaker_id": "2",
          "time": "20210822 14:38"
        },
        {
          "id": "MDRW2100008258.1.7",
          "form": "궁금하긴 한데 너는 어때?",
          "original_form": "궁금하긴 한데 너는 어때?",
          "speaker_id": "1",
          "time": "20210822 14:39"
        },
        {
          "id": "MDRW2100008258.1.8",
          "form": "2화까지는 넘 기대를 했었는지",
          "original_form": "2화까지는 넘 기대를 했었는지",
          "speaker_id": "1",
          "time": "20210822 14:39"
        },
        {
          "id": "MDRW2100008258.1.9",
          "form": "쪼꼼 실망했었는데 관련 방송보니 다음부터가 시작이다! 이랬어서",
          "original_form": "쪼꼼 실망했었는데 관련 방송보니 다음부터가 시작이다! 이랬어서",
          "speaker_id": "1",
          "time": "20210822 14:39"
        },
        {
          "id": "MDRW2100008258.1.10",
          "form": "흠 저도 볼의향은 있는데",
          "original_form": "흠 저도 볼의향은 있는데",
          "speaker_id": "2",
          "time": "20210822 14:41"
        },
        {
          "id": "MDRW2100008258.1.11",
          "form": "실망한게 좀 최근이었어서 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "실망한게 좀 최근이었어서 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:41"
        },
        {
          "id": "MDRW2100008258.1.12",
          "form": "오늘자 런닝맨 보고나서 3화 시도해볼까 싶어지네요 흠..",
          "original_form": "오늘자 런닝맨 보고나서 3화 시도해볼까 싶어지네요 흠..",
          "speaker_id": "2",
          "time": "20210822 14:42"
        },
        {
          "id": "MDRW2100008258.1.13",
          "form": "ㅋㅋㅋㅋ 런닝맨 골수팬같아",
          "original_form": "ㅋㅋㅋㅋ 런닝맨 골수팬같아",
          "speaker_id": "1",
          "time": "20210822 14:42"
        },
        {
          "id": "MDRW2100008258.1.14",
          "form": "나는 마지막으로 챙겨본 예능이 무한도전이라",
          "original_form": "나는 마지막으로 챙겨본 예능이 무한도전이라",
          "speaker_id": "1",
          "time": "20210822 14:42"
        },
        {
          "id": "MDRW2100008258.1.15",
          "form": "넘 까마득한데",
          "original_form": "넘 까마득한데",
          "speaker_id": "1",
          "time": "20210822 14:43"
        },
        {
          "id": "MDRW2100008258.1.16",
          "form": "골수팬은 솔직히 아니고 저도 무한도전밖에 본거 없는데",
          "original_form": "골수팬은 솔직히 아니고 저도 무한도전밖에 본거 없는데",
          "speaker_id": "2",
          "time": "20210822 14:43"
        },
        {
          "id": "MDRW2100008258.1.17",
          "form": "볼게 없어서 최근 몇달부터 챙겨본거예요 ㅋㅋㅋㅋ",
          "original_form": "볼게 없어서 최근 몇달부터 챙겨본거예요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:43"
        },
        {
          "id": "MDRW2100008258.1.18",
          "form": "런닝맨같은게 사실 불편할 요소 별로 없이 걍 틀어놓을 수 있어서 편해서요...",
          "original_form": "런닝맨같은게 사실 불편할 요소 별로 없이 걍 틀어놓을 수 있어서 편해서요...",
          "speaker_id": "2",
          "time": "20210822 14:43"
        },
        {
          "id": "MDRW2100008258.1.19",
          "form": "ㅋㅋㅋㅋ 미션받고 미션하러 뛰는 형식이라 그런가?",
          "original_form": "ㅋㅋㅋㅋ 미션받고 미션하러 뛰는 형식이라 그런가?",
          "speaker_id": "1",
          "time": "20210822 14:45"
        },
        {
          "id": "MDRW2100008258.1.20",
          "form": "그런거 보면 매주마다 새로운 컨텐츠 짜내는 작가들",
          "original_form": "그런거 보면 매주마다 새로운 컨텐츠 짜내는 작가들",
          "speaker_id": "1",
          "time": "20210822 14:45"
        },
        {
          "id": "MDRW2100008258.1.21",
          "form": "참 고생이 많은거같아",
          "original_form": "참 고생이 많은거같아",
          "speaker_id": "1",
          "time": "20210822 14:45"
        },
        {
          "id": "MDRW2100008258.1.22",
          "form": "그쵸 근데 이제 런닝맨이 무한도전 회수 뛰어넘고",
          "original_form": "그쵸 근데 이제 런닝맨이 무한도전 회수 뛰어넘고",
          "speaker_id": "2",
          "time": "20210822 14:46"
        },
        {
          "id": "MDRW2100008258.1.23",
          "form": "가장 많은 회차가진 예능 됐대요 ㅋㅋㅋ",
          "original_form": "가장 많은 회차가진 예능 됐대요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:46"
        },
        {
          "id": "MDRW2100008258.1.24",
          "form": "그만큼 작가들도 좀 챙겨주는 그런게 많지 않을까요?",
          "original_form": "그만큼 작가들도 좀 챙겨주는 그런게 많지 않을까요?",
          "speaker_id": "2",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100008258.1.25",
          "form": "어 정말? 벌써 그렇게 됐구나",
          "original_form": "어 정말? 벌써 그렇게 됐구나",
          "speaker_id": "1",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100008258.1.26",
          "form": "런닝맨 1화 한게 엊그제같은데 ㅋㅋㅋ",
          "original_form": "런닝맨 1화 한게 엊그제같은데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100008258.1.27",
          "form": "그쯤이면 제작팀들 인원도 엄청 많겠다",
          "original_form": "그쯤이면 제작팀들 인원도 엄청 많겠다",
          "speaker_id": "1",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100008258.1.28",
          "form": "그르니까요 ㅋㅋㅋㅋ 런닝맨에서 막내이던 피디가 딴예능가선 대장이고 그러잖아요 이제는 ㅋㅋㅋㅋㅋ",
          "original_form": "그르니까요 ㅋㅋㅋㅋ 런닝맨에서 막내이던 피디가 딴예능가선 대장이고 그러잖아요 이제는 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:49"
        },
        {
          "id": "MDRW2100008258.1.29",
          "form": "암튼 오늘 한 10시? 쯤 런닝맨 볼 예정인데",
          "original_form": "암튼 오늘 한 10시? 쯤 런닝맨 볼 예정인데",
          "speaker_id": "2",
          "time": "20210822 14:49"
        },
        {
          "id": "MDRW2100008258.1.30",
          "form": "같이 볼 의향 있으세여? ㅇㅅㅇ 없음 말구..",
          "original_form": "같이 볼 의향 있으세여? ㅇㅅㅇ 없음 말구..",
          "speaker_id": "2",
          "time": "20210822 14:49"
        },
        {
          "id": "MDRW2100008258.1.31",
          "form": "그래? 그럼 같이 보자",
          "original_form": "그래? 그럼 같이 보자",
          "speaker_id": "1",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008258.1.32",
          "form": "잠들면... 날 버리고 봐줘",
          "original_form": "잠들면... 날 버리고 봐줘",
          "speaker_id": "1",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008258.1.33",
          "form": "이번주꺼야?",
          "original_form": "이번주꺼야?",
          "speaker_id": "1",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008258.1.34",
          "form": "넹 당연하죠 그래서 10시 이후로 잡은건데요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "넹 당연하죠 그래서 10시 이후로 잡은건데요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100008258.1.35",
          "form": "저번주꺼는 진작 봤죠",
          "original_form": "저번주꺼는 진작 봤죠",
          "speaker_id": "2",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100008258.1.36",
          "form": "유튭클립으로 본의아닌 재탕삼탕까지 다 뛰었어요 ㅋㅋ",
          "original_form": "유튭클립으로 본의아닌 재탕삼탕까지 다 뛰었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:56"
        },
        {
          "id": "MDRW2100008258.1.37",
          "form": "세상에 ㅋㅋㅋㅋ 이쯤이면 그냥 틀어두는게 아니라",
          "original_form": "세상에 ㅋㅋㅋㅋ 이쯤이면 그냥 틀어두는게 아니라",
          "speaker_id": "1",
          "time": "20210822 15:04"
        },
        {
          "id": "MDRW2100008258.1.38",
          "form": "찐팬인데 부정하는거잖아",
          "original_form": "찐팬인데 부정하는거잖아",
          "speaker_id": "1",
          "time": "20210822 15:04"
        }
      ]
