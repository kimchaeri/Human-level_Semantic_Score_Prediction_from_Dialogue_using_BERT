      "utterance": [
        {
          "id": "MDRW2100008360.1.1",
          "form": "건강 쪽은 관심 있으신가요?",
          "original_form": "건강 쪽은 관심 있으신가요?",
          "speaker_id": "2",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100008360.1.2",
          "form": "앗 네!",
          "original_form": "앗 네!",
          "speaker_id": "1",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100008360.1.3",
          "form": "요새 건강 걱정이 많아져서",
          "original_form": "요새 건강 걱정이 많아져서",
          "speaker_id": "1",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100008360.1.4",
          "form": "운동도 하고 갤럭시 워치도 사려고 한답니다 ^.^",
          "original_form": "운동도 하고 갤럭시 워치도 사려고 한답니다 ^.^",
          "speaker_id": "1",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100008360.1.5",
          "form": "갤럭시 워치 좋죠",
          "original_form": "갤럭시 워치 좋죠",
          "speaker_id": "2",
          "time": "20210822 16:25"
        },
        {
          "id": "MDRW2100008360.1.6",
          "form": "모델은 어떤 거 생각 중이세요?",
          "original_form": "모델은 어떤 거 생각 중이세요?",
          "speaker_id": "2",
          "time": "20210822 16:25"
        },
        {
          "id": "MDRW2100008360.1.7",
          "form": "이번에 나온 거 4 사람들 휴기 보고",
          "original_form": "이번에 나온 거 4 사람들 휴기 보고",
          "speaker_id": "1",
          "time": "20210822 18:41"
        },
        {
          "id": "MDRW2100008360.1.8",
          "form": "사려고요!!! ㅎㅎ",
          "original_form": "사려고요!!! ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 18:41"
        },
        {
          "id": "MDRW2100008360.1.9",
          "form": "와ㅜ 저도 4 너무 사고 싶어요",
          "original_form": "와ㅜ 저도 4 너무 사고 싶어요",
          "speaker_id": "2",
          "time": "20210822 18:51"
        },
        {
          "id": "MDRW2100008360.1.10",
          "form": "엄청 인기 많더라고요! 심지어 인바디도 가능하다면서요!?",
          "original_form": "엄청 인기 많더라고요! 심지어 인바디도 가능하다면서요!?",
          "speaker_id": "1",
          "time": "20210822 18:52"
        },
        {
          "id": "MDRW2100008360.1.11",
          "form": "맞아요 ㅜ 15초만 있으면 다 알려주는 게 너무 신기해요",
          "original_form": "맞아요 ㅜ 15초만 있으면 다 알려주는 게 너무 신기해요",
          "speaker_id": "2",
          "time": "20210822 18:53"
        },
        {
          "id": "MDRW2100008360.1.12",
          "form": "그니까요 저 애플워치가 혈압? 알려준대서 그걸로 하려다가 그 얘기 듣고 갤워치로 마음 바꿨거든요 ㅎㅎ",
          "original_form": "그니까요 저 애플워치가 혈압? 알려준대서 그걸로 하려다가 그 얘기 듣고 갤워치로 마음 바꿨거든요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100008360.1.13",
          "form": "name2님은 운동 자주 하시는 편이세요?",
          "original_form": "&name2&님은 운동 자주 하시는 편이세요?",
          "speaker_id": "1",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100008360.1.14",
          "form": "지금은 쉬고 있는데",
          "original_form": "지금은 쉬고 있는데",
          "speaker_id": "2",
          "time": "20210823 03:04"
        },
        {
          "id": "MDRW2100008360.1.15",
          "form": "다이어트 때문에 한창 했었어요",
          "original_form": "다이어트 때문에 한창 했었어요",
          "speaker_id": "2",
          "time": "20210823 03:04"
        },
        {
          "id": "MDRW2100008360.1.16",
          "form": "헉 그렇구나! 저도 요즘 피티를 사작했는데",
          "original_form": "헉 그렇구나! 저도 요즘 피티를 사작했는데",
          "speaker_id": "1",
          "time": "20210823 11:08"
        },
        {
          "id": "MDRW2100008360.1.17",
          "form": "너무 힘들어서 잠이 엄청 늘었어요...",
          "original_form": "너무 힘들어서 잠이 엄청 늘었어요...",
          "speaker_id": "1",
          "time": "20210823 11:08"
        },
        {
          "id": "MDRW2100008360.1.18",
          "form": "와ㅜ 피티 하시는군요",
          "original_form": "와ㅜ 피티 하시는군요",
          "speaker_id": "2",
          "time": "20210823 19:57"
        },
        {
          "id": "MDRW2100008360.1.19",
          "form": "운동하면 피곤해서 바로 기절이에요 ㅋㅋㅋ",
          "original_form": "운동하면 피곤해서 바로 기절이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:57"
        },
        {
          "id": "MDRW2100008360.1.20",
          "form": "맞아요... 요새 그래서 잠이 너무 늘었어요",
          "original_form": "맞아요... 요새 그래서 잠이 너무 늘었어요",
          "speaker_id": "1",
          "time": "20210823 20:09"
        },
        {
          "id": "MDRW2100008360.1.21",
          "form": "침대에서 일어나기가 힘들더라거요 ㅎㅎ",
          "original_form": "침대에서 일어나기가 힘들더라거요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 20:09"
        },
        {
          "id": "MDRW2100008360.1.22",
          "form": "name2님은 무슨 운동 하셨었어요?",
          "original_form": "&name2&님은 무슨 운동 하셨었어요?",
          "speaker_id": "1",
          "time": "20210823 20:09"
        },
        {
          "id": "MDRW2100008360.1.23",
