        {
          "id": "MDRW2100020642.1.1",
          "form": "카페에서 먹는거랑은 다르죠",
          "original_form": "카페에서 먹는거랑은 다르죠",
          "speaker_id": "2",
          "time": "20210828 10:42"
        },
        {
          "id": "MDRW2100020642.1.2",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 10:42"
        },
        {
          "id": "MDRW2100020642.1.3",
          "form": "머신 있는 친구들은 멋있게 뽑는데",
          "original_form": "머신 있는 친구들은 멋있게 뽑는데",
          "speaker_id": "1",
          "time": "20210828 10:42"
        },
        {
          "id": "MDRW2100020642.1.4",
          "form": "저는 더치커피파라서",
          "original_form": "저는 더치커피파라서",
          "speaker_id": "1",
          "time": "20210828 10:42"
        },
        {
          "id": "MDRW2100020642.1.5",
          "form": "앗..",
          "original_form": "앗..",
          "speaker_id": "2",
          "time": "20210828 10:42"
        },
        {
          "id": "MDRW2100020642.1.6",
          "form": "초라한가요",
          "original_form": "초라한가요",
          "speaker_id": "2",
          "time": "20210828 10:42"
        },
        {
          "id": "MDRW2100020642.1.7",
          "form": "안멋있어..",
          "original_form": "안멋있어..",
          "speaker_id": "1",
          "time": "20210828 10:42"
        },
        {
          "id": "MDRW2100020642.1.8",
          "form": "졸졸졸",
          "original_form": "졸졸졸",
          "speaker_id": "2",
          "time": "20210828 10:42"
        },
        {
          "id": "MDRW2100020642.1.9",
          "form": "예....",
          "original_form": "예....",
          "speaker_id": "1",
          "time": "20210828 10:42"
        },
        {
          "id": "MDRW2100020642.1.10",
          "form": "나도 위잉 하고싶다",
          "original_form": "나도 위잉 하고싶다",
          "speaker_id": "2",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020642.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020642.1.12",
          "form": "작은 계량컵에 담아서 텀블러에 붓는게 끝이에요",
          "original_form": "작은 계량컵에 담아서 텀블러에 붓는게 끝이에요",
          "speaker_id": "1",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020642.1.13",
          "form": "의외로 간단하네요",
          "original_form": "의외로 간단하네요",
          "speaker_id": "2",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020642.1.14",
          "form": "집에서 해먹기 딱 좋겠다",
          "original_form": "집에서 해먹기 딱 좋겠다",
          "speaker_id": "2",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020642.1.15",
          "form": "네 편한건 엄청 좋아요",
          "original_form": "네 편한건 엄청 좋아요",
          "speaker_id": "1",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020642.1.16",
          "form": "멋이 안날뿐..",
          "original_form": "멋이 안날뿐..",
          "speaker_id": "2",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020642.1.17",
          "form": "name2님은 그럼",
          "original_form": "&name2&님은 그럼",
          "speaker_id": "1",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020642.1.18",
          "form": "집에서 커피 마시고 싶을 때 어떡하시나요",
          "original_form": "집에서 커피 마시고 싶을 때 어떡하시나요",
          "speaker_id": "1",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020642.1.19",
          "form": "카라멜 마끼아또..",
          "original_form": "카라멜 마끼아또..",
          "speaker_id": "1",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100020642.1.20",
          "form": "난이도 너무 높아보여...",
          "original_form": "난이도 너무 높아보여...",
          "speaker_id": "1",
          "time": "20210828 10:44"
        },
        {
