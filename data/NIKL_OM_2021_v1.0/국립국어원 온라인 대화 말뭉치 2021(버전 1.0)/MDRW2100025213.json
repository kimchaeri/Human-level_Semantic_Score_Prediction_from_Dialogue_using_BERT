      "utterance": [
        {
          "id": "MDRW2100025213.1.1",
          "form": "우동스타일 떡볶이 엄청 당기네요ㄷㄷㄷ",
          "original_form": "우동스타일 떡볶이 엄청 당기네요ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210901 01:05"
        },
        {
          "id": "MDRW2100025213.1.2",
          "form": "찾아봐야겠어요",
          "original_form": "찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210901 01:05"
        },
        {
          "id": "MDRW2100025213.1.3",
          "form": "네 함 먹어보세요!",
          "original_form": "네 함 먹어보세요!",
          "speaker_id": "2",
          "time": "20210901 01:05"
        },
        {
          "id": "MDRW2100025213.1.4",
          "form": "저는 가래떡 떡볶이는",
          "original_form": "저는 가래떡 떡볶이는",
          "speaker_id": "2",
          "time": "20210901 01:05"
        },
        {
          "id": "MDRW2100025213.1.5",
          "form": "제대로 된 걸 먹어보지 못했는데",
          "original_form": "제대로 된 걸 먹어보지 못했는데",
          "speaker_id": "2",
          "time": "20210901 01:05"
        },
        {
          "id": "MDRW2100025213.1.6",
          "form": "말나온 김에 한번 사먹어 볼까봐요",
          "original_form": "말나온 김에 한번 사먹어 볼까봐요",
          "speaker_id": "2",
          "time": "20210901 01:05"
        },
        {
          "id": "MDRW2100025213.1.7",
          "form": "설빙에 있는 가래떡 떡볶이 밖에 안먹어봤어요",
          "original_form": "설빙에 있는 가래떡 떡볶이 밖에 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210901 01:06"
        },
        {
          "id": "MDRW2100025213.1.8",
          "form": "아니 드셔보셨네요",
          "original_form": "아니 드셔보셨네요",
          "speaker_id": "1",
          "time": "20210901 01:07"
        },
        {
          "id": "MDRW2100025213.1.9",
          "form": "근데 설빙서 그런것도 팔았군요!",
          "original_form": "근데 설빙서 그런것도 팔았군요!",
          "speaker_id": "1",
          "time": "20210901 01:07"
        },
        {
          "id": "MDRW2100025213.1.10",
          "form": "메론빙수 먹을때만 찾던곳이라 몰랐어요",
          "original_form": "메론빙수 먹을때만 찾던곳이라 몰랐어요",
          "speaker_id": "1",
          "time": "20210901 01:08"
        },
        {
          "id": "MDRW2100025213.1.11",
          "form": "그건 상당히 떡꼬치 같은 느낌이었어요",
          "original_form": "그건 상당히 떡꼬치 같은 느낌이었어요",
          "speaker_id": "2",
          "time": "20210901 01:08"
        },
        {
          "id": "MDRW2100025213.1.12",
          "form": "떡 3개 쥽니더.....비추천",
          "original_form": "떡 3개 쥽니더.....비추천",
          "speaker_id": "2",
          "time": "20210901 01:08"
        },
        {
          "id": "MDRW2100025213.1.13",
          "form": "매론빙수 좋죠",
          "original_form": "매론빙수 좋죠",
          "speaker_id": "2",
          "time": "20210901 01:08"
        },
        {
          "id": "MDRW2100025213.1.14",
          "form": "이번 여름에도 먹었습니다!! 메론 빙수 좋아하시나봐요!",
          "original_form": "이번 여름에도 먹었습니다!! 메론 빙수 좋아하시나봐요!",
          "speaker_id": "2",
          "time": "20210901 01:08"
        },
        {
          "id": "MDRW2100025213.1.15",
          "form": "아 설빙은 역시ㅇ빙수죠",
          "original_form": "아 설빙은 역시ㅇ빙수죠",
          "speaker_id": "1",
          "time": "20210901 01:09"
        },
        {
          "id": "MDRW2100025213.1.16",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210901 01:09"
        },
        {
          "id": "MDRW2100025213.1.17",
          "form": "메론빙수는",
          "original_form": "메론빙수는",
          "speaker_id": "2",
          "time": "20210901 01:09"
        },
        {
          "id": "MDRW2100025213.1.18",
          "form": "썰 때도 재미도 있죠",
          "original_form": "썰 때도 재미도 있죠",
          "speaker_id": "2",
          "time": "20210901 01:09"
        },
        {
          "id": "MDRW2100025213.1.19",
          "form": "네 안의 요거트같은 아이스크림도 좋고ㅎ 최애가 메론이에요",
          "original_form": "네 안의 요거트같은 아이스크림도 좋고ㅎ 최애가 메론이에요",
          "speaker_id": "1",
          "time": "20210901 01:09"
        },
        {
          "id": "MDRW2100025213.1.20",
          "form": "네네ㅎㅎ잘썰어야 하지만ㅎ",
          "original_form": "네네ㅎㅎ잘썰어야 하지만ㅎ",
          "speaker_id": "1",
          "time": "20210901 01:09"
        },
        {
          "id": "MDRW2100025213.1.21",
          "form": "요거트 파우더? 같은 게 되게 톡 쏘고",
          "original_form": "요거트 파우더? 같은 게 되게 톡 쏘고",
          "speaker_id": "2",
          "time": "20210901 01:11"
        },
        {
          "id": "MDRW2100025213.1.22",
          "form": "가루처럼 되어 있는 부분이 은근 새콤하면서 좋아요",
          "original_form": "가루처럼 되어 있는 부분이 은근 새콤하면서 좋아요",
          "speaker_id": "2",
          "time": "20210901 01:11"
        },
        {
          "id": "MDRW2100025213.1.23",
          "form": "ㅎㅎ맞아요 맞아",
          "original_form": "ㅎㅎ맞아요 맞아",
          "speaker_id": "1",
          "time": "20210901 01:12"
        },
        {
          "id": "MDRW2100025213.1.24",
          "form": "name2님의 설빙 최애 빙수는 뭔가요 메론? 망고?",
          "original_form": "&name2&님의 설빙 최애 빙수는 뭔가요 메론? 망고?",
          "speaker_id": "1",
          "time": "20210901 01:12"
        },
        {
          "id": "MDRW2100025213.1.25",
          "form": "저는 오레오 올려져있는 거면 기본적으로 다 호감이에요!",
          "original_form": "저는 오레오 올려져있는 거면 기본적으로 다 호감이에요!",
          "speaker_id": "2",
          "time": "20210901 01:14"
        },
        {
          "id": "MDRW2100025213.1.26",
          "form": "과일 쪽으로는 메론이 최애입니다 ㅎㅎ",
          "original_form": "과일 쪽으로는 메론이 최애입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 01:14"
        },
        {
          "id": "MDRW2100025213.1.27",
          "form": "오레오 빙수는 안 먹어봤는데 맛나나봐요!!",
          "original_form": "오레오 빙수는 안 먹어봤는데 맛나나봐요!!",
          "speaker_id": "1",
          "time": "20210901 01:15"
        },
        {
          "id": "MDRW2100025213.1.28",
          "form": "네네 맞아요 ㅎㅎㅎ",
          "original_form": "네네 맞아요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 01:16"
        },
        {
          "id": "MDRW2100025213.1.29",
          "form": "도전!해볼 음식이 많네요ㅎ",
          "original_form": "도전!해볼 음식이 많네요ㅎ",
          "speaker_id": "1",
          "time": "20210901 01:16"
        },
        {
          "id": "MDRW2100025213.1.30",
          "form": "긴 떡볶이 오레오빙수ㅎ",
          "original_form": "긴 떡볶이 오레오빙수ㅎ",
          "speaker_id": "1",
          "time": "20210901 01:16"
        },
        {
          "id": "MDRW2100025213.1.31",
          "form": "행복하네요~",
          "original_form": "행복하네요~",
          "speaker_id": "2",
          "time": "20210901 01:16"
        }
      ]
