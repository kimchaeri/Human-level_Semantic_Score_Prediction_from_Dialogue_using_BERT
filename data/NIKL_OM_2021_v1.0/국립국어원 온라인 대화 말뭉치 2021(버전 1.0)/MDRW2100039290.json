      "utterance": [
        {
          "id": "MDRW2100039290.1.1",
          "form": "어제 티비보는데",
          "original_form": "어제 티비보는데",
          "speaker_id": "2",
          "time": "20210903 15:53"
        },
        {
          "id": "MDRW2100039290.1.2",
          "form": "석쇠불고기집나오는데",
          "original_form": "석쇠불고기집나오는데",
          "speaker_id": "2",
          "time": "20210903 15:53"
        },
        {
          "id": "MDRW2100039290.1.3",
          "form": "존트맛있어보이더라",
          "original_form": "존트맛있어보이더라",
          "speaker_id": "2",
          "time": "20210903 15:53"
        },
        {
          "id": "MDRW2100039290.1.4",
          "form": "근데 안동에 있대",
          "original_form": "근데 안동에 있대",
          "speaker_id": "2",
          "time": "20210903 15:53"
        },
        {
          "id": "MDRW2100039290.1.5",
          "form": "진짜멀다",
          "original_form": "진짜멀다",
          "speaker_id": "1",
          "time": "20210903 15:54"
        },
        {
          "id": "MDRW2100039290.1.6",
          "form": "근데 석쇠 못참는데",
          "original_form": "근데 석쇠 못참는데",
          "speaker_id": "1",
          "time": "20210903 15:54"
        },
        {
          "id": "MDRW2100039290.1.7",
          "form": "ㅋㅋㅋㅋ 비결이",
          "original_form": "ㅋㅋㅋㅋ 비결이",
          "speaker_id": "2",
          "time": "20210903 15:54"
        },
        {
          "id": "MDRW2100039290.1.8",
          "form": "2.5mm두께래",
          "original_form": "2.5mm두께래",
          "speaker_id": "2",
          "time": "20210903 15:54"
        },
        {
          "id": "MDRW2100039290.1.9",
          "form": "겉바속촉?",
          "original_form": "겉바속촉?",
          "speaker_id": "1",
          "time": "20210903 15:54"
        },
        {
          "id": "MDRW2100039290.1.10",
          "form": "그리고 고기를 쉬게하면안된대",
          "original_form": "그리고 고기를 쉬게하면안된대",
          "speaker_id": "2",
          "time": "20210903 15:54"
        },
        {
          "id": "MDRW2100039290.1.11",
          "form": "그런듯 ㅋㅋㅋㅋ",
          "original_form": "그런듯 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:54"
        },
        {
          "id": "MDRW2100039290.1.12",
          "form": "거기에 쏘주 들이붓고싶댜",
          "original_form": "거기에 쏘주 들이붓고싶댜",
          "speaker_id": "2",
          "time": "20210903 15:54"
        },
        {
          "id": "MDRW2100039290.1.13",
          "form": "오늘 소주 마실거야",
          "original_form": "오늘 소주 마실거야",
          "speaker_id": "1",
          "time": "20210903 15:57"
        },
        {
          "id": "MDRW2100039290.1.14",
          "form": "ㅠ안도ㅑ애",
          "original_form": "ㅠ안도ㅑ애",
          "speaker_id": "2",
          "time": "20210903 15:57"
        },
        {
          "id": "MDRW2100039290.1.15",
          "form": "달달한가",
          "original_form": "달달한가",
          "speaker_id": "1",
          "time": "20210903 15:57"
        },
        {
          "id": "MDRW2100039290.1.16",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "2",
          "time": "20210903 15:57"
        },
        {
          "id": "MDRW2100039290.1.17",
          "form": "밥볶나요",
          "original_form": "밥볶나요",
          "speaker_id": "2",
          "time": "20210903 15:57"
        },
        {
          "id": "MDRW2100039290.1.18",
          "form": "디저트",
          "original_form": "디저트",
          "speaker_id": "2",
          "time": "20210903 15:57"
        },
        {
          "id": "MDRW2100039290.1.19",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210903 15:57"
        },
        {
          "id": "MDRW2100039290.1.20",
          "form": "실망입니다",
          "original_form": "실망입니다",
          "speaker_id": "2",
          "time": "20210903 15:57"
        },
        {
          "id": "MDRW2100039290.1.21",
          "form": "나는 밥이랑같이먹어",
          "original_form": "나는 밥이랑같이먹어",
          "speaker_id": "1",
          "time": "20210903 15:57"
        },
        {
          "id": "MDRW2100039290.1.22",
          "form": "아 다먹고 먹는데 ㅎ",
          "original_form": "아 다먹고 먹는데 ㅎ",
          "speaker_id": "2",
          "time": "20210903 15:58"
        },
        {
          "id": "MDRW2100039290.1.23",
          "form": "그래야 깔끔하다구",
          "original_form": "그래야 깔끔하다구",
          "speaker_id": "2",
          "time": "20210903 15:58"
        },
        {
          "id": "MDRW2100039290.1.24",
          "form": "후식은",
          "original_form": "후식은",
          "speaker_id": "2",
          "time": "20210903 15:58"
        },
        {
          "id": "MDRW2100039290.1.25",
          "form": "흰 쌀밥에 된장바른 고기가 최고지",
          "original_form": "흰 쌀밥에 된장바른 고기가 최고지",
          "speaker_id": "1",
          "time": "20210903 15:59"
        },
        {
          "id": "MDRW2100039290.1.26",
          "form": "맛있긴하지..흰쌀밥최고",
