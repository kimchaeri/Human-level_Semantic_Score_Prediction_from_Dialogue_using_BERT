      "utterance": [
        {
          "id": "MDRW2100036000.1.1",
          "form": "식사 하셨을까요?",
          "original_form": "식사 하셨을까요?",
          "speaker_id": "2",
          "time": "20210830 13:57"
        },
        {
          "id": "MDRW2100036000.1.2",
          "form": "네 방금 먹었어요",
          "original_form": "네 방금 먹었어요",
          "speaker_id": "1",
          "time": "20210830 13:57"
        },
        {
          "id": "MDRW2100036000.1.3",
          "form": "드셨나요?",
          "original_form": "드셨나요?",
          "speaker_id": "1",
          "time": "20210830 13:58"
        },
        {
          "id": "MDRW2100036000.1.4",
          "form": "넵! 전 치킨 먹었어요!",
          "original_form": "넵! 전 치킨 먹었어요!",
          "speaker_id": "2",
          "time": "20210830 13:58"
        },
        {
          "id": "MDRW2100036000.1.5",
          "form": "어떤거 드셨어요?",
          "original_form": "어떤거 드셨어요?",
          "speaker_id": "2",
          "time": "20210830 13:58"
        },
        {
          "id": "MDRW2100036000.1.6",
          "form": "저는 샌드위치요!",
          "original_form": "저는 샌드위치요!",
          "speaker_id": "1",
          "time": "20210830 14:00"
        },
        {
          "id": "MDRW2100036000.1.7",
          "form": "치킨 어디꺼 드셨나요?",
          "original_form": "치킨 어디꺼 드셨나요?",
          "speaker_id": "1",
          "time": "20210830 14:00"
        },
        {
          "id": "MDRW2100036000.1.8",
          "form": "치교치킨이라고 새로 생긴 곳 같더라구요!",
          "original_form": "치교치킨이라고 새로 생긴 곳 같더라구요!",
          "speaker_id": "2",
          "time": "20210830 15:01"
        },
        {
          "id": "MDRW2100036000.1.9",
          "form": "숯불이였어요!",
          "original_form": "숯불이였어요!",
          "speaker_id": "2",
          "time": "20210830 15:02"
        },
        {
          "id": "MDRW2100036000.1.10",
          "form": "숯불치킨 치밥하기 좋죠!",
          "original_form": "숯불치킨 치밥하기 좋죠!",
          "speaker_id": "1",
          "time": "20210830 15:09"
        },
        {
          "id": "MDRW2100036000.1.11",
          "form": "치밥하셨어요?",
          "original_form": "치밥하셨어요?",
          "speaker_id": "1",
          "time": "20210830 15:09"
        },
        {
          "id": "MDRW2100036000.1.12",
          "form": "네!! 주먹밥하고 먹었어요",
          "original_form": "네!! 주먹밥하고 먹었어요",
          "speaker_id": "2",
          "time": "20210830 15:12"
        },
        {
          "id": "MDRW2100036000.1.13",
          "form": "너무 맛있어서 폭식했어요ㅋㅋ",
          "original_form": "너무 맛있어서 폭식했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 15:13"
        },
        {
          "id": "MDRW2100036000.1.14",
          "form": "점심이니까 찬찬히 소화시키시면 되겠네요!",
          "original_form": "점심이니까 찬찬히 소화시키시면 되겠네요!",
          "speaker_id": "1",
          "time": "20210830 15:15"
        },
        {
          "id": "MDRW2100036000.1.15",
          "form": "치킨은 원래 숯불쪽을 좋아하시나요?",
          "original_form": "치킨은 원래 숯불쪽을 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210830 15:15"
        },
        {
          "id": "MDRW2100036000.1.16",
          "form": "넹!",
          "original_form": "넹!",
          "speaker_id": "2",
          "time": "20210830 15:49"
        },
        {
          "id": "MDRW2100036000.1.17",
          "form": "치킨.. 그냥 다 좋아해요",
          "original_form": "치킨.. 그냥 다 좋아해요",
          "speaker_id": "2",
          "time": "20210830 15:49"
        },
        {
          "id": "MDRW2100036000.1.18",
          "form": "하나만 고를 수 없어요",
          "original_form": "하나만 고를 수 없어요",
          "speaker_id": "2",
          "time": "20210830 15:49"
        },
        {
          "id": "MDRW2100036000.1.19",
          "form": "치킨은....고르기 힘들긴하죠...",
          "original_form": "치킨은....고르기 힘들긴하죠...",
          "speaker_id": "1",
          "time": "20210830 15:57"
        },
        {
          "id": "MDRW2100036000.1.20",
          "form": "저는 bbp 후라이드가 제일 좋아요",
          "original_form": "저는 bbp 후라이드가 제일 좋아요",
          "speaker_id": "1",
          "time": "20210830 15:57"
        },
        {
          "id": "MDRW2100036000.1.21",
          "form": "황금올리브?!?!",
          "original_form": "황금올리브?!?!",
          "speaker_id": "2",
          "time": "20210830 16:41"
        },
        {
          "id": "MDRW2100036000.1.22",
          "form": "바삭함이 맛있어요",
          "original_form": "바삭함이 맛있어요",
          "speaker_id": "2",
          "time": "20210830 16:41"
        },
        {
          "id": "MDRW2100036000.1.23",
          "form": "바삭하고 육즙이 풍부해서 좋아요 저는 치킨은 결국 닭맛에 먹나봐요",
          "original_form": "바삭하고 육즙이 풍부해서 좋아요 저는 치킨은 결국 닭맛에 먹나봐요",
          "speaker_id": "1",
          "time": "20210830 16:42"
        },
        {
          "id": "MDRW2100036000.1.24",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210830 18:06"
        },
        {
          "id": "MDRW2100036000.1.25",
          "form": "진짜 매일 먹어도 다르게 먹을 수 있어서 더 매력적이예요",
          "original_form": "진짜 매일 먹어도 다르게 먹을 수 있어서 더 매력적이예요",
