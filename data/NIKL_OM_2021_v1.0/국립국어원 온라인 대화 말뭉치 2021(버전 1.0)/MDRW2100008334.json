      "utterance": [
        {
          "id": "MDRW2100008334.1.1",
          "form": "삼겹살 맛없제??",
          "original_form": "삼겹살 맛없제??",
          "speaker_id": "1",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008334.1.2",
          "form": "요새 좀 글트라이",
          "original_form": "요새 좀 글트라이",
          "speaker_id": "1",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008334.1.3",
          "form": "장보고오께 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "장보고오께 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:14"
        },
        {
          "id": "MDRW2100008334.1.4",
          "form": "겹살이는 오케이 안에꺼도 껍데기있어가ㅠ ㅋㅋㅋㅋ맛있는거 많이 사와여~",
          "original_form": "겹살이는 오케이 안에꺼도 껍데기있어가ㅠ ㅋㅋㅋㅋ맛있는거 많이 사와여~",
          "speaker_id": "2",
          "time": "20210822 15:24"
        },
        {
          "id": "MDRW2100008334.1.5",
          "form": "갔다왔음",
          "original_form": "갔다왔음",
          "speaker_id": "1",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008334.1.6",
          "form": "근데 껍데이 있어야 맛있는거아니가??",
          "original_form": "근데 껍데이 있어야 맛있는거아니가??",
          "speaker_id": "1",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008334.1.7",
          "form": "나는 껍데기 안묵으요 ㅋㅋㅋ",
          "original_form": "나는 껍데기 안묵으요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008334.1.8",
          "form": "언니 껍데기 좋아하져??ㅋㅋㅋㅋ",
          "original_form": "언니 껍데기 좋아하져??ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008334.1.9",
          "form": "어후 ㅋㅋㅋㅋㅋㄱㅋㄱ",
          "original_form": "어후 ㅋㅋㅋㅋㅋㄱㅋㄱ",
          "speaker_id": "1",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008334.1.10",
          "form": "엉 ㅋㅋㄱ 껍데기 매니아",
          "original_form": "엉 ㅋㅋㄱ 껍데기 매니아",
          "speaker_id": "1",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008334.1.11",
          "form": "난 보들보들한거 ㅋㅋㅋ",
          "original_form": "난 보들보들한거 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008334.1.12",
          "form": "보들보들 그튼소리한다 아무거나 다 무야지",
          "original_form": "보들보들 그튼소리한다 아무거나 다 무야지",
          "speaker_id": "1",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008334.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ보기보다 많이 가림 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ보기보다 많이 가림 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:15"
        },
        {
          "id": "MDRW2100008334.1.14",
          "form": "캐가 저녁음 뭔데여??",
          "original_form": "캐가 저녁음 뭔데여??",
          "speaker_id": "2",
          "time": "20210822 16:15"
        },
        {
          "id": "MDRW2100008334.1.15",
          "form": "애들밥 ㅋㅋㅋㅋ",
          "original_form": "애들밥 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008334.1.16",
          "form": "그래 그만좀 가려라 ㅋㅋㅋ",
          "original_form": "그래 그만좀 가려라 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008334.1.17",
          "form": "고등어 ㅋㅋㅋㄱㄱㄱㅋㅋㅋㅋ",
          "original_form": "고등어 ㅋㅋㅋㄱㄱㄱㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008334.1.18",
          "form": "거봉이랑 우유 초콜릿 사왔다 ㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "거봉이랑 우유 초콜릿 사왔다 ㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008334.1.19",
          "form": "우리투가 과일 잘 안먹는데 요새 거봉 잘 먹길래",
          "original_form": "우리투가 과일 잘 안먹는데 요새 거봉 잘 먹길래",
          "speaker_id": "1",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008334.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ아 아까 고둥어얘기했는가??",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 아까 고둥어얘기했는가??",
          "speaker_id": "2",
          "time": "20210822 16:17"
        },
        {
          "id": "MDRW2100008334.1.21",
          "form": "거봉 맛있겠구만요~",
          "original_form": "거봉 맛있겠구만요~",
          "speaker_id": "2",
          "time": "20210822 16:17"
        },
        {
          "id": "MDRW2100008334.1.22",
          "form": "언니집에 우유는 진짜 ㅋㅋㅋㅋ",
          "original_form": "언니집에 우유는 진짜 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:18"
        },
        {
          "id": "MDRW2100008334.1.23",
          "form": "배달받아야 겠더만요~",
          "original_form": "배달받아야 겠더만요~",
          "speaker_id": "2",
          "time": "20210822 16:18"
        },
        {
          "id": "MDRW2100008334.1.24",
          "form": "안핬나??",
          "original_form": "안핬나??",
          "speaker_id": "1",
          "time": "20210822 16:19"
        },
        {
          "id": "MDRW2100008334.1.25",
          "form": "누구랑 했는지 까먹음 ㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "누구랑 했는지 까먹음 ㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210822 16:19"
        },
        {
          "id": "MDRW2100008334.1.26",
          "form": "그래 우유는 늘 2통씩 있어야됨",
          "original_form": "그래 우유는 늘 2통씩 있어야됨",
          "speaker_id": "1",
          "time": "20210822 16:19"
        },
        {
          "id": "MDRW2100008334.1.27",
          "form": "소를 키우던지 해야지 원",
          "original_form": "소를 키우던지 해야지 원",
          "speaker_id": "1",
          "time": "20210822 16:19"
        },
        {
          "id": "MDRW2100008334.1.28",
          "form": "ㅋㅋㅋㅋ뭔 말을 하고픈데 걸릴까봐 패쓰 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ뭔 말을 하고픈데 걸릴까봐 패쓰 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:26"
        },
        {
          "id": "MDRW2100008334.1.29",
          "form": "근데 진짜 커피 겁나 맛남 ㅋㅋ",
          "original_form": "근데 진짜 커피 겁나 맛남 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:26"
        },
        {
          "id": "MDRW2100008334.1.30",
          "form": "육출 할라믄 커피 때려넣고 시작해야 ㅋㅋㅋ이제 그맛을 알았으요 ㅋㅋ",
          "original_form": "육출 할라믄 커피 때려넣고 시작해야 ㅋㅋㅋ이제 그맛을 알았으요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100008334.1.31",
          "form": "훗",
          "original_form": "훗",
          "speaker_id": "1",
          "time": "20210822 18:02"
        },
        {
          "id": "MDRW2100008334.1.32",
          "form": "그래 이제야 커피맛을 알았구먼",
          "original_form": "그래 이제야 커피맛을 알았구먼",
          "speaker_id": "1",
          "time": "20210822 18:02"
        },
        {
          "id": "MDRW2100008334.1.33",
          "form": "커피없이 하루가 힘들어여 ㅋㅋㅋ",
          "original_form": "커피없이 하루가 힘들어여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:50"
        },
        {
          "id": "MDRW2100008334.1.34",
          "form": "우리집 온나",
          "original_form": "우리집 온나",
          "speaker_id": "1",
          "time": "20210822 20:55"
        },
        {
          "id": "MDRW2100008334.1.35",
          "form": "언제든 타주께 ㅎㅎ",
          "original_form": "언제든 타주께 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 20:55"
        },
        {
          "id": "MDRW2100008334.1.36",
          "form": "자나??",
          "original_form": "자나??",
          "speaker_id": "1",
          "time": "20210822 21:32"
        },
        {
          "id": "MDRW2100008334.1.37",
