      "utterance": [
        {
          "id": "MDRW2100001678.1.1",
          "form": "싸이월드부활어떰???",
          "original_form": "싸이월드부활어떰???",
          "speaker_id": "1",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100001678.1.2",
          "form": "아하 그거",
          "original_form": "아하 그거",
          "speaker_id": "2",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100001678.1.3",
          "form": "진짜 대박이드라",
          "original_form": "진짜 대박이드라",
          "speaker_id": "2",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100001678.1.4",
          "form": "사람들 개인정보를 몇년동안 가지고있었단거잖아",
          "original_form": "사람들 개인정보를 몇년동안 가지고있었단거잖아",
          "speaker_id": "2",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100001678.1.5",
          "form": "아 그니까 그거때문에",
          "original_form": "아 그니까 그거때문에",
          "speaker_id": "1",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100001678.1.6",
          "form": "그거 좀 소름....",
          "original_form": "그거 좀 소름....",
          "speaker_id": "2",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100001678.1.7",
          "form": "사람들 말 많더러",
          "original_form": "사람들 말 많더러",
          "speaker_id": "1",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100001678.1.8",
          "form": "라",
          "original_form": "라",
          "speaker_id": "1",
          "time": "20210819 17:59"
        },
        {
          "id": "MDRW2100001678.1.9",
          "form": "아진짜??",
          "original_form": "아진짜??",
          "speaker_id": "2",
          "time": "20210819 18:00"
        },
        {
          "id": "MDRW2100001678.1.10",
          "form": "근데 나라도..소름끼침",
          "original_form": "근데 나라도..소름끼침",
          "speaker_id": "2",
          "time": "20210819 18:00"
        },
        {
          "id": "MDRW2100001678.1.11",
          "form": "나는 싸이세대는 아니지만",
          "original_form": "나는 싸이세대는 아니지만",
          "speaker_id": "2",
          "time": "20210819 18:01"
        },
        {
          "id": "MDRW2100001678.1.12",
          "form": "싸이세대였다면 더 싫었을듯",
          "original_form": "싸이세대였다면 더 싫었을듯",
          "speaker_id": "2",
          "time": "20210819 18:01"
        },
        {
          "id": "MDRW2100001678.1.13",
          "form": "맞아 나는 싸이월드 조금 하긴햇는데",
          "original_form": "맞아 나는 싸이월드 조금 하긴햇는데",
          "speaker_id": "1",
          "time": "20210819 18:04"
        },
        {
          "id": "MDRW2100001678.1.14",
          "form": "진짜 조금이라서",
          "original_form": "진짜 조금이라서",
          "speaker_id": "1",
          "time": "20210819 18:04"
        },
        {
          "id": "MDRW2100001678.1.15",
          "form": "맞어",
          "original_form": "맞어",
          "speaker_id": "2",
          "time": "20210819 18:04"
        },
        {
          "id": "MDRW2100001678.1.16",
          "form": "너는 진짜 어릴때 엄마가 하는거 했잖어",
          "original_form": "너는 진짜 어릴때 엄마가 하는거 했잖어",
          "speaker_id": "2",
          "time": "20210819 18:04"
        },
        {
          "id": "MDRW2100001678.1.17",
          "form": "맞지?ㅋㅋ",
          "original_form": "맞지?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 18:04"
        },
        {
          "id": "MDRW2100001678.1.18",
          "form": "어 엄마가 하는거 따라함",
          "original_form": "어 엄마가 하는거 따라함",
          "speaker_id": "1",
          "time": "20210819 18:04"
        },
        {
          "id": "MDRW2100001678.1.19",
          "form": "나도 그때 친구들 한두명밖에 안했었는데",
          "original_form": "나도 그때 친구들 한두명밖에 안했었는데",
          "speaker_id": "2",
          "time": "20210819 18:04"
        },
        {
          "id": "MDRW2100001678.1.20",
          "form": "나껏도 같이 부활됏으려나?",
          "original_form": "나껏도 같이 부활됏으려나?",
          "speaker_id": "1",
          "time": "20210819 18:05"
        },
        {
          "id": "MDRW2100001678.1.21",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210819 18:05"
        },
        {
          "id": "MDRW2100001678.1.22",
          "form": "부활됐으면 웃기겠다",
          "original_form": "부활됐으면 웃기겠다",
          "speaker_id": "2",
          "time": "20210819 18:05"
        },
        {
          "id": "MDRW2100001678.1.23",
          "form": "맞아 나 완전 별것도아닌거 올려논거 많은데",
          "original_form": "맞아 나 완전 별것도아닌거 올려논거 많은데",
          "speaker_id": "1",
          "time": "20210819 18:05"
        },
        {
          "id": "MDRW2100001678.1.24",
          "form": "한번 드가볼까?",
          "original_form": "한번 드가볼까?",
          "speaker_id": "1",
          "time": "20210819 18:05"
        },
        {
          "id": "MDRW2100001678.1.25",
          "form": "그래그래",
          "original_form": "그래그래",
          "speaker_id": "2",
          "time": "20210819 18:05"
        },
        {
          "id": "MDRW2100001678.1.26",
          "form": "근데 지금 부활한거 맞아??",
          "original_form": "근데 지금 부활한거 맞아??",
          "speaker_id": "2",
          "time": "20210819 18:06"
        },
        {
          "id": "MDRW2100001678.1.27",
          "form": "자세히몰라",
          "original_form": "자세히몰라",
          "speaker_id": "1",
          "time": "20210819 18:06"
        },
        {
          "id": "MDRW2100001678.1.28",
          "form": "미뤄졌다 하든데",
          "original_form": "미뤄졌다 하든데",
          "speaker_id": "2",
          "time": "20210819 18:06"
        },
        {
          "id": "MDRW2100001678.1.29",
          "form": "그럼 아직 아닐듯??",
          "original_form": "그럼 아직 아닐듯??",
          "speaker_id": "2",
          "time": "20210819 18:06"
        },
        {
          "id": "MDRW2100001678.1.30",
          "form": "기다려보자",
          "original_form": "기다려보자",
          "speaker_id": "2",
          "time": "20210819 18:06"
        },
        {
          "id": "MDRW2100001678.1.31",
          "form": "그론가",
          "original_form": "그론가",
          "speaker_id": "1",
          "time": "20210819 18:06"
        },
        {
          "id": "MDRW2100001678.1.32",
          "form": "키킼",
          "original_form": "키킼",
