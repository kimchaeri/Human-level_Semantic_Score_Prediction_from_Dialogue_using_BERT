      "utterance": [
        {
          "id": "MDRW2100025582.1.1",
          "form": "ㅋㅋㅋㅋ진짜 발연기.....",
          "original_form": "ㅋㅋㅋㅋ진짜 발연기.....",
          "speaker_id": "2",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100025582.1.2",
          "form": "ㅋㅋㄱㅋㅋㅋㅋㅋㄱㅋㅋㄱ 성대로 가려지지않는 그의연기",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋㅋㄱㅋㅋㄱ 성대로 가려지지않는 그의연기",
          "speaker_id": "1",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100025582.1.3",
          "form": "은 홍",
          "original_form": "은 홍",
          "speaker_id": "1",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100025582.1.4",
          "form": "박은태는 요새뭐하지",
          "original_form": "박은태는 요새뭐하지",
          "speaker_id": "1",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100025582.1.5",
          "form": "노래는참 다들 기가막히게 하는데",
          "original_form": "노래는참 다들 기가막히게 하는데",
          "speaker_id": "1",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100025582.1.6",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "1",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100025582.1.7",
          "form": "박은태 최근에 젠가....",
          "original_form": "박은태 최근에 젠가....",
          "speaker_id": "2",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025582.1.8",
          "form": "오......",
          "original_form": "오......",
          "speaker_id": "1",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025582.1.9",
          "form": "그 이후는 모르겠다ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅜㅜㅜㅜ",
          "original_form": "그 이후는 모르겠다ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025582.1.10",
          "form": "류님은",
          "original_form": "류님은",
          "speaker_id": "1",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025582.1.11",
          "form": "정말오랜만에",
          "original_form": "정말오랜만에",
          "speaker_id": "1",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025582.1.12",
          "form": "라만차로봤는데",
          "original_form": "라만차로봤는데",
          "speaker_id": "1",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025582.1.13",
          "form": "성대가 더 짱짱하던데....",
          "original_form": "성대가 더 짱짱하던데....",
          "speaker_id": "1",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025582.1.14",
          "form": "류님은 나이 안드셔....?",
          "original_form": "류님은 나이 안드셔....?",
          "speaker_id": "2",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025582.1.15",
          "form": "더짱짱하다니",
          "original_form": "더짱짱하다니",
          "speaker_id": "2",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025582.1.16",
          "form": "ㅋㄱㅋㅋㅋㅋ류님별명류르신...",
          "original_form": "ㅋㄱㅋㅋㅋㅋ류님별명류르신...",
          "speaker_id": "1",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100025582.1.17",
          "form": "더어려지셨던데",
          "original_form": "더어려지셨던데",
          "speaker_id": "1",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100025582.1.18",
          "form": "1열에서봤는데",
          "original_form": "1열에서봤는데",
          "speaker_id": "1",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100025582.1.19",
          "form": "보고깜짝놀랐어 뱀파이언줄알고",
          "original_form": "보고깜짝놀랐어 뱀파이언줄알고",
          "speaker_id": "1",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100025582.1.20",
          "form": "나이는 나만먹었나",
          "original_form": "나이는 나만먹었나",
          "speaker_id": "1",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100025582.1.21",
          "form": "류르신 그거 멸칭 아니었어???\r\n연뮤갤 애들이 기량 떨어졌다고 깔 때 쓴다고\r\n어디서 주워 들어서 안썼는데ㅜㅠ아니었나보네",
          "original_form": "류르신 그거 멸칭 아니었어???\r\n연뮤갤 애들이 기량 떨어졌다고 깔 때 쓴다고\r\n어디서 주워 들어서 안썼는데ㅜㅠ아니었나보네",
          "speaker_id": "2",
          "time": "20210901 13:53"
        },
        {
          "id": "MDRW2100025582.1.22",
          "form": "ㅎ....",
          "original_form": "ㅎ....",
          "speaker_id": "1",
          "time": "20210901 13:55"
        },
        {
          "id": "MDRW2100025582.1.23",
          "form": "저도그래서",
          "original_form": "저도그래서",
          "speaker_id": "1",
          "time": "20210901 13:55"
        },
        {
          "id": "MDRW2100025582.1.24",
          "form": "솔직히잘안써여",
          "original_form": "솔직히잘안써여",
          "speaker_id": "1",
          "time": "20210901 13:55"
        },
        {
          "id": "MDRW2100025582.1.25",
          "form": "난 그래서류르신이라고 안부름",
          "original_form": "난 그래서류르신이라고 안부름",
          "speaker_id": "1",
          "time": "20210901 13:55"
        },
        {
          "id": "MDRW2100025582.1.26",
          "form": "근데요새멸칭인거모르고쓰는사람도많아서",
          "original_form": "근데요새멸칭인거모르고쓰는사람도많아서",
          "speaker_id": "1",
          "time": "20210901 13:55"
        },
        {
          "id": "MDRW2100025582.1.27",
          "form": "그냥쓰나보더라",
          "original_form": "그냥쓰나보더라",
          "speaker_id": "1",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100025582.1.28",
          "form": "모르느사람이 많으니까...",
          "original_form": "모르느사람이 많으니까...",
          "speaker_id": "1",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100025582.1.29",
          "form": "아 그럴수잇지",
          "original_form": "아 그럴수잇지",
          "speaker_id": "2",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100025582.1.30",
          "form": "계속쓰다보면",
          "original_form": "계속쓰다보면",
          "speaker_id": "2",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100025582.1.31",
          "form": "근데 ㅠ 맘아파 그럼 혼자",
          "original_form": "근데 ㅠ 맘아파 그럼 혼자",
          "speaker_id": "1",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100025582.1.32",
          "form": "니가몰알아",
          "original_form": "니가몰알아",
          "speaker_id": "1",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100025582.1.33",
          "form": "조랭이떡보다못한게",
          "original_form": "조랭이떡보다못한게",
          "speaker_id": "1",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100025582.1.34",
          "form": "하고 혼자생각하지",
          "original_form": "하고 혼자생각하지",
          "speaker_id": "1",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100025582.1.35",
          "form": "원래의미퇴색되고 걍별명처럼 되는 경우잇지",
          "original_form": "원래의미퇴색되고 걍별명처럼 되는 경우잇지",
          "speaker_id": "2",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100025582.1.36",
          "form": "누가기량이떨어졌대",
          "original_form": "누가기량이떨어졌대",
          "speaker_id": "1",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100025582.1.37",
          "form": "근데 류님은 기량 운운하기에는",
          "original_form": "근데 류님은 기량 운운하기에는",
          "speaker_id": "2",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100025582.1.38",
          "form": "귀가삐었나봐",
          "original_form": "귀가삐었나봐",
          "speaker_id": "1",
          "time": "20210901 13:57"
        },
        {
          "id": "MDRW2100025582.1.39",
          "form": "걍 지금도 잘하고",
          "original_form": "걍 지금도 잘하고",
          "speaker_id": "2",
          "time": "20210901 13:57"
        },
        {
          "id": "MDRW2100025582.1.40",
          "form": "리빙레전드아니냐ㅜㅠ",
          "original_form": "리빙레전드아니냐ㅜㅠ",
          "speaker_id": "2",
          "time": "20210901 13:57"
        },
        {
          "id": "MDRW2100025582.1.41",
          "form": "10년전이랑",
          "original_form": "10년전이랑",
          "speaker_id": "1",
          "time": "20210901 13:57"
        },
        {
          "id": "MDRW2100025582.1.42",
          "form": "지금이랑",
          "original_form": "지금이랑",
          "speaker_id": "1",
          "time": "20210901 13:57"
        },
        {
          "id": "MDRW2100025582.1.43",
