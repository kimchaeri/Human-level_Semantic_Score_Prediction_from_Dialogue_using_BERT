      "utterance": [
        {
          "id": "MDRW2100012025.1.1",
          "form": "안녕하세요! 식사하셨나요?",
          "original_form": "안녕하세요! 식사하셨나요?",
          "speaker_id": "1",
          "time": "20210824 18:31"
        },
        {
          "id": "MDRW2100012025.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210824 18:31"
        },
        {
          "id": "MDRW2100012025.1.3",
          "form": "네 먹었습니다",
          "original_form": "네 먹었습니다",
          "speaker_id": "2",
          "time": "20210824 18:31"
        },
        {
          "id": "MDRW2100012025.1.4",
          "form": "저는 오리고기먹었습니다",
          "original_form": "저는 오리고기먹었습니다",
          "speaker_id": "2",
          "time": "20210824 18:32"
        },
        {
          "id": "MDRW2100012025.1.5",
          "form": "파르페님은 식사하셨나요?",
          "original_form": "파르페님은 식사하셨나요?",
          "speaker_id": "2",
          "time": "20210824 18:32"
        },
        {
          "id": "MDRW2100012025.1.6",
          "form": "네 저도 순두부찌개랑 같이 먹었어요!",
          "original_form": "네 저도 순두부찌개랑 같이 먹었어요!",
          "speaker_id": "1",
          "time": "20210824 18:32"
        },
        {
          "id": "MDRW2100012025.1.7",
          "form": "오리고기는 구워드셨나요?",
          "original_form": "오리고기는 구워드셨나요?",
          "speaker_id": "1",
          "time": "20210824 18:32"
        },
        {
          "id": "MDRW2100012025.1.8",
          "form": "네! 훈제오리고기였어요",
          "original_form": "네! 훈제오리고기였어요",
          "speaker_id": "2",
          "time": "20210824 18:41"
        },
        {
          "id": "MDRW2100012025.1.9",
          "form": "순두부찌개 맛있겠어요ㅠㅠ 얼큰하니",
          "original_form": "순두부찌개 맛있겠어요ㅠㅠ 얼큰하니",
          "speaker_id": "2",
          "time": "20210824 18:42"
        },
        {
          "id": "MDRW2100012025.1.10",
          "form": "오랜만에 먹었는데 맛있더라고요!",
          "original_form": "오랜만에 먹었는데 맛있더라고요!",
          "speaker_id": "1",
          "time": "20210824 18:43"
        },
        {
          "id": "MDRW2100012025.1.11",
          "form": "무쌈이나 곁을여 드신 거 있으세요?",
          "original_form": "무쌈이나 곁을여 드신 거 있으세요?",
          "speaker_id": "1",
          "time": "20210824 18:43"
        },
        {
          "id": "MDRW2100012025.1.12",
          "form": "네 맞아요!",
          "original_form": "네 맞아요!",
          "speaker_id": "2",
          "time": "20210824 19:48"
        },
        {
          "id": "MDRW2100012025.1.13",
          "form": "배우신분이시군요!",
          "original_form": "배우신분이시군요!",
          "speaker_id": "2",
          "time": "20210824 19:49"
        },
        {
          "id": "MDRW2100012025.1.14",
          "form": "역시 오리고기에는 무쌈이죠!",
          "original_form": "역시 오리고기에는 무쌈이죠!",
          "speaker_id": "2",
          "time": "20210824 19:49"
        },
        {
          "id": "MDRW2100012025.1.15",
          "form": "소스는 뭐랑 드셨나요?",
          "original_form": "소스는 뭐랑 드셨나요?",
          "speaker_id": "1",
          "time": "20210824 19:49"
        },
        {
          "id": "MDRW2100012025.1.16",
          "form": "머스타드랑 해서 먹었습니다",
          "original_form": "머스타드랑 해서 먹었습니다",
          "speaker_id": "2",
          "time": "20210824 19:50"
        },
        {
          "id": "MDRW2100012025.1.17",
          "form": "역시 오리고기에는 머스타드죠!",
          "original_form": "역시 오리고기에는 머스타드죠!",
          "speaker_id": "1",
          "time": "20210824 19:50"
        },
        {
          "id": "MDRW2100012025.1.18",
          "form": "순두부찌개는 사드신건가요?",
          "original_form": "순두부찌개는 사드신건가요?",
          "speaker_id": "2",
          "time": "20210824 19:50"
        },
        {
          "id": "MDRW2100012025.1.19",
          "form": "맞아요!ㅋㅋㅋ",
          "original_form": "맞아요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:50"
        },
        {
          "id": "MDRW2100012025.1.20",
          "form": "맞잘알이시네요",
          "original_form": "맞잘알이시네요",
          "speaker_id": "2",
          "time": "20210824 19:50"
        },
        {
          "id": "MDRW2100012025.1.21",
          "form": "맛잘알",
          "original_form": "맛잘알",
          "speaker_id": "2",
          "time": "20210824 19:50"
        },
        {
          "id": "MDRW2100012025.1.22",
          "form": "아니요 할머니가 끓여주셨어요ㅎㅎ",
          "original_form": "아니요 할머니가 끓여주셨어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 19:50"
        },
        {
          "id": "MDRW2100012025.1.23",
          "form": "저는 순두부 사는 것만 했어요",
          "original_form": "저는 순두부 사는 것만 했어요",
          "speaker_id": "1",
          "time": "20210824 19:50"
        },
        {
          "id": "MDRW2100012025.1.24",
          "form": "우와 만들어먹는 순두부찌개였군요",
          "original_form": "우와 만들어먹는 순두부찌개였군요",
          "speaker_id": "2",
          "time": "20210824 19:59"
        },
        {
          "id": "MDRW2100012025.1.25",
          "form": "엄청 맛있겠어요ㅠㅠㅠ",
          "original_form": "엄청 맛있겠어요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 19:59"
        },
        {
          "id": "MDRW2100012025.1.26",
          "form": "할머니표라니.. 더 맛있을것 같아요",
          "original_form": "할머니표라니.. 더 맛있을것 같아요",
          "speaker_id": "2",
          "time": "20210824 20:00"
        },
        {
          "id": "MDRW2100012025.1.27",
          "form": "방학이라 할머니네에서 매일 맛있는 거 먹고 있어요ㅎㅎ",
          "original_form": "방학이라 할머니네에서 매일 맛있는 거 먹고 있어요ㅎㅎ",
