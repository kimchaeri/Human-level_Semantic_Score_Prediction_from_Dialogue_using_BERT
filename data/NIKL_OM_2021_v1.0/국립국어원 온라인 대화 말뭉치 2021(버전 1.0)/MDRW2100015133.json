      "utterance": [
        {
          "id": "MDRW2100015133.1.1",
          "form": "탕에요??",
          "original_form": "탕에요??",
          "speaker_id": "2",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015133.1.2",
          "form": "오 네 마라탕은 땅콩소스 넣잖아요",
          "original_form": "오 네 마라탕은 땅콩소스 넣잖아요",
          "speaker_id": "1",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015133.1.3",
          "form": "아아 그런가요ㅋㅋ",
          "original_form": "아아 그런가요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100015133.1.4",
          "form": "땅콩 머더라 암튼 그거로 맵기조절할수있는데",
          "original_form": "땅콩 머더라 암튼 그거로 맵기조절할수있는데",
          "speaker_id": "1",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100015133.1.5",
          "form": "탕보다 훠궈를 많이먹어서",
          "original_form": "탕보다 훠궈를 많이먹어서",
          "speaker_id": "2",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100015133.1.6",
          "form": "아하 훠궈파시구나",
          "original_form": "아하 훠궈파시구나",
          "speaker_id": "1",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100015133.1.7",
          "form": "그걸 몰라서 맵게만먹었네요...",
          "original_form": "그걸 몰라서 맵게만먹었네요...",
          "speaker_id": "2",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100015133.1.8",
          "form": "안맵게주문하면 되죵",
          "original_form": "안맵게주문하면 되죵",
          "speaker_id": "1",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100015133.1.9",
          "form": "고기를 땅콩버터에 찍어먹음 맛있더라구용",
          "original_form": "고기를 땅콩버터에 찍어먹음 맛있더라구용",
          "speaker_id": "2",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100015133.1.10",
          "form": "야채가득널궁",
          "original_form": "야채가득널궁",
          "speaker_id": "1",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100015133.1.11",
          "form": "패기한번 부려봤다가",
          "original_form": "패기한번 부려봤다가",
          "speaker_id": "2",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100015133.1.12",
          "form": "마자용 고소한게 좋아요",
          "original_form": "마자용 고소한게 좋아요",
          "speaker_id": "1",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100015133.1.13",
          "form": "맵고 산초냄새만 나고",
          "original_form": "맵고 산초냄새만 나고",
          "speaker_id": "2",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100015133.1.14",
          "form": "실패한 전적이있어서 잘 안먹게되요 탕은ㅋㅋ",
          "original_form": "실패한 전적이있어서 잘 안먹게되요 탕은ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100015133.1.15",
          "form": "산초 좋아하는데",
          "original_form": "산초 좋아하는데",
          "speaker_id": "1",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100015133.1.16",
          "form": "글쿠나",
          "original_form": "글쿠나",
          "speaker_id": "1",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100015133.1.17",
          "form": "저는 마라탕말구 아비꼬카레에서 객기좀 부렸다가",
          "original_form": "저는 마라탕말구 아비꼬카레에서 객기좀 부렸다가",
          "speaker_id": "1",
          "time": "20210825 22:00"
        },
        {
          "id": "MDRW2100015133.1.18",
          "form": "혼났어요",
          "original_form": "혼났어요",
