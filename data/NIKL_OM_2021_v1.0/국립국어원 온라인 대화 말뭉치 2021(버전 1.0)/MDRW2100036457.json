      "utterance": [
        {
          "id": "MDRW2100036457.1.1",
          "form": "저 꿈이 가루요리사인데",
          "original_form": "저 꿈이 가루요리사인데",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100036457.1.2",
          "form": "가루 한개만 더 사면안되나여",
          "original_form": "가루 한개만 더 사면안되나여",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100036457.1.3",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100036457.1.4",
          "form": "어유 그러새요?",
          "original_form": "어유 그러새요?",
          "speaker_id": "1",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100036457.1.5",
          "form": "저기요......",
          "original_form": "저기요......",
          "speaker_id": "1",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100036457.1.6",
          "form": "집앞 떡볶이집 구현하고싶은데",
          "original_form": "집앞 떡볶이집 구현하고싶은데",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100036457.1.7",
          "form": "건강엔 가루가 좋을까요?",
          "original_form": "건강엔 가루가 좋을까요?",
          "speaker_id": "1",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100036457.1.8",
          "form": "그러면 햄버거 드시지마세여",
          "original_form": "그러면 햄버거 드시지마세여",
          "speaker_id": "2",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100036457.1.9",
          "form": "소고기 다시다로 충분해요",
          "original_form": "소고기 다시다로 충분해요",
          "speaker_id": "1",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100036457.1.10",
          "form": "달라요.",
          "original_form": "달라요.",
          "speaker_id": "2",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100036457.1.11",
          "form": "감칠맛은 똑같아요",
          "original_form": "감칠맛은 똑같아요",
          "speaker_id": "1",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100036457.1.12",
          "form": "갈색섀도우랑\r\n베이지색 섀도우랑 같나요?",
          "original_form": "갈색섀도우랑\r\n베이지색 섀도우랑 같나요?",
          "speaker_id": "2",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100036457.1.13",
          "form": "네 그리고 멸치다시다 얼만데여?",
          "original_form": "네 그리고 멸치다시다 얼만데여?",
          "speaker_id": "1",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100036457.1.14",
          "form": "빨간립스틱이랑\r\n분홍립스틱이랑 다른가요?\r\n아니겨든요",
          "original_form": "빨간립스틱이랑\r\n분홍립스틱이랑 다른가요?\r\n아니겨든요",
          "speaker_id": "2",
          "time": "20210830 23:22"
        },
        {
          "id": "MDRW2100036457.1.15",
          "form": "같네요",
          "original_form": "같네요",
          "speaker_id": "1",
          "time": "20210830 23:22"
        },
