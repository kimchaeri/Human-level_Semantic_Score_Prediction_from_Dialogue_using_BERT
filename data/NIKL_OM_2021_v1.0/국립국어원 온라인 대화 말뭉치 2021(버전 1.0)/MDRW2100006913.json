      "utterance": [
        {
          "id": "MDRW2100006913.1.1",
          "form": "혹시 여행가는 건 좋아하시나용?",
          "original_form": "혹시 여행가는 건 좋아하시나용?",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100006913.1.2",
          "form": "이시국이라 쉽지 않지만요ㅠ",
          "original_form": "이시국이라 쉽지 않지만요ㅠ",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100006913.1.3",
          "form": "이시국이라 오히려 더 가고 싶어요 ㅠㅠ",
          "original_form": "이시국이라 오히려 더 가고 싶어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 23:59"
        },
        {
          "id": "MDRW2100006913.1.4",
          "form": "아 진짜 너무 가고싶죠ㅠㅠ",
          "original_form": "아 진짜 너무 가고싶죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:00"
        },
        {
          "id": "MDRW2100006913.1.5",
          "form": "비행기 타고 나갔던 게 전생같아요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "비행기 타고 나갔던 게 전생같아요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:00"
        },
        {
          "id": "MDRW2100006913.1.6",
          "form": "여권이 어떻게 생겼는지 기억나지 않아요",
          "original_form": "여권이 어떻게 생겼는지 기억나지 않아요",
          "speaker_id": "2",
          "time": "20210822 00:01"
        },
        {
          "id": "MDRW2100006913.1.7",
          "form": "정말요ㅠㅠ 여행 어디 다녀오고 싶으세요?",
          "original_form": "정말요ㅠㅠ 여행 어디 다녀오고 싶으세요?",
          "speaker_id": "1",
          "time": "20210822 00:01"
        },
        {
          "id": "MDRW2100006913.1.8",
          "form": "대만도 가고 싶고 유럽도 한번쯤 가보고 싶어요",
          "original_form": "대만도 가고 싶고 유럽도 한번쯤 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100006913.1.9",
          "form": "사실 제주도만 가도 감지덕지지만요 ㅎㅎ",
          "original_form": "사실 제주도만 가도 감지덕지지만요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:04"
        },
        {
          "id": "MDRW2100006913.1.10",
          "form": "제주도 너무 가고 싶어요ㅋㅋㅋㅋ큐",
          "original_form": "제주도 너무 가고 싶어요ㅋㅋㅋㅋ큐",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006913.1.11",
          "form": "저두 유럽! 대만은 좋다고는 많이 들었는데 아직 못 가봤어서ㅠㅠ",
          "original_form": "저두 유럽! 대만은 좋다고는 많이 들었는데 아직 못 가봤어서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006913.1.12",
          "form": "대만 한번 가봤는데 좋아서 한번 더 가봐야지 하고 있어요 ㅎㅎ",
          "original_form": "대만 한번 가봤는데 좋아서 한번 더 가봐야지 하고 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006913.1.13",
          "form": "먹거리의 천국이라고 야시장 꼭 가보라고 하던데",
          "original_form": "먹거리의 천국이라고 야시장 꼭 가보라고 하던데",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006913.1.14",
          "form": "너무 좋을 거 같아요",
          "original_form": "너무 좋을 거 같아요",
          "speaker_id": "1",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006913.1.15",
          "form": "야시장 좋아요 재밌고 ㅎㅎ",
          "original_form": "야시장 좋아요 재밌고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:10"
        },
        {
          "id": "MDRW2100006913.1.16",
          "form": "대만 물가도 괜찮다고 하던데",
          "original_form": "대만 물가도 괜찮다고 하던데",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100006913.1.17",
          "form": "코로나가 언제 진정될지는 모르지만",
          "original_form": "코로나가 언제 진정될지는 모르지만",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100006913.1.18",
          "form": "꼭 가보고 싶네요!",
          "original_form": "꼭 가보고 싶네요!",
          "speaker_id": "1",
          "time": "20210822 00:15"
        },
        {
          "id": "MDRW2100006913.1.19",
          "form": "넹! 꼭 가보세요 ㅎㅎ",
          "original_form": "넹! 꼭 가보세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:17"
        },
        {
          "id": "MDRW2100006913.1.20",
          "form": "그날을 기다리면서 계획 세워봐야겠어요ㅎㅎ",
          "original_form": "그날을 기다리면서 계획 세워봐야겠어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:20"
        }
      ]
