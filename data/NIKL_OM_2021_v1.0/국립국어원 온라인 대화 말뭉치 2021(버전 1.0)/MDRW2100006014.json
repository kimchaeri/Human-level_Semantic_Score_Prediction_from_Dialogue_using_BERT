      "utterance": [
        {
          "id": "MDRW2100006014.1.1",
          "form": "엄마는 왜 카레라이스에 이상한걸 다 넣지",
          "original_form": "엄마는 왜 카레라이스에 이상한걸 다 넣지",
          "speaker_id": "1",
          "time": "20210907 19:00"
        },
        {
          "id": "MDRW2100006014.1.2",
          "form": "엄마는 이게 맛있나보다",
          "original_form": "엄마는 이게 맛있나보다",
          "speaker_id": "2",
          "time": "20210907 19:00"
        },
        {
          "id": "MDRW2100006014.1.3",
          "form": "그런가봄..",
          "original_form": "그런가봄..",
          "speaker_id": "1",
          "time": "20210907 19:00"
        },
        {
          "id": "MDRW2100006014.1.4",
          "form": "카레가아니라 잡탕이다",
          "original_form": "카레가아니라 잡탕이다",
          "speaker_id": "2",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100006014.1.5",
          "form": "맞다..ㅋㅋ 무슨 완두콩이랑 옥수수가 들어있지 ㅋ",
          "original_form": "맞다..ㅋㅋ 무슨 완두콩이랑 옥수수가 들어있지 ㅋ",
          "speaker_id": "1",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100006014.1.6",
          "form": "가지도 들어 있다",
          "original_form": "가지도 들어 있다",
          "speaker_id": "2",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100006014.1.7",
          "form": "ㅋㅋㅋ신기한 조합",
          "original_form": "ㅋㅋㅋ신기한 조합",
          "speaker_id": "1",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100006014.1.8",
          "form": "주는 대로 먹어야지",
          "original_form": "주는 대로 먹어야지",
          "speaker_id": "2",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100006014.1.9",
          "form": "그렇다 ㅎ",
          "original_form": "그렇다 ㅎ",
          "speaker_id": "1",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100006014.1.10",
          "form": "못 먹는 것보단 낫다",
          "original_form": "못 먹는 것보단 낫다",
          "speaker_id": "2",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100006014.1.11",
          "form": "맞당ㅋㅋㅋ 카레 맛은 나니까 됐다",
