      "utterance": [
        {
          "id": "MDRW2100001993.1.1",
          "form": "영화 좋아하시나요?",
          "original_form": "영화 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001993.1.2",
          "form": "네 요즘 많이안보는데 꽂힐때가 있어요",
          "original_form": "네 요즘 많이안보는데 꽂힐때가 있어요",
          "speaker_id": "2",
          "time": "20210819 22:40"
        },
        {
          "id": "MDRW2100001993.1.3",
          "form": "어떤영화 좋아하세요?",
          "original_form": "어떤영화 좋아하세요?",
          "speaker_id": "2",
          "time": "20210819 22:40"
        },
        {
          "id": "MDRW2100001993.1.4",
          "form": "저는 주로 액션물을 보는데 두루두루 잘 보는 편이에여",
          "original_form": "저는 주로 액션물을 보는데 두루두루 잘 보는 편이에여",
          "speaker_id": "1",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001993.1.5",
          "form": "name2님은 어떤 장르 좋아하시나요?",
          "original_form": "&name2&님은 어떤 장르 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210819 22:43"
        },
        {
          "id": "MDRW2100001993.1.6",
          "form": "전 sf나 판타지물이여 ㅎㅎ",
          "original_form": "전 sf나 판타지물이여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100001993.1.7",
          "form": "판타지랑 sf도 재밋죠",
          "original_form": "판타지랑 sf도 재밋죠",
          "speaker_id": "1",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100001993.1.8",
          "form": "젤 좋아하는 영화 잇으세요?",
          "original_form": "젤 좋아하는 영화 잇으세요?",
          "speaker_id": "1",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100001993.1.9",
          "form": "저 인셉션 반지의제왕이 각각최애들이에요",
          "original_form": "저 인셉션 반지의제왕이 각각최애들이에요",
          "speaker_id": "2",
          "time": "20210819 22:50"
        },
        {
          "id": "MDRW2100001993.1.10",
          "form": "반지의제왕은 진짜 영화너무 잘만들어서 아직도 가끔씩보구요ㅠ",
          "original_form": "반지의제왕은 진짜 영화너무 잘만들어서 아직도 가끔씩보구요ㅠ",
          "speaker_id": "2",
          "time": "20210819 22:51"
        },
        {
          "id": "MDRW2100001993.1.11",
          "form": "전 한번본건 잘안보는편인데도 가끔생각나서 봐요ㅜ name1님은요",
          "original_form": "전 한번본건 잘안보는편인데도 가끔생각나서 봐요ㅜ &name1&님은요",
          "speaker_id": "2",
          "time": "20210819 22:51"
        },
        {
          "id": "MDRW2100001993.1.12",
          "form": "아 여름님... 저두 아무리 재밋어도 재탕은 안하거든요....",
          "original_form": "아 여름님... 저두 아무리 재밋어도 재탕은 안하거든요....",
          "speaker_id": "1",
          "time": "20210819 22:52"
        },
        {
          "id": "MDRW2100001993.1.13",
          "form": "반지의 제왕은...cg기술이 별로 안좋은데도 넘 잘 만들엇어요",
          "original_form": "반지의 제왕은...cg기술이 별로 안좋은데도 넘 잘 만들엇어요",
          "speaker_id": "1",
          "time": "20210819 22:52"
        },
        {
          "id": "MDRW2100001993.1.14",
          "form": "아 그때 그시절이요! 근데 반지의 제왕은 넘 잘 만들어서 저도 tv에서 하면 가끔봐요",
          "original_form": "아 그때 그시절이요! 근데 반지의 제왕은 넘 잘 만들어서 저도 tv에서 하면 가끔봐요",
          "speaker_id": "1",
          "time": "20210819 22:53"
        },
        {
          "id": "MDRW2100001993.1.15",
          "form": "네 약간 기억속에 잊을만할때 생각날때가있어요 저도 진짜 한번본거안보는데 가끔보게되더라구여",
          "original_form": "네 약간 기억속에 잊을만할때 생각날때가있어요 저도 진짜 한번본거안보는데 가끔보게되더라구여",
          "speaker_id": "2",
          "time": "20210819 22:53"
        },
        {
          "id": "MDRW2100001993.1.16",
          "form": "전 반지의 제왕중에 왕의 귀환에 아라곤이 유령이랑 같이 손잡고 싸우는거 젤 좋아해요",
          "original_form": "전 반지의 제왕중에 왕의 귀환에 아라곤이 유령이랑 같이 손잡고 싸우는거 젤 좋아해요",
          "speaker_id": "1",
          "time": "20210819 22:54"
        },
        {
          "id": "MDRW2100001993.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:54"
        },
        {
          "id": "MDRW2100001993.1.18",
          "form": "어떤건지 똭알겠어요",
          "original_form": "어떤건지 똭알겠어요",
          "speaker_id": "2",
          "time": "20210819 22:54"
        },
        {
          "id": "MDRW2100001993.1.19",
          "form": "그춐ㅋㅋㅋㅋ",
          "original_form": "그춐ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:55"
        },
        {
          "id": "MDRW2100001993.1.20",
          "form": "그때그시절은 어떤영화에요?",
          "original_form": "그때그시절은 어떤영화에요?",
          "speaker_id": "2",
          "time": "20210819 22:55"
        },
        {
          "id": "MDRW2100001993.1.21",
          "form": "앗 아뇨 cg 잘 못만들던 그때 그시절이라 말한거엿어요",
          "original_form": "앗 아뇨 cg 잘 못만들던 그때 그시절이라 말한거엿어요",
          "speaker_id": "1",
          "time": "20210819 22:55"
        },
        {
          "id": "MDRW2100001993.1.22",
          "form": "아ㅋㅋㅋㅋㅋㅋ전 영화제목인줄알았어요",
          "original_form": "아ㅋㅋㅋㅋㅋㅋ전 영화제목인줄알았어요",
          "speaker_id": "2",
          "time": "20210819 22:56"
        },
        {
          "id": "MDRW2100001993.1.23",
          "form": "그 비슷한게있었던것같아서",
          "original_form": "그 비슷한게있었던것같아서",
          "speaker_id": "2",
          "time": "20210819 22:56"
        },
        {
          "id": "MDRW2100001993.1.24",
          "form": "저도 판타지 좋아하는 편이라 해리포터도 최애영화고 마블 시리즈도 좋아해요",
          "original_form": "저도 판타지 좋아하는 편이라 해리포터도 최애영화고 마블 시리즈도 좋아해요",
          "speaker_id": "1",
          "time": "20210819 22:56"
        },
        {
          "id": "MDRW2100001993.1.25",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ 그러니깐 뭔가 잇엇던거 같네요",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ 그러니깐 뭔가 잇엇던거 같네요",
          "speaker_id": "1",
          "time": "20210819 22:56"
        },
        {
          "id": "MDRW2100001993.1.26",
          "form": "해리포터도 넘 재밌져 \r\n음악나오면 거기로 빠지는것같아요",
          "original_form": "해리포터도 넘 재밌져 \r\n음악나오면 거기로 빠지는것같아요",
          "speaker_id": "2",
          "time": "20210819 22:57"
        }
      ]
