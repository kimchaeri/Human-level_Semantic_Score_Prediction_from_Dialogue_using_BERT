      "utterance": [
        {
          "id": "MDRW2100013960.1.1",
          "form": "가본 여행지중에 가장 좋았던 곳이 어디였어요?",
          "original_form": "가본 여행지중에 가장 좋았던 곳이 어디였어요?",
          "speaker_id": "1",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013960.1.2",
          "form": "제주도요ㅋㅋㅋ",
          "original_form": "제주도요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013960.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013960.1.4",
          "form": "저는 부산이요",
          "original_form": "저는 부산이요",
          "speaker_id": "1",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013960.1.5",
          "form": "제주도 좋았겠다..  저 안가본지 되게 오래됐어요",
          "original_form": "제주도 좋았겠다..  저 안가본지 되게 오래됐어요",
          "speaker_id": "1",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013960.1.6",
          "form": "저도 안가본지 진짜 오래됐어요....ㅋㅋ",
          "original_form": "저도 안가본지 진짜 오래됐어요....ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013960.1.7",
          "form": "지금쯤 많이 바뀌어있을 것 같네요",
          "original_form": "지금쯤 많이 바뀌어있을 것 같네요",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013960.1.8",
          "form": "그런것 같아요",
          "original_form": "그런것 같아요",
          "speaker_id": "1",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100013960.1.9",
          "form": "요즘 다들",
          "original_form": "요즘 다들",
          "speaker_id": "1",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100013960.1.10",
          "form": "제주도여행가잖아요",
          "original_form": "제주도여행가잖아요",
          "speaker_id": "1",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100013960.1.11",
          "form": "제주도 다시 가고싶네요ㅠ 거기서 밤에 하늘 쳐다보면 별이 그렇게 많았었는데",
          "original_form": "제주도 다시 가고싶네요ㅠ 거기서 밤에 하늘 쳐다보면 별이 그렇게 많았었는데",
          "speaker_id": "2",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100013960.1.12",
          "form": "제주도 한달살기 이런거 해보고싶어요",
          "original_form": "제주도 한달살기 이런거 해보고싶어요",
          "speaker_id": "1",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100013960.1.13",
          "form": "저도요ㅜ 다 내려놓고 가서 편하게 즐기고싶어요",
          "original_form": "저도요ㅜ 다 내려놓고 가서 편하게 즐기고싶어요",
          "speaker_id": "2",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100013960.1.14",
          "form": "코로나 이런거 생각안하고",
          "original_form": "코로나 이런거 생각안하고",
          "speaker_id": "1",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100013960.1.15",
          "form": "가보고싶은 여행지 있어요?",
          "original_form": "가보고싶은 여행지 있어요?",
          "speaker_id": "1",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100013960.1.16",
          "form": "일단 제주도랑요ㅋㅋㅋ해외는 스위스...?",
          "original_form": "일단 제주도랑요ㅋㅋㅋ해외는 스위스...?",
          "speaker_id": "2",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100013960.1.17",
          "form": "아 저도 스위스 안가봤는데",
          "original_form": "아 저도 스위스 안가봤는데",
          "speaker_id": "1",
          "time": "20210825 17:38"
        },
        {
          "id": "MDRW2100013960.1.18",
          "form": "제주변 유럽여행 다녀온 사람들의 반이상이 가장 기억에 남는 여행지로 스위스 꼽더라고요",
          "original_form": "제주변 유럽여행 다녀온 사람들의 반이상이 가장 기억에 남는 여행지로 스위스 꼽더라고요",
          "speaker_id": "1",
          "time": "20210825 17:38"
        },
        {
          "id": "MDRW2100013960.1.19",
          "form": "이상하게 스위스에 로망을 가지게 되네요ㅋㅋㅋ",
          "original_form": "이상하게 스위스에 로망을 가지게 되네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100013960.1.20",
          "form": "그럴만 한것 같아요 완전 그림같은 경관이잖아요",
          "original_form": "그럴만 한것 같아요 완전 그림같은 경관이잖아요",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100013960.1.21",
          "form": "저는 터키도 엄청 가보고싶고",
          "original_form": "저는 터키도 엄청 가보고싶고",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100013960.1.22",
          "form": "뉴욕도 가보고싶고",
          "original_form": "뉴욕도 가보고싶고",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100013960.1.23",
          "form": "태국도 안가봤어요",
          "original_form": "태국도 안가봤어요",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100013960.1.24",
          "form": "ㅋㅋㅋ태국에 가면 저는 똠얌꿍부터 찾아먹을거에요",
          "original_form": "ㅋㅋㅋ태국에 가면 저는 똠얌꿍부터 찾아먹을거에요",
          "speaker_id": "2",
          "time": "20210825 17:51"
        }
      ]
