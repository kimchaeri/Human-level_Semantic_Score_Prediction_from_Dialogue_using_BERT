      "utterance": [
        {
          "id": "MDRW2100022260.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210829 11:45"
        },
        {
          "id": "MDRW2100022260.1.2",
          "form": "지금요?",
          "original_form": "지금요?",
          "speaker_id": "2",
          "time": "20210829 11:53"
        },
        {
          "id": "MDRW2100022260.1.3",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210829 12:13"
        },
        {
          "id": "MDRW2100022260.1.4",
          "form": "아니오ㅋㅋㅋㅋ넷플릭스 보고 있았으요",
          "original_form": "아니오ㅋㅋㅋㅋ넷플릭스 보고 있았으요",
          "speaker_id": "2",
          "time": "20210829 12:30"
        },
        {
          "id": "MDRW2100022260.1.5",
          "form": "재미있는 거 보나요?",
          "original_form": "재미있는 거 보나요?",
          "speaker_id": "1",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100022260.1.6",
          "form": "상견니 보고 있어ㅆ어요",
          "original_form": "상견니 보고 있어ㅆ어요",
          "speaker_id": "2",
          "time": "20210829 19:34"
        },
        {
          "id": "MDRW2100022260.1.7",
          "form": "내용이?",
          "original_form": "내용이?",
          "speaker_id": "1",
          "time": "20210831 20:23"
        },
        {
          "id": "MDRW2100022260.1.8",
          "form": "타임슬립물이에요. 자기와 닮은 다른 사람의 몸으로 들어가요",
          "original_form": "타임슬립물이에요. 자기와 닮은 다른 사람의 몸으로 들어가요",
          "speaker_id": "2",
          "time": "20210902 09:36"
        },
        {
          "id": "MDRW2100022260.1.9",
          "form": "잼나겠네어",
          "original_form": "잼나겠네어",
          "speaker_id": "1",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100022260.1.10",
          "form": "초반에 바로 타임슬립하지 않아서 초반을 견뎌야되긴해요 ㅋㅋ",
          "original_form": "초반에 바로 타임슬립하지 않아서 초반을 견뎌야되긴해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100022260.1.11",
          "form": "집안일 하세요?",
          "original_form": "집안일 하세요?",
          "speaker_id": "2",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100022260.1.12",
          "form": "청소담당이요",
          "original_form": "청소담당이요",
          "speaker_id": "1",
          "time": "20210902 09:50"
        },
        {
          "id": "MDRW2100022260.1.13",
          "form": "오 매일 하세요?",
          "original_form": "오 매일 하세요?",
          "speaker_id": "2",
          "time": "20210902 09:52"
        },
        {
          "id": "MDRW2100022260.1.14",
          "form": "일주일동안 1번이요",
          "original_form": "일주일동안 1번이요",
          "speaker_id": "1",
          "time": "20210902 09:55"
        },
        {
          "id": "MDRW2100022260.1.15",
          "form": "그렇구나 가족구성원분들끼리 집안일 분배해서 하시나봐요",
          "original_form": "그렇구나 가족구성원분들끼리 집안일 분배해서 하시나봐요",
