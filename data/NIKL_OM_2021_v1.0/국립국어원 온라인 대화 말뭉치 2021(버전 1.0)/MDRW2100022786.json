      "utterance": [
        {
          "id": "MDRW2100022786.1.1",
          "form": "무슨 종교입니까?",
          "original_form": "무슨 종교입니까?",
          "speaker_id": "1",
          "time": "20210829 17:56"
        },
        {
          "id": "MDRW2100022786.1.2",
          "form": "저는 종교가 없습니다!ㅎㅎ",
          "original_form": "저는 종교가 없습니다!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100022786.1.3",
          "form": "저도 무교입니다",
          "original_form": "저도 무교입니다",
          "speaker_id": "1",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100022786.1.4",
          "form": "그럼 관심있는 종교는 있나요?",
          "original_form": "그럼 관심있는 종교는 있나요?",
          "speaker_id": "1",
          "time": "20210829 17:58"
        },
        {
          "id": "MDRW2100022786.1.5",
          "form": "아니요 딱히 ..",
          "original_form": "아니요 딱히 ..",
          "speaker_id": "2",
          "time": "20210829 18:19"
        },
        {
          "id": "MDRW2100022786.1.6",
          "form": "양가 어른들이  불교이시긴해요~^^",
          "original_form": "양가 어른들이  불교이시긴해요~^^",
          "speaker_id": "2",
          "time": "20210829 18:19"
        },
        {
          "id": "MDRW2100022786.1.7",
          "form": "저도 절에가면  기분이 폄암하5ㅣㄴ 하드라구요",
          "original_form": "저도 절에가면  기분이 폄암하5ㅣㄴ 하드라구요",
          "speaker_id": "1",
          "time": "20210829 18:22"
        },
        {
          "id": "MDRW2100022786.1.8",
          "form": "부처님오신날에 절앞에 현수막 걸려있더라구요",
          "original_form": "부처님오신날에 절앞에 현수막 걸려있더라구요",
          "speaker_id": "2",
          "time": "20210829 18:37"
        },
        {
          "id": "MDRW2100022786.1.9",
          "form": "성당에서 부처님오신날을 축하한다고",
          "original_form": "성당에서 부처님오신날을 축하한다고",
          "speaker_id": "2",
          "time": "20210829 18:37"
        },
        {
          "id": "MDRW2100022786.1.10",
          "form": "너무 보기좋았어요^^",
          "original_form": "너무 보기좋았어요^^",
          "speaker_id": "2",
          "time": "20210829 18:37"
        },
        {
          "id": "MDRW2100022786.1.11",
          "form": "님도 가끔 절에 따라 가시는군요",
          "original_form": "님도 가끔 절에 따라 가시는군요",
          "speaker_id": "1",
          "time": "20210829 18:38"
        },
        {
          "id": "MDRW2100022786.1.12",
          "form": "저도 관광지 같은데 가면 들러보는 ㅈㅇ도",
          "original_form": "저도 관광지 같은데 가면 들러보는 ㅈㅇ도",
          "speaker_id": "1",
          "time": "20210829 18:38"
        },
        {
          "id": "MDRW2100022786.1.13",
          "form": "그때는 그냥 지나가는길에 바깥에서 본거였어요 ㅋㅋ \r\n저도 가끔 그냥 들러보는정도에요^^",
          "original_form": "그때는 그냥 지나가는길에 바깥에서 본거였어요 ㅋㅋ \r\n저도 가끔 그냥 들러보는정도에요^^",
          "speaker_id": "2",
          "time": "20210829 18:39"
        },
        {
          "id": "MDRW2100022786.1.14",
          "form": "너무 배척하고 안좋게 말하는것도 보기싫더라구요",
          "original_form": "너무 배척하고 안좋게 말하는것도 보기싫더라구요",
          "speaker_id": "2",
          "time": "20210829 18:39"
        },
        {
          "id": "MDRW2100022786.1.15",
          "form": "그리고 코로나 사태로 이단들이 너무 싫어졌어요",
          "original_form": "그리고 코로나 사태로 이단들이 너무 싫어졌어요",
          "speaker_id": "2",
          "time": "20210829 18:39"
        },
        {
          "id": "MDRW2100022786.1.16",
          "form": "그러게요 좀 거부반응이 생기더라구요",
          "original_form": "그러게요 좀 거부반응이 생기더라구요",
          "speaker_id": "1",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100022786.1.17",
          "form": "다른종교는 나서서 조심해주는네",
          "original_form": "다른종교는 나서서 조심해주는네",
          "speaker_id": "2",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100022786.1.18",
          "form": "주는데",
          "original_form": "주는데",
          "speaker_id": "2",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100022786.1.19",
          "form": "나라에서 모이지말래도",
          "original_form": "나라에서 모이지말래도",
          "speaker_id": "2",
          "time": "20210829 18:41"
        },
        {
          "id": "MDRW2100022786.1.20",
          "form": "그렇게 모이더니만..",
          "original_form": "그렇게 모이더니만..",
          "speaker_id": "2",
          "time": "20210829 18:41"
        },
        {
          "id": "MDRW2100022786.1.21",
          "form": "끝까지 모이드만요 ㅠ휴..",
          "original_form": "끝까지 모이드만요 ㅠ휴..",
          "speaker_id": "1",
          "time": "20210829 18:41"
        },
        {
          "id": "MDRW2100022786.1.22",
          "form": "이제 코로나 이전의 삶은 없다고들 하잖아요",
          "original_form": "이제 코로나 이전의 삶은 없다고들 하잖아요",
