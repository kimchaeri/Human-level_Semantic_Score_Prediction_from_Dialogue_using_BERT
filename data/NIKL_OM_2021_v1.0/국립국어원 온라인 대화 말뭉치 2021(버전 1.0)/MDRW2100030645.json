      "utterance": [
        {
          "id": "MDRW2100030645.1.1",
          "form": "모르겠습니다",
          "original_form": "모르겠습니다",
          "speaker_id": "2",
          "time": "20210904 11:12"
        },
        {
          "id": "MDRW2100030645.1.2",
          "form": "오타모반이라고 몸에 나는 큰 점인데",
          "original_form": "오타모반이라고 몸에 나는 큰 점인데",
          "speaker_id": "1",
          "time": "20210904 11:13"
        },
        {
          "id": "MDRW2100030645.1.3",
          "form": "저는 몽골리안처럼 나 있거든요",
          "original_form": "저는 몽골리안처럼 나 있거든요",
          "speaker_id": "1",
          "time": "20210904 11:13"
        },
        {
          "id": "MDRW2100030645.1.4",
          "form": "고등학교 때 친구가 놀린 게 상처가 되우 지금 성형 중이에요",
          "original_form": "고등학교 때 친구가 놀린 게 상처가 되우 지금 성형 중이에요",
          "speaker_id": "1",
          "time": "20210904 11:13"
        },
        {
          "id": "MDRW2100030645.1.5",
          "form": "아아 그러시구나 ㅋㅋㅋㅋㅋ",
          "original_form": "아아 그러시구나 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 11:29"
        },
        {
          "id": "MDRW2100030645.1.6",
          "form": "몽고반점이 있으신가봐요",
          "original_form": "몽고반점이 있으신가봐요",
          "speaker_id": "2",
          "time": "20210904 11:30"
        },
        {
          "id": "MDRW2100030645.1.7",
          "form": "네 6번이나 레이저로 지져야 한대요",
          "original_form": "네 6번이나 레이저로 지져야 한대요",
          "speaker_id": "1",
          "time": "20210904 11:30"
        },
        {
          "id": "MDRW2100030645.1.8",
          "form": "친구가 당신에게 용기를 줬네요",
          "original_form": "친구가 당신에게 용기를 줬네요",
          "speaker_id": "2",
          "time": "20210904 11:31"
        },
        {
          "id": "MDRW2100030645.1.9",
          "form": "오늘이 4번째 지지는 날인데",
          "original_form": "오늘이 4번째 지지는 날인데",
          "speaker_id": "1",
          "time": "20210904 11:31"
        },
        {
          "id": "MDRW2100030645.1.10",
          "form": "의사 선생님이 그만 지져도 될 거 같대요",
          "original_form": "의사 선생님이 그만 지져도 될 거 같대요",
          "speaker_id": "1",
          "time": "20210904 11:31"
        },
        {
          "id": "MDRW2100030645.1.11",
          "form": "와 짱이네요 이제 없어진건가요",
          "original_form": "와 짱이네요 이제 없어진건가요",
          "speaker_id": "2",
          "time": "20210904 11:31"
        },
        {
          "id": "MDRW2100030645.1.12",
          "form": "그래서 돌아가는 길이에요",
          "original_form": "그래서 돌아가는 길이에요",
          "speaker_id": "1",
          "time": "20210904 11:31"
        },
        {
          "id": "MDRW2100030645.1.13",
          "form": "축하드려요",
          "original_form": "축하드려요",
          "speaker_id": "2",
          "time": "20210904 11:31"
        },
        {
          "id": "MDRW2100030645.1.14",
          "form": "넹 이제 방댕이 안 까도 돼요",
          "original_form": "넹 이제 방댕이 안 까도 돼요",
          "speaker_id": "1",
          "time": "20210904 11:31"
        },
        {
          "id": "MDRW2100030645.1.15",
          "form": "수치스러웠거든요",
          "original_form": "수치스러웠거든요",
          "speaker_id": "1",
          "time": "20210904 11:31"
        },
        {
          "id": "MDRW2100030645.1.16",
          "form": "돈 굳으셨네용",
          "original_form": "돈 굳으셨네용",
          "speaker_id": "2",
          "time": "20210904 11:32"
        },
        {
          "id": "MDRW2100030645.1.17",
          "form": "네네 아낀 돈으로 눈썹 왁싱하러 가는 길이에요",
          "original_form": "네네 아낀 돈으로 눈썹 왁싱하러 가는 길이에요",
          "speaker_id": "1",
          "time": "20210904 11:32"
        },
        {
          "id": "MDRW2100030645.1.18",
          "form": "미용에 관심이 참 많으시네요",
          "original_form": "미용에 관심이 참 많으시네요",
          "speaker_id": "2",
          "time": "20210904 11:32"
        },
        {
          "id": "MDRW2100030645.1.19",
          "form": "외모지상주의자라서요",
          "original_form": "외모지상주의자라서요",
          "speaker_id": "1",
          "time": "20210904 11:32"
        }
      ]
