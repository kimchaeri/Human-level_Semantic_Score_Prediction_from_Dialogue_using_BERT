      "utterance": [
        {
          "id": "MDRW2100012444.1.1",
          "form": "연애 어떻게 생각하시나요?",
          "original_form": "연애 어떻게 생각하시나요?",
          "speaker_id": "1",
          "time": "20210824 22:03"
        },
        {
          "id": "MDRW2100012444.1.2",
          "form": "연애 주위에서 하는거 보기엔",
          "original_form": "연애 주위에서 하는거 보기엔",
          "speaker_id": "2",
          "time": "20210824 22:03"
        },
        {
          "id": "MDRW2100012444.1.3",
          "form": "워낙 버라이어티하긴 해서",
          "original_form": "워낙 버라이어티하긴 해서",
          "speaker_id": "2",
          "time": "20210824 22:03"
        },
        {
          "id": "MDRW2100012444.1.4",
          "form": "사랑이란 힘들구나 싶어요",
          "original_form": "사랑이란 힘들구나 싶어요",
          "speaker_id": "2",
          "time": "20210824 22:04"
        },
        {
          "id": "MDRW2100012444.1.5",
          "form": "아아... 저도 사랑은 현실이구나 싶더라고요",
          "original_form": "아아... 저도 사랑은 현실이구나 싶더라고요",
          "speaker_id": "1",
          "time": "20210824 22:05"
        },
        {
          "id": "MDRW2100012444.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 22:06"
        },
        {
          "id": "MDRW2100012444.1.7",
          "form": "친구들 울고 웃고",
          "original_form": "친구들 울고 웃고",
          "speaker_id": "2",
          "time": "20210824 22:06"
        },
        {
          "id": "MDRW2100012444.1.8",
          "form": "전 막 그렇게 해본적이 없어서",
          "original_form": "전 막 그렇게 해본적이 없어서",
          "speaker_id": "2",
          "time": "20210824 22:07"
        },
        {
          "id": "MDRW2100012444.1.9",
          "form": "들으면서 신기해해요",
          "original_form": "들으면서 신기해해요",
          "speaker_id": "2",
          "time": "20210824 22:07"
        },
        {
          "id": "MDRW2100012444.1.10",
          "form": "예쁘게 사귀는 커플 보면 또 몽글해지다가",
          "original_form": "예쁘게 사귀는 커플 보면 또 몽글해지다가",
          "speaker_id": "2",
          "time": "20210824 22:07"
        },
        {
          "id": "MDRW2100012444.1.11",
          "form": "맨날 싸우는 커플보면",
          "original_form": "맨날 싸우는 커플보면",
          "speaker_id": "2",
          "time": "20210824 22:07"
        },
        {
          "id": "MDRW2100012444.1.12",
          "form": "아 힘들구나 이래요",
          "original_form": "아 힘들구나 이래요",
          "speaker_id": "2",
          "time": "20210824 22:07"
        },
        {
          "id": "MDRW2100012444.1.13",
          "form": "그래서 그런지 짝사랑 경험만 많아요",
          "original_form": "그래서 그런지 짝사랑 경험만 많아요",
          "speaker_id": "2",
          "time": "20210824 22:11"
        },
        {
          "id": "MDRW2100012444.1.14",
          "form": "로맨스는 미디어에서만 국한되는 거 같기도하고...\n저는 안해봐서 모르겠네요",
          "original_form": "로맨스는 미디어에서만 국한되는 거 같기도하고...\n저는 안해봐서 모르겠네요",
          "speaker_id": "1",
          "time": "20210824 22:11"
        },
        {
          "id": "MDRW2100012444.1.15",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:11"
        },
        {
          "id": "MDRW2100012444.1.16",
          "form": "맞아요 미디어에서 워낙 많이 나와서",
          "original_form": "맞아요 미디어에서 워낙 많이 나와서",
          "speaker_id": "2",
          "time": "20210824 22:11"
        },
        {
          "id": "MDRW2100012444.1.17",
          "form": "그리고 요즘 느끼는건데",
          "original_form": "그리고 요즘 느끼는건데",
          "speaker_id": "2",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012444.1.18",
          "form": "제가 눈이 높은건지 뭔지",
          "original_form": "제가 눈이 높은건지 뭔지",
          "speaker_id": "2",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012444.1.19",
          "form": "좀 다들 그렇더라구요",
          "original_form": "좀 다들 그렇더라구요",
          "speaker_id": "2",
          "time": "20210824 22:13"
        },
        {
          "id": "MDRW2100012444.1.20",
          "form": "아...  저는 연애에 회의적인거 같기도해요",
          "original_form": "아...  저는 연애에 회의적인거 같기도해요",
          "speaker_id": "1",
          "time": "20210824 22:13"
        },
        {
          "id": "MDRW2100012444.1.21",
          "form": "저도 그렇게 되는 것같아요",
          "original_form": "저도 그렇게 되는 것같아요",
          "speaker_id": "2",
          "time": "20210824 22:14"
        },
        {
          "id": "MDRW2100012444.1.22",
          "form": "어느순간 안하고 살고 있어서",
          "original_form": "어느순간 안하고 살고 있어서",
          "speaker_id": "2",
          "time": "20210824 22:14"
        },
        {
          "id": "MDRW2100012444.1.23",
          "form": "현실이 바쁘기도 하구요",
          "original_form": "현실이 바쁘기도 하구요",
          "speaker_id": "2",
          "time": "20210824 22:14"
        },
        {
          "id": "MDRW2100012444.1.24",
          "form": "그쵸 아무래도 현실이... 스스로 챙기기도 급급한 현실이죠",
          "original_form": "그쵸 아무래도 현실이... 스스로 챙기기도 급급한 현실이죠",
          "speaker_id": "1",
          "time": "20210824 22:15"
        },
        {
          "id": "MDRW2100012444.1.25",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012444.1.26",
          "form": "또 코로나때문에 활동도 제한적이고",
          "original_form": "또 코로나때문에 활동도 제한적이고",
          "speaker_id": "2",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012444.1.27",
          "form": "누구를 사귀기 위험하기도 하구요",
          "original_form": "누구를 사귀기 위험하기도 하구요",
          "speaker_id": "2",
          "time": "20210824 22:22"
        },
        {
          "id": "MDRW2100012444.1.28",
          "form": "세상이 험해서",
          "original_form": "세상이 험해서",
          "speaker_id": "2",
          "time": "20210824 22:22"
        },
        {
          "id": "MDRW2100012444.1.29",
          "form": "저는 그리고 지금 관계에 충분히 만족해요.",
          "original_form": "저는 그리고 지금 관계에 충분히 만족해요.",
          "speaker_id": "1",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100012444.1.30",
          "form": "아 저도요",
          "original_form": "아 저도요",
          "speaker_id": "2",
          "time": "20210825 18:58"
        },
        {
          "id": "MDRW2100012444.1.31",
          "form": "뭐 주위 사람들은",
          "original_form": "뭐 주위 사람들은",
          "speaker_id": "2",
          "time": "20210825 18:58"
        },
        {
          "id": "MDRW2100012444.1.32",
          "form": "막상 누구 좋아하면 달라진다하는데",
          "original_form": "막상 누구 좋아하면 달라진다하는데",
          "speaker_id": "2",
          "time": "20210825 18:58"
        },
        {
          "id": "MDRW2100012444.1.33",
          "form": "지금 없으니까요",
          "original_form": "지금 없으니까요",
          "speaker_id": "2",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100012444.1.34",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100012444.1.35",
          "form": "허허...  사람만나면 쉽게 피곤해지는 스타일이라..\n달라진다는게 저한테는 생소하네요",
          "original_form": "허허...  사람만나면 쉽게 피곤해지는 스타일이라..\n달라진다는게 저한테는 생소하네요",
          "speaker_id": "1",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100012444.1.36",
          "form": "저도 막상 놀땐 잘 노는데 집에오면 엄청 피곤한 스타일이에요",
          "original_form": "저도 막상 놀땐 잘 노는데 집에오면 엄청 피곤한 스타일이에요",
          "speaker_id": "2",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100012444.1.37",
          "form": "연애는 또 꾸준하게 연락해야하는데",
          "original_form": "연애는 또 꾸준하게 연락해야하는데",
          "speaker_id": "2",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100012444.1.38",
          "form": "그게 좀 귀찮고 부담스럽더라구요",
          "original_form": "그게 좀 귀찮고 부담스럽더라구요",
          "speaker_id": "2",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100012444.1.39",
          "form": "아... 꾸준히 연락하는건 저도 부담스럽네요",
          "original_form": "아... 꾸준히 연락하는건 저도 부담스럽네요",
          "speaker_id": "1",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100012444.1.40",
          "form": "맞아요 어떤 친구는 1,2시간마다 연락하던데",
          "original_form": "맞아요 어떤 친구는 1,2시간마다 연락하던데",
          "speaker_id": "2",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100012444.1.41",
          "form": "솔직히 전 이해안가요",
          "original_form": "솔직히 전 이해안가요",
          "speaker_id": "2",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100012444.1.42",
          "form": "저는 친한 친구랑도 3일에 한번씩 하는 편인데...1시~2시간마다 연락은 좀... 기피하게 될거 같아요",
          "original_form": "저는 친한 친구랑도 3일에 한번씩 하는 편인데...1시~2시간마다 연락은 좀... 기피하게 될거 같아요",
          "speaker_id": "1",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100012444.1.43",
          "form": "누가 그 기준을 잡게된건지",
          "original_form": "누가 그 기준을 잡게된건지",
          "speaker_id": "2",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100012444.1.44",
          "form": "솔직히 헤어지는 것도 걱정되구요",
          "original_form": "솔직히 헤어지는 것도 걱정되구요",
          "speaker_id": "2",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100012444.1.45",
          "form": "워낙 세상이 흉흉하다보니까요",
          "original_form": "워낙 세상이 흉흉하다보니까요",
          "speaker_id": "2",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100012444.1.46",
          "form": "그러게요... 안전이별을 걱정하게 되네요",
          "original_form": "그러게요... 안전이별을 걱정하게 되네요",
          "speaker_id": "1",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100012444.1.47",
          "form": "맞아요 그리고 전",
          "original_form": "맞아요 그리고 전",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100012444.1.48",
          "form": "그 성병도 좀 불안해서",
          "original_form": "그 성병도 좀 불안해서",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100012444.1.49",
          "form": "연애 꺼리게돼요",
          "original_form": "연애 꺼리게돼요",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100012444.1.50",
          "form": "그렇군요...",
          "original_form": "그렇군요...",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100012444.1.51",
          "form": "네 아무래도 한번 걸리면 돌이킬 수 없으니까요",
          "original_form": "네 아무래도 한번 걸리면 돌이킬 수 없으니까요",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100012444.1.52",
          "form": "사람들이 가다실 많이 맞았으면 좋겠어요",
          "original_form": "사람들이 가다실 많이 맞았으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210825 22:50"
        }
      ]
