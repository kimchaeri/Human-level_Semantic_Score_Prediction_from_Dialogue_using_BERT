        {
          "id": "MDRW2100027903.1.1",
          "form": "안녕하세요. 코로나백신 맞으셨어요?",
          "original_form": "안녕하세요. 코로나백신 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210902 16:00"
        },
        {
          "id": "MDRW2100027903.1.2",
          "form": "안녕하세요. 그제 1차 맞았어요\r\nname1은 맞으셨어요?",
          "original_form": "안녕하세요. 그제 1차 맞았어요\r\n&name1&은 맞으셨어요?",
          "speaker_id": "2",
          "time": "20210902 16:04"
        },
        {
          "id": "MDRW2100027903.1.3",
          "form": "오 며칠전이요?",
          "original_form": "오 며칠전이요?",
          "speaker_id": "1",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100027903.1.4",
          "form": "저는 보름전에 맞았어요",
          "original_form": "저는 보름전에 맞았어요",
          "speaker_id": "1",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100027903.1.5",
          "form": "모더나인데 어떤거 맞으셨어요?",
          "original_form": "모더나인데 어떤거 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100027903.1.6",
          "form": "저는 화이자요",
          "original_form": "저는 화이자요",
          "speaker_id": "2",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100027903.1.7",
          "form": "모더나였는데 변경됐어요",
          "original_form": "모더나였는데 변경됐어요",
          "speaker_id": "2",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100027903.1.8",
          "form": "몸 상태  괜찮으세요?",
          "original_form": "몸 상태  괜찮으세요?",
          "speaker_id": "2",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100027903.1.9",
          "form": "아. 저는 이틀정도 팔이 묵직했어요",
          "original_form": "아. 저는 이틀정도 팔이 묵직했어요",
          "speaker_id": "1",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100027903.1.10",
          "form": "근육통 있으시겠어요",
          "original_form": "근육통 있으시겠어요",
          "speaker_id": "1",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100027903.1.11",
          "form": "저는 일주일 뒤 증상이 왔어요",
          "original_form": "저는 일주일 뒤 증상이 왔어요",
          "speaker_id": "1",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100027903.1.12",
          "form": "근육통은 금방 가셨는데 너무 졸려요",
          "original_form": "근육통은 금방 가셨는데 너무 졸려요",
          "speaker_id": "2",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027903.1.13",
          "form": "계속 몸이 나른하더라구요",
          "original_form": "계속 몸이 나른하더라구요",
          "speaker_id": "2",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027903.1.14",
          "form": "아 푹 쉬셔야해요^^",
          "original_form": "아 푹 쉬셔야해요^^",
          "speaker_id": "1",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027903.1.15",
          "form": "백신이 잘 활동하도록요",
          "original_form": "백신이 잘 활동하도록요",
          "speaker_id": "1",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027903.1.16",
          "form": "저는 겨드랑이가 아팠어요",
          "original_form": "저는 겨드랑이가 아팠어요",
          "speaker_id": "1",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027903.1.17",
          "form": "헉 겨드랑이가 아프기도 하는군요",
          "original_form": "헉 겨드랑이가 아프기도 하는군요",
          "speaker_id": "2",
          "time": "20210902 16:10"
        },
        {
          "id": "MDRW2100027903.1.18",
          "form": "그럼 지금 목 아픈것도 백신때문일 수 있겠어요",
          "original_form": "그럼 지금 목 아픈것도 백신때문일 수 있겠어요",
          "speaker_id": "2",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027903.1.19",
          "form": "여자는 림프절이 붓는것이",
          "original_form": "여자는 림프절이 붓는것이",
          "speaker_id": "1",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027903.1.20",
          "form": "흔한 부작용이랭ᆢ",
          "original_form": "흔한 부작용이랭ᆢ",
          "speaker_id": "1",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027903.1.21",
          "form": "잠 잘못 잤다고 생각했어요",
          "original_form": "잠 잘못 잤다고 생각했어요",
          "speaker_id": "2",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027903.1.22",
          "form": "겨드랑이가 아프더라구요 그래서",
          "original_form": "겨드랑이가 아프더라구요 그래서",
          "speaker_id": "1",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100027903.1.23",
          "form": "아하 림프가 붓는군요",
          "original_form": "아하 림프가 붓는군요",
          "speaker_id": "2",
          "time": "20210902 16:11"
