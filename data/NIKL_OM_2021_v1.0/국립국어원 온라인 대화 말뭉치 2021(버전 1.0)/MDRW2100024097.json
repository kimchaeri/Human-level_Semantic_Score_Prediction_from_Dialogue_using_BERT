      "utterance": [
        {
          "id": "MDRW2100024097.1.1",
          "form": "오늘 뭐먹었어",
          "original_form": "오늘 뭐먹었어",
          "speaker_id": "1",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100024097.1.2",
          "form": "점심에는 그냥 평소에 먹던대로 먹었어요",
          "original_form": "점심에는 그냥 평소에 먹던대로 먹었어요",
          "speaker_id": "2",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100024097.1.3",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100024097.1.4",
          "form": "점심에 돼지고기 먹었어",
          "original_form": "점심에 돼지고기 먹었어",
          "speaker_id": "1",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100024097.1.5",
          "form": "오늘 고기 나오셨네요",
          "original_form": "오늘 고기 나오셨네요",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100024097.1.6",
          "form": "저녁엔 뭐드셨어요",
          "original_form": "저녁엔 뭐드셨어요",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100024097.1.7",
          "form": "저녁은 부침개 묵 나왔어",
          "original_form": "저녁은 부침개 묵 나왔어",
          "speaker_id": "1",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100024097.1.8",
          "form": "어제 부침개먹었는데 또 나왔네",
          "original_form": "어제 부침개먹었는데 또 나왔네",
          "speaker_id": "2",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100024097.1.9",
          "form": "고추장국 나왔는데 된장찌개보다 낫더라",
          "original_form": "고추장국 나왔는데 된장찌개보다 낫더라",
          "speaker_id": "1",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100024097.1.10",
          "form": "된장찌개 많이 나와서 싫어하시잖아요",
          "original_form": "된장찌개 많이 나와서 싫어하시잖아요",
          "speaker_id": "2",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100024097.1.11",
          "form": "너무 자주나와",
          "original_form": "너무 자주나와",
          "speaker_id": "1",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100024097.1.12",
          "form": "넌 저녁에 뭐 먹었어",
          "original_form": "넌 저녁에 뭐 먹었어",
          "speaker_id": "1",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100024097.1.13",
          "form": "초밥 사먹었어요",
          "original_form": "초밥 사먹었어요",
          "speaker_id": "2",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100024097.1.14",
          "form": "맛있는거로 먹지",
          "original_form": "맛있는거로 먹지",
          "speaker_id": "1",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100024097.1.15",
          "form": "맛있었는데 좀 짰어요",
          "original_form": "맛있었는데 좀 짰어요",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100024097.1.16",
          "form": "많이먹지",
          "original_form": "많이먹지",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100024097.1.17",
          "form": "많이는 먹었어요 그래서 물도 많이 먹었어요",
          "original_form": "많이는 먹었어요 그래서 물도 많이 먹었어요",
