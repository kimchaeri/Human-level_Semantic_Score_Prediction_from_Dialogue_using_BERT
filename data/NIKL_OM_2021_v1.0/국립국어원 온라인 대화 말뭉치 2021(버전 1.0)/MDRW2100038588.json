        },
        {
          "id": "MDRW2100038588.1.2",
          "form": "아니용 name1님 연애하시나영",
          "original_form": "아니용 &name1&님 연애하시나영",
          "speaker_id": "2",
          "time": "20210902 16:26"
        },
        {
          "id": "MDRW2100038588.1.3",
          "form": "안해여",
          "original_form": "안해여",
          "speaker_id": "1",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100038588.1.4",
          "form": "왜안해여",
          "original_form": "왜안해여",
          "speaker_id": "1",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100038588.1.5",
          "form": "ㅋㅋ맘에드는 사람이 없어용",
          "original_form": "ㅋㅋ맘에드는 사람이 없어용",
          "speaker_id": "2",
          "time": "20210902 16:27"
        },
        {
          "id": "MDRW2100038588.1.6",
          "form": "그죠 왜없을까요",
          "original_form": "그죠 왜없을까요",
          "speaker_id": "1",
          "time": "20210902 16:29"
        },
        {
          "id": "MDRW2100038588.1.7",
          "form": "얼굴 vs성격",
          "original_form": "얼굴 vs성격",
          "speaker_id": "1",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100038588.1.8",
          "form": "ㅋㅋㅋㅋㅋ둘다는안되나요? 어짜피 얼굴도 잘생아니고 진짜 보통만되면되는데...",
          "original_form": "ㅋㅋㅋㅋㅋ둘다는안되나요? 어짜피 얼굴도 잘생아니고 진짜 보통만되면되는데...",
          "speaker_id": "2",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100038588.1.9",
          "form": "아... 그게 어렵거든요",
          "original_form": "아... 그게 어렵거든요",
          "speaker_id": "1",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100038588.1.10",
          "form": "보통이라...보통의기준이머요",
          "original_form": "보통이라...보통의기준이머요",
          "speaker_id": "1",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100038588.1.11",
          "form": "반반 나눠서 메디안 ??",
          "original_form": "반반 나눠서 메디안 ??",
          "speaker_id": "1",
          "time": "20210902 16:31"
        },
        {
          "id": "MDRW2100038588.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:31"
        },
        {
          "id": "MDRW2100038588.1.13",
          "form": "하긴 근데 보통 기준에 적합하는 사람도 별로업ㄹ네요",
          "original_form": "하긴 근데 보통 기준에 적합하는 사람도 별로업ㄹ네요",
          "speaker_id": "2",
          "time": "20210902 16:31"
        },
        {
          "id": "MDRW2100038588.1.14",
          "form": "ㅋㅋㅋㅋ퍼센트로 얘기해주세요",
          "original_form": "ㅋㅋㅋㅋ퍼센트로 얘기해주세요",
          "speaker_id": "1",
          "time": "20210902 16:32"
        },
        {
          "id": "MDRW2100038588.1.15",
          "form": "제가 생각한건 메디안이에요 ㅋㅋㅋㅋ",
          "original_form": "제가 생각한건 메디안이에요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100038588.1.16",
          "form": "그런가...그정도면 대나?",
          "original_form": "그런가...그정도면 대나?",
          "speaker_id": "1",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100038588.1.17",
          "form": "퍼샌트로 표현하기 힘든것같아요",
          "original_form": "퍼샌트로 표현하기 힘든것같아요",
          "speaker_id": "2",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100038588.1.18",
          "form": "남들기준엔 못생겻는데 내눈에 잘생 이런게있자나요 ㅋㅋㄴㄴ",
          "original_form": "남들기준엔 못생겻는데 내눈에 잘생 이런게있자나요 ㅋㅋㄴㄴ",
          "speaker_id": "2",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100038588.1.19",
          "form": "ㅋㅋㅋㅋ님이조아하는 남자",
          "original_form": "ㅋㅋㅋㅋ님이조아하는 남자",
          "speaker_id": "1",
          "time": "20210902 16:36"
        },
        {
          "id": "MDRW2100038588.1.20",
          "form": "친규들이 못생이라한덕 잇나요",
          "original_form": "친규들이 못생이라한덕 잇나요",
          "speaker_id": "1",
          "time": "20210902 16:36"
        },
        {
          "id": "MDRW2100038588.1.21",
          "form": "적",
          "original_form": "적",
          "speaker_id": "1",
          "time": "20210902 16:36"
        },
        {
          "id": "MDRW2100038588.1.22",
          "form": "ㅋㅋㅋㅋ흐음 아니요 없엇죠 저 좀 눈마니높아여",
          "original_form": "ㅋㅋㅋㅋ흐음 아니요 없엇죠 저 좀 눈마니높아여",
