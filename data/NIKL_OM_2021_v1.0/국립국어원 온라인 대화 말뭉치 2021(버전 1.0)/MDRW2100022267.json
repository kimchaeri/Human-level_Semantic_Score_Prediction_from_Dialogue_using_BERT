      "utterance": [
        {
          "id": "MDRW2100022267.1.1",
          "form": "아하ㅋㅋㅋ 이제 같이 살면 데이트 비용 줄이고 같이 있고 좋져!",
          "original_form": "아하ㅋㅋㅋ 이제 같이 살면 데이트 비용 줄이고 같이 있고 좋져!",
          "speaker_id": "2",
          "time": "20210829 11:50"
        },
        {
          "id": "MDRW2100022267.1.2",
          "form": "그건 좋져",
          "original_form": "그건 좋져",
          "speaker_id": "1",
          "time": "20210829 11:50"
        },
        {
          "id": "MDRW2100022267.1.3",
          "form": "둘이 같이벌어서 빨리 빚갚고 ㅋㅋㅋㅋ",
          "original_form": "둘이 같이벌어서 빨리 빚갚고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:50"
        },
        {
          "id": "MDRW2100022267.1.4",
          "form": "네ㅋㅋㅋㅋ 취미 생활도 같이 하는 편인가요?",
          "original_form": "네ㅋㅋㅋㅋ 취미 생활도 같이 하는 편인가요?",
          "speaker_id": "2",
          "time": "20210829 11:50"
        },
        {
          "id": "MDRW2100022267.1.5",
          "form": "여친 필라테스빼곤 거의 같이하죠",
          "original_form": "여친 필라테스빼곤 거의 같이하죠",
          "speaker_id": "1",
          "time": "20210829 11:51"
        },
        {
          "id": "MDRW2100022267.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:51"
        },
        {
          "id": "MDRW2100022267.1.7",
          "form": "오ㅋㅋㅋㅋ 저도 필라테스 하는데 남친이 약간 필라테스에 관심 보여요",
          "original_form": "오ㅋㅋㅋㅋ 저도 필라테스 하는데 남친이 약간 필라테스에 관심 보여요",
          "speaker_id": "2",
          "time": "20210829 11:51"
        },
        {
          "id": "MDRW2100022267.1.8",
          "form": "같이 하자 하면 넘어 올것도 같은",
          "original_form": "같이 하자 하면 넘어 올것도 같은",
          "speaker_id": "2",
          "time": "20210829 11:51"
        },
        {
          "id": "MDRW2100022267.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:52"
        },
        {
          "id": "MDRW2100022267.1.10",
          "form": "헛",
          "original_form": "헛",
          "speaker_id": "1",
          "time": "20210829 11:52"
        },
        {
          "id": "MDRW2100022267.1.11",
          "form": "쉽지 않을텐데",
          "original_form": "쉽지 않을텐데",
          "speaker_id": "1",
          "time": "20210829 11:52"
        },
        {
          "id": "MDRW2100022267.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:52"
        },
        {
          "id": "MDRW2100022267.1.13",
          "form": "그 외엔 아직 같이 할 관심사는 없어서",
          "original_form": "그 외엔 아직 같이 할 관심사는 없어서",
          "speaker_id": "2",
          "time": "20210829 11:52"
        },
        {
          "id": "MDRW2100022267.1.14",
          "form": "찾아봐야해요",
          "original_form": "찾아봐야해요",
          "speaker_id": "2",
          "time": "20210829 11:52"
        },
        {
          "id": "MDRW2100022267.1.15",
          "form": "아직 일주일이라 그렇지만..",
          "original_form": "아직 일주일이라 그렇지만..",
          "speaker_id": "2",
          "time": "20210829 11:53"
        },
        {
          "id": "MDRW2100022267.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:53"
        },
        {
          "id": "MDRW2100022267.1.17",
          "form": "천천히 찾으면 되죠",
          "original_form": "천천히 찾으면 되죠",
          "speaker_id": "1",
          "time": "20210829 11:53"
        },
        {
          "id": "MDRW2100022267.1.18",
          "form": "오늘 동물원데이트 하신다구여?",
          "original_form": "오늘 동물원데이트 하신다구여?",
          "speaker_id": "1",
          "time": "20210829 11:53"
        },
        {
          "id": "MDRW2100022267.1.19",
          "form": "네ㅋㅋㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:53"
        },
        {
          "id": "MDRW2100022267.1.20",
          "form": "데이트필수코스",
          "original_form": "데이트필수코스",
          "speaker_id": "1",
          "time": "20210829 11:54"
        },
        {
          "id": "MDRW2100022267.1.21",
          "form": "(1~100일)",
          "original_form": "(1~100일)",
          "speaker_id": "1",
          "time": "20210829 11:54"
        },
        {
          "id": "MDRW2100022267.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:54"
        },
        {
          "id": "MDRW2100022267.1.23",
          "form": "마자욬ㅋㅌㅌㅋㅌ초반 아니면 못가는 곳 ㅋㅋㅋㅋ",
          "original_form": "마자욬ㅋㅌㅌㅋㅌ초반 아니면 못가는 곳 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:54"
        },
        {
          "id": "MDRW2100022267.1.24",
          "form": "큰곳인가요?",
          "original_form": "큰곳인가요?",
          "speaker_id": "1",
          "time": "20210829 11:54"
        },
        {
          "id": "MDRW2100022267.1.25",
          "form": "진짜 동물원 한 몇년만에 가는 느낌",
          "original_form": "진짜 동물원 한 몇년만에 가는 느낌",
          "speaker_id": "2",
          "time": "20210829 11:54"
        },
        {
          "id": "MDRW2100022267.1.26",
          "form": "실내동물원인데 한 1300평 한대요",
          "original_form": "실내동물원인데 한 1300평 한대요",
          "speaker_id": "2",
          "time": "20210829 11:55"
        },
        {
          "id": "MDRW2100022267.1.27",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210829 11:55"
        },
        {
          "id": "MDRW2100022267.1.28",
          "form": "저도 아직 안가본 곳이라",
          "original_form": "저도 아직 안가본 곳이라",
          "speaker_id": "2",
          "time": "20210829 11:55"
        },
        {
          "id": "MDRW2100022267.1.29",
          "form": "첨가보시는구나",
          "original_form": "첨가보시는구나",
          "speaker_id": "1",
          "time": "20210829 11:55"
        },
        {
          "id": "MDRW2100022267.1.30",
          "form": "네네 둘다 안가봐서",
          "original_form": "네네 둘다 안가봐서",
          "speaker_id": "2",
          "time": "20210829 11:55"
        },
        {
          "id": "MDRW2100022267.1.31",
          "form": "나름 기대하고 있어요 ㅋㅋㅋㅋ",
          "original_form": "나름 기대하고 있어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:55"
        },
        {
          "id": "MDRW2100022267.1.32",
          "form": "근데 뭐 둘이 같이하면 뭐든 다 좋자나여 ㅋㅋ",
          "original_form": "근데 뭐 둘이 같이하면 뭐든 다 좋자나여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:56"
        },
        {
          "id": "MDRW2100022267.1.33",
          "form": "아직은 손잡는 것도 어색하고 부끄러워서",
          "original_form": "아직은 손잡는 것도 어색하고 부끄러워서",
          "speaker_id": "2",
          "time": "20210829 11:56"
        },
        {
          "id": "MDRW2100022267.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:56"
        },
        {
          "id": "MDRW2100022267.1.35",
          "form": "어머나",
          "original_form": "어머나",
          "speaker_id": "1",
