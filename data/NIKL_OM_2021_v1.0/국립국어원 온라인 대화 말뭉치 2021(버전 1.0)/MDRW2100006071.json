      "utterance": [
        {
          "id": "MDRW2100006071.1.1",
          "form": "종교있으신가요",
          "original_form": "종교있으신가요",
          "speaker_id": "1",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100006071.1.2",
          "form": "아니요 없어요.",
          "original_form": "아니요 없어요.",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100006071.1.3",
          "form": "있으세요?",
          "original_form": "있으세요?",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100006071.1.4",
          "form": "네 저는 기독교요",
          "original_form": "네 저는 기독교요",
          "speaker_id": "1",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100006071.1.5",
          "form": "그럼 교회다니시겠네요",
          "original_form": "그럼 교회다니시겠네요",
          "speaker_id": "2",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100006071.1.6",
          "form": "네 모태신앙이라고 아실까요?",
          "original_form": "네 모태신앙이라고 아실까요?",
          "speaker_id": "1",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100006071.1.7",
          "form": "아니요. 처음들어요ㅎㅎ",
          "original_form": "아니요. 처음들어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100006071.1.8",
          "form": "종교쪽은 아는게 없거든요",
          "original_form": "종교쪽은 아는게 없거든요",
          "speaker_id": "2",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100006071.1.9",
          "form": "아 대부분 아셔서 여쭤봤어요 뱃속부터 기독교라는...ㅎㅎ",
          "original_form": "아 대부분 아셔서 여쭤봤어요 뱃속부터 기독교라는...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100006071.1.10",
          "form": "아ㅋㅋㅋ 저는 친가가 불교쪽이긴 한데",
          "original_form": "아ㅋㅋㅋ 저는 친가가 불교쪽이긴 한데",
          "speaker_id": "2",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100006071.1.11",
          "form": "별 얘기도 하신적 없고 딱히 독실한? 건 아니어서요ㅋㅋㅋ",
          "original_form": "별 얘기도 하신적 없고 딱히 독실한? 건 아니어서요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100006071.1.12",
          "form": "그러시군요 부모님이 절에 다니시는거죠?",
          "original_form": "그러시군요 부모님이 절에 다니시는거죠?",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100006071.1.13",
          "form": "할머니가 다니셔요. 부모님이 가는건 잘 본 적 없구요ㅋㅋ",
          "original_form": "할머니가 다니셔요. 부모님이 가는건 잘 본 적 없구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100006071.1.14",
          "form": "아 그러시군요 불교는.가족들에게 강권하는 분위기는 없는듯해요",
          "original_form": "아 그러시군요 불교는.가족들에게 강권하는 분위기는 없는듯해요",
          "speaker_id": "1",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100006071.1.15",
          "form": "네 그런거 같아요.",
          "original_form": "네 그런거 같아요.",
          "speaker_id": "2",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100006071.1.16",
          "form": "지인 중에도 불교 믿는 분이 있어서 템플스테이는 몇번 갔다왔어요",
          "original_form": "지인 중에도 불교 믿는 분이 있어서 템플스테이는 몇번 갔다왔어요",
          "speaker_id": "2",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100006071.1.17",
          "form": "템플스테이 궁금하네요 어떠셨어요",
          "original_form": "템플스테이 궁금하네요 어떠셨어요",
          "speaker_id": "1",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100006071.1.18",
          "form": "어릴때 간거라 별로 힘들지 않고 재밌었어요ㅋㅋ",
          "original_form": "어릴때 간거라 별로 힘들지 않고 재밌었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100006071.1.19",
          "form": "교육도 예의범절 정도 배우는 거였고요",
          "original_form": "교육도 예의범절 정도 배우는 거였고요",
          "speaker_id": "2",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100006071.1.20",
          "form": "식사는 어떤가요?발우공양?? 이런거 하나요?",
          "original_form": "식사는 어떤가요?발우공양?? 이런거 하나요?",
