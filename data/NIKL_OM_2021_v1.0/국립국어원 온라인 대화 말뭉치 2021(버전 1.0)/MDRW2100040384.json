      "utterance": [
        {
          "id": "MDRW2100040384.1.1",
          "form": "패턴은 엄청 단순하고",
          "original_form": "패턴은 엄청 단순하고",
          "speaker_id": "1",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100040384.1.2",
          "form": "약간 좀 캐붕같기도 하고",
          "original_form": "약간 좀 캐붕같기도 하고",
          "speaker_id": "1",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100040384.1.3",
          "form": "난 시뇨라 되게 자신만만한 지략형 캐릭터로 봣는데",
          "original_form": "난 시뇨라 되게 자신만만한 지략형 캐릭터로 봣는데",
          "speaker_id": "1",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100040384.1.4",
          "form": "hmm",
          "original_form": "hmm",
          "speaker_id": "2",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100040384.1.5",
          "form": "근데 스카라무슈가",
          "original_form": "근데 스카라무슈가",
          "speaker_id": "2",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100040384.1.6",
          "form": "시뇨라까지 묶어서",
          "original_form": "시뇨라까지 묶어서",
          "speaker_id": "2",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100040384.1.7",
          "form": "이번 시즌에선 좀 전형적인 말단 악역느낌나",
          "original_form": "이번 시즌에선 좀 전형적인 말단 악역느낌나",
          "speaker_id": "1",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100040384.1.8",
          "form": "무시하더라구",
          "original_form": "무시하더라구",
          "speaker_id": "2",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100040384.1.9",
          "form": "타탈은 그럴수있다 봤는데",
          "original_form": "타탈은 그럴수있다 봤는데",
          "speaker_id": "2",
          "time": "20210904 17:51"
        },
        {
          "id": "MDRW2100040384.1.10",
          "form": "시뇨라는 좀 지략형 아니었나",
          "original_form": "시뇨라는 좀 지략형 아니었나",
          "speaker_id": "2",
          "time": "20210904 17:51"
        },
        {
          "id": "MDRW2100040384.1.11",
          "form": "ㅇㅇ 나름 여왕 직속 사절로 리월까지 가놓고",
          "original_form": "ㅇㅇ 나름 여왕 직속 사절로 리월까지 가놓고",
          "speaker_id": "1",
          "time": "20210904 17:51"
        },
        {
          "id": "MDRW2100040384.1.12",
          "form": "이제와서 덜떨어진 취급은...흠...",
          "original_form": "이제와서 덜떨어진 취급은...흠...",
          "speaker_id": "1",
          "time": "20210904 17:51"
        },
        {
          "id": "MDRW2100040384.1.13",
          "form": "근데 그건 뭐 타탈도 비슷하긴 한데",
          "original_form": "근데 그건 뭐 타탈도 비슷하긴 한데",
          "speaker_id": "2",
          "time": "20210904 17:51"
        },
        {
          "id": "MDRW2100040384.1.14",
          "form": "일단 더 봐바야지",
          "original_form": "일단 더 봐바야지",
          "speaker_id": "2",
          "time": "20210904 17:51"
        },
        {
          "id": "MDRW2100040384.1.15",
          "form": "타탈은 여왕 사절이 아니라",
          "original_form": "타탈은 여왕 사절이 아니라",
          "speaker_id": "1",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100040384.1.16",
          "form": "신의 심장 들고가면 포상받겠지??? 신난당~~!!",
          "original_form": "신의 심장 들고가면 포상받겠지??? 신난당~~!!",
          "speaker_id": "1",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100040384.1.17",
          "form": "근데 솔직히 이나즈마에선 걍",
          "original_form": "근데 솔직히 이나즈마에선 걍",
          "speaker_id": "2",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100040384.1.18",
          "form": "우인단 왜 있는지 모르겠음",
          "original_form": "우인단 왜 있는지 모르겠음",
          "speaker_id": "2",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100040384.1.19",
          "form": "하고 생각 없이 움직인 느낌이라 ㅎ",
          "original_form": "하고 생각 없이 움직인 느낌이라 ㅎ",
          "speaker_id": "1",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100040384.1.20",
          "form": "라이덴 세탁기 용도 외엔",
          "original_form": "라이덴 세탁기 용도 외엔",
          "speaker_id": "2",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100040384.1.21",
          "form": "스카라무슈도",
          "original_form": "스카라무슈도",
          "speaker_id": "2",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100040384.1.22",
          "form": "걍 똑같이 심장 가져가려고 온거 아냐?",
          "original_form": "걍 똑같이 심장 가져가려고 온거 아냐?",
          "speaker_id": "1",
          "time": "20210904 17:55"
        },
        {
          "id": "MDRW2100040384.1.23",
          "form": "왜 깐지나게 이름 띄우면서",
          "original_form": "왜 깐지나게 이름 띄우면서",
          "speaker_id": "2",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100040384.1.24",
          "form": "등장해놓고",
          "original_form": "등장해놓고",
          "speaker_id": "2",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100040384.1.25",
          "form": "안나와?",
          "original_form": "안나와?",
          "speaker_id": "2",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100040384.1.26",
          "form": "나와?",
          "original_form": "나와?",
          "speaker_id": "2",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100040384.1.27",
          "form": "주인공이",
          "original_form": "주인공이",
          "speaker_id": "1",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100040384.1.28",
          "form": "맞붙으면",
          "original_form": "맞붙으면",
          "speaker_id": "1",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100040384.1.29",
          "form": "죽기 때문잊니다...",
          "original_form": "죽기 때문잊니다...",
          "speaker_id": "1",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100040384.1.30",
          "form": "ㅇㅏ왜냐구......",
          "original_form": "ㅇㅏ왜냐구......",
          "speaker_id": "2",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100040384.1.31",
          "form": "주인공 유사 사안사용자",
          "original_form": "주인공 유사 사안사용자",
          "speaker_id": "2",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100040384.1.32",
          "form": "아니냐구",
          "original_form": "아니냐구",
          "speaker_id": "2",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100040384.1.33",
          "form": "아직 주인공은",
          "original_form": "아직 주인공은",
          "speaker_id": "1",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100040384.1.34",
          "form": "부작용없는 사안 사용자....",
          "original_form": "부작용없는 사안 사용자....",
          "speaker_id": "2",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100040384.1.35",
          "form": "시뇨라랑 싸울 단계인것...",
          "original_form": "시뇨라랑 싸울 단계인것...",
          "speaker_id": "1",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100040384.1.36",
          "form": "아니 근데 이 투력인플레",
          "original_form": "아니 근데 이 투력인플레",
          "speaker_id": "2",
          "time": "20210904 17:57"
        },
        {
          "id": "MDRW2100040384.1.37",
          "form": "어쩌려고 그러지",
          "original_form": "어쩌려고 그러지",
          "speaker_id": "2",
          "time": "20210904 17:57"
        },
        {
          "id": "MDRW2100040384.1.38",
          "form": "만렙 주인공이 시뇨라급이라니",
          "original_form": "만렙 주인공이 시뇨라급이라니",
          "speaker_id": "2",
          "time": "20210904 17:57"
        },
        {
          "id": "MDRW2100040384.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100040384.1.40",
          "form": "ㄱㄴㄲ",
          "original_form": "ㄱㄴㄲ",
          "speaker_id": "1",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100040384.1.41",
          "form": "주인공 언제쯤",
          "original_form": "주인공 언제쯤",
          "speaker_id": "1",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100040384.1.42",
          "form": "신상 안거치고 속성 바로 바꾸게 되나요",
          "original_form": "신상 안거치고 속성 바로 바꾸게 되나요",
