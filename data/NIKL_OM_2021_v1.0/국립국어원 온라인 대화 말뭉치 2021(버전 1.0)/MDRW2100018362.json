      "utterance": [
        {
          "id": "MDRW2100018362.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 00:19"
        },
        {
          "id": "MDRW2100018362.1.2",
          "form": "그다지요.",
          "original_form": "그다지요.",
          "speaker_id": "1",
          "time": "20210827 00:20"
        },
        {
          "id": "MDRW2100018362.1.3",
          "form": "집에 있는게 편하고 좋아요",
          "original_form": "집에 있는게 편하고 좋아요",
          "speaker_id": "1",
          "time": "20210827 00:20"
        },
        {
          "id": "MDRW2100018362.1.4",
          "form": "아ㅎㅎㅎ",
          "original_form": "아ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 00:20"
        },
        {
          "id": "MDRW2100018362.1.5",
          "form": "해외여행은 가고 싶네요",
          "original_form": "해외여행은 가고 싶네요",
          "speaker_id": "1",
          "time": "20210827 00:20"
        },
        {
          "id": "MDRW2100018362.1.6",
          "form": "여행 주로 어디가세요?",
          "original_form": "여행 주로 어디가세요?",
          "speaker_id": "2",
          "time": "20210827 00:20"
        },
        {
          "id": "MDRW2100018362.1.7",
          "form": "코로나가 빨리 끝나면 좋겠네요",
          "original_form": "코로나가 빨리 끝나면 좋겠네요",
          "speaker_id": "1",
          "time": "20210827 00:21"
        },
        {
          "id": "MDRW2100018362.1.8",
          "form": "저두요ㅠㅠ",
          "original_form": "저두요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 00:21"
        },
        {
          "id": "MDRW2100018362.1.9",
          "form": "여행 ..주로 간다는 곳은 없는데 자연을 즐기러 가는 편은 아니에요",
          "original_form": "여행 ..주로 간다는 곳은 없는데 자연을 즐기러 가는 편은 아니에요",
          "speaker_id": "1",
          "time": "20210827 00:23"
        },
        {
          "id": "MDRW2100018362.1.10",
          "form": "해외여행은 영국 다시 가보고 싶어요..",
          "original_form": "해외여행은 영국 다시 가보고 싶어요..",
          "speaker_id": "1",
          "time": "20210827 00:23"
        },
        {
          "id": "MDRW2100018362.1.11",
          "form": "아하 영국 좋죠",
          "original_form": "아하 영국 좋죠",
          "speaker_id": "2",
          "time": "20210827 00:24"
        },
        {
          "id": "MDRW2100018362.1.12",
          "form": "저도 빅벤 다시 보고 싶네요 ㅎㅎ",
          "original_form": "저도 빅벤 다시 보고 싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 00:24"
        },
        {
          "id": "MDRW2100018362.1.13",
          "form": "영국 가보셨어요?",
          "original_form": "영국 가보셨어요?",
          "speaker_id": "1",
          "time": "20210827 00:24"
        },
        {
          "id": "MDRW2100018362.1.14",
          "form": "네네 유럽여행 갔을 때 가봤어요",
          "original_form": "네네 유럽여행 갔을 때 가봤어요",
          "speaker_id": "2",
          "time": "20210827 00:24"
        },
        {
          "id": "MDRW2100018362.1.15",
          "form": "다른 곳은 어디 가보셨어요?",
          "original_form": "다른 곳은 어디 가보셨어요?",
          "speaker_id": "2",
          "time": "20210827 00:24"
        },
        {
          "id": "MDRW2100018362.1.16",
          "form": "영국서 먹었던 스콘이 그립네요",
          "original_form": "영국서 먹었던 스콘이 그립네요",
          "speaker_id": "1",
          "time": "20210827 00:25"
        },
        {
          "id": "MDRW2100018362.1.17",
          "form": "스콘.. 영국 맛집인가요?",
          "original_form": "스콘.. 영국 맛집인가요?",
          "speaker_id": "2",
          "time": "20210827 00:25"
        },
        {
          "id": "MDRW2100018362.1.18",
          "form": "다른데는. . 프랑스 독일 벨기에 베트남...",
          "original_form": "다른데는. . 프랑스 독일 벨기에 베트남...",
          "speaker_id": "1",
          "time": "20210827 00:25"
        },
        {
          "id": "MDRW2100018362.1.19",
          "form": "더 다녀온거 같은데 기억이 안나네요",
          "original_form": "더 다녀온거 같은데 기억이 안나네요",
          "speaker_id": "1",
          "time": "20210827 00:25"
        },
        {
          "id": "MDRW2100018362.1.20",
          "form": "와 많이 가보셨네요",
          "original_form": "와 많이 가보셨네요",
          "speaker_id": "2",
          "time": "20210827 00:25"
        },
        {
          "id": "MDRW2100018362.1.21",
          "form": "부럽다",
          "original_form": "부럽다",
          "speaker_id": "2",
          "time": "20210827 00:25"
        },
        {
          "id": "MDRW2100018362.1.22",
          "form": "먹은지 오래되서 이게 진짜 맛있었던건지 추억 보정인지 모르겠어요",
          "original_form": "먹은지 오래되서 이게 진짜 맛있었던건지 추억 보정인지 모르겠어요",
          "speaker_id": "1",
          "time": "20210827 00:26"
        },
        {
          "id": "MDRW2100018362.1.23",
          "form": "그래서 다시 가보고 싶어요ㅋㅋ",
          "original_form": "그래서 다시 가보고 싶어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:26"
        },
        {
          "id": "MDRW2100018362.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ아 그럴 때 있죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 그럴 때 있죠",
          "speaker_id": "2",
          "time": "20210827 00:26"
        },
        {
          "id": "MDRW2100018362.1.25",
          "form": "저도 딴 지역에서 먹었던 국밥이 그리운데",
          "original_form": "저도 딴 지역에서 먹었던 국밥이 그리운데",
          "speaker_id": "2",
          "time": "20210827 00:27"
        },
        {
          "id": "MDRW2100018362.1.26",
          "form": "유럽여행은 정말 한번 다녀오고 평생 그 추억으로 산다는게 맞는것 같아요",
          "original_form": "유럽여행은 정말 한번 다녀오고 평생 그 추억으로 산다는게 맞는것 같아요",
          "speaker_id": "1",
          "time": "20210827 00:27"
        },
        {
          "id": "MDRW2100018362.1.27",
          "form": "다시 먹으면 그맛이 안날거 같아요",
          "original_form": "다시 먹으면 그맛이 안날거 같아요",
          "speaker_id": "2",
          "time": "20210827 00:27"
        },
        {
          "id": "MDRW2100018362.1.28",
          "form": "어 어디서 맛있게 드셨어요?",
          "original_form": "어 어디서 맛있게 드셨어요?",
          "speaker_id": "1",
          "time": "20210827 00:27"
        },
        {
          "id": "MDRW2100018362.1.29",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 00:27"
        },
        {
          "id": "MDRW2100018362.1.30",
          "form": "전라도 국밥이 맛있더라구요",
          "original_form": "전라도 국밥이 맛있더라구요",
          "speaker_id": "2",
          "time": "20210827 00:27"
        },
        {
          "id": "MDRW2100018362.1.31",
          "form": "전 부산에서 맛집이라던 곳이 맛있었거든요",
          "original_form": "전 부산에서 맛집이라던 곳이 맛있었거든요",
          "speaker_id": "1",
          "time": "20210827 00:27"
        },
        {
          "id": "MDRW2100018362.1.32",
          "form": "국내여행도 재미있어요",
          "original_form": "국내여행도 재미있어요",
          "speaker_id": "2",
          "time": "20210827 00:27"
        },
        {
          "id": "MDRW2100018362.1.33",
          "form": "전라도 갔을때 국밥을 먹었어야했는데ㅜㅜ",
          "original_form": "전라도 갔을때 국밥을 먹었어야했는데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 00:27"
        },
        {
          "id": "MDRW2100018362.1.34",
          "form": "아 부산 국밥 유명하죠",
          "original_form": "아 부산 국밥 유명하죠",
          "speaker_id": "2",
          "time": "20210827 00:27"
        },
        {
          "id": "MDRW2100018362.1.35",
          "form": "같이간 일행이 전라도 국밥을 안좋아해서 못먹은게 아쉽네요",
          "original_form": "같이간 일행이 전라도 국밥을 안좋아해서 못먹은게 아쉽네요",
          "speaker_id": "1",
          "time": "20210827 00:28"
        },
        {
          "id": "MDRW2100018362.1.36",
          "form": "앗 저런 ㅠㅠ",
          "original_form": "앗 저런 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 00:28"
        },
        {
          "id": "MDRW2100018362.1.37",
          "form": "여행다닐때 동행 입맛도 중요하죠",
          "original_form": "여행다닐때 동행 입맛도 중요하죠",
          "speaker_id": "2",
          "time": "20210827 00:28"
        },
        {
          "id": "MDRW2100018362.1.38",
          "form": "잘맞아야하는데...",
          "original_form": "잘맞아야하는데...",
          "speaker_id": "2",
          "time": "20210827 00:28"
        },
        {
          "id": "MDRW2100018362.1.39",
          "form": "유명한 곳들 잘 찾아다니면서 먹는다고 먹었는데 그걸 빠트렸네요",
          "original_form": "유명한 곳들 잘 찾아다니면서 먹는다고 먹었는데 그걸 빠트렸네요",
          "speaker_id": "1",
          "time": "20210827 00:29"
        },
        {
          "id": "MDRW2100018362.1.40",
          "form": "국내여행도 좋죠.",
          "original_form": "국내여행도 좋죠.",
          "speaker_id": "1",
          "time": "20210827 00:30"
        },
        {
          "id": "MDRW2100018362.1.41",
          "form": "국내여행은 당일치기로 주로 하게 되는 것 같아요",
          "original_form": "국내여행은 당일치기로 주로 하게 되는 것 같아요",
          "speaker_id": "1",
          "time": "20210827 00:31"
        },
        {
          "id": "MDRW2100018362.1.42",
          "form": "맞아요 일박넘어가면 힘들더라구요",
          "original_form": "맞아요 일박넘어가면 힘들더라구요",
          "speaker_id": "2",
          "time": "20210827 00:31"
        },
        {
          "id": "MDRW2100018362.1.43",
          "form": "체력이 안돼서ㅋㅋㅋㅠㅠ",
          "original_form": "체력이 안돼서ㅋㅋㅋㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 00:31"
        },
        {
          "id": "MDRW2100018362.1.44",
          "form": "다음 여행지는 생각해두신 곳 있으세요?",
          "original_form": "다음 여행지는 생각해두신 곳 있으세요?",
          "speaker_id": "2",
          "time": "20210827 00:31"
        }
      ]
