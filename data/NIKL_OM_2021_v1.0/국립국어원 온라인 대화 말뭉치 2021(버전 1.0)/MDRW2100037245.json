      "utterance": [
        {
          "id": "MDRW2100037245.1.1",
          "form": "휴 할 말이 너무 많네요;;;",
          "original_form": "휴 할 말이 너무 많네요;;;",
          "speaker_id": "2",
          "time": "20210831 21:50"
        },
        {
          "id": "MDRW2100037245.1.2",
          "form": "네 여자는 잘해봐야 본전이에요",
          "original_form": "네 여자는 잘해봐야 본전이에요",
          "speaker_id": "1",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100037245.1.3",
          "form": "맞아요 그니까요",
          "original_form": "맞아요 그니까요",
          "speaker_id": "2",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100037245.1.4",
          "form": "그리고 아무리 잘 사는 집에 시집가도",
          "original_form": "그리고 아무리 잘 사는 집에 시집가도",
          "speaker_id": "2",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100037245.1.5",
          "form": "남녀평등을 하자니 남녀 돈 평등만 열심히 해요 ㅋㅋㅋㅋ",
          "original_form": "남녀평등을 하자니 남녀 돈 평등만 열심히 해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100037245.1.6",
          "form": "평생 눈치보며 살아요",
          "original_form": "평생 눈치보며 살아요",
          "speaker_id": "2",
          "time": "20210831 21:51"
        },
        {
          "id": "MDRW2100037245.1.7",
          "form": "아니그니까요!!!!",
          "original_form": "아니그니까요!!!!",
          "speaker_id": "2",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100037245.1.8",
          "form": "name1님이랑 말이 잘 통해서 정말 좋네요",
          "original_form": "&name1&님이랑 말이 잘 통해서 정말 좋네요",
          "speaker_id": "2",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100037245.1.9",
          "form": "그죠 돈으로 갑질하거나 하는 경우많죠",
          "original_form": "그죠 돈으로 갑질하거나 하는 경우많죠",
          "speaker_id": "1",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100037245.1.10",
          "form": "네 저도 반가워요 ㅜㅜ",
          "original_form": "네 저도 반가워요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100037245.1.11",
          "form": "연애와 결혼으로 밤새서 대화 가능합니다",
          "original_form": "연애와 결혼으로 밤새서 대화 가능합니다",
          "speaker_id": "2",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100037245.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100037245.1.13",
          "form": "대기업 가문이랑 결혼한",
          "original_form": "대기업 가문이랑 결혼한",
          "speaker_id": "2",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100037245.1.14",
          "form": "똑똑한 여자들",
          "original_form": "똑똑한 여자들",
          "speaker_id": "2",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100037245.1.15",
          "form": "다 거기서 소리소문 없이",
          "original_form": "다 거기서 소리소문 없이",
          "speaker_id": "2",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100037245.1.16",
          "form": "조신하게 며느리 역할만 하고...",
          "original_form": "조신하게 며느리 역할만 하고...",
          "speaker_id": "2",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100037245.1.17",
          "form": "네네 딱 그거죠 그럼 사실 돈주는거도 아니에요",
          "original_form": "네네 딱 그거죠 그럼 사실 돈주는거도 아니에요",
          "speaker_id": "1",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100037245.1.18",
          "form": "글고 여자가 잘살면 남자 기죽일까봐 조심하고 ㅋㅋㅋㅋ웃기죠",
          "original_form": "글고 여자가 잘살면 남자 기죽일까봐 조심하고 ㅋㅋㅋㅋ웃기죠",
          "speaker_id": "1",
          "time": "20210831 21:53"
        },
        {
          "id": "MDRW2100037245.1.19",
          "form": "헐 맞아요",
          "original_form": "헐 맞아요",
          "speaker_id": "2",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100037245.1.20",
          "form": "저 예전에",
          "original_form": "저 예전에",
          "speaker_id": "2",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100037245.1.21",
          "form": "친구네 옆집에",
          "original_form": "친구네 옆집에",
          "speaker_id": "2",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100037245.1.22",
          "form": "여자분이 의사셨는데",
          "original_form": "여자분이 의사셨는데",
          "speaker_id": "2",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100037245.1.23",
          "form": "남편은 직업이 없고",
          "original_form": "남편은 직업이 없고",
          "speaker_id": "2",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100037245.1.24",
          "form": "네 누구랑 결혼했어요?",
          "original_form": "네 누구랑 결혼했어요?",
          "speaker_id": "1",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100037245.1.25",
          "form": "여자가 벌어오는 돈으로 방 한 칸을 드럼 연습공간으로 만들어서",
          "original_form": "여자가 벌어오는 돈으로 방 한 칸을 드럼 연습공간으로 만들어서",
          "speaker_id": "2",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100037245.1.26",
          "form": "남편은 백수였대요",
          "original_form": "남편은 백수였대요",
          "speaker_id": "2",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100037245.1.27",
          "form": "헐;;;;;;; 완전",
          "original_form": "헐;;;;;;; 완전",
          "speaker_id": "1",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100037245.1.28",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100037245.1.29",
          "form": "혼자서 드럼 치고 보컬 연습 받고",
          "original_form": "혼자서 드럼 치고 보컬 연습 받고",
          "speaker_id": "2",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100037245.1.30",
          "form": "근데 거기다가 부인을 때리기까지 했대요",
          "original_form": "근데 거기다가 부인을 때리기까지 했대요",
          "speaker_id": "2",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100037245.1.31",
          "form": "근데 그거 아시죠 은근히",
          "original_form": "근데 그거 아시죠 은근히",
          "speaker_id": "2",
          "time": "20210831 21:54"
        },
        {
          "id": "MDRW2100037245.1.32",
          "form": "헉 돌았네요",
          "original_form": "헉 돌았네요",
          "speaker_id": "1",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100037245.1.33",
          "form": "가방끈 긴 여자들이",
          "original_form": "가방끈 긴 여자들이",
          "speaker_id": "2",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100037245.1.34",
          "form": "남자 잘못 만나는거요",
          "original_form": "남자 잘못 만나는거요",
          "speaker_id": "2",
          "time": "20210831 21:55"
