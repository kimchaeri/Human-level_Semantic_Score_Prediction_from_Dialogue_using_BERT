      "utterance": [
        {
          "id": "MDRW2100025552.1.1",
          "form": "하....",
          "original_form": "하....",
          "speaker_id": "2",
          "time": "20210901 13:15"
        },
        {
          "id": "MDRW2100025552.1.2",
          "form": "딱히 다른 매력이 있는것도 아닌데",
          "original_form": "딱히 다른 매력이 있는것도 아닌데",
          "speaker_id": "2",
          "time": "20210901 13:15"
        },
        {
          "id": "MDRW2100025552.1.3",
          "form": "왜 그러고 살까요",
          "original_form": "왜 그러고 살까요",
          "speaker_id": "2",
          "time": "20210901 13:15"
        },
        {
          "id": "MDRW2100025552.1.4",
          "form": "그러니까요 ㅎㅎㅎ",
          "original_form": "그러니까요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 13:16"
        },
        {
          "id": "MDRW2100025552.1.5",
          "form": "그렇게 잘생긴 남자도 아니었을텐데...",
          "original_form": "그렇게 잘생긴 남자도 아니었을텐데...",
          "speaker_id": "1",
          "time": "20210901 13:16"
        },
        {
          "id": "MDRW2100025552.1.6",
          "form": "못생겼으면 돈이라도 쓰란말이야....",
          "original_form": "못생겼으면 돈이라도 쓰란말이야....",
          "speaker_id": "2",
          "time": "20210901 13:53"
        },
        {
          "id": "MDRW2100025552.1.7",
          "form": "그냥 안만나는게 가장 속편해졌죠...휴",
          "original_form": "그냥 안만나는게 가장 속편해졌죠...휴",
          "speaker_id": "2",
          "time": "20210901 13:54"
        },
        {
          "id": "MDRW2100025552.1.8",
          "form": "맞아요 굳이 위험을 감수해가면서까지",
          "original_form": "맞아요 굳이 위험을 감수해가면서까지",
          "speaker_id": "1",
          "time": "20210901 13:59"
        },
        {
          "id": "MDRW2100025552.1.9",
          "form": "만나야하나 싶고...",
          "original_form": "만나야하나 싶고...",
          "speaker_id": "1",
          "time": "20210901 13:59"
        },
        {
          "id": "MDRW2100025552.1.10",
          "form": "욕구도 안생기고....",
          "original_form": "욕구도 안생기고....",
          "speaker_id": "1",
          "time": "20210901 13:59"
        },
        {
          "id": "MDRW2100025552.1.11",
          "form": "맞아요. 이런게 비혼이지 딴게 비혼인가",
          "original_form": "맞아요. 이런게 비혼이지 딴게 비혼인가",
          "speaker_id": "2",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100025552.1.12",
          "form": "그러니까요!",
          "original_form": "그러니까요!",
          "speaker_id": "1",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100025552.1.13",
          "form": "반사회적인 게 아닌데 말이에요",
          "original_form": "반사회적인 게 아닌데 말이에요",
          "speaker_id": "1",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100025552.1.14",
          "form": "가끔 미디어에서 비혼이랑 미혼을 혼용하는 게 거슬리더라구요",
          "original_form": "가끔 미디어에서 비혼이랑 미혼을 혼용하는 게 거슬리더라구요",
          "speaker_id": "1",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100025552.1.15",
          "form": "사실 전 적극적인 비혼의지까진 아니라",
          "original_form": "사실 전 적극적인 비혼의지까진 아니라",
          "speaker_id": "2",
          "time": "20210901 14:03"
        },
        {
          "id": "MDRW2100025552.1.16",
          "form": "혼용해도 별 상관 없긴한데",
          "original_form": "혼용해도 별 상관 없긴한데",
          "speaker_id": "2",
          "time": "20210901 14:03"
        },
        {
          "id": "MDRW2100025552.1.17",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210901 14:10"
        },
        {
          "id": "MDRW2100025552.1.18",
          "form": "근데 네이버에 검색해보면",
          "original_form": "근데 네이버에 검색해보면",
          "speaker_id": "1",
          "time": "20210901 14:10"
        },
        {
          "id": "MDRW2100025552.1.19",
          "form": "비혼주의자가 결혼을 결심하게 된 이유~ 이러면서",
          "original_form": "비혼주의자가 결혼을 결심하게 된 이유~ 이러면서",
          "speaker_id": "1",
          "time": "20210901 14:10"
        },
        {
          "id": "MDRW2100025552.1.20",
          "form": "뭔가,, 결국 비혼주의자도 좋은 남자 만나면 결혼하고 싶어할거야~ 느낌?!",
          "original_form": "뭔가,, 결국 비혼주의자도 좋은 남자 만나면 결혼하고 싶어할거야~ 느낌?!",
          "speaker_id": "1",
          "time": "20210901 14:11"
        },
        {
          "id": "MDRW2100025552.1.21",
          "form": "뭐 저도 사실 그러니까요",
          "original_form": "뭐 저도 사실 그러니까요",
          "speaker_id": "2",
          "time": "20210901 14:11"
        },
        {
          "id": "MDRW2100025552.1.22",
          "form": "인연이 있으면 할수도 있지 라서",
          "original_form": "인연이 있으면 할수도 있지 라서",
          "speaker_id": "2",
          "time": "20210901 14:11"
        },
        {
          "id": "MDRW2100025552.1.23",
          "form": "근데 일단 좋은 남자를 내놔봐",
          "original_form": "근데 일단 좋은 남자를 내놔봐",
          "speaker_id": "2",
          "time": "20210901 14:12"
        },
        {
          "id": "MDRW2100025552.1.24",
          "form": "ㅋㅋㅋㅋㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 14:12"
        },
        {
          "id": "MDRW2100025552.1.25",
          "form": "ㅋㅋㅋㅋㅋ 그니까요~~~~",
          "original_form": "ㅋㅋㅋㅋㅋ 그니까요~~~~",
          "speaker_id": "1",
          "time": "20210901 14:12"
        },
        {
          "id": "MDRW2100025552.1.26",
          "form": "그러면서 막",
          "original_form": "그러면서 막",
          "speaker_id": "1",
          "time": "20210901 14:12"
        },
        {
          "id": "MDRW2100025552.1.27",
          "form": "별로인 남자들도 엄청 사랑꾼인 것처럼 포장하고...",
          "original_form": "별로인 남자들도 엄청 사랑꾼인 것처럼 포장하고...",
          "speaker_id": "1",
          "time": "20210901 14:12"
        },
        {
          "id": "MDRW2100025552.1.28",
          "form": "ㅋㅋㅋㅠㅠㅠㅜㅡ",
          "original_form": "ㅋㅋㅋㅠㅠㅠㅜㅡ",
          "speaker_id": "2",
          "time": "20210901 14:13"
        },
        {
          "id": "MDRW2100025552.1.29",
          "form": "여자들은 당연히 하는거",
          "original_form": "여자들은 당연히 하는거",
          "speaker_id": "2",
          "time": "20210901 14:13"
        },
        {
          "id": "MDRW2100025552.1.30",
          "form": "엄청 올려쳐주죠",
          "original_form": "엄청 올려쳐주죠",
          "speaker_id": "2",
          "time": "20210901 14:13"
        },
        {
          "id": "MDRW2100025552.1.31",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210901 14:13"
        },
        {
          "id": "MDRW2100025552.1.32",
          "form": "인간 구실만 해도",
          "original_form": "인간 구실만 해도",
          "speaker_id": "1",
          "time": "20210901 14:13"
        },
        {
          "id": "MDRW2100025552.1.33",
          "form": "우쭈쭈 해주니까요!",
          "original_form": "우쭈쭈 해주니까요!",
          "speaker_id": "1",
          "time": "20210901 14:13"
        },
        {
          "id": "MDRW2100025552.1.34",
          "form": "뭐...인간 구실 못하는 애들이 많아졌으니 어쩔수 없는건가...",
          "original_form": "뭐...인간 구실 못하는 애들이 많아졌으니 어쩔수 없는건가...",
          "speaker_id": "2",
          "time": "20210901 14:15"
        },
        {
          "id": "MDRW2100025552.1.35",
          "form": "정말 극악무도한 상대평가....",
          "original_form": "정말 극악무도한 상대평가....{emoji:☆}",
          "speaker_id": "1",
          "time": "20210901 14:18"
        },
        {
          "id": "MDRW2100025552.1.36",
          "form": "우리가 성별반전 되었으면",
          "original_form": "우리가 성별반전 되었으면",
          "speaker_id": "1",
          "time": "20210901 14:18"
        },
        {
          "id": "MDRW2100025552.1.37",
          "form": "(가치관과 정신이 그대로라는 가정 하에)",
          "original_form": "(가치관과 정신이 그대로라는 가정 하에)",
          "speaker_id": "1",
          "time": "20210901 14:18"
        },
        {
          "id": "MDRW2100025552.1.38",
          "form": "유니콘 상위 0.1% 였을거예요",
          "original_form": "유니콘 상위 0.1% 였을거예요",
