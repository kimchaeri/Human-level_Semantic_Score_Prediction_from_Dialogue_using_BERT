      "utterance": [
        {
          "id": "MDRW2100030652.1.1",
          "form": "흘리죠?",
          "original_form": "흘리죠?",
          "speaker_id": "1",
          "time": "20210904 11:02"
        },
        {
          "id": "MDRW2100030652.1.2",
          "form": "흐리죠",
          "original_form": "흐리죠",
          "speaker_id": "1",
          "time": "20210904 11:02"
        },
        {
          "id": "MDRW2100030652.1.3",
          "form": "오늘이요?",
          "original_form": "오늘이요?",
          "speaker_id": "2",
          "time": "20210904 11:03"
        },
        {
          "id": "MDRW2100030652.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210904 11:03"
        },
        {
          "id": "MDRW2100030652.1.5",
          "form": "오늘 엄청 쨍쨍해요!!",
          "original_form": "오늘 엄청 쨍쨍해요!!",
          "speaker_id": "2",
          "time": "20210904 11:03"
        },
        {
          "id": "MDRW2100030652.1.6",
          "form": "그쪽 동네는 흐린가요?",
          "original_form": "그쪽 동네는 흐린가요?",
          "speaker_id": "2",
          "time": "20210904 11:03"
        },
        {
          "id": "MDRW2100030652.1.7",
          "form": "구름이요",
          "original_form": "구름이요",
          "speaker_id": "1",
          "time": "20210904 11:04"
        },
        {
          "id": "MDRW2100030652.1.8",
          "form": "저희는 지금 엄청 덥고",
          "original_form": "저희는 지금 엄청 덥고",
          "speaker_id": "2",
          "time": "20210904 11:04"
        },
        {
          "id": "MDRW2100030652.1.9",
          "form": "해가 쨍쨍합니다",
          "original_form": "해가 쨍쨍합니다",
          "speaker_id": "2",
          "time": "20210904 11:04"
        },
        {
          "id": "MDRW2100030652.1.10",
          "form": "저녁때만 좀 선선해요",
          "original_form": "저녁때만 좀 선선해요",
          "speaker_id": "2",
          "time": "20210904 11:04"
        },
        {
          "id": "MDRW2100030652.1.11",
          "form": "오늘 주말인데 어디 안가세용?",
          "original_form": "오늘 주말인데 어디 안가세용?",
          "speaker_id": "2",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100030652.1.12",
          "form": "아직 생각중이오",
          "original_form": "아직 생각중이오",
          "speaker_id": "1",
          "time": "20210904 11:06"
        },
        {
          "id": "MDRW2100030652.1.13",
          "form": "전 오늘 점심으로 오리고기 먹으러 가기로 했어요",
          "original_form": "전 오늘 점심으로 오리고기 먹으러 가기로 했어요",
          "speaker_id": "2",
          "time": "20210904 11:06"
        },
        {
          "id": "MDRW2100030652.1.14",
          "form": "갔다가 강가에서 산책하려고요",
          "original_form": "갔다가 강가에서 산책하려고요",
          "speaker_id": "2",
          "time": "20210904 11:07"
        },
        {
          "id": "MDRW2100030652.1.15",
          "form": "좋내여",
          "original_form": "좋내여",
          "speaker_id": "1",
          "time": "20210904 11:07"
        },
        {
          "id": "MDRW2100030652.1.16",
          "form": "날씨가 좋아서 산책하고 싶네요",
          "original_form": "날씨가 좋아서 산책하고 싶네요",
          "speaker_id": "2",
          "time": "20210904 11:07"
        },
        {
          "id": "MDRW2100030652.1.17",
          "form": "선선하죠?",
          "original_form": "선선하죠?",
          "speaker_id": "1",
          "time": "20210904 11:10"
        },
        {
          "id": "MDRW2100030652.1.18",
          "form": "네 그늘은 넘 좋네요",
          "original_form": "네 그늘은 넘 좋네요",
          "speaker_id": "2",
          "time": "20210904 11:32"
        },
        {
          "id": "MDRW2100030652.1.19",
          "form": "요즘 아침저녁으론 선선해서",
          "original_form": "요즘 아침저녁으론 선선해서",
          "speaker_id": "2",
          "time": "20210904 11:32"
        },
        {
          "id": "MDRW2100030652.1.20",
          "form": "가디건 가지고 다녀요",
          "original_form": "가디건 가지고 다녀요",
          "speaker_id": "2",
          "time": "20210904 11:32"
        },
        {
          "id": "MDRW2100030652.1.21",
          "form": "조만간 가을옷꺼내야겠어요ㅋㅋㅋ",
          "original_form": "조만간 가을옷꺼내야겠어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 11:32"
        },
        {
          "id": "MDRW2100030652.1.22",
          "form": "그쵸 정리를",
          "original_form": "그쵸 정리를",
          "speaker_id": "1",
          "time": "20210904 11:33"
        }
      ]
