      "utterance": [
        {
          "id": "MDRW2100030698.1.1",
          "form": "갸족이 어떻게 되세요",
          "original_form": "갸족이 어떻게 되세요",
          "speaker_id": "2",
          "time": "20210904 11:33"
        },
        {
          "id": "MDRW2100030698.1.2",
          "form": "남동생 하나 있습니다",
          "original_form": "남동생 하나 있습니다",
          "speaker_id": "1",
          "time": "20210904 11:33"
        },
        {
          "id": "MDRW2100030698.1.3",
          "form": "가족 있으세요?",
          "original_form": "가족 있으세요?",
          "speaker_id": "1",
          "time": "20210904 11:33"
        },
        {
          "id": "MDRW2100030698.1.4",
          "form": "있죠 가족",
          "original_form": "있죠 가족",
          "speaker_id": "2",
          "time": "20210904 11:33"
        },
        {
          "id": "MDRW2100030698.1.5",
          "form": "많아요",
          "original_form": "많아요",
          "speaker_id": "2",
          "time": "20210904 11:33"
        },
        {
          "id": "MDRW2100030698.1.6",
          "form": "여동생 있나요?",
          "original_form": "여동생 있나요?",
          "speaker_id": "1",
          "time": "20210904 11:33"
        },
        {
          "id": "MDRW2100030698.1.7",
          "form": "넹 있어요 돈 잘 버는",
          "original_form": "넹 있어요 돈 잘 버는",
          "speaker_id": "2",
          "time": "20210904 11:33"
        },
        {
          "id": "MDRW2100030698.1.8",
          "form": "여동생에게 혹시 연애 조언도 받으시나요?",
          "original_form": "여동생에게 혹시 연애 조언도 받으시나요?",
          "speaker_id": "1",
          "time": "20210904 11:33"
        },
        {
          "id": "MDRW2100030698.1.9",
          "form": "네 가끔 그렇죠",
          "original_form": "네 가끔 그렇죠",
          "speaker_id": "2",
          "time": "20210904 11:34"
        },
        {
          "id": "MDRW2100030698.1.10",
          "form": "남자친구가 넷플 보러 오라고 하면 뭐라고 해야 하는지",
          "original_form": "남자친구가 넷플 보러 오라고 하면 뭐라고 해야 하는지",
          "speaker_id": "1",
          "time": "20210904 11:34"
        },
        {
          "id": "MDRW2100030698.1.11",
          "form": "여동생한테 물어봐 주시겠어요?",
          "original_form": "여동생한테 물어봐 주시겠어요?",
          "speaker_id": "1",
          "time": "20210904 11:34"
        },
        {
          "id": "MDRW2100030698.1.12",
          "form": "그걸 어뜨케 물어봅니까 이사람아",
          "original_form": "그걸 어뜨케 물어봅니까 이사람아",
          "speaker_id": "2",
          "time": "20210904 12:59"
        },
        {
          "id": "MDRW2100030698.1.13",
          "form": "아앗..!",
          "original_form": "아앗..!",
          "speaker_id": "1",
          "time": "20210904 13:02"
        },
        {
          "id": "MDRW2100030698.1.14",
          "form": "여동생이랑 별로 안 친하신가 봐요",
          "original_form": "여동생이랑 별로 안 친하신가 봐요",
          "speaker_id": "1",
          "time": "20210904 13:02"
        },
        {
          "id": "MDRW2100030698.1.15",
          "form": "네네 별로 안친해요 자격지심 느껴져서요",
          "original_form": "네네 별로 안친해요 자격지심 느껴져서요",
          "speaker_id": "2",
          "time": "20210904 13:02"
        },
        {
          "id": "MDRW2100030698.1.16",
          "form": "동생이 거지인 거 보다는 훨씬 나아요",
          "original_form": "동생이 거지인 거 보다는 훨씬 나아요",
          "speaker_id": "1",
          "time": "20210904 13:03"
        },
        {
          "id": "MDRW2100030698.1.17",
          "form": "아직 어려서 모르시나 본데",
          "original_form": "아직 어려서 모르시나 본데",
          "speaker_id": "1",
          "time": "20210904 13:03"
        },
        {
          "id": "MDRW2100030698.1.18",
          "form": "친구도 거지 친구보다는 건물 있는 친구가 낫고",
          "original_form": "친구도 거지 친구보다는 건물 있는 친구가 낫고",
          "speaker_id": "1",
          "time": "20210904 13:03"
        },
        {
          "id": "MDRW2100030698.1.19",
          "form": "크면 알 거예요",
          "original_form": "크면 알 거예요",
          "speaker_id": "1",
          "time": "20210904 13:03"
        },
        {
          "id": "MDRW2100030698.1.20",
          "form": "전 친해요",
          "original_form": "전 친해요",
          "speaker_id": "1",
          "time": "20210904 13:51"
        }
      ]
