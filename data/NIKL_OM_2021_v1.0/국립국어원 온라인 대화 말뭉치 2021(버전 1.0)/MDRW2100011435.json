        {
          "id": "MDRW2100011435.1.1",
          "form": "식사는요?",
          "original_form": "식사는요?",
          "speaker_id": "1",
          "time": "20210824 12:39"
        },
        {
          "id": "MDRW2100011435.1.2",
          "form": "간단하게 챙겨먹었네요",
          "original_form": "간단하게 챙겨먹었네요",
          "speaker_id": "2",
          "time": "20210824 14:15"
        },
        {
          "id": "MDRW2100011435.1.3",
          "form": "그쪽은요?",
          "original_form": "그쪽은요?",
          "speaker_id": "2",
          "time": "20210824 14:15"
        },
        {
          "id": "MDRW2100011435.1.4",
          "form": "한식이여",
          "original_form": "한식이여",
          "speaker_id": "1",
          "time": "20210824 20:38"
        },
        {
          "id": "MDRW2100011435.1.5",
          "form": "맛있게 드셨나요",
          "original_form": "맛있게 드셨나요",
          "speaker_id": "2",
          "time": "20210824 20:50"
        },
        {
          "id": "MDRW2100011435.1.6",
          "form": "넵 밥심이죠",
          "original_form": "넵 밥심이죠",
          "speaker_id": "1",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100011435.1.7",
          "form": "그쵸 밥 잘챙겨먹어야죠",
          "original_form": "그쵸 밥 잘챙겨먹어야죠",
          "speaker_id": "2",
          "time": "20210825 19:02"
        },
        {
          "id": "MDRW2100011435.1.8",
          "form": "뭐좋아하세요",
          "original_form": "뭐좋아하세요",
          "speaker_id": "2",
          "time": "20210825 19:02"
        },
        {
          "id": "MDRW2100011435.1.9",
          "form": "치킨이요",
          "original_form": "치킨이요",
          "speaker_id": "1",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100011435.1.10",
          "form": "오 어디꺼요",
          "original_form": "오 어디꺼요",
          "speaker_id": "2",
          "time": "20210826 11:58"
        },
        {
          "id": "MDRW2100011435.1.11",
          "form": "네네요",
          "original_form": "네네요",
          "speaker_id": "1",
          "time": "20210826 12:45"
        },
        {
          "id": "MDRW2100011435.1.12",
          "form": "네네 맛있죠",
          "original_form": "네네 맛있죠",
          "speaker_id": "2",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100011435.1.13",
          "form": "후라이드?",
          "original_form": "후라이드?",
          "speaker_id": "2",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100011435.1.14",
          "form": "네 후라이드",
          "original_form": "네 후라이드",
          "speaker_id": "1",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100011435.1.15",
          "form": "네네는 후라이드가 맛있죠",
          "original_form": "네네는 후라이드가 맛있죠",
          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100011435.1.16",
          "form": "치즈볼같은건 안드세요?",
          "original_form": "치즈볼같은건 안드세요?",
          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100011435.1.17",
          "form": "아 것도 좋죠",
          "original_form": "아 것도 좋죠",
          "speaker_id": "1",
          "time": "20210826 18:47"
        },
        {
          "id": "MDRW2100011435.1.18",
          "form": "콜라는 어디께 좋으새요",
          "original_form": "콜라는 어디께 좋으새요",
          "speaker_id": "2",
          "time": "20210826 18:48"
