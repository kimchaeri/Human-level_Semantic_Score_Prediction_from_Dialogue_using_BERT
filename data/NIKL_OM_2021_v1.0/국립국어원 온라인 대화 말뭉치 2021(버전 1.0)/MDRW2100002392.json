      "utterance": [
        {
          "id": "MDRW2100002392.1.1",
          "form": "먹는거 좋아해?",
          "original_form": "먹는거 좋아해?",
          "speaker_id": "2",
          "time": "20210820 11:27"
        },
        {
          "id": "MDRW2100002392.1.2",
          "form": "먹는거 좋인하지",
          "original_form": "먹는거 좋인하지",
          "speaker_id": "1",
          "time": "20210820 11:43"
        },
        {
          "id": "MDRW2100002392.1.3",
          "form": "요즘 다이어트한다고 샐러드 먹는데",
          "original_form": "요즘 다이어트한다고 샐러드 먹는데",
          "speaker_id": "1",
          "time": "20210820 11:43"
        },
        {
          "id": "MDRW2100002392.1.4",
          "form": "샐러드마저 맛있어",
          "original_form": "샐러드마저 맛있어",
          "speaker_id": "1",
          "time": "20210820 11:43"
        },
        {
          "id": "MDRW2100002392.1.5",
          "form": "헐 샐러드가 왜 맛있어 ㅠ \n다이어트용 샐러드 말하는거 아니야?",
          "original_form": "헐 샐러드가 왜 맛있어 ㅠ \n다이어트용 샐러드 말하는거 아니야?",
          "speaker_id": "2",
          "time": "20210820 11:44"
        },
        {
          "id": "MDRW2100002392.1.6",
          "form": "파리바게뜨에서 샐러드 샀는데 생각보다 맛이 좋은데",
          "original_form": "파리바게뜨에서 샐러드 샀는데 생각보다 맛이 좋은데",
          "speaker_id": "1",
          "time": "20210820 11:50"
        },
        {
          "id": "MDRW2100002392.1.7",
          "form": "그런데 이것만 먹으면 오후에 너무 배고파",
          "original_form": "그런데 이것만 먹으면 오후에 너무 배고파",
          "speaker_id": "1",
          "time": "20210820 11:50"
        },
        {
          "id": "MDRW2100002392.1.8",
          "form": "좋아하는 음식은 뭐야?",
          "original_form": "좋아하는 음식은 뭐야?",
          "speaker_id": "1",
          "time": "20210820 11:50"
        },
        {
          "id": "MDRW2100002392.1.9",
          "form": "간이 적당히 되어있어?",
          "original_form": "간이 적당히 되어있어?",
          "speaker_id": "2",
          "time": "20210820 12:17"
        },
        {
          "id": "MDRW2100002392.1.10",
          "form": "그거 먹고 일하는거야? 쓰러지겠어 ㅠ",
          "original_form": "그거 먹고 일하는거야? 쓰러지겠어 ㅠ",
          "speaker_id": "2",
          "time": "20210820 12:17"
        },
        {
          "id": "MDRW2100002392.1.11",
          "form": "나는 한식파야 넌",
          "original_form": "나는 한식파야 넌",
          "speaker_id": "2",
          "time": "20210820 12:17"
        },
        {
          "id": "MDRW2100002392.1.12",
          "form": "나는 가리지않고 다 잘먹는 편이야",
          "original_form": "나는 가리지않고 다 잘먹는 편이야",
          "speaker_id": "1",
          "time": "20210820 12:32"
        },
        {
          "id": "MDRW2100002392.1.13",
          "form": "특별시 좋아하는 음식을 고르지는 못하겠어",
          "original_form": "특별시 좋아하는 음식을 고르지는 못하겠어",
          "speaker_id": "1",
          "time": "20210820 12:32"
        },
        {
          "id": "MDRW2100002392.1.14",
          "form": "가끔 요리하는 것도 좋아하는데",
          "original_form": "가끔 요리하는 것도 좋아하는데",
          "speaker_id": "1",
          "time": "20210820 12:32"
        },
        {
          "id": "MDRW2100002392.1.15",
          "form": "얼마전에는 집에 감자가 많이 있어서 감자수프를 만들었었어",
          "original_form": "얼마전에는 집에 감자가 많이 있어서 감자수프를 만들었었어",
          "speaker_id": "1",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002392.1.16",
          "form": "우와 감자스프 담백하니 맛있겠다",
          "original_form": "우와 감자스프 담백하니 맛있겠다",
          "speaker_id": "2",
          "time": "20210820 12:35"
        },
        {
          "id": "MDRW2100002392.1.17",
          "form": "나는 감자전이랑 고로케밖에 생각못했는데",
          "original_form": "나는 감자전이랑 고로케밖에 생각못했는데",
          "speaker_id": "2",
          "time": "20210820 12:36"
        },
        {
          "id": "MDRW2100002392.1.18",
          "form": "유튜브보니까 쉬워보여서 감자소비하려고 해봤어",
          "original_form": "유튜브보니까 쉬워보여서 감자소비하려고 해봤어",
          "speaker_id": "1",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002392.1.19",
          "form": "최근에 요리한적 있어?",
          "original_form": "최근에 요리한적 있어?",
          "speaker_id": "1",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002392.1.20",
          "form": "나야 주부라 늘 하지 ㅎ",
          "original_form": "나야 주부라 늘 하지 ㅎ",
          "speaker_id": "2",
          "time": "20210820 13:39"
        },
        {
          "id": "MDRW2100002392.1.21",
          "form": "하기싫어도 출근해야하는 곳이 부엌이야 ㅠ",
          "original_form": "하기싫어도 출근해야하는 곳이 부엌이야 ㅠ",
          "speaker_id": "2",
          "time": "20210820 13:40"
        },
        {
          "id": "MDRW2100002392.1.22",
          "form": "아 그렇구나",
          "original_form": "아 그렇구나",
          "speaker_id": "1",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002392.1.23",
          "form": "여름에는 더워서 요리할때 땀나고 힘들던데",
          "original_form": "여름에는 더워서 요리할때 땀나고 힘들던데",
          "speaker_id": "1",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002392.1.24",
          "form": "고생이다",
          "original_form": "고생이다",
          "speaker_id": "1",
          "time": "20210820 13:45"
        },
        {
          "id": "MDRW2100002392.1.25",
          "form": "맞아 조금만 불앞에 서있어도 땀이 줄줄",
          "original_form": "맞아 조금만 불앞에 서있어도 땀이 줄줄",
          "speaker_id": "2",
          "time": "20210820 13:50"
        },
        {
          "id": "MDRW2100002392.1.26",
          "form": "그래도 애들이 밥때 놓치면 안되니까 ㅎ",
          "original_form": "그래도 애들이 밥때 놓치면 안되니까 ㅎ",
          "speaker_id": "2",
          "time": "20210820 13:50"
        },
        {
          "id": "MDRW2100002392.1.27",
          "form": "요리하는거 좋아할까?",
          "original_form": "요리하는거 좋아할까?",
          "speaker_id": "2",
          "time": "20210820 13:50"
        },
        {
          "id": "MDRW2100002392.1.28",
          "form": "가끔하는건 즐겁고 재밌지",
          "original_form": "가끔하는건 즐겁고 재밌지",
          "speaker_id": "1",
          "time": "20210820 13:53"
        },
        {
          "id": "MDRW2100002392.1.29",
          "form": "하지만 매일하라고하면 너무 힘들고 즐겁지 않을 것같아",
          "original_form": "하지만 매일하라고하면 너무 힘들고 즐겁지 않을 것같아",
          "speaker_id": "1",
          "time": "20210820 13:53"
        },
        {
          "id": "MDRW2100002392.1.30",
          "form": "맞아 어쩔땐 짜증도 나",
          "original_form": "맞아 어쩔땐 짜증도 나",
          "speaker_id": "2",
          "time": "20210820 15:31"
        },
        {
          "id": "MDRW2100002392.1.31",
          "form": "요리 취미로 재밌게 하는 사람들 대단해",
          "original_form": "요리 취미로 재밌게 하는 사람들 대단해",
          "speaker_id": "2",
          "time": "20210820 15:32"
        }
      ]
    }
