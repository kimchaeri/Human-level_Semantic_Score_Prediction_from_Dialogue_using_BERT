      "utterance": [
        {
          "id": "MDRW2100045915.1.1",
          "form": "중고거래 해보신 적 있나요?",
          "original_form": "중고거래 해보신 적 있나요?",
          "speaker_id": "2",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100045915.1.2",
          "form": "네 있어요",
          "original_form": "네 있어요",
          "speaker_id": "1",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100045915.1.3",
          "form": "중나 당근 두루두루 써요",
          "original_form": "중나 당근 두루두루 써요",
          "speaker_id": "1",
          "time": "20210908 08:39"
        },
        {
          "id": "MDRW2100045915.1.4",
          "form": "중고거래하세요?",
          "original_form": "중고거래하세요?",
          "speaker_id": "1",
          "time": "20210908 08:39"
        },
        {
          "id": "MDRW2100045915.1.5",
          "form": "네 저는 몇번 팔아봤어요!",
          "original_form": "네 저는 몇번 팔아봤어요!",
          "speaker_id": "2",
          "time": "20210908 08:41"
        },
        {
          "id": "MDRW2100045915.1.6",
          "form": "저는 팔기도하고 사기도하고",
          "original_form": "저는 팔기도하고 사기도하고",
          "speaker_id": "1",
          "time": "20210908 08:41"
        },
        {
          "id": "MDRW2100045915.1.7",
          "form": "사기당할뻔도했어요 ㅎ",
          "original_form": "사기당할뻔도했어요 ㅎ",
          "speaker_id": "1",
          "time": "20210908 08:41"
        },
        {
          "id": "MDRW2100045915.1.8",
          "form": "허얼 사기요?",
          "original_form": "허얼 사기요?",
          "speaker_id": "2",
          "time": "20210908 08:44"
        },
        {
          "id": "MDRW2100045915.1.9",
          "form": "어쩌다가요..?ㅠ",
          "original_form": "어쩌다가요..?ㅠ",
          "speaker_id": "2",
          "time": "20210908 08:44"
        },
        {
          "id": "MDRW2100045915.1.10",
          "form": "아이책사려는데 괜찮은거같아 연락했죠",
          "original_form": "아이책사려는데 괜찮은거같아 연락했죠",
          "speaker_id": "1",
          "time": "20210908 08:45"
        },
        {
          "id": "MDRW2100045915.1.11",
          "form": "주소 입금계좌번회등등 다받고는",
          "original_form": "주소 입금계좌번회등등 다받고는",
          "speaker_id": "1",
          "time": "20210908 08:45"
        },
        {
          "id": "MDRW2100045915.1.12",
          "form": "입금하려는데 안심결제라서 수수료는 제가",
          "original_form": "입금하려는데 안심결제라서 수수료는 제가",
          "speaker_id": "1",
          "time": "20210908 08:47"
        },
        {
          "id": "MDRW2100045915.1.13",
          "form": "같이 입금해야된데요",
          "original_form": "같이 입금해야된데요",
          "speaker_id": "1",
          "time": "20210908 08:47"
        },
        {
          "id": "MDRW2100045915.1.14",
          "form": "원래 자동차감되야하는거아니예요?",
          "original_form": "원래 자동차감되야하는거아니예요?",
          "speaker_id": "1",
          "time": "20210908 08:47"
        },
        {
          "id": "MDRW2100045915.1.15",
          "form": "그때부터 이상하다싶다가",
          "original_form": "그때부터 이상하다싶다가",
          "speaker_id": "1",
          "time": "20210908 08:48"
        },
        {
          "id": "MDRW2100045915.1.16",
          "form": "책 주인은 거제라는데",
          "original_form": "책 주인은 거제라는데",
          "speaker_id": "1",
          "time": "20210908 08:48"
        },
        {
          "id": "MDRW2100045915.1.17",
          "form": "은행은 광주은행???",
          "original_form": "은행은 광주은행???",
          "speaker_id": "1",
          "time": "20210908 08:48"
        },
        {
          "id": "MDRW2100045915.1.18",
          "form": "에엥 ㅡㅡ",
          "original_form": "에엥 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100045915.1.19",
          "form": "그때부터 먼가 삘이...",
          "original_form": "그때부터 먼가 삘이...",
          "speaker_id": "1",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100045915.1.20",
          "form": "이상하다싶었죠",
          "original_form": "이상하다싶었죠",
          "speaker_id": "1",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100045915.1.21",
          "form": "진짜 구리네요",
          "original_form": "진짜 구리네요",
          "speaker_id": "2",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100045915.1.22",
          "form": "무슨 안심결제수수료를 보내라곸 ㅋㅋㅋㅋ",
          "original_form": "무슨 안심결제수수료를 보내라곸 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100045915.1.23",
          "form": "그래서 사진 좀보내달라니까",
          "original_form": "그래서 사진 좀보내달라니까",
          "speaker_id": "1",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045915.1.24",
          "form": "중나에 사진 그대로더라구요",
          "original_form": "중나에 사진 그대로더라구요",
          "speaker_id": "1",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045915.1.25",
          "form": "그래서 거래안한다고 했죠ㅎ",
          "original_form": "그래서 거래안한다고 했죠ㅎ",
          "speaker_id": "1",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045915.1.26",
          "form": "그리고 글도 삭제되고",
          "original_form": "그리고 글도 삭제되고",
          "speaker_id": "1",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100045915.1.27",
          "form": "돈 25마넌 날릴뻔했던 아찔한 ㅋ",
          "original_form": "돈 25마넌 날릴뻔했던 아찔한 ㅋ",
          "speaker_id": "1",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100045915.1.28",
          "form": "진짜;;",
          "original_form": "진짜;;",
          "speaker_id": "2",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100045915.1.29",
          "form": "그래도 바로 눈치채셔서 다행이에요",
          "original_form": "그래도 바로 눈치채셔서 다행이에요",
          "speaker_id": "2",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100045915.1.30",
          "form": "우쩜 그리 개념없는 인간들이 많은지..",
          "original_form": "우쩜 그리 개념없는 인간들이 많은지..",
          "speaker_id": "2",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100045915.1.31",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100045915.1.32",
          "form": "직접 발로뛰어서 벌어야지",
          "original_form": "직접 발로뛰어서 벌어야지",
          "speaker_id": "2",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100045915.1.33",
          "form": "그 책을 키워드알람설정해놨더니",
          "original_form": "그 책을 키워드알람설정해놨더니",
          "speaker_id": "1",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100045915.1.34",
          "form": "침대에 누워서 남 뒤통수 때려서 돈 벌 생각만 ^^",
          "original_form": "침대에 누워서 남 뒤통수 때려서 돈 벌 생각만 ^^",
          "speaker_id": "2",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100045915.1.35",
          "form": "그분 다른아뒤로 같은사진들이 같은글내용으로",
          "original_form": "그분 다른아뒤로 같은사진들이 같은글내용으로",
          "speaker_id": "1",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100045915.1.36",
          "form": "똑같이 올라오더라구요",
          "original_form": "똑같이 올라오더라구요",
          "speaker_id": "1",
          "time": "20210908 08:53"
        }
      ]
