      "utterance": [
        {
          "id": "MDRW2100044263.1.1",
          "form": "학교생활은 어떠샸나요",
          "original_form": "학교생활은 어떠샸나요",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044263.1.2",
          "form": "전 즐거웠어요! name1님은요?",
          "original_form": "전 즐거웠어요! &name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044263.1.3",
          "form": "저도요ㅎ 특히 즐거운 경험룬요?",
          "original_form": "저도요ㅎ 특히 즐거운 경험룬요?",
          "speaker_id": "1",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100044263.1.4",
          "form": "저 야자 째고 놀때요!",
          "original_form": "저 야자 째고 놀때요!",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044263.1.5",
          "form": "아 재밌었겠네요",
          "original_form": "아 재밌었겠네요",
          "speaker_id": "1",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044263.1.6",
          "form": "네! 그냥 학교에서 친구만나서 학교안에서 노는 것보다 재밋었어요 ㅎㅎ",
          "original_form": "네! 그냥 학교에서 친구만나서 학교안에서 노는 것보다 재밋었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044263.1.7",
          "form": "그쵸 아무래도 학교밖이니까",
          "original_form": "그쵸 아무래도 학교밖이니까",
          "speaker_id": "1",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044263.1.8",
          "form": "네! 다른친구들 다 공부하고 있어서 그런가 더 재밌더라고요 ㅋㅋㅋㅋㅎㅎ",
          "original_form": "네! 다른친구들 다 공부하고 있어서 그런가 더 재밌더라고요 ㅋㅋㅋㅋㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044263.1.9",
          "form": "즐거웠겠어요",
          "original_form": "즐거웠겠어요",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044263.1.10",
          "form": "학교수엊은 잘 듣는 편인가요?",
          "original_form": "학교수엊은 잘 듣는 편인가요?",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044263.1.11",
          "form": "잘...들으려고 하는 편이었어요!",
          "original_form": "잘...들으려고 하는 편이었어요!",
          "speaker_id": "2",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044263.1.12",
          "form": "ㅎㅎ 그래요?",
          "original_form": "ㅎㅎ 그래요?",
          "speaker_id": "1",
          "time": "20210907 22:47"
        },
        {
          "id": "MDRW2100044263.1.13",
          "form": "네 ㅋㅋ 안 졸려고 노력했답니다",
          "original_form": "네 ㅋㅋ 안 졸려고 노력했답니다",
          "speaker_id": "2",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100044263.1.14",
          "form": "ㅋㅋ 그거라도 어디예요",
          "original_form": "ㅋㅋ 그거라도 어디예요",
