      "utterance": [
        {
          "id": "MDRW2100006464.1.1",
          "form": "저도 샴푸가 별론거 같아요",
          "original_form": "저도 샴푸가 별론거 같아요",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100006464.1.2",
          "form": "샴푸도 그냥 그렇지만  흰머리가 나려나 봐",
          "original_form": "샴푸도 그냥 그렇지만  흰머리가 나려나 봐",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100006464.1.3",
          "form": "흰머리 슬퍼요ㅠㅠ",
          "original_form": "흰머리 슬퍼요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100006464.1.4",
          "form": "나이가 들면 자연스럽게 나는 거지",
          "original_form": "나이가 들면 자연스럽게 나는 거지",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100006464.1.5",
          "form": "그래도 염색하기 귀찮잖아요",
          "original_form": "그래도 염색하기 귀찮잖아요",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100006464.1.6",
          "form": "나도 염색하기 싫어",
          "original_form": "나도 염색하기 싫어",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100006464.1.7",
          "form": "안하면 회사에서 튀니까",
          "original_form": "안하면 회사에서 튀니까",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100006464.1.8",
          "form": "너무 나이 들어 보이니까",
          "original_form": "너무 나이 들어 보이니까",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100006464.1.9",
          "form": "근데 두피가 너무 간지러워 죽겠다",
          "original_form": "근데 두피가 너무 간지러워 죽겠다",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100006464.1.10",
          "form": "ㅜㅜ그러닊여",
          "original_form": "ㅜㅜ그러닊여",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100006464.1.11",
          "form": "두피샴푸라도 써볼까요",
          "original_form": "두피샴푸라도 써볼까요",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100006464.1.12",
          "form": "그게 효과가 클까",
          "original_form": "그게 효과가 클까",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100006464.1.13",
          "form": "클거에여",
          "original_form": "클거에여",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100006464.1.14",
          "form": "비싸기만 하고 효과가 없을까봐 쓰기가 그러네",
          "original_form": "비싸기만 하고 효과가 없을까봐 쓰기가 그러네",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100006464.1.15",
          "form": "괜찮지 않을까요?",
          "original_form": "괜찮지 않을까요?",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100006464.1.16",
          "form": "한번 써볼까",
          "original_form": "한번 써볼까",
