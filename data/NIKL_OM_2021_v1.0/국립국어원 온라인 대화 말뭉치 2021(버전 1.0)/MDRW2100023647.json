      "utterance": [
        {
          "id": "MDRW2100023647.1.1",
          "form": "이와중에 수욜에 놀러가는데",
          "original_form": "이와중에 수욜에 놀러가는데",
          "speaker_id": "2",
          "time": "20210830 13:42"
        },
        {
          "id": "MDRW2100023647.1.2",
          "form": "와우 목욜까지 비폭탄",
          "original_form": "와우 목욜까지 비폭탄",
          "speaker_id": "1",
          "time": "20210830 13:43"
        },
        {
          "id": "MDRW2100023647.1.3",
          "form": "맙소사",
          "original_form": "맙소사",
          "speaker_id": "2",
          "time": "20210830 13:43"
        },
        {
          "id": "MDRW2100023647.1.4",
          "form": "수영장을 이용해야하는데",
          "original_form": "수영장을 이용해야하는데",
          "speaker_id": "1",
          "time": "20210830 13:43"
        },
        {
          "id": "MDRW2100023647.1.5",
          "form": "ㅋㅋㅋ떠내려갈판",
          "original_form": "ㅋㅋㅋ떠내려갈판",
          "speaker_id": "2",
          "time": "20210830 13:44"
        },
        {
          "id": "MDRW2100023647.1.6",
          "form": "ㅎㅎㅎ 중부지방 100미리 이상이라는데",
          "original_form": "ㅎㅎㅎ 중부지방 100미리 이상이라는데",
          "speaker_id": "1",
          "time": "20210830 13:44"
        },
        {
          "id": "MDRW2100023647.1.7",
          "form": "ㅋㅋㅋ맙소사네",
          "original_form": "ㅋㅋㅋ맙소사네",
          "speaker_id": "2",
          "time": "20210830 13:45"
        },
        {
          "id": "MDRW2100023647.1.8",
          "form": "온도도 막 24도까지 내려가던데",
          "original_form": "온도도 막 24도까지 내려가던데",
          "speaker_id": "1",
          "time": "20210830 13:49"
        },
        {
          "id": "MDRW2100023647.1.9",
          "form": "이제 추워질껀가봐어",
          "original_form": "이제 추워질껀가봐어",
          "speaker_id": "2",
          "time": "20210830 13:50"
        },
        {
          "id": "MDRW2100023647.1.10",
          "form": "웅 추석이 다가오니..",
          "original_form": "웅 추석이 다가오니..",
          "speaker_id": "1",
          "time": "20210830 13:51"
        },
        {
          "id": "MDRW2100023647.1.11",
          "form": "추석 싫다싫어",
          "original_form": "추석 싫다싫어",
          "speaker_id": "2",
          "time": "20210830 13:59"
        },
        {
          "id": "MDRW2100023647.1.12",
          "form": "ㅋㅋ 작년엔 차려주신 점심만 먹고 집에왔는데",
          "original_form": "ㅋㅋ 작년엔 차려주신 점심만 먹고 집에왔는데",
          "speaker_id": "1",
          "time": "20210830 14:06"
        },
        {
          "id": "MDRW2100023647.1.13",
          "form": "벌써 날이 꾸물거리는구만",
          "original_form": "벌써 날이 꾸물거리는구만",
          "speaker_id": "1",
          "time": "20210830 14:29"
        },
        {
          "id": "MDRW2100023647.1.14",
          "form": "ㅋㅋㅋ작년과 같을수있는가",
          "original_form": "ㅋㅋㅋ작년과 같을수있는가",
          "speaker_id": "2",
          "time": "20210830 14:36"
        },
        {
          "id": "MDRW2100023647.1.15",
          "form": "모든것은 달라지기 마련이라고 ㅎㅎ",
          "original_form": "모든것은 달라지기 마련이라고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 14:47"
        },
        {
          "id": "MDRW2100023647.1.16",
          "form": "비가 추적추적",
          "original_form": "비가 추적추적",
          "speaker_id": "1",
          "time": "20210831 13:44"
        },
        {
          "id": "MDRW2100023647.1.17",
          "form": "생각보다 엄청 많이 오지는 않는구만",
          "original_form": "생각보다 엄청 많이 오지는 않는구만",
          "speaker_id": "2",
          "time": "20210831 14:56"
        },
        {
          "id": "MDRW2100023647.1.18",
          "form": "전라도쪽이 폭탄투하래",
          "original_form": "전라도쪽이 폭탄투하래",
          "speaker_id": "1",
          "time": "20210831 15:39"
        },
        {
          "id": "MDRW2100023647.1.19",
          "form": "ㅋㅋ낼은 덜왓으면",
          "original_form": "ㅋㅋ낼은 덜왓으면",
          "speaker_id": "2",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100023647.1.20",
          "form": "부쩍 추워졋어여",
          "original_form": "부쩍 추워졋어여",
          "speaker_id": "2",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100023647.1.21",
          "form": "한살 더 먹어보라고",
          "original_form": "한살 더 먹어보라고",
          "speaker_id": "1",
          "time": "20210831 16:44"
        },
        {
          "id": "MDRW2100023647.1.22",
          "form": "빨리 춥게 해주겠다고 ㅋㅋ",
          "original_form": "빨리 춥게 해주겠다고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 16:44"
        },
        {
          "id": "MDRW2100023647.1.23",
          "form": "ㅋㅋㅋㅋ호빵먹을 계절이 다가오지",
          "original_form": "ㅋㅋㅋㅋ호빵먹을 계절이 다가오지",
          "speaker_id": "2",
          "time": "20210831 16:46"
        },
        {
          "id": "MDRW2100023647.1.24",
          "form": "ㅎㅎㅎ 피자호빵 기다린다",
          "original_form": "ㅎㅎㅎ 피자호빵 기다린다",
          "speaker_id": "1",
          "time": "20210831 16:47"
        }
      ]
