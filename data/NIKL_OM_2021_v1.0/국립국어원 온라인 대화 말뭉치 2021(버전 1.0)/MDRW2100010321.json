      "utterance": [
        {
          "id": "MDRW2100010321.1.1",
          "form": "저녁 뭐드실건가요?",
          "original_form": "저녁 뭐드실건가요?",
          "speaker_id": "1",
          "time": "20210823 16:49"
        },
        {
          "id": "MDRW2100010321.1.2",
          "form": "라면이 땡겨요ㅎ",
          "original_form": "라면이 땡겨요ㅎ",
          "speaker_id": "2",
          "time": "20210823 16:52"
        },
        {
          "id": "MDRW2100010321.1.3",
          "form": "아 저도요",
          "original_form": "아 저도요",
          "speaker_id": "1",
          "time": "20210823 16:58"
        },
        {
          "id": "MDRW2100010321.1.4",
          "form": "무슨라면 좋아하시나요?",
          "original_form": "무슨라면 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210823 16:58"
        },
        {
          "id": "MDRW2100010321.1.5",
          "form": "전 너구리요ㅎ",
          "original_form": "전 너구리요ㅎ",
          "speaker_id": "2",
          "time": "20210823 16:58"
        },
        {
          "id": "MDRW2100010321.1.6",
          "form": "저는 열라면이요ㅋㅋ",
          "original_form": "저는 열라면이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:02"
        },
        {
          "id": "MDRW2100010321.1.7",
          "form": "감자면이랑",
          "original_form": "감자면이랑",
          "speaker_id": "1",
          "time": "20210823 17:02"
        },
        {
          "id": "MDRW2100010321.1.8",
          "form": "신라면도ㅋ",
          "original_form": "신라면도ㅋ",
          "speaker_id": "2",
          "time": "20210823 17:14"
        },
        {
          "id": "MDRW2100010321.1.9",
          "form": "저녁메뉴는 뭔가요?",
          "original_form": "저녁메뉴는 뭔가요?",
          "speaker_id": "1",
          "time": "20210823 17:25"
        },
        {
          "id": "MDRW2100010321.1.10",
          "form": "라면을 실행에 옮기려구요ㅋ",
          "original_form": "라면을 실행에 옮기려구요ㅋ",
          "speaker_id": "2",
          "time": "20210823 17:27"
        },
        {
          "id": "MDRW2100010321.1.11",
          "form": "좋네요ㅋㅋㅋ",
          "original_form": "좋네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:41"
        },
        {
          "id": "MDRW2100010321.1.12",
          "form": "신라면인가요?",
          "original_form": "신라면인가요?",
          "speaker_id": "1",
          "time": "20210823 17:41"
        },
        {
          "id": "MDRW2100010321.1.13",
          "form": "튀김우동이여ㅋ",
          "original_form": "튀김우동이여ㅋ",
          "speaker_id": "2",
          "time": "20210823 17:56"
        },
        {
          "id": "MDRW2100010321.1.14",
          "form": "맛있게드셨나요?",
          "original_form": "맛있게드셨나요?",
          "speaker_id": "1",
          "time": "20210824 10:38"
        },
        {
          "id": "MDRW2100010321.1.15",
          "form": "네 님은요?",
          "original_form": "네 님은요?",
          "speaker_id": "2",
          "time": "20210824 11:18"
        },
        {
          "id": "MDRW2100010321.1.16",
          "form": "네네 오늘 점심은",
          "original_form": "네네 오늘 점심은",
          "speaker_id": "1",
          "time": "20210824 11:22"
        },
        {
          "id": "MDRW2100010321.1.17",
          "form": "뭐드시나요?",
          "original_form": "뭐드시나요?",
          "speaker_id": "1",
          "time": "20210824 11:22"
        },
        {
          "id": "MDRW2100010321.1.18",
          "form": "오늘은 볶음밥요ㅎ",
          "original_form": "오늘은 볶음밥요ㅎ",
          "speaker_id": "2",
          "time": "20210824 11:33"
        }
      ]
