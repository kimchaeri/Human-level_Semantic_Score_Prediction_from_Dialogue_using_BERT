      "utterance": [
        {
          "id": "MDRW2100040835.1.1",
          "form": "중고거래 해봤나요?",
          "original_form": "중고거래 해봤나요?",
          "speaker_id": "2",
          "time": "20210905 09:36"
        },
        {
          "id": "MDRW2100040835.1.2",
          "form": "네 가끔씩 하고 있어요",
          "original_form": "네 가끔씩 하고 있어요",
          "speaker_id": "1",
          "time": "20210905 09:36"
        },
        {
          "id": "MDRW2100040835.1.3",
          "form": "저는 올해 처음 시작했어요",
          "original_form": "저는 올해 처음 시작했어요",
          "speaker_id": "2",
          "time": "20210905 09:40"
        },
        {
          "id": "MDRW2100040835.1.4",
          "form": "진작 할 걸 그랬어요",
          "original_form": "진작 할 걸 그랬어요",
          "speaker_id": "2",
          "time": "20210905 09:40"
        },
        {
          "id": "MDRW2100040835.1.5",
          "form": "기프티콘도 중고로 팔 수 있어요",
          "original_form": "기프티콘도 중고로 팔 수 있어요",
          "speaker_id": "1",
          "time": "20210905 09:40"
        },
        {
          "id": "MDRW2100040835.1.6",
          "form": "이벤트 당첨 된 거 중고로 팔았죠",
          "original_form": "이벤트 당첨 된 거 중고로 팔았죠",
          "speaker_id": "1",
          "time": "20210905 09:40"
        },
        {
          "id": "MDRW2100040835.1.7",
          "form": "저는 그건 안해봐서 쉽지 않을 듯 해요",
          "original_form": "저는 그건 안해봐서 쉽지 않을 듯 해요",
          "speaker_id": "2",
          "time": "20210905 09:42"
        },
        {
          "id": "MDRW2100040835.1.8",
          "form": "그냥 물건 직거래로 여러개 팔아봤어요",
          "original_form": "그냥 물건 직거래로 여러개 팔아봤어요",
          "speaker_id": "2",
          "time": "20210905 09:42"
        },
        {
          "id": "MDRW2100040835.1.9",
          "form": "기프티콘 파는 어플이 있어요",
          "original_form": "기프티콘 파는 어플이 있어요",
          "speaker_id": "1",
          "time": "20210905 09:43"
        },
        {
          "id": "MDRW2100040835.1.10",
          "form": "기프티스타 니콘내콘 이렇게 있어요",
          "original_form": "기프티스타 니콘내콘 이렇게 있어요",
          "speaker_id": "1",
          "time": "20210905 09:43"
        },
        {
          "id": "MDRW2100040835.1.11",
          "form": "어 처음 들어요",
          "original_form": "어 처음 들어요",
          "speaker_id": "2",
          "time": "20210905 09:46"
        },
        {
          "id": "MDRW2100040835.1.12",
          "form": "알아놔야겠네요",
          "original_form": "알아놔야겠네요",
          "speaker_id": "2",
          "time": "20210905 09:46"
        },
        {
          "id": "MDRW2100040835.1.13",
          "form": "네 여기서 팔면 편해요",
          "original_form": "네 여기서 팔면 편해요",
          "speaker_id": "1",
          "time": "20210905 09:46"
        },
        {
          "id": "MDRW2100040835.1.14",
          "form": "저도 종종 기프티콘을 받는데",
          "original_form": "저도 종종 기프티콘을 받는데",
          "speaker_id": "2",
          "time": "20210905 09:51"
        },
        {
          "id": "MDRW2100040835.1.15",
          "form": "꼭 사용하지 않아도 되는 것들이 있거든요",
          "original_form": "꼭 사용하지 않아도 되는 것들이 있거든요",
          "speaker_id": "2",
          "time": "20210905 09:51"
        },
        {
          "id": "MDRW2100040835.1.16",
          "form": "그런걸 팔면 좋겠네요",
          "original_form": "그런걸 팔면 좋겠네요",
          "speaker_id": "2",
          "time": "20210905 09:51"
        },
        {
          "id": "MDRW2100040835.1.17",
          "form": "네 그럴 때 팔면 좋을 거 같아요",
          "original_form": "네 그럴 때 팔면 좋을 거 같아요",
          "speaker_id": "1",
          "time": "20210905 09:51"
        },
        {
          "id": "MDRW2100040835.1.18",
          "form": "그리고 카톡 기프티콘은 유효기간 만료 되면 90% 환불 가능 해요",
          "original_form": "그리고 카톡 기프티콘은 유효기간 만료 되면 90% 환불 가능 해요",
          "speaker_id": "1",
          "time": "20210905 09:51"
        },
        {
          "id": "MDRW2100040835.1.19",
          "form": "근데 그렇게 환불받으면 선물 한 사람이 확인할 수 있어요",
          "original_form": "근데 그렇게 환불받으면 선물 한 사람이 확인할 수 있어요",
          "speaker_id": "1",
          "time": "20210905 09:52"
        },
        {
          "id": "MDRW2100040835.1.20",
          "form": "아 그런 기능도 있어요?",
          "original_form": "아 그런 기능도 있어요?",
          "speaker_id": "2",
          "time": "20210905 09:52"
        },
        {
          "id": "MDRW2100040835.1.21",
          "form": "네 선물 준 사람이 확인할 수 있어요",
