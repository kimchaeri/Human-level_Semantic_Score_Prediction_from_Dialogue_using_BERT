          "speaker_id": "1",
          "time": "20210902 20:00"
        },
        {
          "id": "MDRW2100028441.1.3",
          "form": "한숨만 나온다",
          "original_form": "한숨만 나온다",
          "speaker_id": "2",
          "time": "20210902 20:00"
        },
        {
          "id": "MDRW2100028441.1.4",
          "form": "아ㅋㅋㅋㅋ ㅠㅠ",
          "original_form": "아ㅋㅋㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 20:00"
        },
        {
          "id": "MDRW2100028441.1.5",
          "form": "지금 인턴급으로 일하고 있는게 스트레스지",
          "original_form": "지금 인턴급으로 일하고 있는게 스트레스지",
          "speaker_id": "2",
          "time": "20210902 20:00"
        },
        {
          "id": "MDRW2100028441.1.6",
          "form": "진짜 인수인계 좀 해주세요",
          "original_form": "진짜 인수인계 좀 해주세요",
          "speaker_id": "2",
          "time": "20210902 20:01"
        },
        {
          "id": "MDRW2100028441.1.7",
          "form": "거기로 취업나갈거 아니지",
          "original_form": "거기로 취업나갈거 아니지",
          "speaker_id": "1",
          "time": "20210902 20:01"
        },
        {
          "id": "MDRW2100028441.1.8",
          "form": "업무 할 때 메뉴얼 좀 주세요",
          "original_form": "업무 할 때 메뉴얼 좀 주세요",
          "speaker_id": "2",
          "time": "20210902 20:01"
        },
        {
          "id": "MDRW2100028441.1.9",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210902 20:01"
        },
        {
          "id": "MDRW2100028441.1.10",
          "form": "거기로 취업하다니 무슨 그런",
          "original_form": "거기로 취업하다니 무슨 그런",
          "speaker_id": "2",
          "time": "20210902 20:01"
        },
        {
          "id": "MDRW2100028441.1.11",
          "form": "단순 봉사 느낌인데",
          "original_form": "단순 봉사 느낌인데",
