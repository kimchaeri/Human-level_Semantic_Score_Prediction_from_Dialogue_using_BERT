      "utterance": [
        {
          "id": "MDRW2100013400.1.1",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "1",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013400.1.2",
          "form": "안녕하세요 하는 게임 있으신가요??",
          "original_form": "안녕하세요 하는 게임 있으신가요??",
          "speaker_id": "2",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013400.1.3",
          "form": "저는 꿈의정원만해요 ㅋㅋㅋ",
          "original_form": "저는 꿈의정원만해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013400.1.4",
          "form": "혹시 아시나여",
          "original_form": "혹시 아시나여",
          "speaker_id": "1",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013400.1.5",
          "form": "아 그 수염난 아저씨 나오는거아닌가요?",
          "original_form": "아 그 수염난 아저씨 나오는거아닌가요?",
          "speaker_id": "2",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013400.1.6",
          "form": "이름이 뭐였죠",
          "original_form": "이름이 뭐였죠",
          "speaker_id": "2",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013400.1.7",
          "form": "오스틴인가",
          "original_form": "오스틴인가",
          "speaker_id": "1",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013400.1.8",
          "form": "오 재밌나요?",
          "original_form": "오 재밌나요?",
          "speaker_id": "2",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100013400.1.9",
          "form": "저도  몇번 하다가 힘들어서 지웠네요",
          "original_form": "저도  몇번 하다가 힘들어서 지웠네요",
          "speaker_id": "2",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100013400.1.10",
          "form": "지금 한 5000ㄹ벨 쯤 깼는데",
          "original_form": "지금 한 5000ㄹ벨 쯤 깼는데",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013400.1.11",
          "form": "슬승 지겹고 어려워요",
          "original_form": "슬승 지겹고 어려워요",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013400.1.12",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013400.1.13",
          "form": "아 정원 진짜 이쁘겠네요",
          "original_form": "아 정원 진짜 이쁘겠네요",
          "speaker_id": "2",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013400.1.14",
          "form": "저는 쿠키런해요",
          "original_form": "저는 쿠키런해요",
          "speaker_id": "2",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013400.1.15",
          "form": "오 쿠키런 킹덤? 인가 그거요??",
          "original_form": "오 쿠키런 킹덤? 인가 그거요??",
          "speaker_id": "1",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100013400.1.16",
          "form": "네 인기많아요 ㅋㅋ",
          "original_form": "네 인기많아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100013400.1.17",
          "form": "재밌어요 왕국 꾸미기도 하고",
          "original_form": "재밌어요 왕국 꾸미기도 하고",
          "speaker_id": "2",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013400.1.18",
          "form": "저 그거 좀 하다가 너무 머가 많아서 어려워서 관뒀는데 주변에서는 재밌다 하다라고요 ㅋㅋㅋㅋ",
          "original_form": "저 그거 좀 하다가 너무 머가 많아서 어려워서 관뒀는데 주변에서는 재밌다 하다라고요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013400.1.19",
          "form": "뽑기하는게 재밌어요 ㅋㅋ",
          "original_form": "뽑기하는게 재밌어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013400.1.20",
          "form": "쿠키들도 귀엽고",
          "original_form": "쿠키들도 귀엽고",
          "speaker_id": "2",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013400.1.21",
          "form": "오 마자요 무슨 쿠키들 많더라고요 ㅋㅋㅋㅋㅋ",
          "original_form": "오 마자요 무슨 쿠키들 많더라고요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013400.1.22",
          "form": "여자 캐릭터 같은데 남자인 쿠키도 잇고 ㅋㅋㅋ",
          "original_form": "여자 캐릭터 같은데 남자인 쿠키도 잇고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013400.1.23",
          "form": "다양해요 ㅋㅋㅋㅋ맞아요",
          "original_form": "다양해요 ㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013400.1.24",
          "form": "게임할때 과금도 하나요?",
          "original_form": "게임할때 과금도 하나요?",
          "speaker_id": "2",
          "time": "20210825 16:13"
        }
      ]
    }
