      "utterance": [
        {
          "id": "MDRW2100002277.1.1",
          "form": "알바나 일하시나용~?",
          "original_form": "알바나 일하시나용~?",
          "speaker_id": "2",
          "time": "20210820 09:37"
        },
        {
          "id": "MDRW2100002277.1.2",
          "form": "네 슬픈 직장인입니당",
          "original_form": "네 슬픈 직장인입니당",
          "speaker_id": "1",
          "time": "20210820 11:58"
        },
        {
          "id": "MDRW2100002277.1.3",
          "form": "아영님도 일하시는지요",
          "original_form": "아영님도 일하시는지요",
          "speaker_id": "1",
          "time": "20210820 11:58"
        },
        {
          "id": "MDRW2100002277.1.4",
          "form": "네ㅠ 저는 프리랜서로 일하고 있습니다!!!",
          "original_form": "네ㅠ 저는 프리랜서로 일하고 있습니다!!!",
          "speaker_id": "2",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002277.1.5",
          "form": "그래도 직장가는거에 비해는 상사스트레스는 적은것같아요!",
          "original_form": "그래도 직장가는거에 비해는 상사스트레스는 적은것같아요!",
          "speaker_id": "2",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002277.1.6",
          "form": "프리랜서군요",
          "original_form": "프리랜서군요",
          "speaker_id": "1",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002277.1.7",
          "form": "집에서 일하는 프리이신가요?",
          "original_form": "집에서 일하는 프리이신가요?",
          "speaker_id": "1",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002277.1.8",
          "form": "네~ 재택근무 하고 있습니다~!!",
          "original_form": "네~ 재택근무 하고 있습니다~!!",
          "speaker_id": "2",
          "time": "20210820 16:26"
        },
        {
          "id": "MDRW2100002277.1.9",
          "form": "직장에 대해 스트레스는 없으신가요ㅠ~?",
          "original_form": "직장에 대해 스트레스는 없으신가요ㅠ~?",
          "speaker_id": "2",
          "time": "20210820 16:26"
        },
        {
          "id": "MDRW2100002277.1.10",
          "form": "재택을 안 시켜주는 게",
          "original_form": "재택을 안 시켜주는 게",
          "speaker_id": "1",
          "time": "20210820 18:08"
        },
        {
          "id": "MDRW2100002277.1.11",
          "form": "제일 스트레스예요 ㅠ",
          "original_form": "제일 스트레스예요 ㅠ",
          "speaker_id": "1",
          "time": "20210820 18:08"
        },
        {
          "id": "MDRW2100002277.1.12",
          "form": "아아ㅠㅠ 원래 이정도면 재택해야되는데",
          "original_form": "아아ㅠㅠ 원래 이정도면 재택해야되는데",
          "speaker_id": "2",
          "time": "20210820 18:29"
        },
        {
          "id": "MDRW2100002277.1.13",
          "form": "무조건 출근인가봐요ㅠ",
          "original_form": "무조건 출근인가봐요ㅠ",
          "speaker_id": "2",
          "time": "20210820 18:29"
        },
        {
          "id": "MDRW2100002277.1.14",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210820 18:33"
        },
        {
          "id": "MDRW2100002277.1.15",
          "form": "매일 지하철에서 모르는 다수와 밀접접촉중인데 ㅋㅋㅋ",
          "original_form": "매일 지하철에서 모르는 다수와 밀접접촉중인데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:34"
        },
        {
          "id": "MDRW2100002277.1.16",
          "form": "에고 진짜 그게 더 위험한데ㅠㅠ",
          "original_form": "에고 진짜 그게 더 위험한데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 20:13"
        },
        {
          "id": "MDRW2100002277.1.17",
          "form": "그럼 풀로 근무하시는건가요~(",
          "original_form": "그럼 풀로 근무하시는건가요~(",
          "speaker_id": "2",
          "time": "20210820 20:13"
        },
        {
          "id": "MDRW2100002277.1.18",
          "form": "네에  ㅠ",
          "original_form": "네에  ㅠ",
          "speaker_id": "1",
          "time": "20210820 20:15"
        },
        {
          "id": "MDRW2100002277.1.19",
          "form": "다른 부서는 주1-2회는 하는데 또 우리 부서장은 생각이 없어보이네요 전혀",
          "original_form": "다른 부서는 주1-2회는 하는데 또 우리 부서장은 생각이 없어보이네요 전혀",
          "speaker_id": "1",
          "time": "20210820 20:16"
        },
        {
          "id": "MDRW2100002277.1.20",
          "form": "프리 생활은 얼마나 하신 거예요?",
          "original_form": "프리 생활은 얼마나 하신 거예요?",
          "speaker_id": "1",
          "time": "20210820 20:16"
        },
        {
          "id": "MDRW2100002277.1.21",
          "form": "저는 1년정도 하고 있어요~ 한번 그쪽으로 구하다보니",
          "original_form": "저는 1년정도 하고 있어요~ 한번 그쪽으로 구하다보니",
          "speaker_id": "2",
          "time": "20210820 20:41"
        },
        {
          "id": "MDRW2100002277.1.22",
          "form": "계속 그런쪽으로 이어지더라구요~",
          "original_form": "계속 그런쪽으로 이어지더라구요~",
          "speaker_id": "2",
          "time": "20210820 20:41"
        },
        {
          "id": "MDRW2100002277.1.23",
          "form": "프리랜서 생활 할만하신가요? 저도 1년정도 햇었는데 능력이 부족해서 그런지 많이 힘들어서 다시 월급쟁이를 선택한 케이스라서 ㅋㅋ",
          "original_form": "프리랜서 생활 할만하신가요? 저도 1년정도 햇었는데 능력이 부족해서 그런지 많이 힘들어서 다시 월급쟁이를 선택한 케이스라서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 10:45"
        },
        {
          "id": "MDRW2100002277.1.24",
          "form": "음 매달 격차는 있지만 기본급은꾸준이 나오고ㅠ나머지는 제 선택으로ㅠ하는거니까",
          "original_form": "음 매달 격차는 있지만 기본급은꾸준이 나오고ㅠ나머지는 제 선택으로ㅠ하는거니까",
          "speaker_id": "2",
          "time": "20210823 10:47"
        },
        {
          "id": "MDRW2100002277.1.25",
          "form": "스트레스는 덜한것같아요~",
          "original_form": "스트레스는 덜한것같아요~",
          "speaker_id": "2",
          "time": "20210823 10:47"
        }
      ]
    }
