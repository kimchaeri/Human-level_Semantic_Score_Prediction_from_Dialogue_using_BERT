      "utterance": [
        {
          "id": "MDRW2100040297.1.1",
          "form": "네 제가 하진 않았지만 곡물도 섞었습니다",
          "original_form": "네 제가 하진 않았지만 곡물도 섞었습니다",
          "speaker_id": "2",
          "time": "20210904 15:43"
        },
        {
          "id": "MDRW2100040297.1.2",
          "form": "밥에 곡물도 섞으셨나요",
          "original_form": "밥에 곡물도 섞으셨나요",
          "speaker_id": "1",
          "time": "20210904 15:43"
        },
        {
          "id": "MDRW2100040297.1.3",
          "form": "다 알아들으니 걱정 마세요",
          "original_form": "다 알아들으니 걱정 마세요",
          "speaker_id": "2",
          "time": "20210904 15:43"
        },
        {
          "id": "MDRW2100040297.1.4",
          "form": "말이 너무 웃겨요",
          "original_form": "말이 너무 웃겨요",
          "speaker_id": "1",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100040297.1.5",
          "form": "저는 버튼만 눌렀어요",
          "original_form": "저는 버튼만 눌렀어요",
          "speaker_id": "2",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100040297.1.6",
          "form": "큰일을 하셨네요",
          "original_form": "큰일을 하셨네요",
          "speaker_id": "1",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100040297.1.7",
          "form": "된장찌개 배우고 고기 배우는 건 제가 해야 되겠죠",
          "original_form": "된장찌개 배우고 고기 배우는 건 제가 해야 되겠죠",
          "speaker_id": "2",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100040297.1.8",
          "form": "전화를 싸우는 것도 제가 해야 되겠죠",
          "original_form": "전화를 싸우는 것도 제가 해야 되겠죠",
          "speaker_id": "2",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100040297.1.9",
          "form": "에이요 제 요새에 피코크 제품 잘 나와요",
          "original_form": "에이요 제 요새에 피코크 제품 잘 나와요",
          "speaker_id": "1",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100040297.1.10",
          "form": "연어 회",
          "original_form": "연어 회",
          "speaker_id": "2",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100040297.1.11",
          "form": "전어회",
          "original_form": "전어회",
          "speaker_id": "2",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100040297.1.12",
          "form": "너무 맛있겠다",
          "original_form": "너무 맛있겠다",
          "speaker_id": "1",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100040297.1.13",
          "form": "피코크가 뭐예요",
          "original_form": "피코크가 뭐예요",
          "speaker_id": "2",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100040297.1.14",
          "form": "다 만들어져서 나오는 거",
          "original_form": "다 만들어져서 나오는 거",
          "speaker_id": "1",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100040297.1.15",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100040297.1.16",
          "form": "만들어 줘서 나오는 요리들은 정말 편리하죠",
          "original_form": "만들어 줘서 나오는 요리들은 정말 편리하죠",
          "speaker_id": "2",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100040297.1.17",
          "form": "어제 홈플러스에서 소고기   전골",
          "original_form": "어제 홈플러스에서 소고기   전골",
          "speaker_id": "1",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100040297.1.18",
          "form": "사서왔는데 너무편하고",
          "original_form": "사서왔는데 너무편하고",
          "speaker_id": "1",
          "time": "20210904 15:46"
        },
        {
          "id": "MDRW2100040297.1.19",
