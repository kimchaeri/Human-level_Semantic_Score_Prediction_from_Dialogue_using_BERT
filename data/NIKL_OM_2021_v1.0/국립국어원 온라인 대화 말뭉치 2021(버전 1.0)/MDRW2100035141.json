      "utterance": [
        {
          "id": "MDRW2100035141.1.1",
          "form": "근데 저는 심즈 하다보면",
          "original_form": "근데 저는 심즈 하다보면",
          "speaker_id": "1",
          "time": "20210829 06:51"
        },
        {
          "id": "MDRW2100035141.1.2",
          "form": "한참 재밌다가도",
          "original_form": "한참 재밌다가도",
          "speaker_id": "1",
          "time": "20210829 06:51"
        },
        {
          "id": "MDRW2100035141.1.3",
          "form": "갑자기 질리는 순간이 와요",
          "original_form": "갑자기 질리는 순간이 와요",
          "speaker_id": "1",
          "time": "20210829 06:51"
        },
        {
          "id": "MDRW2100035141.1.4",
          "form": "넹넹 아아 질리는 타이밍이 와요ㅋㅋ",
          "original_form": "넹넹 아아 질리는 타이밍이 와요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 06:51"
        },
        {
          "id": "MDRW2100035141.1.5",
          "form": "앜ㅋㅋㅋㅋㅋㅋ공감ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ공감ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 06:51"
        },
        {
          "id": "MDRW2100035141.1.6",
          "form": "같은 루틴이라",
          "original_form": "같은 루틴이라",
          "speaker_id": "2",
          "time": "20210829 06:51"
        },
        {
          "id": "MDRW2100035141.1.7",
          "form": "그래서 한동안 안 보다가 또 갑자기 몇년 주기로",
          "original_form": "그래서 한동안 안 보다가 또 갑자기 몇년 주기로",
          "speaker_id": "1",
          "time": "20210829 06:51"
        },
        {
          "id": "MDRW2100035141.1.8",
          "form": "접속하고",
          "original_form": "접속하고",
          "speaker_id": "1",
          "time": "20210829 06:51"
        },
        {
          "id": "MDRW2100035141.1.9",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 06:51"
        },
        {
          "id": "MDRW2100035141.1.10",
          "form": "질릴땨까지하고",
          "original_form": "질릴땨까지하고",
          "speaker_id": "2",
          "time": "20210829 06:52"
        },
        {
          "id": "MDRW2100035141.1.11",
          "form": "한동안 안하다가 다시 생각나면 하고 ㅡㅋㅋ",
          "original_form": "한동안 안하다가 다시 생각나면 하고 ㅡㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 06:52"
        },
        {
          "id": "MDRW2100035141.1.12",
          "form": "모바일에서는 배속기능 없어요?",
          "original_form": "모바일에서는 배속기능 없어요?",
          "speaker_id": "1",
          "time": "20210829 06:52"
        },
        {
          "id": "MDRW2100035141.1.13",
          "form": "네네ㅠㅠ 없어용",
          "original_form": "네네ㅠㅠ 없어용",
          "speaker_id": "2",
          "time": "20210829 06:52"
        },
        {
          "id": "MDRW2100035141.1.14",
          "form": "아 근데 저번에",
          "original_form": "아 근데 저번에",
          "speaker_id": "2",
          "time": "20210829 06:52"
        },
        {
          "id": "MDRW2100035141.1.15",
          "form": "현실시간대로 흐르몀 너무 오래걸리잖아요ㅜ",
          "original_form": "현실시간대로 흐르몀 너무 오래걸리잖아요ㅜ",
          "speaker_id": "1",
          "time": "20210829 06:52"
        },
        {
          "id": "MDRW2100035141.1.16",
          "form": "오류나서 제 계정 초기화 됐는데",
          "original_form": "오류나서 제 계정 초기화 됐는데",
          "speaker_id": "2",
          "time": "20210829 06:52"
        },
        {
          "id": "MDRW2100035141.1.17",
          "form": "결제까지해서 이력 남아있을텐데",
          "original_form": "결제까지해서 이력 남아있을텐데",
          "speaker_id": "2",
          "time": "20210829 06:52"
        },
        {
          "id": "MDRW2100035141.1.18",
          "form": "ea 게임사에서 복구를 안해주셔서",
          "original_form": "ea 게임사에서 복구를 안해주셔서",
          "speaker_id": "2",
          "time": "20210829 06:52"
        },
        {
          "id": "MDRW2100035141.1.19",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 06:52"
        },
        {
          "id": "MDRW2100035141.1.20",
          "form": "아.....문의했는데",
          "original_form": "아.....문의했는데",
          "speaker_id": "1",
          "time": "20210829 06:54"
        },
        {
          "id": "MDRW2100035141.1.21",
          "form": "복구안된대요?",
          "original_form": "복구안된대요?",
          "speaker_id": "1",
          "time": "20210829 06:54"
        },
        {
          "id": "MDRW2100035141.1.22",
          "form": "그럴때 진짜......하....",
          "original_form": "그럴때 진짜......하....",
          "speaker_id": "1",
          "time": "20210829 06:54"
        },
        {
          "id": "MDRW2100035141.1.23",
          "form": "네네ㅠㅠ 문의했는데 복구 안되가지고",
          "original_form": "네네ㅠㅠ 문의했는데 복구 안되가지고",
          "speaker_id": "2",
          "time": "20210829 06:54"
        },
        {
          "id": "MDRW2100035141.1.24",
          "form": "게임 캐릭터 처음부터 다시 키워야해서",
          "original_form": "게임 캐릭터 처음부터 다시 키워야해서",
          "speaker_id": "2",
          "time": "20210829 06:54"
        },
        {
          "id": "MDRW2100035141.1.25",
          "form": "ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 06:54"
        },
        {
          "id": "MDRW2100035141.1.26",
          "form": "저도 꿈의집 한창하다가 새폰으로 다시 연동시켜봤는데",
          "original_form": "저도 꿈의집 한창하다가 새폰으로 다시 연동시켜봤는데",
          "speaker_id": "1",
          "time": "20210829 06:54"
        },
        {
          "id": "MDRW2100035141.1.27",
          "form": "아아 요때가 가장 위험한것같아요ㅠㅠ",
          "original_form": "아아 요때가 가장 위험한것같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 06:54"
        },
        {
          "id": "MDRW2100035141.1.28",
          "form": "거의 초반레벨이길래 문의했는데 답이 없어요ㅠ",
          "original_form": "거의 초반레벨이길래 문의했는데 답이 없어요ㅠ",
          "speaker_id": "1",
          "time": "20210829 06:54"
        },
        {
          "id": "MDRW2100035141.1.29",
          "form": "연동하거나 새로운 기기로 접속할때",
          "original_form": "연동하거나 새로운 기기로 접속할때",
          "speaker_id": "2",
          "time": "20210829 06:55"
        },
        {
          "id": "MDRW2100035141.1.30",
          "form": "저도 연동하다가 그렇게 됐어용ㅋㅋㅋ",
          "original_form": "저도 연동하다가 그렇게 됐어용ㅋㅋㅋ",
          "speaker_id": "2",
