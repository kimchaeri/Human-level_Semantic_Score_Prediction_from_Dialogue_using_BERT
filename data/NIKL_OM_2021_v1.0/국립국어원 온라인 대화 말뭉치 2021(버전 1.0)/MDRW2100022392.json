      "utterance": [
        {
          "id": "MDRW2100022392.1.1",
          "form": "여행조아하시나요",
          "original_form": "여행조아하시나요",
          "speaker_id": "1",
          "time": "20210829 13:20"
        },
        {
          "id": "MDRW2100022392.1.2",
          "form": "여행 좋죠! 근데 멀리는 가본적이 없네용",
          "original_form": "여행 좋죠! 근데 멀리는 가본적이 없네용",
          "speaker_id": "2",
          "time": "20210829 13:24"
        },
        {
          "id": "MDRW2100022392.1.3",
          "form": "어디가보셨나요",
          "original_form": "어디가보셨나요",
          "speaker_id": "1",
          "time": "20210829 13:25"
        },
        {
          "id": "MDRW2100022392.1.4",
          "form": "경주 제주 서울 밀양 부산 전주 순천",
          "original_form": "경주 제주 서울 밀양 부산 전주 순천",
          "speaker_id": "2",
          "time": "20210829 13:26"
        },
        {
          "id": "MDRW2100022392.1.5",
          "form": "주로 국내여행이요",
          "original_form": "주로 국내여행이요",
          "speaker_id": "2",
          "time": "20210829 13:27"
        },
        {
          "id": "MDRW2100022392.1.6",
          "form": "제주너무좋죠~",
          "original_form": "제주너무좋죠~",
          "speaker_id": "1",
          "time": "20210829 13:28"
        },
        {
          "id": "MDRW2100022392.1.7",
          "form": "맞아요 제주 진짜 좋았어요! 근데 뚜벅이로 가서 좀 힘들더라구요",
          "original_form": "맞아요 제주 진짜 좋았어요! 근데 뚜벅이로 가서 좀 힘들더라구요",
          "speaker_id": "2",
          "time": "20210829 13:30"
        },
        {
          "id": "MDRW2100022392.1.8",
          "form": "제주는 역시 차가있어야",
          "original_form": "제주는 역시 차가있어야",
          "speaker_id": "1",
          "time": "20210829 13:33"
        },
        {
          "id": "MDRW2100022392.1.9",
          "form": "네 맞아요ㅜㅜ 진짜 버스도 잘 없구 많이 걸어야되요",
          "original_form": "네 맞아요ㅜㅜ 진짜 버스도 잘 없구 많이 걸어야되요",
          "speaker_id": "2",
          "time": "20210829 13:34"
        },
        {
          "id": "MDRW2100022392.1.10",
          "form": "섭지코지 넘 좋았어요",
          "original_form": "섭지코지 넘 좋았어요",
          "speaker_id": "1",
          "time": "20210829 13:37"
        },
        {
          "id": "MDRW2100022392.1.11",
          "form": "거기는 못 가봤네요ㅜㅜ 저 사려니숲이랑 분필모양으로 깎인 돌 있는곳 같었는데 넘 좋았어요!",
          "original_form": "거기는 못 가봤네요ㅜㅜ 저 사려니숲이랑 분필모양으로 깎인 돌 있는곳 같었는데 넘 좋았어요!",
          "speaker_id": "2",
          "time": "20210829 13:41"
        },
        {
          "id": "MDRW2100022392.1.12",
          "form": "사려니숲도 유명하죠!",
          "original_form": "사려니숲도 유명하죠!",
          "speaker_id": "1",
          "time": "20210829 13:48"
        },
        {
          "id": "MDRW2100022392.1.13",
          "form": "네 산책하니까 너무 좋더라구요",
          "original_form": "네 산책하니까 너무 좋더라구요",
          "speaker_id": "2",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100022392.1.14",
          "form": "제주또가고싶네요",
          "original_form": "제주또가고싶네요",
          "speaker_id": "1",
          "time": "20210829 14:06"
        }
      ]
