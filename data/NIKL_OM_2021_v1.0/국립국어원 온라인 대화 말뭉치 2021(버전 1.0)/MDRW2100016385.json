      "utterance": [
        {
          "id": "MDRW2100016385.1.1",
          "form": "스키 좋아하세요??",
          "original_form": "스키 좋아하세요??",
          "speaker_id": "1",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100016385.1.2",
          "form": "넘어져있어요",
          "original_form": "넘어져있어요",
          "speaker_id": "1",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100016385.1.3",
          "form": "ㅇㄴ 지금도 운동중입니다",
          "original_form": "ㅇㄴ 지금도 운동중입니다",
          "speaker_id": "1",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100016385.1.4",
          "form": "무슨 운동 중 이십니까",
          "original_form": "무슨 운동 중 이십니까",
          "speaker_id": "2",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100016385.1.5",
          "form": "유산소 중입니다",
          "original_form": "유산소 중입니다",
          "speaker_id": "1",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100016385.1.6",
          "form": "스태퍼하다가 자전거타고있어요",
          "original_form": "스태퍼하다가 자전거타고있어요",
          "speaker_id": "1",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100016385.1.7",
          "form": "힘들겠다",
          "original_form": "힘들겠다",
          "speaker_id": "2",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100016385.1.8",
          "form": "실내자전거",
          "original_form": "실내자전거",
          "speaker_id": "1",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100016385.1.9",
          "form": "다이어트 해보신 적 있나요",
          "original_form": "다이어트 해보신 적 있나요",
          "speaker_id": "1",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100016385.1.10",
          "form": "없다하셨죠",
          "original_form": "없다하셨죠",
          "speaker_id": "1",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100016385.1.11",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100016385.1.12",
          "form": "네 없어요",
          "original_form": "네 없어요",
          "speaker_id": "2",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100016385.1.13",
          "form": "부릅다...",
          "original_form": "부릅다...",
          "speaker_id": "1",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100016385.1.14",
          "form": "부러워..ㅡ",
          "original_form": "부러워..ㅡ",
          "speaker_id": "1",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100016385.1.15",
          "form": "근데 요즘 체력이 달려서",
          "original_form": "근데 요즘 체력이 달려서",
          "speaker_id": "2",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100016385.1.16",
          "form": "헬스끊어야할거같아요",
          "original_form": "헬스끊어야할거같아요",
          "speaker_id": "2",
          "time": "20210826 12:44"
        },
        {
          "id": "MDRW2100016385.1.17",
          "form": "영양제 가지고는 더이상 안될것같아..",
          "original_form": "영양제 가지고는 더이상 안될것같아..",
          "speaker_id": "2",
          "time": "20210826 12:44"
        },
        {
          "id": "MDRW2100016385.1.18",
          "form": "ㅇㄴ..",
          "original_form": "ㅇㄴ..",
          "speaker_id": "1",
          "time": "20210826 12:44"
        },
        {
          "id": "MDRW2100016385.1.19",
          "form": "군대 아직 안갔다오셨담서요",
          "original_form": "군대 아직 안갔다오셨담서요",
          "speaker_id": "1",
          "time": "20210826 12:45"
        },
        {
          "id": "MDRW2100016385.1.20",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210826 12:45"
        },
        {
          "id": "MDRW2100016385.1.21",
          "form": "군대가서 안다치기만 하면...건강해져서 오긴하던데",
          "original_form": "군대가서 안다치기만 하면...건강해져서 오긴하던데",
          "speaker_id": "1",
          "time": "20210826 12:45"
        },
        {
          "id": "MDRW2100016385.1.22",
          "form": "벌써부터 이래가지고",
          "original_form": "벌써부터 이래가지고",
          "speaker_id": "2",
          "time": "20210826 12:45"
        },
        {
          "id": "MDRW2100016385.1.23",
          "form": "다치면 이제..",
          "original_form": "다치면 이제..",
          "speaker_id": "1",
          "time": "20210826 12:45"
        },
        {
          "id": "MDRW2100016385.1.24",
          "form": "클나는거죠",
          "original_form": "클나는거죠",
          "speaker_id": "1",
          "time": "20210826 12:45"
        },
        {
          "id": "MDRW2100016385.1.25",
          "form": "망하는거져 뭐",
          "original_form": "망하는거져 뭐",
          "speaker_id": "2",
          "time": "20210826 12:45"
        }
      ]
