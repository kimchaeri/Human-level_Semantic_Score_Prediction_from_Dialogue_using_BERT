      "utterance": [
        {
          "id": "MDRW2100037190.1.1",
          "form": "펭 수는 노래를 잘한다는 거 알고 있니",
          "original_form": "펭 수는 노래를 잘한다는 거 알고 있니",
          "speaker_id": "2",
          "time": "20210831 21:23"
        },
        {
          "id": "MDRW2100037190.1.2",
          "form": "펭 수가 부른 캐롤 자주 들었어",
          "original_form": "펭 수가 부른 캐롤 자주 들었어",
          "speaker_id": "1",
          "time": "20210831 21:23"
        },
        {
          "id": "MDRW2100037190.1.3",
          "form": "김태우랑 부른 거 말하는 거지",
          "original_form": "김태우랑 부른 거 말하는 거지",
          "speaker_id": "2",
          "time": "20210831 21:23"
        },
        {
          "id": "MDRW2100037190.1.4",
          "form": "응 맞아 가사가 되게 감동적이야",
          "original_form": "응 맞아 가사가 되게 감동적이야",
          "speaker_id": "1",
          "time": "20210831 21:23"
        },
        {
          "id": "MDRW2100037190.1.5",
          "form": "랩도 잘하던데",
          "original_form": "랩도 잘하던데",
          "speaker_id": "2",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100037190.1.6",
          "form": "윤상 김이나 작사 작곡이야",
          "original_form": "윤상 김이나 작사 작곡이야",
          "speaker_id": "1",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100037190.1.7",
          "form": "좋을 만하네",
          "original_form": "좋을 만하네",
          "speaker_id": "2",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100037190.1.8",
          "form": "맞아 펭 수는 랩도 잘해",
          "original_form": "맞아 펭 수는 랩도 잘해",
          "speaker_id": "1",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100037190.1.9",
          "form": "전에 윤미래랑 같이 하는 거 봤어",
          "original_form": "전에 윤미래랑 같이 하는 거 봤어",
          "speaker_id": "2",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100037190.1.10",
          "form": "팽수 가 노래가 많았네",
          "original_form": "팽수 가 노래가 많았네",
          "speaker_id": "1",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100037190.1.11",
          "form": "멜론에서도 순위가 높더라",
          "original_form": "멜론에서도 순위가 높더라",
          "speaker_id": "2",
          "time": "20210831 21:25"
        },
        {
          "id": "MDRW2100037190.1.12",
          "form": "펭수 가 또 노래 내주면 좋겠다",
          "original_form": "펭수 가 또 노래 내주면 좋겠다",
          "speaker_id": "1",
          "time": "20210831 21:25"
        },
        {
          "id": "MDRW2100037190.1.13",
          "form": "인기가 많으니까 아마 그럴 거야",
          "original_form": "인기가 많으니까 아마 그럴 거야",
          "speaker_id": "2",
