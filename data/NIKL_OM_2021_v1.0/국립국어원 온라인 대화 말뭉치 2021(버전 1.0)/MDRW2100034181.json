      "utterance": [
        {
          "id": "MDRW2100034181.1.1",
          "form": "맞아요 필수죠 장판",
          "original_form": "맞아요 필수죠 장판",
          "speaker_id": "2",
          "time": "20210827 20:25"
        },
        {
          "id": "MDRW2100034181.1.2",
          "form": "그래서 옷도 4걔절용이 다 필요하고..",
          "original_form": "그래서 옷도 4걔절용이 다 필요하고..",
          "speaker_id": "1",
          "time": "20210827 20:25"
        },
        {
          "id": "MDRW2100034181.1.3",
          "form": "아이템도 4계절용 다 필요하고",
          "original_form": "아이템도 4계절용 다 필요하고",
          "speaker_id": "1",
          "time": "20210827 20:25"
        },
        {
          "id": "MDRW2100034181.1.4",
          "form": "그래서 짐이 많을  수 밖에 없어요",
          "original_form": "그래서 짐이 많을  수 밖에 없어요",
          "speaker_id": "1",
          "time": "20210827 20:25"
        },
        {
          "id": "MDRW2100034181.1.5",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100034181.1.6",
          "form": "모기장도 있러야 하고",
          "original_form": "모기장도 있러야 하고",
          "speaker_id": "2",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100034181.1.7",
          "form": "진짜 계절 짐 많네요",
          "original_form": "진짜 계절 짐 많네요",
          "speaker_id": "2",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100034181.1.8",
          "form": "맞다.. 모기장..",
          "original_form": "맞다.. 모기장..",
          "speaker_id": "1",
          "time": "20210827 20:27"
        },
        {
          "id": "MDRW2100034181.1.9",
          "form": "진짜 한국사람들의  집집에는 온갖게 다 있을거에요",
          "original_form": "진짜 한국사람들의  집집에는 온갖게 다 있을거에요",
          "speaker_id": "1",
          "time": "20210827 20:27"
        },
        {
          "id": "MDRW2100034181.1.10",
          "form": "연 기온이 60도나 차이나는 반도국가라..ㅎ",
          "original_form": "연 기온이 60도나 차이나는 반도국가라..ㅎ",
          "speaker_id": "1",
          "time": "20210827 20:27"
        },
        {
          "id": "MDRW2100034181.1.11",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:27"
        },
        {
          "id": "MDRW2100034181.1.12",
          "form": "다 가지고 사는게 신기해요 ㅎㅎ",
          "original_form": "다 가지고 사는게 신기해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:27"
        },
        {
          "id": "MDRW2100034181.1.13",
          "form": "오 60도군요",
          "original_form": "오 60도군요",
          "speaker_id": "2",
          "time": "20210827 20:28"
        },
        {
          "id": "MDRW2100034181.1.14",
          "form": "그렇네요 ㅎㅎ",
          "original_form": "그렇네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:28"
        },
        {
          "id": "MDRW2100034181.1.15",
          "form": "그렇게 차이나니 극과 극을 준비하고",
          "original_form": "그렇게 차이나니 극과 극을 준비하고",
          "speaker_id": "2",
          "time": "20210827 20:30"
        },
        {
          "id": "MDRW2100034181.1.16",
          "form": "올해는 겨울ㅊ덜 추워야 하는데",
          "original_form": "올해는 겨울ㅊ덜 추워야 하는데",
          "speaker_id": "2",
          "time": "20210827 20:30"
        },
        {
          "id": "MDRW2100034181.1.17",
          "form": "그니까요.. 극과 극..ㅎㅎ 강인해질 수 밖에 없는 환경",
          "original_form": "그니까요.. 극과 극..ㅎㅎ 강인해질 수 밖에 없는 환경",
          "speaker_id": "1",
          "time": "20210827 20:31"
        },
        {
          "id": "MDRW2100034181.1.18",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210827 20:31"
        },
        {
          "id": "MDRW2100034181.1.19",
          "form": "작년엔 추워서 난방비가 ㅜㅜ",
          "original_form": "작년엔 추워서 난방비가 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 20:31"
        },
        {
          "id": "MDRW2100034181.1.20",
          "form": "겨울에 가난해져요",
          "original_form": "겨울에 가난해져요",
          "speaker_id": "2",
          "time": "20210827 20:31"
        },
        {
          "id": "MDRW2100034181.1.21",
          "form": "여름엔 전기세 ㅋㅋ겨울 난방비",
          "original_form": "여름엔 전기세 ㅋㅋ겨울 난방비",
          "speaker_id": "2",
          "time": "20210827 20:32"
        },
        {
          "id": "MDRW2100034181.1.22",
          "form": "근데 또 안쓸수 없는 돈이잖아요.. 생존필수비용~",
          "original_form": "근데 또 안쓸수 없는 돈이잖아요.. 생존필수비용~",
          "speaker_id": "1",
          "time": "20210827 20:38"
        },
        {
          "id": "MDRW2100034181.1.23",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100034181.1.24",
          "form": "전 그래서 겨울엔 그냥 팍팍 틀어요",
          "original_form": "전 그래서 겨울엔 그냥 팍팍 틀어요",
          "speaker_id": "2",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100034181.1.25",
          "form": "보통 30 은 넘는거 같ㅇㅏ요 ㅜㅜ",
          "original_form": "보통 30 은 넘는거 같ㅇㅏ요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100034181.1.26",
          "form": "일단 낼은 빨해좀 어찌 잘 말혀봐야 겠어요",
          "original_form": "일단 낼은 빨해좀 어찌 잘 말혀봐야 겠어요",
          "speaker_id": "2",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100034181.1.27",
          "form": "말려봐야",
          "original_form": "말려봐야",
          "speaker_id": "2",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100034181.1.28",
          "form": "우산도 작은건 안돼요",
          "original_form": "우산도 작은건 안돼요",
          "speaker_id": "2",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100034181.1.29",
          "form": "요샌 비가 갑자기 쏟아져서요",
          "original_form": "요샌 비가 갑자기 쏟아져서요",
          "speaker_id": "2",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100034181.1.30",
          "form": "많이",
          "original_form": "많이",
          "speaker_id": "2",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100034181.1.31",
          "form": "우산도 크기별로 필요하죠.. 가방에 들어가는 3단 기본 2단 장우산도 필요하구요",
          "original_form": "우산도 크기별로 필요하죠.. 가방에 들어가는 3단 기본 2단 장우산도 필요하구요",
          "speaker_id": "1",
          "time": "20210827 20:46"
        },
        {
          "id": "MDRW2100034181.1.32",
          "form": "네ㅎㅎ",
          "original_form": "네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:46"
        },
        {
          "id": "MDRW2100034181.1.33",
          "form": "요샌 하도 많이 와서 쫄딱 젖었네요 ㅋㅋ",
          "original_form": "요샌 하도 많이 와서 쫄딱 젖었네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:47"
        },
        {
          "id": "MDRW2100034181.1.34",
          "form": "소나기오고",
          "original_form": "소나기오고",
          "speaker_id": "2",
          "time": "20210827 20:47"
        },
        {
          "id": "MDRW2100034181.1.35",
          "form": "근데 또 작ㄴ것도 편해서 가지고 다니고요",
          "original_form": "근데 또 작ㄴ것도 편해서 가지고 다니고요",
          "speaker_id": "2",
          "time": "20210827 20:47"
        },
        {
          "id": "MDRW2100034181.1.36",
          "form": "그니까요.. 작은거 들고 나갔는데 큰 비오면 아무소용없고.. 장우산 들고 나갔는데 비 안오면 완전 짐이고..",
          "original_form": "그니까요.. 작은거 들고 나갔는데 큰 비오면 아무소용없고.. 장우산 들고 나갔는데 비 안오면 완전 짐이고..",
          "speaker_id": "1",
