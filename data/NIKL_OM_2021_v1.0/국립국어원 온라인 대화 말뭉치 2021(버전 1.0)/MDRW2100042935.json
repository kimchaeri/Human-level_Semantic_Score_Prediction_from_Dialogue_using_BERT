      "utterance": [
        {
          "id": "MDRW2100042935.1.1",
          "form": "요새날씨가선선하죠",
          "original_form": "요새날씨가선선하죠",
          "speaker_id": "1",
          "time": "20210907 11:46"
        },
        {
          "id": "MDRW2100042935.1.2",
          "form": "네 산책하기 너무 좋네요ㅎ",
          "original_form": "네 산책하기 너무 좋네요ㅎ",
          "speaker_id": "2",
          "time": "20210907 11:49"
        },
        {
          "id": "MDRW2100042935.1.3",
          "form": "ㅎㅎ긴팔입고돌아댕기기좋은날씨에요",
          "original_form": "ㅎㅎ긴팔입고돌아댕기기좋은날씨에요",
          "speaker_id": "1",
          "time": "20210907 11:52"
        },
        {
          "id": "MDRW2100042935.1.4",
          "form": "네 이제 옷장에서 긴팔 옷 슬슬 찿아 꺼내야겠어요",
          "original_form": "네 이제 옷장에서 긴팔 옷 슬슬 찿아 꺼내야겠어요",
          "speaker_id": "2",
          "time": "20210907 12:00"
        },
        {
          "id": "MDRW2100042935.1.5",
          "form": "ㅎㅎ저는가디건꺼냈어요",
          "original_form": "ㅎㅎ저는가디건꺼냈어요",
          "speaker_id": "1",
          "time": "20210907 12:01"
        },
        {
          "id": "MDRW2100042935.1.6",
          "form": "가을은 가디건의 계절이죠ㅎㅎㅎ",
          "original_form": "가을은 가디건의 계절이죠ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 13:03"
        },
        {
          "id": "MDRW2100042935.1.7",
          "form": "ㅎㅎㅎ그춍 비그쳤는데 비오나요?",
          "original_form": "ㅎㅎㅎ그춍 비그쳤는데 비오나요?",
          "speaker_id": "1",
          "time": "20210907 13:58"
        },
        {
          "id": "MDRW2100042935.1.8",
          "form": "여긴 비가 계속 오다말다 하네요ㅎㅎ",
          "original_form": "여긴 비가 계속 오다말다 하네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 16:42"
        },
        {
          "id": "MDRW2100042935.1.9",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100042935.1.10",
          "form": "비싫죠",
          "original_form": "비싫죠",
          "speaker_id": "1",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100042935.1.11",
          "form": "네 요즘은 맑은하늘 보기가 넘 힘드네요ㅠ",
          "original_form": "네 요즘은 맑은하늘 보기가 넘 힘드네요ㅠ",
          "speaker_id": "2",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100042935.1.12",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210907 19:29"
        },
        {
          "id": "MDRW2100042935.1.13",
          "form": "춥겠어요이세",
          "original_form": "춥겠어요이세",
          "speaker_id": "1",
          "time": "20210907 19:29"
        },
        {
          "id": "MDRW2100042935.1.14",
          "form": "제",
          "original_form": "제",
          "speaker_id": "1",
          "time": "20210907 19:29"
        },
        {
          "id": "MDRW2100042935.1.15",
          "form": "밤엔 벌써 춥죠ㅎㅎ 해지면 겉옷 입고 나가야해요ㅎ",
          "original_form": "밤엔 벌써 춥죠ㅎㅎ 해지면 겉옷 입고 나가야해요ㅎ",
