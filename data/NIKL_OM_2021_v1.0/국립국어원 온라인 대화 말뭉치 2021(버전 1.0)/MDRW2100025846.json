      "utterance": [
        {
          "id": "MDRW2100025846.1.1",
          "form": "세븐틴",
          "original_form": "세븐틴",
          "speaker_id": "1",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100025846.1.2",
          "form": "아니 셉무슨일이야",
          "original_form": "아니 셉무슨일이야",
          "speaker_id": "1",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100025846.1.3",
          "form": "중멤들은 어딜가나 고통...",
          "original_form": "중멤들은 어딜가나 고통...",
          "speaker_id": "2",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100025846.1.4",
          "form": "중국 중심 활동이라니ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "중국 중심 활동이라니ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100025846.1.5",
          "form": "이거 중국에서",
          "original_form": "이거 중국에서",
          "speaker_id": "1",
          "time": "20210901 17:24"
        },
        {
          "id": "MDRW2100025846.1.6",
          "form": "못하게한거아닐까",
          "original_form": "못하게한거아닐까",
          "speaker_id": "1",
          "time": "20210901 17:25"
        },
        {
          "id": "MDRW2100025846.1.7",
          "form": "이제중국 공구 어려워진다던데",
          "original_form": "이제중국 공구 어려워진다던데",
          "speaker_id": "1",
          "time": "20210901 17:25"
        },
        {
          "id": "MDRW2100025846.1.8",
          "form": "블핑 공구그래서 엎어졌다고",
          "original_form": "블핑 공구그래서 엎어졌다고",
          "speaker_id": "1",
          "time": "20210901 17:25"
        },
        {
          "id": "MDRW2100025846.1.9",
          "form": "중국 공구 열리긴 하는데 전처럼 수량공개는 못할듯ㅜㅜ",
          "original_form": "중국 공구 열리긴 하는데 전처럼 수량공개는 못할듯ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 17:25"
        },
        {
          "id": "MDRW2100025846.1.10",
          "form": "그런제한들에 영향이",
          "original_form": "그런제한들에 영향이",
          "speaker_id": "1",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100025846.1.11",
          "form": "없지않을것같아",
          "original_form": "없지않을것같아",
          "speaker_id": "1",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100025846.1.12",
          "form": "ㅎㅎ 근데 중멤들은",
          "original_form": "ㅎㅎ 근데 중멤들은",
          "speaker_id": "1",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100025846.1.13",
          "form": "홍콩이나 남중국해도 그렇고",
          "original_form": "홍콩이나 남중국해도 그렇고",
          "speaker_id": "1",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100025846.1.14",
          "form": "우리나라두문제생기면",
          "original_form": "우리나라두문제생기면",
          "speaker_id": "1",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100025846.1.15",
          "form": "중국지지한다구올릴듯",
          "original_form": "중국지지한다구올릴듯",
          "speaker_id": "1",
          "time": "20210901 17:26"
        },
        {
          "id": "MDRW2100025846.1.16",
          "form": "지금까지 별 일이 없어서 안올렸던것뿐..",
          "original_form": "지금까지 별 일이 없어서 안올렸던것뿐..",
          "speaker_id": "1",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100025846.1.17",
          "form": "중멤들 진짜 노답이야..... 제발 입 다물어의 표본",
          "original_form": "중멤들 진짜 노답이야..... 제발 입 다물어의 표본",
          "speaker_id": "2",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100025846.1.18",
          "form": "스엠 제발 중국좀버려",
          "original_form": "스엠 제발 중국좀버려",
          "speaker_id": "1",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100025846.1.19",
          "form": "웨이션인가 거기는 난리던데",
          "original_form": "웨이션인가 거기는 난리던데",
          "speaker_id": "1",
          "time": "20210901 17:27"
        },
        {
          "id": "MDRW2100025846.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025846.1.21",
          "form": "거긴 우선 루카스부터...",
          "original_form": "거긴 우선 루카스부터...",
          "speaker_id": "2",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025846.1.22",
          "form": "루카스인가",
          "original_form": "루카스인가",
          "speaker_id": "1",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025846.1.23",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025846.1.24",
          "form": "그러고보니 둘기쓰",
          "original_form": "그러고보니 둘기쓰",
          "speaker_id": "1",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025846.1.25",
          "form": "오리지날둘기 크리스",
          "original_form": "오리지날둘기 크리스",
          "speaker_id": "1",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025846.1.26",
          "form": "찬열+돈안씀",
          "original_form": "찬열+돈안씀",
          "speaker_id": "2",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100025846.1.27",
          "form": "더 악질이여",
          "original_form": "더 악질이여",
          "speaker_id": "2",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100025846.1.28",
          "form": "사형까지도 본다며",
          "original_form": "사형까지도 본다며",
          "speaker_id": "1",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100025846.1.29",
          "form": "ㅇㅇㅇㅇㅇ ㄹㅇ",
          "original_form": "ㅇㅇㅇㅇㅇ ㄹㅇ",
          "speaker_id": "1",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100025846.1.30",
          "form": "더악질",
          "original_form": "더악질",
          "speaker_id": "1",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100025846.1.31",
          "form": "그래도 걔 국적이 외국이라",
          "original_form": "그래도 걔 국적이 외국이라",
          "speaker_id": "2",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100025846.1.32",
          "form": "사형까지는 안갈거같다고 하더라",
          "original_form": "사형까지는 안갈거같다고 하더라",
          "speaker_id": "2",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100025846.1.33",
          "form": "아하...",
          "original_form": "아하...",
          "speaker_id": "1",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100025846.1.34",
          "form": "캐나다사람이었지",
          "original_form": "캐나다사람이었지",
          "speaker_id": "1",
          "time": "20210901 17:29"
        }
      ]
