      "utterance": [
        {
          "id": "MDRW2100009559.1.1",
          "form": "식사는 부모님과 같이 드시나요?",
          "original_form": "식사는 부모님과 같이 드시나요?",
          "speaker_id": "1",
          "time": "20210823 00:40"
        },
        {
          "id": "MDRW2100009559.1.2",
          "form": "네 거의그래요",
          "original_form": "네 거의그래요",
          "speaker_id": "2",
          "time": "20210823 00:40"
        },
        {
          "id": "MDRW2100009559.1.3",
          "form": "여름이라 국수 자주 먹어요",
          "original_form": "여름이라 국수 자주 먹어요",
          "speaker_id": "2",
          "time": "20210823 00:40"
        },
        {
          "id": "MDRW2100009559.1.4",
          "form": "오이냉국이랑요",
          "original_form": "오이냉국이랑요",
          "speaker_id": "2",
          "time": "20210823 00:40"
        },
        {
          "id": "MDRW2100009559.1.5",
          "form": "너무 좋죠~",
          "original_form": "너무 좋죠~",
          "speaker_id": "1",
          "time": "20210823 00:41"
        },
        {
          "id": "MDRW2100009559.1.6",
          "form": "네 ㅎㅎ 뜨거운걸 잘못먹겠어요",
          "original_form": "네 ㅎㅎ 뜨거운걸 잘못먹겠어요",
          "speaker_id": "2",
          "time": "20210823 00:41"
        },
        {
          "id": "MDRW2100009559.1.7",
          "form": "음식 유튜브나 블로그 자주보시는 곳 있으세요?",
          "original_form": "음식 유튜브나 블로그 자주보시는 곳 있으세요?",
          "speaker_id": "2",
          "time": "20210823 00:42"
        },
        {
          "id": "MDRW2100009559.1.8",
          "form": "아 저도요! 뜨거운데 맵기까지 하면 너무 뜨겁고 맵고 이중고 ㅋ 항상 덜어서 식혀 먹어요",
          "original_form": "아 저도요! 뜨거운데 맵기까지 하면 너무 뜨겁고 맵고 이중고 ㅋ 항상 덜어서 식혀 먹어요",
          "speaker_id": "1",
          "time": "20210823 00:42"
        },
        {
          "id": "MDRW2100009559.1.9",
          "form": "아하 네 ㅎㅎㅎ 저도 매운걸 잘 못먹어서요",
          "original_form": "아하 네 ㅎㅎㅎ 저도 매운걸 잘 못먹어서요",
          "speaker_id": "2",
          "time": "20210823 00:42"
        },
        {
          "id": "MDRW2100009559.1.10",
          "form": "요리할때 검색해서 블로그 보곤해요",
          "original_form": "요리할때 검색해서 블로그 보곤해요",
          "speaker_id": "1",
          "time": "20210823 00:43"
        },
        {
          "id": "MDRW2100009559.1.11",
          "form": "유튜브는 요리할때 참고하기로는 별로인거 같더라구요",
          "original_form": "유튜브는 요리할때 참고하기로는 별로인거 같더라구요",
          "speaker_id": "2",
          "time": "20210823 00:43"
        },
        {
          "id": "MDRW2100009559.1.12",
          "form": "저는 블로그가 더 좋았어요",
          "original_form": "저는 블로그가 더 좋았어요",
          "speaker_id": "2",
          "time": "20210823 00:43"
        },
        {
          "id": "MDRW2100009559.1.13",
          "form": "맞아요 어느세월에 다 듣고 있어요 ㅋㅋ",
          "original_form": "맞아요 어느세월에 다 듣고 있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:43"
        },
        {
          "id": "MDRW2100009559.1.14",
          "form": "글로 배우는 요리",
          "original_form": "글로 배우는 요리",
          "speaker_id": "1",
          "time": "20210823 00:43"
        },
        {
          "id": "MDRW2100009559.1.15",
          "form": "유튜브는 영상이 예쁜거? 그걸로 보는거같구요",
          "original_form": "유튜브는 영상이 예쁜거? 그걸로 보는거같구요",
          "speaker_id": "2",
          "time": "20210823 00:44"
        },
        {
          "id": "MDRW2100009559.1.16",
          "form": "막 넘어가니까 좀 애매해요",
          "original_form": "막 넘어가니까 좀 애매해요",
          "speaker_id": "2",
          "time": "20210823 00:44"
        },
        {
          "id": "MDRW2100009559.1.17",
          "form": "맞아요 ㅋㅋ ㅎ손도 느린데 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ ㅎ손도 느린데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:44"
        },
        {
          "id": "MDRW2100009559.1.18",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210823 00:44"
        },
        {
          "id": "MDRW2100009559.1.19",
          "form": "유튜브에 잘먹이는 엄마인가",
          "original_form": "유튜브에 잘먹이는 엄마인가",
          "speaker_id": "2",
          "time": "20210823 00:45"
        },
        {
          "id": "MDRW2100009559.1.20",
          "form": "저도 요리는 잘 못해서 맨날 검색해서 하거든요 요리 잘하시나요?",
          "original_form": "저도 요리는 잘 못해서 맨날 검색해서 하거든요 요리 잘하시나요?",
          "speaker_id": "1",
          "time": "20210823 00:45"
        },
        {
          "id": "MDRW2100009559.1.21",
          "form": "아뇨 잘하진않고 라면끓이는거보단 조금더 할수있어요",
          "original_form": "아뇨 잘하진않고 라면끓이는거보단 조금더 할수있어요",
          "speaker_id": "2",
          "time": "20210823 00:45"
        }
      ]
