      "utterance": [
        {
          "id": "MDRW2100018838.1.1",
          "form": "뮤지컬 보신 적 잇나요?",
          "original_form": "뮤지컬 보신 적 잇나요?",
          "speaker_id": "1",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100018838.1.2",
          "form": "네 좋아해요 자주는 못보지만",
          "original_form": "네 좋아해요 자주는 못보지만",
          "speaker_id": "2",
          "time": "20210827 12:45"
        },
        {
          "id": "MDRW2100018838.1.3",
          "form": "최근에 뭐 보셨나요",
          "original_form": "최근에 뭐 보셨나요",
          "speaker_id": "2",
          "time": "20210827 12:45"
        },
        {
          "id": "MDRW2100018838.1.4",
          "form": "저는 레드북 봣어요",
          "original_form": "저는 레드북 봣어요",
          "speaker_id": "1",
          "time": "20210827 13:02"
        },
        {
          "id": "MDRW2100018838.1.5",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210827 13:03"
        },
        {
          "id": "MDRW2100018838.1.6",
          "form": "뮤지컬은 아니고 연극 아마데우스봤어요",
          "original_form": "뮤지컬은 아니고 연극 아마데우스봤어요",
          "speaker_id": "2",
          "time": "20210827 15:08"
        },
        {
          "id": "MDRW2100018838.1.7",
          "form": "차지연님캐스트",
          "original_form": "차지연님캐스트",
          "speaker_id": "2",
          "time": "20210827 15:08"
        },
        {
          "id": "MDRW2100018838.1.8",
          "form": "오 저는 최근에 차지연님 뮤지컬 레드북 봤는데",
          "original_form": "오 저는 최근에 차지연님 뮤지컬 레드북 봤는데",
          "speaker_id": "1",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100018838.1.9",
          "form": "연극 어떠셨나요?",
          "original_form": "연극 어떠셨나요?",
          "speaker_id": "1",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100018838.1.10",
          "form": "오 그러셨군요",
          "original_form": "오 그러셨군요",
          "speaker_id": "2",
          "time": "20210827 15:40"
        },
        {
          "id": "MDRW2100018838.1.11",
          "form": "대사량이 어마무지하던데 그걸 소화하는 연기력에 감탄하면서 봤어요",
          "original_form": "대사량이 어마무지하던데 그걸 소화하는 연기력에 감탄하면서 봤어요",
          "speaker_id": "2",
          "time": "20210827 15:41"
        },
        {
          "id": "MDRW2100018838.1.12",
          "form": "레드북은 재밌게보셨나요",
          "original_form": "레드북은 재밌게보셨나요",
          "speaker_id": "2",
          "time": "20210827 15:41"
        },
        {
          "id": "MDRW2100018838.1.13",
          "form": "네 너무 재밌어서 3번이나 봤어요 ㅎㅎ",
          "original_form": "네 너무 재밌어서 3번이나 봤어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 16:07"
        },
        {
          "id": "MDRW2100018838.1.14",
          "form": "최근에 영화관은 가셨나요??",
          "original_form": "최근에 영화관은 가셨나요??",
          "speaker_id": "1",
          "time": "20210827 16:07"
        },
        {
          "id": "MDRW2100018838.1.15",
          "form": "와 그정도 나중에 새시즌 하면 봐야겠어요",
          "original_form": "와 그정도 나중에 새시즌 하면 봐야겠어요",
          "speaker_id": "2",
          "time": "20210827 21:49"
        },
        {
          "id": "MDRW2100018838.1.16",
          "form": "네 얼마전에 보러갔어요",
          "original_form": "네 얼마전에 보러갔어요",
          "speaker_id": "2",
          "time": "20210827 21:49"
        },
        {
          "id": "MDRW2100018838.1.17",
          "form": "영화본거있어요?",
          "original_form": "영화본거있어요?",
          "speaker_id": "2",
          "time": "20210827 21:50"
        },
        {
          "id": "MDRW2100018838.1.18",
          "form": "저는 모가디슈랑 인질이요",
          "original_form": "저는 모가디슈랑 인질이요",
          "speaker_id": "1",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100018838.1.19",
          "form": "뭐보셨어요??",
          "original_form": "뭐보셨어요??",
          "speaker_id": "1",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100018838.1.20",
          "form": "저는 블랙위도우요",
          "original_form": "저는 블랙위도우요",
          "speaker_id": "2",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100018838.1.21",
          "form": "재밋엇나요??",
          "original_form": "재밋엇나요??",
          "speaker_id": "1",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100018838.1.22",
          "form": "네 재밌게봤어요!!",
          "original_form": "네 재밌게봤어요!!",
          "speaker_id": "2",
          "time": "20210827 22:27"
        },
        {
          "id": "MDRW2100018838.1.23",
          "form": "저도 봐봐야겟어요",
