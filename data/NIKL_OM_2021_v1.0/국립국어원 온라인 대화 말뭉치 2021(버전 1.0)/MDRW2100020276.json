      "utterance": [
        {
          "id": "MDRW2100020276.1.1",
          "form": "오늘은 빨래를 해야 해",
          "original_form": "오늘은 빨래를 해야 해",
          "speaker_id": "2",
          "time": "20210828 00:55"
        },
        {
          "id": "MDRW2100020276.1.2",
          "form": "이유가 있니",
          "original_form": "이유가 있니",
          "speaker_id": "1",
          "time": "20210828 00:55"
        },
        {
          "id": "MDRW2100020276.1.3",
          "form": "최근에 계속 비 와서 빨래를 못 했거든",
          "original_form": "최근에 계속 비 와서 빨래를 못 했거든",
          "speaker_id": "2",
          "time": "20210828 00:55"
        },
        {
          "id": "MDRW2100020276.1.4",
          "form": "오늘은 맑던데",
          "original_form": "오늘은 맑던데",
          "speaker_id": "1",
          "time": "20210828 00:55"
        },
        {
          "id": "MDRW2100020276.1.5",
          "form": "맞아 그래서 지금 꼭 해야 해",
          "original_form": "맞아 그래서 지금 꼭 해야 해",
          "speaker_id": "2",
          "time": "20210828 00:55"
        },
        {
          "id": "MDRW2100020276.1.6",
          "form": "빨래할 때 색깔 9분 하는 거 알고 있지",
          "original_form": "빨래할 때 색깔 9분 하는 거 알고 있지",
          "speaker_id": "1",
          "time": "20210828 00:55"
        },
        {
          "id": "MDRW2100020276.1.7",
          "form": "당연히 알고 있지",
          "original_form": "당연히 알고 있지",
          "speaker_id": "2",
          "time": "20210828 00:55"
        },
        {
          "id": "MDRW2100020276.1.8",
          "form": "섬유유연제는 쓰는 편이니",
          "original_form": "섬유유연제는 쓰는 편이니",
          "speaker_id": "1",
          "time": "20210828 00:56"
        },
        {
          "id": "MDRW2100020276.1.9",
          "form": "나는 사실 좋아하지 않아",
          "original_form": "나는 사실 좋아하지 않아",
          "speaker_id": "2",
          "time": "20210828 00:56"
        },
        {
          "id": "MDRW2100020276.1.10",
          "form": "섬유유연제를 쓰면은 대신 부드러워",
          "original_form": "섬유유연제를 쓰면은 대신 부드러워",
          "speaker_id": "1",
          "time": "20210828 00:56"
        },
        {
          "id": "MDRW2100020276.1.11",
          "form": "나는 세제의 냄새가 더 좋아",
          "original_form": "나는 세제의 냄새가 더 좋아",
          "speaker_id": "2",
          "time": "20210828 00:56"
        },
        {
          "id": "MDRW2100020276.1.12",
          "form": "빨래를 좋아한다니 다행이다",
          "original_form": "빨래를 좋아한다니 다행이다",
          "speaker_id": "1",
          "time": "20210828 00:56"
        },
        {
          "id": "MDRW2100020276.1.13",
          "form": "나도 그렇게 생각해",
          "original_form": "나도 그렇게 생각해",
          "speaker_id": "2",
          "time": "20210828 00:56"
        },
        {
          "id": "MDRW2100020276.1.14",
          "form": "내일은",
          "original_form": "내일은",
          "speaker_id": "1",
