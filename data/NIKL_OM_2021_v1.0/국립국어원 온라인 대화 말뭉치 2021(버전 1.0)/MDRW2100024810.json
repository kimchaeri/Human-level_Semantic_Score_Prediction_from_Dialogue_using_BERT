          "time": "20210831 19:18"
        },
        {
          "id": "MDRW2100024810.1.3",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210831 20:28"
        },
        {
          "id": "MDRW2100024810.1.4",
          "form": "전 주로 청소기 빨래해요",
          "original_form": "전 주로 청소기 빨래해요",
          "speaker_id": "2",
          "time": "20210831 20:30"
        },
        {
          "id": "MDRW2100024810.1.5",
          "form": "매일하세요?",
          "original_form": "매일하세요?",
          "speaker_id": "2",
          "time": "20210831 20:30"
        },
        {
          "id": "MDRW2100024810.1.6",
          "form": "일주일에한번정도요",
          "original_form": "일주일에한번정도요",
          "speaker_id": "1",
          "time": "20210831 20:31"
        },
        {
          "id": "MDRW2100024810.1.7",
          "form": "저도 주말에 대부분 하는거같아요",
          "original_form": "저도 주말에 대부분 하는거같아요",
          "speaker_id": "2",
          "time": "20210831 21:02"
        },
        {
          "id": "MDRW2100024810.1.8",
          "form": "가족과 분담해서 하시는거죠?",
          "original_form": "가족과 분담해서 하시는거죠?",
          "speaker_id": "2",
