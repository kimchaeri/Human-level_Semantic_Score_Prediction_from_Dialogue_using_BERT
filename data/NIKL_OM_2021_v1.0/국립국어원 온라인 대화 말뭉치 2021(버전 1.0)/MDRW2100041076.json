        {
          "id": "MDRW2100041076.1.1",
          "form": "다시 드라마로 복귀 피고인인 나연희역으로 주연",
          "original_form": "다시 드라마로 복귀 피고인인 나연희역으로 주연",
          "speaker_id": "2",
          "time": "20210905 16:35"
        },
        {
          "id": "MDRW2100041076.1.2",
          "form": "피고인 좀 인기 있었죠?",
          "original_form": "피고인 좀 인기 있었죠?",
          "speaker_id": "1",
          "time": "20210905 16:35"
        },
        {
          "id": "MDRW2100041076.1.3",
          "form": "싱글와아프 라희에서 주연하고",
          "original_form": "싱글와아프 라희에서 주연하고",
          "speaker_id": "2",
          "time": "20210905 16:35"
        },
        {
          "id": "MDRW2100041076.1.4",
          "form": "그 드라마도 또 생소하네",
          "original_form": "그 드라마도 또 생소하네",
          "speaker_id": "1",
          "time": "20210905 16:36"
        },
        {
          "id": "MDRW2100041076.1.5",
          "form": "엉 피고인이 28.3% 지성이랑 엄기준 나옴",
          "original_form": "엉 피고인이 28.3% 지성이랑 엄기준 나옴",
          "speaker_id": "2",
          "time": "20210905 16:36"
        },
        {
          "id": "MDRW2100041076.1.6",
          "form": "아 기억나네요",
          "original_form": "아 기억나네요",
          "speaker_id": "1",
          "time": "20210905 16:36"
        },
        {
          "id": "MDRW2100041076.1.7",
          "form": "드라맥스에서 한건데 별로",
          "original_form": "드라맥스에서 한건데 별로",
          "speaker_id": "2",
          "time": "20210905 16:36"
        },
        {
          "id": "MDRW2100041076.1.8",
          "form": "엄기준 악역으로 나와서 인기 끔",
          "original_form": "엄기준 악역으로 나와서 인기 끔",
          "speaker_id": "1",
          "time": "20210905 16:36"
        },
        {
          "id": "MDRW2100041076.1.9",
          "form": "너의 등짝에 스매싱",
          "original_form": "너의 등짝에 스매싱",
          "speaker_id": "2",
          "time": "20210905 16:37"
        },
        {
          "id": "MDRW2100041076.1.10",
          "form": "ㅋㅋ  안 봤음요",
          "original_form": "ㅋㅋ  안 봤음요",
          "speaker_id": "1",
          "time": "20210905 16:37"
        },
        {
          "id": "MDRW2100041076.1.11",
          "form": "오현경이 한해에 2~3편은 찍네",
          "original_form": "오현경이 한해에 2~3편은 찍네",
          "speaker_id": "2",
          "time": "20210905 16:37"
        },
        {
          "id": "MDRW2100041076.1.12",
          "form": "열일하고 있네요",
          "original_form": "열일하고 있네요",
          "speaker_id": "1",
          "time": "20210905 16:37"
        },
        {
          "id": "MDRW2100041076.1.13",
          "form": "그러게 완전 열일한다",
          "original_form": "그러게 완전 열일한다",
          "speaker_id": "2",
          "time": "20210905 16:37"
        },
        {
          "id": "MDRW2100041076.1.14",
          "form": "숨바꼭질 하연주 주연으로 드라마하고",
          "original_form": "숨바꼭질 하연주 주연으로 드라마하고",
