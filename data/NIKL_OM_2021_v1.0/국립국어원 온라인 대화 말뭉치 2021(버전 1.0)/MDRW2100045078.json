      },
      "utterance": [
        {
          "id": "MDRW2100045078.1.1",
          "form": "유멬미때는 하지도 않은듯",
          "original_form": "유멬미때는 하지도 않은듯",
          "speaker_id": "2",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045078.1.2",
          "form": "저 스밍놓은지",
          "original_form": "저 스밍놓은지",
          "speaker_id": "1",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045078.1.3",
          "form": "너무오래도ㅒㅆ는데",
          "original_form": "너무오래도ㅒㅆ는데",
          "speaker_id": "1",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045078.1.4",
          "form": "일단 음싸는 하나있는게 편해서",
          "original_form": "일단 음싸는 하나있는게 편해서",
          "speaker_id": "1",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100045078.1.5",
          "form": "멜론은 괘씸하고",
          "original_form": "멜론은 괘씸하고",
          "speaker_id": "1",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045078.1.6",
          "form": "스포티파이ㅡㄴ 비싸고",
          "original_form": "스포티파이ㅡㄴ 비싸고",
          "speaker_id": "1",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045078.1.7",
          "form": "지니로...",
          "original_form": "지니로...",
          "speaker_id": "1",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045078.1.8",
          "form": "스포티파이 그거",
          "original_form": "스포티파이 그거",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045078.1.9",
          "form": "국내음원 제때 들어오긴 하나여...?",
          "original_form": "국내음원 제때 들어오긴 하나여...?",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045078.1.10",
          "form": "해외곡들 들을거면 괜찮을지 모르겠는데",
          "original_form": "해외곡들 들을거면 괜찮을지 모르겠는데",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045078.1.11",
          "form": "국내음원 듣는 사람은 차라리 유튭뮤직을 쓰지",
          "original_form": "국내음원 듣는 사람은 차라리 유튭뮤직을 쓰지",
          "speaker_id": "2",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045078.1.12",
          "form": "ㄴㄴㄴㄴ",
          "original_form": "ㄴㄴㄴㄴ",
          "speaker_id": "1",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045078.1.13",
          "form": "스포티파이 잘 안쓸거같은데",
          "original_form": "스포티파이 잘 안쓸거같은데",
          "speaker_id": "2",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045078.1.14",
          "form": "별로인것같음",
          "original_form": "별로인것같음",
          "speaker_id": "1",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045078.1.15",
          "form": "곡없는것도많대",
          "original_form": "곡없는것도많대",
          "speaker_id": "1",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045078.1.16",
          "form": "그럴거같아...",
          "original_form": "그럴거같아...",
          "speaker_id": "2",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045078.1.17",
          "form": "멜지플벅같은",
          "original_form": "멜지플벅같은",
          "speaker_id": "2",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045078.1.18",
          "form": "음원 사이트 쓰거나",
          "original_form": "음원 사이트 쓰거나",
          "speaker_id": "2",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045078.1.19",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045078.1.20",
          "form": "아예 유튭뮤직 쓰거나",
          "original_form": "아예 유튭뮤직 쓰거나",
          "speaker_id": "2",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045078.1.21",
          "form": "유툽뮤직쓸까",
          "original_form": "유툽뮤직쓸까",
          "speaker_id": "1",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045078.1.22",
          "form": "둘중 하나인 느낌",
          "original_form": "둘중 하나인 느낌",
          "speaker_id": "2",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045078.1.23",
          "form": "요새는..",
          "original_form": "요새는..",
          "speaker_id": "2",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045078.1.24",
          "form": "어차피 프리미엄인데...",
          "original_form": "어차피 프리미엄인데...",
          "speaker_id": "1",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045078.1.25",
          "form": "저는프리미엄",
          "original_form": "저는프리미엄",
          "speaker_id": "1",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045078.1.26",
          "form": "프리미엄이면 유튭뮤직 포함 되지 않아여?.",
          "original_form": "프리미엄이면 유튭뮤직 포함 되지 않아여?.",
          "speaker_id": "2",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045078.1.27",
          "form": "아닌가",
          "original_form": "아닌가",
          "speaker_id": "2",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045078.1.28",
          "form": "6개월ㅇ라",
          "original_form": "6개월ㅇ라",
          "speaker_id": "1",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045078.1.29",
          "form": "포함 ㅇㅇ",
          "original_form": "포함 ㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 00:48"
        },
        {
          "id": "MDRW2100045078.1.30",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045078.1.31",
          "form": "그래서",
          "original_form": "그래서",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045078.1.32",
          "form": "좀 그거를 써봐야겠어",
          "original_form": "좀 그거를 써봐야겠어",
          "speaker_id": "1",
          "time": "20210908 00:50"
        }
      ]
    }
  ]
