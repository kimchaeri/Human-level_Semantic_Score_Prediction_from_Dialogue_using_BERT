      "utterance": [
        {
          "id": "MDRW2100006498.1.1",
          "form": "이제 수박 안나오는가",
          "original_form": "이제 수박 안나오는가",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100006498.1.2",
          "form": "시장에 아직 있더라",
          "original_form": "시장에 아직 있더라",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100006498.1.3",
          "form": "진짜로?",
          "original_form": "진짜로?",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100006498.1.4",
          "form": "그래 아까봤다",
          "original_form": "그래 아까봤다",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100006498.1.5",
          "form": "오 그러면 내일 사먹자",
          "original_form": "오 그러면 내일 사먹자",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100006498.1.6",
          "form": "갑자기 수박이 먹고싶어",
          "original_form": "갑자기 수박이 먹고싶어",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100006498.1.7",
          "form": "웅웅 ㅋㅋ 내일 내가 사올게",
          "original_form": "웅웅 ㅋㅋ 내일 내가 사올게",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100006498.1.8",
          "form": "무겁다",
          "original_form": "무겁다",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100006498.1.9",
          "form": "그럼시장에서 만나자",
          "original_form": "그럼시장에서 만나자",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100006498.1.10",
          "form": "왜",
          "original_form": "왜",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100006498.1.11",
          "form": "아빠가 들어줘 ㅋㅋ",
          "original_form": "아빠가 들어줘 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100006498.1.12",
          "form": "그러던지 ㅋㅋ",
          "original_form": "그러던지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:59"
        },
        {
