      "utterance": [
        {
          "id": "MDRW2100014590.1.1",
          "form": "공연 좋아하시나요?",
          "original_form": "공연 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014590.1.2",
          "form": "네 뮤지컬좋아합니다",
          "original_form": "네 뮤지컬좋아합니다",
          "speaker_id": "2",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014590.1.3",
          "form": "뮤지컬 직접 보러 가시는 편이세요?",
          "original_form": "뮤지컬 직접 보러 가시는 편이세요?",
          "speaker_id": "1",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014590.1.4",
          "form": "네 코로나전에는요",
          "original_form": "네 코로나전에는요",
          "speaker_id": "2",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014590.1.5",
          "form": "저도 예전에는 자주 갔었는데 요즘 못가서 아쉽네요",
          "original_form": "저도 예전에는 자주 갔었는데 요즘 못가서 아쉽네요",
          "speaker_id": "1",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100014590.1.6",
          "form": "가장 기억에 남는 공연은 뭔가요?",
          "original_form": "가장 기억에 남는 공연은 뭔가요?",
          "speaker_id": "1",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014590.1.7",
          "form": "엘리자벳이요 옥주현나올때 갔는데 진짜잘하더라구요",
          "original_form": "엘리자벳이요 옥주현나올때 갔는데 진짜잘하더라구요",
          "speaker_id": "2",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014590.1.8",
          "form": "아 저는 레베카 봤어요!",
          "original_form": "아 저는 레베카 봤어요!",
          "speaker_id": "1",
          "time": "20210825 20:13"
        },
        {
          "id": "MDRW2100014590.1.9",
          "form": "옥주현 캐스팅은 아니었지만요",
          "original_form": "옥주현 캐스팅은 아니었지만요",
          "speaker_id": "1",
          "time": "20210825 20:13"
        },
        {
          "id": "MDRW2100014590.1.10",
          "form": "레베카!못봤는데 보고싶네요",
          "original_form": "레베카!못봤는데 보고싶네요",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014590.1.11",
          "form": "진짜 추천해요",
          "original_form": "진짜 추천해요",
          "speaker_id": "1",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014590.1.12",
          "form": "네ㅎ좋은작품 추천 감사해요",
          "original_form": "네ㅎ좋은작품 추천 감사해요",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014590.1.13",
          "form": "위키드도 추천합니다",
          "original_form": "위키드도 추천합니다",
          "speaker_id": "1",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014590.1.14",
          "form": "유명한건데 다 못봤네요~ㅎ 어서 코로나끝나고 맘껏보고싶어요",
          "original_form": "유명한건데 다 못봤네요~ㅎ 어서 코로나끝나고 맘껏보고싶어요",
          "speaker_id": "2",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014590.1.15",
          "form": "그니까요 저도 공연가고싶어요",
          "original_form": "그니까요 저도 공연가고싶어요",
          "speaker_id": "1",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014590.1.16",
          "form": "보통 인터파크로 예약하시나요?",
          "original_form": "보통 인터파크로 예약하시나요?",
          "speaker_id": "1",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014590.1.17",
          "form": "네 거의인터파크만 썼어요",
          "original_form": "네 거의인터파크만 썼어요",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014590.1.18",
          "form": "인터파크티켓이 진짜 편한거같아요",
          "original_form": "인터파크티켓이 진짜 편한거같아요",
          "speaker_id": "1",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014590.1.19",
          "form": "예매대기 해보셨나요?",
          "original_form": "예매대기 해보셨나요?",
          "speaker_id": "1",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014590.1.20",
          "form": "네~ 천원인가 그렇던데 충분히 가치가있더라구요",
          "original_form": "네~ 천원인가 그렇던데 충분히 가치가있더라구요",
          "speaker_id": "2",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014590.1.21",
          "form": "아 진짜 예매대기 성공했을때 그 기분 말로 표현이 안됩니다",
          "original_form": "아 진짜 예매대기 성공했을때 그 기분 말로 표현이 안됩니다",
          "speaker_id": "1",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014590.1.22",
          "form": "저는 밤잠없어서 새벽2시쯤 취소표 풀리는것도 많이 잡아요ㅋ",
          "original_form": "저는 밤잠없어서 새벽2시쯤 취소표 풀리는것도 많이 잡아요ㅋ",
          "speaker_id": "2",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014590.1.23",
          "form": "진짜요? 전 밤잠이 많아서 늘 예대만 믿습니다",
          "original_form": "진짜요? 전 밤잠이 많아서 늘 예대만 믿습니다",
          "speaker_id": "1",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014590.1.24",
          "form": "스탠딩 좋아하세요?",
          "original_form": "스탠딩 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014590.1.25",
          "form": "해본적은없는데 더늙기전에 해보고싶어요ㅎ",
          "original_form": "해본적은없는데 더늙기전에 해보고싶어요ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:54"
        }
      ]
    }
