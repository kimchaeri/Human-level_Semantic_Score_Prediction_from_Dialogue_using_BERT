      "utterance": [
        {
          "id": "MDRW2100017939.1.1",
          "form": "도루코 써보니까 다른가요?",
          "original_form": "도루코 써보니까 다른가요?",
          "speaker_id": "1",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100017939.1.2",
          "form": "진짜 좀 다른거같아요",
          "original_form": "진짜 좀 다른거같아요",
          "speaker_id": "2",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100017939.1.3",
          "form": "밀리는것도 그렇고",
          "original_form": "밀리는것도 그렇고",
          "speaker_id": "2",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100017939.1.4",
          "form": "역쉬... 브랜드란....",
          "original_form": "역쉬... 브랜드란....",
          "speaker_id": "1",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100017939.1.5",
          "form": "피부에 민감도도 좀 적은거같아서 좋아요..!",
          "original_form": "피부에 민감도도 좀 적은거같아서 좋아요..!",
          "speaker_id": "2",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100017939.1.6",
          "form": "안다치시겠네요 이제",
          "original_form": "안다치시겠네요 이제",
          "speaker_id": "1",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100017939.1.7",
          "form": "다행이에요 ㅎㅎ",
          "original_form": "다행이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100017939.1.8",
          "form": "돈 좀 쓰면 좋은 물건 건지더라구요 ㅎㅎ",
          "original_form": "돈 좀 쓰면 좋은 물건 건지더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100017939.1.9",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100017939.1.10",
          "form": "진짜 잘건진거같은..생각이 ㅠㅠ",
          "original_form": "진짜 잘건진거같은..생각이 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100017939.1.11",
          "form": "ㅎㅎ 전동칫솔도 만족하세요?",
          "original_form": "ㅎㅎ 전동칫솔도 만족하세요?",
          "speaker_id": "1",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100017939.1.12",
          "form": "칫솔은 브랜드 안보고 그냥 샀는데",
          "original_form": "칫솔은 브랜드 안보고 그냥 샀는데",
          "speaker_id": "2",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100017939.1.13",
          "form": "우선은 대만족!",
          "original_form": "우선은 대만족!",
          "speaker_id": "2",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100017939.1.14",
          "form": "ㅎㅎ다행다행!",
          "original_form": "ㅎㅎ다행다행!",
          "speaker_id": "1",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100017939.1.15",
          "form": "나중에 돈 더모아서 브랜드로 한번 갈아탈까 고민중이긴해요 ㅎㅎ",
          "original_form": "나중에 돈 더모아서 브랜드로 한번 갈아탈까 고민중이긴해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100017939.1.16",
          "form": "오 좋죠 좋죠",
          "original_form": "오 좋죠 좋죠",
          "speaker_id": "1",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100017939.1.17",
          "form": "동기부여되구 좋네요",
          "original_form": "동기부여되구 좋네요",
          "speaker_id": "1",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100017939.1.18",
          "form": "이왕 할꺼 인터넷 찾아보고 살려고요",
          "original_form": "이왕 할꺼 인터넷 찾아보고 살려고요",
          "speaker_id": "2",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100017939.1.19",
          "form": "전 컴퓨터 새로 사고 싶어서 돈 모으고 있어요",
          "original_form": "전 컴퓨터 새로 사고 싶어서 돈 모으고 있어요",
          "speaker_id": "1",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100017939.1.20",
          "form": "오..!",
          "original_form": "오..!",
          "speaker_id": "2",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100017939.1.21",
          "form": "저도 지금 돈모으고 있는데",
          "original_form": "저도 지금 돈모으고 있는데",
          "speaker_id": "2",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100017939.1.22",
          "form": "노트북살려구 ㅋㅋㅋ",
          "original_form": "노트북살려구 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100017939.1.23",
          "form": "자료조사 좋네요!",
          "original_form": "자료조사 좋네요!",
          "speaker_id": "1",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100017939.1.24",
          "form": "오 어느 노트북이요?",
          "original_form": "오 어느 노트북이요?",
          "speaker_id": "1",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100017939.1.25",
          "form": "게이밍노트북으로 하나 살려고요!",
          "original_form": "게이밍노트북으로 하나 살려고요!",
          "speaker_id": "2",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100017939.1.26",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100017939.1.27",
          "form": "게이밍노트 비싸죠 ㅎㅎ",
          "original_form": "게이밍노트 비싸죠 ㅎㅎ",
