        },
        {
          "id": "MDRW2100016807.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 15:31"
        },
        {
          "id": "MDRW2100016807.1.3",
          "form": "키우는 동물 있어요?",
          "original_form": "키우는 동물 있어요?",
          "speaker_id": "1",
          "time": "20210826 15:32"
        },
        {
          "id": "MDRW2100016807.1.4",
          "form": "키우는 동물 없는데 키우고 싶은건 있어요!",
          "original_form": "키우는 동물 없는데 키우고 싶은건 있어요!",
          "speaker_id": "2",
          "time": "20210826 15:32"
        },
        {
          "id": "MDRW2100016807.1.5",
          "form": "오 뭐 키우고싶어요?",
          "original_form": "오 뭐 키우고싶어요?",
          "speaker_id": "1",
          "time": "20210826 15:33"
        },
        {
          "id": "MDRW2100016807.1.6",
          "form": "앵무새요!!",
          "original_form": "앵무새요!!",
          "speaker_id": "2",
          "time": "20210827 13:25"
        },
        {
          "id": "MDRW2100016807.1.7",
          "form": "유튜브로 보는데 너무 귀엽더라구여ㅜㅜ",
          "original_form": "유튜브로 보는데 너무 귀엽더라구여ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 13:25"
        },
        {
          "id": "MDRW2100016807.1.8",
          "form": "헐 맞아요ㅋㅋㅋ 저는 그 하얗고 깃털 노란? 왕관앵무새인가",
          "original_form": "헐 맞아요ㅋㅋㅋ 저는 그 하얗고 깃털 노란? 왕관앵무새인가",
          "speaker_id": "1",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100016807.1.9",
          "form": "너무귀엽던데",
          "original_form": "너무귀엽던데",
          "speaker_id": "1",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100016807.1.10",
          "form": "오호 노란깃털 귀엽겟어여",
          "original_form": "오호 노란깃털 귀엽겟어여",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100016807.1.11",
          "form": "제가본건 흰색 앵무새인데",
          "original_form": "제가본건 흰색 앵무새인데",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100016807.1.12",
          "form": "이름이 name3이에여 근데 성격이 강아지같아용 넘귀 ㅜㅜ",
          "original_form": "이름이 &name3&이에여 근데 성격이 강아지같아용 넘귀 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100016807.1.13",
          "form": "ㅜㅜㅜㅠㅜ귀여울거같아요",
          "original_form": "ㅜㅜㅜㅠㅜ귀여울거같아요",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100016807.1.14",
          "form": "앵무새가 진짜 애기같더라구요",
          "original_form": "앵무새가 진짜 애기같더라구요",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100016807.1.15",
          "form": "마자여ㅜㅜ",
          "original_form": "마자여ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100016807.1.16",
          "form": "안아주고 쓰다듬어주는거 좋아하더라구여ㅠㅠ",
          "original_form": "안아주고 쓰다듬어주는거 좋아하더라구여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100016807.1.17",
          "form": "저는 유툽 보기전에는",
          "original_form": "저는 유툽 보기전에는",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100016807.1.18",
          "form": "앵무새가 그렇게 교감이 잘되는 애들인지 몰랐어요",
          "original_form": "앵무새가 그렇게 교감이 잘되는 애들인지 몰랐어요",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100016807.1.19",
          "form": "네ㅋㅋㅋ주인이 귀찮게 굴면 짜증도 내던데 ㅋㅋ",
          "original_form": "네ㅋㅋㅋ주인이 귀찮게 굴면 짜증도 내던데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100016807.1.20",
          "form": "아악!이래여 ㅋㅋ",
          "original_form": "아악!이래여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100016807.1.21",
          "form": "ㅋㅋㅋㅋㅋ저도 본거같아요",
          "original_form": "ㅋㅋㅋㅋㅋ저도 본거같아요",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100016807.1.22",
          "form": "하 너무귀여워요",
          "original_form": "하 너무귀여워요",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100016807.1.23",
          "form": "저도 좀 꼼꼼한 성격이었다면 키웠을텐데",
          "original_form": "저도 좀 꼼꼼한 성격이었다면 키웠을텐데",
