      "utterance": [
        {
          "id": "MDRW2100027081.1.1",
          "form": "커피마시는중",
          "original_form": "커피마시는중",
          "speaker_id": "2",
          "time": "20210902 09:14"
        },
        {
          "id": "MDRW2100027081.1.2",
          "form": "아침드셨나요",
          "original_form": "아침드셨나요",
          "speaker_id": "1",
          "time": "20210902 09:15"
        },
        {
          "id": "MDRW2100027081.1.3",
          "form": "먹고있어요",
          "original_form": "먹고있어요",
          "speaker_id": "2",
          "time": "20210902 09:15"
        },
        {
          "id": "MDRW2100027081.1.4",
          "form": "볶음밥이요",
          "original_form": "볶음밥이요",
          "speaker_id": "2",
          "time": "20210902 09:15"
        },
        {
          "id": "MDRW2100027081.1.5",
          "form": "드셨나요?",
          "original_form": "드셨나요?",
          "speaker_id": "2",
          "time": "20210902 09:15"
        },
        {
          "id": "MDRW2100027081.1.6",
          "form": "볶음밥과 커피를 같이요?",
          "original_form": "볶음밥과 커피를 같이요?",
          "speaker_id": "1",
          "time": "20210902 09:15"
        },
        {
          "id": "MDRW2100027081.1.7",
          "form": "념념굳",
          "original_form": "념념굳",
          "speaker_id": "1",
          "time": "20210902 09:15"
        },
        {
          "id": "MDRW2100027081.1.8",
          "form": "넹ㅋㅋㅋㅋ",
          "original_form": "넹ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:15"
        },
        {
          "id": "MDRW2100027081.1.9",
          "form": "식사하면서 휴대폰도 하는군요",
          "original_form": "식사하면서 휴대폰도 하는군요",
          "speaker_id": "1",
          "time": "20210902 09:16"
        },
        {
          "id": "MDRW2100027081.1.10",
          "form": "커피는 제 하루의 활력소이지요",
          "original_form": "커피는 제 하루의 활력소이지요",
          "speaker_id": "2",
          "time": "20210902 09:16"
        },
        {
          "id": "MDRW2100027081.1.11",
          "form": "저는 장어를 먹었습니다",
          "original_form": "저는 장어를 먹었습니다",
          "speaker_id": "1",
          "time": "20210902 09:16"
        },
        {
          "id": "MDRW2100027081.1.12",
          "form": "멀티가 됩니다",
          "original_form": "멀티가 됩니다",
          "speaker_id": "2",
          "time": "20210902 09:16"
        },
        {
          "id": "MDRW2100027081.1.13",
          "form": "장어 좋지요",
          "original_form": "장어 좋지요",
          "speaker_id": "2",
          "time": "20210902 09:16"
        },
        {
          "id": "MDRW2100027081.1.14",
          "form": "며칠전 저도 먹었습니다",
          "original_form": "며칠전 저도 먹었습니다",
          "speaker_id": "2",
          "time": "20210902 09:16"
        },
        {
          "id": "MDRW2100027081.1.15",
          "form": "원기회복에좋지요",
          "original_form": "원기회복에좋지요",
          "speaker_id": "2",
          "time": "20210902 09:16"
        },
        {
          "id": "MDRW2100027081.1.16",
          "form": "다른반찬은 없나요",
          "original_form": "다른반찬은 없나요",
          "speaker_id": "2",
          "time": "20210902 09:17"
        },
        {
          "id": "MDRW2100027081.1.17",
          "form": "고구마줄기를 먹었습니다",
          "original_form": "고구마줄기를 먹었습니다",
          "speaker_id": "1",
          "time": "20210902 09:17"
        },
        {
          "id": "MDRW2100027081.1.18",
          "form": "장어가 통통해서 맛있습니다",
          "original_form": "장어가 통통해서 맛있습니다",
          "speaker_id": "1",
          "time": "20210902 09:18"
        },
        {
          "id": "MDRW2100027081.1.19",
          "form": "무슨볶음밥 드시나요",
          "original_form": "무슨볶음밥 드시나요",
          "speaker_id": "1",
          "time": "20210902 09:18"
        },
        {
          "id": "MDRW2100027081.1.20",
          "form": "김치볶음밥이요",
          "original_form": "김치볶음밥이요",
          "speaker_id": "2",
          "time": "20210902 09:18"
        },
        {
          "id": "MDRW2100027081.1.21",
          "form": "스팸이들어가있어서",
          "original_form": "스팸이들어가있어서",
          "speaker_id": "2",
          "time": "20210902 09:18"
        },
        {
          "id": "MDRW2100027081.1.22",
          "form": "짭짤합니다",
          "original_form": "짭짤합니다",
          "speaker_id": "2",
          "time": "20210902 09:18"
        },
        {
          "id": "MDRW2100027081.1.23",
          "form": "마시는건 없으세요?",
          "original_form": "마시는건 없으세요?",
          "speaker_id": "2",
          "time": "20210902 09:19"
        },
        {
          "id": "MDRW2100027081.1.24",
          "form": "곧 카페가서 커피마실거에요",
          "original_form": "곧 카페가서 커피마실거에요",
          "speaker_id": "1",
          "time": "20210902 09:20"
        },
        {
          "id": "MDRW2100027081.1.25",
          "form": "아메리카노를 마실거에요",
          "original_form": "아메리카노를 마실거에요",
          "speaker_id": "1",
          "time": "20210902 09:20"
        },
        {
          "id": "MDRW2100027081.1.26",
          "form": "카페가시는군요",
          "original_form": "카페가시는군요",
          "speaker_id": "2",
          "time": "20210902 09:20"
        },
        {
          "id": "MDRW2100027081.1.27",
          "form": "맛집있나요",
          "original_form": "맛집있나요",
          "speaker_id": "2",
          "time": "20210902 09:21"
        },
        {
          "id": "MDRW2100027081.1.28",
          "form": "카페 화장실이 깨끗합니다",
          "original_form": "카페 화장실이 깨끗합니다",
          "speaker_id": "1",
          "time": "20210902 09:21"
        },
        {
          "id": "MDRW2100027081.1.29",
          "form": "유명하죠",
          "original_form": "유명하죠",
          "speaker_id": "1",
          "time": "20210902 09:21"
        },
        {
          "id": "MDRW2100027081.1.30",
          "form": "아주 중요하죠",
          "original_form": "아주 중요하죠",
          "speaker_id": "2",
          "time": "20210902 09:21"
        },
        {
          "id": "MDRW2100027081.1.31",
          "form": "좋아하는 음식이 있나요?",
          "original_form": "좋아하는 음식이 있나요?",
          "speaker_id": "1",
          "time": "20210902 09:21"
        }
      ]
    }
