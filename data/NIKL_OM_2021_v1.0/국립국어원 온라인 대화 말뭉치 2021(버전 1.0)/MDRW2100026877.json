      "utterance": [
        {
          "id": "MDRW2100026877.1.1",
          "form": "오늘은 1시에 자야겟어여 ㅋ",
          "original_form": "오늘은 1시에 자야겟어여 ㅋ",
          "speaker_id": "1",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100026877.1.2",
          "form": "ㅎㅎ 그래보이시는듯요ㅎㅎ",
          "original_form": "ㅎㅎ 그래보이시는듯요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100026877.1.3",
          "form": "피곤하긴 한거같음 ㅋ",
          "original_form": "피곤하긴 한거같음 ㅋ",
          "speaker_id": "1",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100026877.1.4",
          "form": "맞아요ㅎ 피곤하면 주무셔요ㅎㅎ",
          "original_form": "맞아요ㅎ 피곤하면 주무셔요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100026877.1.5",
          "form": "오랜만에해서그런가ㅎㅎ",
          "original_form": "오랜만에해서그런가ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100026877.1.6",
          "form": "어색한가?ㅎㅎㅎㄹㅎ",
          "original_form": "어색한가?ㅎㅎㅎㄹㅎ",
          "speaker_id": "2",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100026877.1.7",
          "form": "이게 폰이 바뀌고",
          "original_form": "이게 폰이 바뀌고",
          "speaker_id": "2",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100026877.1.8",
          "form": "그전폰으로 하는거긴한데ㅎㅎ 좀 불편해여ㅎ",
          "original_form": "그전폰으로 하는거긴한데ㅎㅎ 좀 불편해여ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100026877.1.9",
          "form": "아아 ㅋㅋ ㅎㅎㅎ 아무래도",
          "original_form": "아아 ㅋㅋ ㅎㅎㅎ 아무래도",
          "speaker_id": "1",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100026877.1.10",
          "form": "음.. 뭐라고해야하지ㅎ 집안일을 했는데",
          "original_form": "음.. 뭐라고해야하지ㅎ 집안일을 했는데",
          "speaker_id": "2",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026877.1.11",
          "form": "안한것같은느낌?",
          "original_form": "안한것같은느낌?",
          "speaker_id": "2",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026877.1.12",
          "form": "저도 세컨폰 왔다갓다 쓰면",
          "original_form": "저도 세컨폰 왔다갓다 쓰면",
          "speaker_id": "1",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026877.1.13",
          "form": "불편하긴 하더라구요",
          "original_form": "불편하긴 하더라구요",
          "speaker_id": "1",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026877.1.14",
          "form": "그니깐요ㅎ",
          "original_form": "그니깐요ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026877.1.15",
          "form": "누워서해야하는데",
          "original_form": "누워서해야하는데",
          "speaker_id": "2",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026877.1.16",
          "form": "엎드려서 하고있으니 더그런듯요ㅎ",
          "original_form": "엎드려서 하고있으니 더그런듯요ㅎ",
          "speaker_id": "2",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026877.1.17",
          "form": "엎드려하면  어깨 목 나가요 ㅎㅎ",
          "original_form": "엎드려하면  어깨 목 나가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026877.1.18",
          "form": "그러는 저도 엎드려서 ㅋ",
          "original_form": "그러는 저도 엎드려서 ㅋ",
          "speaker_id": "1",
          "time": "20210902 00:53"
        },
        {
          "id": "MDRW2100026877.1.19",
          "form": "ㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:53"
        },
        {
          "id": "MDRW2100026877.1.20",
          "form": "집안일은 왜이렇게 하기가싫죠?",
          "original_form": "집안일은 왜이렇게 하기가싫죠?",
          "speaker_id": "2",
          "time": "20210902 00:53"
        },
        {
          "id": "MDRW2100026877.1.21",
          "form": "청소주기도 빨리돌아오고",
          "original_form": "청소주기도 빨리돌아오고",
          "speaker_id": "2",
          "time": "20210902 00:53"
        },
        {
          "id": "MDRW2100026877.1.22",
          "form": "누군가는 꼭 해야 하는일이라서여 ㅋ",
          "original_form": "누군가는 꼭 해야 하는일이라서여 ㅋ",
          "speaker_id": "1",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100026877.1.23",
          "form": "아... 정답ㅎㅎ",
          "original_form": "아... 정답ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100026877.1.24",
          "form": "쓰. 레기버리는데",
          "original_form": "쓰. 레기버리는데",
          "speaker_id": "2",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100026877.1.25",
          "form": "해도 티안나고",
          "original_form": "해도 티안나고",
          "speaker_id": "1",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100026877.1.26",
          "form": "저희집은 거의 뭐 ....",
          "original_form": "저희집은 거의 뭐 ....",
          "speaker_id": "2",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100026877.1.27",
          "form": "안하면 더 티나거 ㅋ",
          "original_form": "안하면 더 티나거 ㅋ",
          "speaker_id": "1",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100026877.1.28",
          "form": "택배사라ㅎㅎ",
          "original_form": "택배사라ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100026877.1.29",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100026877.1.30",
          "form": "택배 하루만 안버리면",
          "original_form": "택배 하루만 안버리면",
          "speaker_id": "2",
          "time": "20210902 00:55"
        },
        {
          "id": "MDRW2100026877.1.31",
          "form": "저도 택배가 거의 ㅋㅋ",
          "original_form": "저도 택배가 거의 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:55"
        },
        {
          "id": "MDRW2100026877.1.32",
          "form": "어마무시하게 쌓아져있어요",
          "original_form": "어마무시하게 쌓아져있어요",
          "speaker_id": "2",
          "time": "20210902 00:55"
        }
      ]
