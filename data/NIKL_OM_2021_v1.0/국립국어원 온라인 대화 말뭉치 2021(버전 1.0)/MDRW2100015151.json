      "utterance": [
        {
          "id": "MDRW2100015151.1.1",
          "form": "저녁은 뭘 드셨나요??",
          "original_form": "저녁은 뭘 드셨나요??",
          "speaker_id": "1",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015151.1.2",
          "form": "전 그냥 밥에다 멸치먹었어용",
          "original_form": "전 그냥 밥에다 멸치먹었어용",
          "speaker_id": "2",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100015151.1.3",
          "form": "김치랑..",
          "original_form": "김치랑..",
          "speaker_id": "2",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100015151.1.4",
          "form": "시리얼도 먹고 주스도 먹고",
          "original_form": "시리얼도 먹고 주스도 먹고",
          "speaker_id": "2",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100015151.1.5",
          "form": "오늘 되게 잡다하게 많이 먹었어요ㅋㅋㅠ",
          "original_form": "오늘 되게 잡다하게 많이 먹었어요ㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100015151.1.6",
          "form": "name1님은 뭐드셨어요?",
          "original_form": "&name1&님은 뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100015151.1.7",
          "form": "이것저것 많이 드셨네요! 맛있었겟어요",
          "original_form": "이것저것 많이 드셨네요! 맛있었겟어요",
          "speaker_id": "1",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015151.1.8",
          "form": "저는 들깨수제비요!",
          "original_form": "저는 들깨수제비요!",
          "speaker_id": "1",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015151.1.9",
          "form": "오 맛있는거 드셨네요! 저도 수제비 칼국수 이런거 진짜 좋아해요",
          "original_form": "오 맛있는거 드셨네요! 저도 수제비 칼국수 이런거 진짜 좋아해요",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015151.1.10",
          "form": "국밥류는 좋아하세요?",
          "original_form": "국밥류는 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015151.1.11",
          "form": "저 콩나물국밥 완전 사랑해요!!",
          "original_form": "저 콩나물국밥 완전 사랑해요!!",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015151.1.12",
          "form": "헐 저두요",
          "original_form": "헐 저두요",
          "speaker_id": "2",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100015151.1.13",
          "form": "전 국밥류 다 좋아해요 얼큰한거진짜",
          "original_form": "전 국밥류 다 좋아해요 얼큰한거진짜",
          "speaker_id": "2",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100015151.1.14",
          "form": "전주 가서 드셔보셨나요??",
          "original_form": "전주 가서 드셔보셨나요??",
          "speaker_id": "1",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015151.1.15",
          "form": "삼백집 처음 먹고 감동받았잖아요",
          "original_form": "삼백집 처음 먹고 감동받았잖아요",
          "speaker_id": "1",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015151.1.16",
          "form": "헐 전 아직 전주까진 진출못했는데",
          "original_form": "헐 전 아직 전주까진 진출못했는데",
          "speaker_id": "2",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015151.1.17",
          "form": "맛이 역시 다르던가요??",
          "original_form": "맛이 역시 다르던가요??",
          "speaker_id": "2",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015151.1.18",
          "form": "다르더라구요 거기서 파는 모주도 같이 한잔 하면 캬",
          "original_form": "다르더라구요 거기서 파는 모주도 같이 한잔 하면 캬",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015151.1.19",
          "form": "이야... 저도 꼭 가봐야겠네요 모주하니까 갑자기 막걸리 파전에 생각나요ㅋㅋ",
          "original_form": "이야... 저도 꼭 가봐야겠네요 모주하니까 갑자기 막걸리 파전에 생각나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015151.1.20",
          "form": "파전이",
          "original_form": "파전이",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015151.1.21",
          "form": "이런 장마 날씨에는 파전이 딱인데",
          "original_form": "이런 장마 날씨에는 파전이 딱인데",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015151.1.22",
          "form": "하 파전.....",
          "original_form": "하 파전.....",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015151.1.23",
          "form": "어제 김치전 먹었는데",
          "original_form": "어제 김치전 먹었는데",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015151.1.24",
          "form": "파전도 땡기네요ㅠㅠ",
          "original_form": "파전도 땡기네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015151.1.25",
          "form": "아 김치전ㅜㅜ",
          "original_form": "아 김치전ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015151.1.26",
          "form": "예전에는 막 치즈 넣어먹고 그랬는데",
          "original_form": "예전에는 막 치즈 넣어먹고 그랬는데",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015151.1.27",
          "form": "아 이시간에 괜히 땡기네요",
          "original_form": "아 이시간에 괜히 땡기네요",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015151.1.28",
          "form": "아 치즈김치전 ㅠㅠ 이런 맛잘알",
          "original_form": "아 치즈김치전 ㅠㅠ 이런 맛잘알",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015151.1.29",
          "form": "동동주도 땡기네요",
          "original_form": "동동주도 땡기네요",
          "speaker_id": "1",
          "time": "20210825 22:25"
        },
        {
          "id": "MDRW2100015151.1.30",
          "form": "ㅋㅋ아ㅠㅠㅠ 역시 한국인 입맛에는...",
          "original_form": "ㅋㅋ아ㅠㅠㅠ 역시 한국인 입맛에는...",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015151.1.31",
          "form": "그럼 혹시 육회는 좋아하시나요?",
          "original_form": "그럼 혹시 육회는 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015151.1.32",
          "form": "육회를 제가 못먹어요ㅠㅠ",
          "original_form": "육회를 제가 못먹어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:34"
        }
      ]
