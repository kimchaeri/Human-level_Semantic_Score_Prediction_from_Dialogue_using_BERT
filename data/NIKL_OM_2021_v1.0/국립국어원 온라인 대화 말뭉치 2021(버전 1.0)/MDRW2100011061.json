      "utterance": [
        {
          "id": "MDRW2100011061.1.1",
          "form": "있지.",
          "original_form": "있지.",
          "speaker_id": "2",
          "time": "20210824 00:15"
        },
        {
          "id": "MDRW2100011061.1.2",
          "form": "너는 있어?",
          "original_form": "너는 있어?",
          "speaker_id": "2",
          "time": "20210824 00:15"
        },
        {
          "id": "MDRW2100011061.1.3",
          "form": "흠 그랬두나",
          "original_form": "흠 그랬두나",
          "speaker_id": "2",
          "time": "20210824 00:15"
        },
        {
          "id": "MDRW2100011061.1.4",
          "form": "근데 나는 성공한 적은 없어서 잘 모르겠네",
          "original_form": "근데 나는 성공한 적은 없어서 잘 모르겠네",
          "speaker_id": "2",
          "time": "20210824 00:15"
        },
        {
          "id": "MDRW2100011061.1.5",
          "form": "나는 이제 시작해보려고해",
          "original_form": "나는 이제 시작해보려고해",
          "speaker_id": "1",
          "time": "20210824 00:15"
        },
        {
          "id": "MDRW2100011061.1.6",
          "form": "곤약은 어때?",
          "original_form": "곤약은 어때?",
          "speaker_id": "2",
          "time": "20210824 00:15"
        },
        {
          "id": "MDRW2100011061.1.7",
          "form": "곤약이 다이어터들에게 좋다더라.",
          "original_form": "곤약이 다이어터들에게 좋다더라.",
          "speaker_id": "2",
          "time": "20210824 00:15"
        },
        {
          "id": "MDRW2100011061.1.8",
          "form": "흠 좋다 난 곤약을 좋아하기도해",
          "original_form": "흠 좋다 난 곤약을 좋아하기도해",
          "speaker_id": "1",
          "time": "20210824 00:15"
        },
        {
          "id": "MDRW2100011061.1.9",
          "form": "근데 그걸 양념에 비벼먹으면",
          "original_form": "근데 그걸 양념에 비벼먹으면",
          "speaker_id": "1",
          "time": "20210824 00:15"
        },
        {
          "id": "MDRW2100011061.1.10",
          "form": "아니면 닭의 퍽퍽한 살을 먹어봐!",
          "original_form": "아니면 닭의 퍽퍽한 살을 먹어봐!",
          "speaker_id": "2",
          "time": "20210824 00:15"
        },
        {
          "id": "MDRW2100011061.1.11",
          "form": "다이어트가 안되겠지?",
          "original_form": "다이어트가 안되겠지?",
          "speaker_id": "1",
          "time": "20210824 00:15"
        },
        {
          "id": "MDRW2100011061.1.12",
          "form": "맞아. 난 그걸 불닭소스에 비벼먹었어",
          "original_form": "맞아. 난 그걸 불닭소스에 비벼먹었어",
          "speaker_id": "2",
          "time": "20210824 00:15"
        },
        {
          "id": "MDRW2100011061.1.13",
          "form": "닭을.",
          "original_form": "닭을.",
          "speaker_id": "2",
          "time": "20210824 00:15"
        },
        {
          "id": "MDRW2100011061.1.14",
          "form": "음 그건 싫어 ㅠㅠ 맛있는 다이어트 음식은 없을까",
          "original_form": "음 그건 싫어 ㅠㅠ 맛있는 다이어트 음식은 없을까",
          "speaker_id": "1",
          "time": "20210824 00:15"
        },
        {
          "id": "MDRW2100011061.1.15",
          "form": "참 맛있얼어",
          "original_form": "참 맛있얼어",
          "speaker_id": "2",
          "time": "20210824 00:15"
        },
        {
          "id": "MDRW2100011061.1.16",
          "form": "닭 소시지가 맛있다더라",
          "original_form": "닭 소시지가 맛있다더라",
          "speaker_id": "2",
          "time": "20210824 00:16"
        },
        {
          "id": "MDRW2100011061.1.17",
          "form": "맛있는 음식으로 다이어트도 하고싶어",
          "original_form": "맛있는 음식으로 다이어트도 하고싶어",
          "speaker_id": "1",
          "time": "20210824 00:16"
        },
        {
          "id": "MDRW2100011061.1.18",
          "form": "난 안먹어봤오.",
          "original_form": "난 안먹어봤오.",
          "speaker_id": "2",
          "time": "20210824 00:16"
        },
        {
          "id": "MDRW2100011061.1.19",
          "form": "확실한건 닭양쌈은 맛없어",
          "original_form": "확실한건 닭양쌈은 맛없어",
          "speaker_id": "2",
          "time": "20210824 00:16"
        },
        {
          "id": "MDRW2100011061.1.20",
          "form": "아 나는 닭소세지는 먹어봤어",
          "original_form": "아 나는 닭소세지는 먹어봤어",
          "speaker_id": "1",
          "time": "20210824 00:16"
        },
        {
          "id": "MDRW2100011061.1.21",
          "form": "그냥 무난해",
          "original_form": "그냥 무난해",
          "speaker_id": "1",
          "time": "20210824 00:16"
        },
        {
          "id": "MDRW2100011061.1.22",
          "form": "나는 비위가 강한데 세개째에 구역질을 햌ㅅ단다",
          "original_form": "나는 비위가 강한데 세개째에 구역질을 햌ㅅ단다",
          "speaker_id": "2",
          "time": "20210824 00:16"
        },
        {
          "id": "MDRW2100011061.1.23",
          "form": "일반 소세지와 엄청 비슷해",
          "original_form": "일반 소세지와 엄청 비슷해",
          "speaker_id": "1",
          "time": "20210824 00:16"
        },
        {
          "id": "MDRW2100011061.1.24",
          "form": "오 기대가 된다.",
          "original_form": "오 기대가 된다.",
          "speaker_id": "2",
          "time": "20210824 00:16"
        },
        {
          "id": "MDRW2100011061.1.25",
          "form": "세상에ㅠㅠ",
          "original_form": "세상에ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 00:16"
        },
        {
          "id": "MDRW2100011061.1.26",
          "form": "매운맛도 있어?",
          "original_form": "매운맛도 있어?",
          "speaker_id": "2",
          "time": "20210824 00:16"
        },
        {
          "id": "MDRW2100011061.1.27",
          "form": "나중에 먹어봐 이건 토안하길바라",
          "original_form": "나중에 먹어봐 이건 토안하길바라",
          "speaker_id": "1",
          "time": "20210824 00:16"
        }
      ]
    }
  ]
