      "utterance": [
        {
          "id": "MDRW2100021156.1.1",
          "form": "왜요? 맛없는 것을 먹었나요?",
          "original_form": "왜요? 맛없는 것을 먹었나요?",
          "speaker_id": "2",
          "time": "20210828 16:25"
        },
        {
          "id": "MDRW2100021156.1.2",
          "form": "크로와상은  그 부서지는 맛이잖아요",
          "original_form": "크로와상은  그 부서지는 맛이잖아요",
          "speaker_id": "1",
          "time": "20210828 16:25"
        },
        {
          "id": "MDRW2100021156.1.3",
          "form": "근데 크로플은 그걸 다 뭉개버리니 싫어요",
          "original_form": "근데 크로플은 그걸 다 뭉개버리니 싫어요",
          "speaker_id": "1",
          "time": "20210828 16:26"
        },
        {
          "id": "MDRW2100021156.1.4",
          "form": "아. 생각해보니 그렇네요",
          "original_form": "아. 생각해보니 그렇네요",
          "speaker_id": "2",
          "time": "20210828 16:26"
        },
        {
          "id": "MDRW2100021156.1.5",
          "form": "전 크로플을 안 먹어봐서요ㅎㅎ",
          "original_form": "전 크로플을 안 먹어봐서요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 16:26"
        },
        {
          "id": "MDRW2100021156.1.6",
          "form": "아 일반 와플은 좋아해요ㅎㅎ",
          "original_form": "아 일반 와플은 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 16:26"
        },
        {
          "id": "MDRW2100021156.1.7",
          "form": "사과잼에 버터크림 바른거요",
          "original_form": "사과잼에 버터크림 바른거요",
          "speaker_id": "1",
          "time": "20210828 16:27"
        },
        {
          "id": "MDRW2100021156.1.8",
          "form": "그거 맛있죠",
          "original_form": "그거 맛있죠",
          "speaker_id": "2",
          "time": "20210828 16:27"
        },
        {
          "id": "MDRW2100021156.1.9",
          "form": "요새는 길거리에서 잘 못봤어요",
          "original_form": "요새는 길거리에서 잘 못봤어요",
          "speaker_id": "2",
          "time": "20210828 16:27"
        },
        {
          "id": "MDRW2100021156.1.10",
          "form": "저도요ㅜ 왜 없는지",
          "original_form": "저도요ㅜ 왜 없는지",
          "speaker_id": "1",
          "time": "20210828 16:27"
        },
        {
          "id": "MDRW2100021156.1.11",
          "form": "그나마 흑당밀크티 맛집서 배달이 돼요",
          "original_form": "그나마 흑당밀크티 맛집서 배달이 돼요",
          "speaker_id": "1",
          "time": "20210828 16:27"
        },
        {
          "id": "MDRW2100021156.1.12",
          "form": "오 잘됐네요 우리 집 근처에도 있었으면",
          "original_form": "오 잘됐네요 우리 집 근처에도 있었으면",
          "speaker_id": "2",
          "time": "20210828 16:28"
        },
        {
          "id": "MDRW2100021156.1.13",
          "form": "배달앱서 검색해보면 나올 거예요",
          "original_form": "배달앱서 검색해보면 나올 거예요",
          "speaker_id": "1",
          "time": "20210828 16:28"
        },
        {
          "id": "MDRW2100021156.1.14",
          "form": "옛날 와플로 검색하세요",
          "original_form": "옛날 와플로 검색하세요",
          "speaker_id": "1",
          "time": "20210828 16:28"
        },
        {
          "id": "MDRW2100021156.1.15",
          "form": "아 요샌 기본이 아니라 옛날애플이라 부르나요",
          "original_form": "아 요샌 기본이 아니라 옛날애플이라 부르나요",
          "speaker_id": "2",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100021156.1.16",
          "form": "씁쓸하네요",
          "original_form": "씁쓸하네요",
          "speaker_id": "2",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100021156.1.17",
          "form": "요새 생크림이 듬뿍든 와플이 기본인듯해요",
          "original_form": "요새 생크림이 듬뿍든 와플이 기본인듯해요",
          "speaker_id": "1",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100021156.1.18",
          "form": "아 뚱카롱처럼 말이지요",
          "original_form": "아 뚱카롱처럼 말이지요",
          "speaker_id": "2",
          "time": "20210828 16:30"
        },
        {
          "id": "MDRW2100021156.1.19",
          "form": "생크림이라 느끼하진 않겠네요",
          "original_form": "생크림이라 느끼하진 않겠네요",
          "speaker_id": "2",
          "time": "20210828 16:30"
        },
        {
          "id": "MDRW2100021156.1.20",
          "form": "그게ㅜ 저가 생크림 쓰면 엄청 느끼해요",
          "original_form": "그게ㅜ 저가 생크림 쓰면 엄청 느끼해요",
          "speaker_id": "1",
          "time": "20210828 16:30"
        },
        {
          "id": "MDRW2100021156.1.21",
          "form": "아이고야 그러려면 많이 넣지를 말지",
          "original_form": "아이고야 그러려면 많이 넣지를 말지",
          "speaker_id": "2",
          "time": "20210828 16:30"
        },
        {
          "id": "MDRW2100021156.1.22",
          "form": "전에 그거 먹고 배탈났었네요",
          "original_form": "전에 그거 먹고 배탈났었네요",
          "speaker_id": "1",
          "time": "20210828 16:31"
        }
      ]
