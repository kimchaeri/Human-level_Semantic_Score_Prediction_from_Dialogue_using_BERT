      "utterance": [
        {
          "id": "MDRW2100018689.1.1",
          "form": "학생이신가요ㅎㅎ",
          "original_form": "학생이신가요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100018689.1.2",
          "form": "아니요 학생이세요?",
          "original_form": "아니요 학생이세요?",
          "speaker_id": "1",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100018689.1.3",
          "form": "아니요 졸업한지",
          "original_form": "아니요 졸업한지",
          "speaker_id": "2",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018689.1.4",
          "form": "8년정도....",
          "original_form": "8년정도....",
          "speaker_id": "2",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018689.1.5",
          "form": "꽤 되셨구나",
          "original_form": "꽤 되셨구나",
          "speaker_id": "1",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018689.1.6",
          "form": "요즘은 엠티를 못가는곳이 많대요",
          "original_form": "요즘은 엠티를 못가는곳이 많대요",
          "speaker_id": "1",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018689.1.7",
          "form": "저희는 엠티매년갔었는데...",
          "original_form": "저희는 엠티매년갔었는데...",
          "speaker_id": "1",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018689.1.8",
          "form": "요새요? 불가능하죠 ㅎㅎ",
          "original_form": "요새요? 불가능하죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018689.1.9",
          "form": "망할 코로나",
          "original_form": "망할 코로나",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018689.1.10",
          "form": "수업도 못하지 싶은데요 제대로",
          "original_form": "수업도 못하지 싶은데요 제대로",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018689.1.11",
          "form": "그죠 실습수업 들은 비대면 때문에 고생이래요",
          "original_form": "그죠 실습수업 들은 비대면 때문에 고생이래요",
          "speaker_id": "1",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100018689.1.12",
          "form": "그럼 고생이죠 재미도 없고",
          "original_form": "그럼 고생이죠 재미도 없고",
          "speaker_id": "2",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100018689.1.13",
          "form": "학교다니실때 엠티 많이가셨나요?",
          "original_form": "학교다니실때 엠티 많이가셨나요?",
          "speaker_id": "1",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100018689.1.14",
          "form": "네 많이 갔어요 name1님은?",
          "original_form": "네 많이 갔어요 &name1&님은?",
          "speaker_id": "2",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100018689.1.15",
          "form": "새내기때 한번가고 안갔어요",
          "original_form": "새내기때 한번가고 안갔어요",
          "speaker_id": "1",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100018689.1.16",
          "form": "술을 안좋아하다보니 끼기 애매해서요",
          "original_form": "술을 안좋아하다보니 끼기 애매해서요",
          "speaker_id": "1",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100018689.1.17",
          "form": "ㅋㅋ안좋아하시는군요",
          "original_form": "ㅋㅋ안좋아하시는군요",
          "speaker_id": "2",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100018689.1.18",
          "form": "학과는 취업에 도움되셨나요",
          "original_form": "학과는 취업에 도움되셨나요",
          "speaker_id": "2",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100018689.1.19",
          "form": "아니요 다른쪽 공부를해서 전공에상관 없이 일하고있어요",
          "original_form": "아니요 다른쪽 공부를해서 전공에상관 없이 일하고있어요",
          "speaker_id": "1",
          "time": "20210827 16:49"
        },
        {
          "id": "MDRW2100018689.1.20",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210827 16:49"
        },
        {
          "id": "MDRW2100018689.1.21",
          "form": "첫직장은 연관 좀 있었는데",
          "original_form": "첫직장은 연관 좀 있었는데",
          "speaker_id": "2",
          "time": "20210827 18:38"
        },
        {
          "id": "MDRW2100018689.1.22",
          "form": "지금은 상관 없어졌어요 ㅎㅎ",
          "original_form": "지금은 상관 없어졌어요 ㅎㅎ",
