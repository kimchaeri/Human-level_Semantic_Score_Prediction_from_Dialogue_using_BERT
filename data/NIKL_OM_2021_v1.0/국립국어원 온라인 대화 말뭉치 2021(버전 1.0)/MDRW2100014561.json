      "utterance": [
        {
          "id": "MDRW2100014561.1.1",
          "form": "여행계획하신거 있으신가요?",
          "original_form": "여행계획하신거 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014561.1.2",
          "form": "허어엉 아뇨",
          "original_form": "허어엉 아뇨",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014561.1.3",
          "form": "사실 코로나 전에 대만 여행갈라했거든요",
          "original_form": "사실 코로나 전에 대만 여행갈라했거든요",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014561.1.4",
          "form": "근데 일케됐어요",
          "original_form": "근데 일케됐어요",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014561.1.5",
          "form": "헉..ㅠ",
          "original_form": "헉..ㅠ",
          "speaker_id": "1",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014561.1.6",
          "form": "저도 대만 가보고싶었는데",
          "original_form": "저도 대만 가보고싶었는데",
          "speaker_id": "1",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014561.1.7",
          "form": "저는 곧 강릉 갈 계획이에요",
          "original_form": "저는 곧 강릉 갈 계획이에요",
          "speaker_id": "1",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100014561.1.8",
          "form": "강릉!!!!",
          "original_form": "강릉!!!!",
          "speaker_id": "2",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100014561.1.9",
          "form": "뭐가 유명한가요 강릉",
          "original_form": "뭐가 유명한가요 강릉",
          "speaker_id": "2",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100014561.1.10",
          "form": "뭐땜시 강릉으로 정하셨나요",
          "original_form": "뭐땜시 강릉으로 정하셨나요",
          "speaker_id": "2",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100014561.1.11",
          "form": "툇마루 커피도 유명하고",
          "original_form": "툇마루 커피도 유명하고",
          "speaker_id": "1",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100014561.1.12",
          "form": "꼬막이나 닭강정도 유명하죠!!",
          "original_form": "꼬막이나 닭강정도 유명하죠!!",
          "speaker_id": "1",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100014561.1.13",
          "form": "물회!!",
          "original_form": "물회!!",
          "speaker_id": "1",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100014561.1.14",
          "form": "강릉으로 정한건..작년부터 같이 여행가자고 조른 동생이 있었는데",
          "original_form": "강릉으로 정한건..작년부터 같이 여행가자고 조른 동생이 있었는데",
          "speaker_id": "1",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100014561.1.15",
          "form": "1년동안 강릉강릉노래를 불러서 가기로했어요!",
          "original_form": "1년동안 강릉강릉노래를 불러서 가기로했어요!",
          "speaker_id": "1",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100014561.1.16",
          "form": "어어어억 물회!!!",
          "original_form": "어어어억 물회!!!",
          "speaker_id": "2",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014561.1.17",
          "form": "닭강정",
          "original_form": "닭강정",
          "speaker_id": "2",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014561.1.18",
          "form": "아 맞어요 닭강정 유명하다 했어요",
          "original_form": "아 맞어요 닭강정 유명하다 했어요",
          "speaker_id": "2",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014561.1.19",
          "form": "안반데기라고",
          "original_form": "안반데기라고",
          "speaker_id": "1",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014561.1.20",
          "form": "별보는곳도 유명해요!!",
          "original_form": "별보는곳도 유명해요!!",
          "speaker_id": "1",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014561.1.21",
          "form": "왐마 동생 데리고 여행이요?  좋은 분이시구나",
          "original_form": "왐마 동생 데리고 여행이요?  좋은 분이시구나",
          "speaker_id": "2",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014561.1.22",
          "form": "동네친구에요 ㅋㅋㅋ나이만 한살 어린!!",
          "original_form": "동네친구에요 ㅋㅋㅋ나이만 한살 어린!!",
          "speaker_id": "1",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014561.1.23",
          "form": "아아 그렇다면 아주 즐거운 여행 가능이죸ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아아 그렇다면 아주 즐거운 여행 가능이죸ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014561.1.24",
          "form": "안반데기..오늘 꿀팁 많이 얻어갑니다",
          "original_form": "안반데기..오늘 꿀팁 많이 얻어갑니다",
          "speaker_id": "2",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014561.1.25",
          "form": "메모하면서 하는 중",
          "original_form": "메모하면서 하는 중",
          "speaker_id": "2",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014561.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ뇌에만 메모하시는거잖아요 ㅎ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ뇌에만 메모하시는거잖아요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100014561.1.27",
