        },
        {
          "id": "MDRW2100016248.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100016248.1.3",
          "form": "최근에 머 사셨나요",
          "original_form": "최근에 머 사셨나요",
          "speaker_id": "1",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100016248.1.4",
          "form": "제가 산건 아니고 방금 시계받았어요",
          "original_form": "제가 산건 아니고 방금 시계받았어요",
          "speaker_id": "2",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100016248.1.5",
          "form": "졸업선물로요",
          "original_form": "졸업선물로요",
          "speaker_id": "2",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100016248.1.6",
          "form": "매우매우신나요",
          "original_form": "매우매우신나요",
          "speaker_id": "2",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100016248.1.7",
          "form": "name1님은 사신 것 있으신가옷?",
          "original_form": "&name1&님은 사신 것 있으신가옷?",
          "speaker_id": "2",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100016248.1.8",
          "form": "시계요?",
          "original_form": "시계요?",
          "speaker_id": "1",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100016248.1.9",
          "form": "뭘 졸업하샸는데요?",
          "original_form": "뭘 졸업하샸는데요?",
          "speaker_id": "1",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100016248.1.10",
          "form": "고닥교요",
          "original_form": "고닥교요",
          "speaker_id": "2",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100016248.1.11",
          "form": "네 손목시계받았어옷",
          "original_form": "네 손목시계받았어옷",
          "speaker_id": "2",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100016248.1.12",
          "form": "엄마꺼 계속 차고 다녔었거든요",
          "original_form": "엄마꺼 계속 차고 다녔었거든요",
          "speaker_id": "2",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100016248.1.13",
          "form": "제 시계가 시계바늘이 와장창 되는 바람에",
          "original_form": "제 시계가 시계바늘이 와장창 되는 바람에",
          "speaker_id": "2",
          "time": "20210828 10:20"
        },
        {
          "id": "MDRW2100016248.1.14",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210828 10:20"
        },
        {
          "id": "MDRW2100016248.1.15",
          "form": "전 운동기구 샀어요",
          "original_form": "전 운동기구 샀어요",
          "speaker_id": "1",
          "time": "20210828 10:20"
        },
        {
          "id": "MDRW2100016248.1.16",
          "form": "뭐사셨나요?",
          "original_form": "뭐사셨나요?",
          "speaker_id": "2",
          "time": "20210828 10:20"
        },
        {
          "id": "MDRW2100016248.1.17",
          "form": "튜빙밴드요",
          "original_form": "튜빙밴드요",
          "speaker_id": "1",
          "time": "20210828 10:21"
        },
        {
          "id": "MDRW2100016248.1.18",
          "form": "근력운동 조질겁니다",
          "original_form": "근력운동 조질겁니다",
          "speaker_id": "1",
          "time": "20210828 10:21"
        },
        {
          "id": "MDRW2100016248.1.19",
          "form": "재밌네요",
          "original_form": "재밌네요",
          "speaker_id": "2",
          "time": "20210828 10:22"
        },
        {
          "id": "MDRW2100016248.1.20",
          "form": "종아리 알은 뭘로 푸세요 그럼 ?",
          "original_form": "종아리 알은 뭘로 푸세요 그럼 ?",
          "speaker_id": "2",
          "time": "20210828 10:23"
        },
        {
          "id": "MDRW2100016248.1.21",
          "form": "아 근력이 종아리가 아닌가",
          "original_form": "아 근력이 종아리가 아닌가",
          "speaker_id": "2",
          "time": "20210828 10:23"
        },
        {
          "id": "MDRW2100016248.1.22",
          "form": "ㅈㅅ요 제가 좀 무식해서",
          "original_form": "ㅈㅅ요 제가 좀 무식해서",
          "speaker_id": "2",
          "time": "20210828 10:23"
        },
        {
          "id": "MDRW2100016248.1.23",
          "form": "그건 스트레칭이죠",
          "original_form": "그건 스트레칭이죠",
          "speaker_id": "1",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100016248.1.24",
          "form": "종아리 알 안 풀어요",
          "original_form": "종아리 알 안 풀어요",
          "speaker_id": "1",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100016248.1.25",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100016248.1.26",
          "form": "다리 아프면 다리 올리고 자면 돼요",
          "original_form": "다리 아프면 다리 올리고 자면 돼요",
          "speaker_id": "1",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100016248.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100016248.1.28",
          "form": "저 가디건 사고 싶은데",
          "original_form": "저 가디건 사고 싶은데",
          "speaker_id": "2",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100016248.1.29",
          "form": "혹시 추천해주실래요?",
          "original_form": "혹시 추천해주실래요?",
          "speaker_id": "2",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100016248.1.30",
          "form": "웬지 의류에 관심이 많을 것 같아서어ㅗ",
          "original_form": "웬지 의류에 관심이 많을 것 같아서어ㅗ",
          "speaker_id": "2",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100016248.1.31",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100016248.1.32",
          "form": "완두색 추천드립니다",
          "original_form": "완두색 추천드립니다",
          "speaker_id": "1",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100016248.1.33",
          "form": "꼬임이 큰거요",
          "original_form": "꼬임이 큰거요",
          "speaker_id": "1",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100016248.1.34",
          "form": "오 찾아볼게요",
          "original_form": "오 찾아볼게요",
          "speaker_id": "2",
          "time": "20210828 10:26"
        }
      ]
    }
