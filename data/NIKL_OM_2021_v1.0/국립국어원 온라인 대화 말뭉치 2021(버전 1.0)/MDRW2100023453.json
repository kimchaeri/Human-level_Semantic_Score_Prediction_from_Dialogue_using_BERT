        {
          "id": "MDRW2100023453.1.6",
          "form": "판에서 밧어",
          "original_form": "판에서 밧어",
          "speaker_id": "2",
          "time": "20210830 03:26"
        },
        {
          "id": "MDRW2100023453.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 03:26"
        },
        {
          "id": "MDRW2100023453.1.8",
          "form": "세상에사람은많으니까",
          "original_form": "세상에사람은많으니까",
          "speaker_id": "1",
          "time": "20210830 03:26"
        },
        {
          "id": "MDRW2100023453.1.9",
          "form": "그건 그래...",
          "original_form": "그건 그래...",
          "speaker_id": "2",
          "time": "20210830 03:27"
        },
        {
          "id": "MDRW2100023453.1.10",
