      "utterance": [
        {
          "id": "MDRW2100010721.1.1",
          "form": "봉사활동도 하신 적 있나요?",
          "original_form": "봉사활동도 하신 적 있나요?",
          "speaker_id": "1",
          "time": "20210823 21:08"
        },
        {
          "id": "MDRW2100010721.1.2",
          "form": "코로나 전에는 자주 했었어요",
          "original_form": "코로나 전에는 자주 했었어요",
          "speaker_id": "2",
          "time": "20210823 21:12"
        },
        {
          "id": "MDRW2100010721.1.3",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "2",
          "time": "20210823 21:13"
        },
        {
          "id": "MDRW2100010721.1.4",
          "form": "저는 대학생 땐 했는데 그 후론 안 했네요ㅜ",
          "original_form": "저는 대학생 땐 했는데 그 후론 안 했네요ㅜ",
          "speaker_id": "1",
          "time": "20210823 21:28"
        },
        {
          "id": "MDRW2100010721.1.5",
          "form": "어떤거 하셨어요?",
          "original_form": "어떤거 하셨어요?",
          "speaker_id": "1",
          "time": "20210823 21:28"
        },
        {
          "id": "MDRW2100010721.1.6",
          "form": "주로 교육 봉사하고 복지관에서 3개월 정도 했던 거 같아요",
          "original_form": "주로 교육 봉사하고 복지관에서 3개월 정도 했던 거 같아요",
          "speaker_id": "2",
          "time": "20210823 21:30"
        },
        {
          "id": "MDRW2100010721.1.7",
          "form": "복지관은 어떤분들이 계신 곳인가요??",
          "original_form": "복지관은 어떤분들이 계신 곳인가요??",
          "speaker_id": "1",
          "time": "20210823 21:37"
        },
        {
          "id": "MDRW2100010721.1.8",
          "form": "노인 복지관이었어요! 토요일마다 노인분들 대상으로 프로그램을 진행했는데 보조하는 역할을 맡았어요",
          "original_form": "노인 복지관이었어요! 토요일마다 노인분들 대상으로 프로그램을 진행했는데 보조하는 역할을 맡았어요",
          "speaker_id": "2",
          "time": "20210823 21:43"
        },
        {
          "id": "MDRW2100010721.1.9",
          "form": "아 저희 집 근처에도 있는 것 같네요!",
          "original_form": "아 저희 집 근처에도 있는 것 같네요!",
          "speaker_id": "1",
          "time": "20210823 23:06"
        },
        {
          "id": "MDRW2100010721.1.10",
          "form": "힘들진 않으셨나요?",
          "original_form": "힘들진 않으셨나요?",
          "speaker_id": "1",
          "time": "20210823 23:06"
        },
        {
          "id": "MDRW2100010721.1.11",
          "form": "안내하고 프로그램 전후로 청소 같은 거 해서 크게 어렵진 않았어요",
          "original_form": "안내하고 프로그램 전후로 청소 같은 거 해서 크게 어렵진 않았어요",
          "speaker_id": "2",
          "time": "20210824 07:08"
        },
        {
          "id": "MDRW2100010721.1.12",
          "form": "name1은 어떤 봉사활동 하셨나요?",
          "original_form": "&name1&은 어떤 봉사활동 하셨나요?",
          "speaker_id": "2",
          "time": "20210824 07:08"
        },
        {
          "id": "MDRW2100010721.1.13",
          "form": "저는 캠페인 같은거 참여하거나",
          "original_form": "저는 캠페인 같은거 참여하거나",
          "speaker_id": "1",
          "time": "20210824 07:49"
        },
        {
          "id": "MDRW2100010721.1.14",
          "form": "유기견 보호소요!",
          "original_form": "유기견 보호소요!",
          "speaker_id": "1",
          "time": "20210824 07:49"
        },
        {
          "id": "MDRW2100010721.1.15",
          "form": "유기견 보호소에서는 주로 어떤 활동을 하나요?",
          "original_form": "유기견 보호소에서는 주로 어떤 활동을 하나요?",
          "speaker_id": "2",
          "time": "20210824 07:49"
        },
        {
          "id": "MDRW2100010721.1.16",
          "form": "밥 주고 똥 치워주고 사료 날랐습니다ㅋㅋㅋ",
          "original_form": "밥 주고 똥 치워주고 사료 날랐습니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 08:17"
        },
        {
          "id": "MDRW2100010721.1.17",
          "form": "코로나 끝나면 다시 하실 예정인가요??",
          "original_form": "코로나 끝나면 다시 하실 예정인가요??",
          "speaker_id": "1",
          "time": "20210824 08:18"
        },
        {
          "id": "MDRW2100010721.1.18",
          "form": "시간 맞춰서 틈틈히 하려고요",
          "original_form": "시간 맞춰서 틈틈히 하려고요",
          "speaker_id": "2",
          "time": "20210824 08:19"
        },
        {
          "id": "MDRW2100010721.1.19",
          "form": "하면 주말에나 할 거 같긴 해요",
          "original_form": "하면 주말에나 할 거 같긴 해요",
