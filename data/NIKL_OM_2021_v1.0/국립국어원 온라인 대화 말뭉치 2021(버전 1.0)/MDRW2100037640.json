      "utterance": [
        {
          "id": "MDRW2100037640.1.1",
          "form": "네 교촌 좋아해요 ㅎㅎ 없어서 못먹죠",
          "original_form": "네 교촌 좋아해요 ㅎㅎ 없어서 못먹죠",
          "speaker_id": "2",
          "time": "20210901 14:03"
        },
        {
          "id": "MDRW2100037640.1.2",
          "form": "오 저는 허니콤보 좋아하는데 무슨 메뉴 좋아하시나요?",
          "original_form": "오 저는 허니콤보 좋아하는데 무슨 메뉴 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210901 14:03"
        },
        {
          "id": "MDRW2100037640.1.3",
          "form": "저도 허니콤보좋아하구요 윙세트좋아요",
          "original_form": "저도 허니콤보좋아하구요 윙세트좋아요",
          "speaker_id": "2",
          "time": "20210901 14:04"
        },
        {
          "id": "MDRW2100037640.1.4",
          "form": "허니콤보 맛있더라구요!",
          "original_form": "허니콤보 맛있더라구요!",
          "speaker_id": "1",
          "time": "20210901 14:05"
        },
        {
          "id": "MDRW2100037640.1.5",
          "form": "매운건 잘드세요?",
          "original_form": "매운건 잘드세요?",
          "speaker_id": "1",
          "time": "20210901 14:05"
        },
        {
          "id": "MDRW2100037640.1.6",
          "form": "아뇨 신라면 정도만 먹을 수 있어요",
          "original_form": "아뇨 신라면 정도만 먹을 수 있어요",
          "speaker_id": "2",
          "time": "20210901 14:06"
        },
        {
          "id": "MDRW2100037640.1.7",
          "form": "오 저도 매운거 잘못먹어요ㅎㅎ",
          "original_form": "오 저도 매운거 잘못먹어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 14:07"
        },
        {
          "id": "MDRW2100037640.1.8",
          "form": "그럼 레드콤보는 못드시나요?",
          "original_form": "그럼 레드콤보는 못드시나요?",
          "speaker_id": "1",
          "time": "20210901 14:07"
        },
        {
          "id": "MDRW2100037640.1.9",
          "form": "못먹는 정돈 아닌데 맵더라구요",
          "original_form": "못먹는 정돈 아닌데 맵더라구요",
          "speaker_id": "2",
          "time": "20210901 14:08"
        },
        {
          "id": "MDRW2100037640.1.10",
          "form": "어떨땐 배아파요",
          "original_form": "어떨땐 배아파요",
          "speaker_id": "2",
          "time": "20210901 14:08"
        },
        {
          "id": "MDRW2100037640.1.11",
          "form": "역시 레드보단 허니콤보가 맛있는거 같아요...",
          "original_form": "역시 레드보단 허니콤보가 맛있는거 같아요...",
          "speaker_id": "1",
          "time": "20210901 14:08"
        },
        {
          "id": "MDRW2100037640.1.12",
          "form": "피자도 좋아하시나요?",
          "original_form": "피자도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210901 14:09"
        },
        {
          "id": "MDRW2100037640.1.13",
          "form": "네 피자 좋아해요 ㅎㅎ",
          "original_form": "네 피자 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 14:09"
        },
        {
          "id": "MDRW2100037640.1.14",
          "form": "저는 포테이토 피자 좋아하는데",
          "original_form": "저는 포테이토 피자 좋아하는데",
          "speaker_id": "1",
          "time": "20210901 14:10"
        },
        {
          "id": "MDRW2100037640.1.15",
          "form": "어떤 피자 좋아하시나요?",
          "original_form": "어떤 피자 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210901 14:10"
        },
        {
          "id": "MDRW2100037640.1.16",
          "form": "저도 포테이토 피자 좋아요 치즈 피자도 좋아요",
          "original_form": "저도 포테이토 피자 좋아요 치즈 피자도 좋아요",
          "speaker_id": "2",
          "time": "20210901 14:11"
        },
        {
          "id": "MDRW2100037640.1.17",
          "form": "배달자주시키세요?",
          "original_form": "배달자주시키세요?",
          "speaker_id": "2",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100037640.1.18",
          "form": "네 최근들어 자주 시키게 되더라구요ㅠㅠ",
          "original_form": "네 최근들어 자주 시키게 되더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100037640.1.19",
          "form": "배달 자주 시키시나요?",
          "original_form": "배달 자주 시키시나요?",
          "speaker_id": "1",
