      "utterance": [
        {
          "id": "MDRW2100034114.1.1",
          "form": "지금 학생이세요 ?",
          "original_form": "지금 학생이세요 ?",
          "speaker_id": "1",
          "time": "20210827 16:15"
        },
        {
          "id": "MDRW2100034114.1.2",
          "form": "네 대학생입니다!",
          "original_form": "네 대학생입니다!",
          "speaker_id": "2",
          "time": "20210827 16:15"
        },
        {
          "id": "MDRW2100034114.1.3",
          "form": "오 ! 아직 개강 안했죠 ??",
          "original_form": "오 ! 아직 개강 안했죠 ??",
          "speaker_id": "1",
          "time": "20210827 16:16"
        },
        {
          "id": "MDRW2100034114.1.4",
          "form": "다음 달에 개강해요",
          "original_form": "다음 달에 개강해요",
          "speaker_id": "2",
          "time": "20210827 16:17"
        },
        {
          "id": "MDRW2100034114.1.5",
          "form": "몇학년이에요?",
          "original_form": "몇학년이에요?",
          "speaker_id": "1",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100034114.1.6",
          "form": "1학년이요! 올해 입학했어요",
          "original_form": "1학년이요! 올해 입학했어요",
          "speaker_id": "2",
          "time": "20210827 16:22"
        },
        {
          "id": "MDRW2100034114.1.7",
          "form": "헐헐 ㅋㅋㅋ 완전 설레겠어요",
          "original_form": "헐헐 ㅋㅋㅋ 완전 설레겠어요",
          "speaker_id": "1",
          "time": "20210827 16:23"
        },
        {
          "id": "MDRW2100034114.1.8",
          "form": "1학기때 대면수업했죠???",
          "original_form": "1학기때 대면수업했죠???",
          "speaker_id": "1",
          "time": "20210827 16:23"
        },
        {
          "id": "MDRW2100034114.1.9",
          "form": "대면이랑 비대면 섞어서 했는데 거리두기 3단계부터는 거의다 비대면으로 전환했었어요",
          "original_form": "대면이랑 비대면 섞어서 했는데 거리두기 3단계부터는 거의다 비대면으로 전환했었어요",
          "speaker_id": "2",
          "time": "20210827 16:24"
        },
        {
          "id": "MDRW2100034114.1.10",
          "form": "아 헐 ㅠㅠ 친해진 동기는 있어요?",
          "original_form": "아 헐 ㅠㅠ 친해진 동기는 있어요?",
          "speaker_id": "1",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100034114.1.11",
          "form": "같은 수업 들었던 동기 몇 명이랑은 좀 친해졌어요!",
          "original_form": "같은 수업 들었던 동기 몇 명이랑은 좀 친해졌어요!",
          "speaker_id": "2",
          "time": "20210827 16:31"
        },
        {
          "id": "MDRW2100034114.1.12",
          "form": "오오 ! 원래 막 엠티도 가고 해야되는데",
          "original_form": "오오 ! 원래 막 엠티도 가고 해야되는데",
          "speaker_id": "1",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100034114.1.13",
          "form": "그런거 아예 안했죠 ㅠㅠ ?",
          "original_form": "그런거 아예 안했죠 ㅠㅠ ?",
          "speaker_id": "1",
          "time": "20210827 16:34"
        },
        {
          "id": "MDRW2100034114.1.14",
          "form": "네ㅠㅠ 작년부터 다 취소됐다고 하더라고요",
          "original_form": "네ㅠㅠ 작년부터 다 취소됐다고 하더라고요",
          "speaker_id": "2",
          "time": "20210827 16:35"
        },
        {
          "id": "MDRW2100034114.1.15",
          "form": "ㅠㅠ 아쉽겠어요 그런곳에서 선배들이랑 친해져야되는데 !",
          "original_form": "ㅠㅠ 아쉽겠어요 그런곳에서 선배들이랑 친해져야되는데 !",
          "speaker_id": "1",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100034114.1.16",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
