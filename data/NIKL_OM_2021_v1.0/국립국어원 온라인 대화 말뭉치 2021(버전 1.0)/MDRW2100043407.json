      "utterance": [
        {
          "id": "MDRW2100043407.1.1",
          "form": "혹시 초코찰떡꼬치 아세요?",
          "original_form": "혹시 초코찰떡꼬치 아세요?",
          "speaker_id": "2",
          "time": "20210907 18:51"
        },
        {
          "id": "MDRW2100043407.1.2",
          "form": "그 편의점에 파는거 말씀하시는건가요?!",
          "original_form": "그 편의점에 파는거 말씀하시는건가요?!",
          "speaker_id": "1",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100043407.1.3",
          "form": "네 맞아요!!",
          "original_form": "네 맞아요!!",
          "speaker_id": "2",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043407.1.4",
          "form": "드셔보셨나요?",
          "original_form": "드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043407.1.5",
          "form": "아녀 못먹어봤어요 쫀득하구 맛있나요?!",
          "original_form": "아녀 못먹어봤어요 쫀득하구 맛있나요?!",
          "speaker_id": "1",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043407.1.6",
          "form": "네네!!",
          "original_form": "네네!!",
          "speaker_id": "2",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100043407.1.7",
          "form": "그거 스트레스받을때 드세요",
          "original_form": "그거 스트레스받을때 드세요",
          "speaker_id": "2",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100043407.1.8",
          "form": "당뇨오는 맛이에요",
          "original_form": "당뇨오는 맛이에요",
          "speaker_id": "2",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100043407.1.9",
          "form": "이 세상 단맛이 아니랍니다",
          "original_form": "이 세상 단맛이 아니랍니다",
          "speaker_id": "2",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100043407.1.10",
          "form": "헉 .. 저 완전 단맛 좋아해요 담에 사먹어봐야겠네욬ㅋ",
          "original_form": "헉 .. 저 완전 단맛 좋아해요 담에 사먹어봐야겠네욬ㅋ",
          "speaker_id": "1",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100043407.1.11",
          "form": "네네 달아서 머리가 띵해질수 있으니 주의하세요!!!",
          "original_form": "네네 달아서 머리가 띵해질수 있으니 주의하세요!!!",
          "speaker_id": "2",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100043407.1.12",
          "form": "가끔 그렇게 미치게 단거가 끌리는 날이 있죠",
          "original_form": "가끔 그렇게 미치게 단거가 끌리는 날이 있죠",
          "speaker_id": "1",
          "time": "20210907 19:02"
        },
        {
          "id": "MDRW2100043407.1.13",
          "form": "다음에 편의점가서 있으면 사야겠어요 ㅎㅎ",
          "original_form": "다음에 편의점가서 있으면 사야겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:02"
        },
        {
          "id": "MDRW2100043407.1.14",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "2",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100043407.1.15",
          "form": "전지금 편의점가서 불닭볶음면을 사야겠어요",
          "original_form": "전지금 편의점가서 불닭볶음면을 사야겠어요",
          "speaker_id": "2",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100043407.1.16",
          "form": "오 저 오늘 불닭먹었어요 ㅋㅋㅋ",
          "original_form": "오 저 오늘 불닭먹었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100043407.1.17",
          "form": "불닭 진짜 맛있죠",
          "original_form": "불닭 진짜 맛있죠",
          "speaker_id": "1",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100043407.1.18",
          "form": "헐 역시 국민라면",
          "original_form": "헐 역시 국민라면",
          "speaker_id": "2",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043407.1.19",
          "form": "매운라면 최고입니다",
          "original_form": "매운라면 최고입니다",
          "speaker_id": "2",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043407.1.20",
          "form": "맞아요 진짜 맛있죠.. 불닭 개발하신 분 천재예요",
          "original_form": "맞아요 진짜 맛있죠.. 불닭 개발하신 분 천재예요",
          "speaker_id": "1",
          "time": "20210907 19:10"
        },
        {
          "id": "MDRW2100043407.1.21",
          "form": "진짜요",
          "original_form": "진짜요",
          "speaker_id": "2",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043407.1.22",
          "form": "전국민 매운맛의 기준을 높여놨어요",
          "original_form": "전국민 매운맛의 기준을 높여놨어요",
