      "utterance": [
        {
          "id": "MDRW2100012746.1.1",
          "form": "전에 템플스테이 하고 싶다고 했잖아",
          "original_form": "전에 템플스테이 하고 싶다고 했잖아",
          "speaker_id": "1",
          "time": "20210825 00:38"
        },
        {
          "id": "MDRW2100012746.1.2",
          "form": "옛날이지 지금은 글쎄",
          "original_form": "옛날이지 지금은 글쎄",
          "speaker_id": "2",
          "time": "20210825 00:39"
        },
        {
          "id": "MDRW2100012746.1.3",
          "form": "ㅎㅎ영암 월출산 도갑산에 엄청 저렴하게 하는데",
          "original_form": "ㅎㅎ영암 월출산 도갑산에 엄청 저렴하게 하는데",
          "speaker_id": "1",
          "time": "20210825 00:39"
        },
        {
          "id": "MDRW2100012746.1.4",
          "form": "그래 그럼뭐",
          "original_form": "그래 그럼뭐",
          "speaker_id": "2",
          "time": "20210825 00:39"
        },
        {
          "id": "MDRW2100012746.1.5",
          "form": "여기에 둘레길이라고 있는데 엄청 유명한가봐",
          "original_form": "여기에 둘레길이라고 있는데 엄청 유명한가봐",
          "speaker_id": "1",
          "time": "20210825 00:40"
        },
        {
          "id": "MDRW2100012746.1.6",
          "form": "템플스테이랑 둘레길 걷기에는 체력이 저질이다",
          "original_form": "템플스테이랑 둘레길 걷기에는 체력이 저질이다",
          "speaker_id": "2",
          "time": "20210825 00:40"
        },
        {
          "id": "MDRW2100012746.1.7",
          "form": "ㅋㅋ그 둘레길중에서 1구간이 있는데 온천이 나온데",
          "original_form": "ㅋㅋ그 둘레길중에서 1구간이 있는데 온천이 나온데",
          "speaker_id": "1",
          "time": "20210825 00:41"
        },
        {
          "id": "MDRW2100012746.1.8",
          "form": "자연온천?",
          "original_form": "자연온천?",
          "speaker_id": "2",
          "time": "20210825 00:41"
        },
        {
          "id": "MDRW2100012746.1.9",
          "form": "엉 신기하지",
          "original_form": "엉 신기하지",
          "speaker_id": "1",
          "time": "20210825 00:41"
        },
        {
          "id": "MDRW2100012746.1.10",
          "form": "2가지중에 한가지만 하면 괜찮겠다",
          "original_form": "2가지중에 한가지만 하면 괜찮겠다",
          "speaker_id": "2",
          "time": "20210825 00:42"
        },
        {
          "id": "MDRW2100012746.1.11",
          "form": "템플스테이라고 무거운것만 아니고 여러가지 놀이프로그램도 한다네",
          "original_form": "템플스테이라고 무거운것만 아니고 여러가지 놀이프로그램도 한다네",
          "speaker_id": "1",
          "time": "20210825 00:43"
        },
        {
          "id": "MDRW2100012746.1.12",
          "form": "글쎄 난 애매",
          "original_form": "글쎄 난 애매",
          "speaker_id": "2",
          "time": "20210825 00:43"
        },
        {
          "id": "MDRW2100012746.1.13",
          "form": "근처에 낙지가 유명해서 낙지마을도 있데",
          "original_form": "근처에 낙지가 유명해서 낙지마을도 있데",
          "speaker_id": "1",
          "time": "20210825 00:44"
        },
        {
          "id": "MDRW2100012746.1.14",
          "form": "경주에 가면 벚꽃이 많잖아",
          "original_form": "경주에 가면 벚꽃이 많잖아",
