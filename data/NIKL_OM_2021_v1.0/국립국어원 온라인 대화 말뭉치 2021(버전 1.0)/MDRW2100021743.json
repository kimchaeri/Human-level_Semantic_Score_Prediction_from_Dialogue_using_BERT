      "utterance": [
        {
          "id": "MDRW2100021743.1.1",
          "form": "여행 어디 기억나세요?",
          "original_form": "여행 어디 기억나세요?",
          "speaker_id": "1",
          "time": "20210828 22:03"
        },
        {
          "id": "MDRW2100021743.1.2",
          "form": "홍콩이요 ㅋㅋ",
          "original_form": "홍콩이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:04"
        },
        {
          "id": "MDRW2100021743.1.3",
          "form": "홍콩 구룡? 어쩌구요? ㅋㅋㅋ",
          "original_form": "홍콩 구룡? 어쩌구요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:08"
        },
        {
          "id": "MDRW2100021743.1.4",
          "form": "저도 출장으로 한번 가봣어요..",
          "original_form": "저도 출장으로 한번 가봣어요..",
          "speaker_id": "1",
          "time": "20210828 22:08"
        },
        {
          "id": "MDRW2100021743.1.5",
          "form": "ㅋㅋ아녀 저는 그 쪽은 잘 안 다니고 센트럴 쪽으로 다녀서요",
          "original_form": "ㅋㅋ아녀 저는 그 쪽은 잘 안 다니고 센트럴 쪽으로 다녀서요",
          "speaker_id": "2",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100021743.1.6",
          "form": "해외 많이 다녀보셨어요?",
          "original_form": "해외 많이 다녀보셨어요?",
          "speaker_id": "2",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100021743.1.7",
          "form": "아뇨.. 중국 일본 정도요..",
          "original_form": "아뇨.. 중국 일본 정도요..",
          "speaker_id": "1",
          "time": "20210828 22:12"
        },
        {
          "id": "MDRW2100021743.1.8",
          "form": "홍콩 엄청 덥지 않앜ㅅ나요?",
          "original_form": "홍콩 엄청 덥지 않앜ㅅ나요?",
          "speaker_id": "1",
          "time": "20210828 22:12"
        },
        {
          "id": "MDRW2100021743.1.9",
          "form": "아하...",
          "original_form": "아하...",
          "speaker_id": "2",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100021743.1.10",
          "form": "저 더위를 별로 안 타서 다닐만 했어요 ㅋㅋ",
          "original_form": "저 더위를 별로 안 타서 다닐만 했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100021743.1.11",
          "form": "그럼 다른나라는 가보신 적 없으세요?",
          "original_form": "그럼 다른나라는 가보신 적 없으세요?",
          "speaker_id": "2",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100021743.1.12",
          "form": "네.. 어디가보셧어요?",
          "original_form": "네.. 어디가보셧어요?",
          "speaker_id": "1",
          "time": "20210828 22:14"
        },
        {
          "id": "MDRW2100021743.1.13",
          "form": "전 뭐 동남아 몇군데 가보고...",
          "original_form": "전 뭐 동남아 몇군데 가보고...",
          "speaker_id": "2",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021743.1.14",
          "form": "유럽 정도?? 많이는.안 다녔어요 ㅋㅋ",
          "original_form": "유럽 정도?? 많이는.안 다녔어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021743.1.15",
          "form": "ㅋㅋ 엄청 다니셧네여..",
          "original_form": "ㅋㅋ 엄청 다니셧네여..",
          "speaker_id": "1",
          "time": "20210828 22:17"
        },
        {
          "id": "MDRW2100021743.1.16",
          "form": "유럽은  여러나라 순회하셧나요?",
          "original_form": "유럽은  여러나라 순회하셧나요?",
          "speaker_id": "1",
          "time": "20210828 22:17"
        },
        {
          "id": "MDRW2100021743.1.17",
          "form": "많이는 아니구요",
          "original_form": "많이는 아니구요",
          "speaker_id": "2",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021743.1.18",
          "form": "5개 갔다왔나? 그랬어요",
          "original_form": "5개 갔다왔나? 그랬어요",
          "speaker_id": "2",
          "time": "20210828 22:19"
        },
        {
          "id": "MDRW2100021743.1.19",
          "form": "배낭 여행이엇어요?",
          "original_form": "배낭 여행이엇어요?",
          "speaker_id": "1",
          "time": "20210828 22:20"
        },
        {
          "id": "MDRW2100021743.1.20",
          "form": "ㅋㅋㅋ 아녀 저 힘든 여행 별로 안 좋아해서요",
          "original_form": "ㅋㅋㅋ 아녀 저 힘든 여행 별로 안 좋아해서요",
          "speaker_id": "2",
          "time": "20210828 22:20"
        },
        {
          "id": "MDRW2100021743.1.21",
          "form": "걍 슬렁슬렁 다녀왔어요",
          "original_form": "걍 슬렁슬렁 다녀왔어요",
          "speaker_id": "2",
          "time": "20210828 22:20"
        },
        {
          "id": "MDRW2100021743.1.22",
          "form": "요즘은 배낭 거의 극소수일듯요..",
          "original_form": "요즘은 배낭 거의 극소수일듯요..",
          "speaker_id": "1",
          "time": "20210828 22:21"
        }
