      "utterance": [
        {
          "id": "MDRW2100014780.1.1",
          "form": "재미님은 어떤노래 들으세요?",
          "original_form": "재미님은 어떤노래 들으세요?",
          "speaker_id": "2",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014780.1.2",
          "form": "저 요즘 놀뭐 보고 에스지워너비..",
          "original_form": "저 요즘 놀뭐 보고 에스지워너비..",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014780.1.3",
          "form": "너무 뻔한가요",
          "original_form": "너무 뻔한가요",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014780.1.4",
          "form": "근데 질풍가도 되게 벅차오르네요",
          "original_form": "근데 질풍가도 되게 벅차오르네요",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014780.1.5",
          "form": "왜 좋아하시는지 알 것  같아요",
          "original_form": "왜 좋아하시는지 알 것  같아요",
          "speaker_id": "1",
          "time": "20210825 20:52"
        },
        {
          "id": "MDRW2100014780.1.6",
          "form": "그쵸 그거 쾌걸근육맨 노래인데",
          "original_form": "그쵸 그거 쾌걸근육맨 노래인데",
          "speaker_id": "2",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014780.1.7",
          "form": "이런 좋은 노래를 혼자만 듣다니",
          "original_form": "이런 좋은 노래를 혼자만 듣다니",
          "speaker_id": "1",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014780.1.8",
          "form": "약간 치어리딩 노래 느낌이죠?! 그대에게처럼",
          "original_form": "약간 치어리딩 노래 느낌이죠?! 그대에게처럼",
          "speaker_id": "2",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014780.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014780.1.10",
          "form": "SG 워너비 어떤노래 젤 좋아하세요?",
          "original_form": "SG 워너비 어떤노래 젤 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014780.1.11",
          "form": "저 우리의 얘기를 쓰겠소 좋아하는데 아시나요",
          "original_form": "저 우리의 얘기를 쓰겠소 좋아하는데 아시나요",
          "speaker_id": "1",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014780.1.12",
          "form": "헉 몰라요",
          "original_form": "헉 몰라요",
          "speaker_id": "2",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014780.1.13",
          "form": "들어보고올게요",
          "original_form": "들어보고올게요",
          "speaker_id": "2",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014780.1.14",
          "form": "그럴거같았어요",
          "original_form": "그럴거같았어요",
          "speaker_id": "1",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014780.1.15",
          "form": "괜찮아요 많은 분들이 모르세요",
          "original_form": "괜찮아요 많은 분들이 모르세요",
          "speaker_id": "1",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014780.1.16",
          "form": "이석훈 증말..",
          "original_form": "이석훈 증말..",
