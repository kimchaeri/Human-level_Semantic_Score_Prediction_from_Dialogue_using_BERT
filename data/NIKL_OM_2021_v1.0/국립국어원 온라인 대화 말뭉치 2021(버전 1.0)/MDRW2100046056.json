          "speaker_id": "2",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046056.1.9",
          "form": "차도",
          "original_form": "차도",
          "speaker_id": "2",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046056.1.10",
          "form": "취향이 막 비싼거",
          "original_form": "취향이 막 비싼거",
          "speaker_id": "2",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046056.1.11",
          "form": "차도 예쁜 거 사고싶고",
          "original_form": "차도 예쁜 거 사고싶고",
          "speaker_id": "1",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046056.1.12",
          "form": "하",
          "original_form": "하",
          "speaker_id": "1",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046056.1.13",
          "form": "돈도없으면서 ㅋㅋㅋ",
          "original_form": "돈도없으면서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046056.1.14",
          "form": "정말 이 취향이란",
          "original_form": "정말 이 취향이란",
          "speaker_id": "1",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046056.1.15",
          "form": "왤케 쓸데없이 눈만 높을까요",
          "original_form": "왤케 쓸데없이 눈만 높을까요",
          "speaker_id": "1",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046056.1.16",
          "form": "이게좀",
          "original_form": "이게좀",
          "speaker_id": "2",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046056.1.17",
          "form": "핫딜하다보며네",
          "original_form": "핫딜하다보며네",
          "speaker_id": "2",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046056.1.18",
          "form": "더그렇지않니요 ㅋㅋㅋㅋ",
          "original_form": "더그렇지않니요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046056.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046056.1.20",
          "form": "소재같은거보고그러니까 ㅋㅋ",
          "original_form": "소재같은거보고그러니까 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046056.1.21",
          "form": "그러니까요ㅋㅋㅋㅋ",
          "original_form": "그러니까요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046056.1.22",
          "form": "차도 핫딜 안되나ㅋㄲㅋㅋㅋㅋ",
          "original_form": "차도 핫딜 안되나ㅋㄲㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046056.1.23",
          "form": "저렴한돈으로 고급짐연출해야해서 ㅋㅋㅋ",
          "original_form": "저렴한돈으로 고급짐연출해야해서 ㅋㅋㅋ",
