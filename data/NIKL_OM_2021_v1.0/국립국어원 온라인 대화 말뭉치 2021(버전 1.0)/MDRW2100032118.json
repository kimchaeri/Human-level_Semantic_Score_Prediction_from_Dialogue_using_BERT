        {
          "id": "MDRW2100032118.1.1",
          "form": "하이!",
          "original_form": "하이!",
          "speaker_id": "1",
          "time": "20210905 12:43"
        },
        {
          "id": "MDRW2100032118.1.2",
          "form": "하이하이",
          "original_form": "하이하이",
          "speaker_id": "2",
          "time": "20210905 12:43"
        },
        {
          "id": "MDRW2100032118.1.3",
          "form": "룸메랑은 잘지내?",
          "original_form": "룸메랑은 잘지내?",
          "speaker_id": "1",
          "time": "20210905 12:44"
        },
        {
          "id": "MDRW2100032118.1.4",
          "form": "별일없지뭐",
          "original_form": "별일없지뭐",
          "speaker_id": "2",
          "time": "20210905 12:44"
        },
        {
          "id": "MDRW2100032118.1.5",
          "form": "신기하다 니가 독립해서 혼자서 집안일도하고",
          "original_form": "신기하다 니가 독립해서 혼자서 집안일도하고",
          "speaker_id": "1",
          "time": "20210905 12:44"
        },
        {
          "id": "MDRW2100032118.1.6",
          "form": "나오면 다하는거지",
          "original_form": "나오면 다하는거지",
          "speaker_id": "2",
          "time": "20210905 12:44"
        },
        {
          "id": "MDRW2100032118.1.7",
          "form": "설거지나 욕실청소같은건 룸메가 해줘서 좋음",
          "original_form": "설거지나 욕실청소같은건 룸메가 해줘서 좋음",
          "speaker_id": "2",
          "time": "20210905 12:45"
        },
        {
          "id": "MDRW2100032118.1.8",
          "form": "설거지 귀찮지...",
          "original_form": "설거지 귀찮지...",
          "speaker_id": "1",
          "time": "20210905 12:45"
        },
        {
          "id": "MDRW2100032118.1.9",
          "form": "기름기있눈 음식은 왤케많은건지",
          "original_form": "기름기있눈 음식은 왤케많은건지",
          "speaker_id": "1",
          "time": "20210905 12:46"
        },
        {
          "id": "MDRW2100032118.1.10",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210905 12:46"
        },
        {
          "id": "MDRW2100032118.1.11",
          "form": "기름기있는거 심한거는 설거지 두탕해도 미끌하다니까...",
          "original_form": "기름기있는거 심한거는 설거지 두탕해도 미끌하다니까...",
          "speaker_id": "2",
          "time": "20210905 12:46"
        },
        {
          "id": "MDRW2100032118.1.12",
          "form": "니 룸메는 꼼꼼해서 그런거 잘하겠네",
          "original_form": "니 룸메는 꼼꼼해서 그런거 잘하겠네",
          "speaker_id": "1",
          "time": "20210905 12:46"
        },
        {
          "id": "MDRW2100032118.1.13",
          "form": "글치 아무래도 나보다 훨 잘함",
          "original_form": "글치 아무래도 나보다 훨 잘함",
          "speaker_id": "2",
          "time": "20210905 12:46"
        },
        {
          "id": "MDRW2100032118.1.14",
          "form": "비위도 약할건데 진짜 둘이 잘만났다",
          "original_form": "비위도 약할건데 진짜 둘이 잘만났다",
          "speaker_id": "1",
          "time": "20210905 12:47"
        },
        {
          "id": "MDRW2100032118.1.15",
          "form": "그니까 싫어하는 집안일이 달라서",
          "original_form": "그니까 싫어하는 집안일이 달라서",
          "speaker_id": "2",
          "time": "20210905 12:47"
        },
        {
          "id": "MDRW2100032118.1.16",
          "form": "서로대신해주면 딱임",
          "original_form": "서로대신해주면 딱임",
          "speaker_id": "2",
          "time": "20210905 12:47"
        },
        {
          "id": "MDRW2100032118.1.17",
          "form": "좋다좋아",
          "original_form": "좋다좋아",
