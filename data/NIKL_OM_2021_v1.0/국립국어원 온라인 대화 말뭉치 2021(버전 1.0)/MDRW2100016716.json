      "utterance": [
        {
          "id": "MDRW2100016716.1.1",
          "form": "좋아하는 음식 뭐 있으세요?",
          "original_form": "좋아하는 음식 뭐 있으세요?",
          "speaker_id": "1",
          "time": "20210826 14:56"
        },
        {
          "id": "MDRW2100016716.1.2",
          "form": "저는 한식을 주로 좋아해요",
          "original_form": "저는 한식을 주로 좋아해요",
          "speaker_id": "2",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100016716.1.3",
          "form": "토종한국인",
          "original_form": "토종한국인",
          "speaker_id": "2",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100016716.1.4",
          "form": "전 예전엔 양식만 좋아했는데 나이들다보니 한식이 너무 맛나요",
          "original_form": "전 예전엔 양식만 좋아했는데 나이들다보니 한식이 너무 맛나요",
          "speaker_id": "1",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100016716.1.5",
          "form": "맞아요ㅎㅎ소화기관이 노화돼서인지",
          "original_form": "맞아요ㅎㅎ소화기관이 노화돼서인지",
          "speaker_id": "2",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100016716.1.6",
          "form": "한식먹어야 속이 편안",
          "original_form": "한식먹어야 속이 편안",
          "speaker_id": "2",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100016716.1.7",
          "form": "맞아요 매운거 빼고 ㅎㅎ",
          "original_form": "맞아요 매운거 빼고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100016716.1.8",
          "form": "오 저도 맵찔이에요",
          "original_form": "오 저도 맵찔이에요",
          "speaker_id": "2",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100016716.1.9",
          "form": "저랑 같네요 ㅋㅋㅋ 전엔 좋아했는데 요샌 속이 너무 쓰리더라고요 ㅠㅠ",
          "original_form": "저랑 같네요 ㅋㅋㅋ 전엔 좋아했는데 요샌 속이 너무 쓰리더라고요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100016716.1.10",
          "form": "전 원래부터.. \r\n주로 집에서 만들어드세요?",
          "original_form": "전 원래부터.. \r\n주로 집에서 만들어드세요?",
          "speaker_id": "2",
          "time": "20210826 15:03"
        },
        {
          "id": "MDRW2100016716.1.11",
          "form": "요개 너무 시켜먹어서 되도록 안 그러려고 노력중이에요 ㅠㅠ",
          "original_form": "요개 너무 시켜먹어서 되도록 안 그러려고 노력중이에요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 15:03"
        },
        {
          "id": "MDRW2100016716.1.12",
          "form": "요즘 배달이 너무 편하니 ..그렇게 되더라구요ㅋㅋ",
          "original_form": "요즘 배달이 너무 편하니 ..그렇게 되더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:06"
        },
        {
          "id": "MDRW2100016716.1.13",
          "form": "집밥 너무 귀찮아요ㅎ",
          "original_form": "집밥 너무 귀찮아요ㅎ",
          "speaker_id": "2",
          "time": "20210826 15:06"
        },
        {
          "id": "MDRW2100016716.1.14",
          "form": "맞아요 배달 안 되는 음식이 없잖아요",
          "original_form": "맞아요 배달 안 되는 음식이 없잖아요",
          "speaker_id": "1",
          "time": "20210826 15:06"
        }
