      "utterance": [
        {
          "id": "MDRW2100002874.1.1",
          "form": "다이어트는 평생 숙제인듯",
          "original_form": "다이어트는 평생 숙제인듯",
          "speaker_id": "2",
          "time": "20210820 16:11"
        },
        {
          "id": "MDRW2100002874.1.2",
          "form": "그렇게 생각해 꾸준히 관리해야하니까",
          "original_form": "그렇게 생각해 꾸준히 관리해야하니까",
          "speaker_id": "1",
          "time": "20210820 16:16"
        },
        {
          "id": "MDRW2100002874.1.3",
          "form": "그래도 다이어트해야 건강유지할 수 있으니까",
          "original_form": "그래도 다이어트해야 건강유지할 수 있으니까",
          "speaker_id": "1",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002874.1.4",
          "form": "노력해야지",
          "original_form": "노력해야지",
          "speaker_id": "1",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002874.1.5",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210820 16:18"
        },
        {
          "id": "MDRW2100002874.1.6",
          "form": "다이어트에 성공했었어",
          "original_form": "다이어트에 성공했었어",
          "speaker_id": "2",
          "time": "20210820 16:18"
        },
        {
          "id": "MDRW2100002874.1.7",
          "form": "그때 내마음가짐도 달라지고 좋더라고",
          "original_form": "그때 내마음가짐도 달라지고 좋더라고",
          "speaker_id": "2",
          "time": "20210820 16:19"
        },
        {
          "id": "MDRW2100002874.1.8",
          "form": "돌아가고싶구만? ㅋㅋ",
          "original_form": "돌아가고싶구만? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:19"
        },
        {
          "id": "MDRW2100002874.1.9",
          "form": "그치 자신감도 생기고 긍정적으로 변하니까",
          "original_form": "그치 자신감도 생기고 긍정적으로 변하니까",
          "speaker_id": "1",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002874.1.10",
          "form": "나는 결혼후에 급격하게 살이쪄서",
          "original_form": "나는 결혼후에 급격하게 살이쪄서",
          "speaker_id": "1",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002874.1.11",
          "form": "다이어트 시작했어",
          "original_form": "다이어트 시작했어",
          "speaker_id": "1",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002874.1.12",
          "form": "몇키로쪘어?",
          "original_form": "몇키로쪘어?",
          "speaker_id": "2",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002874.1.13",
          "form": "60초반에서 70초반이 됐어",
          "original_form": "60초반에서 70초반이 됐어",
          "speaker_id": "1",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002874.1.14",
          "form": "1년반동안",
          "original_form": "1년반동안",
          "speaker_id": "1",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002874.1.15",
          "form": ".....흐음",
          "original_form": ".....흐음",
          "speaker_id": "2",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002874.1.16",
          "form": "우리남편 60후반에서 80키로 넘...",
          "original_form": "우리남편 60후반에서 80키로 넘...",
          "speaker_id": "2",
          "time": "20210820 16:24"
        },
        {
          "id": "MDRW2100002874.1.17",
          "form": "아내가 잘먹인거라고 정리하자",
          "original_form": "아내가 잘먹인거라고 정리하자",
          "speaker_id": "2",
          "time": "20210820 16:24"
        },
        {
          "id": "MDRW2100002874.1.18",
          "form": "행복하구나 너도 내남편도",
          "original_form": "행복하구나 너도 내남편도",
          "speaker_id": "2",
          "time": "20210820 16:24"
        },
        {
          "id": "MDRW2100002874.1.19",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002874.1.20",
          "form": "근데 몸무게가 60 초반이면",
          "original_form": "근데 몸무게가 60 초반이면",
          "speaker_id": "2",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002874.1.21",
          "form": "나도 아내가 너무 잘챙겨줘서",
          "original_form": "나도 아내가 너무 잘챙겨줘서",
          "speaker_id": "1",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002874.1.22",
          "form": "남자가 그래도 되는겨?",
          "original_form": "남자가 그래도 되는겨?",
          "speaker_id": "2",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002874.1.23",
          "form": "마른편이었는데",
          "original_form": "마른편이었는데",
          "speaker_id": "1",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002874.1.24",
          "form": "나랑 몇키로 차이 안나네 ㅠㅠ 자괴감 든다 name1아",
          "original_form": "나랑 몇키로 차이 안나네 ㅠㅠ 자괴감 든다 &name1&아",
          "speaker_id": "2",
          "time": "20210820 16:26"
        },
        {
          "id": "MDRW2100002874.1.25",
