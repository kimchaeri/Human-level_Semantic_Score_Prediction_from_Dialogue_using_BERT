      "utterance": [
        {
          "id": "MDRW2100046042.1.1",
          "form": "공연 좋아하세요?",
          "original_form": "공연 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100046042.1.2",
          "form": "아니요 예전에는 종종 보러다녔는데",
          "original_form": "아니요 예전에는 종종 보러다녔는데",
          "speaker_id": "2",
          "time": "20210908 09:49"
        },
        {
          "id": "MDRW2100046042.1.3",
          "form": "안 본 지 10년은 된 거 같아요",
          "original_form": "안 본 지 10년은 된 거 같아요",
          "speaker_id": "2",
          "time": "20210908 09:50"
        },
        {
          "id": "MDRW2100046042.1.4",
          "form": "오오 안보러 가시는 이유가 따로 있나요?",
          "original_form": "오오 안보러 가시는 이유가 따로 있나요?",
          "speaker_id": "1",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046042.1.5",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046042.1.6",
          "form": "결혼하고 애들 생긴 뒤로는",
          "original_form": "결혼하고 애들 생긴 뒤로는",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046042.1.7",
          "form": "못가고 있어요",
          "original_form": "못가고 있어요",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046042.1.8",
          "form": "최근에 뭐 보신 거 있나요??",
          "original_form": "최근에 뭐 보신 거 있나요??",
          "speaker_id": "2",
          "time": "20210908 09:51"
        },
        {
          "id": "MDRW2100046042.1.9",
          "form": "아무래도 쉽지 않겠네요 ㅠㅠㅋㅋ",
          "original_form": "아무래도 쉽지 않겠네요 ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046042.1.10",
          "form": "저도 학생때는 콘서트도 보고 페스티벌도 다녔는데",
          "original_form": "저도 학생때는 콘서트도 보고 페스티벌도 다녔는데",
          "speaker_id": "1",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046042.1.11",
          "form": "일하고 나니 쉽지 않은 것 같아요 ㅋㅋ",
          "original_form": "일하고 나니 쉽지 않은 것 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046042.1.12",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046042.1.13",
          "form": "시간내기가",
          "original_form": "시간내기가",
          "speaker_id": "2",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046042.1.14",
          "form": "쉽지 않은 거 같아요",
          "original_form": "쉽지 않은 거 같아요",
          "speaker_id": "2",
          "time": "20210908 09:52"
        },
        {
          "id": "MDRW2100046042.1.15",
          "form": "극장 가시나요??",
          "original_form": "극장 가시나요??",
          "speaker_id": "2",
          "time": "20210908 09:53"
        },
        {
          "id": "MDRW2100046042.1.16",
          "form": "코로나 전엔 자주 갔는데 요샌 쉽지 않네요 ㅜㅜ",
          "original_form": "코로나 전엔 자주 갔는데 요샌 쉽지 않네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046042.1.17",
          "form": "대통님은요?",
          "original_form": "대통님은요?",
          "speaker_id": "1",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046042.1.18",
          "form": "저도 코로나 터지고 나서는",
          "original_form": "저도 코로나 터지고 나서는",
          "speaker_id": "2",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046042.1.19",
          "form": "한 번도 안 간 거 같아요",
          "original_form": "한 번도 안 간 거 같아요",
          "speaker_id": "2",
          "time": "20210908 09:54"
        },
        {
          "id": "MDRW2100046042.1.20",
          "form": "아이들이랑 극장도 가고",
          "original_form": "아이들이랑 극장도 가고",
          "speaker_id": "2",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046042.1.21",
          "form": "뮤지컬도 보러 갔었는데",
          "original_form": "뮤지컬도 보러 갔었는데",
          "speaker_id": "2",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046042.1.22",
          "form": "아 그러고 보니 아이들 보는 뮤지컬은 몇 번 봤었네요ㅎㅎ",
          "original_form": "아 그러고 보니 아이들 보는 뮤지컬은 몇 번 봤었네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 09:55"
        },
        {
          "id": "MDRW2100046042.1.23",
          "form": "아아 세상에 ㅜㅜ",
          "original_form": "아아 세상에 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046042.1.24",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046042.1.25",
          "form": "세상에 라니 ㅎㅎ 너무 귀엽네요 ㅎㅎ",
          "original_form": "세상에 라니 ㅎㅎ 너무 귀엽네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046042.1.26",
          "form": "ㅋㅋㅋㅋㅋ 결혼 전에는 어떤 공연 좋아하셨어요?",
          "original_form": "ㅋㅋㅋㅋㅋ 결혼 전에는 어떤 공연 좋아하셨어요?",
          "speaker_id": "1",
          "time": "20210908 09:58"
        },
        {
          "id": "MDRW2100046042.1.27",
          "form": "뮤지컬도 보러다녔는데",
          "original_form": "뮤지컬도 보러다녔는데",
          "speaker_id": "2",
          "time": "20210908 09:59"
        },
        {
          "id": "MDRW2100046042.1.28",
          "form": "많이는 못봤구요",
          "original_form": "많이는 못봤구요",
          "speaker_id": "2",
          "time": "20210908 09:59"
        },
        {
          "id": "MDRW2100046042.1.29",
          "form": "콘서트는 못가봤어요",
          "original_form": "콘서트는 못가봤어요",
          "speaker_id": "2",
          "time": "20210908 09:59"
        },
        {
          "id": "MDRW2100046042.1.30",
          "form": "어떤게 기억에 남으시나요??",
          "original_form": "어떤게 기억에 남으시나요??",
          "speaker_id": "2",
          "time": "20210908 09:59"
        },
        {
          "id": "MDRW2100046042.1.31",
          "form": "저는 팬은 아닌데 친한 언니랑 연말에 시아준수 콘서트를 다녀왔는뎈ㅋㅋㅋㅋ 바로 다음날 열애설이 나더라두뇨",
          "original_form": "저는 팬은 아닌데 친한 언니랑 연말에 시아준수 콘서트를 다녀왔는뎈ㅋㅋㅋㅋ 바로 다음날 열애설이 나더라두뇨",
          "speaker_id": "1",
          "time": "20210908 10:00"
        },
        {
          "id": "MDRW2100046042.1.32",
          "form": "그래서 팬들은 어떤마음일까 생각 많이 했던것 같아요 ㅋㅋ",
          "original_form": "그래서 팬들은 어떤마음일까 생각 많이 했던것 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:01"
        },
        {
          "id": "MDRW2100046042.1.33",
          "form": "아하 ㅎㅎㅎ 시아준수 콘서트",
          "original_form": "아하 ㅎㅎㅎ 시아준수 콘서트",
          "speaker_id": "2",
          "time": "20210908 10:02"
        },
        {
          "id": "MDRW2100046042.1.34",
          "form": "다음날 하니?? 랑 열애설이 났나봐요",
          "original_form": "다음날 하니?? 랑 열애설이 났나봐요",
          "speaker_id": "2",
          "time": "20210908 10:02"
        },
        {
          "id": "MDRW2100046042.1.35",
          "form": "콘서트 재밌을거같아요",
          "original_form": "콘서트 재밌을거같아요",
