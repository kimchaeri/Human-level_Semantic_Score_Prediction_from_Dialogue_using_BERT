      "utterance": [
        {
          "id": "MDRW2100032313.1.1",
          "form": "책 읽는 것 좋아하시나요?",
          "original_form": "책 읽는 것 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210907 19:25"
        },
        {
          "id": "MDRW2100032313.1.2",
          "form": "아 ㅋㅋㅋ제가 가장 할말이없는 분야군요ㅠㅠ",
          "original_form": "아 ㅋㅋㅋ제가 가장 할말이없는 분야군요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100032313.1.3",
          "form": "어릴때는 책읽는거 좋아했는데",
          "original_form": "어릴때는 책읽는거 좋아했는데",
          "speaker_id": "1",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100032313.1.4",
          "form": "지금은 거의안봐요ㅠ",
          "original_form": "지금은 거의안봐요ㅠ",
          "speaker_id": "1",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100032313.1.5",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100032313.1.6",
          "form": "저는 요즘 책 좀 보려고",
          "original_form": "저는 요즘 책 좀 보려고",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100032313.1.7",
          "form": "책 소개 오디오클립도 듣고",
          "original_form": "책 소개 오디오클립도 듣고",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100032313.1.8",
          "form": "그러고 있어요",
          "original_form": "그러고 있어요",
          "speaker_id": "2",
          "time": "20210907 20:03"
        },
        {
          "id": "MDRW2100032313.1.9",
          "form": "오 책 소개 오디오클립은 어디서들어요?",
          "original_form": "오 책 소개 오디오클립은 어디서들어요?",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100032313.1.10",
          "form": "오디오클립이라는 어플에서",
          "original_form": "오디오클립이라는 어플에서",
          "speaker_id": "2",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100032313.1.11",
          "form": "책읽아웃이라는 걸 듣고있어요",
          "original_form": "책읽아웃이라는 걸 듣고있어요",
          "speaker_id": "2",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100032313.1.12",
          "form": "ㅋㅋ제목이 너무 귀여워요 ㅋㅋㅋ 책읽아웃ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋ제목이 너무 귀여워요 ㅋㅋㅋ 책읽아웃ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100032313.1.13",
          "form": "네 귀엽죠",
          "original_form": "네 귀엽죠",
          "speaker_id": "2",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100032313.1.14",
          "form": "저희 동네 도서관도 잘돼 있어서 책 읽기 좋아요",
          "original_form": "저희 동네 도서관도 잘돼 있어서 책 읽기 좋아요",
          "speaker_id": "2",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100032313.1.15",
          "form": "오 정말요? 그런거 부러워요ㅠ 도서관이 가까우세요?",
          "original_form": "오 정말요? 그런거 부러워요ㅠ 도서관이 가까우세요?",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100032313.1.16",
          "form": "네 가깝기도 하고",
          "original_form": "네 가깝기도 하고",
          "speaker_id": "2",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100032313.1.17",
          "form": "근처 지하철역에 상호대차 서비스가 돼서",
          "original_form": "근처 지하철역에 상호대차 서비스가 돼서",
          "speaker_id": "2",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100032313.1.18",
          "form": "퇴근길에 빌리고 반납할수 있어요",
          "original_form": "퇴근길에 빌리고 반납할수 있어요",
          "speaker_id": "2",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100032313.1.19",
          "form": "우와 정말 편리하고 좋겠어요 !!",
          "original_form": "우와 정말 편리하고 좋겠어요 !!",
          "speaker_id": "1",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100032313.1.20",
          "form": "네 감사한 서비스죠",
          "original_form": "네 감사한 서비스죠",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100032313.1.21",
          "form": "덕분에 책읽기를 가까이할 수 있고요",
          "original_form": "덕분에 책읽기를 가까이할 수 있고요",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100032313.1.22",
          "form": "저는 책 빌릴 곳도 마땅치 않아서 더 안보게 되는것같아요",
          "original_form": "저는 책 빌릴 곳도 마땅치 않아서 더 안보게 되는것같아요",
          "speaker_id": "1",
          "time": "20210907 21:04"
        }
      ]
