      "utterance": [
        {
          "id": "MDRW2100035978.1.1",
          "form": "식사 하셨어용?",
          "original_form": "식사 하셨어용?",
          "speaker_id": "2",
          "time": "20210830 13:04"
        },
        {
          "id": "MDRW2100035978.1.2",
          "form": "점심은 그냥 도시락먹엇어욤",
          "original_form": "점심은 그냥 도시락먹엇어욤",
          "speaker_id": "1",
          "time": "20210830 13:05"
        },
        {
          "id": "MDRW2100035978.1.3",
          "form": "어떤 도시락 드셨어용?",
          "original_form": "어떤 도시락 드셨어용?",
          "speaker_id": "2",
          "time": "20210830 13:05"
        },
        {
          "id": "MDRW2100035978.1.4",
          "form": "저는 바베큐 치킨 먹었어욥!",
          "original_form": "저는 바베큐 치킨 먹었어욥!",
          "speaker_id": "2",
          "time": "20210830 13:05"
        },
        {
          "id": "MDRW2100035978.1.5",
          "form": "그냥 백반이에요\n맛난거 드셧네요 흑흑",
          "original_form": "그냥 백반이에요\n맛난거 드셧네요 흑흑",
          "speaker_id": "1",
          "time": "20210830 13:06"
        },
        {
          "id": "MDRW2100035978.1.6",
          "form": "앗ㅠ 오늘 맛있는게 먹고싶더라구요",
          "original_form": "앗ㅠ 오늘 맛있는게 먹고싶더라구요",
          "speaker_id": "2",
          "time": "20210830 13:53"
        },
        {
          "id": "MDRW2100035978.1.7",
          "form": "후식 드셨나요?",
          "original_form": "후식 드셨나요?",
          "speaker_id": "2",
          "time": "20210830 13:53"
        },
        {
          "id": "MDRW2100035978.1.8",
          "form": "너무잠와서",
          "original_form": "너무잠와서",
          "speaker_id": "1",
          "time": "20210830 13:54"
        },
        {
          "id": "MDRW2100035978.1.9",
          "form": "아메리카노마실려구요",
          "original_form": "아메리카노마실려구요",
          "speaker_id": "1",
          "time": "20210830 13:54"
        },
        {
          "id": "MDRW2100035978.1.10",
          "form": "집에가고싶어요ㅋㅋㅋ",
          "original_form": "집에가고싶어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:54"
        },
        {
          "id": "MDRW2100035978.1.11",
          "form": "앗ㅋㅋㅋ 밥먹고 집 보내줬으면..ㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋ 밥먹고 집 보내줬으면..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:57"
        },
        {
          "id": "MDRW2100035978.1.12",
          "form": "왜 밥먹으면 졸릴까요",
          "original_form": "왜 밥먹으면 졸릴까요",
          "speaker_id": "2",
          "time": "20210830 13:57"
        },
        {
          "id": "MDRW2100035978.1.13",
          "form": "흑흑 ㅠㅠ",
          "original_form": "흑흑 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 13:57"
        },
        {
          "id": "MDRW2100035978.1.14",
          "form": "커피마시구 잠깨야것어요",
          "original_form": "커피마시구 잠깨야것어요",
          "speaker_id": "1",
          "time": "20210830 13:57"
        },
        {
          "id": "MDRW2100035978.1.15",
          "form": "ㅜㅜ저도 참다가 한 잔 타고 있어요ㅠ",
          "original_form": "ㅜㅜ저도 참다가 한 잔 타고 있어요ㅠ",
          "speaker_id": "2",
          "time": "20210830 15:01"
        },
        {
          "id": "MDRW2100035978.1.16",
          "form": "ㅋㅋ비가 왓다갓다하네요",
          "original_form": "ㅋㅋ비가 왓다갓다하네요",
          "speaker_id": "1",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100035978.1.17",
          "form": "저는 오늘 일찍퇴근하는날이라서",
          "original_form": "저는 오늘 일찍퇴근하는날이라서",
          "speaker_id": "1",
          "time": "20210901 16:47"
        },
        {
          "id": "MDRW2100035978.1.18",
          "form": "밥먹고 한숨자고일어낫어요",
          "original_form": "밥먹고 한숨자고일어낫어요",
          "speaker_id": "1",
          "time": "20210901 16:48"
        },
        {
          "id": "MDRW2100035978.1.19",
          "form": "오!! 굿이네욤",
          "original_form": "오!! 굿이네욤",
          "speaker_id": "2",
          "time": "20210901 18:48"
        },
        {
          "id": "MDRW2100035978.1.20",
          "form": "오늘같은날은 쉬어줘야된다구요",
          "original_form": "오늘같은날은 쉬어줘야된다구요",
          "speaker_id": "2",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100035978.1.21",
          "form": "저녁드셧나용",
          "original_form": "저녁드셧나용",
          "speaker_id": "1",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100035978.1.22",
          "form": "넹! 방금 먹었어요 드셨어요?",
          "original_form": "넹! 방금 먹었어요 드셨어요?",
          "speaker_id": "2",
          "time": "20210901 18:54"
        },
        {
          "id": "MDRW2100035978.1.23",
          "form": "오 저는 점심 늦게 먹어서 저녁은 패쓰할려구용",
          "original_form": "오 저는 점심 늦게 먹어서 저녁은 패쓰할려구용",
          "speaker_id": "1",
          "time": "20210901 18:55"
        }
      ]
