      "utterance": [
        {
          "id": "MDRW2100025248.1.1",
          "form": "가리는거는 없는데",
          "original_form": "가리는거는 없는데",
          "speaker_id": "2",
          "time": "20210901 02:45"
        },
        {
          "id": "MDRW2100025248.1.2",
          "form": "잘 기억이 안나네요 ㅋㅋ",
          "original_form": "잘 기억이 안나네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 02:45"
        },
        {
          "id": "MDRW2100025248.1.3",
          "form": "아ㅎㅎ 전 가을 되면 전어 꼭 찾아먹거든요ㅎ",
          "original_form": "아ㅎㅎ 전 가을 되면 전어 꼭 찾아먹거든요ㅎ",
          "speaker_id": "1",
          "time": "20210901 02:47"
        },
        {
          "id": "MDRW2100025248.1.4",
          "form": "근데 호불호가 많이 갈리는 회라ㅜ",
          "original_form": "근데 호불호가 많이 갈리는 회라ㅜ",
          "speaker_id": "1",
          "time": "20210901 02:47"
        },
        {
          "id": "MDRW2100025248.1.5",
          "form": "가시 때문인가요",
          "original_form": "가시 때문인가요",
          "speaker_id": "2",
          "time": "20210901 02:48"
        },
        {
          "id": "MDRW2100025248.1.6",
          "form": "이번 가을에는 도전해보죠",
          "original_form": "이번 가을에는 도전해보죠",
          "speaker_id": "2",
          "time": "20210901 02:48"
        },
        {
          "id": "MDRW2100025248.1.7",
          "form": "그...흰살생선보다 기름기가 있는 생선이라서요ㅎ",
          "original_form": "그...흰살생선보다 기름기가 있는 생선이라서요ㅎ",
          "speaker_id": "1",
          "time": "20210901 02:48"
        },
        {
          "id": "MDRW2100025248.1.8",
          "form": "제입엔 고소하쥬",
          "original_form": "제입엔 고소하쥬",
          "speaker_id": "1",
          "time": "20210901 02:48"
        },
        {
          "id": "MDRW2100025248.1.9",
          "form": "연어랑은 또 다른 느낌인가요",
          "original_form": "연어랑은 또 다른 느낌인가요",
          "speaker_id": "2",
          "time": "20210901 02:49"
        },
        {
          "id": "MDRW2100025248.1.10",
          "form": "물론 세꼬시 특유의 식감문제도 있구요",
          "original_form": "물론 세꼬시 특유의 식감문제도 있구요",
          "speaker_id": "1",
          "time": "20210901 02:49"
        },
        {
          "id": "MDRW2100025248.1.11",
          "form": "전혀 다른 맛이에요ㅎ",
          "original_form": "전혀 다른 맛이에요ㅎ",
          "speaker_id": "1",
          "time": "20210901 02:49"
        },
        {
          "id": "MDRW2100025248.1.12",
          "form": "아 대방어 쪽인가요",
          "original_form": "아 대방어 쪽인가요",
          "speaker_id": "2",
          "time": "20210901 02:49"
        },
        {
          "id": "MDRW2100025248.1.13",
          "form": "굳이 따지면요",
          "original_form": "굳이 따지면요",
          "speaker_id": "1",
          "time": "20210901 02:50"
        },
        {
          "id": "MDRW2100025248.1.14",
          "form": "혹시 병어회는 드셔보셨어요?",
          "original_form": "혹시 병어회는 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210901 02:50"
        },
        {
          "id": "MDRW2100025248.1.15",
          "form": "약간 그쪽 계열인데",
          "original_form": "약간 그쪽 계열인데",
          "speaker_id": "1",
          "time": "20210901 02:50"
        },
        {
          "id": "MDRW2100025248.1.16",
          "form": "병어도 기억이 잘 ㅋㅋ 먹는거만 먹어서",
          "original_form": "병어도 기억이 잘 ㅋㅋ 먹는거만 먹어서",
          "speaker_id": "2",
          "time": "20210901 02:51"
        },
        {
          "id": "MDRW2100025248.1.17",
          "form": "다음에 먹어보죠 꼭!!",
          "original_form": "다음에 먹어보죠 꼭!!",
          "speaker_id": "2",
          "time": "20210901 02:51"
        },
        {
          "id": "MDRW2100025248.1.18",
          "form": "네네 지금 물올라서 딱이에요",
          "original_form": "네네 지금 물올라서 딱이에요",
          "speaker_id": "1",
          "time": "20210901 02:52"
        },
        {
          "id": "MDRW2100025248.1.19",
          "form": "늦가을되면 뼈 억세서 못먹게 되니ㅜ",
          "original_form": "늦가을되면 뼈 억세서 못먹게 되니ㅜ",
          "speaker_id": "1",
          "time": "20210901 02:52"
        },
        {
          "id": "MDRW2100025248.1.20",
          "form": "넵ㅋㅋ 다음번 회는 꼭 전어로",
          "original_form": "넵ㅋㅋ 다음번 회는 꼭 전어로",
          "speaker_id": "2",
          "time": "20210901 02:54"
        },
        {
          "id": "MDRW2100025248.1.21",
          "form": "넵 먹구 감상 들려주세욪ㅎ",
          "original_form": "넵 먹구 감상 들려주세욪ㅎ",
          "speaker_id": "1",
          "time": "20210901 03:00"
        },
        {
          "id": "MDRW2100025248.1.22",
          "form": "네ㅋㅋ 그러지요",
          "original_form": "네ㅋㅋ 그러지요",
          "speaker_id": "2",
