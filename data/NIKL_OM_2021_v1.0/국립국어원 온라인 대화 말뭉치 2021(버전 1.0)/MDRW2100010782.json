      "utterance": [
        {
          "id": "MDRW2100010782.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 21:44"
        },
        {
          "id": "MDRW2100010782.1.2",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210823 21:44"
        },
        {
          "id": "MDRW2100010782.1.3",
          "form": "해외여행도요",
          "original_form": "해외여행도요",
          "speaker_id": "2",
          "time": "20210823 21:44"
        },
        {
          "id": "MDRW2100010782.1.4",
          "form": "해외여행 어디 가보셨어요?",
          "original_form": "해외여행 어디 가보셨어요?",
          "speaker_id": "1",
          "time": "20210823 21:45"
        },
        {
          "id": "MDRW2100010782.1.5",
          "form": "여행지 대기 해볼래요?",
          "original_form": "여행지 대기 해볼래요?",
          "speaker_id": "2",
          "time": "20210823 21:45"
        },
        {
          "id": "MDRW2100010782.1.6",
          "form": "중국이요",
          "original_form": "중국이요",
          "speaker_id": "2",
          "time": "20210823 21:45"
        },
        {
          "id": "MDRW2100010782.1.7",
          "form": "중국 어디 다녀오셨나요",
          "original_form": "중국 어디 다녀오셨나요",
          "speaker_id": "1",
          "time": "20210823 21:46"
        },
        {
          "id": "MDRW2100010782.1.8",
          "form": "광저우요",
          "original_form": "광저우요",
          "speaker_id": "2",
          "time": "20210823 21:46"
        },
        {
          "id": "MDRW2100010782.1.9",
          "form": "간곳 말해봐요 하나씩 ㅎㅎ",
          "original_form": "간곳 말해봐요 하나씩 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 21:46"
        },
        {
          "id": "MDRW2100010782.1.10",
          "form": "저는 일본 오사카요",
          "original_form": "저는 일본 오사카요",
          "speaker_id": "1",
          "time": "20210823 21:46"
        },
        {
          "id": "MDRW2100010782.1.11",
          "form": "저는 스위스요",
          "original_form": "저는 스위스요",
          "speaker_id": "2",
          "time": "20210823 21:47"
        },
        {
          "id": "MDRW2100010782.1.12",
          "form": "일본 도쿄도 갔었어요",
          "original_form": "일본 도쿄도 갔었어요",
          "speaker_id": "1",
          "time": "20210823 21:47"
        },
        {
          "id": "MDRW2100010782.1.13",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210823 21:48"
        },
        {
          "id": "MDRW2100010782.1.14",
          "form": "2010년이요",
          "original_form": "2010년이요",
          "speaker_id": "2",
          "time": "20210823 21:48"
        },
        {
          "id": "MDRW2100010782.1.15",
          "form": "도쿄 어떠셨어요?",
          "original_form": "도쿄 어떠셨어요?",
          "speaker_id": "1",
          "time": "20210823 21:48"
        },
        {
          "id": "MDRW2100010782.1.16",
          "form": "좋았어요",
          "original_form": "좋았어요",
          "speaker_id": "2",
          "time": "20210823 21:48"
        },
        {
          "id": "MDRW2100010782.1.17",
          "form": "한국같아요",
          "original_form": "한국같아요",
          "speaker_id": "2",
          "time": "20210823 21:48"
        },
        {
          "id": "MDRW2100010782.1.18",
          "form": "이탈리아도갔어요",
          "original_form": "이탈리아도갔어요",
          "speaker_id": "2",
          "time": "20210823 21:48"
        },
        {
          "id": "MDRW2100010782.1.19",
          "form": "또 어디 가셨어요?",
          "original_form": "또 어디 가셨어요?",
          "speaker_id": "1",
          "time": "20210823 21:48"
        },
        {
          "id": "MDRW2100010782.1.20",
          "form": "이탈리아 로마요?",
          "original_form": "이탈리아 로마요?",
          "speaker_id": "1",
          "time": "20210823 21:48"
        },
        {
          "id": "MDRW2100010782.1.21",
          "form": "쭉 돌아다넜어요",
          "original_form": "쭉 돌아다넜어요",
          "speaker_id": "2",
          "time": "20210823 21:49"
        },
        {
          "id": "MDRW2100010782.1.22",
          "form": "쪽지 주고받기로했는데",
          "original_form": "쪽지 주고받기로했는데",
          "speaker_id": "2",
          "time": "20210823 21:49"
        },
        {
          "id": "MDRW2100010782.1.23",
          "form": "까먹었네요 ㅠ",
          "original_form": "까먹었네요 ㅠ",
          "speaker_id": "2",
          "time": "20210823 21:49"
        },
        {
          "id": "MDRW2100010782.1.24",
          "form": "아 쪽지 주고받았으면 재밌었겠네요",
          "original_form": "아 쪽지 주고받았으면 재밌었겠네요",
          "speaker_id": "1",
          "time": "20210823 21:50"
        }
      ]
