      "utterance": [
        {
          "id": "MDRW2100022517.1.1",
          "form": "아 저는 쓰레기통에 씌워놔요",
          "original_form": "아 저는 쓰레기통에 씌워놔요",
          "speaker_id": "2",
          "time": "20210829 15:18"
        },
        {
          "id": "MDRW2100022517.1.2",
          "form": "아 ㅋㅋ 그거 괜찮네요",
          "original_form": "아 ㅋㅋ 그거 괜찮네요",
          "speaker_id": "1",
          "time": "20210829 15:18"
        },
        {
          "id": "MDRW2100022517.1.3",
          "form": "저는 일부러 종량제 사이즈 맞는 쓰레기통 샀어요 ㅜ",
          "original_form": "저는 일부러 종량제 사이즈 맞는 쓰레기통 샀어요 ㅜ",
          "speaker_id": "1",
          "time": "20210829 15:19"
        },
        {
          "id": "MDRW2100022517.1.4",
          "form": "아 그렇군요 ㅋㅋㅋㅋㅋ",
          "original_form": "아 그렇군요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:19"
        },
        {
          "id": "MDRW2100022517.1.5",
          "form": "그렇게 하니까 바로 버리기 편리하긴 해요",
          "original_form": "그렇게 하니까 바로 버리기 편리하긴 해요",
          "speaker_id": "1",
          "time": "20210829 15:19"
        },
        {
          "id": "MDRW2100022517.1.6",
          "form": "쓰레기통어디서삭어요?",
          "original_form": "쓰레기통어디서삭어요?",
          "speaker_id": "2",
          "time": "20210829 15:19"
        },
        {
          "id": "MDRW2100022517.1.7",
          "form": "쓰레기통은 주워왔어요",
          "original_form": "쓰레기통은 주워왔어요",
          "speaker_id": "1",
          "time": "20210829 15:19"
        },
        {
          "id": "MDRW2100022517.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ누가 버렸더라구요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ누가 버렸더라구요",
          "speaker_id": "1",
          "time": "20210829 15:19"
        },
        {
          "id": "MDRW2100022517.1.9",
          "form": "ㅋㅋㅋㅋㅋ아 중고잉가요",
          "original_form": "ㅋㅋㅋㅋㅋ아 중고잉가요",
          "speaker_id": "2",
          "time": "20210829 15:19"
        },
        {
          "id": "MDRW2100022517.1.10",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "1",
          "time": "20210829 15:19"
        },
        {
          "id": "MDRW2100022517.1.11",
          "form": "나름 중고 구매네요",
          "original_form": "나름 중고 구매네요",
          "speaker_id": "2",
          "time": "20210829 15:19"
        },
        {
          "id": "MDRW2100022517.1.12",
          "form": "중고거래로 샀어요 헤헤",
          "original_form": "중고거래로 샀어요 헤헤",
          "speaker_id": "1",
          "time": "20210829 15:19"
        },
        {
          "id": "MDRW2100022517.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:19"
        },
        {
          "id": "MDRW2100022517.1.14",
          "form": "왜 버리냐고 했더니 너무 작대요",
          "original_form": "왜 버리냐고 했더니 너무 작대요",
          "speaker_id": "1",
          "time": "20210829 15:20"
        },
        {
          "id": "MDRW2100022517.1.15",
          "form": "근데 전 1인가구라 딱맞거든요",
          "original_form": "근데 전 1인가구라 딱맞거든요",
          "speaker_id": "1",
          "time": "20210829 15:20"
        },
        {
          "id": "MDRW2100022517.1.16",
          "form": "아아 그렇구나 ㅋㅋㅋㅋㅋ중고거래 잘되셨네요",
          "original_form": "아아 그렇구나 ㅋㅋㅋㅋㅋ중고거래 잘되셨네요",
          "speaker_id": "2",
          "time": "20210829 15:20"
        },
        {
          "id": "MDRW2100022517.1.17",
          "form": "중고거래 자주하세요?",
          "original_form": "중고거래 자주하세요?",
          "speaker_id": "2",
          "time": "20210829 15:20"
        },
        {
          "id": "MDRW2100022517.1.18",
          "form": "그러니까요 ㅎㅎ 중고거래 자주하진 않아요 ㅜㅜ",
          "original_form": "그러니까요 ㅎㅎ 중고거래 자주하진 않아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 15:20"
        },
        {
          "id": "MDRW2100022517.1.19",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210829 15:20"
        },
        {
          "id": "MDRW2100022517.1.20",
          "form": "아 그렇군요 저두 잘 안해요 ㅋㅋㅋㅋ",
          "original_form": "아 그렇군요 저두 잘 안해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:20"
        },
        {
          "id": "MDRW2100022517.1.21",
          "form": "뭔가 귀찮기두해서요",
          "original_form": "뭔가 귀찮기두해서요",
          "speaker_id": "2",
          "time": "20210829 15:20"
        },
        {
          "id": "MDRW2100022517.1.22",
          "form": "선물 같은거 포장할 때도 다이소 좋은 것 같아요",
          "original_form": "선물 같은거 포장할 때도 다이소 좋은 것 같아요",
          "speaker_id": "1",
