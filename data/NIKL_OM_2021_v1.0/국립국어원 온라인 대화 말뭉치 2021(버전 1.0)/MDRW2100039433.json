      "utterance": [
        {
          "id": "MDRW2100039433.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100039433.1.2",
          "form": "안녕하세요~!",
          "original_form": "안녕하세요~!",
          "speaker_id": "2",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100039433.1.3",
          "form": "대학교생활 즐거우신가용?",
          "original_form": "대학교생활 즐거우신가용?",
          "speaker_id": "1",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100039433.1.4",
          "form": "아니용.. 최악이애,,,,",
          "original_form": "아니용.. 최악이애,,,,",
          "speaker_id": "2",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100039433.1.5",
          "form": "앗ㅜㅜ또르르",
          "original_form": "앗ㅜㅜ또르르",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100039433.1.6",
          "form": "무슨 일 있으세요?ㅜㅜ",
          "original_form": "무슨 일 있으세요?ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100039433.1.7",
          "form": "교수님의 티엠아이쇼만 듣고있으려니 정신적 스트레스가 크네요...",
          "original_form": "교수님의 티엠아이쇼만 듣고있으려니 정신적 스트레스가 크네요...",
          "speaker_id": "2",
          "time": "20210903 17:39"
        },
        {
          "id": "MDRW2100039433.1.8",
          "form": "name1님?은 대학생이신가요??",
          "original_form": "&name1&님?은 대학생이신가요??",
          "speaker_id": "2",
          "time": "20210903 17:39"
        },
        {
          "id": "MDRW2100039433.1.9",
          "form": "전 졸업했어요",
          "original_form": "전 졸업했어요",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039433.1.10",
          "form": "수강신청 망하셨군요ㅜㅜ",
          "original_form": "수강신청 망하셨군요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039433.1.11",
          "form": "티엠아이만 남발하는 교수님이라니",
          "original_form": "티엠아이만 남발하는 교수님이라니",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039433.1.12",
          "form": "어떤분야의 TMI를 늘어놓으시는거에요?",
          "original_form": "어떤분야의 TMI를 늘어놓으시는거에요?",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100039433.1.13",
          "form": "그냥 자기 학부생일적 얘기와... 다른 교수님의 교수님일적 이야기요ㅜㅜ",
          "original_form": "그냥 자기 학부생일적 얘기와... 다른 교수님의 교수님일적 이야기요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100039433.1.14",
          "form": "수강신청은 성공했는데",
          "original_form": "수강신청은 성공했는데",
          "speaker_id": "2",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100039433.1.15",
          "form": "교양 사기당했어요ㅋㅋㅋㅋ",
          "original_form": "교양 사기당했어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:45"
        },
        {
          "id": "MDRW2100039433.1.16",
          "form": "애..애도를 표합니다",
          "original_form": "애..애도를 표합니다",
          "speaker_id": "1",
          "time": "20210903 17:46"
        },
        {
          "id": "MDRW2100039433.1.17",
          "form": "첨에만 그러고 정상궤도로 복귀하셔야할텐데",
          "original_form": "첨에만 그러고 정상궤도로 복귀하셔야할텐데",
          "speaker_id": "1",
          "time": "20210903 17:46"
        },
        {
          "id": "MDRW2100039433.1.18",
          "form": "감사합니다ㅜㅜ... 맞아요.. 근데 저번학기도 그러셨어서ㅋㅋ 이제 거의 포기했어용",
          "original_form": "감사합니다ㅜㅜ... 맞아요.. 근데 저번학기도 그러셨어서ㅋㅋ 이제 거의 포기했어용",
          "speaker_id": "2",
          "time": "20210903 17:49"
        },
        {
          "id": "MDRW2100039433.1.19",
          "form": "허얼 학점이라도 후하게 남발하는 스타일이시길ㅜㅜ",
          "original_form": "허얼 학점이라도 후하게 남발하는 스타일이시길ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 17:50"
        },
        {
          "id": "MDRW2100039433.1.20",
          "form": "으악 학점ㅋㅋㅋㅋㅋ 제가 저번학기 알바하느라 망했었어요,,,,",
          "original_form": "으악 학점ㅋㅋㅋㅋㅋ 제가 저번학기 알바하느라 망했었어요,,,,",
          "speaker_id": "2",
          "time": "20210903 17:50"
        },
        {
          "id": "MDRW2100039433.1.21",
          "form": "일단 열심히하면 학점 주시는 스타일인거 같아요",
          "original_form": "일단 열심히하면 학점 주시는 스타일인거 같아요",
          "speaker_id": "2",
          "time": "20210903 17:51"
        },
        {
          "id": "MDRW2100039433.1.22",
          "form": "저는 시간이 모자라서....",
          "original_form": "저는 시간이 모자라서....",
          "speaker_id": "2",
          "time": "20210903 17:51"
        },
        {
          "id": "MDRW2100039433.1.23",
          "form": "알바랑 학업병행 쉽지않죠 고생많으셨겠어요",
          "original_form": "알바랑 학업병행 쉽지않죠 고생많으셨겠어요",
          "speaker_id": "1",
          "time": "20210903 17:51"
        },
        {
          "id": "MDRW2100039433.1.24",
          "form": "지옥이었어요ㅜㅠ 감사합니다",
          "original_form": "지옥이었어요ㅜㅠ 감사합니다",
          "speaker_id": "2",
          "time": "20210903 17:51"
        },
        {
          "id": "MDRW2100039433.1.25",
          "form": "위로받는 기분이에요",
          "original_form": "위로받는 기분이에요",
          "speaker_id": "2",
          "time": "20210903 17:51"
        }
      ]
    }
