      "utterance": [
        {
          "id": "MDRW2100016737.1.1",
          "form": "배달음식 중에 뭐 좋아하시나요?",
          "original_form": "배달음식 중에 뭐 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 15:24"
        },
        {
          "id": "MDRW2100016737.1.2",
          "form": "치킨이요!",
          "original_form": "치킨이요!",
          "speaker_id": "1",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100016737.1.3",
          "form": "오~ 저도요",
          "original_form": "오~ 저도요",
          "speaker_id": "2",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100016737.1.4",
          "form": "어디 치킨좋아하시나요",
          "original_form": "어디 치킨좋아하시나요",
          "speaker_id": "2",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100016737.1.5",
          "form": "전 교촌이요",
          "original_form": "전 교촌이요",
          "speaker_id": "2",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100016737.1.6",
          "form": "저는 뿌링클 좋아해요ㅎㅎ",
          "original_form": "저는 뿌링클 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:26"
        },
        {
          "id": "MDRW2100016737.1.7",
          "form": "아 뿌링클 맛있죠",
          "original_form": "아 뿌링클 맛있죠",
          "speaker_id": "2",
          "time": "20210826 15:44"
        },
        {
          "id": "MDRW2100016737.1.8",
          "form": "생각하니까 치킨 먹고싶네요",
          "original_form": "생각하니까 치킨 먹고싶네요",
          "speaker_id": "2",
          "time": "20210826 15:44"
        },
        {
          "id": "MDRW2100016737.1.9",
          "form": "저녁은 뭐드실지 생각하셨나요?",
          "original_form": "저녁은 뭐드실지 생각하셨나요?",
          "speaker_id": "2",
          "time": "20210826 15:44"
        },
        {
          "id": "MDRW2100016737.1.10",
          "form": "비빔면 먹으려고용",
          "original_form": "비빔면 먹으려고용",
          "speaker_id": "1",
          "time": "20210826 15:47"
        },
        {
          "id": "MDRW2100016737.1.11",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210826 15:47"
        },
        {
          "id": "MDRW2100016737.1.12",
          "form": "전 카레요ㅎㅎ",
          "original_form": "전 카레요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:47"
        },
        {
          "id": "MDRW2100016737.1.13",
          "form": "직접 해드시나요?",
          "original_form": "직접 해드시나요?",
          "speaker_id": "1",
          "time": "20210826 15:48"
        },
        {
          "id": "MDRW2100016737.1.14",
          "form": "엄마가 보통 해주세요, 요리는 아직 잘 못해서요",
          "original_form": "엄마가 보통 해주세요, 요리는 아직 잘 못해서요",
          "speaker_id": "2",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100016737.1.15",
          "form": "엄마찬스는 진리죠",
          "original_form": "엄마찬스는 진리죠",
          "speaker_id": "1",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016737.1.16",
          "form": "엄마가 해주시면 더 맛있고ㅎㅎ",
          "original_form": "엄마가 해주시면 더 맛있고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016737.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 15:52"
        }
      ]
