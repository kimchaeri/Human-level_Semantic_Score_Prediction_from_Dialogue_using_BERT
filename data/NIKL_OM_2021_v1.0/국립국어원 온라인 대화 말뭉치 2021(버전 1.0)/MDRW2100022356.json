          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100022356.1.3",
          "form": "먹으러가자",
          "original_form": "먹으러가자",
          "speaker_id": "2",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100022356.1.4",
          "form": "ㅋㅋㅋㄱㄱ 어디꺼??",
          "original_form": "ㅋㅋㅋㄱㄱ 어디꺼??",
          "speaker_id": "1",
          "time": "20210829 13:05"
        },
        {
          "id": "MDRW2100022356.1.5",
          "form": "떡볶이 맛있는데 있어요??",
          "original_form": "떡볶이 맛있는데 있어요??",
          "speaker_id": "1",
          "time": "20210829 13:05"
        },
        {
          "id": "MDRW2100022356.1.6",
          "form": "없다ㅋ 그냥 어디든 나가서ㅋㅋ",
          "original_form": "없다ㅋ 그냥 어디든 나가서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:54"
        },
        {
          "id": "MDRW2100022356.1.7",
          "form": "우리도 나가서커피좀먹자",
          "original_form": "우리도 나가서커피좀먹자",
          "speaker_id": "2",
          "time": "20210829 15:54"
        },
        {
          "id": "MDRW2100022356.1.8",
          "form": "ㅋㅋㅋㅋㅋㄱㄱ 난 맨날 나가서 커피마시는데",
          "original_form": "ㅋㅋㅋㅋㅋㄱㄱ 난 맨날 나가서 커피마시는데",
          "speaker_id": "1",
          "time": "20210829 16:26"
        },
        {
          "id": "MDRW2100022356.1.9",
          "form": "흥",
          "original_form": "흥",
          "speaker_id": "2",
          "time": "20210829 16:35"
        },
        {
          "id": "MDRW2100022356.1.10",
          "form": "나도 맨날나가서 먹자",
          "original_form": "나도 맨날나가서 먹자",
          "speaker_id": "2",
          "time": "20210829 16:35"
        },
        {
          "id": "MDRW2100022356.1.11",
          "form": "나도 맨날 먹자는 무슨말인데 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "나도 맨날 먹자는 무슨말인데 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:35"
        },
        {
          "id": "MDRW2100022356.1.12",
          "form": "먹고싶다고ㅋㅋ",
          "original_form": "먹고싶다고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022356.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱㅋ",
          "speaker_id": "1",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022356.1.14",
          "form": "address으로 오슈",
          "original_form": "&address&으로 오슈",
          "speaker_id": "1",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022356.1.15",
          "form": "맛있는 커피 사주끼요 ㅋㅋㅋㅋ",
          "original_form": "맛있는 커피 사주끼요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 17:13"
        },
        {
          "id": "MDRW2100022356.1.16",
          "form": "헐ㅋㅋ address 가다가 지침ㅋㅋ",
          "original_form": "헐ㅋㅋ &address& 가다가 지침ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:16"
        },
        {
          "id": "MDRW2100022356.1.17",
          "form": "ㅋㄹㄹㅋㄱ 지치지말고 오슈",
          "original_form": "ㅋㄹㄹㅋㄱ 지치지말고 오슈",
          "speaker_id": "1",
          "time": "20210829 20:56"
        },
        {
          "id": "MDRW2100022356.1.18",
          "form": "맛있는커피 마시려면 그정도는 해야지",
          "original_form": "맛있는커피 마시려면 그정도는 해야지",
          "speaker_id": "1",
          "time": "20210829 20:56"
        },
        {
          "id": "MDRW2100022356.1.19",
          "form": "맛있나,",
          "original_form": "맛있나,",
          "speaker_id": "2",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100022356.1.20",
          "form": "맛있지ㅎㅎ",
          "original_form": "맛있지ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 07:32"
        },
        {
          "id": "MDRW2100022356.1.21",
          "form": "어제 9시에 뻗었음",
          "original_form": "어제 9시에 뻗었음",
          "speaker_id": "1",
          "time": "20210830 07:32"
        },
        {
          "id": "MDRW2100022356.1.22",
          "form": "잘했네ㅋ",
          "original_form": "잘했네ㅋ",
          "speaker_id": "2",
          "time": "20210830 08:23"
        },
        {
          "id": "MDRW2100022356.1.23",
          "form": "신나게놀고",
          "original_form": "신나게놀고",
          "speaker_id": "2",
          "time": "20210830 08:23"
        },
        {
          "id": "MDRW2100022356.1.24",
          "form": "내가 만든커피가 젤 맛있는거같다 ㅋㅋㅋㅋㅋ",
          "original_form": "내가 만든커피가 젤 맛있는거같다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 08:37"
        },
        {
          "id": "MDRW2100022356.1.25",
          "form": "집에\r\n오소 ㅋㅋㅋㅋㅋ",
          "original_form": "집에\r\n오소 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 08:37"
        },
        {
          "id": "MDRW2100022356.1.26",
          "form": "ㅋㅋ 그래 니가 유명카페네ㅋㅋ",
          "original_form": "ㅋㅋ 그래 니가 유명카페네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 08:49"
        },
        {
          "id": "MDRW2100022356.1.27",
          "form": "그래 언니 name1네 커피집으로 오세요",
          "original_form": "그래 언니 &name1&네 커피집으로 오세요",
          "speaker_id": "1",
          "time": "20210830 08:50"
        },
        {
          "id": "MDRW2100022356.1.28",
          "form": "어제 캠핑 간 내가 의령 이거든",
          "original_form": "어제 캠핑 간 내가 의령 이거든",
          "speaker_id": "1",
          "time": "20210830 08:50"
        },
        {
          "id": "MDRW2100022356.1.29",
          "form": "거기 망개떡이 유명하다고 해서 망개떡 사 먹었는데 완전 맛있더라",
          "original_form": "거기 망개떡이 유명하다고 해서 망개떡 사 먹었는데 완전 맛있더라",
          "speaker_id": "1",
          "time": "20210830 08:50"
        },
        {
          "id": "MDRW2100022356.1.30",
          "form": "의령 가본적없는데 궁금하네ㅋ",
          "original_form": "의령 가본적없는데 궁금하네ㅋ",
          "speaker_id": "2",
          "time": "20210830 10:43"
        },
        {
          "id": "MDRW2100022356.1.31",
          "form": "거기 토끼 동산 조금만 더 가면 되는데",
          "original_form": "거기 토끼 동산 조금만 더 가면 되는데",
          "speaker_id": "1",
          "time": "20210830 10:52"
        },
        {
          "id": "MDRW2100022356.1.32",
          "form": "가까워서 되게 좋더라구요",
          "original_form": "가까워서 되게 좋더라구요",
