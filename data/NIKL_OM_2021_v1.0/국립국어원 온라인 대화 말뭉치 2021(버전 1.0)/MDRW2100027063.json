        },
        {
          "id": "MDRW2100027063.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210902 08:40"
        },
        {
          "id": "MDRW2100027063.1.3",
          "form": "혹시 믿는 종교있으세요?",
          "original_form": "혹시 믿는 종교있으세요?",
          "speaker_id": "2",
          "time": "20210902 08:40"
        },
        {
          "id": "MDRW2100027063.1.4",
          "form": "기독교 모태였는데 지금은 무교에요",
          "original_form": "기독교 모태였는데 지금은 무교에요",
          "speaker_id": "1",
          "time": "20210902 08:41"
        },
        {
          "id": "MDRW2100027063.1.5",
          "form": "아ㅋㅋ 저도 무굔데",
          "original_form": "아ㅋㅋ 저도 무굔데",
          "speaker_id": "2",
          "time": "20210902 08:42"
        },
        {
          "id": "MDRW2100027063.1.6",
          "form": "저희집은 천주교 집안이긴해요",
          "original_form": "저희집은 천주교 집안이긴해요",
          "speaker_id": "2",
          "time": "20210902 08:43"
        },
        {
          "id": "MDRW2100027063.1.7",
          "form": "종교를 믿으라고 안하시던가요?",
          "original_form": "종교를 믿으라고 안하시던가요?",
          "speaker_id": "1",
          "time": "20210902 08:44"
        },
        {
          "id": "MDRW2100027063.1.8",
          "form": "어렸을 땐 성당도 가보고 그랬는데",
          "original_form": "어렸을 땐 성당도 가보고 그랬는데",
          "speaker_id": "2",
          "time": "20210902 08:51"
        },
        {
          "id": "MDRW2100027063.1.9",
          "form": "좀 의미없이 흘러가는 시간같아서 안갔어요",
          "original_form": "좀 의미없이 흘러가는 시간같아서 안갔어요",
          "speaker_id": "2",
          "time": "20210902 08:51"
        },
        {
          "id": "MDRW2100027063.1.10",
          "form": "아직도 큰집가서 제사올릴짼 주기도문외우고 그러긴 하지만;;ㅎㅎ",
          "original_form": "아직도 큰집가서 제사올릴짼 주기도문외우고 그러긴 하지만;;ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 08:52"
        },
        {
          "id": "MDRW2100027063.1.11",
          "form": "천주교는 제사 지내나요?",
          "original_form": "천주교는 제사 지내나요?",
          "speaker_id": "1",
          "time": "20210902 08:53"
        },
        {
          "id": "MDRW2100027063.1.12",
          "form": "기독교는 제사를 안지내서 잘 몰라요",
          "original_form": "기독교는 제사를 안지내서 잘 몰라요",
          "speaker_id": "1",
          "time": "20210902 08:53"
        },
        {
          "id": "MDRW2100027063.1.13",
          "form": "네 그 교황인가 승인해줘서 한국만 제사인정해준다고 하더라구요",
          "original_form": "네 그 교황인가 승인해줘서 한국만 제사인정해준다고 하더라구요",
          "speaker_id": "2",
          "time": "20210902 08:54"
        },
        {
          "id": "MDRW2100027063.1.14",
          "form": "원래는 천주교도 제사 금지하는데 한국만 문화적인 거 인정해줘서 가능한 건가봐요",
          "original_form": "원래는 천주교도 제사 금지하는데 한국만 문화적인 거 인정해줘서 가능한 건가봐요",
          "speaker_id": "2",
          "time": "20210902 08:55"
        },
        {
          "id": "MDRW2100027063.1.15",
          "form": "와 천주교는 그렇군요",
          "original_form": "와 천주교는 그렇군요",
          "speaker_id": "1",
          "time": "20210902 08:57"
        },
        {
          "id": "MDRW2100027063.1.16",
          "form": "처음 알았어요",
          "original_form": "처음 알았어요",
          "speaker_id": "1",
          "time": "20210902 08:57"
        },
        {
          "id": "MDRW2100027063.1.17",
          "form": "그래서 추석같은 때에 병풍에 제사상 다차리고 그앞에서 주기도문외워요",
          "original_form": "그래서 추석같은 때에 병풍에 제사상 다차리고 그앞에서 주기도문외워요",
          "speaker_id": "2",
          "time": "20210902 08:58"
        },
        {
          "id": "MDRW2100027063.1.18",
          "form": "ㅋㅋㅋㅋㅋ저는 어릴때 그게 당연한 건줄 알았는데 다들 놀라더라구요",
          "original_form": "ㅋㅋㅋㅋㅋ저는 어릴때 그게 당연한 건줄 알았는데 다들 놀라더라구요",
          "speaker_id": "2",
          "time": "20210902 08:58"
        },
        {
          "id": "MDRW2100027063.1.19",
          "form": "저도 지금 엄청 놀랐어요",
          "original_form": "저도 지금 엄청 놀랐어요",
          "speaker_id": "1",
          "time": "20210902 08:59"
        },
        {
          "id": "MDRW2100027063.1.20",
          "form": "기독교는 그럼 아예 제사도 안지내고 추석때 만나면 뭐하는지 궁금하네요",
          "original_form": "기독교는 그럼 아예 제사도 안지내고 추석때 만나면 뭐하는지 궁금하네요",
          "speaker_id": "2",
          "time": "20210902 09:02"
        },
        {
          "id": "MDRW2100027063.1.21",
          "form": "추석때 만나서 밥먹기 전에",
          "original_form": "추석때 만나서 밥먹기 전에",
          "speaker_id": "1",
          "time": "20210902 09:04"
        },
        {
          "id": "MDRW2100027063.1.22",
          "form": "따로 예배드려요",
          "original_form": "따로 예배드려요",
          "speaker_id": "1",
          "time": "20210902 09:04"
        },
        {
          "id": "MDRW2100027063.1.23",
          "form": "아하 그렇군요 그럼 다같이 교회로가서 예배드리는건가요??",
          "original_form": "아하 그렇군요 그럼 다같이 교회로가서 예배드리는건가요??",
          "speaker_id": "2",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100027063.1.24",
          "form": "그러는 경우도 있고",
          "original_form": "그러는 경우도 있고",
          "speaker_id": "1",
          "time": "20210902 09:50"
        },
        {
          "id": "MDRW2100027063.1.25",
          "form": "집에서 따로 드리기도 해요",
          "original_form": "집에서 따로 드리기도 해요",
          "speaker_id": "1",
          "time": "20210902 09:50"
        },
        {
          "id": "MDRW2100027063.1.26",
          "form": "아 신기하다 저는 추석때 항상 집안에서만 있었어가지고",
          "original_form": "아 신기하다 저는 추석때 항상 집안에서만 있었어가지고",
          "speaker_id": "2",
          "time": "20210902 09:52"
        },
        {
          "id": "MDRW2100027063.1.27",
          "form": "다같이 어딜간다는게 상상이잘안가요",
          "original_form": "다같이 어딜간다는게 상상이잘안가요",
          "speaker_id": "2",
          "time": "20210902 09:52"
        },
        {
          "id": "MDRW2100027063.1.28",
          "form": "성묘같은 것도 안하시나여?",
          "original_form": "성묘같은 것도 안하시나여?",
          "speaker_id": "2",
          "time": "20210902 09:52"
        },
        {
          "id": "MDRW2100027063.1.29",
          "form": "네 성묘도 안해요",
          "original_form": "네 성묘도 안해요",
          "speaker_id": "1",
          "time": "20210902 09:54"
        }
      ]
