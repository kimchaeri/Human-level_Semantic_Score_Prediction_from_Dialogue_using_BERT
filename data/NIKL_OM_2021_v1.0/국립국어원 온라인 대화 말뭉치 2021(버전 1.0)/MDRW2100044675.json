      "utterance": [
        {
          "id": "MDRW2100044675.1.1",
          "form": "날씨가 선선하네요 ㅠ",
          "original_form": "날씨가 선선하네요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044675.1.2",
          "form": "비가 왔다 해 떳다 비오고 그래서 그런가 습도 때문에 계속 더워요ㅋ\n회사에 선풍김켜고 일했어요ㅋ",
          "original_form": "비가 왔다 해 떳다 비오고 그래서 그런가 습도 때문에 계속 더워요ㅋ\n회사에 선풍김켜고 일했어요ㅋ",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044675.1.3",
          "form": "밤엔 좀 쌀쌀한거같아요\n창문열고 주무세요?",
          "original_form": "밤엔 좀 쌀쌀한거같아요\n창문열고 주무세요?",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044675.1.4",
          "form": "아뇨 ㅠㅠ 밤에는 너무 추워요 ㅋㅋ",
          "original_form": "아뇨 ㅠㅠ 밤에는 너무 추워요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044675.1.5",
          "form": "전기장판 꺼냇어요 ㅠ",
          "original_form": "전기장판 꺼냇어요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044675.1.6",
          "form": "밤엔 좀 쌀쌀하던데 거긴 추우신가봐요\n지역이 어디세요?",
          "original_form": "밤엔 좀 쌀쌀하던데 거긴 추우신가봐요\n지역이 어디세요?",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044675.1.7",
          "form": "서울이용!!!",
          "original_form": "서울이용!!!",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044675.1.8",
          "form": "제가 추위를 잘타서ㅜㅠ 가족들은 잘 자드라구요 ㅋㅋ",
          "original_form": "제가 추위를 잘타서ㅜㅠ 가족들은 잘 자드라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044675.1.9",
          "form": "아~ 저보다 북쪽이네요ㅎ\n전 구미살아요",
          "original_form": "아~ 저보다 북쪽이네요ㅎ\n전 구미살아요",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044675.1.10",
          "form": "그래서 온도차가 있을거같아요",
          "original_form": "그래서 온도차가 있을거같아요",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044675.1.11",
          "form": "아 그러시군요 !!!",
          "original_form": "아 그러시군요 !!!",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044675.1.12",
          "form": "이번 여름에 모기 많으셧나요?",
          "original_form": "이번 여름에 모기 많으셧나요?",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044675.1.13",
          "form": "여긴 단체로 주변 방역을 해줘서 많이 없어요\n다른곳가면 얼굴로도 오더라구요",
          "original_form": "여긴 단체로 주변 방역을 해줘서 많이 없어요\n다른곳가면 얼굴로도 오더라구요",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044675.1.14",
          "form": "저는 엄청 뜯겻어요 ㅜㅜ",
          "original_form": "저는 엄청 뜯겻어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044675.1.15",
          "form": "그래도 이제 선선해지고 모기가 안보이네용",
          "original_form": "그래도 이제 선선해지고 모기가 안보이네용",
          "speaker_id": "1",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044675.1.16",
          "form": "요즘 모기 심하던데 흉 안생겼어요?",
          "original_form": "요즘 모기 심하던데 흉 안생겼어요?",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044675.1.17",
          "form": "저 어제 카페 앞에서 모기봤어요ㅋ\n여긴 아직 있나봐요",
          "original_form": "저 어제 카페 앞에서 모기봤어요ㅋ\n여긴 아직 있나봐요",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044675.1.18",
          "form": "생겼어요 ㅠㅠ 벅벅 긁어서 ㅎㅎ",
          "original_form": "생겼어요 ㅠㅠ 벅벅 긁어서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044675.1.19",
          "form": "안긁을수없죠ㅜㅜ",
          "original_form": "안긁을수없죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044675.1.20",
          "form": "헉스 ㅠㅠ 가을모기도 있나바여",
          "original_form": "헉스 ㅠㅠ 가을모기도 있나바여",
          "speaker_id": "1",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044675.1.21",
          "form": "모기 바퀴 없어져야하는데",
          "original_form": "모기 바퀴 없어져야하는데",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044675.1.22",
          "form": "여긴 아직 있네요\n어여 없어지길",
          "original_form": "여긴 아직 있네요\n어여 없어지길",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044675.1.23",
          "form": "한번 물리면 띵띵 부어서 싫어요ㅜㅜ",
          "original_form": "한번 물리면 띵띵 부어서 싫어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044675.1.24",
          "form": "맞아요 ㅠㅠㅠ 저는 집앞에 산이있어서 ㅋㅋㅋ",
          "original_form": "맞아요 ㅠㅠㅠ 저는 집앞에 산이있어서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044675.1.25",
          "form": "추워져도 모기가..",
          "original_form": "추워져도 모기가..",
          "speaker_id": "1",
          "time": "20210908 00:05"
        }
      ]
    }
  ]
