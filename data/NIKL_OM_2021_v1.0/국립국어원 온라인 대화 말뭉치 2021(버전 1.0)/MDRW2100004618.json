      "utterance": [
        {
          "id": "MDRW2100004618.1.1",
          "form": "아 배구?",
          "original_form": "아 배구?",
          "speaker_id": "2",
          "time": "20210821 17:31"
        },
        {
          "id": "MDRW2100004618.1.2",
          "form": "이번 올림픽 처음으로 챙겨본 것 같아요",
          "original_form": "이번 올림픽 처음으로 챙겨본 것 같아요",
          "speaker_id": "1",
          "time": "20210821 17:31"
        },
        {
          "id": "MDRW2100004618.1.3",
          "form": "보셧는지?",
          "original_form": "보셧는지?",
          "speaker_id": "2",
          "time": "20210821 17:31"
        },
        {
          "id": "MDRW2100004618.1.4",
          "form": "제여자 보샸는지?",
          "original_form": "제여자 보샸는지?",
          "speaker_id": "2",
          "time": "20210821 17:31"
        },
        {
          "id": "MDRW2100004618.1.5",
          "form": "그거 안본사람이랑 겸상x",
          "original_form": "그거 안본사람이랑 겸상x",
          "speaker_id": "1",
          "time": "20210821 17:31"
        },
        {
          "id": "MDRW2100004618.1.6",
          "form": "제여자...그...브라질선수...",
          "original_form": "제여자...그...브라질선수...",
          "speaker_id": "1",
          "time": "20210821 17:32"
        },
        {
          "id": "MDRW2100004618.1.7",
          "form": "그래 페가라이",
          "original_form": "그래 페가라이",
          "speaker_id": "1",
          "time": "20210821 17:32"
        },
        {
          "id": "MDRW2100004618.1.8",
          "form": "체케바라 라고 할뻔했네",
          "original_form": "체케바라 라고 할뻔했네",
          "speaker_id": "1",
          "time": "20210821 17:32"
        },
        {
          "id": "MDRW2100004618.1.9",
          "form": "전 김연경 일반인 여자친구",
          "original_form": "전 김연경 일반인 여자친구",
          "speaker_id": "2",
          "time": "20210821 17:33"
        },
        {
          "id": "MDRW2100004618.1.10",
          "form": "죄송한데 제 와이프 내연녀세요?",
          "original_form": "죄송한데 제 와이프 내연녀세요?",
          "speaker_id": "1",
          "time": "20210821 17:36"
        },
        {
          "id": "MDRW2100004618.1.11",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 17:37"
        },
        {
          "id": "MDRW2100004618.1.12",
          "form": "저는 월수금 님은 화목토",
          "original_form": "저는 월수금 님은 화목토",
          "speaker_id": "1",
          "time": "20210821 17:38"
        },
        {
          "id": "MDRW2100004618.1.13",
          "form": "일욜은 쉬게 해줍시다",
          "original_form": "일욜은 쉬게 해줍시다",
          "speaker_id": "1",
          "time": "20210821 17:38"
        },
        {
          "id": "MDRW2100004618.1.14",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "2",
          "time": "20210821 17:44"
        },
        {
          "id": "MDRW2100004618.1.15",
          "form": "배구선수들은 배구를햐서 키가큰걸까요 키가커서배구선수인걸까요",
          "original_form": "배구선수들은 배구를햐서 키가큰걸까요 키가커서배구선수인걸까요",
          "speaker_id": "2",
          "time": "20210821 17:44"
        },
        {
          "id": "MDRW2100004618.1.16",
          "form": "부러워요",
          "original_form": "부러워요",
          "speaker_id": "2",
          "time": "20210821 17:44"
        },
        {
          "id": "MDRW2100004618.1.17",
          "form": "어릴때 배구를했더라면...!!",
          "original_form": "어릴때 배구를했더라면...!!",
          "speaker_id": "2",
          "time": "20210821 17:44"
        },
        {
          "id": "MDRW2100004618.1.18",
          "form": "김연경선수도 어릴땐 키 작았고 고딩때부터 컸다는데",
          "original_form": "김연경선수도 어릴땐 키 작았고 고딩때부터 컸다는데",
          "speaker_id": "1",
          "time": "20210821 17:51"
        },
        {
          "id": "MDRW2100004618.1.19",
          "form": "배구영향도 있을거같아요",
          "original_form": "배구영향도 있을거같아요",
          "speaker_id": "1",
          "time": "20210821 17:51"
        },
        {
          "id": "MDRW2100004618.1.20",
          "form": "그렇구나....배구...배구....",
          "original_form": "그렇구나....배구...배구....",
          "speaker_id": "2",
          "time": "20210821 17:53"
        },
        {
          "id": "MDRW2100004618.1.21",
          "form": "어릴 때 운동을 하면 좋았을텐데",
          "original_form": "어릴 때 운동을 하면 좋았을텐데",
          "speaker_id": "1",
          "time": "20210821 17:54"
        },
        {
          "id": "MDRW2100004618.1.22",
          "form": "항상 스탠드에 앉아있기만 했네요",
          "original_form": "항상 스탠드에 앉아있기만 했네요",
          "speaker_id": "1",
          "time": "20210821 17:54"
        },
        {
          "id": "MDRW2100004618.1.23",
          "form": "아쉬워라...",
          "original_form": "아쉬워라...",
          "speaker_id": "2",
          "time": "20210821 17:55"
        },
        {
          "id": "MDRW2100004618.1.24",
          "form": "경기 직관가보신적잇어요??",
          "original_form": "경기 직관가보신적잇어요??",
          "speaker_id": "2",
          "time": "20210821 17:55"
        },
        {
          "id": "MDRW2100004618.1.25",
          "form": "어릴 때 모부님 따라 축구경기를 보러간 적은 있어요",
          "original_form": "어릴 때 모부님 따라 축구경기를 보러간 적은 있어요",
          "speaker_id": "1",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004618.1.26",
          "form": "근데 개노잼이라 칰만 먹다 왔어요",
          "original_form": "근데 개노잼이라 칰만 먹다 왔어요",
          "speaker_id": "1",
          "time": "20210821 17:57"
        },
        {
          "id": "MDRW2100004618.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ저는 고등학생때 학교에서 야구보러간적있어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ저는 고등학생때 학교에서 야구보러간적있어요",
          "speaker_id": "2",
          "time": "20210821 17:57"
        },
        {
          "id": "MDRW2100004618.1.28",
          "form": "와 신기하다 학교에서 운동경기도 보러가요?",
          "original_form": "와 신기하다 학교에서 운동경기도 보러가요?",
          "speaker_id": "1",
          "time": "20210821 17:57"
        },
        {
          "id": "MDRW2100004618.1.29",
          "form": "그러니까욬ㅋㅋㅋㅋ근데 야구 룰을 몰라서 그냥 햇빛에 앉아잇는사람되고왔어요...",
          "original_form": "그러니까욬ㅋㅋㅋㅋ근데 야구 룰을 몰라서 그냥 햇빛에 앉아잇는사람되고왔어요...",
          "speaker_id": "2",
          "time": "20210821 17:59"
        },
        {
          "id": "MDRW2100004618.1.30",
          "form": "님도 저랑 비슷하네요...",
          "original_form": "님도 저랑 비슷하네요...",
          "speaker_id": "1",
          "time": "20210821 18:00"
        },
        {
          "id": "MDRW2100004618.1.31",
          "form": "주선자만 신나는 기이한 광경",
          "original_form": "주선자만 신나는 기이한 광경",
          "speaker_id": "1",
          "time": "20210821 18:00"
        },
        {
          "id": "MDRW2100004618.1.32",
          "form": "그 때 여배를 보러갔으면 이런 일이 없었을 거 아냐",
          "original_form": "그 때 여배를 보러갔으면 이런 일이 없었을 거 아냐",
          "speaker_id": "1",
          "time": "20210821 18:00"
        },
        {
          "id": "MDRW2100004618.1.33",
          "form": "사람들이 트렌드를 몰라",
          "original_form": "사람들이 트렌드를 몰라",
          "speaker_id": "1",
          "time": "20210821 18:00"
        },
        {
          "id": "MDRW2100004618.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ하...그니까요....",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ하...그니까요....",
