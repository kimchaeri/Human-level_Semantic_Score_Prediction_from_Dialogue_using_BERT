      "utterance": [
        {
          "id": "MDRW2100020940.1.1",
          "form": "사이드는 가끔요",
          "original_form": "사이드는 가끔요",
          "speaker_id": "2",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100020940.1.2",
          "form": "뭐로 드세요??",
          "original_form": "뭐로 드세요??",
          "speaker_id": "1",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100020940.1.3",
          "form": "주로 튀김류요",
          "original_form": "주로 튀김류요",
          "speaker_id": "2",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100020940.1.4",
          "form": "잘 어울리죠",
          "original_form": "잘 어울리죠",
          "speaker_id": "1",
          "time": "20210828 14:21"
        },
        {
          "id": "MDRW2100020940.1.5",
          "form": "그죠",
          "original_form": "그죠",
          "speaker_id": "2",
          "time": "20210828 14:21"
        },
        {
          "id": "MDRW2100020940.1.6",
          "form": "전 튀김은 신전 오징어가 맛있더라구요",
          "original_form": "전 튀김은 신전 오징어가 맛있더라구요",
          "speaker_id": "1",
          "time": "20210828 14:21"
        },
        {
          "id": "MDRW2100020940.1.7",
          "form": "아 오징어튀김은 진리죠",
          "original_form": "아 오징어튀김은 진리죠",
          "speaker_id": "2",
          "time": "20210828 14:21"
        },
        {
          "id": "MDRW2100020940.1.8",
          "form": "맞아여",
          "original_form": "맞아여",
          "speaker_id": "1",
          "time": "20210828 14:21"
        },
        {
          "id": "MDRW2100020940.1.9",
          "form": "요즘엔 떡볶이가 너무 큰거같아요",
          "original_form": "요즘엔 떡볶이가 너무 큰거같아요",
          "speaker_id": "2",
          "time": "20210828 14:21"
        },
        {
          "id": "MDRW2100020940.1.10",
          "form": "양이 너무 많아요",
          "original_form": "양이 너무 많아요",
          "speaker_id": "1",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100020940.1.11",
          "form": "비싸고 ㅠ",
          "original_form": "비싸고 ㅠ",
          "speaker_id": "1",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100020940.1.12",
          "form": "일인분씩을 안팔아서 사이드를 먹고픈데 못시킬때도 많아요",
          "original_form": "일인분씩을 안팔아서 사이드를 먹고픈데 못시킬때도 많아요",
          "speaker_id": "2",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100020940.1.13",
          "form": "공감해요",
          "original_form": "공감해요",
          "speaker_id": "1",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100020940.1.14",
          "form": "신전은 양이 적어서 좋더라구요",
          "original_form": "신전은 양이 적어서 좋더라구요",
          "speaker_id": "1",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100020940.1.15",
          "form": "와 신전 다음에 먹어봐야겠어요",
          "original_form": "와 신전 다음에 먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210828 14:22"
