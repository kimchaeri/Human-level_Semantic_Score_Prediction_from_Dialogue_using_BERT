        },
        {
          "id": "MDRW2100007554.1.2",
          "form": "어떨 때 다투냐면",
          "original_form": "어떨 때 다투냐면",
          "speaker_id": "1",
          "time": "20210822 03:10"
        },
        {
          "id": "MDRW2100007554.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 03:10"
        },
        {
          "id": "MDRW2100007554.1.4",
          "form": "반말하면 싸우고 집안일 안해서 싸우고",
          "original_form": "반말하면 싸우고 집안일 안해서 싸우고",
          "speaker_id": "1",
          "time": "20210822 03:11"
        },
        {
          "id": "MDRW2100007554.1.5",
          "form": "저 약간 야 이거 못참아요",
          "original_form": "저 약간 야 이거 못참아요",
          "speaker_id": "1",
          "time": "20210822 03:11"
        },
        {
          "id": "MDRW2100007554.1.6",
          "form": "다른건 다 참는데 야 너 니",
          "original_form": "다른건 다 참는데 야 너 니",
          "speaker_id": "1",
          "time": "20210822 03:11"
        },
        {
          "id": "MDRW2100007554.1.7",
          "form": "이거 못참겠더라구요",
          "original_form": "이거 못참겠더라구요",
          "speaker_id": "1",
          "time": "20210822 03:11"
        },
        {
          "id": "MDRW2100007554.1.8",
          "form": "야??? 야를 하나요?!",
          "original_form": "야??? 야를 하나요?!",
          "speaker_id": "2",
          "time": "20210822 03:12"
        },
        {
          "id": "MDRW2100007554.1.9",
          "form": "이녀석",
          "original_form": "이녀석",
          "speaker_id": "2",
          "time": "20210822 03:12"
        },
        {
          "id": "MDRW2100007554.1.10",
          "form": "저 말을 쓴다는건.. 서로 화났을 때 쓴다는건데..",
          "original_form": "저 말을 쓴다는건.. 서로 화났을 때 쓴다는건데..",
          "speaker_id": "1",
          "time": "20210822 03:12"
        },
        {
          "id": "MDRW2100007554.1.11",
          "form": "니?!!!!!",
          "original_form": "니?!!!!!",
          "speaker_id": "2",
          "time": "20210822 03:12"
        },
        {
          "id": "MDRW2100007554.1.12",
          "form": "화난상태에서 저거하면 눈돌더라구요",
          "original_form": "화난상태에서 저거하면 눈돌더라구요",
          "speaker_id": "1",
          "time": "20210822 03:12"
        },
        {
          "id": "MDRW2100007554.1.13",
          "form": "못참을만두",
          "original_form": "못참을만두",
          "speaker_id": "2",
          "time": "20210822 03:12"
        },
        {
          "id": "MDRW2100007554.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 03:12"
        },
        {
          "id": "MDRW2100007554.1.15",
          "form": "니? 뭐라고 했니?",
          "original_form": "니? 뭐라고 했니?",
          "speaker_id": "2",
          "time": "20210822 03:12"
        },
        {
          "id": "MDRW2100007554.1.16",
          "form": "저 말들으면 그 때부터 전쟁",
          "original_form": "저 말들으면 그 때부터 전쟁",
          "speaker_id": "1",
          "time": "20210822 03:12"
        },
        {
          "id": "MDRW2100007554.1.17",
          "form": "이렇게 자동반사될거같아요",
          "original_form": "이렇게 자동반사될거같아요",
          "speaker_id": "2",
          "time": "20210822 03:12"
        },
        {
          "id": "MDRW2100007554.1.18",
          "form": "다시말해봐",
          "original_form": "다시말해봐",
          "speaker_id": "1",
          "time": "20210822 03:12"
        },
        {
          "id": "MDRW2100007554.1.19",
          "form": "니이이???",
          "original_form": "니이이???",
          "speaker_id": "2",
          "time": "20210822 03:12"
        },
        {
          "id": "MDRW2100007554.1.20",
          "form": "이거 자동적",
          "original_form": "이거 자동적",
          "speaker_id": "1",
          "time": "20210822 03:12"
        },
        {
          "id": "MDRW2100007554.1.21",
          "form": "다시말해보라고 시끄럽고 다시말해봐",
          "original_form": "다시말해보라고 시끄럽고 다시말해봐",
          "speaker_id": "1",
          "time": "20210822 03:13"
        },
        {
          "id": "MDRW2100007554.1.22",
          "form": "4살차이라 그래도 싸우는군요",
          "original_form": "4살차이라 그래도 싸우는군요",
          "speaker_id": "2",
          "time": "20210822 03:13"
        },
        {
          "id": "MDRW2100007554.1.23",
          "form": "저는 바로",
          "original_form": "저는 바로",
          "speaker_id": "2",
          "time": "20210822 03:13"
        },
        {
          "id": "MDRW2100007554.1.24",
          "form": "이거 몇번 하면 사과하더라구요",
          "original_form": "이거 몇번 하면 사과하더라구요",
