      "utterance": [
        {
          "id": "MDRW2100036501.1.1",
          "form": "어머 정말?",
          "original_form": "어머 정말?",
          "speaker_id": "2",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100036501.1.2",
          "form": "그래서 새로 사려는데 추천하는거있어?",
          "original_form": "그래서 새로 사려는데 추천하는거있어?",
          "speaker_id": "1",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100036501.1.3",
          "form": "어떤 우산인데?",
          "original_form": "어떤 우산인데?",
          "speaker_id": "2",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100036501.1.4",
          "form": "설마 내가 빌려준 건 아니겠지?",
          "original_form": "설마 내가 빌려준 건 아니겠지?",
          "speaker_id": "2",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100036501.1.5",
          "form": "다이소 오천원짜리 우산이야",
          "original_form": "다이소 오천원짜리 우산이야",
          "speaker_id": "1",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036501.1.6",
          "form": "미키마우스 우산...",
          "original_form": "미키마우스 우산...",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036501.1.7",
          "form": "너 나한테 물건 안빌려주잖아",
          "original_form": "너 나한테 물건 안빌려주잖아",
          "speaker_id": "1",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036501.1.8",
          "form": "아 그래? 다행이다",
          "original_form": "아 그래? 다행이다",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036501.1.9",
          "form": "내 건 아냐!",
          "original_form": "내 건 아냐!",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036501.1.10",
          "form": "아냐 ㅎㅎ",
          "original_form": "아냐 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036501.1.11",
          "form": "나는 양우산을 써",
          "original_form": "나는 양우산을 써",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036501.1.12",
          "form": "여름에도 유용해",
          "original_form": "여름에도 유용해",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036501.1.13",
          "form": "어머 그거 효과가있어?",
          "original_form": "어머 그거 효과가있어?",
          "speaker_id": "1",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036501.1.14",
          "form": "근데 둘이 우산을 쓸 순 없어",
          "original_form": "근데 둘이 우산을 쓸 순 없어",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036501.1.15",
          "form": "응 양우산은 참 좋아",
          "original_form": "응 양우산은 참 좋아",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036501.1.16",
          "form": "나도 엄청 살지말지 고민했었는데",
          "original_form": "나도 엄청 살지말지 고민했었는데",
          "speaker_id": "1",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036501.1.17",
          "form": "쿠팡에 크로커다일 양우산 좋더라",
          "original_form": "쿠팡에 크로커다일 양우산 좋더라",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036501.1.18",
          "form": "크기도 작아서 가방에 쏙 들어가",
          "original_form": "크기도 작아서 가방에 쏙 들어가",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036501.1.19",
          "form": "그걸 산거야??",
          "original_form": "그걸 산거야??",
          "speaker_id": "1",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036501.1.20",
          "form": "정말 추천해",
          "original_form": "정말 추천해",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036501.1.21",
          "form": "세상에 너 돈이 많구나",
          "original_form": "세상에 너 돈이 많구나",
          "speaker_id": "1",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036501.1.22",
          "form": "응ㅁ",
          "original_form": "응ㅁ",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036501.1.23",
          "form": "비싸지는 않니??",
          "original_form": "비싸지는 않니??",
          "speaker_id": "1",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036501.1.24",
          "form": "엄마가 사줬어",
          "original_form": "엄마가 사줬어",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036501.1.25",
          "form": "흠 역시 어머니의 사랑",
          "original_form": "흠 역시 어머니의 사랑",
          "speaker_id": "1",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100036501.1.26",
          "form": "세일해서 만원초반이었던거 같아",
          "original_form": "세일해서 만원초반이었던거 같아",
          "speaker_id": "2",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100036501.1.27",
          "form": "아 그정도면 살만하구나",
          "original_form": "아 그정도면 살만하구나",
          "speaker_id": "1",
          "time": "20210830 23:54"
        }
      ]
    }
  ]
