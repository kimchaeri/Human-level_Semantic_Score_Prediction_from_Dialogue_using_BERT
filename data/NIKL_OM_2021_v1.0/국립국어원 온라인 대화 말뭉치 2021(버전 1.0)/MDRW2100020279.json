      "utterance": [
        {
          "id": "MDRW2100020279.1.1",
          "form": "기억이ㅜㅜ 그 우리집준호 다음에",
          "original_form": "기억이ㅜㅜ 그 우리집준호 다음에",
          "speaker_id": "2",
          "time": "20210828 00:57"
        },
        {
          "id": "MDRW2100020279.1.2",
          "form": "나오는노래였는데",
          "original_form": "나오는노래였는데",
          "speaker_id": "2",
          "time": "20210828 00:57"
        },
        {
          "id": "MDRW2100020279.1.3",
          "form": "그 박력있는춤이 넘",
          "original_form": "그 박력있는춤이 넘",
          "speaker_id": "2",
          "time": "20210828 00:57"
        },
        {
          "id": "MDRW2100020279.1.4",
          "form": "심쿵ㅋㅋㅋㅋ",
          "original_form": "심쿵ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:57"
        },
        {
          "id": "MDRW2100020279.1.5",
          "form": "나중에 찾아봐야겠어요 ㅋㅋㅋ",
          "original_form": "나중에 찾아봐야겠어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:59"
        },
        {
          "id": "MDRW2100020279.1.6",
          "form": "뮤지컬넘버는 뭐들으세요?",
          "original_form": "뮤지컬넘버는 뭐들으세요?",
          "speaker_id": "1",
          "time": "20210828 00:59"
        },
        {
          "id": "MDRW2100020279.1.7",
          "form": "힝 ㅜㅜ못찾겠다",
          "original_form": "힝 ㅜㅜ못찾겠다",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020279.1.8",
          "form": "저 여러가지들어요ㅋㅋㅋ \n레베카 위키드 모차르트 엘리자벳 등등..",
          "original_form": "저 여러가지들어요ㅋㅋㅋ \n레베카 위키드 모차르트 엘리자벳 등등..",
          "speaker_id": "2",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020279.1.9",
          "form": "뮤지컬 좋아하시나봐요!",
          "original_form": "뮤지컬 좋아하시나봐요!",
          "speaker_id": "1",
          "time": "20210828 01:00"
        },
        {
          "id": "MDRW2100020279.1.10",
          "form": "처음에 박효신팬이여서",
          "original_form": "처음에 박효신팬이여서",
          "speaker_id": "2",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020279.1.11",
          "form": "엘리자벳으로 처음보곸ㅋ..그담에 모차르트보고...",
          "original_form": "엘리자벳으로 처음보곸ㅋ..그담에 모차르트보고...",
          "speaker_id": "2",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020279.1.12",
          "form": "그러다 확장됬어요",
          "original_form": "그러다 확장됬어요",
          "speaker_id": "2",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020279.1.13",
          "form": "아 박효신팬이셨구나~ 언제부터 좋아하셨어요?",
          "original_form": "아 박효신팬이셨구나~ 언제부터 좋아하셨어요?",
          "speaker_id": "1",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020279.1.14",
          "form": "10년 된것같아요..ㅋㅋ",
          "original_form": "10년 된것같아요..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020279.1.15",
          "form": "넘었나...",
          "original_form": "넘었나...",
          "speaker_id": "2",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020279.1.16",
          "form": "그때 사귄 남자친구가 박효신팬이여서",
          "original_form": "그때 사귄 남자친구가 박효신팬이여서",
          "speaker_id": "2",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020279.1.17",
          "form": "콘서트 데리고가줬다가 ....ㅋㅋㅋㅋㅋ 저도 그날부터 박효신 좋아했어요",
          "original_form": "콘서트 데리고가줬다가 ....ㅋㅋㅋㅋㅋ 저도 그날부터 박효신 좋아했어요",
          "speaker_id": "2",
          "time": "20210828 01:03"
        },
        {
          "id": "MDRW2100020279.1.18",
          "form": "ㅋㅋㅋ 전 예에에어전 노래들밖에 몰라요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 전 예에에어전 노래들밖에 몰라요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020279.1.19",
          "form": "해줄수없는일 이런거 ㅋㅋㅋ",
          "original_form": "해줄수없는일 이런거 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020279.1.20",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020279.1.21",
          "form": "저 그노래 아직도좋아해요",
          "original_form": "저 그노래 아직도좋아해요",
          "speaker_id": "2",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020279.1.22",
          "form": "흐어엉 ㅠㅠㅠㅠㅠㅠ",
          "original_form": "흐어엉 ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020279.1.23",
          "form": "너무좋아요",
          "original_form": "너무좋아요",
          "speaker_id": "2",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020279.1.24",
          "form": "그노래진짜좋아요!!",
          "original_form": "그노래진짜좋아요!!",
          "speaker_id": "1",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020279.1.25",
          "form": "그때창법도 지금창법도 다 좋아요 ㅋㅋ",
          "original_form": "그때창법도 지금창법도 다 좋아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020279.1.26",
          "form": "전 요즘노래도좋지만 그때 그.감성노래가",
          "original_form": "전 요즘노래도좋지만 그때 그.감성노래가",
          "speaker_id": "2",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020279.1.27",
          "form": "좋은거같아요",
          "original_form": "좋은거같아요",
          "speaker_id": "2",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020279.1.28",
          "form": "맞아요맞아요 감성이 다르죠 그시절 ㅋㅋㅋㅋ",
          "original_form": "맞아요맞아요 감성이 다르죠 그시절 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020279.1.29",
          "form": "전남친은 떠나갔지만 ㅠㅠㅠ대장나무가 남았어요",
          "original_form": "전남친은 떠나갔지만 ㅠㅠㅠ대장나무가 남았어요",
          "speaker_id": "2",
          "time": "20210828 01:04"
        }
      ]
