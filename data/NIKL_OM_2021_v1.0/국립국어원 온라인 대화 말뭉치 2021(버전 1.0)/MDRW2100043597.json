        {
          "id": "MDRW2100043597.1.2",
          "form": "뮤지컬 비싸잖아",
          "original_form": "뮤지컬 비싸잖아",
          "speaker_id": "2",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043597.1.3",
          "form": "비싸도 그 값을 하더라",
          "original_form": "비싸도 그 값을 하더라",
          "speaker_id": "1",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043597.1.4",
          "form": "같이 가자",
          "original_form": "같이 가자",
          "speaker_id": "2",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043597.1.5",
          "form": "웅 ㅋㅋ 엄마도 보고싶죠?",
          "original_form": "웅 ㅋㅋ 엄마도 보고싶죠?",
          "speaker_id": "1",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043597.1.6",
          "form": "재밌는 거 있나",
          "original_form": "재밌는 거 있나",
          "speaker_id": "2",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043597.1.7",
          "form": "찾아봐야할듯",
          "original_form": "찾아봐야할듯",
          "speaker_id": "1",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043597.1.8",
          "form": "근데코로나 인데 볼 수 있나",
          "original_form": "근데코로나 인데 볼 수 있나",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043597.1.9",
          "form": "웅웅 요새 다 하더라",
          "original_form": "웅웅 요새 다 하더라",
          "speaker_id": "1",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043597.1.10",
          "form": "그래도 조심해야 할 듯",
          "original_form": "그래도 조심해야 할 듯",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043597.1.11",
          "form": "그래 재밌는 거 찾으면 얘기해라",
          "original_form": "그래 재밌는 거 찾으면 얘기해라",
          "speaker_id": "2",
          "time": "20210907 19:40"
