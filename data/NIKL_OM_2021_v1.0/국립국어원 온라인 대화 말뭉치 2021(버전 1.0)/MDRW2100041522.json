      "utterance": [
        {
          "id": "MDRW2100041522.1.1",
          "form": "건강을 위해 먹는 영양제 있나요?",
          "original_form": "건강을 위해 먹는 영양제 있나요?",
          "speaker_id": "1",
          "time": "20210906 08:52"
        },
        {
          "id": "MDRW2100041522.1.2",
          "form": "저 프로폴리스 먹어요",
          "original_form": "저 프로폴리스 먹어요",
          "speaker_id": "2",
          "time": "20210906 12:04"
        },
        {
          "id": "MDRW2100041522.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210906 12:04"
        },
        {
          "id": "MDRW2100041522.1.4",
          "form": "프로폴리스 저희 집에도 있어요",
          "original_form": "프로폴리스 저희 집에도 있어요",
          "speaker_id": "1",
          "time": "20210906 14:53"
        },
        {
          "id": "MDRW2100041522.1.5",
          "form": "저는 뼈 건강을 위해 비타민 디와 칼슘 먹고 있고",
          "original_form": "저는 뼈 건강을 위해 비타민 디와 칼슘 먹고 있고",
          "speaker_id": "1",
          "time": "20210906 14:53"
        },
        {
          "id": "MDRW2100041522.1.6",
          "form": "감기예방을 위해 비타민 씨를 먹고 있어요",
          "original_form": "감기예방을 위해 비타민 씨를 먹고 있어요",
          "speaker_id": "1",
          "time": "20210906 14:53"
        },
        {
          "id": "MDRW2100041522.1.7",
          "form": "혹시 유산균은 안드시나요?",
          "original_form": "혹시 유산균은 안드시나요?",
          "speaker_id": "1",
          "time": "20210906 14:53"
        },
        {
          "id": "MDRW2100041522.1.8",
          "form": "유산균 이제 먹을까해요",
          "original_form": "유산균 이제 먹을까해요",
          "speaker_id": "2",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100041522.1.9",
          "form": "드세요???",
          "original_form": "드세요???",
          "speaker_id": "2",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100041522.1.10",
          "form": "최근에 먹기 시작했어요",
          "original_form": "최근에 먹기 시작했어요",
          "speaker_id": "1",
          "time": "20210906 15:31"
        },
        {
          "id": "MDRW2100041522.1.11",
          "form": "그런데 자기에게 맞는 유산균이 있다고 해서",
          "original_form": "그런데 자기에게 맞는 유산균이 있다고 해서",
          "speaker_id": "1",
          "time": "20210906 15:31"
        },
        {
          "id": "MDRW2100041522.1.12",
          "form": "지금 먹는 거 끝나면 다른 것도 먹어볼까 생각중이에요",
          "original_form": "지금 먹는 거 끝나면 다른 것도 먹어볼까 생각중이에요",
          "speaker_id": "1",
          "time": "20210906 15:31"
        },
        {
          "id": "MDRW2100041522.1.13",
          "form": "지금 어떤거",
          "original_form": "지금 어떤거",
          "speaker_id": "2",
          "time": "20210906 15:32"
        },
        {
          "id": "MDRW2100041522.1.14",
          "form": "드시고 계세요?",
          "original_form": "드시고 계세요?",
          "speaker_id": "2",
          "time": "20210906 15:32"
        },
        {
          "id": "MDRW2100041522.1.15",
          "form": "오빠한테 선물 받은거라 잘 모르겠는데",
          "original_form": "오빠한테 선물 받은거라 잘 모르겠는데",
          "speaker_id": "1",
          "time": "20210906 15:32"
        },
        {
          "id": "MDRW2100041522.1.16",
          "form": "이름 알려진 건 아닌거 같아요",
          "original_form": "이름 알려진 건 아닌거 같아요",
          "speaker_id": "1",
          "time": "20210906 15:32"
        },
        {
          "id": "MDRW2100041522.1.17",
          "form": "아하 유명한건",
          "original_form": "아하 유명한건",
          "speaker_id": "2",
          "time": "20210906 15:33"
        },
        {
          "id": "MDRW2100041522.1.18",
          "form": "락토핏",
          "original_form": "락토핏",
          "speaker_id": "2",
          "time": "20210906 15:33"
        },
        {
          "id": "MDRW2100041522.1.19",
          "form": "이거저라두요",
          "original_form": "이거저라두요",
          "speaker_id": "2",
          "time": "20210906 15:33"
        },
        {
          "id": "MDRW2100041522.1.20",
          "form": "들어봤어요",
          "original_form": "들어봤어요",
          "speaker_id": "1",
          "time": "20210906 15:48"
        },
        {
          "id": "MDRW2100041522.1.21",
          "form": "근데 저희 오빠가 잘 안알려졌는데 좋다는 거 이런거 추천 잘 하는 분이라 ㅎ ㅎ",
          "original_form": "근데 저희 오빠가 잘 안알려졌는데 좋다는 거 이런거 추천 잘 하는 분이라 ㅎ ㅎ",
          "speaker_id": "1",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100041522.1.22",
          "form": "일단 주신거 먹고 있어요",
          "original_form": "일단 주신거 먹고 있어요",
          "speaker_id": "1",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100041522.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ헉",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ헉",
          "speaker_id": "2",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100041522.1.24",
          "form": "어디껀지 귱금하네요.",
          "original_form": "어디껀지 귱금하네요.",
          "speaker_id": "2",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100041522.1.25",
          "form": "잠시만요",
          "original_form": "잠시만요",
          "speaker_id": "1",
          "time": "20210906 15:53"
        },
        {
          "id": "MDRW2100041522.1.26",
          "form": "헬로팜 꺼라고 돼있네요",
          "original_form": "헬로팜 꺼라고 돼있네요",
          "speaker_id": "1",
          "time": "20210906 15:53"
        },
        {
          "id": "MDRW2100041522.1.27",
          "form": "프리미엄 프로바이오틱스",
          "original_form": "프리미엄 프로바이오틱스",
          "speaker_id": "1",
          "time": "20210906 15:53"
        },
        {
          "id": "MDRW2100041522.1.28",
          "form": "헬로팜 쳐봐야겠어요",
          "original_form": "헬로팜 쳐봐야겠어요",
          "speaker_id": "2",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100041522.1.29",
          "form": "네 전 오빠가 가져다준거라 과연 온라인에서도 파는지는 모르겠네요",
          "original_form": "네 전 오빠가 가져다준거라 과연 온라인에서도 파는지는 모르겠네요",
          "speaker_id": "1",
          "time": "20210906 15:58"
        },
        {
          "id": "MDRW2100041522.1.30",
          "form": "아합.. 그럼 .. 아쉽네요",
          "original_form": "아합.. 그럼 .. 아쉽네요",
          "speaker_id": "2",
          "time": "20210906 15:59"
        }
      ]
