      "utterance": [
        {
          "id": "MDRW2100017969.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100017969.1.2",
          "form": "안녕하세요! 저녁 뭐드셨나요?",
          "original_form": "안녕하세요! 저녁 뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100017969.1.3",
          "form": "저는 오늘 깐풍기먹었어요~",
          "original_form": "저는 오늘 깐풍기먹었어요~",
          "speaker_id": "1",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100017969.1.4",
          "form": "맛있겠어용! 배달로시키셨나요?",
          "original_form": "맛있겠어용! 배달로시키셨나요?",
          "speaker_id": "2",
          "time": "20210826 21:55"
        },
        {
          "id": "MDRW2100017969.1.5",
          "form": "아 저희아버지께서 테이크아웃하셨어요 ㅎㅎ",
          "original_form": "아 저희아버지께서 테이크아웃하셨어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100017969.1.6",
          "form": "좋은 아버님을 두셨네요!ㅎㅎ 깐풍기 오랜만에 먹어보고싶어요!",
          "original_form": "좋은 아버님을 두셨네요!ㅎㅎ 깐풍기 오랜만에 먹어보고싶어요!",
          "speaker_id": "2",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100017969.1.7",
          "form": "저는 토스트 배달해서 먹었어요!",
          "original_form": "저는 토스트 배달해서 먹었어요!",
          "speaker_id": "2",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100017969.1.8",
          "form": "앗ㅎㅎ그런가요~ 오 토스트라면 이삭토스트인가요??",
          "original_form": "앗ㅎㅎ그런가요~ 오 토스트라면 이삭토스트인가요??",
          "speaker_id": "1",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100017969.1.9",
          "form": "넵맞아요! 요즘은 토스트도 배달돼서 넘좋은것같아요ㅎㅎ",
          "original_form": "넵맞아요! 요즘은 토스트도 배달돼서 넘좋은것같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100017969.1.10",
          "form": "그쵸~ 역시 한국은 배달의민족",
          "original_form": "그쵸~ 역시 한국은 배달의민족",
          "speaker_id": "1",
          "time": "20210826 22:11"
        },
        {
          "id": "MDRW2100017969.1.11",
          "form": "떡볶이를 가장 많이 시켜먹는것같아요 저는ㅎㅎ",
          "original_form": "떡볶이를 가장 많이 시켜먹는것같아요 저는ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 22:13"
        },
        {
          "id": "MDRW2100017969.1.12",
          "form": "아하! 저는 떡볶이 요새 밀키트 잘나와서 그걸로먹어요~",
          "original_form": "아하! 저는 떡볶이 요새 밀키트 잘나와서 그걸로먹어요~",
          "speaker_id": "1",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100017969.1.13",
          "form": "밀키트도좋죠! 요즘엔 구성도 좋고 맛도 있더라구요!",
          "original_form": "밀키트도좋죠! 요즘엔 구성도 좋고 맛도 있더라구요!",
          "speaker_id": "2",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100017969.1.14",
          "form": "맞아요~ 금미옥떡볶이 최애에요 ㅎㅎ",
          "original_form": "맞아요~ 금미옥떡볶이 최애에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:16"
        },
        {
          "id": "MDRW2100017969.1.15",
          "form": "오 처음들어봐용! 한번 사봐야겠어요!ㅎㅎ",
          "original_form": "오 처음들어봐용! 한번 사봐야겠어요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 22:16"
        },
        {
          "id": "MDRW2100017969.1.16",
          "form": "다른키트도 사신적있으신가용",
          "original_form": "다른키트도 사신적있으신가용",
          "speaker_id": "2",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100017969.1.17",
          "form": "네 ㅋㅋ 미로떡볶이도 먹어봤어요! 그것도 먹을만해요 ㅎㅎ",
          "original_form": "네 ㅋㅋ 미로떡볶이도 먹어봤어요! 그것도 먹을만해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:17"
        },
        {
          "id": "MDRW2100017969.1.18",
          "form": "추천감사드려용ㅎㅎㅎ! 주문하러 가야겠어요",
          "original_form": "추천감사드려용ㅎㅎㅎ! 주문하러 가야겠어요",
          "speaker_id": "2",
          "time": "20210826 22:18"
        }
      ]
