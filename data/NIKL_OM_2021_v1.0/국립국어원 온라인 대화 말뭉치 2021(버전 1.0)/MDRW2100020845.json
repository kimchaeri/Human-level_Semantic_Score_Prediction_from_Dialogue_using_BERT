        },
        {
          "id": "MDRW2100020845.1.2",
          "form": "엠버성능은 좀",
          "original_form": "엠버성능은 좀",
          "speaker_id": "2",
          "time": "20210828 13:22"
        },
        {
          "id": "MDRW2100020845.1.3",
          "form": "너무해",
          "original_form": "너무해",
          "speaker_id": "2",
          "time": "20210828 13:22"
        },
        {
          "id": "MDRW2100020845.1.4",
          "form": "왜 걔가 견습기사가 아닌건지 모를지경",
          "original_form": "왜 걔가 견습기사가 아닌건지 모를지경",
          "speaker_id": "2",
          "time": "20210828 13:22"
        },
        {
          "id": "MDRW2100020845.1.5",
          "form": "ㅇㅇ 그니까",
          "original_form": "ㅇㅇ 그니까",
          "speaker_id": "1",
          "time": "20210828 13:24"
        },
        {
          "id": "MDRW2100020845.1.6",
          "form": "성능은 엠버보다 일일퀘스트 주는 걔가 더 좋을둣 ㅋㅋㅋ",
          "original_form": "성능은 엠버보다 일일퀘스트 주는 걔가 더 좋을둣 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:24"
        },
        {
          "id": "MDRW2100020845.1.7",
          "form": "맨날 통나무 쓰러트리기 시키는 npc",
          "original_form": "맨날 통나무 쓰러트리기 시키는 npc",
          "speaker_id": "1",
          "time": "20210828 13:24"
        },
        {
          "id": "MDRW2100020845.1.8",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:25"
        },
        {
          "id": "MDRW2100020845.1.9",
          "form": "정찰을 잘하나?",
          "original_form": "정찰을 잘하나?",
          "speaker_id": "2",
          "time": "20210828 13:25"
        },
        {
          "id": "MDRW2100020845.1.10",
          "form": "딱...히..",
          "original_form": "딱...히..",
          "speaker_id": "1",
          "time": "20210828 13:25"
        },
        {
          "id": "MDRW2100020845.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그건 그래",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그건 그래",
          "speaker_id": "2",
          "time": "20210828 13:26"
        },
        {
          "id": "MDRW2100020845.1.12",
          "form": "수상한 이방인인 주인공",
          "original_form": "수상한 이방인인 주인공",
