      "utterance": [
        {
          "id": "MDRW2100018817.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100018817.1.2",
          "form": "건강하시지요?",
          "original_form": "건강하시지요?",
          "speaker_id": "2",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100018817.1.3",
          "form": "네 건강합니다..!ㅋㅋㅋ",
          "original_form": "네 건강합니다..!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018817.1.4",
          "form": "또자엄마는 어떠신가요",
          "original_form": "또자엄마는 어떠신가요",
          "speaker_id": "1",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018817.1.5",
          "form": "네 저도 건강해요ㅎ",
          "original_form": "네 저도 건강해요ㅎ",
          "speaker_id": "2",
          "time": "20210827 12:53"
        },
        {
          "id": "MDRW2100018817.1.6",
          "form": "운동하시는 거 있으세요?",
          "original_form": "운동하시는 거 있으세요?",
          "speaker_id": "1",
          "time": "20210827 12:56"
        },
        {
          "id": "MDRW2100018817.1.7",
          "form": "숨쉬기운동말고는ㅠ",
          "original_form": "숨쉬기운동말고는ㅠ",
          "speaker_id": "2",
          "time": "20210827 13:54"
        },
        {
          "id": "MDRW2100018817.1.8",
          "form": "앗ㅋㅋㅋ운동안하셔도 건강하시네요",
          "original_form": "앗ㅋㅋㅋ운동안하셔도 건강하시네요",
          "speaker_id": "1",
          "time": "20210827 13:55"
        },
        {
          "id": "MDRW2100018817.1.9",
          "form": "영양제 드시는 거 있으세요?",
          "original_form": "영양제 드시는 거 있으세요?",
          "speaker_id": "1",
          "time": "20210827 13:55"
        },
        {
          "id": "MDRW2100018817.1.10",
          "form": "눈 영양제만 먹어여",
          "original_form": "눈 영양제만 먹어여",
          "speaker_id": "2",
          "time": "20210827 16:10"
        },
        {
          "id": "MDRW2100018817.1.11",
          "form": "루테인?",
          "original_form": "루테인?",
          "speaker_id": "1",
          "time": "20210827 16:19"
        },
        {
          "id": "MDRW2100018817.1.12",
          "form": "넵 눈이 피곤해서요",
          "original_form": "넵 눈이 피곤해서요",
          "speaker_id": "2",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100018817.1.13",
          "form": "저도 전에 먹었는데 지금은 휴약기 갖고 있어요",
          "original_form": "저도 전에 먹었는데 지금은 휴약기 갖고 있어요",
          "speaker_id": "1",
          "time": "20210827 16:29"
        },
        {
          "id": "MDRW2100018817.1.14",
          "form": "계속 먹으면 안좋다길래",
          "original_form": "계속 먹으면 안좋다길래",
          "speaker_id": "1",
          "time": "20210827 16:29"
        },
        {
          "id": "MDRW2100018817.1.15",
          "form": "앗 그렇구나요",
          "original_form": "앗 그렇구나요",
          "speaker_id": "2",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100018817.1.16",
          "form": "네ㅎㅎㅎ 그래서 가끔 쉬어주며 먹고있어요",
          "original_form": "네ㅎㅎㅎ 그래서 가끔 쉬어주며 먹고있어요",
