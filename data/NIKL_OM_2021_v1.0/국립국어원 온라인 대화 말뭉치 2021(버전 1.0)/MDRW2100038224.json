      "utterance": [
        {
          "id": "MDRW2100038224.1.1",
          "form": "첨 윤식당 재밌었는데",
          "original_form": "첨 윤식당 재밌었는데",
          "speaker_id": "2",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100038224.1.2",
          "form": "엉 시즌1이네",
          "original_form": "엉 시즌1이네",
          "speaker_id": "1",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100038224.1.3",
          "form": "2도 괜찮았어",
          "original_form": "2도 괜찮았어",
          "speaker_id": "2",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100038224.1.4",
          "form": "엉 스페인인가 그랬나",
          "original_form": "엉 스페인인가 그랬나",
          "speaker_id": "1",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100038224.1.5",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100038224.1.6",
          "form": "지금은 보라카이인가 1이니",
          "original_form": "지금은 보라카이인가 1이니",
          "speaker_id": "1",
          "time": "20210902 01:09"
        },
        {
          "id": "MDRW2100038224.1.7",
          "form": "그런가 가물가물 하다",
          "original_form": "그런가 가물가물 하다",
          "speaker_id": "2",
          "time": "20210902 01:10"
        },
        {
          "id": "MDRW2100038224.1.8",
          "form": "그 바닷가 해변에 신구가 알바생을 나오고",
          "original_form": "그 바닷가 해변에 신구가 알바생을 나오고",
          "speaker_id": "1",
          "time": "20210902 01:10"
        },
        {
          "id": "MDRW2100038224.1.9",
          "form": "응 그건 기억나는데 지역은",
          "original_form": "응 그건 기억나는데 지역은",
          "speaker_id": "2",
          "time": "20210902 01:10"
        },
        {
          "id": "MDRW2100038224.1.10",
          "form": "저기가 2호점일듯",
          "original_form": "저기가 2호점일듯",
          "speaker_id": "1",
          "time": "20210902 01:11"
        },
        {
          "id": "MDRW2100038224.1.11",
          "form": "엉 철거되고 바로 장만한",
          "original_form": "엉 철거되고 바로 장만한",
          "speaker_id": "2",
          "time": "20210902 01:11"
        },
        {
          "id": "MDRW2100038224.1.12",
          "form": "그러고 보면 방송도 참 흥미진진하다",
          "original_form": "그러고 보면 방송도 참 흥미진진하다",
          "speaker_id": "1",
          "time": "20210902 01:11"
        },
        {
          "id": "MDRW2100038224.1.13",
          "form": "응 무슨일이 생길줄 모르니",
          "original_form": "응 무슨일이 생길줄 모르니",
          "speaker_id": "2",
          "time": "20210902 01:11"
        },
        {
          "id": "MDRW2100038224.1.14",
          "form": "엉 갑자기 출연배우가 음주운전이나 사고를 일으킬수 있으니",
          "original_form": "엉 갑자기 출연배우가 음주운전이나 사고를 일으킬수 있으니",
          "speaker_id": "1",
          "time": "20210902 01:12"
        },
        {
          "id": "MDRW2100038224.1.15",
          "form": "윤식당 2호점은 진자 하루만에 했네",
          "original_form": "윤식당 2호점은 진자 하루만에 했네",
          "speaker_id": "1",
