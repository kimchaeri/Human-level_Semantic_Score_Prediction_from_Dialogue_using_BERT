      "utterance": [
        {
          "id": "MDRW2100033414.1.1",
          "form": "오늘 아침 먹었나요?",
          "original_form": "오늘 아침 먹었나요?",
          "speaker_id": "1",
          "time": "20210907 07:43"
        },
        {
          "id": "MDRW2100033414.1.2",
          "form": "아뇨 오늘 아침 바빠서 미숫가루 한 잔 마시고 출근했어요",
          "original_form": "아뇨 오늘 아침 바빠서 미숫가루 한 잔 마시고 출근했어요",
          "speaker_id": "2",
          "time": "20210907 08:43"
        },
        {
          "id": "MDRW2100033414.1.3",
          "form": "name1님은 식사하셨어요?",
          "original_form": "&name1&님은 식사하셨어요?",
          "speaker_id": "2",
          "time": "20210907 08:43"
        },
        {
          "id": "MDRW2100033414.1.4",
          "form": "저도 드디어 편의점 샌드위치먹었어요!!",
          "original_form": "저도 드디어 편의점 샌드위치먹었어요!!",
          "speaker_id": "1",
          "time": "20210907 09:30"
        },
        {
          "id": "MDRW2100033414.1.5",
          "form": "name1님 출근도 이른시간에하고",
          "original_form": "&name1&님 출근도 이른시간에하고",
          "speaker_id": "2",
          "time": "20210907 09:38"
        },
        {
          "id": "MDRW2100033414.1.6",
          "form": "식사도 샌드위치로 때우고",
          "original_form": "식사도 샌드위치로 때우고",
          "speaker_id": "2",
          "time": "20210907 09:38"
        },
        {
          "id": "MDRW2100033414.1.7",
          "form": "힘드실것같아요",
          "original_form": "힘드실것같아요",
          "speaker_id": "2",
          "time": "20210907 09:38"
        },
        {
          "id": "MDRW2100033414.1.8",
          "form": "아ㅜㅜ매일아침마다 반복할것같아요",
          "original_form": "아ㅜㅜ매일아침마다 반복할것같아요",
          "speaker_id": "1",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100033414.1.9",
          "form": "출근시간이 빠른가요?",
          "original_form": "출근시간이 빠른가요?",
          "speaker_id": "2",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100033414.1.10",
          "form": "아침에 일찍 일어나야해요ㅜㅜ",
          "original_form": "아침에 일찍 일어나야해요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 09:48"
        },
        {
          "id": "MDRW2100033414.1.11",
          "form": "직장이 멀어서요ㅜㅜ집이랑",
          "original_form": "직장이 멀어서요ㅜㅜ집이랑",
          "speaker_id": "1",
          "time": "20210907 09:48"
        },
        {
          "id": "MDRW2100033414.1.12",
          "form": "그래서 아침을 못먹어요ㅜㅜ",
          "original_form": "그래서 아침을 못먹어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 09:48"
        },
        {
          "id": "MDRW2100033414.1.13",
          "form": "중간에 편의점들러서 사먹어야해요",
          "original_form": "중간에 편의점들러서 사먹어야해요",
          "speaker_id": "1",
          "time": "20210907 09:48"
        },
        {
          "id": "MDRW2100033414.1.14",
          "form": "흠... 안타깝네요",
          "original_form": "흠... 안타깝네요",
          "speaker_id": "2",
          "time": "20210907 10:01"
        },
        {
          "id": "MDRW2100033414.1.15",
          "form": "저도 아침에 미숫가루 한 잔 마시고 왔는데",
          "original_form": "저도 아침에 미숫가루 한 잔 마시고 왔는데",
          "speaker_id": "2",
          "time": "20210907 10:01"
        },
        {
          "id": "MDRW2100033414.1.16",
          "form": "힘내시고 점심 든든하게 드세요",
          "original_form": "힘내시고 점심 든든하게 드세요",
          "speaker_id": "2",
          "time": "20210907 10:05"
        },
        {
          "id": "MDRW2100033414.1.17",
          "form": "점심 함박스테이크나와서 버텄어오!!",
          "original_form": "점심 함박스테이크나와서 버텄어오!!",
          "speaker_id": "1",
          "time": "20210907 17:10"
        },
        {
          "id": "MDRW2100033414.1.18",
          "form": "낼은 새우튀김나와요",
          "original_form": "낼은 새우튀김나와요",
          "speaker_id": "1",
          "time": "20210907 17:10"
        },
        {
          "id": "MDRW2100033414.1.19",
          "form": "점심때 유치원에서 나온건가요?",
          "original_form": "점심때 유치원에서 나온건가요?",
          "speaker_id": "2",
          "time": "20210907 17:38"
        },
        {
          "id": "MDRW2100033414.1.20",
          "form": "맛있었겠다",
          "original_form": "맛있었겠다",
          "speaker_id": "2",
          "time": "20210907 17:38"
        },
        {
          "id": "MDRW2100033414.1.21",
          "form": "퇴근은 하셨어요?",
          "original_form": "퇴근은 하셨어요?",
          "speaker_id": "2",
          "time": "20210907 17:38"
        },
        {
          "id": "MDRW2100033414.1.22",
          "form": "출근하기전에 사먹은거죠ㅜㅜ",
          "original_form": "출근하기전에 사먹은거죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 17:43"
        },
        {
          "id": "MDRW2100033414.1.23",
