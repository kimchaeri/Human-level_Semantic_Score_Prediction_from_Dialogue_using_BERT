      "utterance": [
        {
          "id": "MDRW2100047547.1.1",
          "form": "ㅜㅜㅜ만화책샀는데 방금",
          "original_form": "ㅜㅜㅜ만화책샀는데 방금",
          "speaker_id": "1",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100047547.1.2",
          "form": "카드가안보엿ㅜㅜ",
          "original_form": "카드가안보엿ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100047547.1.3",
          "form": "헐 잃어버린거?",
          "original_form": "헐 잃어버린거?",
          "speaker_id": "2",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100047547.1.4",
          "form": "슬퍼요ㅜㅜ따흑흑",
          "original_form": "슬퍼요ㅜㅜ따흑흑",
          "speaker_id": "1",
          "time": "20210826 19:50"
        },
        {
          "id": "MDRW2100047547.1.5",
          "form": "일단 가게에 두고온거일수도",
          "original_form": "일단 가게에 두고온거일수도",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100047547.1.6",
          "form": "서점 다시가봄?",
          "original_form": "서점 다시가봄?",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100047547.1.7",
          "form": "아마 아침에 빵살때 안받았나봥",
          "original_form": "아마 아침에 빵살때 안받았나봥",
          "speaker_id": "1",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100047547.1.8",
          "form": "헐 그럼 책 못산거임?",
          "original_form": "헐 그럼 책 못산거임?",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100047547.1.9",
          "form": "서점아니궁 아침 출근길에 들러야함",
          "original_form": "서점아니궁 아침 출근길에 들러야함",
          "speaker_id": "1",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100047547.1.10",
          "form": "다른카드에 다행히 돈이 있었당 6000원^^",
          "original_form": "다른카드에 다행히 돈이 있었당 6000원^^",
          "speaker_id": "1",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100047547.1.11",
          "form": "저런 신용카드임?",
          "original_form": "저런 신용카드임?",
          "speaker_id": "2",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100047547.1.12",
          "form": "체쿠",
          "original_form": "체쿠",
          "speaker_id": "1",
          "time": "20210826 19:51"
        },
        {
          "id": "MDRW2100047547.1.13",
          "form": "신고 안해도 되려나",
          "original_form": "신고 안해도 되려나",
          "speaker_id": "2",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100047547.1.14",
          "form": "다행히 쓴 연락은안옴ㅜㅜ",
          "original_form": "다행히 쓴 연락은안옴ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100047547.1.15",
          "form": "낼 아침에 가게 가보구 신고하려고ㅜㅜ",
          "original_form": "낼 아침에 가게 가보구 신고하려고ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100047547.1.16",
          "form": "누가 주워서 쓰면 우째요",
          "original_form": "누가 주워서 쓰면 우째요",
          "speaker_id": "2",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100047547.1.17",
          "form": "그전에",
          "original_form": "그전에",
          "speaker_id": "2",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100047547.1.18",
          "form": "내가이렇게",
          "original_form": "내가이렇게",
          "speaker_id": "1",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100047547.1.19",
          "form": "자꾸카드늘어남ㅜ",
          "original_form": "자꾸카드늘어남ㅜ",
          "speaker_id": "1",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100047547.1.20",
          "form": "딴데서 잃어버린거라서",
          "original_form": "딴데서 잃어버린거라서",
          "speaker_id": "2",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100047547.1.21",
          "form": "아무도 아직 안씀",
          "original_form": "아무도 아직 안씀",
          "speaker_id": "1",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100047547.1.22",
          "form": "ㅋㅋ 어른이 되어간다는 증거",
          "original_form": "ㅋㅋ 어른이 되어간다는 증거",
          "speaker_id": "2",
          "time": "20210826 19:52"
        },
        {
          "id": "MDRW2100047547.1.23",
          "form": "안돼ㅜㅜㅜㅜㅜ내카드ㅜㅜㅜㅜ",
          "original_form": "안돼ㅜㅜㅜㅜㅜ내카드ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100047547.1.24",
          "form": "그거루 밥먹는데ㅜㅜ",
          "original_form": "그거루 밥먹는데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100047547.1.25",
          "form": "서점에서 지갑뒤지다가 ㅜ 매우 당황했었음ㅜㅜㅜ",
          "original_form": "서점에서 지갑뒤지다가 ㅜ 매우 당황했었음ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 19:53"
        },
        {
          "id": "MDRW2100047547.1.26",
          "form": "못찾으면우짜누",
          "original_form": "못찾으면우짜누",
          "speaker_id": "2",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100047547.1.27",
          "form": "재발급공짜이려나",
          "original_form": "재발급공짜이려나",
          "speaker_id": "2",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100047547.1.28",
          "form": "나를봐요 거지라서 신용카드도 체크카드도없음 ㅋㅋ",
          "original_form": "나를봐요 거지라서 신용카드도 체크카드도없음 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:54"
        }
      ]
