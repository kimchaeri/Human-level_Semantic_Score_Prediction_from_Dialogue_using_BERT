      "utterance": [
        {
          "id": "MDRW2100039889.1.1",
          "form": "공산품이 비싼것같아요",
          "original_form": "공산품이 비싼것같아요",
          "speaker_id": "2",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039889.1.2",
          "form": "음식은요?",
          "original_form": "음식은요?",
          "speaker_id": "1",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039889.1.3",
          "form": "아이옷이나 장난감도 미리 챙겨가더라고요",
          "original_form": "아이옷이나 장난감도 미리 챙겨가더라고요",
          "speaker_id": "2",
          "time": "20210903 22:21"
        },
        {
          "id": "MDRW2100039889.1.4",
          "form": "올캐가 요리하고 식당 이용은 안할 것 같아요 ㅎㅎ",
          "original_form": "올캐가 요리하고 식당 이용은 안할 것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:22"
        },
        {
          "id": "MDRW2100039889.1.5",
          "form": "아 ㅎ 좋네요",
          "original_form": "아 ㅎ 좋네요",
          "speaker_id": "1",
          "time": "20210903 22:24"
        },
        {
          "id": "MDRW2100039889.1.6",
          "form": "한국에서 선물많이 가져가야겠어요",
          "original_form": "한국에서 선물많이 가져가야겠어요",
          "speaker_id": "1",
          "time": "20210903 22:25"
        },
        {
          "id": "MDRW2100039889.1.7",
          "form": "갈때 컨테이너로 이미 한 가득 갖고 갔고 거기서도 한국인 커뮤니티가 있나봐요",
          "original_form": "갈때 컨테이너로 이미 한 가득 갖고 갔고 거기서도 한국인 커뮤니티가 있나봐요",
          "speaker_id": "2",
          "time": "20210903 22:26"
        },
        {
          "id": "MDRW2100039889.1.8",
          "form": "귀국하면소 물품 팔거나 서로 나눔도 하고 ㅎㅎ",
          "original_form": "귀국하면소 물품 팔거나 서로 나눔도 하고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:26"
        },
        {
          "id": "MDRW2100039889.1.9",
          "form": "오 그럼 서로서로 좋겠네요",
          "original_form": "오 그럼 서로서로 좋겠네요",
          "speaker_id": "1",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100039889.1.10",
          "form": "기념품은 역시 자석일까요?",
          "original_form": "기념품은 역시 자석일까요?",
          "speaker_id": "1",
          "time": "20210903 22:27"
        },
        {
          "id": "MDRW2100039889.1.11",
          "form": "두바이 스벅컵을 한번 사왔는데 ㅋㅋ",
          "original_form": "두바이 스벅컵을 한번 사왔는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100039889.1.12",
          "form": "저에게는 별 메리트가 없어요",
          "original_form": "저에게는 별 메리트가 없어요",
          "speaker_id": "2",
          "time": "20210903 22:28"
        },
        {
          "id": "MDRW2100039889.1.13",
          "form": "ㅎㅎ나라별로 스벅이 만만하죠",
          "original_form": "ㅎㅎ나라별로 스벅이 만만하죠",
          "speaker_id": "1",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100039889.1.14",
          "form": "고이모셨다가 당근에 나오겠어요",
          "original_form": "고이모셨다가 당근에 나오겠어요",
          "speaker_id": "1",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100039889.1.15",
          "form": "그러게요 저도 처음엔 별 생각없었는데 자석이 기념되고 좋더라고요",
          "original_form": "그러게요 저도 처음엔 별 생각없었는데 자석이 기념되고 좋더라고요",
          "speaker_id": "2",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100039889.1.16",
          "form": "그동안 안샀던게 좀 아쉬워요",
          "original_form": "그동안 안샀던게 좀 아쉬워요",
          "speaker_id": "2",
          "time": "20210903 22:30"
        },
        {
          "id": "MDRW2100039889.1.17",
          "form": "맞아옹 이젠 못가서 구하기도 어려우니",
          "original_form": "맞아옹 이젠 못가서 구하기도 어려우니",
          "speaker_id": "1",
          "time": "20210903 22:30"
        },
        {
          "id": "MDRW2100039889.1.18",
          "form": "메이드인 차이나지만",
          "original_form": "메이드인 차이나지만",
          "speaker_id": "1",
          "time": "20210903 22:30"
        },
        {
          "id": "MDRW2100039889.1.19",
          "form": "그러게요 ㅋㅋ세계에 있는 메이드인 차이나 ㅎㅎ",
          "original_form": "그러게요 ㅋㅋ세계에 있는 메이드인 차이나 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100039889.1.20",
          "form": "못산거 거기서 다살수있겟어요 ㅎ",
          "original_form": "못산거 거기서 다살수있겟어요 ㅎ",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100039889.1.21",
          "form": "악 그러네요 정작 중국을 가본적이ㅡ없오요",
          "original_form": "악 그러네요 정작 중국을 가본적이ㅡ없오요",
          "speaker_id": "2",
          "time": "20210903 22:47"
        }
      ]
