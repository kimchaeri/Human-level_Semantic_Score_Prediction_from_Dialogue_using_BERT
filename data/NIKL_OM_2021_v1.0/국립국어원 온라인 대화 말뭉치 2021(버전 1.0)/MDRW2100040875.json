      "utterance": [
        {
          "id": "MDRW2100040875.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210905 10:46"
        },
        {
          "id": "MDRW2100040875.1.2",
          "form": "좋았던 여행지 있으세요?",
          "original_form": "좋았던 여행지 있으세요?",
          "speaker_id": "1",
          "time": "20210905 10:46"
        },
        {
          "id": "MDRW2100040875.1.3",
          "form": "유럽이 제일 기억에남슴다",
          "original_form": "유럽이 제일 기억에남슴다",
          "speaker_id": "2",
          "time": "20210905 10:47"
        },
        {
          "id": "MDRW2100040875.1.4",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210905 10:47"
        },
        {
          "id": "MDRW2100040875.1.5",
          "form": "전 프랑스 죽기전에 열번은 더 가보고싶어요..",
          "original_form": "전 프랑스 죽기전에 열번은 더 가보고싶어요..",
          "speaker_id": "1",
          "time": "20210905 10:47"
        },
        {
          "id": "MDRW2100040875.1.6",
          "form": "유럽 어디가셧나용",
          "original_form": "유럽 어디가셧나용",
          "speaker_id": "2",
          "time": "20210905 10:47"
        },
        {
          "id": "MDRW2100040875.1.7",
          "form": "파리 올림픽ㅠㅠ",
          "original_form": "파리 올림픽ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 10:48"
        },
        {
          "id": "MDRW2100040875.1.8",
          "form": "서유럽은 대부분 가봤어요",
          "original_form": "서유럽은 대부분 가봤어요",
          "speaker_id": "1",
          "time": "20210905 10:48"
        },
        {
          "id": "MDRW2100040875.1.9",
          "form": "오스트리아도 좋았어요",
          "original_form": "오스트리아도 좋았어요",
          "speaker_id": "1",
          "time": "20210905 10:49"
        },
        {
          "id": "MDRW2100040875.1.10",
          "form": "저는 유럽 너무좋앗어서",
          "original_form": "저는 유럽 너무좋앗어서",
          "speaker_id": "2",
          "time": "20210905 10:49"
        },
        {
          "id": "MDRW2100040875.1.11",
          "form": "신행도 갈려구",
          "original_form": "신행도 갈려구",
          "speaker_id": "2",
          "time": "20210905 10:49"
        },
        {
          "id": "MDRW2100040875.1.12",
          "form": "미루고잇어요ㅋㅋㅋㅋ",
          "original_form": "미루고잇어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:49"
        },
        {
          "id": "MDRW2100040875.1.13",
          "form": "그러시군요~~",
          "original_form": "그러시군요~~",
          "speaker_id": "1",
          "time": "20210905 10:51"
        },
        {
          "id": "MDRW2100040875.1.14",
          "form": "예 코시국 끝나는날 다시가겟져",
          "original_form": "예 코시국 끝나는날 다시가겟져",
          "speaker_id": "2",
          "time": "20210905 10:53"
        },
        {
          "id": "MDRW2100040875.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210905 11:05"
        },
        {
          "id": "MDRW2100040875.1.16",
          "form": "어딜 젤 먼저 가고싶으세요",
          "original_form": "어딜 젤 먼저 가고싶으세요",
          "speaker_id": "1",
          "time": "20210905 11:05"
        },
        {
          "id": "MDRW2100040875.1.17",
          "form": "축구보러",
          "original_form": "축구보러",
          "speaker_id": "2",
          "time": "20210905 11:07"
        },
        {
          "id": "MDRW2100040875.1.18",
          "form": "영국 스페인 가고싶어용",
          "original_form": "영국 스페인 가고싶어용",
          "speaker_id": "2",
          "time": "20210905 11:07"
        },
        {
          "id": "MDRW2100040875.1.19",
          "form": "그런날이올지모르겟네요ㅠㅠ",
          "original_form": "그런날이올지모르겟네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 11:08"
        },
        {
          "id": "MDRW2100040875.1.20",
          "form": "축구좋아하시는군요",
          "original_form": "축구좋아하시는군요",
          "speaker_id": "1",
          "time": "20210905 21:16"
        },
        {
          "id": "MDRW2100040875.1.21",
          "form": "진짜요 ㅜ 흐",
          "original_form": "진짜요 ㅜ 흐",
          "speaker_id": "1",
          "time": "20210905 21:16"
        },
        {
          "id": "MDRW2100040875.1.22",
          "form": "스페인 맛난것도 많죠",
          "original_form": "스페인 맛난것도 많죠",
          "speaker_id": "1",
          "time": "20210905 21:16"
        },
        {
          "id": "MDRW2100040875.1.23",
          "form": "남자친구 버킷리스트래요ㅋㅋ",
          "original_form": "남자친구 버킷리스트래요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 09:52"
        },
        {
          "id": "MDRW2100040875.1.24",
          "form": "국내여행은 다녀오신곳잇으신가요",
          "original_form": "국내여행은 다녀오신곳잇으신가요",
          "speaker_id": "2",
          "time": "20210906 09:53"
        },
        {
          "id": "MDRW2100040875.1.25",
          "form": "강릉 좋았어요",
          "original_form": "강릉 좋았어요",
          "speaker_id": "1",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100040875.1.26",
          "form": "강릉 저도 좋더라구요",
          "original_form": "강릉 저도 좋더라구요",
          "speaker_id": "2",
          "time": "20210906 17:41"
        },
        {
          "id": "MDRW2100040875.1.27",
          "form": "강원도 다시가고싶어요",
          "original_form": "강원도 다시가고싶어요",
