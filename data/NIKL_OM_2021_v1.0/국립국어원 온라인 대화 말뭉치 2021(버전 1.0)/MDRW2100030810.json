      "utterance": [
        {
          "id": "MDRW2100030810.1.1",
          "form": "지하수로같은것도 나오는데",
          "original_form": "지하수로같은것도 나오는데",
          "speaker_id": "2",
          "time": "20210904 12:47"
        },
        {
          "id": "MDRW2100030810.1.2",
          "form": "어떻게 했더니",
          "original_form": "어떻게 했더니",
          "speaker_id": "2",
          "time": "20210904 12:47"
        },
        {
          "id": "MDRW2100030810.1.3",
          "form": "그거 막 아래에서 드라이아이스 뿌리는거같았어요",
          "original_form": "그거 막 아래에서 드라이아이스 뿌리는거같았어요",
          "speaker_id": "1",
          "time": "20210904 12:47"
        },
        {
          "id": "MDRW2100030810.1.4",
          "form": "무대도 막 움직이고",
          "original_form": "무대도 막 움직이고",
          "speaker_id": "1",
          "time": "20210904 12:48"
        },
        {
          "id": "MDRW2100030810.1.5",
          "form": "대단하네",
          "original_form": "대단하네",
          "speaker_id": "2",
          "time": "20210904 12:48"
        },
        {
          "id": "MDRW2100030810.1.6",
          "form": "고생 많았겠다",
          "original_form": "고생 많았겠다",
          "speaker_id": "2",
          "time": "20210904 12:48"
        },
        {
          "id": "MDRW2100030810.1.7",
          "form": "그리고 크리스틴 거울같은것도 그냥 장치로 하고",
          "original_form": "그리고 크리스틴 거울같은것도 그냥 장치로 하고",
          "speaker_id": "1",
          "time": "20210904 12:48"
        },
        {
          "id": "MDRW2100030810.1.8",
          "form": "현대과학의 힘을 느꼈달까 ㅎㅎ",
          "original_form": "현대과학의 힘을 느꼈달까 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 12:48"
        },
        {
          "id": "MDRW2100030810.1.9",
          "form": "엄마도 한번 보고싶네",
          "original_form": "엄마도 한번 보고싶네",
          "speaker_id": "2",
          "time": "20210904 12:48"
        },
        {
          "id": "MDRW2100030810.1.10",
          "form": "나중에 보여드릴게요",
          "original_form": "나중에 보여드릴게요",
          "speaker_id": "1",
          "time": "20210904 12:48"
        },
        {
          "id": "MDRW2100030810.1.11",
          "form": "아마 무료공개일거라",
          "original_form": "아마 무료공개일거라",
          "speaker_id": "1",
          "time": "20210904 12:48"
        },
        {
          "id": "MDRW2100030810.1.12",
          "form": "어디서 보니?",
          "original_form": "어디서 보니?",
          "speaker_id": "2",
          "time": "20210904 12:49"
        },
        {
          "id": "MDRW2100030810.1.13",
          "form": "유튜브인가 아니면 btv인가 있었던거같아요",
          "original_form": "유튜브인가 아니면 btv인가 있었던거같아요",
          "speaker_id": "1",
          "time": "20210904 12:49"
        },
        {
          "id": "MDRW2100030810.1.14",
          "form": "25주년인가 40주년인가",
          "original_form": "25주년인가 40주년인가",
          "speaker_id": "1",
          "time": "20210904 12:49"
        },
        {
          "id": "MDRW2100030810.1.15",
          "form": "엄마 그런거 잘 못하니까 와서 해줘",
          "original_form": "엄마 그런거 잘 못하니까 와서 해줘",
          "speaker_id": "2",
          "time": "20210904 12:49"
        },
        {
          "id": "MDRW2100030810.1.16",
          "form": "슬슬 배우실때가 됐는데 ㅠㅋㅋㅋㅋ",
          "original_form": "슬슬 배우실때가 됐는데 ㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:49"
        },
        {
          "id": "MDRW2100030810.1.17",
          "form": "못하겠더라 그런거",
          "original_form": "못하겠더라 그런거",
          "speaker_id": "2",
          "time": "20210904 12:49"
        },
        {
          "id": "MDRW2100030810.1.18",
          "form": "암튼 노래 되게 잘불러서 좋아요",
          "original_form": "암튼 노래 되게 잘불러서 좋아요",
          "speaker_id": "1",
          "time": "20210904 12:50"
        },
        {
          "id": "MDRW2100030810.1.19",
          "form": "저는 think of me 좋아해서",
          "original_form": "저는 think of me 좋아해서",
          "speaker_id": "1",
          "time": "20210904 12:50"
        },
        {
          "id": "MDRW2100030810.1.20",
          "form": "영화에서 다 바뀌는거 멋있지",
          "original_form": "영화에서 다 바뀌는거 멋있지",
          "speaker_id": "2",
          "time": "20210904 12:50"
        },
        {
          "id": "MDRW2100030810.1.21",
          "form": "뮤지컬에서 잘 기억이 안나는데",
          "original_form": "뮤지컬에서 잘 기억이 안나는데",
          "speaker_id": "1",
          "time": "20210904 12:50"
        },
        {
          "id": "MDRW2100030810.1.22",
          "form": "아마 그쪽도 뒤에서 열심히 사람들 돌아다니구",
          "original_form": "아마 그쪽도 뒤에서 열심히 사람들 돌아다니구",
          "speaker_id": "1",
          "time": "20210904 12:51"
        },
        {
          "id": "MDRW2100030810.1.23",
          "form": "그랬던거같아요",
          "original_form": "그랬던거같아요",
          "speaker_id": "1",
          "time": "20210904 12:51"
        },
        {
          "id": "MDRW2100030810.1.24",
          "form": "옷도 갈아입고",
