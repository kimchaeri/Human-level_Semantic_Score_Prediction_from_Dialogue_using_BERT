      "utterance": [
        {
          "id": "MDRW2100025066.1.1",
          "form": "제가 있는 곳은 비가 엄청 오네요",
          "original_form": "제가 있는 곳은 비가 엄청 오네요",
          "speaker_id": "1",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100025066.1.2",
          "form": "여기도 비가 조금 왔다가 지금은 그쳤어요!",
          "original_form": "여기도 비가 조금 왔다가 지금은 그쳤어요!",
          "speaker_id": "2",
          "time": "20210901 08:58"
        },
        {
          "id": "MDRW2100025066.1.3",
          "form": "오늘은 어떤 옷을 입으셨나요?",
          "original_form": "오늘은 어떤 옷을 입으셨나요?",
          "speaker_id": "2",
          "time": "20210901 08:58"
        },
        {
          "id": "MDRW2100025066.1.4",
          "form": "긴팔에 반바지요",
          "original_form": "긴팔에 반바지요",
          "speaker_id": "1",
          "time": "20210901 09:24"
        },
        {
          "id": "MDRW2100025066.1.5",
          "form": "좀 쌀쌀한데 비가 또 올수 있으니까요",
          "original_form": "좀 쌀쌀한데 비가 또 올수 있으니까요",
          "speaker_id": "1",
          "time": "20210901 09:24"
        },
        {
          "id": "MDRW2100025066.1.6",
          "form": "맞아요 오늘은 바람도 잘 불고 날이 쌀쌀하네요",
          "original_form": "맞아요 오늘은 바람도 잘 불고 날이 쌀쌀하네요",
          "speaker_id": "2",
          "time": "20210901 09:31"
        },
        {
          "id": "MDRW2100025066.1.7",
          "form": "가을같아요 ㅎㅎ",
          "original_form": "가을같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 09:32"
        },
        {
          "id": "MDRW2100025066.1.8",
          "form": "이동할때 비가 안오면 좋겠네요!",
          "original_form": "이동할때 비가 안오면 좋겠네요!",
          "speaker_id": "2",
          "time": "20210901 09:32"
        },
        {
          "id": "MDRW2100025066.1.9",
          "form": "네, 아직은 안오네요",
          "original_form": "네, 아직은 안오네요",
          "speaker_id": "1",
          "time": "20210901 09:53"
        },
        {
          "id": "MDRW2100025066.1.10",
          "form": "중간엔 와도 되니까 퇴근길에만 오지 마라~",
          "original_form": "중간엔 와도 되니까 퇴근길에만 오지 마라~",
          "speaker_id": "1",
          "time": "20210901 09:54"
        },
        {
          "id": "MDRW2100025066.1.11",
          "form": "그러게요 퇴근길에는 비가 안와야 할텐데 말이에요!",
          "original_form": "그러게요 퇴근길에는 비가 안와야 할텐데 말이에요!",
          "speaker_id": "2",
          "time": "20210901 15:43"
        },
        {
          "id": "MDRW2100025066.1.12",
          "form": "집안에서만 있을때 비가 오는게 꽤 보기좋다고 생각했는데 밖이면 곤란할때가 많죠 ㅎㅎ",
          "original_form": "집안에서만 있을때 비가 오는게 꽤 보기좋다고 생각했는데 밖이면 곤란할때가 많죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 15:44"
        },
        {
          "id": "MDRW2100025066.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100025066.1.14",
          "form": "저 어제 퇴근하는데 신발에 물들어가서...ㅠㅠ",
          "original_form": "저 어제 퇴근하는데 신발에 물들어가서...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100025066.1.15",
          "form": "헉 어제도 비가 왔었군요 그럼 너무 불쾌하죠",
          "original_form": "헉 어제도 비가 왔었군요 그럼 너무 불쾌하죠",
          "speaker_id": "2",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100025066.1.16",
          "form": "고생하셨겠어요 ㅠ",
          "original_form": "고생하셨겠어요 ㅠ",
          "speaker_id": "2",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100025066.1.17",
          "form": "그래도 요즘 미세먼지 없어서 좋아했는데 말이에요",
          "original_form": "그래도 요즘 미세먼지 없어서 좋아했는데 말이에요",
          "speaker_id": "2",
          "time": "20210901 15:52"
        },
        {
          "id": "MDRW2100025066.1.18",
          "form": "하... 미세먼지 없는건 참좋죠",
          "original_form": "하... 미세먼지 없는건 참좋죠",
          "speaker_id": "1",
          "time": "20210901 15:55"
        },
        {
          "id": "MDRW2100025066.1.19",
          "form": "근데 마스크를 써야하는ㅠ",
          "original_form": "근데 마스크를 써야하는ㅠ",
          "speaker_id": "1",
          "time": "20210901 15:55"
        },
        {
          "id": "MDRW2100025066.1.20",
          "form": "맞아요 ㅎㅎ 미세먼지가 없어도 마스크를 써야 하네요..",
          "original_form": "맞아요 ㅎㅎ 미세먼지가 없어도 마스크를 써야 하네요..",
          "speaker_id": "2",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100025066.1.21",
          "form": "ㅠㅠ 내일은 날씨가 어떨지 모르겠어요",
          "original_form": "ㅠㅠ 내일은 날씨가 어떨지 모르겠어요",
          "speaker_id": "2",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100025066.1.22",
          "form": "어느새 9월이네요!",
          "original_form": "어느새 9월이네요!",
          "speaker_id": "2",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100025066.1.23",
          "form": "네 시간이 어쩜 이리 빠른지",
          "original_form": "네 시간이 어쩜 이리 빠른지",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100025066.1.24",
          "form": "그러게요 날이 쌀쌀해지고 9월이 맞는것 같아요",
          "original_form": "그러게요 날이 쌀쌀해지고 9월이 맞는것 같아요",
          "speaker_id": "2",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100025066.1.25",
          "form": "아직 비가 오나요?여긴 그쳤어요!",
          "original_form": "아직 비가 오나요?여긴 그쳤어요!",
          "speaker_id": "2",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100025066.1.26",
          "form": "여기도 그쳤어요",
          "original_form": "여기도 그쳤어요",
          "speaker_id": "1",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100025066.1.27",
          "form": "다행이에요. 지금 퇴근중이거든요",
          "original_form": "다행이에요. 지금 퇴근중이거든요",
          "speaker_id": "1",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100025066.1.28",
          "form": "다행이네요!",
          "original_form": "다행이네요!",
          "speaker_id": "2",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100025066.1.29",
          "form": "여기는 마른벼락이 굉장히 많이 치고있어요 기상이변이네요 ㅠ",
          "original_form": "여기는 마른벼락이 굉장히 많이 치고있어요 기상이변이네요 ㅠ",
          "speaker_id": "2",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100025066.1.30",
          "form": "헐? 마른 벼락이요?",
          "original_form": "헐? 마른 벼락이요?",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100025066.1.31",
          "form": "무섭네요.ㄷㄷ",
          "original_form": "무섭네요.ㄷㄷ",
          "speaker_id": "1",
          "time": "20210901 22:27"
        }
      ]
