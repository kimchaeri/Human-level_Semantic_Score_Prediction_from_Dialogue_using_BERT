      "utterance": [
        {
          "id": "MDRW2100025993.1.1",
          "form": "최근에 공연보신적있으세요?",
          "original_form": "최근에 공연보신적있으세요?",
          "speaker_id": "1",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100025993.1.2",
          "form": "아~ 코로나때문에 못봤죠..",
          "original_form": "아~ 코로나때문에 못봤죠..",
          "speaker_id": "2",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100025993.1.3",
          "form": "대신 전시회는 다녀왔네요",
          "original_form": "대신 전시회는 다녀왔네요",
          "speaker_id": "2",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100025993.1.4",
          "form": "네 그렇죠ㅜ",
          "original_form": "네 그렇죠ㅜ",
          "speaker_id": "1",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100025993.1.5",
          "form": "무민을 좋아해서",
          "original_form": "무민을 좋아해서",
          "speaker_id": "2",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100025993.1.6",
          "form": "저도 코로나때문에 잘못보네요ㅜ",
          "original_form": "저도 코로나때문에 잘못보네요ㅜ",
          "speaker_id": "1",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100025993.1.7",
          "form": "무민 원화 전시회를 다녀왔는데",
          "original_form": "무민 원화 전시회를 다녀왔는데",
          "speaker_id": "2",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100025993.1.8",
          "form": "아 무민ㅋㅋ",
          "original_form": "아 무민ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100025993.1.9",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100025993.1.10",
          "form": "너무 좋았어서 한번 더 가려고 예매해놨어요ㅎㅎ",
          "original_form": "너무 좋았어서 한번 더 가려고 예매해놨어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100025993.1.11",
          "form": "어떠셨어요?",
          "original_form": "어떠셨어요?",
          "speaker_id": "1",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100025993.1.12",
          "form": "아 그렇구나ㅋㅋ",
          "original_form": "아 그렇구나ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100025993.1.13",
          "form": "진짜 좋으셨나보네요",
          "original_form": "진짜 좋으셨나보네요",
          "speaker_id": "1",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100025993.1.14",
          "form": "그냥 인터넷에서 볼 수 있는 그림만",
          "original_form": "그냥 인터넷에서 볼 수 있는 그림만",
          "speaker_id": "2",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100025993.1.15",
          "form": "있을 줄 알았는데",
          "original_form": "있을 줄 알았는데",
          "speaker_id": "2",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100025993.1.16",
          "form": "원화가 생각보다 너무 좋았고",
          "original_form": "원화가 생각보다 너무 좋았고",
          "speaker_id": "2",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100025993.1.17",
          "form": "네네 ㅋㅋ",
          "original_form": "네네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100025993.1.18",
          "form": "전시장 연출도 잘 해뒀더라고요",
          "original_form": "전시장 연출도 잘 해뒀더라고요",
          "speaker_id": "2",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100025993.1.19",
          "form": "보시고싶은",
          "original_form": "보시고싶은",
          "speaker_id": "2",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100025993.1.20",
          "form": "공연은 있으세요?",
          "original_form": "공연은 있으세요?",
          "speaker_id": "2",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100025993.1.21",
          "form": "무민 귀엽죠ㅋㅋ",
          "original_form": "무민 귀엽죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100025993.1.22",
          "form": "저도 한번 가고싶네요ㅋㅋ",
          "original_form": "저도 한번 가고싶네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100025993.1.23",
          "form": "네ㅋㅋㅋ귀여워요",
          "original_form": "네ㅋㅋㅋ귀여워요",
          "speaker_id": "2",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100025993.1.24",
          "form": "기회되면 가보세요",
          "original_form": "기회되면 가보세요",
          "speaker_id": "2",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100025993.1.25",
          "form": "생각보다 정말 잘 해놨어요",
          "original_form": "생각보다 정말 잘 해놨어요",
          "speaker_id": "2",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100025993.1.26",
          "form": "저는 뮤지컬이나 콘서트보러가고싶은데",
          "original_form": "저는 뮤지컬이나 콘서트보러가고싶은데",
          "speaker_id": "1",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100025993.1.27",
          "form": "못가서 아쉬워요ㅜㅋㅋ",
          "original_form": "못가서 아쉬워요ㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100025993.1.28",
          "form": "나중에",
          "original_form": "나중에",
          "speaker_id": "1",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100025993.1.29",
          "form": "아 요즘 보기 힘든 종류죠ㅠㅠ",
          "original_form": "아 요즘 보기 힘든 종류죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100025993.1.30",
          "form": "기회되면",
          "original_form": "기회되면",
          "speaker_id": "1",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100025993.1.31",
          "form": "가봐야겠네요ㅋㄱ",
          "original_form": "가봐야겠네요ㅋㄱ",
          "speaker_id": "1",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100025993.1.32",
          "form": "네ㅎㅎ",
          "original_form": "네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100025993.1.33",
          "form": "애들도 좋아할것같아요.ㅋ",
          "original_form": "애들도 좋아할것같아요.ㅋ",
          "speaker_id": "1",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100025993.1.34",
          "form": "아이들이 어리면 정말 좋아할거예요",
          "original_form": "아이들이 어리면 정말 좋아할거예요",
          "speaker_id": "2",
          "time": "20210901 18:40"
        }
      ]
    }
