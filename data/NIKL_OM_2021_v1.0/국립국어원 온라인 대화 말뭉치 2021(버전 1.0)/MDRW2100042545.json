      "utterance": [
        {
          "id": "MDRW2100042545.1.1",
          "form": "네 럼청 오네요",
          "original_form": "네 럼청 오네요",
          "speaker_id": "2",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100042545.1.2",
          "form": "제가 사는 곳도요ㅠㅠ",
          "original_form": "제가 사는 곳도요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100042545.1.3",
          "form": "비가와서 그런가 춥더라고요ㅜ",
          "original_form": "비가와서 그런가 춥더라고요ㅜ",
          "speaker_id": "1",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100042545.1.4",
          "form": "네 벌써 가을인가봐요",
          "original_form": "네 벌써 가을인가봐요",
          "speaker_id": "2",
          "time": "20210906 22:48"
        },
        {
          "id": "MDRW2100042545.1.5",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210906 22:48"
        },
        {
          "id": "MDRW2100042545.1.6",
          "form": "그래서 너무 좋아요!!!",
          "original_form": "그래서 너무 좋아요!!!",
          "speaker_id": "1",
          "time": "20210906 22:48"
        },
        {
          "id": "MDRW2100042545.1.7",
          "form": "네  가을 좋아하시나요",
          "original_form": "네  가을 좋아하시나요",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042545.1.8",
          "form": "네 가을 넘 좋아해요~~~~~!",
          "original_form": "네 가을 넘 좋아해요~~~~~!",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042545.1.9",
          "form": "시원한 바람이 좋지 않나요?",
          "original_form": "시원한 바람이 좋지 않나요?",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042545.1.10",
          "form": "맞아요.시원한 바람 좋죠",
          "original_form": "맞아요.시원한 바람 좋죠",
          "speaker_id": "2",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042545.1.11",
          "form": "그쵸? 저는 그래서 가을이 제일 좋아요!",
          "original_form": "그쵸? 저는 그래서 가을이 제일 좋아요!",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042545.1.12",
          "form": "단풍도 좋고요",
          "original_form": "단풍도 좋고요",
          "speaker_id": "2",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042545.1.13",
          "form": "맞아요~~! 저는 더위에 약해서 여름이 너무 싫고 가을이 좋더라고요!",
          "original_form": "맞아요~~! 저는 더위에 약해서 여름이 너무 싫고 가을이 좋더라고요!",
          "speaker_id": "1",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042545.1.14",
          "form": "가을만릐 색채도 좋아요",
          "original_form": "가을만릐 색채도 좋아요",
          "speaker_id": "2",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042545.1.15",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042545.1.16",
          "form": "그 특유의 빨강 주황색이 가득한 풍경이 좋죠",
          "original_form": "그 특유의 빨강 주황색이 가득한 풍경이 좋죠",
          "speaker_id": "1",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042545.1.17",
          "form": "ㅎ.ㅎ",
          "original_form": "ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042545.1.18",
          "form": "울긋불긋한 색채가 너무 곱고이뻐료",
          "original_form": "울긋불긋한 색채가 너무 곱고이뻐료",
          "speaker_id": "2",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042545.1.19",
          "form": "맞아요~~~!",
          "original_form": "맞아요~~~!",
          "speaker_id": "1",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042545.1.20",
          "form": "그럼 다른 계절도 좋아하시나요?",
          "original_form": "그럼 다른 계절도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042545.1.21",
          "form": "봄이나 여름 겨울중에요!",
          "original_form": "봄이나 여름 겨울중에요!",
          "speaker_id": "1",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042545.1.22",
          "form": "음 겨울이료.",
          "original_form": "음 겨울이료.",
          "speaker_id": "2",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042545.1.23",
          "form": "겨울 좋죠ㅠ",
          "original_form": "겨울 좋죠ㅠ",
          "speaker_id": "1",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042545.1.24",
          "form": "저는 겨울옷이 좋아요!",
          "original_form": "저는 겨울옷이 좋아요!",
          "speaker_id": "1",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042545.1.25",
          "form": "더운게 없는 것도 좋고 벌레가 없는 것도 좋아요!!",
          "original_form": "더운게 없는 것도 좋고 벌레가 없는 것도 좋아요!!",
          "speaker_id": "1",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042545.1.26",
          "form": "하얀 눈이 소복히 내린 산리 아뻐 보여서요",
          "original_form": "하얀 눈이 소복히 내린 산리 아뻐 보여서요",
          "speaker_id": "2",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042545.1.27",
          "form": "맞아요! ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요! ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042545.1.28",
          "form": "저는 그 겨울에 눈 오는 것도 넘 좋아요!",
          "original_form": "저는 그 겨울에 눈 오는 것도 넘 좋아요!",
