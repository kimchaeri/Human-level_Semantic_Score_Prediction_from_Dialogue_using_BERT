      "utterance": [
        {
          "id": "MDRW2100017621.1.1",
          "form": "쇼핑 좋아하세요?",
          "original_form": "쇼핑 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100017621.1.2",
          "form": "돈쓰는건 다 재밌어요 ㅋㅋㅋㅋ",
          "original_form": "돈쓰는건 다 재밌어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100017621.1.3",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100017621.1.4",
          "form": "쇼핑 최고죠 ㅋㅋ",
          "original_form": "쇼핑 최고죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:24"
        },
        {
          "id": "MDRW2100017621.1.5",
          "form": "주로 쇼핑 어디서 하세요?",
          "original_form": "주로 쇼핑 어디서 하세요?",
          "speaker_id": "1",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017621.1.6",
          "form": "저는 인터넷에서 거의 다 사요 ㅋㅋㅋ",
          "original_form": "저는 인터넷에서 거의 다 사요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017621.1.7",
          "form": "요즘 인터넷에 사는 게 최고인 것 같아요",
          "original_form": "요즘 인터넷에 사는 게 최고인 것 같아요",
          "speaker_id": "1",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100017621.1.8",
          "form": "한국 배달이랑 택배 진짜 빨라요",
          "original_form": "한국 배달이랑 택배 진짜 빨라요",
          "speaker_id": "1",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100017621.1.9",
          "form": "맞아요 저렴하고 빨라서요",
          "original_form": "맞아요 저렴하고 빨라서요",
          "speaker_id": "2",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100017621.1.10",
          "form": "요즘 생수도 무료로 배달해주니 편리해졌어요~",
          "original_form": "요즘 생수도 무료로 배달해주니 편리해졌어요~",
          "speaker_id": "1",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100017621.1.11",
          "form": "맞아요 저도 생수 먹는데 편해요",
          "original_form": "맞아요 저도 생수 먹는데 편해요",
          "speaker_id": "2",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017621.1.12",
          "form": "요즘 생수 논란 있는 거 보셨어요?",
          "original_form": "요즘 생수 논란 있는 거 보셨어요?",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100017621.1.13",
          "form": "봤어요 ㅠㅠ 동원먹는데 다행히 문제인 수원지는 아니더라구요 제가 받은 거는",
          "original_form": "봤어요 ㅠㅠ 동원먹는데 다행히 문제인 수원지는 아니더라구요 제가 받은 거는",
          "speaker_id": "2",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017621.1.14",
          "form": "동원샘물이요? 다행이네요 ㅜㅜ",
          "original_form": "동원샘물이요? 다행이네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017621.1.15",
          "form": "네 근데 나중에 뭘먹어야할지 ㅠㅠ",
          "original_form": "네 근데 나중에 뭘먹어야할지 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017621.1.16",
          "form": "저 그래서 어제 생수 주문할 때 수원지 잘보고 주문했어요",
          "original_form": "저 그래서 어제 생수 주문할 때 수원지 잘보고 주문했어요",
          "speaker_id": "1",
          "time": "20210826 20:34"
        }
