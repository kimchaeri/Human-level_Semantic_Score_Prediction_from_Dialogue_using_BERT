      "utterance": [
        {
          "id": "MDRW2100011448.1.1",
          "form": "언니 우리집 애벌레 번데기 된거 봤나??",
          "original_form": "언니 우리집 애벌레 번데기 된거 봤나??",
          "speaker_id": "1",
          "time": "20210824 12:51"
        },
        {
          "id": "MDRW2100011448.1.2",
          "form": "봤다ㅋ 우리집 구피도 잘크고있음",
          "original_form": "봤다ㅋ 우리집 구피도 잘크고있음",
          "speaker_id": "2",
          "time": "20210824 13:59"
        },
        {
          "id": "MDRW2100011448.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ구피",
          "original_form": "ㅋㅋㅋㅋㅋㅋ구피",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011448.1.4",
          "form": "고양이 키우고 싶구만요",
          "original_form": "고양이 키우고 싶구만요",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011448.1.5",
          "form": "요새 문뜩 또",
          "original_form": "요새 문뜩 또",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011448.1.6",
          "form": "번데기를 바라보고 있는 투를 보니",
          "original_form": "번데기를 바라보고 있는 투를 보니",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011448.1.7",
          "form": "양이 생각이....",
          "original_form": "양이 생각이....",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011448.1.8",
          "form": "우리도 고양이 보고있잖아",
          "original_form": "우리도 고양이 보고있잖아",
          "speaker_id": "2",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011448.1.9",
          "form": "러시안블루남매ㅜㅜ",
          "original_form": "러시안블루남매ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100011448.1.10",
          "form": "울신량 봄이 산이 강이 보고싶다고ㄱㅋ",
          "original_form": "울신량 봄이 산이 강이 보고싶다고ㄱㅋ",
          "speaker_id": "2",
          "time": "20210824 14:50"
        },
        {
          "id": "MDRW2100011448.1.11",
          "form": "봄이 산이 강이가 누군데",
          "original_form": "봄이 산이 강이가 누군데",
          "speaker_id": "1",
          "time": "20210824 15:54"
        },
        {
          "id": "MDRW2100011448.1.12",
          "form": "고양이 친구들이름ㅡㅡ",
          "original_form": "고양이 친구들이름ㅡㅡ",
          "speaker_id": "2",
          "time": "20210824 16:01"
        },
        {
          "id": "MDRW2100011448.1.13",
          "form": "아하 고양이 이름 ㅎㅎ",
          "original_form": "아하 고양이 이름 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 17:24"
        },
        {
          "id": "MDRW2100011448.1.14",
          "form": "거기 고양이까페??",
          "original_form": "거기 고양이까페??",
          "speaker_id": "1",
          "time": "20210824 17:24"
        },
        {
          "id": "MDRW2100011448.1.15",
          "form": "언니네 고양이 입양하면 보러가야겠다",
          "original_form": "언니네 고양이 입양하면 보러가야겠다",
          "speaker_id": "1",
          "time": "20210824 17:24"
        },
        {
          "id": "MDRW2100011448.1.16",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100011448.1.17",
          "form": "입양 하고싶다만ㅋ",
          "original_form": "입양 하고싶다만ㅋ",
          "speaker_id": "2",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100011448.1.18",
          "form": "고양이 구피 헉",
          "original_form": "고양이 구피 헉",
          "speaker_id": "2",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100011448.1.19",
          "form": "ㅋㅋㅋㅋㄱ 고양이가 구피를 구경하고 있겠군",
          "original_form": "ㅋㅋㅋㅋㄱ 고양이가 구피를 구경하고 있겠군",
          "speaker_id": "1",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100011448.1.20",
          "form": "입양해요",
          "original_form": "입양해요",
          "speaker_id": "1",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100011448.1.21",
          "form": "ㅋㅋ 쉽지않군ㅋ",
          "original_form": "ㅋㅋ 쉽지않군ㅋ",
          "speaker_id": "2",
          "time": "20210824 18:12"
        },
        {
          "id": "MDRW2100011448.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱ 빠른 입양 기대하겠음",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱ 빠른 입양 기대하겠음",
          "speaker_id": "1",
          "time": "20210824 20:54"
        },
        {
          "id": "MDRW2100011448.1.23",
          "form": "",
          "original_form": "",
          "speaker_id": "2",
          "time": "20210824 21:07"
        },
        {
          "id": "MDRW2100011448.1.24",
          "form": "말을 하소 ㅋㅋ",
          "original_form": "말을 하소 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:06"
        },
        {
          "id": "MDRW2100011448.1.25",
          "form": "입양 기다린다니까??",
          "original_form": "입양 기다린다니까??",
          "speaker_id": "1",
          "time": "20210824 22:06"
        },
        {
          "id": "MDRW2100011448.1.26",
          "form": "우리 투가 매일 방문 할 예정",
          "original_form": "우리 투가 매일 방문 할 예정",
          "speaker_id": "1",
          "time": "20210824 22:06"
        },
        {
          "id": "MDRW2100011448.1.27",
          "form": "아하 그렇구만",
          "original_form": "아하 그렇구만",
          "speaker_id": "2",
          "time": "20210824 22:43"
        },
        {
          "id": "MDRW2100011448.1.28",
          "form": "언제 하겠노",
          "original_form": "언제 하겠노",
          "speaker_id": "2",
          "time": "20210824 22:43"
        },
        {
          "id": "MDRW2100011448.1.29",
          "form": "일단 구피 먼저 잘 키우겠다",
          "original_form": "일단 구피 먼저 잘 키우겠다",
          "speaker_id": "2",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100011448.1.30",
          "form": "고양이는 안 되겠군",
          "original_form": "고양이는 안 되겠군",
          "speaker_id": "1",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100011448.1.31",
          "form": "우리 투 초등학생 되면 같이 생각해 봅시다",
          "original_form": "우리 투 초등학생 되면 같이 생각해 봅시다",
          "speaker_id": "1",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100011448.1.32",
          "form": "근데 형부도 고양이 윽시 좋아하는가보네",
          "original_form": "근데 형부도 고양이 윽시 좋아하는가보네",
          "speaker_id": "1",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100011448.1.33",
          "form": "좋아하지ㅋㅋㅋ",
          "original_form": "좋아하지ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100011448.1.34",
          "form": "그럼 키우자 ㅋㅋㅋㅋ",
          "original_form": "그럼 키우자 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:52"
        }
      ]
