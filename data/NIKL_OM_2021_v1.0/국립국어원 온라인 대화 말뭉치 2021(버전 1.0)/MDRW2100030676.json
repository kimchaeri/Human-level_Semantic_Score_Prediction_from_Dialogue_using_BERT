      "utterance": [
        {
          "id": "MDRW2100030676.1.1",
          "form": "전 티비 자주 봐요",
          "original_form": "전 티비 자주 봐요",
          "speaker_id": "2",
          "time": "20210904 11:18"
        },
        {
          "id": "MDRW2100030676.1.2",
          "form": "요즘 시청률 안 나올텐데",
          "original_form": "요즘 시청률 안 나올텐데",
          "speaker_id": "1",
          "time": "20210904 11:20"
        },
        {
          "id": "MDRW2100030676.1.3",
          "form": "기다리는 맛이 있어요",
          "original_form": "기다리는 맛이 있어요",
          "speaker_id": "2",
          "time": "20210904 11:21"
        },
        {
          "id": "MDRW2100030676.1.4",
          "form": "본방사수하시나봐요",
          "original_form": "본방사수하시나봐요",
          "speaker_id": "1",
          "time": "20210904 11:22"
        },
        {
          "id": "MDRW2100030676.1.5",
          "form": "넹 조아하는 프로요",
          "original_form": "넹 조아하는 프로요",
          "speaker_id": "2",
          "time": "20210904 11:22"
        },
        {
          "id": "MDRW2100030676.1.6",
          "form": "뭐좋아하시나여",
          "original_form": "뭐좋아하시나여",
          "speaker_id": "1",
          "time": "20210904 11:22"
        },
        {
          "id": "MDRW2100030676.1.7",
          "form": "주로 예능이요",
          "original_form": "주로 예능이요",
          "speaker_id": "2",
          "time": "20210904 11:23"
        },
        {
          "id": "MDRW2100030676.1.8",
          "form": "전 나혼산 편스토랑 이런거 재미없어요",
          "original_form": "전 나혼산 편스토랑 이런거 재미없어요",
          "speaker_id": "1",
          "time": "20210904 11:23"
        },
        {
          "id": "MDRW2100030676.1.9",
          "form": "노잼",
          "original_form": "노잼",
          "speaker_id": "1",
          "time": "20210904 11:23"
        },
        {
          "id": "MDRW2100030676.1.10",
          "form": "유툽은 뭐가 재밌나요",
          "original_form": "유툽은 뭐가 재밌나요",
          "speaker_id": "2",
          "time": "20210904 11:23"
        },
        {
          "id": "MDRW2100030676.1.11",
          "form": "삼프로요",
          "original_form": "삼프로요",
          "speaker_id": "1",
          "time": "20210904 11:24"
        },
        {
          "id": "MDRW2100030676.1.12",
          "form": "전본방으로보는게 좋더라구요",
          "original_form": "전본방으로보는게 좋더라구요",
          "speaker_id": "2",
          "time": "20210904 11:24"
        },
        {
          "id": "MDRW2100030676.1.13",
          "form": "네 한가하신가바여",
          "original_form": "네 한가하신가바여",
          "speaker_id": "1",
          "time": "20210904 11:25"
        },
        {
          "id": "MDRW2100030676.1.14",
          "form": "삼프로 저도 봐봤어요",
          "original_form": "삼프로 저도 봐봤어요",
          "speaker_id": "2",
          "time": "20210904 11:25"
        },
        {
          "id": "MDRW2100030676.1.15",
          "form": "헉 아시나\u0000여",
          "original_form": "헉 아시나\u0000여",
          "speaker_id": "1",
          "time": "20210904 11:25"
        },
        {
          "id": "MDRW2100030676.1.16",
          "form": "넹 지인이 보아서요",
          "original_form": "넹 지인이 보아서요",
          "speaker_id": "2",
          "time": "20210904 11:26"
        },
        {
          "id": "MDRW2100030676.1.17",
          "form": "글쿤요",
          "original_form": "글쿤요",
          "speaker_id": "1",
          "time": "20210904 11:31"
        }
      ]
