      "utterance": [
        {
          "id": "MDRW2100027871.1.1",
          "form": "스포츠는 좋아하시나요??",
          "original_form": "스포츠는 좋아하시나요??",
          "speaker_id": "2",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100027871.1.2",
          "form": "죄송해요 정말 잘 모르구 안봐서 ㅠ",
          "original_form": "죄송해요 정말 잘 모르구 안봐서 ㅠ",
          "speaker_id": "1",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100027871.1.3",
          "form": "저도 스포츠 별로 안좋아해요ㅎㅎ",
          "original_form": "저도 스포츠 별로 안좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100027871.1.4",
          "form": "월드컵조차..",
          "original_form": "월드컵조차..",
          "speaker_id": "2",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100027871.1.5",
          "form": "오 싫은 걸 공유해봐요 ㅎㅎ",
          "original_form": "오 싫은 걸 공유해봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100027871.1.6",
          "form": "그냥 야구 축구 다... 별로..",
          "original_form": "그냥 야구 축구 다... 별로..",
          "speaker_id": "2",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100027871.1.7",
          "form": "특히 야구 싫어요",
          "original_form": "특히 야구 싫어요",
          "speaker_id": "2",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100027871.1.8",
          "form": "편가르는거 ㅎㅎ",
          "original_form": "편가르는거 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100027871.1.9",
          "form": "응원하면서 술먹고 ㅎㅎ",
          "original_form": "응원하면서 술먹고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100027871.1.10",
          "form": "우선.. 야구선수들 사생활이..싫어요",
          "original_form": "우선.. 야구선수들 사생활이..싫어요",
          "speaker_id": "2",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100027871.1.11",
          "form": "아하 최근 문제가 되었었죠",
          "original_form": "아하 최근 문제가 되었었죠",
          "speaker_id": "1",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100027871.1.12",
          "form": "아직도 무관중 관람인가여",
          "original_form": "아직도 무관중 관람인가여",
          "speaker_id": "1",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100027871.1.13",
          "form": "오늘 단계조정하는거같던데 관람관련해서는 이야기없더라고요",
          "original_form": "오늘 단계조정하는거같던데 관람관련해서는 이야기없더라고요",
          "speaker_id": "2",
          "time": "20210906 21:52"
        },
        {
          "id": "MDRW2100027871.1.14",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "1",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100027871.1.15",
          "form": "육상이나 양궁은 좋아요",
          "original_form": "육상이나 양궁은 좋아요",
          "speaker_id": "2",
          "time": "20210906 21:55"
        },
        {
          "id": "MDRW2100027871.1.16",
          "form": "올림픽 보셨어요?",
          "original_form": "올림픽 보셨어요?",
          "speaker_id": "2",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100027871.1.17",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210907 08:46"
        },
        {
          "id": "MDRW2100027871.1.18",
          "form": "배구는 봤어요",
          "original_form": "배구는 봤어요",
          "speaker_id": "1",
          "time": "20210907 08:46"
        },
        {
          "id": "MDRW2100027871.1.19",
