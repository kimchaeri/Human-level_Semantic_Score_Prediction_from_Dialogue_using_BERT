      "utterance": [
        {
          "id": "MDRW2100007626.1.1",
          "form": "일해요?",
          "original_form": "일해요?",
          "speaker_id": "1",
          "time": "20210822 08:40"
        },
        {
          "id": "MDRW2100007626.1.2",
          "form": "가끔 집에서 부업만 하죠!",
          "original_form": "가끔 집에서 부업만 하죠!",
          "speaker_id": "2",
          "time": "20210822 11:24"
        },
        {
          "id": "MDRW2100007626.1.3",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210822 11:38"
        },
        {
          "id": "MDRW2100007626.1.4",
          "form": "기프티콘을 받아서 파는 그런 부업이요!",
          "original_form": "기프티콘을 받아서 파는 그런 부업이요!",
          "speaker_id": "2",
          "time": "20210822 11:56"
        },
        {
          "id": "MDRW2100007626.1.5",
          "form": "오 돈되나요?",
          "original_form": "오 돈되나요?",
          "speaker_id": "1",
          "time": "20210822 12:03"
        },
        {
          "id": "MDRW2100007626.1.6",
          "form": "네 돈 좀 되긴하더라고요!",
          "original_form": "네 돈 좀 되긴하더라고요!",
          "speaker_id": "2",
          "time": "20210822 12:21"
        },
        {
          "id": "MDRW2100007626.1.7",
          "form": "기프티콘 싸지 않나요?",
          "original_form": "기프티콘 싸지 않나요?",
          "speaker_id": "1",
          "time": "20210822 12:23"
        },
        {
          "id": "MDRW2100007626.1.8",
          "form": "싼데 그거를 좀 많이 팔면 나가긴하죠",
          "original_form": "싼데 그거를 좀 많이 팔면 나가긴하죠",
          "speaker_id": "2",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007626.1.9",
          "form": "박리다매네요",
          "original_form": "박리다매네요",
          "speaker_id": "1",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007626.1.10",
          "form": "어떻게 알아보셨어요?",
          "original_form": "어떻게 알아보셨어요?",
          "speaker_id": "1",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007626.1.11",
          "form": "처음엔 지인이 하자고 해서 했었는데",
          "original_form": "처음엔 지인이 하자고 해서 했었는데",
          "speaker_id": "2",
          "time": "20210822 12:32"
        },
        {
          "id": "MDRW2100007626.1.12",
          "form": "지금은 저혼자서 이제 하고있어요",
          "original_form": "지금은 저혼자서 이제 하고있어요",
          "speaker_id": "2",
          "time": "20210822 12:32"
        },
        {
          "id": "MDRW2100007626.1.13",
          "form": "한지는 2년정도 된거같네요",
          "original_form": "한지는 2년정도 된거같네요",
          "speaker_id": "2",
          "time": "20210822 12:32"
        },
        {
          "id": "MDRW2100007626.1.14",
          "form": "대단하네요",
          "original_form": "대단하네요",
          "speaker_id": "1",
          "time": "20210822 12:46"
        },
        {
          "id": "MDRW2100007626.1.15",
          "form": "꾸준하게 하니 돈이 좀 벌리긴하더라고요",
          "original_form": "꾸준하게 하니 돈이 좀 벌리긴하더라고요",
          "speaker_id": "2",
          "time": "20210822 12:48"
        },
        {
          "id": "MDRW2100007626.1.16",
          "form": "그래서 그거 때문인지 하게 되네요 ㅎㅎ",
          "original_form": "그래서 그거 때문인지 하게 되네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:48"
        },
        {
          "id": "MDRW2100007626.1.17",
          "form": "저도 알아봐야겠어요",
          "original_form": "저도 알아봐야겠어요",
          "speaker_id": "1",
          "time": "20210822 12:50"
        },
        {
          "id": "MDRW2100007626.1.18",
          "form": "좀 힘들지만 노하우가 생기면 괜찮을거에요!",
          "original_form": "좀 힘들지만 노하우가 생기면 괜찮을거에요!",
          "speaker_id": "2",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100007626.1.19",
          "form": "시작이 힘들죠",
          "original_form": "시작이 힘들죠",
          "speaker_id": "1",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100007626.1.20",
          "form": "그쵸 저도",
          "original_form": "그쵸 저도",
          "speaker_id": "2",
          "time": "20210822 13:17"
        },
        {
          "id": "MDRW2100007626.1.21",
          "form": "처음에 시작이 엄청 힘들었어요 ㅠㅠ",
          "original_form": "처음에 시작이 엄청 힘들었어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 13:17"
        }
      ]
