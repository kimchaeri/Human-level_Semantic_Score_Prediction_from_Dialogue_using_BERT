      "utterance": [
        {
          "id": "MDRW2100021584.1.1",
          "form": "집안은 화목한 편이세요?",
          "original_form": "집안은 화목한 편이세요?",
          "speaker_id": "1",
          "time": "20210828 20:38"
        },
        {
          "id": "MDRW2100021584.1.2",
          "form": "네 부모님과 삼남매 강아지 요렇게 여섯강아지에요",
          "original_form": "네 부모님과 삼남매 강아지 요렇게 여섯강아지에요",
          "speaker_id": "2",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021584.1.3",
          "form": "잉 여섯가족이에요 ㅋㅋㅌㅌ",
          "original_form": "잉 여섯가족이에요 ㅋㅋㅌㅌ",
          "speaker_id": "2",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021584.1.4",
          "form": "어머 가족구성원이 엄청 많네요",
          "original_form": "어머 가족구성원이 엄청 많네요",
          "speaker_id": "1",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021584.1.5",
          "form": "강아지가 젤 행복하겠어요 ㅎㅎ",
          "original_form": "강아지가 젤 행복하겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021584.1.6",
          "form": "다들 너무 바빠서ㅠ 제가 시간을 많이 내려해요",
          "original_form": "다들 너무 바빠서ㅠ 제가 시간을 많이 내려해요",
          "speaker_id": "2",
          "time": "20210828 22:28"
        },
        {
          "id": "MDRW2100021584.1.7",
          "form": "어머 그래요?",
          "original_form": "어머 그래요?",
          "speaker_id": "1",
          "time": "20210828 22:29"
        },
        {
          "id": "MDRW2100021584.1.8",
          "form": "그럼 집안일은 주로 누가 해요?",
          "original_form": "그럼 집안일은 주로 누가 해요?",
          "speaker_id": "1",
          "time": "20210828 22:29"
        },
        {
          "id": "MDRW2100021584.1.9",
          "form": "제가해요 ㅋㅋ제가 다합니다",
          "original_form": "제가해요 ㅋㅋ제가 다합니다",
          "speaker_id": "2",
          "time": "20210828 22:52"
        },
        {
          "id": "MDRW2100021584.1.10",
          "form": "헐 힘들진 않으세요?",
          "original_form": "헐 힘들진 않으세요?",
          "speaker_id": "1",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100021584.1.11",
          "form": "가족 구성원도 많으신데..",
          "original_form": "가족 구성원도 많으신데..",
          "speaker_id": "1",
          "time": "20210828 22:53"
        },
        {
          "id": "MDRW2100021584.1.12",
          "form": "아 사실 엄마아빤 너무 바쁘시구",
          "original_form": "아 사실 엄마아빤 너무 바쁘시구",
          "speaker_id": "2",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100021584.1.13",
          "form": "언니랑 동생은 외국, 타지역 이렇게있어요",
          "original_form": "언니랑 동생은 외국, 타지역 이렇게있어요",
          "speaker_id": "2",
          "time": "20210828 22:54"
        },
        {
          "id": "MDRW2100021584.1.14",
          "form": "아아 그렇군요",
          "original_form": "아아 그렇군요",
          "speaker_id": "1",
          "time": "20210828 23:04"
        },
        {
          "id": "MDRW2100021584.1.15",
          "form": "아무리 그래도 혼자 다 하긴 힘들 것 같아요",
          "original_form": "아무리 그래도 혼자 다 하긴 힘들 것 같아요",
          "speaker_id": "1",
          "time": "20210828 23:04"
        },
        {
          "id": "MDRW2100021584.1.16",
          "form": "직장도 다니시지 않아요?",
          "original_form": "직장도 다니시지 않아요?",
          "speaker_id": "1",
          "time": "20210828 23:04"
        },
        {
          "id": "MDRW2100021584.1.17",
          "form": "맞아용 흑흑",
          "original_form": "맞아용 흑흑",
          "speaker_id": "2",
          "time": "20210828 23:33"
        },
        {
          "id": "MDRW2100021584.1.18",
          "form": "그래서 몸이 병난거같아요",
          "original_form": "그래서 몸이 병난거같아요",
          "speaker_id": "2",
          "time": "20210828 23:33"
        },
        {
          "id": "MDRW2100021584.1.19",
          "form": "허걱 어떡해여...",
          "original_form": "허걱 어떡해여...",
          "speaker_id": "1",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100021584.1.20",
          "form": "어디 어프신거에요?",
          "original_form": "어디 어프신거에요?",
          "speaker_id": "1",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100021584.1.21",
          "form": "아뇨 그냥 컨디션이 매일 안좋네요 ㅋㅋ",
          "original_form": "아뇨 그냥 컨디션이 매일 안좋네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100021584.1.22",
          "form": "허걱 어떡해요",
          "original_form": "허걱 어떡해요",
          "speaker_id": "1",
          "time": "20210829 11:07"
        },
        {
          "id": "MDRW2100021584.1.23",
          "form": "가족분들이 좀 도와주어야 되는거 아니에요?",
          "original_form": "가족분들이 좀 도와주어야 되는거 아니에요?",
          "speaker_id": "1",
          "time": "20210829 11:07"
        }
      ]
