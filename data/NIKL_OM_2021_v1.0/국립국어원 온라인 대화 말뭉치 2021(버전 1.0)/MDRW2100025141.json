      "utterance": [
        {
          "id": "MDRW2100025141.1.1",
          "form": "넌 결혼할거니?",
          "original_form": "넌 결혼할거니?",
          "speaker_id": "1",
          "time": "20210831 23:37"
        },
        {
          "id": "MDRW2100025141.1.2",
          "form": "아니 난 비혼이야",
          "original_form": "아니 난 비혼이야",
          "speaker_id": "2",
          "time": "20210831 23:37"
        },
        {
          "id": "MDRW2100025141.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210831 23:37"
        },
        {
          "id": "MDRW2100025141.1.4",
          "form": "사실 나도 그래",
          "original_form": "사실 나도 그래",
          "speaker_id": "1",
          "time": "20210831 23:37"
        },
        {
          "id": "MDRW2100025141.1.5",
          "form": "근데 결혼식은 하고싶어",
          "original_form": "근데 결혼식은 하고싶어",
          "speaker_id": "1",
          "time": "20210831 23:37"
        },
        {
          "id": "MDRW2100025141.1.6",
          "form": "내가 비혼식하면 와주라",
          "original_form": "내가 비혼식하면 와주라",
          "speaker_id": "1",
          "time": "20210831 23:38"
        },
        {
          "id": "MDRW2100025141.1.7",
          "form": "남편없이 결혼식 올리게??",
          "original_form": "남편없이 결혼식 올리게??",
          "speaker_id": "2",
          "time": "20210831 23:38"
        },
        {
          "id": "MDRW2100025141.1.8",
          "form": "앜ㅋㅋㅋ 좋은 생각이다",
          "original_form": "앜ㅋㅋㅋ 좋은 생각이다",
          "speaker_id": "2",
          "time": "20210831 23:38"
        },
        {
          "id": "MDRW2100025141.1.9",
          "form": "입간판 세워야지 강동원으로",
          "original_form": "입간판 세워야지 강동원으로",
          "speaker_id": "1",
          "time": "20210831 23:38"
        },
        {
          "id": "MDRW2100025141.1.10",
          "form": "연애는 할거니?",
          "original_form": "연애는 할거니?",
          "speaker_id": "1",
          "time": "20210831 23:38"
        },
        {
          "id": "MDRW2100025141.1.11",
          "form": "강동원의 인권을 지켜주세요",
          "original_form": "강동원의 인권을 지켜주세요",
          "speaker_id": "2",
          "time": "20210831 23:38"
        },
        {
          "id": "MDRW2100025141.1.12",
          "form": "딱히 생각은 없어",
          "original_form": "딱히 생각은 없어",
          "speaker_id": "2",
          "time": "20210831 23:38"
        },
        {
          "id": "MDRW2100025141.1.13",
          "form": "솔로엔딩이구나",
          "original_form": "솔로엔딩이구나",
          "speaker_id": "1",
          "time": "20210831 23:38"
        },
        {
          "id": "MDRW2100025141.1.14",
          "form": "히든엔딩이네",
          "original_form": "히든엔딩이네",
          "speaker_id": "1",
          "time": "20210831 23:39"
        },
        {
          "id": "MDRW2100025141.1.15",
          "form": "이상형은 어떻게되니?",
          "original_form": "이상형은 어떻게되니?",
          "speaker_id": "1",
          "time": "20210831 23:39"
        },
        {
          "id": "MDRW2100025141.1.16",
          "form": "나는 김수현",
          "original_form": "나는 김수현",
          "speaker_id": "2",
          "time": "20210831 23:39"
        },
        {
          "id": "MDRW2100025141.1.17",
          "form": "성격은?",
          "original_form": "성격은?",
          "speaker_id": "1",
          "time": "20210831 23:39"
        },
        {
          "id": "MDRW2100025141.1.18",
          "form": "처럼 생긴 남자",
          "original_form": "처럼 생긴 남자",
          "speaker_id": "2",
          "time": "20210831 23:39"
        },
        {
          "id": "MDRW2100025141.1.19",
          "form": "운동 좋아하고 다정하면 좋겠어",
          "original_form": "운동 좋아하고 다정하면 좋겠어",
          "speaker_id": "2",
          "time": "20210831 23:39"
        },
        {
          "id": "MDRW2100025141.1.20",
          "form": "너는 이상형 있어?",
          "original_form": "너는 이상형 있어?",
          "speaker_id": "2",
          "time": "20210831 23:40"
        },
        {
          "id": "MDRW2100025141.1.21",
          "form": "내말을 잘들어줬으면 좋겠어",
          "original_form": "내말을 잘들어줬으면 좋겠어",
          "speaker_id": "1",
          "time": "20210831 23:40"
        },
        {
          "id": "MDRW2100025141.1.22",
          "form": "잘생겨야해",
          "original_form": "잘생겨야해",
          "speaker_id": "1",
          "time": "20210831 23:40"
        },
        {
          "id": "MDRW2100025141.1.23",
          "form": "중요하지",
          "original_form": "중요하지",
          "speaker_id": "2",
          "time": "20210831 23:40"
        },
        {
          "id": "MDRW2100025141.1.24",
          "form": "그치 얼굴이 최고야",
          "original_form": "그치 얼굴이 최고야",
          "speaker_id": "1",
          "time": "20210831 23:40"
        }
      ]
    }
