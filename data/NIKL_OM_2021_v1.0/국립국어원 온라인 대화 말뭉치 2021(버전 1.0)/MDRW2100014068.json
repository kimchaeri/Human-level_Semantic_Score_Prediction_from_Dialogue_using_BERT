      "utterance": [
        {
          "id": "MDRW2100014068.1.1",
          "form": "저 원래 밤고구마파였는데 요즘은 호박이 더 좋아요ㅋㅋㅋ",
          "original_form": "저 원래 밤고구마파였는데 요즘은 호박이 더 좋아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014068.1.2",
          "form": "ㅋㅋㅋ 전 호박고구마파입니다",
          "original_form": "ㅋㅋㅋ 전 호박고구마파입니다",
          "speaker_id": "1",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014068.1.3",
          "form": "밤은 너무퍽퍽해요",
          "original_form": "밤은 너무퍽퍽해요",
          "speaker_id": "1",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014068.1.4",
          "form": "전 그 펵퍽한맛에 먹었어요ㅎㅎㅎ",
          "original_form": "전 그 펵퍽한맛에 먹었어요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100014068.1.5",
          "form": "우유랑 같이 먹으면 꿀맛!",
          "original_form": "우유랑 같이 먹으면 꿀맛!",
          "speaker_id": "2",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100014068.1.6",
          "form": "ㅋㅋㅋ취향이 반대시군요",
          "original_form": "ㅋㅋㅋ취향이 반대시군요",
          "speaker_id": "1",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014068.1.7",
          "form": "저녁은드셨나요?",
          "original_form": "저녁은드셨나요?",
          "speaker_id": "1",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014068.1.8",
          "form": "아뇨 아직이요~~!! 비빔국수 먹을까 생갓중이에요",
          "original_form": "아뇨 아직이요~~!! 비빔국수 먹을까 생갓중이에요",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014068.1.9",
          "form": "메뉴 정하셨나요?",
          "original_form": "메뉴 정하셨나요?",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014068.1.10",
          "form": "아직이요ㅋㅋㅋㅋ",
          "original_form": "아직이요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014068.1.11",
          "form": "저는 부찌생각중이랍니다",
          "original_form": "저는 부찌생각중이랍니다",
          "speaker_id": "1",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014068.1.12",
          "form": "오 집에소 만들어 드시는건가요?",
          "original_form": "오 집에소 만들어 드시는건가요?",
          "speaker_id": "2",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100014068.1.13",
          "form": "아니요 배달이요ㅎㅎㅎㄹ",
          "original_form": "아니요 배달이요ㅎㅎㅎㄹ",
          "speaker_id": "1",
          "time": "20210825 18:11"
        },
        {
          "id": "MDRW2100014068.1.14",
          "form": "비조리로 시키시나용?ㅋㅋ",
          "original_form": "비조리로 시키시나용?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100014068.1.15",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014068.1.16",
          "form": "저녁잘드셨나요?",
          "original_form": "저녁잘드셨나요?",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014068.1.17",
          "form": "네 먹었어요ㅋㅋ부대찌개는 가격대가 얼마정도인가요??",
          "original_form": "네 먹었어요ㅋㅋ부대찌개는 가격대가 얼마정도인가요??",
          "speaker_id": "2",
