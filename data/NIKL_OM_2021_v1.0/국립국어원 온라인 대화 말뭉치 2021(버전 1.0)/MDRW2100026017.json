        },
        {
          "id": "MDRW2100026017.1.2",
          "form": "네.안녕하세요 저녁 드셨나요",
          "original_form": "네.안녕하세요 저녁 드셨나요",
          "speaker_id": "1",
          "time": "20210907 19:00"
        },
        {
          "id": "MDRW2100026017.1.3",
          "form": "네 먹엇어요 저녁드셧어요?",
          "original_form": "네 먹엇어요 저녁드셧어요?",
          "speaker_id": "2",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100026017.1.4",
          "form": "저 채식라면요",
          "original_form": "저 채식라면요",
          "speaker_id": "1",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100026017.1.5",
          "form": "채식라면이 잇어요?",
          "original_form": "채식라면이 잇어요?",
          "speaker_id": "2",
          "time": "20210907 19:02"
        },
        {
          "id": "MDRW2100026017.1.6",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210907 19:02"
        },
        {
          "id": "MDRW2100026017.1.7",
          "form": "애터미에서 나온거요",
          "original_form": "애터미에서 나온거요",
          "speaker_id": "1",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100026017.1.8",
          "form": "채식라면 풀무원에서도 맛난거 있어요 초록색",
          "original_form": "채식라면 풀무원에서도 맛난거 있어요 초록색",
          "speaker_id": "1",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100026017.1.9",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100026017.1.10",
          "form": "일반 라면이랑 맛이 다른가요?",
          "original_form": "일반 라면이랑 맛이 다른가요?",
          "speaker_id": "2",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100026017.1.11",
          "form": "저는 채식선호해서 더 맛있다?",
          "original_form": "저는 채식선호해서 더 맛있다?",
          "speaker_id": "1",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100026017.1.12",
          "form": "그런느낌",
          "original_form": "그런느낌",
          "speaker_id": "1",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100026017.1.13",
          "form": "오 왠지 훨씬 국물이 깔끔할것같아요",
          "original_form": "오 왠지 훨씬 국물이 깔끔할것같아요",
          "speaker_id": "2",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100026017.1.14",
          "form": "네네 맞아여",
          "original_form": "네네 맞아여",
          "speaker_id": "1",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100026017.1.15",
          "form": "제주음식은 다 고기국물이라 ㅎㅎ",
          "original_form": "제주음식은 다 고기국물이라 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100026017.1.16",
          "form": "저도 궁금해서 먹어봐야겟어요 나중에 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 궁금해서 먹어봐야겟어요 나중에 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100026017.1.17",
          "form": "제가 이름 검색해볼게여",
          "original_form": "제가 이름 검색해볼게여",
          "speaker_id": "1",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100026017.1.18",
          "form": "앗 감사합니다",
          "original_form": "앗 감사합니다",
          "speaker_id": "2",
          "time": "20210907 19:05"
        },
        {
          "id": "MDRW2100026017.1.19",
          "form": "풀무원 정백홍라면이 있어요",
          "original_form": "풀무원 정백홍라면이 있어요",
          "speaker_id": "1",
          "time": "20210907 19:06"
        },
        {
          "id": "MDRW2100026017.1.20",
          "form": "정면 백면 홍면",
          "original_form": "정면 백면 홍면",
          "speaker_id": "1",
          "time": "20210907 19:06"
        },
        {
          "id": "MDRW2100026017.1.21",
          "form": "그중에 정면",
          "original_form": "그중에 정면",
          "speaker_id": "1",
          "time": "20210907 19:06"
        },
        {
          "id": "MDRW2100026017.1.22",
          "form": "우와 감사합니다",
          "original_form": "우와 감사합니다",
          "speaker_id": "2",
          "time": "20210907 19:07"
        },
        {
          "id": "MDRW2100026017.1.23",
          "form": "나중에 꼭 사먹어볼게요",
          "original_form": "나중에 꼭 사먹어볼게요",
