      "utterance": [
        {
          "id": "MDRW2100023799.1.1",
          "form": "비가 많이 왔어요~",
          "original_form": "비가 많이 왔어요~",
          "speaker_id": "1",
          "time": "20210901 14:41"
        },
        {
          "id": "MDRW2100023799.1.2",
          "form": "장마같아요",
          "original_form": "장마같아요",
          "speaker_id": "2",
          "time": "20210901 14:42"
        },
        {
          "id": "MDRW2100023799.1.3",
          "form": "가을이 다가 왔는데요",
          "original_form": "가을이 다가 왔는데요",
          "speaker_id": "1",
          "time": "20210901 14:42"
        },
        {
          "id": "MDRW2100023799.1.4",
          "form": "가을은 천고마비",
          "original_form": "가을은 천고마비",
          "speaker_id": "2",
          "time": "20210901 14:43"
        },
        {
          "id": "MDRW2100023799.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210901 14:43"
        },
        {
          "id": "MDRW2100023799.1.6",
          "form": "미세먼지도별로없지요",
          "original_form": "미세먼지도별로없지요",
          "speaker_id": "2",
          "time": "20210901 14:44"
        },
        {
          "id": "MDRW2100023799.1.7",
          "form": "그렇지요",
          "original_form": "그렇지요",
          "speaker_id": "1",
          "time": "20210901 14:44"
        },
        {
          "id": "MDRW2100023799.1.8",
          "form": "일교차큰요즘 감기조심하시고",
          "original_form": "일교차큰요즘 감기조심하시고",
          "speaker_id": "2",
          "time": "20210901 14:45"
        },
        {
          "id": "MDRW2100023799.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210901 14:45"
        },
        {
          "id": "MDRW2100023799.1.10",
          "form": "따뜻한 옷 입고 다녀야지요",
          "original_form": "따뜻한 옷 입고 다녀야지요",
          "speaker_id": "1",
          "time": "20210901 14:45"
        },
        {
          "id": "MDRW2100023799.1.11",
          "form": "겉옷 간단히 입어요",
          "original_form": "겉옷 간단히 입어요",
          "speaker_id": "2",
          "time": "20210901 14:46"
        },
        {
          "id": "MDRW2100023799.1.12",
          "form": "가디건 입으면 되요",
          "original_form": "가디건 입으면 되요",
          "speaker_id": "1",
          "time": "20210901 14:47"
        },
        {
          "id": "MDRW2100023799.1.13",
          "form": "겨울 되기전에 옷 장만해도 될것 같아요",
          "original_form": "겨울 되기전에 옷 장만해도 될것 같아요",
          "speaker_id": "1",
          "time": "20210901 14:47"
        },
        {
          "id": "MDRW2100023799.1.14",
          "form": "알겠어요",
          "original_form": "알겠어요",
          "speaker_id": "2",
          "time": "20210901 14:48"
        },
        {
          "id": "MDRW2100023799.1.15",
          "form": "추울땐 모자쓰면 좋어요",
          "original_form": "추울땐 모자쓰면 좋어요",
          "speaker_id": "1",
          "time": "20210901 14:48"
        },
        {
          "id": "MDRW2100023799.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210901 14:49"
        },
        {
          "id": "MDRW2100023799.1.17",
          "form": "장갑고 끼고요",
          "original_form": "장갑고 끼고요",
          "speaker_id": "1",
          "time": "20210901 14:49"
        },
        {
          "id": "MDRW2100023799.1.18",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210901 14:49"
        },
        {
          "id": "MDRW2100023799.1.19",
          "form": "목도리 해도 보온되요",
          "original_form": "목도리 해도 보온되요",
          "speaker_id": "2",
          "time": "20210901 14:50"
        },
        {
          "id": "MDRW2100023799.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210901 14:50"
        },
        {
          "id": "MDRW2100023799.1.21",
          "form": "여러옷 겹쳐 입어도 되요",
          "original_form": "여러옷 겹쳐 입어도 되요",
          "speaker_id": "1",
          "time": "20210901 14:50"
        },
        {
          "id": "MDRW2100023799.1.22",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210901 14:51"
        },
        {
          "id": "MDRW2100023799.1.23",
          "form": "뜬근한 음식 먹어도 추위 이길수 있어요",
          "original_form": "뜬근한 음식 먹어도 추위 이길수 있어요",
          "speaker_id": "1",
          "time": "20210901 14:51"
        },
        {
          "id": "MDRW2100023799.1.24",
          "form": "네 겨울엔 빙판길 위헝해요",
          "original_form": "네 겨울엔 빙판길 위헝해요",
          "speaker_id": "2",
          "time": "20210901 14:53"
        },
        {
          "id": "MDRW2100023799.1.25",
          "form": "조심해서 다녀야지요",
          "original_form": "조심해서 다녀야지요",
          "speaker_id": "1",
          "time": "20210901 14:53"
        },
        {
          "id": "MDRW2100023799.1.26",
          "form": "운전도 조심해야합니다.",
          "original_form": "운전도 조심해야합니다.",
          "speaker_id": "1",
          "time": "20210901 14:54"
        },
