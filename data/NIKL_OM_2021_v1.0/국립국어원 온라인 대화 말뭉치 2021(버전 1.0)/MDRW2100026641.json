      "utterance": [
        {
          "id": "MDRW2100026641.1.1",
          "form": "만석 종종 시켜드세요?",
          "original_form": "만석 종종 시켜드세요?",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100026641.1.2",
          "form": "시켜먹을정도는 아닌데 롯데에서 팔때 몇번 사먹었어요 ㅋㅋㅋ",
          "original_form": "시켜먹을정도는 아닌데 롯데에서 팔때 몇번 사먹었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026641.1.3",
          "form": "아 저도요",
          "original_form": "아 저도요",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026641.1.4",
          "form": "닭은 그냥 가까운곳에서 시켜먹는게 제일이잖아요 ㅋㅋ",
          "original_form": "닭은 그냥 가까운곳에서 시켜먹는게 제일이잖아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026641.1.5",
          "form": "무슨 음식 제일 좋아하세요?",
          "original_form": "무슨 음식 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100026641.1.6",
          "form": "요즘에 좋아하는건 달고나...인데 이건 간식이고",
          "original_form": "요즘에 좋아하는건 달고나...인데 이건 간식이고",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026641.1.7",
          "form": "달고나 맛있죠ㅜㅜㅜㅜㅜ",
          "original_form": "달고나 맛있죠ㅜㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026641.1.8",
          "form": "밥은 안가려요 ㅋㅋㅋ 쌀국수도 맛있고 피자도 맛있고",
          "original_form": "밥은 안가려요 ㅋㅋㅋ 쌀국수도 맛있고 피자도 맛있고",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026641.1.9",
          "form": "아 저도 다좋아해요",
          "original_form": "아 저도 다좋아해요",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026641.1.10",
          "form": "쌀국수 못먹은지 한참됐어요",
          "original_form": "쌀국수 못먹은지 한참됐어요",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026641.1.11",
          "form": "요즘 잘하는 가게가 많아서 한번씩 다 먹어보기도 힘든거 같아요 ㅋㅋㅋ",
          "original_form": "요즘 잘하는 가게가 많아서 한번씩 다 먹어보기도 힘든거 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026641.1.12",
          "form": "맞아요 맛집 너무 많아요",
          "original_form": "맞아요 맛집 너무 많아요",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026641.1.13",
          "form": "가고픈데 코로나땜에",
          "original_form": "가고픈데 코로나땜에",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026641.1.14",
          "form": "꺼려져요ㅜㅜㅜ",
          "original_form": "꺼려져요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026641.1.15",
          "form": "그러니까요 코로나만 아니어도 가서 먹는데 배달 안하는 곳도 은근히 있잖아요",
          "original_form": "그러니까요 코로나만 아니어도 가서 먹는데 배달 안하는 곳도 은근히 있잖아요",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026641.1.16",
          "form": "맞아요 진짜 맛집은 코로나에도",
          "original_form": "맞아요 진짜 맛집은 코로나에도",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026641.1.17",
          "form": "손님 많더라구요",
          "original_form": "손님 많더라구요",
          "speaker_id": "1",
          "time": "20210901 23:27"
        }
      ]
