      "utterance": [
        {
          "id": "MDRW2100008302.1.1",
          "form": "출발해서 도착까지요?",
          "original_form": "출발해서 도착까지요?",
          "speaker_id": "2",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008302.1.2",
          "form": "숙박시설은 어떤가요?",
          "original_form": "숙박시설은 어떤가요?",
          "speaker_id": "2",
          "time": "20210822 14:53"
        },
        {
          "id": "MDRW2100008302.1.3",
          "form": "네ㅋㅋ",
          "original_form": "네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 14:59"
        },
        {
          "id": "MDRW2100008302.1.4",
          "form": "숙박 안좋아요!",
          "original_form": "숙박 안좋아요!",
          "speaker_id": "1",
          "time": "20210822 14:59"
        },
        {
          "id": "MDRW2100008302.1.5",
          "form": "아무래도 외져있어서 그런가보네요",
          "original_form": "아무래도 외져있어서 그런가보네요",
          "speaker_id": "2",
          "time": "20210822 15:00"
        },
        {
          "id": "MDRW2100008302.1.6",
          "form": "울릉도 추천하실만한 장소있나요?",
          "original_form": "울릉도 추천하실만한 장소있나요?",
          "speaker_id": "2",
          "time": "20210822 15:00"
        },
        {
          "id": "MDRW2100008302.1.7",
          "form": "네 음 독도요ㅎㅎ",
          "original_form": "네 음 독도요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 16:59"
        },
        {
          "id": "MDRW2100008302.1.8",
          "form": "울릉도가서 독도추천하시는건가요?ㅎ",
          "original_form": "울릉도가서 독도추천하시는건가요?ㅎ",
          "speaker_id": "2",
          "time": "20210822 17:02"
        },
        {
          "id": "MDRW2100008302.1.9",
          "form": "보통 두군데 같이 가나요?",
          "original_form": "보통 두군데 같이 가나요?",
          "speaker_id": "2",
          "time": "20210822 17:03"
        },
        {
          "id": "MDRW2100008302.1.10",
          "form": "네 그쵸 독도는 울릉도에서 또 배 타고 두시간 가야해서",
          "original_form": "네 그쵸 독도는 울릉도에서 또 배 타고 두시간 가야해서",
          "speaker_id": "1",
          "time": "20210822 17:04"
        },
        {
          "id": "MDRW2100008302.1.11",
          "form": "배멀리 나겠어요ㅎㅎ",
          "original_form": "배멀리 나겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 17:05"
        },
        {
          "id": "MDRW2100008302.1.12",
          "form": "배멀미",
          "original_form": "배멀미",
          "speaker_id": "2",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008302.1.13",
          "form": "네 멀미약 먹어야죠",
          "original_form": "네 멀미약 먹어야죠",
          "speaker_id": "1",
          "time": "20210822 18:00"
        },
        {
          "id": "MDRW2100008302.1.14",
          "form": "울릉도는 몇박몇일 다녀오셨어요?",
          "original_form": "울릉도는 몇박몇일 다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210822 18:02"
        },
        {
          "id": "MDRW2100008302.1.15",
          "form": "저 2박~3일이요ㅎㅎ",
          "original_form": "저 2박~3일이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 18:03"
        },
        {
          "id": "MDRW2100008302.1.16",
          "form": "강릉에 하루 있었구요",
          "original_form": "강릉에 하루 있었구요",
          "speaker_id": "1",
          "time": "20210822 18:03"
        },
        {
          "id": "MDRW2100008302.1.17",
          "form": "그렇게 가면 비용은 얼마나드나요?",
          "original_form": "그렇게 가면 비용은 얼마나드나요?",
          "speaker_id": "2",
          "time": "20210822 18:04"
        },
        {
          "id": "MDRW2100008302.1.18",
          "form": "둘이서 백 안들었어요ㅎㅎ",
          "original_form": "둘이서 백 안들었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008302.1.19",
          "form": "아 애기는 없으시군요",
          "original_form": "아 애기는 없으시군요",
          "speaker_id": "2",
          "time": "20210822 18:13"
        },
        {
          "id": "MDRW2100008302.1.20",
          "form": "저도 기회되면 울릉도가봐야겠네요",
          "original_form": "저도 기회되면 울릉도가봐야겠네요",
          "speaker_id": "2",
          "time": "20210822 18:13"
        }
      ]
    }
