      "utterance": [
        {
          "id": "MDRW2100007428.1.1",
          "form": "요즘 머리가 잘빠지는데",
          "original_form": "요즘 머리가 잘빠지는데",
          "speaker_id": "1",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007428.1.2",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007428.1.3",
          "form": "헉 왜요?",
          "original_form": "헉 왜요?",
          "speaker_id": "2",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007428.1.4",
          "form": "탈모에 좋은게 뭐있을까요?",
          "original_form": "탈모에 좋은게 뭐있을까요?",
          "speaker_id": "1",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007428.1.5",
          "form": "스트레스받으시나요?",
          "original_form": "스트레스받으시나요?",
          "speaker_id": "2",
          "time": "20210822 02:03"
        },
        {
          "id": "MDRW2100007428.1.6",
          "form": "모르겠어요  자꾸뽑아서 모근이 약해졌나ㅠㅠ",
          "original_form": "모르겠어요  자꾸뽑아서 모근이 약해졌나ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007428.1.7",
          "form": "저는 원형탈모 온적있었어요",
          "original_form": "저는 원형탈모 온적있었어요",
          "speaker_id": "2",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007428.1.8",
          "form": "헉 진짜요?",
          "original_form": "헉 진짜요?",
          "speaker_id": "1",
          "time": "20210822 02:04"
        },
        {
          "id": "MDRW2100007428.1.9",
          "form": "육아가 힘들었는지",
          "original_form": "육아가 힘들었는지",
          "speaker_id": "2",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007428.1.10",
          "form": "친구도 스트레스 받아서 원형탈모 오던데",
          "original_form": "친구도 스트레스 받아서 원형탈모 오던데",
          "speaker_id": "1",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007428.1.11",
          "form": "탈모샴푸가 도움이 될지 모르겠어요",
          "original_form": "탈모샴푸가 도움이 될지 모르겠어요",
          "speaker_id": "1",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007428.1.12",
          "form": "둘째 돌쯤됐을때 일명 돌치례라고 원인모르게 아파서 차도도없고 ㅜㅠ",
          "original_form": "둘째 돌쯤됐을때 일명 돌치례라고 원인모르게 아파서 차도도없고 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210822 02:05"
        },
        {
          "id": "MDRW2100007428.1.13",
          "form": "새치도 점점 늘어나요  나이먹어서 근가",
          "original_form": "새치도 점점 늘어나요  나이먹어서 근가",
          "speaker_id": "1",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007428.1.14",
          "form": "그렇게 하루하루보내다가 어느날 미용실가니 원형탈모있으시다고 ㅜㅠ",
          "original_form": "그렇게 하루하루보내다가 어느날 미용실가니 원형탈모있으시다고 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007428.1.15",
          "form": "깜놀했어요",
          "original_form": "깜놀했어요",
          "speaker_id": "2",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007428.1.16",
          "form": "어찌하셨어요? 병원가신거예요?",
          "original_form": "어찌하셨어요? 병원가신거예요?",
          "speaker_id": "1",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007428.1.17",
          "form": "네 병원가서 주사맞았어요",
          "original_form": "네 병원가서 주사맞았어요",
          "speaker_id": "2",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007428.1.18",
          "form": "약이 있나요?",
          "original_form": "약이 있나요?",
          "speaker_id": "1",
          "time": "20210822 02:06"
        },
        {
          "id": "MDRW2100007428.1.19",
          "form": "주사만맞았어요",
          "original_form": "주사만맞았어요",
          "speaker_id": "2",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007428.1.20",
          "form": "근데 확실히 효과는 있더라구요",
          "original_form": "근데 확실히 효과는 있더라구요",
          "speaker_id": "2",
          "time": "20210822 02:07"
        },
        {
          "id": "MDRW2100007428.1.21",
          "form": "대머리랑은 다른거라 약효과있나보네요",
          "original_form": "대머리랑은 다른거라 약효과있나보네요",
          "speaker_id": "1",
          "time": "20210822 02:08"
        },
        {
          "id": "MDRW2100007428.1.22",
          "form": "스트레스가 만병의 근원인가봐요ㅠㅠ",
          "original_form": "스트레스가 만병의 근원인가봐요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 02:09"
        }
      ]
