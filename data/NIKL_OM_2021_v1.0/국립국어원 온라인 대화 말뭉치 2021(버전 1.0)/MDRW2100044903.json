      "utterance": [
        {
          "id": "MDRW2100044903.1.1",
          "form": "어디꺼?",
          "original_form": "어디꺼?",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044903.1.2",
          "form": "파바",
          "original_form": "파바",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044903.1.3",
          "form": "파바 롤케익... 무난하져",
          "original_form": "파바 롤케익... 무난하져",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044903.1.4",
          "form": "엄청 맛있지는 않은데 받아서",
          "original_form": "엄청 맛있지는 않은데 받아서",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044903.1.5",
          "form": "선물로 많이 주져",
          "original_form": "선물로 많이 주져",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044903.1.6",
          "form": "그쵸?",
          "original_form": "그쵸?",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044903.1.7",
          "form": "저도 줄때도 많아요",
          "original_form": "저도 줄때도 많아요",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044903.1.8",
          "form": "저도 종종 주고 받아요",
          "original_form": "저도 종종 주고 받아요",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044903.1.9",
          "form": "먹을땐 좋으니까",
          "original_form": "먹을땐 좋으니까",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044903.1.10",
          "form": "맞아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044903.1.11",
          "form": "근대 다른 롤케익이 더 좋긴해요",
          "original_form": "근대 다른 롤케익이 더 좋긴해요",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044903.1.12",
          "form": "도지마?",
          "original_form": "도지마?",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044903.1.13",
          "form": "아 맞있죠 크림",
          "original_form": "아 맞있죠 크림",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044903.1.14",
          "form": "부드러워서 좋아요",
          "original_form": "부드러워서 좋아요",
