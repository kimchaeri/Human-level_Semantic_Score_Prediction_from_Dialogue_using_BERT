        {
          "id": "MDRW2100020552.1.1",
          "form": "name2 님은 아침 드셨나요?",
          "original_form": "&name2& 님은 아침 드셨나요?",
          "speaker_id": "1",
          "time": "20210828 09:41"
        },
        {
          "id": "MDRW2100020552.1.2",
          "form": "아니요ㅠㅠ 저는 아침 안먹어요!",
          "original_form": "아니요ㅠㅠ 저는 아침 안먹어요!",
          "speaker_id": "2",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020552.1.3",
          "form": "저도요ㅋㅋ",
          "original_form": "저도요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100020552.1.4",
          "form": "점심은 뭐 드실 거예요?",
          "original_form": "점심은 뭐 드실 거예요?",
          "speaker_id": "1",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100020552.1.5",
          "form": "쭈꾸미볶음요!!",
          "original_form": "쭈꾸미볶음요!!",
          "speaker_id": "2",
          "time": "20210828 10:38"
        },
        {
          "id": "MDRW2100020552.1.6",
          "form": "쭈꾸미 좋아하세여??ㅋㅋㅋ",
          "original_form": "쭈꾸미 좋아하세여??ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100020552.1.7",
          "form": "오 맛있겠네요 자주는 안 먹지만 가끔 먹으면 맛있게 먹어요ㅋㅋ",
          "original_form": "오 맛있겠네요 자주는 안 먹지만 가끔 먹으면 맛있게 먹어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:40"
        },
        {
          "id": "MDRW2100020552.1.8",
          "form": "전 쌀국수 먹을거예요",
          "original_form": "전 쌀국수 먹을거예요",
          "speaker_id": "1",
          "time": "20210828 10:40"
        },
        {
          "id": "MDRW2100020552.1.9",
          "form": "저두요ㅋㅋㅋ 한번씩 먹으면 맛있어요",
          "original_form": "저두요ㅋㅋㅋ 한번씩 먹으면 맛있어요",
          "speaker_id": "2",
          "time": "20210828 10:41"
        },
        {
          "id": "MDRW2100020552.1.10",
          "form": "쌀국수 맛있겠다!!!!ㅋㅋㅋㅋ",
          "original_form": "쌀국수 맛있겠다!!!!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:41"
        },
        {
          "id": "MDRW2100020552.1.11",
          "form": "베트남 음식 좋아하세요 ?",
          "original_form": "베트남 음식 좋아하세요 ?",
          "speaker_id": "1",
          "time": "20210828 10:41"
        },
        {
          "id": "MDRW2100020552.1.12",
          "form": "네! 좋아해요ㅋㅋㅋ",
          "original_form": "네! 좋아해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020552.1.13",
          "form": "근데 한번 찐 베트남 음식점 갔었는데 월남쌈 한국이랑 엄청 다르더라구욬ㅋㅋ",
          "original_form": "근데 한번 찐 베트남 음식점 갔었는데 월남쌈 한국이랑 엄청 다르더라구욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100020552.1.14",
          "form": "전 한국식이 좋아요ㅋㅋㅋ",
          "original_form": "전 한국식이 좋아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100020552.1.15",
          "form": "베트남 월남쌈은 어때요??",
          "original_form": "베트남 월남쌈은 어때요??",
          "speaker_id": "1",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100020552.1.16",
          "form": "뭔가 너무 자연의 맛이랄까... 그랬던 기억이 있네요",
          "original_form": "뭔가 너무 자연의 맛이랄까... 그랬던 기억이 있네요",
          "speaker_id": "2",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020552.1.17",
          "form": "한국 월나삼이 재료도 풍성하고 맛있는거같아요ㅋㅋㅋ",
          "original_form": "한국 월나삼이 재료도 풍성하고 맛있는거같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020552.1.18",
          "form": "오 그렇군요...",
          "original_form": "오 그렇군요...",
          "speaker_id": "1",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020552.1.19",
          "form": "뭐든 한국식이 최고예요ㅋㅋㅋ",
          "original_form": "뭐든 한국식이 최고예요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020552.1.20",
          "form": "한국인들은 음식에 진심이라 한국화되면 더 맛있어지는거 같아요ㅋㅋㅋㅋ",
          "original_form": "한국인들은 음식에 진심이라 한국화되면 더 맛있어지는거 같아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020552.1.21",
          "form": "ㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020552.1.22",
          "form": "월남쌈 얘기하니까 오랜만에 먹고싶네요ㅋㅋㅋ",
          "original_form": "월남쌈 얘기하니까 오랜만에 먹고싶네요ㅋㅋㅋ",
