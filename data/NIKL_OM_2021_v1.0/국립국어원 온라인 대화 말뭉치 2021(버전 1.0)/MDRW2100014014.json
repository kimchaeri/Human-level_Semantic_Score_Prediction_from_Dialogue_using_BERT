      "utterance": [
        {
          "id": "MDRW2100014014.1.1",
          "form": "특히 전문화 더ㅣ지 않은 일반인들이 bj를 하면서",
          "original_form": "특히 전문화 더ㅣ지 않은 일반인들이 bj를 하면서",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014014.1.2",
          "form": "애들한테 더 안좋은 영향을 미치는 거 같아요...",
          "original_form": "애들한테 더 안좋은 영향을 미치는 거 같아요...",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100014014.1.3",
          "form": "전 그래서 bj들.영상 거의 안봐요",
          "original_form": "전 그래서 bj들.영상 거의 안봐요",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100014014.1.4",
          "form": "맞아요.....",
          "original_form": "맞아요.....",
          "speaker_id": "1",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100014014.1.5",
          "form": "저도 음 게임좋아해서",
          "original_form": "저도 음 게임좋아해서",
          "speaker_id": "1",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100014014.1.6",
          "form": "게임 bj 욕안하는 사람만 봤었는데",
          "original_form": "게임 bj 욕안하는 사람만 봤었는데",
          "speaker_id": "1",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100014014.1.7",
          "form": "그사람들도 참 다 터지더라구요",
          "original_form": "그사람들도 참 다 터지더라구요",
          "speaker_id": "1",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100014014.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100014014.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 혹시 이번에 터지신 그 bj분인 가요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 혹시 이번에 터지신 그 bj분인 가요?",
          "speaker_id": "2",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100014014.1.10",
          "form": "아 그분은 본적없구",
          "original_form": "아 그분은 본적없구",
          "speaker_id": "1",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100014014.1.11",
          "form": "원래 대도서관?",
          "original_form": "원래 대도서관?",
          "speaker_id": "1",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100014014.1.12",
          "form": "그분 봤었어요!",
          "original_form": "그분 봤었어요!",
          "speaker_id": "1",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100014014.1.13",
          "form": "아 맞아요 전 그 분 그거 하는 건 봤었는데",
          "original_form": "아 맞아요 전 그 분 그거 하는 건 봤었는데",
          "speaker_id": "2",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100014014.1.14",
          "form": "닌텐도 스위치 춤추는 거",
          "original_form": "닌텐도 스위치 춤추는 거",
          "speaker_id": "2",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100014014.1.15",
          "form": "약간 뭐랄까 연예인처럼 거리두기하고싶은데",
          "original_form": "약간 뭐랄까 연예인처럼 거리두기하고싶은데",
          "speaker_id": "1",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100014014.1.16",
          "form": "워낙 노출이 될수밖에 없는 직업이다보니",
          "original_form": "워낙 노출이 될수밖에 없는 직업이다보니",
          "speaker_id": "1",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100014014.1.17",
          "form": "너무 그사람에 대해서 잘알게되니까",
          "original_form": "너무 그사람에 대해서 잘알게되니까",
          "speaker_id": "1",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100014014.1.18",
          "form": "실망이 커지는것같아요",
          "original_form": "실망이 커지는것같아요",
          "speaker_id": "1",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100014014.1.19",
          "form": "맞아요... 정말 연예인이나 공인은 좀 괜찮다 싶다가도",
          "original_form": "맞아요... 정말 연예인이나 공인은 좀 괜찮다 싶다가도",
          "speaker_id": "2",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100014014.1.20",
          "form": "결국엔 실망만 남는...",
          "original_form": "결국엔 실망만 남는...",
          "speaker_id": "2",
          "time": "20210825 16:55"
