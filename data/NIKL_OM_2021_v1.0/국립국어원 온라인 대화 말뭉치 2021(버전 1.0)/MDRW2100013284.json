      "utterance": [
        {
          "id": "MDRW2100013284.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013284.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100013284.1.3",
          "form": "하시는 운동 있으신가요?",
          "original_form": "하시는 운동 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100013284.1.4",
          "form": "지금은 안하고 있고 전에 합기도했었어요!",
          "original_form": "지금은 안하고 있고 전에 합기도했었어요!",
          "speaker_id": "2",
          "time": "20210825 13:56"
        },
        {
          "id": "MDRW2100013284.1.5",
          "form": "합기도 너무멋지싶니다",
          "original_form": "합기도 너무멋지싶니다",
          "speaker_id": "1",
          "time": "20210825 13:56"
        },
        {
          "id": "MDRW2100013284.1.6",
          "form": "합기도는 어떻게 시작하게되셨나요",
          "original_form": "합기도는 어떻게 시작하게되셨나요",
          "speaker_id": "1",
          "time": "20210825 13:56"
        },
        {
          "id": "MDRW2100013284.1.7",
          "form": "하하 중학생때 부모님손에 강제로 시작하게 됐어요ㅎㅎ",
          "original_form": "하하 중학생때 부모님손에 강제로 시작하게 됐어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:57"
        },
        {
          "id": "MDRW2100013284.1.8",
          "form": "그래도 막상 해보니 재밌더라고요!",
          "original_form": "그래도 막상 해보니 재밌더라고요!",
          "speaker_id": "2",
          "time": "20210825 13:57"
        },
        {
          "id": "MDRW2100013284.1.9",
          "form": "어떤점에서 매럭을 느끼셨나요",
          "original_form": "어떤점에서 매럭을 느끼셨나요",
          "speaker_id": "1",
          "time": "20210825 13:57"
        },
        {
          "id": "MDRW2100013284.1.10",
          "form": "무예랑 무술이랑 복합적으로 다양하게 다 하더라고요ㅋㅋ",
          "original_form": "무예랑 무술이랑 복합적으로 다양하게 다 하더라고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100013284.1.11",
          "form": "운동 하시는거 있으신가요?",
          "original_form": "운동 하시는거 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100013284.1.12",
          "form": "너무재밌겠어요",
          "original_form": "너무재밌겠어요",
          "speaker_id": "1",
          "time": "20210825 13:59"
        },
        {
          "id": "MDRW2100013284.1.13",
          "form": "저는 운동은 따로 못하고 저녁마다 산책다녀요",
          "original_form": "저는 운동은 따로 못하고 저녁마다 산책다녀요",
          "speaker_id": "1",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100013284.1.14",
          "form": "오 산책도 좋죠!",
          "original_form": "오 산책도 좋죠!",
          "speaker_id": "2",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100013284.1.15",
          "form": "요즘 코로나땜에 헬스장가기도 그렇고ㅜㅜ",
          "original_form": "요즘 코로나땜에 헬스장가기도 그렇고ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100013284.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013284.1.17",
          "form": "헬스장 갔다가 코로나걸리면..",
          "original_form": "헬스장 갔다가 코로나걸리면..",
          "speaker_id": "1",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013284.1.18",
          "form": "으....너무 끔찍해요...",
          "original_form": "으....너무 끔찍해요...",
          "speaker_id": "2",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013284.1.19",
          "form": "마스크쓰고 헬스하기도 그렇고 허허....",
          "original_form": "마스크쓰고 헬스하기도 그렇고 허허....",
          "speaker_id": "2",
          "time": "20210825 14:01"
        },
        {
          "id": "MDRW2100013284.1.20",
          "form": "맞아요 마스크쓰고 운동하는거 못할짓이더라구요",
          "original_form": "맞아요 마스크쓰고 운동하는거 못할짓이더라구요",
          "speaker_id": "1",
          "time": "20210825 14:05"
        }
      ]
