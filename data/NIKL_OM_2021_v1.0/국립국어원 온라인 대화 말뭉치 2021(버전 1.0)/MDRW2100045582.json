      "utterance": [
        {
          "id": "MDRW2100045582.1.1",
          "form": "이번에는 제가 먹은 과일 맞춰보실래요?",
          "original_form": "이번에는 제가 먹은 과일 맞춰보실래요?",
          "speaker_id": "1",
          "time": "20210908 03:49"
        },
        {
          "id": "MDRW2100045582.1.2",
          "form": "조아요.",
          "original_form": "조아요.",
          "speaker_id": "2",
          "time": "20210908 03:51"
        },
        {
          "id": "MDRW2100045582.1.3",
          "form": "당연히 제철과일이갰죠?",
          "original_form": "당연히 제철과일이갰죠?",
          "speaker_id": "2",
          "time": "20210908 03:51"
        },
        {
          "id": "MDRW2100045582.1.4",
          "form": "네 맞아요 맛있는 제철과일",
          "original_form": "네 맞아요 맛있는 제철과일",
          "speaker_id": "1",
          "time": "20210908 03:52"
        },
        {
          "id": "MDRW2100045582.1.5",
          "form": "음.... 샤인머스켓?",
          "original_form": "음.... 샤인머스켓?",
          "speaker_id": "2",
          "time": "20210908 03:53"
        },
        {
          "id": "MDRW2100045582.1.6",
          "form": "(은 지금 제가 먹고 싶은것....",
          "original_form": "(은 지금 제가 먹고 싶은것....",
          "speaker_id": "2",
          "time": "20210908 03:53"
        },
        {
          "id": "MDRW2100045582.1.7",
          "form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋ자꾸 드시고 싶으신 거 말씀하시네!!!",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋ자꾸 드시고 싶으신 거 말씀하시네!!!",
          "speaker_id": "1",
          "time": "20210908 03:54"
        },
        {
          "id": "MDRW2100045582.1.8",
          "form": "근데 맛있겠어요ㅋㅋㅋ",
          "original_form": "근데 맛있겠어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:54"
        },
        {
          "id": "MDRW2100045582.1.9",
          "form": "샤인머스켓 아닙니다~!",
          "original_form": "샤인머스켓 아닙니다~!",
          "speaker_id": "1",
          "time": "20210908 03:54"
        },
        {
          "id": "MDRW2100045582.1.10",
          "form": "먹고 싶은걸 어떻개욬ㅋㅋㅋㅋ큐ㅠㅠ",
          "original_form": "먹고 싶은걸 어떻개욬ㅋㅋㅋㅋ큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 03:54"
        },
        {
          "id": "MDRW2100045582.1.11",
          "form": "씨가 있나요?",
          "original_form": "씨가 있나요?",
          "speaker_id": "2",
          "time": "20210908 03:54"
        },
        {
          "id": "MDRW2100045582.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:55"
        },
        {
          "id": "MDRW2100045582.1.13",
          "form": "네 씨가 있습니다!",
          "original_form": "네 씨가 있습니다!",
          "speaker_id": "1",
          "time": "20210908 03:55"
        },
        {
          "id": "MDRW2100045582.1.14",
          "form": "초록색인가요?",
          "original_form": "초록색인가요?",
          "speaker_id": "2",
          "time": "20210908 03:55"
        },
        {
          "id": "MDRW2100045582.1.15",
          "form": "아니요! 초록색이 아닙니다~~~",
          "original_form": "아니요! 초록색이 아닙니다~~~",
          "speaker_id": "1",
          "time": "20210908 03:55"
        },
        {
          "id": "MDRW2100045582.1.16",
          "form": "그럼 빨간색인가요?",
          "original_form": "그럼 빨간색인가요?",
          "speaker_id": "2",
          "time": "20210908 03:57"
        },
        {
          "id": "MDRW2100045582.1.17",
          "form": "아니요 빨간색이 아니예요!",
          "original_form": "아니요 빨간색이 아니예요!",
          "speaker_id": "1",
          "time": "20210908 03:57"
        },
        {
          "id": "MDRW2100045582.1.18",
          "form": "초록색 과일도 빨간색 과일도 아니여유",
          "original_form": "초록색 과일도 빨간색 과일도 아니여유",
          "speaker_id": "1",
          "time": "20210908 03:58"
        },
        {
          "id": "MDRW2100045582.1.19",
          "form": "그럼 보라색?!",
          "original_form": "그럼 보라색?!",
          "speaker_id": "2",
          "time": "20210908 03:59"
        },
        {
          "id": "MDRW2100045582.1.20",
          "form": "앗 이거 거의 다 알려주는 거 아니예요? 네 보라색입니다ㅋㅋㅋ",
          "original_form": "앗 이거 거의 다 알려주는 거 아니예요? 네 보라색입니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:59"
        },
        {
          "id": "MDRW2100045582.1.21",
          "form": "포도요!",
          "original_form": "포도요!",
          "speaker_id": "2",
          "time": "20210908 04:00"
        },
        {
          "id": "MDRW2100045582.1.22",
          "form": "정답 포도~!!!",
          "original_form": "정답 포도~!!!",
          "speaker_id": "1",
          "time": "20210908 04:01"
        },
        {
          "id": "MDRW2100045582.1.23",
          "form": "ㅋㅋㅋ포도 색이 너무 유니크해서",
          "original_form": "ㅋㅋㅋ포도 색이 너무 유니크해서",
          "speaker_id": "1",
          "time": "20210908 04:01"
        },
        {
          "id": "MDRW2100045582.1.24",
          "form": "금방 맞춰브렀어요",
          "original_form": "금방 맞춰브렀어요",
          "speaker_id": "1",
          "time": "20210908 04:01"
        },
        {
          "id": "MDRW2100045582.1.25",
          "form": "에잇! 처음부터 샤인하다가",
          "original_form": "에잇! 처음부터 샤인하다가",
          "speaker_id": "2",
          "time": "20210908 04:01"
        },
        {
          "id": "MDRW2100045582.1.26",
          "form": "포도하려고 했는뎈ㅋㅋㅋ",
          "original_form": "포도하려고 했는뎈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:01"
        },
        {
          "id": "MDRW2100045582.1.27",
          "form": "빙빙 돌았네욬ㅋㅋㅋ",
          "original_form": "빙빙 돌았네욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:01"
        },
        {
          "id": "MDRW2100045582.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅌㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅌㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:01"
        },
        {
          "id": "MDRW2100045582.1.29",
          "form": "그래도 멀리 안 돌았어요 짱 빨리 했시야",
          "original_form": "그래도 멀리 안 돌았어요 짱 빨리 했시야",
          "speaker_id": "1",
          "time": "20210908 04:02"
        },
        {
          "id": "MDRW2100045582.1.30",
