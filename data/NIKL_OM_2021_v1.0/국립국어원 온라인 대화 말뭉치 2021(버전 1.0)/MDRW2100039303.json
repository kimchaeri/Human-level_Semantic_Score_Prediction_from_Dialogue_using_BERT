      "utterance": [
        {
          "id": "MDRW2100039303.1.1",
          "form": "점심 뭐 드셨나요?",
          "original_form": "점심 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210903 15:59"
        },
        {
          "id": "MDRW2100039303.1.2",
          "form": "된장국이요!",
          "original_form": "된장국이요!",
          "speaker_id": "2",
          "time": "20210903 16:00"
        },
        {
          "id": "MDRW2100039303.1.3",
          "form": "맛있겠어요",
          "original_form": "맛있겠어요",
          "speaker_id": "1",
          "time": "20210903 16:01"
        },
        {
          "id": "MDRW2100039303.1.4",
          "form": "마자요. 시판된장에 호박이랑 두부 넣어서 끓인건데 국물이 맛있어요 ㅎㅎ",
          "original_form": "마자요. 시판된장에 호박이랑 두부 넣어서 끓인건데 국물이 맛있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 17:01"
        },
        {
          "id": "MDRW2100039303.1.5",
          "form": "특별한 비법이 있나여? ㅋㅋㅋ",
          "original_form": "특별한 비법이 있나여? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:06"
        },
        {
          "id": "MDRW2100039303.1.6",
          "form": "가족이 만드러준거라 저는 잘 모르겠어요",
          "original_form": "가족이 만드러준거라 저는 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210903 17:10"
        },
        {
          "id": "MDRW2100039303.1.7",
          "form": "앗 그렇군요",
          "original_form": "앗 그렇군요",
          "speaker_id": "1",
          "time": "20210903 17:11"
        },
        {
          "id": "MDRW2100039303.1.8",
          "form": "심심이에서 요리 잘하시는 분들 많이 봐서 편견이 생겼나봐요",
          "original_form": "심심이에서 요리 잘하시는 분들 많이 봐서 편견이 생겼나봐요",
          "speaker_id": "1",
          "time": "20210903 17:11"
        },
        {
          "id": "MDRW2100039303.1.9",
          "form": "악ㅋㅋㅋ 그렇군요 name1님은 점심 뭐드셨어요!",
          "original_form": "악ㅋㅋㅋ 그렇군요 &name1&님은 점심 뭐드셨어요!",
          "speaker_id": "2",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100039303.1.10",
          "form": "김치국에 돼지고기 볶음이여",
          "original_form": "김치국에 돼지고기 볶음이여",
          "speaker_id": "1",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100039303.1.11",
          "form": "돼지고기 좋아하는데 맛있었겠네요..",
          "original_form": "돼지고기 좋아하는데 맛있었겠네요..",
          "speaker_id": "2",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100039303.1.12",
          "form": "소스가 니맛도 내맛도 아니여서 별로였어요",
          "original_form": "소스가 니맛도 내맛도 아니여서 별로였어요",
          "speaker_id": "1",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039303.1.13",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039303.1.14",
          "form": "이럴수가.. 돼지고기는 웬만해서 다 괜찮은데 소스가.. 많이 맛이 없었군요...",
          "original_form": "이럴수가.. 돼지고기는 웬만해서 다 괜찮은데 소스가.. 많이 맛이 없었군요...",
          "speaker_id": "2",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100039303.1.15",
          "form": "여기 식당이 음식을 잘 못 하나 봐요ㅠㅜ",
          "original_form": "여기 식당이 음식을 잘 못 하나 봐요ㅠㅜ",
