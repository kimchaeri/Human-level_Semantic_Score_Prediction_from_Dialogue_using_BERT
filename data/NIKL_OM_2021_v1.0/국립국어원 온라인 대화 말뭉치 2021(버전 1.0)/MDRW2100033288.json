      "utterance": [
        {
          "id": "MDRW2100033288.1.1",
          "form": "저 수요일에 접종해요 ㅠㅠ",
          "original_form": "저 수요일에 접종해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100033288.1.2",
          "form": "뭐로 맞으세요?",
          "original_form": "뭐로 맞으세요?",
          "speaker_id": "2",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100033288.1.3",
          "form": "화이자모더나둘중하나인데",
          "original_form": "화이자모더나둘중하나인데",
          "speaker_id": "1",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100033288.1.4",
          "form": "아직 안정해졌어요",
          "original_form": "아직 안정해졌어요",
          "speaker_id": "1",
          "time": "20210906 21:20"
        },
        {
          "id": "MDRW2100033288.1.5",
          "form": "그렇군요 병원에 들어오는 백신에 따라 다르겠네요",
          "original_form": "그렇군요 병원에 들어오는 백신에 따라 다르겠네요",
          "speaker_id": "2",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100033288.1.6",
          "form": "모더나가 물량 공급할만큼 큰 회사인가봐요",
          "original_form": "모더나가 물량 공급할만큼 큰 회사인가봐요",
          "speaker_id": "1",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100033288.1.7",
          "form": "미국 제약회사인 것 같던데요",
          "original_form": "미국 제약회사인 것 같던데요",
          "speaker_id": "2",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100033288.1.8",
          "form": "그런데 모더나 공급이 아직 다 안 된 것 같아요",
          "original_form": "그런데 모더나 공급이 아직 다 안 된 것 같아요",
          "speaker_id": "2",
          "time": "20210906 21:27"
        },
        {
          "id": "MDRW2100033288.1.9",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100033288.1.10",
          "form": "그래서 텀이 길군요",
          "original_form": "그래서 텀이 길군요",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100033288.1.11",
          "form": "백신 확보하고 예약을 받아야는데 운영이 불안정한 것 같아요",
          "original_form": "백신 확보하고 예약을 받아야는데 운영이 불안정한 것 같아요",
          "speaker_id": "2",
          "time": "20210906 21:33"
        },
        {
          "id": "MDRW2100033288.1.12",
          "form": "왠지 모더나보담 화이자를 맞고싶은 듯",
          "original_form": "왠지 모더나보담 화이자를 맞고싶은 듯",
          "speaker_id": "1",
          "time": "20210906 21:34"
        },
        {
          "id": "MDRW2100033288.1.13",
          "form": "화이자 희망하시나요?",
          "original_form": "화이자 희망하시나요?",
          "speaker_id": "2",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100033288.1.14",
          "form": "네넴",
          "original_form": "네넴",
          "speaker_id": "1",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100033288.1.15",
          "form": "모더나 사건사고가 많아서여",
          "original_form": "모더나 사건사고가 많아서여",
          "speaker_id": "1",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100033288.1.16",
          "form": "백신 부작용이 모더나가 더 많나봐요?",
          "original_form": "백신 부작용이 모더나가 더 많나봐요?",
          "speaker_id": "2",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100033288.1.17",
          "form": "제가 들은 뉴스가 그런것 같아요",
          "original_form": "제가 들은 뉴스가 그런것 같아요",
          "speaker_id": "1",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100033288.1.18",
          "form": "그래도 국가에서 진행하는 일이니까 협조해야겠죠",
          "original_form": "그래도 국가에서 진행하는 일이니까 협조해야겠죠",
