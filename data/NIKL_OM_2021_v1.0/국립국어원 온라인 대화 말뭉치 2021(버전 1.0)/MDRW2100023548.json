      "utterance": [
        {
          "id": "MDRW2100023548.1.1",
          "form": "여름 휴가로 여행 다녀오셨나요?",
          "original_form": "여름 휴가로 여행 다녀오셨나요?",
          "speaker_id": "2",
          "time": "20210830 11:41"
        },
        {
          "id": "MDRW2100023548.1.2",
          "form": "아뇨 저는 집콕했습니다ㅎ",
          "original_form": "아뇨 저는 집콕했습니다ㅎ",
          "speaker_id": "1",
          "time": "20210830 11:42"
        },
        {
          "id": "MDRW2100023548.1.3",
          "form": "어디 다녀오셨어요?",
          "original_form": "어디 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210830 11:42"
        },
        {
          "id": "MDRW2100023548.1.4",
          "form": "저두 집콕이요ㅠㅠㅠ",
          "original_form": "저두 집콕이요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 11:46"
        },
        {
          "id": "MDRW2100023548.1.5",
          "form": "요즘엔 집콕이 최고에요ㅠㅠㅠ",
          "original_form": "요즘엔 집콕이 최고에요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 12:16"
        },
        {
          "id": "MDRW2100023548.1.6",
          "form": "나중에 어디 여행가고싶으세요?",
          "original_form": "나중에 어디 여행가고싶으세요?",
          "speaker_id": "1",
          "time": "20210830 12:16"
        },
        {
          "id": "MDRW2100023548.1.7",
          "form": "제주도로 가고싶어요!",
          "original_form": "제주도로 가고싶어요!",
          "speaker_id": "2",
          "time": "20210830 12:27"
        },
        {
          "id": "MDRW2100023548.1.8",
          "form": "저도 제주도 가고싶네요 ㅠ요즘도 많이들 가던데",
          "original_form": "저도 제주도 가고싶네요 ㅠ요즘도 많이들 가던데",
          "speaker_id": "1",
          "time": "20210830 12:44"
        },
        {
          "id": "MDRW2100023548.1.9",
          "form": "제주도에 수플레 케이크집도 가보고 싶어요",
          "original_form": "제주도에 수플레 케이크집도 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210830 12:55"
        },
        {
          "id": "MDRW2100023548.1.10",
          "form": "맛집인가보네요 상호가 뭐에용?",
          "original_form": "맛집인가보네요 상호가 뭐에용?",
          "speaker_id": "1",
          "time": "20210830 12:56"
        },
        {
          "id": "MDRW2100023548.1.11",
          "form": "당당 이요!",
          "original_form": "당당 이요!",
          "speaker_id": "2",
          "time": "20210830 13:51"
        },
        {
          "id": "MDRW2100023548.1.12",
          "form": "귀엽네용 한번 찾아봐야겠어요",
          "original_form": "귀엽네용 한번 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210830 14:02"
        },
        {
          "id": "MDRW2100023548.1.13",
          "form": "저는 고등어회가 먹고싶네요",
          "original_form": "저는 고등어회가 먹고싶네요",
          "speaker_id": "1",
          "time": "20210830 14:02"
        },
        {
          "id": "MDRW2100023548.1.14",
          "form": "고등어회는 못 먹어봤어요ㅠ 맛있나요??",
          "original_form": "고등어회는 못 먹어봤어요ㅠ 맛있나요??",
          "speaker_id": "2",
          "time": "20210830 14:07"
        },
        {
          "id": "MDRW2100023548.1.15",
          "form": "저도 제주도에서 첨 맛봤는데 반했어요ㅜㅜ",
          "original_form": "저도 제주도에서 첨 맛봤는데 반했어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 14:09"
        },
        {
          "id": "MDRW2100023548.1.16",
          "form": "본격적으로 가을이 되면 그냥 고등어 회 찾아서 먹어봐야겠네여ㅋㅋ 제주도는 무리니까여ㅠ",
          "original_form": "본격적으로 가을이 되면 그냥 고등어 회 찾아서 먹어봐야겠네여ㅋㅋ 제주도는 무리니까여ㅠ",
          "speaker_id": "2",
          "time": "20210830 14:11"
        }
      ]
