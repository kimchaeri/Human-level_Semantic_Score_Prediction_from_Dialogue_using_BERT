        },
        {
          "id": "MDRW2100020693.1.2",
          "form": "안녕하세요 ~~",
          "original_form": "안녕하세요 ~~",
          "speaker_id": "2",
          "time": "20210828 11:15"
        },
        {
          "id": "MDRW2100020693.1.3",
          "form": "콜라파세요 사이다파세요 ??",
          "original_form": "콜라파세요 사이다파세요 ??",
          "speaker_id": "2",
          "time": "20210828 11:16"
        },
        {
          "id": "MDRW2100020693.1.4",
          "form": "저는 사이다요~",
          "original_form": "저는 사이다요~",
          "speaker_id": "1",
          "time": "20210828 11:16"
        },
        {
          "id": "MDRW2100020693.1.5",
          "form": "오 저도에요",
          "original_form": "오 저도에요",
          "speaker_id": "2",
          "time": "20210828 11:29"
        },
        {
          "id": "MDRW2100020693.1.6",
          "form": "오오 칠성 혹은 스프라이트 하나둘셋?",
          "original_form": "오오 칠성 혹은 스프라이트 하나둘셋?",
          "speaker_id": "1",
          "time": "20210828 11:31"
        },
        {
          "id": "MDRW2100020693.1.7",
          "form": "하나둘셋 !",
          "original_form": "하나둘셋 !",
          "speaker_id": "2",
          "time": "20210828 11:42"
        },
        {
          "id": "MDRW2100020693.1.8",
          "form": "전 칠성이욬ㅋㅋㅋ",
          "original_form": "전 칠성이욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:34"
        },
        {
          "id": "MDRW2100020693.1.9",
          "form": "저도 칠성이에요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 칠성이에요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:46"
        },
        {
          "id": "MDRW2100020693.1.10",
          "form": "칠성이 달달해서 맛있어요",
          "original_form": "칠성이 달달해서 맛있어요",
          "speaker_id": "2",
          "time": "20210828 15:46"
        },
        {
          "id": "MDRW2100020693.1.11",
          "form": "맞아요~ 스프라이트 밍밍!",
          "original_form": "맞아요~ 스프라이트 밍밍!",
          "speaker_id": "1",
          "time": "20210828 16:11"
        },
        {
          "id": "MDRW2100020693.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요 !!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요 !!!",
          "speaker_id": "2",
          "time": "20210829 18:37"
        },
        {
          "id": "MDRW2100020693.1.13",
          "form": "바람이 선선한게 ~ 마라탕이 너무 먹고 싶어지네요 ㅎ.ㅡㄱ흑",
          "original_form": "바람이 선선한게 ~ 마라탕이 너무 먹고 싶어지네요 ㅎ.ㅡㄱ흑",
          "speaker_id": "2",
          "time": "20210829 18:37"
        },
        {
          "id": "MDRW2100020693.1.14",
          "form": "앜ㅋㅋㅋㅋ 마라탕은 어떤이유를가져오더라도 먹고싶긴하죠",
          "original_form": "앜ㅋㅋㅋㅋ 마라탕은 어떤이유를가져오더라도 먹고싶긴하죠",
          "speaker_id": "1",
          "time": "20210830 10:53"
        },
        {
          "id": "MDRW2100020693.1.15",
          "form": "마라탕에 뭐넣어드시나요?",
          "original_form": "마라탕에 뭐넣어드시나요?",
          "speaker_id": "1",
          "time": "20210830 10:53"
        },
        {
          "id": "MDRW2100020693.1.16",
          "form": "ㅋㅋㅋㅋ맞아요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ맞아요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:00"
        },
        {
          "id": "MDRW2100020693.1.17",
          "form": "마라탕에.일단 고수를 넣고",
          "original_form": "마라탕에.일단 고수를 넣고",
          "speaker_id": "2",
          "time": "20210831 13:00"
        },
        {
          "id": "MDRW2100020693.1.18",
          "form": "면 종류별로 다 담고",
          "original_form": "면 종류별로 다 담고",
          "speaker_id": "2",
          "time": "20210831 13:00"
        },
        {
          "id": "MDRW2100020693.1.19",
          "form": "청경채넣고 목이버섯넣고",
          "original_form": "청경채넣고 목이버섯넣고",
          "speaker_id": "2",
          "time": "20210831 13:00"
        },
        {
          "id": "MDRW2100020693.1.20",
          "form": "또 뭐있죠 기억이 안나네어ㅗ",
          "original_form": "또 뭐있죠 기억이 안나네어ㅗ",
          "speaker_id": "2",
          "time": "20210831 13:01"
        },
        {
          "id": "MDRW2100020693.1.21",
          "form": "요",
          "original_form": "요",
          "speaker_id": "2",
          "time": "20210831 13:01"
        },
        {
          "id": "MDRW2100020693.1.22",
          "form": "name1님도 마라탕 좋아하세요 ?",
          "original_form": "&name1&님도 마라탕 좋아하세요 ?",
          "speaker_id": "2",
          "time": "20210831 13:01"
        },
        {
          "id": "MDRW2100020693.1.23",
          "form": "와웅! 저도 마라탕 좋아하는데 즐겨먹진않아요~ㅎㅎ",
          "original_form": "와웅! 저도 마라탕 좋아하는데 즐겨먹진않아요~ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 14:01"
        },
        {
          "id": "MDRW2100020693.1.24",
          "form": "저도 언급하신것들 다 넣어먹어요! 거기다가 새우완자까지넣는답니다 ㅎㅎ",
          "original_form": "저도 언급하신것들 다 넣어먹어요! 거기다가 새우완자까지넣는답니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 14:01"
        },
        {
          "id": "MDRW2100020693.1.25",
          "form": "새우완자 오 ! 맛이 조금 달라지나요 ??",
          "original_form": "새우완자 오 ! 맛이 조금 달라지나요 ??",
          "speaker_id": "2",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100020693.1.26",
          "form": "음 글쎄요 맛은 그냥 마라탕맛 똑같은데 그냥 새우좋아하시면 넣으시면 대만족하실듯요!",
