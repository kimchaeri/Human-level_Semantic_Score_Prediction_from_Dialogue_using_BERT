      "utterance": [
        {
          "id": "MDRW2100021089.1.1",
          "form": "코로나 끝나면 어디 제일 가고 싶으세요??",
          "original_form": "코로나 끝나면 어디 제일 가고 싶으세요??",
          "speaker_id": "2",
          "time": "20210828 15:47"
        },
        {
          "id": "MDRW2100021089.1.2",
          "form": "국내는 일단 안동이요",
          "original_form": "국내는 일단 안동이요",
          "speaker_id": "1",
          "time": "20210828 15:48"
        },
        {
          "id": "MDRW2100021089.1.3",
          "form": "해외는 대만 가고싶어요",
          "original_form": "해외는 대만 가고싶어요",
          "speaker_id": "1",
          "time": "20210828 15:48"
        },
        {
          "id": "MDRW2100021089.1.4",
          "form": "어디가고싶으세요?",
          "original_form": "어디가고싶으세요?",
          "speaker_id": "1",
          "time": "20210828 15:49"
        },
        {
          "id": "MDRW2100021089.1.5",
          "form": "저는크로아티아랑스위스요!!",
          "original_form": "저는크로아티아랑스위스요!!",
          "speaker_id": "2",
          "time": "20210828 15:55"
        },
        {
          "id": "MDRW2100021089.1.6",
          "form": "국내는생각안햐봣는데",
          "original_form": "국내는생각안햐봣는데",
          "speaker_id": "2",
          "time": "20210828 15:55"
        },
        {
          "id": "MDRW2100021089.1.7",
          "form": "안동에뭐있어용??",
          "original_form": "안동에뭐있어용??",
          "speaker_id": "2",
          "time": "20210828 15:56"
        },
        {
          "id": "MDRW2100021089.1.8",
          "form": "안동에 한옥마을도 있고 월영교도 좋고요",
          "original_form": "안동에 한옥마을도 있고 월영교도 좋고요",
          "speaker_id": "1",
          "time": "20210828 15:57"
        },
        {
          "id": "MDRW2100021089.1.9",
          "form": "한적해서 쉬러가기 좋아요",
          "original_form": "한적해서 쉬러가기 좋아요",
          "speaker_id": "1",
          "time": "20210828 15:58"
        },
        {
          "id": "MDRW2100021089.1.10",
          "form": "스위스가보셨어요?",
          "original_form": "스위스가보셨어요?",
          "speaker_id": "1",
          "time": "20210828 15:58"
        },
        {
          "id": "MDRW2100021089.1.11",
          "form": "역시여행은힐링이최고죵",
          "original_form": "역시여행은힐링이최고죵",
          "speaker_id": "2",
          "time": "20210828 16:08"
        },
        {
          "id": "MDRW2100021089.1.12",
          "form": "아니요ㅠㅠㅠ",
          "original_form": "아니요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 16:08"
        },
        {
          "id": "MDRW2100021089.1.13",
          "form": "아직못가봤어요ㅠㅠㅠ",
          "original_form": "아직못가봤어요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 16:08"
        },
        {
          "id": "MDRW2100021089.1.14",
          "form": "아하 그러시구나 다들 코로나 끝나면 추억이였던곳 가고싶어하시길래 물어봤어요",
          "original_form": "아하 그러시구나 다들 코로나 끝나면 추억이였던곳 가고싶어하시길래 물어봤어요",
          "speaker_id": "1",
          "time": "20210828 16:09"
        },
        {
          "id": "MDRW2100021089.1.15",
          "form": "저도 못가봤네요ㅠㅠ",
          "original_form": "저도 못가봤네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 16:09"
        },
        {
          "id": "MDRW2100021089.1.16",
          "form": "제일 최근에 여행 가신곳은 어디예요?",
          "original_form": "제일 최근에 여행 가신곳은 어디예요?",
          "speaker_id": "1",
          "time": "20210828 16:09"
        },
        {
          "id": "MDRW2100021089.1.17",
          "form": "오 추억 있는 곳 가는 것도 좋네요!!",
          "original_form": "오 추억 있는 곳 가는 것도 좋네요!!",
          "speaker_id": "2",
          "time": "20210828 16:15"
        },
        {
          "id": "MDRW2100021089.1.18",
          "form": "가장 최근도 거의 뭐 1년 전이지만",
          "original_form": "가장 최근도 거의 뭐 1년 전이지만",
          "speaker_id": "2",
          "time": "20210828 16:16"
        },
        {
          "id": "MDRW2100021089.1.19",
          "form": "강릉인것같아요ㅎㅎㅎ",
          "original_form": "강릉인것같아요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 16:16"
        },
        {
          "id": "MDRW2100021089.1.20",
          "form": "name1님은요??",
          "original_form": "&name1&님은요??",
          "speaker_id": "2",
          "time": "20210828 16:16"
        },
        {
          "id": "MDRW2100021089.1.21",
          "form": "저는 2년전에 전주요",
          "original_form": "저는 2년전에 전주요",
          "speaker_id": "1",
          "time": "20210828 16:26"
        },
        {
          "id": "MDRW2100021089.1.22",
          "form": "강릉! 어디가셨나요?",
          "original_form": "강릉! 어디가셨나요?",
          "speaker_id": "1",
          "time": "20210828 16:26"
        },
        {
          "id": "MDRW2100021089.1.23",
          "form": "어우 오래되셨네요",
          "original_form": "어우 오래되셨네요",
          "speaker_id": "2",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100021089.1.24",
          "form": "강릉 도깨비 촬영지 갔다 왔어요",
          "original_form": "강릉 도깨비 촬영지 갔다 왔어요",
          "speaker_id": "2",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100021089.1.25",
          "form": "사람도많이없고",
          "original_form": "사람도많이없고",
          "speaker_id": "2",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100021089.1.26",
          "form": "좋더라구용",
          "original_form": "좋더라구용",
          "speaker_id": "2",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100021089.1.27",
          "form": "사람이 많이 없다니 너무 솔깃해요",
          "original_form": "사람이 많이 없다니 너무 솔깃해요",
          "speaker_id": "1",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100021089.1.28",
          "form": "저는 강릉 평창때 다녀오고",
          "original_form": "저는 강릉 평창때 다녀오고",
          "speaker_id": "1",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100021089.1.29",
          "form": "한번도 안가봤거든요",
          "original_form": "한번도 안가봤거든요",
          "speaker_id": "1",
          "time": "20210828 16:40"
        },
        {
          "id": "MDRW2100021089.1.30",
          "form": "진짜 한적하고 좋았어요",
          "original_form": "진짜 한적하고 좋았어요",
          "speaker_id": "2",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100021089.1.31",
          "form": "정보감사해요",
          "original_form": "정보감사해요",
          "speaker_id": "1",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100021089.1.32",
          "form": "코로나 잠잠해지면 가봐야겠어요",
          "original_form": "코로나 잠잠해지면 가봐야겠어요",
          "speaker_id": "1",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100021089.1.33",
          "form": "좋아요!!",
          "original_form": "좋아요!!",
          "speaker_id": "2",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100021089.1.34",
          "form": "딱이날씨에가면",
          "original_form": "딱이날씨에가면",
          "speaker_id": "2",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100021089.1.35",
          "form": "좋을텐데",
          "original_form": "좋을텐데",
          "speaker_id": "2",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100021089.1.36",
          "form": "아쉬워요ㅠㅠ",
          "original_form": "아쉬워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100021089.1.37",
          "form": "그죠 이맘때 여행가기 너무 좋은데ㅠㅠ",
          "original_form": "그죠 이맘때 여행가기 너무 좋은데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 17:00"
        }
