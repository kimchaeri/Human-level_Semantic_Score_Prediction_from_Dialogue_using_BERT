        {
          "id": "MDRW2100018505.1.1",
          "form": "게임해요?",
          "original_form": "게임해요?",
          "speaker_id": "1",
          "time": "20210827 07:01"
        },
        {
          "id": "MDRW2100018505.1.2",
          "form": "아니요 게임하세요",
          "original_form": "아니요 게임하세요",
          "speaker_id": "2",
          "time": "20210827 07:01"
        },
        {
          "id": "MDRW2100018505.1.3",
          "form": "폰겜이요",
          "original_form": "폰겜이요",
          "speaker_id": "1",
          "time": "20210827 07:02"
        },
        {
          "id": "MDRW2100018505.1.4",
          "form": "어떤거 하세요?",
          "original_form": "어떤거 하세요?",
          "speaker_id": "2",
          "time": "20210827 07:02"
        },
        {
          "id": "MDRW2100018505.1.5",
          "form": "브롤이요",
          "original_form": "브롤이요",
          "speaker_id": "1",
          "time": "20210827 07:02"
        },
        {
          "id": "MDRW2100018505.1.6",
          "form": "어떤 게임이에요?",
          "original_form": "어떤 게임이에요?",
          "speaker_id": "2",
          "time": "20210827 07:03"
        },
        {
          "id": "MDRW2100018505.1.7",
          "form": "초딩겜이여",
          "original_form": "초딩겜이여",
          "speaker_id": "1",
          "time": "20210827 07:06"
        },
        {
          "id": "MDRW2100018505.1.8",
          "form": "다른게임은 안하세요?",
          "original_form": "다른게임은 안하세요?",
          "speaker_id": "2",
          "time": "20210827 07:11"
        },
        {
          "id": "MDRW2100018505.1.9",
          "form": "네 이것만",
          "original_form": "네 이것만",
          "speaker_id": "1",
          "time": "20210827 07:11"
        },
        {
          "id": "MDRW2100018505.1.10",
          "form": "주변에서 인기 있는게임은 어떤거에요?",
          "original_form": "주변에서 인기 있는게임은 어떤거에요?",
          "speaker_id": "2",
          "time": "20210827 07:12"
        },
        {
          "id": "MDRW2100018505.1.11",
          "form": "롤많이 하던데",
          "original_form": "롤많이 하던데",
          "speaker_id": "1",
          "time": "20210827 07:14"
        },
        {
          "id": "MDRW2100018505.1.12",
          "form": "많이 들어봤어요",
          "original_form": "많이 들어봤어요",
          "speaker_id": "2",
          "time": "20210827 07:15"
        },
        {
          "id": "MDRW2100018505.1.13",
          "form": "요즘엔 로아?도 많이 하더라구요",
          "original_form": "요즘엔 로아?도 많이 하더라구요",
