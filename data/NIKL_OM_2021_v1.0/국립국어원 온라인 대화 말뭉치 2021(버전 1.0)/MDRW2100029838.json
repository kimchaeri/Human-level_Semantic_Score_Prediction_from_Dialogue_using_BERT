      "utterance": [
        {
          "id": "MDRW2100029838.1.1",
          "form": "동아리 드신 적 있으신가요?",
          "original_form": "동아리 드신 적 있으신가요?",
          "speaker_id": "1",
          "time": "20210903 14:44"
        },
        {
          "id": "MDRW2100029838.1.2",
          "form": "학생 때는 ca? 막 그런거 시켜주잖아요",
          "original_form": "학생 때는 ca? 막 그런거 시켜주잖아요",
          "speaker_id": "2",
          "time": "20210903 14:45"
        },
        {
          "id": "MDRW2100029838.1.3",
          "form": "완전 개꿀 영화감상부",
          "original_form": "완전 개꿀 영화감상부",
          "speaker_id": "2",
          "time": "20210903 14:46"
        },
        {
          "id": "MDRW2100029838.1.4",
          "form": "오 저는 십자수부 였어요",
          "original_form": "오 저는 십자수부 였어요",
          "speaker_id": "1",
          "time": "20210903 14:48"
        },
        {
          "id": "MDRW2100029838.1.5",
          "form": "친구가 같이 하자고 해서",
          "original_form": "친구가 같이 하자고 해서",
          "speaker_id": "1",
          "time": "20210903 14:48"
        },
        {
          "id": "MDRW2100029838.1.6",
          "form": "헉 저두 중딩때 십자수부도 했었어요!",
          "original_form": "헉 저두 중딩때 십자수부도 했었어요!",
          "speaker_id": "2",
          "time": "20210903 14:50"
        },
        {
          "id": "MDRW2100029838.1.7",
          "form": "완전..",
          "original_form": "완전..",
          "speaker_id": "2",
          "time": "20210903 14:50"
        },
        {
          "id": "MDRW2100029838.1.8",
          "form": "뒷면 개판",
          "original_form": "뒷면 개판",
          "speaker_id": "2",
          "time": "20210903 14:50"
        },
        {
          "id": "MDRW2100029838.1.9",
          "form": "아직도 기억나요 앞면 실 두께는 들쭉날쭉하고 뒷면은 오락가락하고 완성은 겨우 했는데 완성 사진과 제거가 달라!",
          "original_form": "아직도 기억나요 앞면 실 두께는 들쭉날쭉하고 뒷면은 오락가락하고 완성은 겨우 했는데 완성 사진과 제거가 달라!",
          "speaker_id": "2",
          "time": "20210903 14:51"
        },
        {
          "id": "MDRW2100029838.1.10",
          "form": "저는 하라는 십자수 안 하고 친구랑 만화책 돌려봤어요",
          "original_form": "저는 하라는 십자수 안 하고 친구랑 만화책 돌려봤어요",
          "speaker_id": "1",
          "time": "20210903 14:51"
        },
        {
          "id": "MDRW2100029838.1.11",
          "form": "역시 똑똑하십니다",
          "original_form": "역시 똑똑하십니다",
          "speaker_id": "2",
          "time": "20210903 14:51"
        },
        {
          "id": "MDRW2100029838.1.12",
          "form": "최고의 선택",
          "original_form": "최고의 선택",
          "speaker_id": "2",
          "time": "20210903 14:51"
        },
        {
          "id": "MDRW2100029838.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그래도 완성하셨잖아요 저보다야 낫죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그래도 완성하셨잖아요 저보다야 낫죠",
          "speaker_id": "1",
          "time": "20210903 14:51"
        },
        {
          "id": "MDRW2100029838.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ겨우 두손만한 크기 반년동안 했는걸요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ겨우 두손만한 크기 반년동안 했는걸요",
          "speaker_id": "2",
          "time": "20210903 14:52"
        },
        {
          "id": "MDRW2100029838.1.15",
          "form": "그정도면 잘 하신 거죸ㅋㅋㅋㅋㅋ손톱보다 작은 스티치로 채우는 거 자나요ㅋㅋㅋ",
          "original_form": "그정도면 잘 하신 거죸ㅋㅋㅋㅋㅋ손톱보다 작은 스티치로 채우는 거 자나요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:52"
        },
        {
          "id": "MDRW2100029838.1.16",
          "form": "잘못해가지고 실 다시 다 풀고ㅜㅜㅠ 엉망진창",
          "original_form": "잘못해가지고 실 다시 다 풀고ㅜㅜㅠ 엉망진창",
          "speaker_id": "2",
          "time": "20210903 14:54"
        },
        {
          "id": "MDRW2100029838.1.17",
          "form": "그뒤로 뭔가 꼬물거리는건 포기했는데",
          "original_form": "그뒤로 뭔가 꼬물거리는건 포기했는데",
          "speaker_id": "2",
          "time": "20210903 14:55"
        },
        {
          "id": "MDRW2100029838.1.18",
          "form": "아까 말씀하신 그림?은 고등학교 때 만화동아리부가 있어서 구경갔는데 신기하더라구요ㅋㅋ",
          "original_form": "아까 말씀하신 그림?은 고등학교 때 만화동아리부가 있어서 구경갔는데 신기하더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:55"
        },
        {
          "id": "MDRW2100029838.1.19",
          "form": "예술의 꿈.. 아직도 꾸는중..(꾸기만 함)",
          "original_form": "예술의 꿈.. 아직도 꾸는중..(꾸기만 함)",
          "speaker_id": "2",
          "time": "20210903 14:56"
        },
        {
          "id": "MDRW2100029838.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요 손으로 뚝딱뚝딱하는 사람들 보면 넘 신기해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아요 손으로 뚝딱뚝딱하는 사람들 보면 넘 신기해요",
          "speaker_id": "1",
          "time": "20210903 14:57"
        },
        {
          "id": "MDRW2100029838.1.21",
          "form": "저도 먼가 미련을 못 버리고 공예 쪽 자꾸 기웃거려요",
          "original_form": "저도 먼가 미련을 못 버리고 공예 쪽 자꾸 기웃거려요",
          "speaker_id": "1",
          "time": "20210903 14:57"
        },
        {
          "id": "MDRW2100029838.1.22",
          "form": "기웃기웃.. 요즘 자수도 유행이길래 기웃기웃..ㅋㅋㅋㅋ",
          "original_form": "기웃기웃.. 요즘 자수도 유행이길래 기웃기웃..ㅋㅋㅋㅋ",
