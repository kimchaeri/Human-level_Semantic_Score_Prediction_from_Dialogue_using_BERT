      "utterance": [
        {
          "id": "MDRW2100014111.1.1",
          "form": "싸워봤어요?",
          "original_form": "싸워봤어요?",
          "speaker_id": "1",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014111.1.2",
          "form": "엇 아뇨 저",
          "original_form": "엇 아뇨 저",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014111.1.3",
          "form": "나이차이가 꽤 많이나서",
          "original_form": "나이차이가 꽤 많이나서",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014111.1.4",
          "form": "복종하며 살았슴다",
          "original_form": "복종하며 살았슴다",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014111.1.5",
          "form": "얼마나 난다고 하셨죠",
          "original_form": "얼마나 난다고 하셨죠",
          "speaker_id": "1",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014111.1.6",
          "form": "8살 나요 name1님은 누나분과 차이 얼마나 나세여?",
          "original_form": "8살 나요 &name1&님은 누나분과 차이 얼마나 나세여?",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014111.1.7",
          "form": "4살차이요",
          "original_form": "4살차이요",
          "speaker_id": "1",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014111.1.8",
          "form": "좀 나는 편이시네요",
          "original_form": "좀 나는 편이시네요",
          "speaker_id": "2",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014111.1.9",
          "form": "8살 차이면.. 대들기도 무섭긴 하겠다",
          "original_form": "8살 차이면.. 대들기도 무섭긴 하겠다",
          "speaker_id": "1",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014111.1.10",
          "form": "어우 글고 오빠다보니",
          "original_form": "어우 글고 오빠다보니",
          "speaker_id": "2",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014111.1.11",
          "form": "절대로",
          "original_form": "절대로",
          "speaker_id": "2",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014111.1.12",
          "form": "근데 이제 맞으면 바로 엄마한테 달려가죠",
          "original_form": "근데 이제 맞으면 바로 엄마한테 달려가죠",
          "speaker_id": "2",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014111.1.13",
          "form": "우리는 둘 다 성격이 거지같아서 둘 중 하나가 사라지기 직전까지 싸웠거든요",
          "original_form": "우리는 둘 다 성격이 거지같아서 둘 중 하나가 사라지기 직전까지 싸웠거든요",
          "speaker_id": "1",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014111.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014111.1.15",
          "form": "달려가면 어머니는 뭐라 하시던가요",
          "original_form": "달려가면 어머니는 뭐라 하시던가요",
          "speaker_id": "1",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014111.1.16",
          "form": "누가 이기는 편이었죠?!",
          "original_form": "누가 이기는 편이었죠?!",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014111.1.17",
          "form": "오빠 혼냈슴다",
          "original_form": "오빠 혼냈슴다",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014111.1.18",
          "form": "동생 패는 거 아니라고",
          "original_form": "동생 패는 거 아니라고",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014111.1.19",
          "form": "저는 제가 졌어요",
          "original_form": "저는 제가 졌어요",
          "speaker_id": "1",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014111.1.20",
          "form": "누나가 디지게 때려서",
          "original_form": "누나가 디지게 때려서",
          "speaker_id": "1",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014111.1.21",
          "form": "부모님한테 일러보신 적은 없으신가요",
          "original_form": "부모님한테 일러보신 적은 없으신가요",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014111.1.22",
          "form": "이르면",
          "original_form": "이르면",
          "speaker_id": "1",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014111.1.23",
          "form": "누나좀 잡아달라고",
          "original_form": "누나좀 잡아달라고",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014111.1.24",
          "form": "더맞아써요",
          "original_form": "더맞아써요",
          "speaker_id": "1",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014111.1.25",
          "form": "ㅇㄴ 강하게 키우시네",
          "original_form": "ㅇㄴ 강하게 키우시네",
          "speaker_id": "2",
          "time": "20210825 17:21"
        }
      ]
    }
