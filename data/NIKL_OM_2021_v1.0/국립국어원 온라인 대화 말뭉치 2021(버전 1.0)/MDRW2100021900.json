      "utterance": [
        {
          "id": "MDRW2100021900.1.1",
          "form": "오로바시 같이 무찌른? 해적 스토리",
          "original_form": "오로바시 같이 무찌른? 해적 스토리",
          "speaker_id": "2",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100021900.1.2",
          "form": "발견했어",
          "original_form": "발견했어",
          "speaker_id": "2",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100021900.1.3",
          "form": "하지만 게임을 하면서 보상을 포기할 순 없다구",
          "original_form": "하지만 게임을 하면서 보상을 포기할 순 없다구",
          "speaker_id": "1",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100021900.1.4",
          "form": "몬드출신이래",
          "original_form": "몬드출신이래",
          "speaker_id": "2",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100021900.1.5",
          "form": "몰락한 마음이라는데",
          "original_form": "몰락한 마음이라는데",
          "speaker_id": "2",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100021900.1.6",
          "form": "이거 맞는것같아",
          "original_form": "이거 맞는것같아",
          "speaker_id": "2",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100021900.1.7",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "1",
          "time": "20210828 23:41"
        },
        {
          "id": "MDRW2100021900.1.8",
          "form": "그런듯",
          "original_form": "그런듯",
          "speaker_id": "1",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100021900.1.9",
          "form": "바다괴수랑 맞서 싸우다 죽은 해적이면",
          "original_form": "바다괴수랑 맞서 싸우다 죽은 해적이면",
          "speaker_id": "2",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100021900.1.10",
          "form": "그거밖에 없지",
          "original_form": "그거밖에 없지",
          "speaker_id": "2",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100021900.1.11",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100021900.1.12",
          "form": "그리고",
          "original_form": "그리고",
          "speaker_id": "1",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100021900.1.13",
          "form": "제빙의 관 알지?",
          "original_form": "제빙의 관 알지?",
          "speaker_id": "1",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100021900.1.14",
          "form": "그 시리즈는 캐릭터가 아니라 문명에 대한 떡밥임",
          "original_form": "그 시리즈는 캐릭터가 아니라 문명에 대한 떡밥임",
          "speaker_id": "1",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100021900.1.15",
          "form": "근데 좀 난해하긴 하다....",
          "original_form": "근데 좀 난해하긴 하다....",
          "speaker_id": "2",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100021900.1.16",
          "form": "걍 풀어달라....",
          "original_form": "걍 풀어달라....",
          "speaker_id": "2",
          "time": "20210828 23:44"
        },
        {
          "id": "MDRW2100021900.1.17",
          "form": "ㄱㄴㄲ",
          "original_form": "ㄱㄴㄲ",
          "speaker_id": "1",
          "time": "20210828 23:44"
        },
        {
          "id": "MDRW2100021900.1.18",
          "form": "이거 좀 공식에서",
          "original_form": "이거 좀 공식에서",
          "speaker_id": "1",
          "time": "20210828 23:45"
        },
        {
          "id": "MDRW2100021900.1.19",
          "form": "깔끔하게 정리해줬음 좋겠어",
          "original_form": "깔끔하게 정리해줬음 좋겠어",
          "speaker_id": "1",
          "time": "20210828 23:45"
        },
        {
          "id": "MDRW2100021900.1.20",
          "form": "ㅇㅇ.......",
          "original_form": "ㅇㅇ.......",
          "speaker_id": "2",
          "time": "20210828 23:45"
        },
        {
          "id": "MDRW2100021900.1.21",
          "form": "저 제빙의 관 시리즈가",
          "original_form": "저 제빙의 관 시리즈가",
          "speaker_id": "1",
          "time": "20210828 23:45"
        },
        {
          "id": "MDRW2100021900.1.22",
          "form": "딱 네개만 있거든",
          "original_form": "딱 네개만 있거든",
          "speaker_id": "1",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100021900.1.23",
          "form": "제빙 제화 제수 제뢰고",
          "original_form": "제빙 제화 제수 제뢰고",
          "speaker_id": "1",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100021900.1.24",
          "form": "순서대로 문명을 멸망시키는데 썼던 원소임",
          "original_form": "순서대로 문명을 멸망시키는데 썼던 원소임",
          "speaker_id": "1",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100021900.1.25",
          "form": "그럼 벤티랑 종려는?",
          "original_form": "그럼 벤티랑 종려는?",
          "speaker_id": "2",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100021900.1.26",
          "form": "게임 설정 상 대륙에는 지금 도시 외에",
          "original_form": "게임 설정 상 대륙에는 지금 도시 외에",
          "speaker_id": "1",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100021900.1.27",
          "form": "문명이 4개가 더 존재했도",
          "original_form": "문명이 4개가 더 존재했도",
          "speaker_id": "1",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100021900.1.28",
          "form": "그중 가장 마지막 문명이 켄리아",
          "original_form": "그중 가장 마지막 문명이 켄리아",
          "speaker_id": "1",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100021900.1.29",
          "form": "벤티랑 종려는 2~3 사이로 추청함",
          "original_form": "벤티랑 종려는 2~3 사이로 추청함",
          "speaker_id": "1",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100021900.1.30",
          "form": "왜냐면 최초의 문명은",
          "original_form": "왜냐면 최초의 문명은",
          "speaker_id": "1",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100021900.1.31",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100021900.1.32",
          "form": "셀레스티아의 의지를 직접 들을 수 있다고 표현되어있는데",
          "original_form": "셀레스티아의 의지를 직접 들을 수 있다고 표현되어있는데",
          "speaker_id": "1",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100021900.1.33",
          "form": "켄리아만 멸망시킨게 아니야?",
          "original_form": "켄리아만 멸망시킨게 아니야?",
          "speaker_id": "2",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100021900.1.34",
          "form": "이 얘기는 신이 없단 소리",
          "original_form": "이 얘기는 신이 없단 소리",
          "speaker_id": "1",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100021900.1.35",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100021900.1.36",
          "form": "대표적으로 게임에서 확인되는 선령도",
          "original_form": "대표적으로 게임에서 확인되는 선령도",
          "speaker_id": "1",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100021900.1.37",
          "form": "??",
          "original_form": "??",
          "speaker_id": "2",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100021900.1.38",
          "form": "멸망한 문명의 사람들이 영혼만 남아서 떠도는거임",
          "original_form": "멸망한 문명의 사람들이 영혼만 남아서 떠도는거임",
          "speaker_id": "1",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100021900.1.39",
          "form": "선령에 설정이란게",
          "original_form": "선령에 설정이란게",
          "speaker_id": "2",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100021900.1.40",
          "form": "있었어????",
          "original_form": "있었어????",
          "speaker_id": "2",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100021900.1.41",
          "form": "ㅇㅇ ㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇ ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100021900.1.42",
          "form": "플레이 하다보면",
          "original_form": "플레이 하다보면",
          "speaker_id": "1",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100021900.1.43",
          "form": "중간중간 워프할 때 간단한 설정 설명 뜨잖아",
          "original_form": "중간중간 워프할 때 간단한 설정 설명 뜨잖아",
          "speaker_id": "1",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100021900.1.44",
          "form": "그중에 선령에 대한 것도 있음",
          "original_form": "그중에 선령에 대한 것도 있음",
          "speaker_id": "1",
          "time": "20210828 23:48"
        }
      ]
