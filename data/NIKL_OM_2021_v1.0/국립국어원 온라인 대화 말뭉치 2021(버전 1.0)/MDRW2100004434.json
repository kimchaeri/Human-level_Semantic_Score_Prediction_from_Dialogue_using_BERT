      "utterance": [
        {
          "id": "MDRW2100004434.1.1",
          "form": "그러게 한여름 더위에서는 면종류가 최고지",
          "original_form": "그러게 한여름 더위에서는 면종류가 최고지",
          "speaker_id": "2",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004434.1.2",
          "form": "비가 오니 라면 땡기네",
          "original_form": "비가 오니 라면 땡기네",
          "speaker_id": "1",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004434.1.3",
          "form": "손호준 보조경력이 많아져서 이제 프로보조란다",
          "original_form": "손호준 보조경력이 많아져서 이제 프로보조란다",
          "speaker_id": "2",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004434.1.4",
          "form": "그때 대게 보조 잘 했던걸로 기억난다",
          "original_form": "그때 대게 보조 잘 했던걸로 기억난다",
          "speaker_id": "1",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004434.1.5",
          "form": "나 라면 어제 먹었음 냉장고가 텅비어서",
          "original_form": "나 라면 어제 먹었음 냉장고가 텅비어서",
          "speaker_id": "2",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004434.1.6",
          "form": "배달 안시켜 먹고?",
          "original_form": "배달 안시켜 먹고?",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004434.1.7",
          "form": "점심으로 저녁은 당연히 시켜먹었지 뽈찜",
          "original_form": "점심으로 저녁은 당연히 시켜먹었지 뽈찜",
          "speaker_id": "2",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004434.1.8",
          "form": "점심 뽈찜 저녁 라면이네",
          "original_form": "점심 뽈찜 저녁 라면이네",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004434.1.9",
          "form": "불맛 메밀맛이란다 ㅋㅋ",
          "original_form": "불맛 메밀맛이란다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004434.1.10",
          "form": "그런거까지 할수 있네",
          "original_form": "그런거까지 할수 있네",
          "speaker_id": "1",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004434.1.11",
          "form": "근데 차승원 손 보면 항상 다쳐있다",
          "original_form": "근데 차승원 손 보면 항상 다쳐있다",
          "speaker_id": "2",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004434.1.12",
          "form": "칼에 비었나?",
          "original_form": "칼에 비었나?",
          "speaker_id": "1",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004434.1.13",
          "form": "밴드 감은 손이 계속 보임",
          "original_form": "밴드 감은 손이 계속 보임",
          "speaker_id": "2",
          "time": "20210821 16:30"
        },
        {
          "id": "MDRW2100004434.1.14",
          "form": "일을 많이 해서 다쳤는가 보네",
          "original_form": "일을 많이 해서 다쳤는가 보네",
          "speaker_id": "1",
          "time": "20210821 16:30"
        },
        {
          "id": "MDRW2100004434.1.15",
          "form": "비었거다 데였거나 했을거 같다 너무 빨리 하다보니 그런가봐",
          "original_form": "비었거다 데였거나 했을거 같다 너무 빨리 하다보니 그런가봐",
          "speaker_id": "2",
          "time": "20210821 16:30"
        }
      ]
