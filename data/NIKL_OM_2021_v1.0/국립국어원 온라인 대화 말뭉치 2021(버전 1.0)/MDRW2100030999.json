        {
          "id": "MDRW2100030999.1.1",
          "form": "중고거래해요?",
          "original_form": "중고거래해요?",
          "speaker_id": "1",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100030999.1.2",
          "form": "아니요! 잘 안해요",
          "original_form": "아니요! 잘 안해요",
          "speaker_id": "2",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100030999.1.3",
          "form": "전 인터넷쇼핑 자주해요",
          "original_form": "전 인터넷쇼핑 자주해요",
          "speaker_id": "2",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100030999.1.4",
          "form": "전 당근",
          "original_form": "전 당근",
          "speaker_id": "1",
          "time": "20210904 15:18"
        },
        {
          "id": "MDRW2100030999.1.5",
          "form": "저희 부모님도 당근자주하셔요",
          "original_form": "저희 부모님도 당근자주하셔요",
          "speaker_id": "2",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100030999.1.6",
          "form": "무료나눔도 자주하고",
          "original_form": "무료나눔도 자주하고",
          "speaker_id": "2",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100030999.1.7",
          "form": "좋네오",
          "original_form": "좋네오",
          "speaker_id": "1",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100030999.1.8",
          "form": "무료나눔하면",
          "original_form": "무료나눔하면",
          "speaker_id": "2",
          "time": "20210904 15:21"
        },
        {
          "id": "MDRW2100030999.1.9",
          "form": "상대측에서도 간단하게 나누실것 가져오시는 분도 있더라고요",
          "original_form": "상대측에서도 간단하게 나누실것 가져오시는 분도 있더라고요",
          "speaker_id": "2",
          "time": "20210904 15:21"
        },
        {
          "id": "MDRW2100030999.1.10",
          "form": "아직은 따뜻한 세상이에요",
          "original_form": "아직은 따뜻한 세상이에요",
          "speaker_id": "2",
          "time": "20210904 15:21"
        },
        {
          "id": "MDRW2100030999.1.11",
          "form": "그렇더라구요",
          "original_form": "그렇더라구요",
          "speaker_id": "1",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100030999.1.12",
          "form": "저는 얼마전에주문한",
          "original_form": "저는 얼마전에주문한",
          "speaker_id": "2",
          "time": "20210904 15:27"
        },
        {
          "id": "MDRW2100030999.1.13",
          "form": "아이보리색 운동화가 빨리 왔으면 좋겠어요",
          "original_form": "아이보리색 운동화가 빨리 왔으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210904 15:27"
        },
        {
          "id": "MDRW2100030999.1.14",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210904 15:28"
        },
        {
          "id": "MDRW2100030999.1.15",
          "form": "헉 배송이왔는데",
          "original_form": "헉 배송이왔는데",
          "speaker_id": "2",
          "time": "20210904 15:30"
        },
        {
          "id": "MDRW2100030999.1.16",
          "form": "까먹고안찾아왔던거였어오",
          "original_form": "까먹고안찾아왔던거였어오",
          "speaker_id": "2",
          "time": "20210904 15:30"
        },
        {
          "id": "MDRW2100030999.1.17",
          "form": "얼른 가요",
          "original_form": "얼른 가요",
          "speaker_id": "1",
          "time": "20210904 15:35"
        },
        {
          "id": "MDRW2100030999.1.18",
          "form": "오늘 나갔다 들어오면서 찾아와야겠어요!!",
