      "utterance": [
        {
          "id": "MDRW2100030062.1.1",
          "form": "백신 맞으셨어요?",
          "original_form": "백신 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210903 20:26"
        },
        {
          "id": "MDRW2100030062.1.2",
          "form": "전 이미 2차까지 맞았거든요",
          "original_form": "전 이미 2차까지 맞았거든요",
          "speaker_id": "2",
          "time": "20210903 20:26"
        },
        {
          "id": "MDRW2100030062.1.3",
          "form": "제가 잘못",
          "original_form": "제가 잘못",
          "speaker_id": "1",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100030062.1.4",
          "form": "맞으셨나요?",
          "original_form": "맞으셨나요?",
          "speaker_id": "2",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100030062.1.5",
          "form": "네..",
          "original_form": "네..",
          "speaker_id": "1",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100030062.1.6",
          "form": "학교는 초비상사태라",
          "original_form": "학교는 초비상사태라",
          "speaker_id": "1",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100030062.1.7",
          "form": "2차까지 다요?",
          "original_form": "2차까지 다요?",
          "speaker_id": "2",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100030062.1.8",
          "form": "전 얀센요",
          "original_form": "전 얀센요",
          "speaker_id": "1",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100030062.1.9",
          "form": "한번만 맞으면 되잖아요",
          "original_form": "한번만 맞으면 되잖아요",
          "speaker_id": "2",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100030062.1.10",
          "form": "얀센도 맞도 열나고 그러나요?",
          "original_form": "얀센도 맞도 열나고 그러나요?",
          "speaker_id": "2",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100030062.1.11",
          "form": "네..한번 맞았어요",
          "original_form": "네..한번 맞았어요",
          "speaker_id": "1",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100030062.1.12",
          "form": "전 화이자 2차는 각오하라고 해서 엄청 겁 먹었는데 정말 아무렇지도 않았어요",
          "original_form": "전 화이자 2차는 각오하라고 해서 엄청 겁 먹었는데 정말 아무렇지도 않았어요",
          "speaker_id": "2",
          "time": "20210903 20:30"
        },
        {
          "id": "MDRW2100030062.1.13",
          "form": "저도 하나도 안아팠네요",
          "original_form": "저도 하나도 안아팠네요",
          "speaker_id": "1",
          "time": "20210903 20:30"
        },
        {
          "id": "MDRW2100030062.1.14",
          "form": "다행이세요 .",
          "original_form": "다행이세요 .",
          "speaker_id": "2",
          "time": "20210903 20:31"
        },
        {
          "id": "MDRW2100030062.1.15",
          "form": "저희 신랑은 10부제로 예약했는데 이게 자리가 없던데요 ㅡ.ㅡ",
          "original_form": "저희 신랑은 10부제로 예약했는데 이게 자리가 없던데요 ㅡ.ㅡ",
          "speaker_id": "2",
          "time": "20210903 20:31"
        },
        {
          "id": "MDRW2100030062.1.16",
          "form": "그래요?",
          "original_form": "그래요?",
          "speaker_id": "1",
          "time": "20210903 20:32"
        },
        {
          "id": "MDRW2100030062.1.17",
          "form": "저는 일찍 맞다보니 잘몰랐네요",
          "original_form": "저는 일찍 맞다보니 잘몰랐네요",
          "speaker_id": "1",
          "time": "20210903 20:32"
        },
        {
          "id": "MDRW2100030062.1.18",
          "form": "신랑이 평일엔 지방에 있는데 일하는 곳에서 맞으려니 자리가 없어요 ㅡ.ㅡ",
          "original_form": "신랑이 평일엔 지방에 있는데 일하는 곳에서 맞으려니 자리가 없어요 ㅡ.ㅡ",
          "speaker_id": "2",
          "time": "20210903 20:32"
        },
        {
          "id": "MDRW2100030062.1.19",
          "form": "그래서 연가내고 집쪽으로 올라와서 맞았어요",
          "original_form": "그래서 연가내고 집쪽으로 올라와서 맞았어요",
          "speaker_id": "2",
          "time": "20210903 20:32"
        },
        {
          "id": "MDRW2100030062.1.20",
          "form": "다들많이 맞아서 빨리 일상을 찾고싶네요",
          "original_form": "다들많이 맞아서 빨리 일상을 찾고싶네요",
          "speaker_id": "1",
          "time": "20210903 20:33"
        }
