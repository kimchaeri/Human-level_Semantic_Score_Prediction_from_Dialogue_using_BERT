      "utterance": [
        {
          "id": "MDRW2100046909.1.1",
          "form": "오징어 게임 봤어?",
          "original_form": "오징어 게임 봤어?",
          "speaker_id": "1",
          "time": "20211005 21:08"
        },
        {
          "id": "MDRW2100046909.1.2",
          "form": "어 봤어",
          "original_form": "어 봤어",
          "speaker_id": "2",
          "time": "20211005 21:08"
        },
        {
          "id": "MDRW2100046909.1.3",
          "form": "감상이 어땠어?",
          "original_form": "감상이 어땠어?",
          "speaker_id": "1",
          "time": "20211005 21:08"
        },
        {
          "id": "MDRW2100046909.1.4",
          "form": "좀 잔인한 장면이 많긴한데",
          "original_form": "좀 잔인한 장면이 많긴한데",
          "speaker_id": "2",
          "time": "20211005 21:09"
        },
        {
          "id": "MDRW2100046909.1.5",
          "form": "웃긴 장면들도 많아서 재밌었던거 같아",
          "original_form": "웃긴 장면들도 많아서 재밌었던거 같아",
          "speaker_id": "2",
          "time": "20211005 21:09"
        },
        {
          "id": "MDRW2100046909.1.6",
          "form": "너는 어때?",
          "original_form": "너는 어때?",
          "speaker_id": "2",
          "time": "20211005 21:09"
        },
        {
          "id": "MDRW2100046909.1.7",
          "form": "난 아직 보진 않았어, 미국 반응도 봤어?",
          "original_form": "난 아직 보진 않았어, 미국 반응도 봤어?",
          "speaker_id": "1",
          "time": "20211005 21:09"
        },
        {
          "id": "MDRW2100046909.1.8",
          "form": "아니 미국 반응은 어떤데?",
          "original_form": "아니 미국 반응은 어떤데?",
          "speaker_id": "2",
          "time": "20211005 21:10"
        },
        {
          "id": "MDRW2100046909.1.9",
          "form": "달고나를 만들거나 유니폼을 구매하더라고",
          "original_form": "달고나를 만들거나 유니폼을 구매하더라고",
          "speaker_id": "1",
          "time": "20211005 21:10"
        },
        {
          "id": "MDRW2100046909.1.10",
          "form": "와우 파급력이 정말 대단하구나!",
          "original_form": "와우 파급력이 정말 대단하구나!",
          "speaker_id": "2",
          "time": "20211005 21:11"
        },
        {
          "id": "MDRW2100046909.1.11",
          "form": "너는 어떤 경로로 오징어게임을 봤어?",
          "original_form": "너는 어떤 경로로 오징어게임을 봤어?",
          "speaker_id": "1",
          "time": "20211005 21:11"
        },
        {
          "id": "MDRW2100046909.1.12",
          "form": "나는 유튜브를 통해 먼저 접했어",
          "original_form": "나는 유튜브를 통해 먼저 접했어",
          "speaker_id": "2",
          "time": "20211005 21:12"
        },
        {
          "id": "MDRW2100046909.1.13",
          "form": "소개해주는 유튜버들 영상 보고 찾아봤어",
          "original_form": "소개해주는 유튜버들 영상 보고 찾아봤어",
          "speaker_id": "2",
          "time": "20211005 21:12"
        },
        {
          "id": "MDRW2100046909.1.14",
          "form": "클립의 형태로 본거야?",
          "original_form": "클립의 형태로 본거야?",
          "speaker_id": "1",
          "time": "20211005 21:12"
        },
        {
          "id": "MDRW2100046909.1.15",
          "form": "어떤 유튜버 소개를 봤어?",
          "original_form": "어떤 유튜버 소개를 봤어?",
          "speaker_id": "1",
          "time": "20211005 21:12"
        },
        {
          "id": "MDRW2100046909.1.16",
          "form": "음....그건 잘 모르겠어",
          "original_form": "음....그건 잘 모르겠어",
          "speaker_id": "2",
          "time": "20211005 21:13"
        },
        {
          "id": "MDRW2100046909.1.17",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20211005 21:13"
        },
        {
          "id": "MDRW2100046909.1.18",
          "form": "알고리즘에 뜬거라",
          "original_form": "알고리즘에 뜬거라",
          "speaker_id": "2",
          "time": "20211005 21:13"
        }
