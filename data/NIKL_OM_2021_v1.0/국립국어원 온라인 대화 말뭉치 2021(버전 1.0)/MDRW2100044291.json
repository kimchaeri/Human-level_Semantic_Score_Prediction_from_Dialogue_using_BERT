      "utterance": [
        {
          "id": "MDRW2100044291.1.1",
          "form": "내일은 실내업무는 두시간이지?",
          "original_form": "내일은 실내업무는 두시간이지?",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044291.1.2",
          "form": "이것도 일이네",
          "original_form": "이것도 일이네",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044291.1.3",
          "form": "타이핑하기",
          "original_form": "타이핑하기",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044291.1.4",
          "form": "실내업무는 4시간",
          "original_form": "실내업무는 4시간",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044291.1.5",
          "form": "오후는 실내업무니까",
          "original_form": "오후는 실내업무니까",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044291.1.6",
          "form": "부업이지",
          "original_form": "부업이지",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044291.1.7",
          "form": "부업..",
          "original_form": "부업..",
          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044291.1.8",
          "form": "내가 물어온 부업에 나보다 열심이었던 메이님",
          "original_form": "내가 물어온 부업에 나보다 열심이었던 메이님",
          "speaker_id": "1",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044291.1.9",
          "form": "열심히 하자",
          "original_form": "열심히 하자",
          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044291.1.10",
          "form": "장하다",
          "original_form": "장하다",
          "speaker_id": "1",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044291.1.11",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044291.1.12",
          "form": "부업체질인가봐",
          "original_form": "부업체질인가봐",
          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044291.1.13",
          "form": "언니 얼른 먼저 부업 끝내고 쉬어",
          "original_form": "언니 얼른 먼저 부업 끝내고 쉬어",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044291.1.14",
          "form": "응 마지막 몇 개 봉투 붙이는게",
          "original_form": "응 마지막 몇 개 봉투 붙이는게",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044291.1.15",
          "form": "은근히 안끝나네",
          "original_form": "은근히 안끝나네",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044291.1.16",
          "form": "힘들지",
          "original_form": "힘들지",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044291.1.17",
          "form": "응 마지막  고비야",
          "original_form": "응 마지막  고비야",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044291.1.18",
          "form": "급여를 위한",
          "original_form": "급여를 위한",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044291.1.19",
          "form": "시간에 쫓기는게 힘들지뭐",
          "original_form": "시간에 쫓기는게 힘들지뭐",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044291.1.20",
          "form": "급여를 위해 흑흑",
          "original_form": "급여를 위해 흑흑",
          "speaker_id": "1",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044291.1.21",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044291.1.22",
          "form": "갑자기 업무 마감이 당겨져서",
          "original_form": "갑자기 업무 마감이 당겨져서",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044291.1.23",
          "form": "봉투 오백장 붙인 사람들 정말 대단하네",
          "original_form": "봉투 오백장 붙인 사람들 정말 대단하네",
          "speaker_id": "1",
          "time": "20210907 22:15"
        }
