        },
        {
          "id": "MDRW2100037022.1.2",
          "form": "와인 24 아세요?",
          "original_form": "와인 24 아세요?",
          "speaker_id": "1",
          "time": "20210831 17:40"
        },
        {
          "id": "MDRW2100037022.1.3",
          "form": "네 알아요..",
          "original_form": "네 알아요..",
          "speaker_id": "2",
          "time": "20210831 17:43"
        },
        {
          "id": "MDRW2100037022.1.4",
          "form": "사용하시나요?",
          "original_form": "사용하시나요?",
          "speaker_id": "1",
          "time": "20210831 17:43"
        },
        {
          "id": "MDRW2100037022.1.5",
          "form": "네...알죠",
          "original_form": "네...알죠",
          "speaker_id": "2",
          "time": "20210831 17:44"
        },
        {
          "id": "MDRW2100037022.1.6",
          "form": "오늘까지 할인 많이 되는 쿠폰이 있어요",
          "original_form": "오늘까지 할인 많이 되는 쿠폰이 있어요",
          "speaker_id": "1",
          "time": "20210831 17:44"
        },
        {
          "id": "MDRW2100037022.1.7",
          "form": "오 ..맞다!!!잊엇네요",
          "original_form": "오 ..맞다!!!잊엇네요",
          "speaker_id": "2",
          "time": "20210831 17:46"
        },
        {
          "id": "MDRW2100037022.1.8",
          "form": "5천원 할인되니까 꿀이에뇨",
          "original_form": "5천원 할인되니까 꿀이에뇨",
          "speaker_id": "1",
          "time": "20210831 17:52"
        },
        {
          "id": "MDRW2100037022.1.9",
          "form": "아 맞아요 ㅜㅜㅜㅜ 이제 6시간정도 남았으니 올른 결제해야겟어욥ㅋㅋㅋ",
          "original_form": "아 맞아요 ㅜㅜㅜㅜ 이제 6시간정도 남았으니 올른 결제해야겟어욥ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:53"
        },
        {
          "id": "MDRW2100037022.1.10",
          "form": "네 그게 바틀 준다고도 해요",
          "original_form": "네 그게 바틀 준다고도 해요",
          "speaker_id": "1",
          "time": "20210831 17:55"
        },
        {
          "id": "MDRW2100037022.1.11",
          "form": "오..그렇군요. 저도 탐나네요 받고싶당",
          "original_form": "오..그렇군요. 저도 탐나네요 받고싶당",
          "speaker_id": "2",
          "time": "20210831 17:56"
        },
        {
          "id": "MDRW2100037022.1.12",
          "form": "그바틀 과연 받을수있을진 모르지만..",
          "original_form": "그바틀 과연 받을수있을진 모르지만..",
