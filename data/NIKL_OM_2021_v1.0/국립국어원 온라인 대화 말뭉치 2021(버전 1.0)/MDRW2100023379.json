      "utterance": [
        {
          "id": "MDRW2100023379.1.1",
          "form": "안녕하세용 선호하는 치킨 브랜드 있으세용?",
          "original_form": "안녕하세용 선호하는 치킨 브랜드 있으세용?",
          "speaker_id": "1",
          "time": "20210830 01:09"
        },
        {
          "id": "MDRW2100023379.1.2",
          "form": "아뇨 ㅠㅠ 친구가 지코바 추천해줬는데",
          "original_form": "아뇨 ㅠㅠ 친구가 지코바 추천해줬는데",
          "speaker_id": "2",
          "time": "20210830 01:10"
        },
        {
          "id": "MDRW2100023379.1.3",
          "form": "앗 그거 훈제? 치킨으로 유명하더라구용!",
          "original_form": "앗 그거 훈제? 치킨으로 유명하더라구용!",
          "speaker_id": "1",
          "time": "20210830 01:10"
        },
        {
          "id": "MDRW2100023379.1.4",
          "form": "저는 튀긴음식을.. 많이먹는 편은 아니라성",
          "original_form": "저는 튀긴음식을.. 많이먹는 편은 아니라성",
          "speaker_id": "2",
          "time": "20210830 01:10"
        },
        {
          "id": "MDRW2100023379.1.5",
          "form": "맞아여!",
          "original_form": "맞아여!",
          "speaker_id": "2",
          "time": "20210830 01:10"
        },
        {
          "id": "MDRW2100023379.1.6",
          "form": "아하ㅠㅠ 건강식 위주로 드시나요?",
          "original_form": "아하ㅠㅠ 건강식 위주로 드시나요?",
          "speaker_id": "1",
          "time": "20210830 01:10"
        },
        {
          "id": "MDRW2100023379.1.7",
          "form": "저두그거듣구 궁금했는데",
          "original_form": "저두그거듣구 궁금했는데",
          "speaker_id": "2",
          "time": "20210830 01:10"
        },
        {
          "id": "MDRW2100023379.1.8",
          "form": "건강챙기는 사람은 아닌데 대학생때 너무 많이먹어서 질렸어요ㅋㅋ",
          "original_form": "건강챙기는 사람은 아닌데 대학생때 너무 많이먹어서 질렸어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 01:11"
        },
        {
          "id": "MDRW2100023379.1.9",
          "form": "ㅎㅎ 전 아직 시도는 못해봤어용",
          "original_form": "ㅎㅎ 전 아직 시도는 못해봤어용",
          "speaker_id": "1",
          "time": "20210830 01:11"
        },
        {
          "id": "MDRW2100023379.1.10",
          "form": "아하ㅠㅠ",
          "original_form": "아하ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 01:11"
        },
        {
          "id": "MDRW2100023379.1.11",
          "form": "어제는 뭐 드셨어용?",
          "original_form": "어제는 뭐 드셨어용?",
          "speaker_id": "1",
          "time": "20210830 01:11"
        },
        {
          "id": "MDRW2100023379.1.12",
          "form": "어제?? 는 모르겠구 오늘은 말할거 있어욥!!",
          "original_form": "어제?? 는 모르겠구 오늘은 말할거 있어욥!!",
          "speaker_id": "2",
          "time": "20210830 01:11"
        },
        {
          "id": "MDRW2100023379.1.13",
          "form": "이번에 이차돌 네고왕",
          "original_form": "이번에 이차돌 네고왕",
          "speaker_id": "2",
          "time": "20210830 01:12"
        },
        {
          "id": "MDRW2100023379.1.14",
          "form": "하는거 보셨어요?",
          "original_form": "하는거 보셨어요?",
          "speaker_id": "2",
          "time": "20210830 01:12"
        },
        {
          "id": "MDRW2100023379.1.15",
          "form": "앗 네네!",
          "original_form": "앗 네네!",
          "speaker_id": "1",
          "time": "20210830 01:12"
        },
        {
          "id": "MDRW2100023379.1.16",
          "form": "특양이 양고기맞아용?",
          "original_form": "특양이 양고기맞아용?",
          "speaker_id": "1",
          "time": "20210830 01:12"
        },
        {
          "id": "MDRW2100023379.1.17",
          "form": "저 갈까말까 고민중이에요ㅋㅋㅋ",
          "original_form": "저 갈까말까 고민중이에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:12"
        },
        {
          "id": "MDRW2100023379.1.18",
          "form": "오늘은 그거 먹었어요~~ 저는 배달 ㅎㅎ",
          "original_form": "오늘은 그거 먹었어요~~ 저는 배달 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:12"
        },
        {
          "id": "MDRW2100023379.1.19",
          "form": "님은 머드셨어요?",
          "original_form": "님은 머드셨어요?",
          "speaker_id": "2",
          "time": "20210830 01:12"
        },
        {
          "id": "MDRW2100023379.1.20",
          "form": "그럼 쫄면이랑 같이 드셨겠네요!",
          "original_form": "그럼 쫄면이랑 같이 드셨겠네요!",
          "speaker_id": "1",
          "time": "20210830 01:12"
        },
        {
          "id": "MDRW2100023379.1.21",
          "form": "저는 치킨이랑 부대찌개 먹었어요ㅎㅎ",
          "original_form": "저는 치킨이랑 부대찌개 먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:12"
        },
        {
          "id": "MDRW2100023379.1.22",
          "form": "마자욥!! 아 거기 쫄면 ㅜㅜ 대박",
          "original_form": "마자욥!! 아 거기 쫄면 ㅜㅜ 대박",
          "speaker_id": "2",
          "time": "20210830 01:13"
        },
        {
          "id": "MDRW2100023379.1.23",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210830 01:13"
        },
        {
          "id": "MDRW2100023379.1.24",
          "form": "치킨 어디꺼 드셨어요?",
          "original_form": "치킨 어디꺼 드셨어요?",
          "speaker_id": "2",
          "time": "20210830 01:13"
        },
        {
          "id": "MDRW2100023379.1.25",
          "form": "오 맛있구나 저도 배달로 시켜볼까봐요ㅋㅋㅋ",
          "original_form": "오 맛있구나 저도 배달로 시켜볼까봐요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:13"
        },
        {
          "id": "MDRW2100023379.1.26",
          "form": "저 치킨매니아 새우치킨",
          "original_form": "저 치킨매니아 새우치킨",
          "speaker_id": "1",
          "time": "20210830 01:13"
        },
        {
          "id": "MDRW2100023379.1.27",
          "form": "좋아해용",
          "original_form": "좋아해용",
          "speaker_id": "1",
          "time": "20210830 01:13"
        },
        {
          "id": "MDRW2100023379.1.28",
          "form": "네 배달 고기냄새 안나서 괜찮아요!",
          "original_form": "네 배달 고기냄새 안나서 괜찮아요!",
          "speaker_id": "2",
          "time": "20210830 01:13"
        },
        {
          "id": "MDRW2100023379.1.29",
          "form": "추천 감사해용!ㅎㅎ",
          "original_form": "추천 감사해용!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:13"
        },
        {
          "id": "MDRW2100023379.1.30",
          "form": "새우치킨 드셔보셨나용?",
          "original_form": "새우치킨 드셔보셨나용?",
          "speaker_id": "1",
