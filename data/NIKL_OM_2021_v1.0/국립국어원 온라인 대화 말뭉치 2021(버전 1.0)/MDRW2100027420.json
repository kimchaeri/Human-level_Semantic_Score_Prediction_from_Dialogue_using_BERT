        {
          "id": "MDRW2100027420.1.1",
          "form": "겜하세요?",
          "original_form": "겜하세요?",
          "speaker_id": "1",
          "time": "20210902 12:27"
        },
        {
          "id": "MDRW2100027420.1.2",
          "form": "네ㅎㅎ 롤 자주 해요",
          "original_form": "네ㅎㅎ 롤 자주 해요",
          "speaker_id": "2",
          "time": "20210902 12:28"
        },
        {
          "id": "MDRW2100027420.1.3",
          "form": "퍼즐게임해요 저는",
          "original_form": "퍼즐게임해요 저는",
          "speaker_id": "1",
          "time": "20210902 12:41"
        },
        {
          "id": "MDRW2100027420.1.4",
          "form": "어떤 퍼즐겜 주로 하세요?",
          "original_form": "어떤 퍼즐겜 주로 하세요?",
          "speaker_id": "2",
          "time": "20210902 12:43"
        },
        {
          "id": "MDRW2100027420.1.5",
          "form": "저는 퍼즐앤드래곤 하는데",
          "original_form": "저는 퍼즐앤드래곤 하는데",
          "speaker_id": "2",
          "time": "20210902 12:43"
        },
        {
          "id": "MDRW2100027420.1.6",
          "form": "오 그거 알것도 같아요. 저는 로얄 뭐인데",
          "original_form": "오 그거 알것도 같아요. 저는 로얄 뭐인데",
          "speaker_id": "1",
          "time": "20210902 12:48"
        },
        {
          "id": "MDRW2100027420.1.7",
          "form": "로얄로 시작하는 게임은 모르겠네요",
          "original_form": "로얄로 시작하는 게임은 모르겠네요",
          "speaker_id": "2",
          "time": "20210902 12:49"
        },
        {
          "id": "MDRW2100027420.1.8",
          "form": "로얄매치네용ㅋ",
          "original_form": "로얄매치네용ㅋ",
          "speaker_id": "1",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100027420.1.9",
          "form": "아하 그거 재밌나요",
          "original_form": "아하 그거 재밌나요",
          "speaker_id": "2",
          "time": "20210902 12:54"
        },
        {
          "id": "MDRW2100027420.1.10",
          "form": "할만해요ㅋㅋㅋ 함 해보세요ㅋ",
          "original_form": "할만해요ㅋㅋㅋ 함 해보세요ㅋ",
          "speaker_id": "1",
          "time": "20210902 12:55"
        },
        {
          "id": "MDRW2100027420.1.11",
          "form": "지금 막 깔려구요ㅋㅋ",
          "original_form": "지금 막 깔려구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:56"
        },
        {
          "id": "MDRW2100027420.1.12",
          "form": "오오ㅋ 퍼즐겜 좋아하시나봐영ㅋ",
          "original_form": "오오ㅋ 퍼즐겜 좋아하시나봐영ㅋ",
          "speaker_id": "1",
          "time": "20210902 12:57"
        },
        {
          "id": "MDRW2100027420.1.13",
          "form": "간단하게 할수 있어서 좋아해요",
          "original_form": "간단하게 할수 있어서 좋아해요",
          "speaker_id": "2",
          "time": "20210902 12:59"
        },
        {
          "id": "MDRW2100027420.1.14",
          "form": "오래 안해도되구요",
          "original_form": "오래 안해도되구요",
