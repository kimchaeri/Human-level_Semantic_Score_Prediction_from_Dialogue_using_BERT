      "utterance": [
        {
          "id": "MDRW2100033596.1.1",
          "form": "학교 다닐 때",
          "original_form": "학교 다닐 때",
          "speaker_id": "2",
          "time": "20210824 16:32"
        },
        {
          "id": "MDRW2100033596.1.2",
          "form": "스트레스 주로 뭐때문에",
          "original_form": "스트레스 주로 뭐때문에",
          "speaker_id": "2",
          "time": "20210824 16:32"
        },
        {
          "id": "MDRW2100033596.1.3",
          "form": "받으셨어요?",
          "original_form": "받으셨어요?",
          "speaker_id": "2",
          "time": "20210824 16:32"
        },
        {
          "id": "MDRW2100033596.1.4",
          "form": "아무래도 대인관계",
          "original_form": "아무래도 대인관계",
          "speaker_id": "1",
          "time": "20210824 16:33"
        },
        {
          "id": "MDRW2100033596.1.5",
          "form": "아 대인관계....",
          "original_form": "아 대인관계....",
          "speaker_id": "2",
          "time": "20210824 16:33"
        },
        {
          "id": "MDRW2100033596.1.6",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210824 16:34"
        },
        {
          "id": "MDRW2100033596.1.7",
          "form": "저도 대인관계로 스트레스 받응 적도 있는데",
          "original_form": "저도 대인관계로 스트레스 받응 적도 있는데",
          "speaker_id": "2",
          "time": "20210824 16:38"
        },
        {
          "id": "MDRW2100033596.1.8",
          "form": "최대는 발표였어요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "최대는 발표였어요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:38"
        },
        {
          "id": "MDRW2100033596.1.9",
          "form": "발표 너무 부담스러워요",
          "original_form": "발표 너무 부담스러워요",
          "speaker_id": "1",
          "time": "20210824 16:38"
        },
        {
          "id": "MDRW2100033596.1.10",
          "form": "막 시작하기도 전에 심장 쿵쿵거리고",
          "original_form": "막 시작하기도 전에 심장 쿵쿵거리고",
          "speaker_id": "1",
          "time": "20210824 16:38"
        },
        {
          "id": "MDRW2100033596.1.11",
          "form": "남들 앞에 선다는 게 좀 부담스러운 것 같아요",
          "original_form": "남들 앞에 선다는 게 좀 부담스러운 것 같아요",
          "speaker_id": "1",
          "time": "20210824 16:38"
        },
        {
          "id": "MDRW2100033596.1.12",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210824 16:39"
        },
        {
          "id": "MDRW2100033596.1.13",
          "form": "정말 너무 어려운 거 같아요",
          "original_form": "정말 너무 어려운 거 같아요",
          "speaker_id": "2",
          "time": "20210824 16:39"
        },
        {
          "id": "MDRW2100033596.1.14",
          "form": "수강 신청 어땠어요",
          "original_form": "수강 신청 어땠어요",
          "speaker_id": "1",
          "time": "20210824 16:40"
        },
        {
          "id": "MDRW2100033596.1.15",
          "form": "수강신청은 실패 한적도 있지만",
          "original_form": "수강신청은 실패 한적도 있지만",
          "speaker_id": "2",
          "time": "20210824 16:40"
        },
        {
          "id": "MDRW2100033596.1.16",
          "form": "거의 올클이였어요 운이 좋았죠",
          "original_form": "거의 올클이였어요 운이 좋았죠",
          "speaker_id": "2",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100033596.1.17",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100033596.1.18",
          "form": "오진짜 부럽다",
          "original_form": "오진짜 부럽다",
          "speaker_id": "1",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100033596.1.19",
          "form": "저는 손이 느려서 실패한 적이 더 많아요",
          "original_form": "저는 손이 느려서 실패한 적이 더 많아요",
          "speaker_id": "1",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100033596.1.20",
          "form": "수업 듣는 것까지 경쟁을 해야 한다는 게 너무 짜증났어요",
          "original_form": "수업 듣는 것까지 경쟁을 해야 한다는 게 너무 짜증났어요",
          "speaker_id": "1",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100033596.1.21",
          "form": "저도 그때만 운이 따르더라구요",
          "original_form": "저도 그때만 운이 따르더라구요",
          "speaker_id": "2",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100033596.1.22",
          "form": "어 맞아요",
          "original_form": "어 맞아요",
          "speaker_id": "2",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100033596.1.23",
          "form": "등록금 내는데 어이없어요",
          "original_form": "등록금 내는데 어이없어요",
          "speaker_id": "2",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100033596.1.24",
          "form": "등록금이 싼 것도 아니고 비싼데",
          "original_form": "등록금이 싼 것도 아니고 비싼데",
          "speaker_id": "1",
          "time": "20210824 16:42"
        },
        {
          "id": "MDRW2100033596.1.25",
          "form": "수업도 내 마음대로 듣지도 못하고",
          "original_form": "수업도 내 마음대로 듣지도 못하고",
          "speaker_id": "1",
          "time": "20210824 16:42"
        },
        {
          "id": "MDRW2100033596.1.26",
          "form": "대학이 뭐 이래 했던 거 같아요",
          "original_form": "대학이 뭐 이래 했던 거 같아요",
          "speaker_id": "1",
          "time": "20210824 16:42"
        },
        {
          "id": "MDRW2100033596.1.27",
          "form": "그러니까요 대학 체제가",
          "original_form": "그러니까요 대학 체제가",
          "speaker_id": "2",
          "time": "20210824 16:42"
        },
        {
          "id": "MDRW2100033596.1.28",
          "form": "너무 어이없어요",
          "original_form": "너무 어이없어요",
          "speaker_id": "2",
          "time": "20210824 16:42"
        },
        {
          "id": "MDRW2100033596.1.29",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210824 16:44"
        },
        {
          "id": "MDRW2100033596.1.30",
          "form": "등록금 아까워요",
          "original_form": "등록금 아까워요",
