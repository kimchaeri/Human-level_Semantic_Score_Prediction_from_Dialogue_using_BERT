      "utterance": [
        {
          "id": "MDRW2100003702.1.1",
          "form": "글을 많이 쓰거나 하진 않는데 그래도 눈팅 자주하고",
          "original_form": "글을 많이 쓰거나 하진 않는데 그래도 눈팅 자주하고",
          "speaker_id": "1",
          "time": "20210821 10:56"
        },
        {
          "id": "MDRW2100003702.1.2",
          "form": "질문도하고ㅋ",
          "original_form": "질문도하고ㅋ",
          "speaker_id": "1",
          "time": "20210821 10:56"
        },
        {
          "id": "MDRW2100003702.1.3",
          "form": "저도 가입해볼까봐요",
          "original_form": "저도 가입해볼까봐요",
          "speaker_id": "2",
          "time": "20210821 10:57"
        },
        {
          "id": "MDRW2100003702.1.4",
          "form": "정보가 많나요?",
          "original_form": "정보가 많나요?",
          "speaker_id": "2",
          "time": "20210821 11:00"
        },
        {
          "id": "MDRW2100003702.1.5",
          "form": "한번 보세요~ 이벤트가 주 종목인것같아요 거기는ㅎ",
          "original_form": "한번 보세요~ 이벤트가 주 종목인것같아요 거기는ㅎ",
          "speaker_id": "1",
          "time": "20210821 11:00"
        },
        {
          "id": "MDRW2100003702.1.6",
          "form": "오 좋은 카페네요",
          "original_form": "오 좋은 카페네요",
          "speaker_id": "2",
          "time": "20210821 11:01"
        },
        {
          "id": "MDRW2100003702.1.7",
          "form": "합리적 소비생활 맞나요?",
          "original_form": "합리적 소비생활 맞나요?",
          "speaker_id": "2",
          "time": "20210821 11:01"
        },
        {
          "id": "MDRW2100003702.1.8",
          "form": "현명한소비",
          "original_form": "현명한소비",
          "speaker_id": "1",
          "time": "20210821 11:01"
        },
        {
          "id": "MDRW2100003702.1.9",
          "form": "에요ㅎㅎ:",
          "original_form": "에요ㅎㅎ:",
          "speaker_id": "1",
          "time": "20210821 11:01"
        },
        {
          "id": "MDRW2100003702.1.10",
          "form": "중간에한번 이름이 바꼈어서",
          "original_form": "중간에한번 이름이 바꼈어서",
          "speaker_id": "1",
          "time": "20210821 11:02"
        },
        {
          "id": "MDRW2100003702.1.11",
          "form": "앗 저는 왜 저렇게 생각했을까요 ㅎㅎ",
          "original_form": "앗 저는 왜 저렇게 생각했을까요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 11:02"
        },
        {
          "id": "MDRW2100003702.1.12",
          "form": "아까전에 합리적인소비라고제가ㅋㅋ",
          "original_form": "아까전에 합리적인소비라고제가ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 11:02"
        },
        {
          "id": "MDRW2100003702.1.13",
          "form": "첫이름이 그거였어요",
          "original_form": "첫이름이 그거였어요",
          "speaker_id": "1",
          "time": "20210821 11:02"
        },
        {
          "id": "MDRW2100003702.1.14",
          "form": "아~~그러셨구나 ㅋㅋ 저는 제 뇌가 이상해졌는지 알았어요 ㅎㅎ",
          "original_form": "아~~그러셨구나 ㅋㅋ 저는 제 뇌가 이상해졌는지 알았어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 11:02"
        },
        {
          "id": "MDRW2100003702.1.15",
          "form": "ㅋㅋ 정상이십니다ㅋㅋㅋ",
          "original_form": "ㅋㅋ 정상이십니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 11:03"
        },
        {
          "id": "MDRW2100003702.1.16",
          "form": "ㅋㅋ 다행이네요 ㅎㅎ",
          "original_form": "ㅋㅋ 다행이네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 11:04"
        },
        {
          "id": "MDRW2100003702.1.17",
          "form": "그 짠카페가 업체도 많이 끼여있고? 주 된게 이벤트나 그런게 아니고 한번 무슨일도 있어서",
          "original_form": "그 짠카페가 업체도 많이 끼여있고? 주 된게 이벤트나 그런게 아니고 한번 무슨일도 있어서",
          "speaker_id": "1",
          "time": "20210821 11:04"
        },
        {
          "id": "MDRW2100003702.1.18",
          "form": "사람들이 많이 실망하고 갈아탔어요",
          "original_form": "사람들이 많이 실망하고 갈아탔어요",
          "speaker_id": "1",
          "time": "20210821 11:04"
        },
        {
          "id": "MDRW2100003702.1.19",
          "form": "아~~그렇군요 어쩐지 예전에 비해 정보보다는 판매글이 많더라고요",
          "original_form": "아~~그렇군요 어쩐지 예전에 비해 정보보다는 판매글이 많더라고요",
          "speaker_id": "2",
          "time": "20210821 11:05"
        },
        {
          "id": "MDRW2100003702.1.20",
          "form": "아 맞아요~ 주 활동자분들이 많이 옮기셨어요",
          "original_form": "아 맞아요~ 주 활동자분들이 많이 옮기셨어요",
          "speaker_id": "1",
          "time": "20210821 11:08"
        }
      ]
