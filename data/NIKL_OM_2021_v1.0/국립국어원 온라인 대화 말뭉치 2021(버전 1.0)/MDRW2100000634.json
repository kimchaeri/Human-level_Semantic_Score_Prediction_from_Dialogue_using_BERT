      "utterance": [
        {
          "id": "MDRW2100000634.1.1",
          "form": "재택근무 좋겠어",
          "original_form": "재택근무 좋겠어",
          "speaker_id": "1",
          "time": "20210819 17:51"
        },
        {
          "id": "MDRW2100000634.1.2",
          "form": "주부인경우는 힘들어",
          "original_form": "주부인경우는 힘들어",
          "speaker_id": "2",
          "time": "20210819 17:51"
        },
        {
          "id": "MDRW2100000634.1.3",
          "form": "왜 괜찮을거 같은데",
          "original_form": "왜 괜찮을거 같은데",
          "speaker_id": "1",
          "time": "20210819 17:51"
        },
        {
          "id": "MDRW2100000634.1.4",
          "form": "일에다가 가사일까지 해야하니",
          "original_form": "일에다가 가사일까지 해야하니",
          "speaker_id": "2",
          "time": "20210819 17:51"
        },
        {
          "id": "MDRW2100000634.1.5",
          "form": "그렇겠다 주부는 힘드네",
          "original_form": "그렇겠다 주부는 힘드네",
          "speaker_id": "1",
          "time": "20210819 17:52"
        },
        {
          "id": "MDRW2100000634.1.6",
          "form": "응 힘들어도 해야지",
          "original_form": "응 힘들어도 해야지",
          "speaker_id": "2",
          "time": "20210819 17:52"
        },
        {
          "id": "MDRW2100000634.1.7",
          "form": "집에서 일하니 안전은 하겠다",
          "original_form": "집에서 일하니 안전은 하겠다",
          "speaker_id": "1",
          "time": "20210819 17:52"
        },
        {
          "id": "MDRW2100000634.1.8",
          "form": "보안에 신경써야겠지",
          "original_form": "보안에 신경써야겠지",
          "speaker_id": "2",
          "time": "20210819 17:53"
        },
        {
          "id": "MDRW2100000634.1.9",
          "form": "전용노트북이런거 돌아가면서 사용하던데",
          "original_form": "전용노트북이런거 돌아가면서 사용하던데",
          "speaker_id": "1",
          "time": "20210819 17:53"
        },
        {
          "id": "MDRW2100000634.1.10",
          "form": "응 아님 개인꺼 쓰기도 하더라",
          "original_form": "응 아님 개인꺼 쓰기도 하더라",
          "speaker_id": "2",
          "time": "20210819 17:53"
        },
        {
          "id": "MDRW2100000634.1.11",
          "form": "개인용 없으면 좀 그렇겠다",
          "original_form": "개인용 없으면 좀 그렇겠다",
          "speaker_id": "1",
          "time": "20210819 17:55"
        },
        {
          "id": "MDRW2100000634.1.12",
          "form": "응 요즘 폰으로 다 하니까 컴퓨터 엾는집도 있어",
          "original_form": "응 요즘 폰으로 다 하니까 컴퓨터 엾는집도 있어",
          "speaker_id": "2",
          "time": "20210819 17:55"
        },
        {
          "id": "MDRW2100000634.1.13",
          "form": "재택근무면 단축근로인가?",
          "original_form": "재택근무면 단축근로인가?",
          "speaker_id": "1",
          "time": "20210819 17:56"
        },
        {
          "id": "MDRW2100000634.1.14",
          "form": "정해진 근무시간 다 하겠지",
          "original_form": "정해진 근무시간 다 하겠지",
          "speaker_id": "2",
          "time": "20210819 17:56"
        },
        {
          "id": "MDRW2100000634.1.15",
          "form": "업무능률은 좋을지 모르겠네?",
          "original_form": "업무능률은 좋을지 모르겠네?",
          "speaker_id": "1",
          "time": "20210819 17:57"
        },
        {
          "id": "MDRW2100000634.1.16",
          "form": "상황이 이러니 재택근무가 많아지겠지",
          "original_form": "상황이 이러니 재택근무가 많아지겠지",
          "speaker_id": "2",
          "time": "20210819 17:57"
