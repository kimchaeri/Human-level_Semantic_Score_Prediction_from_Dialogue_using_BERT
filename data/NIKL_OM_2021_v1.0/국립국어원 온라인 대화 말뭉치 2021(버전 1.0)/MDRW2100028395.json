        },
        {
          "id": "MDRW2100028395.1.2",
          "form": "name2님 지역엔 오늘 비가 왔나요?",
          "original_form": "&name2&님 지역엔 오늘 비가 왔나요?",
          "speaker_id": "1",
          "time": "20210902 19:38"
        },
        {
          "id": "MDRW2100028395.1.3",
          "form": "어제는 비는 안왔어요ㅎ",
          "original_form": "어제는 비는 안왔어요ㅎ",
          "speaker_id": "2",
          "time": "20210903 09:05"
        },
        {
          "id": "MDRW2100028395.1.4",
          "form": "오늘은 무척덥네요ㅠ",
          "original_form": "오늘은 무척덥네요ㅠ",
          "speaker_id": "2",
          "time": "20210903 09:06"
        },
        {
          "id": "MDRW2100028395.1.5",
          "form": "그런가요~",
          "original_form": "그런가요~",
          "speaker_id": "1",
          "time": "20210903 09:56"
        },
        {
          "id": "MDRW2100028395.1.6",
          "form": "여긴 조금 쌀쌀해요",
          "original_form": "여긴 조금 쌀쌀해요",
          "speaker_id": "1",
          "time": "20210903 09:56"
        },
        {
          "id": "MDRW2100028395.1.7",
          "form": "아침에 제가 출근늦어서 뛰어서 그러가ㅠ",
          "original_form": "아침에 제가 출근늦어서 뛰어서 그러가ㅠ",
          "speaker_id": "2",
          "time": "20210903 09:57"
        },
        {
          "id": "MDRW2100028395.1.8",
          "form": "경기도인데 어느쪽이세요?",
          "original_form": "경기도인데 어느쪽이세요?",
          "speaker_id": "2",
          "time": "20210903 09:57"
        },
        {
          "id": "MDRW2100028395.1.9",
          "form": "서울이요~",
          "original_form": "서울이요~",
          "speaker_id": "1",
          "time": "20210903 09:59"
        },
        {
          "id": "MDRW2100028395.1.10",
          "form": "일교차크죠 요즘~",
          "original_form": "일교차크죠 요즘~",
          "speaker_id": "1",
          "time": "20210903 10:00"
        },
        {
          "id": "MDRW2100028395.1.11",
          "form": "아ㅎㅎㅎㅎ가깝네요ㅋ 수도권은 아침저녁엔 좀 쌀쌀할때도ㅋ",
          "original_form": "아ㅎㅎㅎㅎ가깝네요ㅋ 수도권은 아침저녁엔 좀 쌀쌀할때도ㅋ",
          "speaker_id": "2",
          "time": "20210903 10:17"
        },
        {
          "id": "MDRW2100028395.1.12",
          "form": "맞아요 ㅋㅋㅋ 이럴땐 가디건같은거 꼭 챙겨다녀요",
          "original_form": "맞아요 ㅋㅋㅋ 이럴땐 가디건같은거 꼭 챙겨다녀요",
          "speaker_id": "1",
          "time": "20210903 12:44"
        },
        {
          "id": "MDRW2100028395.1.13",
          "form": "저도요ㅠㅜㅋ",
          "original_form": "저도요ㅠㅜㅋ",
          "speaker_id": "2",
          "time": "20210903 12:54"
        },
        {
          "id": "MDRW2100028395.1.14",
          "form": "아예 사무실에뒀어요ㅜ",
          "original_form": "아예 사무실에뒀어요ㅜ",
          "speaker_id": "2",
          "time": "20210903 12:54"
        },
        {
          "id": "MDRW2100028395.1.15",
          "form": "잘하셨어요",
          "original_form": "잘하셨어요",
          "speaker_id": "1",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100028395.1.16",
          "form": "추우면 감기걸려요",
          "original_form": "추우면 감기걸려요",
          "speaker_id": "1",
          "time": "20210903 16:52"
        },
        {
          "id": "MDRW2100028395.1.17",
          "form": "ㅎㅎ에어컨을 너무켜서요ㅜ",
          "original_form": "ㅎㅎ에어컨을 너무켜서요ㅜ",
          "speaker_id": "2",
          "time": "20210903 18:48"
        },
        {
          "id": "MDRW2100028395.1.18",
          "form": "그죠 저는 에어컨 맨날 줄이는사람이에요ㅋㅋㅋ",
          "original_form": "그죠 저는 에어컨 맨날 줄이는사람이에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:50"
        },
        {
          "id": "MDRW2100028395.1.19",
          "form": "북극곰 불쌍해요",
          "original_form": "북극곰 불쌍해요",
          "speaker_id": "1",
          "time": "20210903 18:50"
        },
        {
          "id": "MDRW2100028395.1.20",
          "form": "ㅋㅋㅋㅋㄲ그쵸 사람이 제일 나쁘다는ㅜ",
          "original_form": "ㅋㅋㅋㅋㄲ그쵸 사람이 제일 나쁘다는ㅜ",
          "speaker_id": "2",
          "time": "20210903 18:59"
        },
        {
          "id": "MDRW2100028395.1.21",
          "form": "ㅋㅋㅋ맞아요 기상이변 안당하려면",
          "original_form": "ㅋㅋㅋ맞아요 기상이변 안당하려면",
