        },
        {
          "id": "MDRW2100030702.1.2",
          "form": "늦게맞으니 모더나이네",
          "original_form": "늦게맞으니 모더나이네",
          "speaker_id": "2",
          "time": "20210904 11:35"
        },
        {
          "id": "MDRW2100030702.1.3",
          "form": "그렇네~좋은거 맞았네",
          "original_form": "그렇네~좋은거 맞았네",
          "speaker_id": "1",
          "time": "20210904 11:48"
        },
        {
          "id": "MDRW2100030702.1.4",
          "form": "ㅇㅇ아무느낌도 없네",
          "original_form": "ㅇㅇ아무느낌도 없네",
          "speaker_id": "2",
          "time": "20210904 12:58"
        },
        {
          "id": "MDRW2100030702.1.5",
          "form": "나는 어지럽던데ㅜ",
          "original_form": "나는 어지럽던데ㅜ",
          "speaker_id": "1",
          "time": "20210904 13:43"
        },
        {
          "id": "MDRW2100030702.1.6",
          "form": "화이자가 그렇다하드라",
          "original_form": "화이자가 그렇다하드라",
          "speaker_id": "2",
          "time": "20210904 13:46"
        },
        {
          "id": "MDRW2100030702.1.7",
          "form": "아 맞나ㅜㅜ",
          "original_form": "아 맞나ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 13:52"
        },
        {
          "id": "MDRW2100030702.1.8",
          "form": "고열나고",
          "original_form": "고열나고",
          "speaker_id": "2",
          "time": "20210904 13:55"
        },
        {
          "id": "MDRW2100030702.1.9",
          "form": "암튼 예방접종하고 조심해라",
          "original_form": "암튼 예방접종하고 조심해라",
          "speaker_id": "1",
          "time": "20210904 14:30"
        },
        {
          "id": "MDRW2100030702.1.10",
          "form": "이러면서 쉬는거다",
          "original_form": "이러면서 쉬는거다",
          "speaker_id": "1",
          "time": "20210904 14:30"
        },
        {
          "id": "MDRW2100030702.1.11",
          "form": "어 누웠다",
          "original_form": "어 누웠다",
          "speaker_id": "2",
          "time": "20210904 14:31"
        },
        {
          "id": "MDRW2100030702.1.12",
          "form": "계속 누워있어라 청소하지말고",
          "original_form": "계속 누워있어라 청소하지말고",
          "speaker_id": "1",
          "time": "20210904 17:03"
        },
        {
          "id": "MDRW2100030702.1.13",
          "form": "밥해야지 ㅜ",
          "original_form": "밥해야지 ㅜ",
          "speaker_id": "2",
          "time": "20210904 17:12"
        },
        {
          "id": "MDRW2100030702.1.14",
          "form": "시켜먹어라ㅜ",
          "original_form": "시켜먹어라ㅜ",
          "speaker_id": "1",
          "time": "20210904 17:13"
        },
        {
          "id": "MDRW2100030702.1.15",
          "form": "낼부터 시키지머 ㅠ재료땜에 ㅜ",
          "original_form": "낼부터 시키지머 ㅠ재료땜에 ㅜ",
          "speaker_id": "2",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100030702.1.16",
          "form": "백신이 사람 골로 가게한다ㅜ",
