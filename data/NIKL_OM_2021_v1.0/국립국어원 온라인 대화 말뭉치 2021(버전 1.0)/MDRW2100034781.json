      "utterance": [
        {
          "id": "MDRW2100034781.1.1",
          "form": "오늘 너무 더웠어요",
          "original_form": "오늘 너무 더웠어요",
          "speaker_id": "2",
          "time": "20210828 20:34"
        },
        {
          "id": "MDRW2100034781.1.2",
          "form": "비 안오니까 덥더라고요",
          "original_form": "비 안오니까 덥더라고요",
          "speaker_id": "1",
          "time": "20210828 20:36"
        },
        {
          "id": "MDRW2100034781.1.3",
          "form": "외출하셨어요?",
          "original_form": "외출하셨어요?",
          "speaker_id": "1",
          "time": "20210828 20:36"
        },
        {
          "id": "MDRW2100034781.1.4",
          "form": "오늘 할머니랑 강아지랑 같이 동네산책 했어요",
          "original_form": "오늘 할머니랑 강아지랑 같이 동네산책 했어요",
          "speaker_id": "2",
          "time": "20210828 20:36"
        },
        {
          "id": "MDRW2100034781.1.5",
          "form": "산책하기 좋은 날씨였죠! 조금 덥긴하지만ㅋㅋㅋ",
          "original_form": "산책하기 좋은 날씨였죠! 조금 덥긴하지만ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:37"
        },
        {
          "id": "MDRW2100034781.1.6",
          "form": "강아지가 좋아했을 거 같아요",
          "original_form": "강아지가 좋아했을 거 같아요",
          "speaker_id": "1",
          "time": "20210828 20:37"
        },
        {
          "id": "MDRW2100034781.1.7",
          "form": "맞아요 더워도 비오는날보단 좋아요",
          "original_form": "맞아요 더워도 비오는날보단 좋아요",
          "speaker_id": "2",
          "time": "20210828 22:19"
        },
        {
          "id": "MDRW2100034781.1.8",
          "form": "다음주에도 비소식이 있더라고요ㅠㅠ",
          "original_form": "다음주에도 비소식이 있더라고요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:19"
        },
        {
          "id": "MDRW2100034781.1.9",
          "form": "네 다음주 내내 비던데 부산은 ㅜ",
          "original_form": "네 다음주 내내 비던데 부산은 ㅜ",
          "speaker_id": "2",
          "time": "20210828 22:19"
        },
        {
          "id": "MDRW2100034781.1.10",
          "form": "부산에 사시나요?",
          "original_form": "부산에 사시나요?",
          "speaker_id": "1",
          "time": "20210828 22:20"
        },
        {
          "id": "MDRW2100034781.1.11",
          "form": "네부산이에용",
          "original_form": "네부산이에용",
          "speaker_id": "2",
          "time": "20210828 22:28"
        },
        {
          "id": "MDRW2100034781.1.12",
          "form": "이번에 태풍 피해 없으셨나요?ㅠㅠ",
          "original_form": "이번에 태풍 피해 없으셨나요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:30"
        },
        {
          "id": "MDRW2100034781.1.13",
          "form": "네 피해는 없었어요ㅠ",
          "original_form": "네 피해는 없었어요ㅠ",
          "speaker_id": "2",
          "time": "20210828 22:52"
        },
        {
          "id": "MDRW2100034781.1.14",
          "form": "너무 다행이네요!",
          "original_form": "너무 다행이네요!",
          "speaker_id": "1",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100034781.1.15",
          "form": "여름 다 끝나가는데 늦은 태풍이 와서 놀랐어요ㅠㅠ",
          "original_form": "여름 다 끝나가는데 늦은 태풍이 와서 놀랐어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:57"
        },
        {
          "id": "MDRW2100034781.1.16",
          "form": "그쵸 ㅠ 가을에 태풍이 더 잦을 수도 있을것같아요ㅠ",
          "original_form": "그쵸 ㅠ 가을에 태풍이 더 잦을 수도 있을것같아요ㅠ",
