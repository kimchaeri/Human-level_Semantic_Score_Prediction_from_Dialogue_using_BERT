      "utterance": [
        {
          "id": "MDRW2100043578.1.1",
          "form": "그래 맞다 아빠랑 아는 사람들이랑 같이 간다",
          "original_form": "그래 맞다 아빠랑 아는 사람들이랑 같이 간다",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043578.1.2",
          "form": "좋겠다 ㅜㅜ 나도 가고싶어",
          "original_form": "좋겠다 ㅜㅜ 나도 가고싶어",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043578.1.3",
          "form": "딸내미도 같이 갈래",
          "original_form": "딸내미도 같이 갈래",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043578.1.4",
          "form": "그러고 싶다..",
          "original_form": "그러고 싶다..",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043578.1.5",
          "form": "안 되지",
          "original_form": "안 되지",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043578.1.6",
          "form": "ㅋㅋ당연히 안되지..그냥 해보는소리다",
          "original_form": "ㅋㅋ당연히 안되지..그냥 해보는소리다",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043578.1.7",
          "form": "여름 휴가 임 있었잖아",
          "original_form": "여름 휴가 임 있었잖아",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043578.1.8",
          "form": "맞다..ㅎㅎ",
          "original_form": "맞다..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043578.1.9",
          "form": "다음에 같이 가자 가족끼리",
          "original_form": "다음에 같이 가자 가족끼리",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043578.1.10",
          "form": "웅웅~ 잘 다녀와용",
          "original_form": "웅웅~ 잘 다녀와용",
          "speaker_id": "1",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043578.1.11",
          "form": "기대된다",
          "original_form": "기대된다",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100043578.1.12",
          "form": "가면 뭐해?",
