      "utterance": [
        {
          "id": "MDRW2100018506.1.1",
          "form": "책봐요?",
          "original_form": "책봐요?",
          "speaker_id": "1",
          "time": "20210827 07:01"
        },
        {
          "id": "MDRW2100018506.1.2",
          "form": "요즘은 잘 안봐요 밀리의 서재 자주 이용해요",
          "original_form": "요즘은 잘 안봐요 밀리의 서재 자주 이용해요",
          "speaker_id": "2",
          "time": "20210827 07:44"
        },
        {
          "id": "MDRW2100018506.1.3",
          "form": "읽어주는?",
          "original_form": "읽어주는?",
          "speaker_id": "1",
          "time": "20210827 07:58"
        },
        {
          "id": "MDRW2100018506.1.4",
          "form": "아니요읽는거요 오디오도있어요",
          "original_form": "아니요읽는거요 오디오도있어요",
          "speaker_id": "2",
          "time": "20210827 08:28"
        },
        {
          "id": "MDRW2100018506.1.5",
          "form": "아 그런가요",
          "original_form": "아 그런가요",
          "speaker_id": "1",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100018506.1.6",
          "form": "근데 폰으로 보다보니 잘 안봐지네요",
          "original_form": "근데 폰으로 보다보니 잘 안봐지네요",
          "speaker_id": "2",
          "time": "20210827 11:00"
        },
        {
          "id": "MDRW2100018506.1.7",
          "form": "그런가여?",
          "original_form": "그런가여?",
          "speaker_id": "1",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100018506.1.8",
          "form": "네네 그래도 자주봐야죠",
          "original_form": "네네 그래도 자주봐야죠",
          "speaker_id": "2",
          "time": "20210827 21:00"
        },
        {
          "id": "MDRW2100018506.1.9",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210827 21:10"
        },
        {
          "id": "MDRW2100018506.1.10",
          "form": "책 잘안봐서 큰일이에요",
          "original_form": "책 잘안봐서 큰일이에요",
          "speaker_id": "2",
          "time": "20210827 21:22"
        },
        {
          "id": "MDRW2100018506.1.11",
          "form": "그쳐",
          "original_form": "그쳐",
          "speaker_id": "1",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100018506.1.12",
          "form": "오늘도안볼거같아요 ㅋ",
          "original_form": "오늘도안볼거같아요 ㅋ",
          "speaker_id": "2",
          "time": "20210827 21:28"
        },
        {
          "id": "MDRW2100018506.1.13",
          "form": "불금은 쉬죠",
          "original_form": "불금은 쉬죠",
          "speaker_id": "1",
          "time": "20210827 21:31"
        },
        {
          "id": "MDRW2100018506.1.14",
          "form": "맞아요 ㅋㅋ 티비볼거에요",
          "original_form": "맞아요 ㅋㅋ 티비볼거에요",
