      "utterance": [
        {
          "id": "MDRW2100015274.1.1",
          "form": "음 글쎄 없는 거 같아",
          "original_form": "음 글쎄 없는 거 같아",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015274.1.2",
          "form": "나도 별로 사고싶은거 없긴 해",
          "original_form": "나도 별로 사고싶은거 없긴 해",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015274.1.3",
          "form": "쇼핑 가는 것도 별로야?",
          "original_form": "쇼핑 가는 것도 별로야?",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015274.1.4",
          "form": "아니 쇼핑 하는건 좋은데 관심 있는게 딱히 없어서",
          "original_form": "아니 쇼핑 하는건 좋은데 관심 있는게 딱히 없어서",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015274.1.5",
          "form": "하긴 먹는 거 좋아하는 것도 아니고",
          "original_form": "하긴 먹는 거 좋아하는 것도 아니고",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015274.1.6",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015274.1.7",
          "form": "난 먹고싶은 것도 너무 많은데",
          "original_form": "난 먹고싶은 것도 너무 많은데",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015274.1.8",
          "form": "ㅋㅋㅋ 먹어",
          "original_form": "ㅋㅋㅋ 먹어",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015274.1.9",
          "form": "근데 잘 안팔더라고",
          "original_form": "근데 잘 안팔더라고",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015274.1.10",
          "form": "아 그럴 수 있저",
          "original_form": "아 그럴 수 있저",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015274.1.11",
          "form": "무화과쨈 살까 잠깐 고민했는데 마땅치 않았어",
          "original_form": "무화과쨈 살까 잠깐 고민했는데 마땅치 않았어",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015274.1.12",
          "form": "무화과 잼 맛있어?",
          "original_form": "무화과 잼 맛있어?",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015274.1.13",
          "form": "응 난 되게 맛있더라고",
          "original_form": "응 난 되게 맛있더라고",
          "speaker_id": "2",
