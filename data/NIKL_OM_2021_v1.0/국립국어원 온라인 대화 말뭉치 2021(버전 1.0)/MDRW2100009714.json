        {
          "id": "MDRW2100009714.1.1",
          "form": "여긴안와요 흐리긴하네요",
          "original_form": "여긴안와요 흐리긴하네요",
          "speaker_id": "2",
          "time": "20210823 08:18"
        },
        {
          "id": "MDRW2100009714.1.2",
          "form": "여기도요",
          "original_form": "여기도요",
          "speaker_id": "1",
          "time": "20210823 08:35"
        },
        {
          "id": "MDRW2100009714.1.3",
          "form": "어떤날씨좋아하세요?",
          "original_form": "어떤날씨좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 09:12"
        },
        {
          "id": "MDRW2100009714.1.4",
          "form": "구름이요",
          "original_form": "구름이요",
          "speaker_id": "1",
          "time": "20210823 09:51"
        },
        {
          "id": "MDRW2100009714.1.5",
          "form": "구름낀날이요?",
          "original_form": "구름낀날이요?",
          "speaker_id": "2",
          "time": "20210823 14:38"
        },
        {
          "id": "MDRW2100009714.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210823 17:06"
        },
        {
          "id": "MDRW2100009714.1.7",
          "form": "전 맑은날이요 비그친고 해뜨거나",
          "original_form": "전 맑은날이요 비그친고 해뜨거나",
          "speaker_id": "2",
          "time": "20210823 17:17"
        },
        {
          "id": "MDRW2100009714.1.8",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210823 22:59"
        },
        {
          "id": "MDRW2100009714.1.9",
          "form": "대부분 맑은날을 좋아하지 않나요?",
          "original_form": "대부분 맑은날을 좋아하지 않나요?",
          "speaker_id": "2",
          "time": "20210824 09:42"
        },
        {
          "id": "MDRW2100009714.1.10",
          "form": "저는 더워서",
          "original_form": "저는 더워서",
          "speaker_id": "1",
          "time": "20210824 12:43"
        },
        {
          "id": "MDRW2100009714.1.11",
          "form": "아 저는여름도 좋아서요",
          "original_form": "아 저는여름도 좋아서요",
          "speaker_id": "2",
          "time": "20210824 13:28"
        },
        {
          "id": "MDRW2100009714.1.12",
          "form": "물놀이도 젤좋고",
          "original_form": "물놀이도 젤좋고",
          "speaker_id": "2",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100009714.1.13",
          "form": "바다좋죠",
          "original_form": "바다좋죠",
          "speaker_id": "1",
          "time": "20210824 20:38"
        },
        {
