        {
          "id": "MDRW2100042149.1.1",
          "form": "날씨도 좋으니 여행 가고 싶네요",
          "original_form": "날씨도 좋으니 여행 가고 싶네요",
          "speaker_id": "1",
          "time": "20210906 20:11"
        },
        {
          "id": "MDRW2100042149.1.2",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210906 20:13"
        },
        {
          "id": "MDRW2100042149.1.3",
          "form": "name1님은 어디 여행가보셨나요?",
          "original_form": "&name1&님은 어디 여행가보셨나요?",
          "speaker_id": "2",
          "time": "20210906 20:13"
        },
        {
          "id": "MDRW2100042149.1.4",
          "form": "코로나 전에는 휴가때마다 해외여행 다니고",
          "original_form": "코로나 전에는 휴가때마다 해외여행 다니고",
          "speaker_id": "1",
          "time": "20210906 20:13"
        },
        {
          "id": "MDRW2100042149.1.5",
          "form": "국내도 열심히 여행 다녔는데",
          "original_form": "국내도 열심히 여행 다녔는데",
          "speaker_id": "1",
          "time": "20210906 20:14"
        },
        {
          "id": "MDRW2100042149.1.6",
          "form": "그럼 어디가 가장 좋으셨나요?",
          "original_form": "그럼 어디가 가장 좋으셨나요?",
          "speaker_id": "2",
          "time": "20210906 20:15"
        },
        {
          "id": "MDRW2100042149.1.7",
          "form": "국내에서는 순천이요",
          "original_form": "국내에서는 순천이요",
          "speaker_id": "1",
          "time": "20210906 20:16"
        },
        {
          "id": "MDRW2100042149.1.8",
          "form": "가을철에 순천만 가서 일몰 보면",
          "original_form": "가을철에 순천만 가서 일몰 보면",
          "speaker_id": "1",
          "time": "20210906 20:16"
        },
        {
          "id": "MDRW2100042149.1.9",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042149.1.10",
          "form": "저는 춘천이요",
          "original_form": "저는 춘천이요",
          "speaker_id": "2",
          "time": "20210906 21:42"
        },
        {
          "id": "MDRW2100042149.1.11",
          "form": "춘천 좋죠~",
          "original_form": "춘천 좋죠~",
          "speaker_id": "1",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042149.1.12",
          "form": "추천해주실만한 곳이 있나요?",
          "original_form": "추천해주실만한 곳이 있나요?",
          "speaker_id": "1",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042149.1.13",
          "form": "저 레일바이크요",
          "original_form": "저 레일바이크요",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042149.1.14",
          "form": "오 레일바이크",
          "original_form": "오 레일바이크",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042149.1.15",
          "form": "힘든데 재미있어요ㅋㅋㅋㅋ",
          "original_form": "힘든데 재미있어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042149.1.16",
          "form": "풍경이 아름다웠나요?",
          "original_form": "풍경이 아름다웠나요?",
          "speaker_id": "1",
          "time": "20210906 21:44"
        },
        {
          "id": "MDRW2100042149.1.17",
          "form": "네 정말 예뻤어요",
          "original_form": "네 정말 예뻤어요",
          "speaker_id": "2",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042149.1.18",
          "form": "name1님은 기념품 사시나요?",
          "original_form": "&name1&님은 기념품 사시나요?",
          "speaker_id": "2",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042149.1.19",
          "form": "다음에 춘천 가면 레일바이크 타봐야겠네용",
          "original_form": "다음에 춘천 가면 레일바이크 타봐야겠네용",
          "speaker_id": "1",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042149.1.20",
          "form": "저는 기념품 사는 편인데",
          "original_form": "저는 기념품 사는 편인데",
