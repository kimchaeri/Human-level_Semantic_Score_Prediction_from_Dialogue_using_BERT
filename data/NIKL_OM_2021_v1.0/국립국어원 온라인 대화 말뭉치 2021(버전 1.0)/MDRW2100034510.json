      "utterance": [
        {
          "id": "MDRW2100034510.1.1",
          "form": "저녁에 뭐드실지 정하셨어요?ㅋㅋㅋ",
          "original_form": "저녁에 뭐드실지 정하셨어요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100034510.1.2",
          "form": "먹고싶은게 많아서 고민중이에요 ㅋㅋㅋㅋㅋ",
          "original_form": "먹고싶은게 많아서 고민중이에요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100034510.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100034510.1.4",
          "form": "전 저녁은 가볍게 먹으려구요 ㅋㅋㅋㅋ",
          "original_form": "전 저녁은 가볍게 먹으려구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100034510.1.5",
          "form": "근데 택배시킨 디저트들이 오늘 오겠네요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 택배시킨 디저트들이 오늘 오겠네요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:10"
        },
        {
          "id": "MDRW2100034510.1.6",
          "form": "뭐 시키셧어요??",
          "original_form": "뭐 시키셧어요??",
          "speaker_id": "2",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100034510.1.7",
          "form": "디저트 좋아하시는구나",
          "original_form": "디저트 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100034510.1.8",
          "form": "환장해요",
          "original_form": "환장해요",
          "speaker_id": "1",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100034510.1.9",
          "form": "저 최근에 아이디어스 할인하길래 샀는데",
          "original_form": "저 최근에 아이디어스 할인하길래 샀는데",
          "speaker_id": "1",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100034510.1.10",
          "form": "마카롱이요!",
          "original_form": "마카롱이요!",
          "speaker_id": "1",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100034510.1.11",
          "form": "마카롱!!",
          "original_form": "마카롱!!",
          "speaker_id": "2",
          "time": "20210828 14:19"
        },
        {
          "id": "MDRW2100034510.1.12",
          "form": "넘 맛나요",
          "original_form": "넘 맛나요",
          "speaker_id": "1",
          "time": "20210828 14:19"
        },
        {
          "id": "MDRW2100034510.1.13",
          "form": "커피랑 먹어줘야죠 ㅠㅠ",
          "original_form": "커피랑 먹어줘야죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 14:19"
        },
        {
          "id": "MDRW2100034510.1.14",
          "form": "무슨 맛 즐겨드세요? 워낙 맛이 다양하지만 ㅋㅋㅋㅋ",
          "original_form": "무슨 맛 즐겨드세요? 워낙 맛이 다양하지만 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:19"
        },
        {
          "id": "MDRW2100034510.1.15",
          "form": "저는 어제 먹은건데",
          "original_form": "저는 어제 먹은건데",
          "speaker_id": "1",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100034510.1.16",
          "form": "크림브륄레..!! 먹고 기절했어요",
          "original_form": "크림브륄레..!! 먹고 기절했어요",
          "speaker_id": "1",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100034510.1.17",
          "form": "ㅇ_ㅇ!!????",
          "original_form": "ㅇ_ㅇ!!????",
          "speaker_id": "2",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100034510.1.18",
          "form": "크림브륄레 마카롱도 잇어요???",
          "original_form": "크림브륄레 마카롱도 잇어요???",
          "speaker_id": "2",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100034510.1.19",
          "form": "네 ㅋㅋㅋㅋ 요새 엄청 다양하더라구요",
          "original_form": "네 ㅋㅋㅋㅋ 요새 엄청 다양하더라구요",
          "speaker_id": "1",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100034510.1.20",
          "form": "앙버터 마카롱이랑 피스타치오 인절미도 있어요!!",
          "original_form": "앙버터 마카롱이랑 피스타치오 인절미도 있어요!!",
          "speaker_id": "1",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100034510.1.21",
          "form": "어떤 천재가 만들어낸거지 ㄷㄷ",
          "original_form": "어떤 천재가 만들어낸거지 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100034510.1.22",
          "form": "저도 피스타치오맛 좋아해요",
          "original_form": "저도 피스타치오맛 좋아해요",
          "speaker_id": "2",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100034510.1.23",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210828 14:22"
        },
        {
