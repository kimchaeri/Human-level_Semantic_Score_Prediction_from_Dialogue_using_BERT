      "utterance": [
        {
          "id": "MDRW2100022248.1.1",
          "form": "교회다니세요?",
          "original_form": "교회다니세요?",
          "speaker_id": "2",
          "time": "20210829 12:35"
        },
        {
          "id": "MDRW2100022248.1.2",
          "form": "무교입니다!",
          "original_form": "무교입니다!",
          "speaker_id": "1",
          "time": "20210829 12:36"
        },
        {
          "id": "MDRW2100022248.1.3",
          "form": "교회 다니세요?",
          "original_form": "교회 다니세요?",
          "speaker_id": "1",
          "time": "20210829 12:36"
        },
        {
          "id": "MDRW2100022248.1.4",
          "form": "아뇨 무교죠ㅋㅋ",
          "original_form": "아뇨 무교죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:43"
        },
        {
          "id": "MDRW2100022248.1.5",
          "form": "부모님도 종교 안믿으시나요?",
          "original_form": "부모님도 종교 안믿으시나요?",
          "speaker_id": "2",
          "time": "20210829 12:44"
        },
        {
          "id": "MDRW2100022248.1.6",
          "form": "네 안 믿으세요 ㅋㅋㅋㅋㅋ",
          "original_form": "네 안 믿으세요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:44"
        },
        {
          "id": "MDRW2100022248.1.7",
          "form": "가끔 등산 갔다가 그 산에 절 있으면 절 들리는 정도세요 ㅋㅋㅋㅋ",
          "original_form": "가끔 등산 갔다가 그 산에 절 있으면 절 들리는 정도세요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:45"
        },
        {
          "id": "MDRW2100022248.1.8",
          "form": "저희 부모님 불교신데 강요하지않으세요",
          "original_form": "저희 부모님 불교신데 강요하지않으세요",
          "speaker_id": "2",
          "time": "20210829 12:46"
        },
        {
          "id": "MDRW2100022248.1.9",
          "form": "나중에 늙어서는 믿는거 좋은거같아요",
          "original_form": "나중에 늙어서는 믿는거 좋은거같아요",
          "speaker_id": "2",
          "time": "20210829 12:47"
        },
        {
          "id": "MDRW2100022248.1.10",
          "form": "불교나 천주교는 강요하는 문화가 없는 것 같아요",
          "original_form": "불교나 천주교는 강요하는 문화가 없는 것 같아요",
          "speaker_id": "1",
          "time": "20210829 12:47"
        },
        {
          "id": "MDRW2100022248.1.11",
          "form": "친구 만나는 모임삼아 다니는 것 정도는 괜찮을 것 같아요!",
          "original_form": "친구 만나는 모임삼아 다니는 것 정도는 괜찮을 것 같아요!",
          "speaker_id": "1",
          "time": "20210829 12:47"
        },
        {
          "id": "MDRW2100022248.1.12",
          "form": "맞아요 전 비혼이라 나중에 사교겸 믿음겸 ㅎㅎ",
          "original_form": "맞아요 전 비혼이라 나중에 사교겸 믿음겸 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 12:49"
        },
        {
          "id": "MDRW2100022248.1.13",
          "form": "사교겸 믿음겸 좋은 것 같아요 ㅋㅋㅋㅋ",
          "original_form": "사교겸 믿음겸 좋은 것 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:02"
        },
        {
          "id": "MDRW2100022248.1.14",
          "form": "어느 종교든 딱 과몰입되지않는 그정도 선이면 좋은 것 같아요!",
          "original_form": "어느 종교든 딱 과몰입되지않는 그정도 선이면 좋은 것 같아요!",
          "speaker_id": "1",
          "time": "20210829 13:02"
        },
        {
          "id": "MDRW2100022248.1.15",
          "form": "맞아요 뭐든 과몰입은 위험한거같아요 불교나 천주교 생각중입니다ㅋㅋ 묵주반지가 목표죠",
          "original_form": "맞아요 뭐든 과몰입은 위험한거같아요 불교나 천주교 생각중입니다ㅋㅋ 묵주반지가 목표죠",
          "speaker_id": "2",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100022248.1.16",
          "form": "저 선물 받은 묵주반지 있는데 가끔 마음의 안정이 필요할 때 껴서 돌려봐요 ㅋㅋㅋㅋ",
          "original_form": "저 선물 받은 묵주반지 있는데 가끔 마음의 안정이 필요할 때 껴서 돌려봐요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:05"
        },
        {
          "id": "MDRW2100022248.1.17",
          "form": "묵주반지는 지금이라도 목표달성 가능합니다!",
          "original_form": "묵주반지는 지금이라도 목표달성 가능합니다!",
          "speaker_id": "1",
          "time": "20210829 13:05"
        },
        {
          "id": "MDRW2100022248.1.18",
          "form": "ㅋㅋ 좋은 지인을 두셨네요 저도 그냥 반지만이라도 살까 보고있긴해요",
          "original_form": "ㅋㅋ 좋은 지인을 두셨네요 저도 그냥 반지만이라도 살까 보고있긴해요",
          "speaker_id": "2",
          "time": "20210829 13:25"
        },
        {
          "id": "MDRW2100022248.1.19",
          "form": "일상용으로도 괜찮은 것 같아요!",
          "original_form": "일상용으로도 괜찮은 것 같아요!",
          "speaker_id": "1",
          "time": "20210829 13:27"
        },
        {
          "id": "MDRW2100022248.1.20",
          "form": "십자가가 너무 크지않아서 부담이 없습니다 ㅎㅎ",
          "original_form": "십자가가 너무 크지않아서 부담이 없습니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 13:27"
        },
        {
          "id": "MDRW2100022248.1.21",
          "form": "심리적으로 효과가 있는것 같아요",
          "original_form": "심리적으로 효과가 있는것 같아요",
          "speaker_id": "2",
          "time": "20210829 13:31"
        }
      ]
    }
  ]
