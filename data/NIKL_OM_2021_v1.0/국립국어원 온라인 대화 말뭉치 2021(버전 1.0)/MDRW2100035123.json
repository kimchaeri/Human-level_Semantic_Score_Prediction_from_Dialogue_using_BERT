          "speaker_id": "1",
          "time": "20210829 03:22"
        },
        {
          "id": "MDRW2100035123.1.4",
          "form": "바로 앞바다에서 잡으셨는지",
          "original_form": "바로 앞바다에서 잡으셨는지",
          "speaker_id": "1",
          "time": "20210829 03:22"
        },
        {
          "id": "MDRW2100035123.1.5",
          "form": "안비리고",
          "original_form": "안비리고",
          "speaker_id": "1",
          "time": "20210829 03:22"
        },
        {
          "id": "MDRW2100035123.1.6",
          "form": "탱탱하고",
          "original_form": "탱탱하고",
          "speaker_id": "1",
          "time": "20210829 03:22"
        },
        {
          "id": "MDRW2100035123.1.7",
          "form": "갈치튀김도 먹었는데",
          "original_form": "갈치튀김도 먹었는데",
          "speaker_id": "1",
          "time": "20210829 03:22"
        },
        {
          "id": "MDRW2100035123.1.8",
          "form": "맛있었어요 ㅋㅋㅋ",
          "original_form": "맛있었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:22"
        },
        {
          "id": "MDRW2100035123.1.9",
          "form": "갈치넘좋은데ㅠㅠ",
          "original_form": "갈치넘좋은데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 03:22"
        },
        {
          "id": "MDRW2100035123.1.10",
          "form": "제주도라서 그런지",
          "original_form": "제주도라서 그런지",
          "speaker_id": "1",
          "time": "20210829 03:22"
        },
        {
          "id": "MDRW2100035123.1.11",
          "form": "생선 ㅠㅠㅠ",
          "original_form": "생선 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 03:22"
        },
        {
          "id": "MDRW2100035123.1.12",
          "form": "생일이라고 하니까 미역국 만들어주셨는데",
          "original_form": "생일이라고 하니까 미역국 만들어주셨는데",
          "speaker_id": "1",
          "time": "20210829 03:22"
        },
        {
          "id": "MDRW2100035123.1.13",
          "form": "고등어회도",
          "original_form": "고등어회도",
          "speaker_id": "2",
          "time": "20210829 03:23"
        },
        {
          "id": "MDRW2100035123.1.14",
          "form": "성게가 들어가있고",
          "original_form": "성게가 들어가있고",
          "speaker_id": "1",
          "time": "20210829 03:23"
        },
        {
          "id": "MDRW2100035123.1.15",
          "form": "신기했어요",
          "original_form": "신기했어요",
          "speaker_id": "1",
          "time": "20210829 03:23"
        },
        {
          "id": "MDRW2100035123.1.16",
          "form": "고등어회도 맛있나용?",
          "original_form": "고등어회도 맛있나용?",
          "speaker_id": "1",
          "time": "20210829 03:23"
        },
        {
          "id": "MDRW2100035123.1.17",
          "form": "맞아요 ㅋㅋㅋㅋ이모도 성게미역국해준느데",
          "original_form": "맞아요 ㅋㅋㅋㅋ이모도 성게미역국해준느데",
          "speaker_id": "2",
          "time": "20210829 03:23"
        },
        {
          "id": "MDRW2100035123.1.18",
          "form": "저도 못먹어봣어요",
          "original_form": "저도 못먹어봣어요",
          "speaker_id": "2",
          "time": "20210829 03:23"
        },
        {
          "id": "MDRW2100035123.1.19",
          "form": "아 갑자기 제주도 가고싶네요ㅋㅋㅋㅋ",
          "original_form": "아 갑자기 제주도 가고싶네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:23"
        },
        {
          "id": "MDRW2100035123.1.20",
          "form": "ㅠㅠㅠㅋㅋㅋ 진짜 신선해야먹을수있데요",
          "original_form": "ㅠㅠㅠㅋㅋㅋ 진짜 신선해야먹을수있데요",
