      "utterance": [
        {
          "id": "MDRW2100005947.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 17:12"
        },
        {
          "id": "MDRW2100005947.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210907 17:14"
        },
        {
          "id": "MDRW2100005947.1.3",
          "form": "여행 못간지도 오래되었다 그죠ㅠㅠ",
          "original_form": "여행 못간지도 오래되었다 그죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 17:14"
        },
        {
          "id": "MDRW2100005947.1.4",
          "form": "맞아요 마지막 여행 언제이세요?",
          "original_form": "맞아요 마지막 여행 언제이세요?",
          "speaker_id": "1",
          "time": "20210907 17:14"
        },
        {
          "id": "MDRW2100005947.1.5",
          "form": "저는 3년전쯤에 전주간게 마지막이예요",
          "original_form": "저는 3년전쯤에 전주간게 마지막이예요",
          "speaker_id": "2",
          "time": "20210907 17:15"
        },
        {
          "id": "MDRW2100005947.1.6",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210907 17:15"
        },
        {
          "id": "MDRW2100005947.1.7",
          "form": "헐 ... 엄청 오래되셨네요",
          "original_form": "헐 ... 엄청 오래되셨네요",
          "speaker_id": "1",
          "time": "20210907 17:17"
        },
        {
          "id": "MDRW2100005947.1.8",
          "form": "근데 저도인게 2년전쯤? 스키장 갔던게 마지막인것같아요",
          "original_form": "근데 저도인게 2년전쯤? 스키장 갔던게 마지막인것같아요",
          "speaker_id": "1",
          "time": "20210907 17:17"
        },
        {
          "id": "MDRW2100005947.1.9",
          "form": "스키장어디로 가셨어요?",
          "original_form": "스키장어디로 가셨어요?",
          "speaker_id": "2",
          "time": "20210907 17:19"
        },
        {
          "id": "MDRW2100005947.1.10",
          "form": "지산리조트로 갔다왔어요",
          "original_form": "지산리조트로 갔다왔어요",
          "speaker_id": "1",
          "time": "20210907 17:20"
        },
        {
          "id": "MDRW2100005947.1.11",
          "form": "원래 스키만 타다가 처음으로 보드탔던건데",
          "original_form": "원래 스키만 타다가 처음으로 보드탔던건데",
          "speaker_id": "1",
          "time": "20210907 17:20"
        },
        {
          "id": "MDRW2100005947.1.12",
          "form": "너무 재밌더라구요",
          "original_form": "너무 재밌더라구요",
          "speaker_id": "1",
          "time": "20210907 17:20"
        },
        {
          "id": "MDRW2100005947.1.13",
          "form": "보드가 더 취향이셨어요?",
          "original_form": "보드가 더 취향이셨어요?",
          "speaker_id": "2",
          "time": "20210907 17:21"
        },
        {
          "id": "MDRW2100005947.1.14",
          "form": "네 전 스키보단 보드가 재밌더라구요",
          "original_form": "네 전 스키보단 보드가 재밌더라구요",
          "speaker_id": "1",
          "time": "20210907 17:23"
        },
        {
          "id": "MDRW2100005947.1.15",
          "form": "혹시 코로나 끝나면 여행 가보고 싶은곳 있으세요?",
          "original_form": "혹시 코로나 끝나면 여행 가보고 싶은곳 있으세요?",
          "speaker_id": "2",
          "time": "20210907 17:24"
        },
        {
          "id": "MDRW2100005947.1.16",
          "form": "전 요즘 밤바다보고싶어요",
          "original_form": "전 요즘 밤바다보고싶어요",
          "speaker_id": "1",
          "time": "20210907 17:24"
        },
        {
          "id": "MDRW2100005947.1.17",
          "form": "name2님은 가보고 싶으신곳 있으세요?",
          "original_form": "&name2&님은 가보고 싶으신곳 있으세요?",
          "speaker_id": "1",
          "time": "20210907 17:24"
        },
        {
          "id": "MDRW2100005947.1.18",
          "form": "저는 안동이나 하동가고싶어요",
          "original_form": "저는 안동이나 하동가고싶어요",
          "speaker_id": "2",
          "time": "20210907 17:25"
        },
        {
          "id": "MDRW2100005947.1.19",
          "form": "밤바다좋죠 부산사람이라 가끔 가는데 바다는 보면 속이 시원해요",
          "original_form": "밤바다좋죠 부산사람이라 가끔 가는데 바다는 보면 속이 시원해요",
          "speaker_id": "2",
          "time": "20210907 17:26"
        },
        {
          "id": "MDRW2100005947.1.20",
          "form": "부산 가봤는데 저 시장 돌아다니는걸 좋아해서",
          "original_form": "부산 가봤는데 저 시장 돌아다니는걸 좋아해서",
          "speaker_id": "1",
          "time": "20210907 17:26"
        },
        {
          "id": "MDRW2100005947.1.21",
          "form": "부산 시장이 되게 재밌더라구요",
          "original_form": "부산 시장이 되게 재밌더라구요",
          "speaker_id": "1",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100005947.1.22",
          "form": "헉 저도 어디가면 시장 꼭가보는데!",
          "original_form": "헉 저도 어디가면 시장 꼭가보는데!",
          "speaker_id": "2",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100005947.1.23",
          "form": "시장구경재밌죠",
          "original_form": "시장구경재밌죠",
          "speaker_id": "2",
