      "utterance": [
        {
          "id": "MDRW2100008353.1.1",
          "form": "그러니까요 그래서 피부과 못가요",
          "original_form": "그러니까요 그래서 피부과 못가요",
          "speaker_id": "2",
          "time": "20210822 15:21"
        },
        {
          "id": "MDRW2100008353.1.2",
          "form": "딸이다 보니깐 더 신경쓰이시겠어요",
          "original_form": "딸이다 보니깐 더 신경쓰이시겠어요",
          "speaker_id": "1",
          "time": "20210822 15:21"
        },
        {
          "id": "MDRW2100008353.1.3",
          "form": "네 전 나본적 없어서 참 어찌할지",
          "original_form": "네 전 나본적 없어서 참 어찌할지",
          "speaker_id": "2",
          "time": "20210822 15:22"
        },
        {
          "id": "MDRW2100008353.1.4",
          "form": "좋다는건 다 발라요",
          "original_form": "좋다는건 다 발라요",
          "speaker_id": "2",
          "time": "20210822 15:22"
        },
        {
          "id": "MDRW2100008353.1.5",
          "form": "그래야죠 ㅠㅠ 피부는 다시 회복하기가 힘들더라구요",
          "original_form": "그래야죠 ㅠㅠ 피부는 다시 회복하기가 힘들더라구요",
          "speaker_id": "1",
          "time": "20210822 15:23"
        },
        {
          "id": "MDRW2100008353.1.6",
          "form": "맞아요 요새 느껴져요",
          "original_form": "맞아요 요새 느껴져요",
          "speaker_id": "2",
          "time": "20210822 15:23"
        },
        {
          "id": "MDRW2100008353.1.7",
          "form": "그걸 저는 30대중반이되서야 알아서.. 진짜 많이 후회해요 ㅠㅠ",
          "original_form": "그걸 저는 30대중반이되서야 알아서.. 진짜 많이 후회해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:23"
        },
        {
          "id": "MDRW2100008353.1.8",
          "form": "미리 관리 해야죠",
          "original_form": "미리 관리 해야죠",
          "speaker_id": "2",
          "time": "20210822 15:23"
        },
        {
          "id": "MDRW2100008353.1.9",
          "form": "전 40넘어 이제 아니 반성해요",
          "original_form": "전 40넘어 이제 아니 반성해요",
          "speaker_id": "2",
          "time": "20210822 15:23"
        },
        {
          "id": "MDRW2100008353.1.10",
          "form": "흑.. 몸도 확실이 축축쳐지고 관절도 안 좋고.. 벌써 저는 그래요 ㅠㅠ",
          "original_form": "흑.. 몸도 확실이 축축쳐지고 관절도 안 좋고.. 벌써 저는 그래요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:24"
        },
        {
          "id": "MDRW2100008353.1.11",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210822 15:24"
        },
        {
          "id": "MDRW2100008353.1.12",
          "form": "전 맨홀에 빠져 수술후 무릎이",
          "original_form": "전 맨홀에 빠져 수술후 무릎이",
          "speaker_id": "2",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100008353.1.13",
          "form": "다들 들으면 웃어요",
          "original_form": "다들 들으면 웃어요",
          "speaker_id": "2",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100008353.1.14",
          "form": "코미디죠",
          "original_form": "코미디죠",
          "speaker_id": "2",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100008353.1.15",
          "form": "2주나 입ㄴ원했었어요",
          "original_form": "2주나 입ㄴ원했었어요",
          "speaker_id": "2",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100008353.1.16",
          "form": "맨홀이 열려있었나요?!!",
          "original_form": "맨홀이 열려있었나요?!!",
          "speaker_id": "1",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100008353.1.17",
          "form": "맨홀에 다리 빠지는 사람이 있더라구요 저요",
          "original_form": "맨홀에 다리 빠지는 사람이 있더라구요 저요",
          "speaker_id": "2",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100008353.1.18",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100008353.1.19",
          "form": "아... 그게 왜 열려있었지 ㅠㅠㅠㅠㅠ",
          "original_form": "아... 그게 왜 열려있었지 ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100008353.1.20",
          "form": "풀사이에 안보였어요",
          "original_form": "풀사이에 안보였어요",
          "speaker_id": "2",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100008353.1.21",
