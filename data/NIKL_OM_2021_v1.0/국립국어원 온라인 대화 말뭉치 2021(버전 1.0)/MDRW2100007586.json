          "time": "20210822 03:33"
        },
        {
          "id": "MDRW2100007586.1.4",
          "form": "학원!",
          "original_form": "학원!",
          "speaker_id": "2",
          "time": "20210822 03:33"
        },
        {
          "id": "MDRW2100007586.1.5",
          "form": "오 학원",
          "original_form": "오 학원",
          "speaker_id": "1",
          "time": "20210822 03:33"
        },
        {
          "id": "MDRW2100007586.1.6",
          "form": "너는???",
          "original_form": "너는???",
          "speaker_id": "2",
          "time": "20210822 03:33"
        },
        {
          "id": "MDRW2100007586.1.7",
          "form": "공부 쫌 하나본데~",
          "original_form": "공부 쫌 하나본데~",
          "speaker_id": "1",
          "time": "20210822 03:33"
        },
        {
          "id": "MDRW2100007586.1.8",
          "form": "나는 식당이랑 병원 ㅎㅋㅎ",
          "original_form": "나는 식당이랑 병원 ㅎㅋㅎ",
          "speaker_id": "1",
          "time": "20210822 03:33"
        },
        {
          "id": "MDRW2100007586.1.9",
          "form": "헐헐 병원 어떤거??",
          "original_form": "헐헐 병원 어떤거??",
          "speaker_id": "2",
          "time": "20210822 03:33"
        },
        {
          "id": "MDRW2100007586.1.10",
          "form": "쿠팡 알바도 해보려구",
          "original_form": "쿠팡 알바도 해보려구",
          "speaker_id": "1",
          "time": "20210822 03:33"
        },
        {
          "id": "MDRW2100007586.1.11",
          "form": "한의원이용",
          "original_form": "한의원이용",
          "speaker_id": "1",
          "time": "20210822 03:33"
        },
        {
          "id": "MDRW2100007586.1.12",
          "form": "병원 아르바이트 신기행",
          "original_form": "병원 아르바이트 신기행",
          "speaker_id": "2",
          "time": "20210822 03:33"
        },
        {
          "id": "MDRW2100007586.1.13",
          "form": "아아 접수해주시는분??!",
          "original_form": "아아 접수해주시는분??!",
          "speaker_id": "2",
          "time": "20210822 03:34"
        },
        {
          "id": "MDRW2100007586.1.14",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "1",
          "time": "20210822 03:34"
        },
        {
          "id": "MDRW2100007586.1.15",
          "form": "어때 힘들어??",
          "original_form": "어때 힘들어??",
          "speaker_id": "2",
          "time": "20210822 03:34"
        },
        {
          "id": "MDRW2100007586.1.16",
          "form": "그거랑 잡일이랑..... 핫팩 깔아주는 거?",
          "original_form": "그거랑 잡일이랑..... 핫팩 깔아주는 거?",
          "speaker_id": "1",
          "time": "20210822 03:34"
        },
        {
          "id": "MDRW2100007586.1.17",
          "form": "개꿀이지",
          "original_form": "개꿀이지",
          "speaker_id": "1",
          "time": "20210822 03:34"
        },
        {
          "id": "MDRW2100007586.1.18",
          "form": "아아ㅠㅠ 물리치료이런거 돕는거구낭ㅠㅠ",
          "original_form": "아아ㅠㅠ 물리치료이런거 돕는거구낭ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 03:34"
        },
        {
          "id": "MDRW2100007586.1.19",
          "form": "데스크에서 트위터하구 쇼핑하구 그래",
          "original_form": "데스크에서 트위터하구 쇼핑하구 그래",
