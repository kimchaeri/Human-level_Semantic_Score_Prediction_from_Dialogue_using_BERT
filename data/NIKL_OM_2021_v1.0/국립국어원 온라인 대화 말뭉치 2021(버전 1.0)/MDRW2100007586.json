          "speaker_id": "2",
          "time": "20210822 03:34"
        },
        {
          "id": "MDRW2100007586.1.22",
          "form": "아유 당연하지",
          "original_form": "아유 당연하지",
          "speaker_id": "1",
          "time": "20210822 03:34"
        },
        {
          "id": "MDRW2100007586.1.23",
