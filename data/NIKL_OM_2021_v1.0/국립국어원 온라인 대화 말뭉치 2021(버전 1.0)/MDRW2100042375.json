      "utterance": [
        {
          "id": "MDRW2100042375.1.1",
          "form": "몸에 지장이요.?",
          "original_form": "몸에 지장이요.?",
          "speaker_id": "1",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042375.1.2",
          "form": "넹..알러지잇으면...",
          "original_form": "넹..알러지잇으면...",
          "speaker_id": "2",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042375.1.3",
          "form": "먹으면..",
          "original_form": "먹으면..",
          "speaker_id": "2",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042375.1.4",
          "form": "멀쩡했나봐요ㅋㅋ",
          "original_form": "멀쩡했나봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042375.1.5",
          "form": "꼬막이나 새우드실때 가려우세요?",
          "original_form": "꼬막이나 새우드실때 가려우세요?",
          "speaker_id": "1",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042375.1.6",
          "form": "아니요 ㅋㅋㅋㅋ",
          "original_form": "아니요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042375.1.7",
          "form": "저는 원래 그런줄알았어욪ㅋㅋ",
          "original_form": "저는 원래 그런줄알았어욪ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042375.1.8",
          "form": "몇십년을ㅋㅋ",
          "original_form": "몇십년을ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042375.1.9",
          "form": "헐 잇으세요알러지?",
          "original_form": "헐 잇으세요알러지?",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042375.1.10",
          "form": "몰랐는데 입주위 가려운겈ㅋ",
          "original_form": "몰랐는데 입주위 가려운겈ㅋ",
          "speaker_id": "1",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042375.1.11",
          "form": "네ㅋㅋ",
          "original_form": "네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042375.1.12",
          "form": "헉 ㅋㅋㅋㅋ",
          "original_form": "헉 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042375.1.13",
          "form": "그냥 드시나요 요즘은",
          "original_form": "그냥 드시나요 요즘은",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042375.1.14",
          "form": "그냥 먹어요ㅋㅋ 큰문젠 아니니까요 ㅋㅋ",
          "original_form": "그냥 먹어요ㅋㅋ 큰문젠 아니니까요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042375.1.15",
          "form": "ㅋㅋㅋㅋㅋ 맛을 포기하기 어렵죠",
          "original_form": "ㅋㅋㅋㅋㅋ 맛을 포기하기 어렵죠",
          "speaker_id": "2",
          "time": "20210906 23:06"
        },
        {
          "id": "MDRW2100042375.1.16",
          "form": "ㅋㅋ네 새우는 포기할수 없어요!!! 껍질만 조심하면 됩니다",
          "original_form": "ㅋㅋ네 새우는 포기할수 없어요!!! 껍질만 조심하면 됩니다",
          "speaker_id": "1",
          "time": "20210906 23:06"
        },
        {
          "id": "MDRW2100042375.1.17",
          "form": "아하 막 부어오르진 않나요",
          "original_form": "아하 막 부어오르진 않나요",
          "speaker_id": "2",
          "time": "20210906 23:08"
        },
        {
          "id": "MDRW2100042375.1.18",
          "form": "네네 가렵기만ㅋㅋ 전 다들 그런줄 알았어요",
