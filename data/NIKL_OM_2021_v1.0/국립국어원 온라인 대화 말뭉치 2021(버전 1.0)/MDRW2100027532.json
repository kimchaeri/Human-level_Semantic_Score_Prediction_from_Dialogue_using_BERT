        },
        {
          "id": "MDRW2100027532.1.6",
          "form": "친구가 정욱진 알더라",
          "original_form": "친구가 정욱진 알더라",
          "speaker_id": "1",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027532.1.7",
          "form": "어떻게?",
          "original_form": "어떻게?",
