      "utterance": [
        {
          "id": "MDRW2100019522.1.1",
          "form": "안녕하세요? ㄷ화괜찮으세요?",
          "original_form": "안녕하세요? ㄷ화괜찮으세요?",
          "speaker_id": "1",
          "time": "20210827 20:02"
        },
        {
          "id": "MDRW2100019522.1.2",
          "form": "안녕하세요! 답이 많이 늦었지만 대화 괜찮습니다ㅎㅎ",
          "original_form": "안녕하세요! 답이 많이 늦었지만 대화 괜찮습니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100019522.1.3",
          "form": "저녁 식사 하셨나요?",
          "original_form": "저녁 식사 하셨나요?",
          "speaker_id": "2",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100019522.1.4",
          "form": "네네 지금 먹고 왔어요",
          "original_form": "네네 지금 먹고 왔어요",
          "speaker_id": "1",
          "time": "20210830 20:09"
        },
        {
          "id": "MDRW2100019522.1.5",
          "form": "그냥 밥이랑 김이랑 반찬 가지구이를 먹었습니다",
          "original_form": "그냥 밥이랑 김이랑 반찬 가지구이를 먹었습니다",
          "speaker_id": "1",
          "time": "20210830 20:10"
        },
        {
          "id": "MDRW2100019522.1.6",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210830 20:10"
        },
        {
          "id": "MDRW2100019522.1.7",
          "form": "저는 김밥 먹었어요",
          "original_form": "저는 김밥 먹었어요",
          "speaker_id": "2",
          "time": "20210830 20:13"
        },
        {
          "id": "MDRW2100019522.1.8",
          "form": "편의점 김밥이요ㅋㅋㅋ",
          "original_form": "편의점 김밥이요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:13"
        },
        {
          "id": "MDRW2100019522.1.9",
          "form": "가지 좋아하세요?",
          "original_form": "가지 좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 20:13"
        },
        {
          "id": "MDRW2100019522.1.10",
          "form": "좀 더 좋은걸 드시지 ㅠㅠ",
          "original_form": "좀 더 좋은걸 드시지 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100019522.1.11",
          "form": "사줄것도 아니면서 잔소리하기 ㅋㅋㅋ",
          "original_form": "사줄것도 아니면서 잔소리하기 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100019522.1.12",
          "form": "전 가지는 가지냐 오이냐 같은 재료보단",
          "original_form": "전 가지는 가지냐 오이냐 같은 재료보단",
          "speaker_id": "1",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100019522.1.13",
          "form": "조리법에따라 호불호가 갈려요",
          "original_form": "조리법에따라 호불호가 갈려요",
          "speaker_id": "1",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100019522.1.14",
          "form": "아하 무슨 말인지 알겠어요.",
          "original_form": "아하 무슨 말인지 알겠어요.",
          "speaker_id": "2",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100019522.1.15",
          "form": "저도 엄마가 해준 가지무침은 먹기 싫어서 도망다녔는데",
          "original_form": "저도 엄마가 해준 가지무침은 먹기 싫어서 도망다녔는데",
          "speaker_id": "2",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100019522.1.16",
          "form": "에프에서 구운 가지는 넘 맛있어서 놀랐거든요ㅋㅋ",
          "original_form": "에프에서 구운 가지는 넘 맛있어서 놀랐거든요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100019522.1.17",
          "form": "가지무침은 느낌이 물컹해서",
          "original_form": "가지무침은 느낌이 물컹해서",
          "speaker_id": "1",
          "time": "20210830 20:16"
        },
        {
          "id": "MDRW2100019522.1.18",
          "form": "가래침 삼키는 느낌이죠",
          "original_form": "가래침 삼키는 느낌이죠",
          "speaker_id": "1",
          "time": "20210830 20:16"
        },
        {
          "id": "MDRW2100019522.1.19",
          "form": "비유가 정말 리얼하고 싫으네요...",
          "original_form": "비유가 정말 리얼하고 싫으네요...",
          "speaker_id": "2",
          "time": "20210830 20:16"
        },
        {
          "id": "MDRW2100019522.1.20",
          "form": "혹시 어릴때 좀 예민한 타입이셨나요?",
          "original_form": "혹시 어릴때 좀 예민한 타입이셨나요?",
          "speaker_id": "1",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100019522.1.21",
          "form": "넹 예민킹ㅋㅋㅋ 지금도 편식은 심한 편이예요",
          "original_form": "넹 예민킹ㅋㅋㅋ 지금도 편식은 심한 편이예요",
          "speaker_id": "2",
          "time": "20210830 20:18"
        },
        {
          "id": "MDRW2100019522.1.22",
          "form": "그럼 안익숙한 촉감을 대개 싫어해ㅐ요",
          "original_form": "그럼 안익숙한 촉감을 대개 싫어해ㅐ요",
          "speaker_id": "1",
          "time": "20210830 20:18"
        },
        {
          "id": "MDRW2100019522.1.23",
          "form": "나이가 들면 의지로 누룰순 있지만ㅋㅋ",
          "original_form": "나이가 들면 의지로 누룰순 있지만ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:19"
        },
        {
          "id": "MDRW2100019522.1.24",
          "form": "마저요 촉감이랑 냄새...",
          "original_form": "마저요 촉감이랑 냄새...",
          "speaker_id": "2",
          "time": "20210830 20:19"
        },
        {
          "id": "MDRW2100019522.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 저는 이제 어른이 되었고 먹기 싫은 걸 안 먹을 수 있는 사람이 되었습니다.",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 저는 이제 어른이 되었고 먹기 싫은 걸 안 먹을 수 있는 사람이 되었습니다.",
          "speaker_id": "2",
          "time": "20210830 20:19"
        },
        {
          "id": "MDRW2100019522.1.26",
          "form": "하지만 어른도 먹기 싫은걸 억지로 먹을떄가 있어요",
          "original_form": "하지만 어른도 먹기 싫은걸 억지로 먹을떄가 있어요",
          "speaker_id": "1",
          "time": "20210830 20:19"
        },
        {
          "id": "MDRW2100019522.1.27",
          "form": "환자가 되어서 코 ㅋ에 튜브를 꽂음ㄴ",
          "original_form": "환자가 되어서 코 ㅋ에 튜브를 꽂음ㄴ",
          "speaker_id": "1",
          "time": "20210830 20:19"
        },
        {
          "id": "MDRW2100019522.1.28",
          "form": "다시 꿀렁한 유동식을 먹어야하죠",
          "original_form": "다시 꿀렁한 유동식을 먹어야하죠",
          "speaker_id": "1",
          "time": "20210830 20:20"
        },
        {
          "id": "MDRW2100019522.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 맞어요 약은... 어쩔 수 없죠...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 맞어요 약은... 어쩔 수 없죠...",
          "speaker_id": "2",
          "time": "20210830 20:20"
        }
      ]
