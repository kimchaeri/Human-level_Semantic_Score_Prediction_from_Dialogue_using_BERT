      "utterance": [
        {
          "id": "MDRW2100009594.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210823 01:16"
        },
        {
          "id": "MDRW2100009594.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210823 07:04"
        },
        {
          "id": "MDRW2100009594.1.3",
          "form": "책 좋아하세요?",
          "original_form": "책 좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 07:04"
        },
        {
          "id": "MDRW2100009594.1.4",
          "form": "책..많이는 안 읽었는데",
          "original_form": "책..많이는 안 읽었는데",
          "speaker_id": "1",
          "time": "20210823 13:29"
        },
        {
          "id": "MDRW2100009594.1.5",
          "form": "읽으려고 노력중이에요",
          "original_form": "읽으려고 노력중이에요",
          "speaker_id": "1",
          "time": "20210823 13:30"
        },
        {
          "id": "MDRW2100009594.1.6",
          "form": "다들 그렇죠 ㅎㅎ",
          "original_form": "다들 그렇죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 14:11"
        },
        {
          "id": "MDRW2100009594.1.7",
          "form": "책읽는 시간내는게 은근 어려운것같아요",
          "original_form": "책읽는 시간내는게 은근 어려운것같아요",
          "speaker_id": "2",
          "time": "20210823 14:11"
        },
        {
          "id": "MDRW2100009594.1.8",
          "form": "재밌개 보신거 있나요?",
          "original_form": "재밌개 보신거 있나요?",
          "speaker_id": "2",
          "time": "20210823 14:11"
        },
        {
          "id": "MDRW2100009594.1.9",
          "form": "저는 최근에 비혼 에세이 <이번 생은 나 혼자 산다> 랑 채식주의자 재밌게 읽었어요!",
          "original_form": "저는 최근에 비혼 에세이 <이번 생은 나 혼자 산다> 랑 채식주의자 재밌게 읽었어요!",
          "speaker_id": "1",
          "time": "20210823 14:11"
        },
        {
          "id": "MDRW2100009594.1.10",
          "form": "채식주의자는 재밌다고 많이 들었는데!",
          "original_form": "채식주의자는 재밌다고 많이 들었는데!",
          "speaker_id": "2",
          "time": "20210823 14:12"
        },
        {
          "id": "MDRW2100009594.1.11",
          "form": "에세이도 재밌을것같네요",
          "original_form": "에세이도 재밌을것같네요",
          "speaker_id": "2",
          "time": "20210823 14:12"
        },
        {
          "id": "MDRW2100009594.1.12",
          "form": "저도 읽어봐야겠어요",
          "original_form": "저도 읽어봐야겠어요",
          "speaker_id": "2",
          "time": "20210823 14:12"
        },
        {
          "id": "MDRW2100009594.1.13",
          "form": "ㅎㅎname2님은 책 좋아하세요?",
          "original_form": "ㅎㅎ&name2&님은 책 좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 14:13"
        },
        {
          "id": "MDRW2100009594.1.14",
          "form": "혹시 따님이 기가 세요라고 들어보셨나요?",
          "original_form": "혹시 따님이 기가 세요라고 들어보셨나요?",
          "speaker_id": "2",
          "time": "20210823 14:13"
        },
        {
          "id": "MDRW2100009594.1.15",
          "form": "저는 책 좋아하죠 ㅎㅎ",
          "original_form": "저는 책 좋아하죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 14:13"
        },
        {
          "id": "MDRW2100009594.1.16",
          "form": "최근에 좋아졌어요",
          "original_form": "최근에 좋아졌어요",
          "speaker_id": "2",
          "time": "20210823 14:13"
        },
        {
          "id": "MDRW2100009594.1.17",
          "form": "헐!!! 네 하말넘많 분들이 쓰신 책이잖아요!!",
          "original_form": "헐!!! 네 하말넘많 분들이 쓰신 책이잖아요!!",
          "speaker_id": "1",
          "time": "20210823 14:14"
        },
        {
          "id": "MDRW2100009594.1.18",
          "form": "근데 아직 못읽어봤어요ㅜ",
          "original_form": "근데 아직 못읽어봤어요ㅜ",
          "speaker_id": "1",
          "time": "20210823 14:14"
        },
        {
          "id": "MDRW2100009594.1.19",
          "form": "저도 유튜브 재밌게보고있거든요 ㅎㅎ",
          "original_form": "저도 유튜브 재밌게보고있거든요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 14:15"
        },
        {
          "id": "MDRW2100009594.1.20",
          "form": "엄청 술술 잘읽히더라고요!!",
          "original_form": "엄청 술술 잘읽히더라고요!!",
          "speaker_id": "2",
          "time": "20210823 14:15"
        },
        {
          "id": "MDRW2100009594.1.21",
          "form": "정말요?? 사실 저는 살면서 기가 세다는 말을 한 번도 들은 적이 없어요",
          "original_form": "정말요?? 사실 저는 살면서 기가 세다는 말을 한 번도 들은 적이 없어요",
          "speaker_id": "1",
          "time": "20210823 14:16"
        },
        {
          "id": "MDRW2100009594.1.22",
          "form": "없어서",
          "original_form": "없어서",
          "speaker_id": "1",
          "time": "20210823 14:16"
        },
        {
          "id": "MDRW2100009594.1.23",
          "form": "먼가 읽으면서 공감이 안 될 수도 있겠다는 생각이 들었었어요",
          "original_form": "먼가 읽으면서 공감이 안 될 수도 있겠다는 생각이 들었었어요",
          "speaker_id": "1",
          "time": "20210823 14:16"
        },
        {
          "id": "MDRW2100009594.1.24",
          "form": "ㅋㅋㅋㅋㅋ너무 웃기는 핑계죠",
          "original_form": "ㅋㅋㅋㅋㅋ너무 웃기는 핑계죠",
          "speaker_id": "1",
          "time": "20210823 14:16"
        },
        {
          "id": "MDRW2100009594.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ그럴수도 있겠어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그럴수도 있겠어요",
          "speaker_id": "2",
          "time": "20210823 14:21"
        },
        {
          "id": "MDRW2100009594.1.26",
          "form": "근데 저도 기가세다는 말을 잘 안들어봤는데",
          "original_form": "근데 저도 기가세다는 말을 잘 안들어봤는데",
          "speaker_id": "2",
          "time": "20210823 14:21"
        },
        {
          "id": "MDRW2100009594.1.27",
          "form": "공감가는 내용이 많더라고요",
          "original_form": "공감가는 내용이 많더라고요",
          "speaker_id": "2",
          "time": "20210823 14:21"
        },
        {
          "id": "MDRW2100009594.1.28",
          "form": "자기계발이나.. 미래 계획같은 것도 생각하게됐고요",
          "original_form": "자기계발이나.. 미래 계획같은 것도 생각하게됐고요",
          "speaker_id": "2",
          "time": "20210823 14:22"
        },
        {
          "id": "MDRW2100009594.1.29",
          "form": "우와!! 자기계발이랑 미래계획이라니 지금 저한테 너무 필요한 내용이예요",
          "original_form": "우와!! 자기계발이랑 미래계획이라니 지금 저한테 너무 필요한 내용이예요",
          "speaker_id": "1",
          "time": "20210823 14:23"
        },
        {
          "id": "MDRW2100009594.1.30",
          "form": "가볍게 읽기 좋으실거예요 ㅎㅎ 2시간? 그정도면 되겠더라고요",
          "original_form": "가볍게 읽기 좋으실거예요 ㅎㅎ 2시간? 그정도면 되겠더라고요",
          "speaker_id": "2",
          "time": "20210823 14:25"
        },
        {
          "id": "MDRW2100009594.1.31",
          "form": "저 저 책보고 책읽기시작해서",
          "original_form": "저 저 책보고 책읽기시작해서",
          "speaker_id": "2",
          "time": "20210823 14:25"
        },
        {
          "id": "MDRW2100009594.1.32",
          "form": "근 3년간 읽은 책 권수만큼 읽었어요 ㅋㅋㅋ진짜 작고 소박했다..",
          "original_form": "근 3년간 읽은 책 권수만큼 읽었어요 ㅋㅋㅋ진짜 작고 소박했다..",
          "speaker_id": "2",
          "time": "20210823 14:25"
        },
        {
          "id": "MDRW2100009594.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋ책읽는것도 공부처럼 진짜 시간을 내서 읽어야 하더라구요ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ책읽는것도 공부처럼 진짜 시간을 내서 읽어야 하더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210823 14:26"
        },
        {
          "id": "MDRW2100009594.1.34",
          "form": "요즘 여자들이 비혼이랑 페미니즘 도서에 관심을 정말 많이 가지는 거 같아요",
          "original_form": "요즘 여자들이 비혼이랑 페미니즘 도서에 관심을 정말 많이 가지는 거 같아요",
          "speaker_id": "1",
          "time": "20210823 14:27"
        },
        {
          "id": "MDRW2100009594.1.35",
          "form": "그래서 저는 대중교통 타면서 읽었어요 ㅎㅎ",
          "original_form": "그래서 저는 대중교통 타면서 읽었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 18:41"
        },
        {
          "id": "MDRW2100009594.1.36",
          "form": "지금은 시간 남을때도 못참고 읽고있죠",
          "original_form": "지금은 시간 남을때도 못참고 읽고있죠",
          "speaker_id": "2",
          "time": "20210823 18:41"
        },
        {
          "id": "MDRW2100009594.1.37",
          "form": "비혼 페미니즘.. 관심 안 갖기에는 세상이 너무 여자한테 퍽퍽해요...ㅠㅜ",
          "original_form": "비혼 페미니즘.. 관심 안 갖기에는 세상이 너무 여자한테 퍽퍽해요...ㅠㅜ",
          "speaker_id": "2",
          "time": "20210823 18:41"
        },
        {
          "id": "MDRW2100009594.1.38",
          "form": "마자요..",
          "original_form": "마자요..",
          "speaker_id": "1",
          "time": "20210823 18:42"
        }
      ]
