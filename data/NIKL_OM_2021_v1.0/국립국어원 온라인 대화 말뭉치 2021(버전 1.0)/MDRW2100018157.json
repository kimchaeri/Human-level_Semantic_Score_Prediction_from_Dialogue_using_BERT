      "utterance": [
        {
          "id": "MDRW2100018157.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018157.1.2",
          "form": "요즘 날씨가 오락가락하네요",
          "original_form": "요즘 날씨가 오락가락하네요",
          "speaker_id": "1",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018157.1.3",
          "form": "맞아요. 태풍오고 비오고 해 쨍쨍찌고 오락가락 많이해요.",
          "original_form": "맞아요. 태풍오고 비오고 해 쨍쨍찌고 오락가락 많이해요.",
          "speaker_id": "2",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100018157.1.4",
          "form": "기후변화가 휙휙 바뀌는걸 체감하게 되네요.",
          "original_form": "기후변화가 휙휙 바뀌는걸 체감하게 되네요.",
          "speaker_id": "2",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018157.1.5",
          "form": "요새 며칠동안 비만오다가",
          "original_form": "요새 며칠동안 비만오다가",
          "speaker_id": "1",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018157.1.6",
          "form": "오늘 해떠서 더운거보고",
          "original_form": "오늘 해떠서 더운거보고",
          "speaker_id": "1",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018157.1.7",
          "form": "아직은 여름이구나 느끼네요",
          "original_form": "아직은 여름이구나 느끼네요",
          "speaker_id": "1",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018157.1.8",
          "form": "옛날에는 그래도 봄, 여름, 가을, 겨울이 있었는데 지구온난화가 심해져서 여름과 겨울이 길더라고요. 특히 여름!",
          "original_form": "옛날에는 그래도 봄, 여름, 가을, 겨울이 있었는데 지구온난화가 심해져서 여름과 겨울이 길더라고요. 특히 여름!",
          "speaker_id": "2",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018157.1.9",
          "form": "여름이랑 겨울 둘 중에 어느 날씨가 더 좋으신가요",
          "original_form": "여름이랑 겨울 둘 중에 어느 날씨가 더 좋으신가요",
          "speaker_id": "1",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100018157.1.10",
          "form": "둘 중엔 겨울이 더 좋아요.",
          "original_form": "둘 중엔 겨울이 더 좋아요.",
          "speaker_id": "2",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018157.1.11",
          "form": "추운것도 싫지만 아무래도 더운게 더 힘들죠.",
          "original_form": "추운것도 싫지만 아무래도 더운게 더 힘들죠.",
          "speaker_id": "2",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018157.1.12",
          "form": "그렇군요 저는 반대에요",
          "original_form": "그렇군요 저는 반대에요",
          "speaker_id": "1",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018157.1.13",
          "form": "겨울보단 여름이 더 편하더라구여",
          "original_form": "겨울보단 여름이 더 편하더라구여",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018157.1.14",
          "form": "물론 여름되면 죽어나지만",
          "original_form": "물론 여름되면 죽어나지만",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018157.1.15",
          "form": "겨울엔 활동하는게 너무 제한적이라 ㅋㅋ",
          "original_form": "겨울엔 활동하는게 너무 제한적이라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018157.1.16",
          "form": "확실히 겨울 활동은 제한이 있죠.",
          "original_form": "확실히 겨울 활동은 제한이 있죠.",
          "speaker_id": "2",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018157.1.17",
          "form": "주로 겨울에는 집에만 있으신편인가요",
          "original_form": "주로 겨울에는 집에만 있으신편인가요",
          "speaker_id": "1",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018157.1.18",
          "form": "네. 아무래도 좋아하긴 하지만 추위를 유독 타서 집에서 따듯하게 있네요.",
          "original_form": "네. 아무래도 좋아하긴 하지만 추위를 유독 타서 집에서 따듯하게 있네요.",
          "speaker_id": "2",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018157.1.19",
          "form": "활동을 잘 안하기도 하고요.",
          "original_form": "활동을 잘 안하기도 하고요.",
          "speaker_id": "2",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018157.1.20",
          "form": "저도 큰 활동은 안하지만",
          "original_form": "저도 큰 활동은 안하지만",
          "speaker_id": "1",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018157.1.21",
          "form": "대학생이라서",
          "original_form": "대학생이라서",
          "speaker_id": "1",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018157.1.22",
          "form": "어쩔수없이 등교해야할땐",
          "original_form": "어쩔수없이 등교해야할땐",
          "speaker_id": "1",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018157.1.23",
          "form": "차라리 여름이 편한거같더라구여",
          "original_form": "차라리 여름이 편한거같더라구여",
          "speaker_id": "1",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018157.1.24",
          "form": "겨울엔 눈이나 눈과 비랑 둘 다 내리면 움직일때 불편하니까요.",
          "original_form": "겨울엔 눈이나 눈과 비랑 둘 다 내리면 움직일때 불편하니까요.",
          "speaker_id": "2",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018157.1.25",
          "form": "여름은 하늘도 화창하고요!",
          "original_form": "여름은 하늘도 화창하고요!",
          "speaker_id": "2",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018157.1.26",
          "form": "이제 추워질텐데",
          "original_form": "이제 추워질텐데",
          "speaker_id": "1",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018157.1.27",
          "form": "날씨 추우면 감기걸릴까봐",
          "original_form": "날씨 추우면 감기걸릴까봐",
          "speaker_id": "1",
          "time": "20210826 23:14"
        },
        {
          "id": "MDRW2100018157.1.28",
          "form": "걱정이네요",
          "original_form": "걱정이네요",
          "speaker_id": "1",
          "time": "20210826 23:14"
        }
      ]
    }
  ]
