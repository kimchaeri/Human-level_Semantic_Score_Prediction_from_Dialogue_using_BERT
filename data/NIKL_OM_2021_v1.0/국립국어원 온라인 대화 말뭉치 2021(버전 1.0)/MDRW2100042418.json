      "utterance": [
        {
          "id": "MDRW2100042418.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042418.1.2",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "2",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042418.1.3",
          "form": "오늘 저녁 뭐드셨어요?",
          "original_form": "오늘 저녁 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042418.1.4",
          "form": "목살 조금 구워먹었어요",
          "original_form": "목살 조금 구워먹었어요",
          "speaker_id": "2",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042418.1.5",
          "form": "오오 고기 맛있져",
          "original_form": "오오 고기 맛있져",
          "speaker_id": "1",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042418.1.6",
          "form": "목살이랑 삼겹살중에 목살더선호하시나요",
          "original_form": "목살이랑 삼겹살중에 목살더선호하시나요",
          "speaker_id": "1",
          "time": "20210906 22:05"
        },
        {
          "id": "MDRW2100042418.1.7",
          "form": "삼겹살을 더 좋아하긴 해요",
          "original_form": "삼겹살을 더 좋아하긴 해요",
          "speaker_id": "2",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042418.1.8",
          "form": "오늘은 목살 남은게 있었거든요ㅋㅋ",
          "original_form": "오늘은 목살 남은게 있었거든요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042418.1.9",
          "form": "ㅋㅋㅋㅋㅋ아하 집에있는걸로",
          "original_form": "ㅋㅋㅋㅋㅋ아하 집에있는걸로",
          "speaker_id": "1",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100042418.1.10",
          "form": "저도 삼겹살파 이긴해여",
          "original_form": "저도 삼겹살파 이긴해여",
          "speaker_id": "1",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100042418.1.11",
          "form": "육류고기중엔 어떤고기젤좋아하세요?",
          "original_form": "육류고기중엔 어떤고기젤좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100042418.1.12",
          "form": "돼지고기요.",
          "original_form": "돼지고기요.",
          "speaker_id": "2",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100042418.1.13",
          "form": "저도 1순위 돼지고기 그다음닭고기입니다 ㅋㅋㅋㅋ",
          "original_form": "저도 1순위 돼지고기 그다음닭고기입니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042418.1.14",
          "form": "닭고기도 좋아요. 치킨 맛있져ㅋㅋ",
          "original_form": "닭고기도 좋아요. 치킨 맛있져ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042418.1.15",
          "form": "네 ㅋㅋㅋㅋㅋ치킨은 일주일에1번은 시켜먹는것같아요",
          "original_form": "네 ㅋㅋㅋㅋㅋ치킨은 일주일에1번은 시켜먹는것같아요",
          "speaker_id": "1",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042418.1.16",
          "form": "맞아요. 저도 주말에 시켜먹었어요",
          "original_form": "맞아요. 저도 주말에 시켜먹었어요",
          "speaker_id": "2",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100042418.1.17",
          "form": "어디거 자주 드세여?",
          "original_form": "어디거 자주 드세여?",
          "speaker_id": "2",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100042418.1.18",
          "form": "전 교촌 허니콤보랑 요즘은 노랑통닭 알마치여",
          "original_form": "전 교촌 허니콤보랑 요즘은 노랑통닭 알마치여",
          "speaker_id": "1",
