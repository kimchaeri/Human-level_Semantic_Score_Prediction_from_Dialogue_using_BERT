      "utterance": [
        {
          "id": "MDRW2100010204.1.1",
          "form": "너 나중에 자취할거임?",
          "original_form": "너 나중에 자취할거임?",
          "speaker_id": "1",
          "time": "20210824 11:18"
        },
        {
          "id": "MDRW2100010204.1.2",
          "form": "응 기회와 돈만 된다면 할 거야",
          "original_form": "응 기회와 돈만 된다면 할 거야",
          "speaker_id": "2",
          "time": "20210824 11:57"
        },
        {
          "id": "MDRW2100010204.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210824 11:57"
        },
        {
          "id": "MDRW2100010204.1.4",
          "form": "아직모르겠음",
          "original_form": "아직모르겠음",
          "speaker_id": "1",
          "time": "20210824 12:32"
        },
        {
          "id": "MDRW2100010204.1.5",
          "form": "엄마시골내려간다고하면자취하게",
          "original_form": "엄마시골내려간다고하면자취하게",
          "speaker_id": "1",
          "time": "20210824 12:32"
        },
        {
          "id": "MDRW2100010204.1.6",
          "form": "하게되면대학근처에서할거임?",
          "original_form": "하게되면대학근처에서할거임?",
          "speaker_id": "1",
          "time": "20210824 12:33"
        },
        {
          "id": "MDRW2100010204.1.7",
          "form": "아니 아마 대학 졸업 전 까지는 안할 것 같고",
          "original_form": "아니 아마 대학 졸업 전 까지는 안할 것 같고",
          "speaker_id": "2",
          "time": "20210824 12:34"
        },
        {
          "id": "MDRW2100010204.1.8",
          "form": "졸업 후에도 대학 근처에는 하기 싫을 것 같음",
          "original_form": "졸업 후에도 대학 근처에는 하기 싫을 것 같음",
          "speaker_id": "2",
          "time": "20210824 12:34"
        },
        {
          "id": "MDRW2100010204.1.9",
          "form": "왜",
          "original_form": "왜",
          "speaker_id": "1",
          "time": "20210824 12:34"
        },
        {
          "id": "MDRW2100010204.1.10",
          "form": "경기도아님?",
          "original_form": "경기도아님?",
          "speaker_id": "1",
          "time": "20210824 12:34"
        },
        {
          "id": "MDRW2100010204.1.11",
          "form": "address 쪽인데",
          "original_form": "&address& 쪽인데",
          "speaker_id": "2",
          "time": "20210824 12:36"
        },
        {
          "id": "MDRW2100010204.1.12",
          "form": "그냥 대학 근처에는 살기 싫음",
          "original_form": "그냥 대학 근처에는 살기 싫음",
          "speaker_id": "2",
          "time": "20210824 12:36"
        },
        {
          "id": "MDRW2100010204.1.13",
          "form": "그것이 학생의 마음이니까",
          "original_form": "그것이 학생의 마음이니까",
          "speaker_id": "2",
          "time": "20210824 12:36"
        },
        {
          "id": "MDRW2100010204.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아그렇긴함",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ아그렇긴함",
          "speaker_id": "1",
          "time": "20210824 12:36"
        },
        {
          "id": "MDRW2100010204.1.15",
          "form": "학교는그냥싫어",
          "original_form": "학교는그냥싫어",
          "speaker_id": "1",
          "time": "20210824 12:36"
        },
        {
          "id": "MDRW2100010204.1.16",
          "form": "서울쪽으로갈거임?",
          "original_form": "서울쪽으로갈거임?",
          "speaker_id": "1",
          "time": "20210824 12:37"
        },
        {
          "id": "MDRW2100010204.1.17",
          "form": "그니까 꼴도 보기 싫어",
          "original_form": "그니까 꼴도 보기 싫어",
          "speaker_id": "2",
          "time": "20210824 12:37"
        },
        {
          "id": "MDRW2100010204.1.18",
          "form": "엥 오 아마",
          "original_form": "엥 오 아마",
          "speaker_id": "2",
          "time": "20210824 12:37"
        },
        {
          "id": "MDRW2100010204.1.19",
          "form": "역근처에 주변에 병원과 놀이 시설이 많은 곳이었으면 좋겠다",
          "original_form": "역근처에 주변에 병원과 놀이 시설이 많은 곳이었으면 좋겠다",
          "speaker_id": "2",
          "time": "20210824 12:37"
        },
        {
          "id": "MDRW2100010204.1.20",
          "form": "너는 뭐 가고 싶은 곳의 조건 같은 거 있어??",
          "original_form": "너는 뭐 가고 싶은 곳의 조건 같은 거 있어??",
          "speaker_id": "2",
          "time": "20210824 12:38"
        },
        {
          "id": "MDRW2100010204.1.21",
          "form": "완전완벽한집조건인데",
          "original_form": "완전완벽한집조건인데",
          "speaker_id": "1",
          "time": "20210824 12:38"
        },
        {
          "id": "MDRW2100010204.1.22",
          "form": "도시기만하면좋음",
          "original_form": "도시기만하면좋음",
          "speaker_id": "1",
          "time": "20210824 12:38"
        },
        {
          "id": "MDRW2100010204.1.23",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210824 12:38"
        },
        {
          "id": "MDRW2100010204.1.24",
          "form": "근처에",
          "original_form": "근처에",
          "speaker_id": "1",
          "time": "20210824 12:38"
        },
        {
          "id": "MDRW2100010204.1.25",
          "form": "술",
          "original_form": "술",
          "speaker_id": "1",
          "time": "20210824 12:38"
        },
        {
          "id": "MDRW2100010204.1.26",
          "form": "파는곳",
          "original_form": "파는곳",
          "speaker_id": "1",
          "time": "20210824 12:39"
        },
        {
          "id": "MDRW2100010204.1.27",
          "form": "없는데",
          "original_form": "없는데",
          "speaker_id": "1",
          "time": "20210824 12:39"
        },
        {
          "id": "MDRW2100010204.1.28",
          "form": "아 ㅇㅈㅇㅈ",
          "original_form": "아 ㅇㅈㅇㅈ",
          "speaker_id": "2",
          "time": "20210824 12:39"
        },
        {
          "id": "MDRW2100010204.1.29",
          "form": "시끄러울거같음",
          "original_form": "시끄러울거같음",
          "speaker_id": "1",
          "time": "20210824 12:39"
        },
        {
          "id": "MDRW2100010204.1.30",
          "form": "아 맞지 진짜 집 가는 길에 불안하기도 하고",
          "original_form": "아 맞지 진짜 집 가는 길에 불안하기도 하고",
          "speaker_id": "2",
          "time": "20210824 12:39"
        },
        {
          "id": "MDRW2100010204.1.31",
          "form": "진상들 난리 엄청 칠듯",
          "original_form": "진상들 난리 엄청 칠듯",
          "speaker_id": "2",
          "time": "20210824 12:39"
        },
        {
          "id": "MDRW2100010204.1.32",
          "form": "맞아. 그것도 엄청 중요하다",
          "original_form": "맞아. 그것도 엄청 중요하다",
          "speaker_id": "1",
          "time": "20210824 12:39"
        },
        {
          "id": "MDRW2100010204.1.33",
          "form": "너무구석지면위험할듯",
          "original_form": "너무구석지면위험할듯",
          "speaker_id": "1",
          "time": "20210824 12:39"
        }
      ]
