      "utterance": [
        {
          "id": "MDRW2100003510.1.1",
          "form": "90년대 노래 저고 좋아해요 ㅎㅎ",
          "original_form": "90년대 노래 저고 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 00:02"
        },
        {
          "id": "MDRW2100003510.1.2",
          "form": "어떤 노래 좋아하시는데요?",
          "original_form": "어떤 노래 좋아하시는데요?",
          "speaker_id": "1",
          "time": "20210821 00:02"
        },
        {
          "id": "MDRW2100003510.1.3",
          "form": "전 다요ㅎㅎ 특히 쿨ㅎㅎ",
          "original_form": "전 다요ㅎㅎ 특히 쿨ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 00:02"
        },
        {
          "id": "MDRW2100003510.1.4",
          "form": "아로하~~~ㅎㅎ",
          "original_form": "아로하~~~ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 00:02"
        },
        {
          "id": "MDRW2100003510.1.5",
          "form": "쿨 노래 좋죠",
          "original_form": "쿨 노래 좋죠",
          "speaker_id": "1",
          "time": "20210821 00:03"
        },
        {
          "id": "MDRW2100003510.1.6",
          "form": "어둔 불빛아래 촛불하나ㅎㅎ와인잔에담긴 약속하나ㅎㅎ",
          "original_form": "어둔 불빛아래 촛불하나ㅎㅎ와인잔에담긴 약속하나ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 00:03"
        },
        {
          "id": "MDRW2100003510.1.7",
          "form": "하나같이 가사가 너무좋은것같애요ㅎ",
          "original_form": "하나같이 가사가 너무좋은것같애요ㅎ",
          "speaker_id": "2",
          "time": "20210821 00:03"
        },
        {
          "id": "MDRW2100003510.1.8",
          "form": "티켓예약하고싶은정도에요ㅎㅎ",
          "original_form": "티켓예약하고싶은정도에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 00:03"
        },
        {
          "id": "MDRW2100003510.1.9",
          "form": "어떤노래좋아하세요?",
          "original_form": "어떤노래좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 00:03"
        },
        {
          "id": "MDRW2100003510.1.10",
          "form": "저는 사랑합니다  요 ㅎ",
          "original_form": "저는 사랑합니다  요 ㅎ",
          "speaker_id": "1",
          "time": "20210821 00:04"
        },
        {
          "id": "MDRW2100003510.1.11",
          "form": "이재훈이 부른거요",
          "original_form": "이재훈이 부른거요",
          "speaker_id": "1",
          "time": "20210821 00:04"
        },
        {
          "id": "MDRW2100003510.1.12",
          "form": "저희 어릴때는 거의 고백노래 였을거 같아요 ㅎㅎㄹ",
          "original_form": "저희 어릴때는 거의 고백노래 였을거 같아요 ㅎㅎㄹ",
          "speaker_id": "1",
          "time": "20210821 00:05"
        },
        {
          "id": "MDRW2100003510.1.13",
          "form": "ㅎㅎㅎ 좋죠ㅎ 이재훈이 정말 노래잘해요ㅎ 쿨노래중에 달링이라는 노래아세요?ㅎ 그것도 좋은데",
          "original_form": "ㅎㅎㅎ 좋죠ㅎ 이재훈이 정말 노래잘해요ㅎ 쿨노래중에 달링이라는 노래아세요?ㅎ 그것도 좋은데",
          "speaker_id": "2",
          "time": "20210821 00:06"
        },
        {
          "id": "MDRW2100003510.1.14",
          "form": "아 알아용~ ㅎㅎ",
          "original_form": "아 알아용~ ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 00:06"
        },
        {
          "id": "MDRW2100003510.1.15",
          "form": "전 노래못부르는데ㅎ 18번이었어요ㅎㅎ",
          "original_form": "전 노래못부르는데ㅎ 18번이었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 00:07"
        },
        {
          "id": "MDRW2100003510.1.16",
          "form": "그냥 뭐라지 노래부르면서 떨림이 좋다고해야하나ㅎ마이크만 잡으면 그곳이 콘서트장이었어요ㅎ",
          "original_form": "그냥 뭐라지 노래부르면서 떨림이 좋다고해야하나ㅎ마이크만 잡으면 그곳이 콘서트장이었어요ㅎ",
          "speaker_id": "2",
          "time": "20210821 00:08"
        },
        {
          "id": "MDRW2100003510.1.17",
          "form": "뭔지 알거 같아요 ㅎㅎ",
          "original_form": "뭔지 알거 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 00:09"
        },
        {
          "id": "MDRW2100003510.1.18",
          "form": "근데 아로하 이제 조정석노래아니에요?ㅎㅎㅎ",
          "original_form": "근데 아로하 이제 조정석노래아니에요?ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 00:11"
        },
        {
          "id": "MDRW2100003510.1.19",
          "form": "조정석이 뮤지컬 배우였었나?ㅎ 노래엄청잘하던데요ㅎ",
          "original_form": "조정석이 뮤지컬 배우였었나?ㅎ 노래엄청잘하던데요ㅎ",
          "speaker_id": "2",
          "time": "20210821 00:11"
        },
        {
          "id": "MDRW2100003510.1.20",
          "form": "ㅎㅎㅎ 요즘분들은 조정석씨 노래인줄 알거에요 ㅋ",
          "original_form": "ㅎㅎㅎ 요즘분들은 조정석씨 노래인줄 알거에요 ㅋ",
          "speaker_id": "1",
          "time": "20210821 00:12"
        },
        {
          "id": "MDRW2100003510.1.21",
          "form": "뮤지커루배우출신 맞아요 ㅎ",
          "original_form": "뮤지커루배우출신 맞아요 ㅎ",
          "speaker_id": "1",
          "time": "20210821 00:13"
        },
        {
          "id": "MDRW2100003510.1.22",
          "form": "맞죠ㅎ 제기억에 분명 뮤지컬쪽이었어요ㅎ",
          "original_form": "맞죠ㅎ 제기억에 분명 뮤지컬쪽이었어요ㅎ",
          "speaker_id": "2",
          "time": "20210821 00:14"
        },
        {
          "id": "MDRW2100003510.1.23",
          "form": "뮤지컬 배우 출신 배우들이  생각보다 많아요 ㅎㅎ",
          "original_form": "뮤지컬 배우 출신 배우들이  생각보다 많아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 00:14"
        },
        {
          "id": "MDRW2100003510.1.24",
          "form": "슬의2 너무재밌게 보고있어서ㅎㅎ 왜 콘서트는 안하는지몰라요ㅎ 하면 대박일텐데",
          "original_form": "슬의2 너무재밌게 보고있어서ㅎㅎ 왜 콘서트는 안하는지몰라요ㅎ 하면 대박일텐데",
          "speaker_id": "2",
          "time": "20210821 00:14"
        },
        {
          "id": "MDRW2100003510.1.25",
          "form": "비대면으로라도 하지ㅎㅎ",
          "original_form": "비대면으로라도 하지ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 00:15"
        },
        {
          "id": "MDRW2100003510.1.26",
          "form": "음  아직  종영이 안되서 그러지 않을까요 ㅎㄹ",
          "original_form": "음  아직  종영이 안되서 그러지 않을까요 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210821 00:16"
        },
        {
          "id": "MDRW2100003510.1.27",
          "form": "콘서트도 연습하고 준비 기간디 꽤 걸리자나요 ㅎㅎ",
          "original_form": "콘서트도 연습하고 준비 기간디 꽤 걸리자나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 00:16"
        },
        {
          "id": "MDRW2100003510.1.28",
          "form": "글죠 지금은 드라마에 집중할 시간이라ㅎ 그래도 음원으로라도 들을수 있어서 정말좋아요ㅎ",
          "original_form": "글죠 지금은 드라마에 집중할 시간이라ㅎ 그래도 음원으로라도 들을수 있어서 정말좋아요ㅎ",
          "speaker_id": "2",
          "time": "20210821 00:19"
        },
        {
          "id": "MDRW2100003510.1.29",
          "form": "뮤지컬배우들이라 노래도 잘하고",
          "original_form": "뮤지컬배우들이라 노래도 잘하고",
          "speaker_id": "2",
          "time": "20210821 00:19"
        },
        {
          "id": "MDRW2100003510.1.30",
          "form": "전  몰아보려고 안보고 있지요 ~.~",
          "original_form": "전  몰아보려고 안보고 있지요 ~.~",
          "speaker_id": "1",
          "time": "20210821 00:19"
        },
        {
          "id": "MDRW2100003510.1.31",
          "form": "전 몰아보면 일에 지장을줘요ㅠㅍ",
          "original_form": "전 몰아보면 일에 지장을줘요ㅠㅍ",
          "speaker_id": "2",
          "time": "20210821 00:20"
        },
        {
          "id": "MDRW2100003510.1.32",
          "form": "어제도 드라마 틀어놓고 심심이하다가 날밤새서ㅠㅠ",
          "original_form": "어제도 드라마 틀어놓고 심심이하다가 날밤새서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 00:20"
        },
        {
          "id": "MDRW2100003510.1.33",
          "form": "ㅎㅎㅎ 잠안자실거 같은 느낌 ㅎㅎ",
          "original_form": "ㅎㅎㅎ 잠안자실거 같은 느낌 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 00:21"
        },
        {
          "id": "MDRW2100003510.1.34",
          "form": "그래도 일은 또해야하니ㅎㅎ매일 피곤한느낌이에요ㅎㅎ",
          "original_form": "그래도 일은 또해야하니ㅎㅎ매일 피곤한느낌이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 00:21"
        },
        {
          "id": "MDRW2100003510.1.35",
          "form": "어디 잠자는 영화없나요? 티켓 바로 예약할수있는데",
          "original_form": "어디 잠자는 영화없나요? 티켓 바로 예약할수있는데",
          "speaker_id": "2",
