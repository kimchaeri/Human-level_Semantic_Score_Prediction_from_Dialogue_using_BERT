      "utterance": [
        {
          "id": "MDRW2100016561.1.1",
          "form": "뮤지컬 좋아하시나요",
          "original_form": "뮤지컬 좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100016561.1.2",
          "form": "종종 보러가는 편이에요. 님은요?",
          "original_form": "종종 보러가는 편이에요. 님은요?",
          "speaker_id": "2",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100016561.1.3",
          "form": "한번도 못봤어요ㅠ",
          "original_form": "한번도 못봤어요ㅠ",
          "speaker_id": "1",
          "time": "20210826 14:55"
        },
        {
          "id": "MDRW2100016561.1.4",
          "form": "보고 싶은 작품 있나요?",
          "original_form": "보고 싶은 작품 있나요?",
          "speaker_id": "2",
          "time": "20210826 15:22"
        },
        {
          "id": "MDRW2100016561.1.5",
          "form": "헤드윅이요!",
          "original_form": "헤드윅이요!",
          "speaker_id": "1",
          "time": "20210826 16:28"
        },
        {
          "id": "MDRW2100016561.1.6",
          "form": "요즘 공연중인 것 같던데",
          "original_form": "요즘 공연중인 것 같던데",
          "speaker_id": "2",
          "time": "20210826 16:28"
        },
        {
          "id": "MDRW2100016561.1.7",
          "form": "표 구하기 어렵다고 들었어요",
          "original_form": "표 구하기 어렵다고 들었어요",
          "speaker_id": "2",
          "time": "20210826 16:29"
        },
        {
          "id": "MDRW2100016561.1.8",
          "form": "헉 많이비싸죠근데",
          "original_form": "헉 많이비싸죠근데",
          "speaker_id": "1",
          "time": "20210826 16:35"
        },
        {
          "id": "MDRW2100016561.1.9",
          "form": "자리에 따라 달라요. 할인도 잘 찾아봐야하구요",
          "original_form": "자리에 따라 달라요. 할인도 잘 찾아봐야하구요",
          "speaker_id": "2",
          "time": "20210826 16:36"
        },
        {
          "id": "MDRW2100016561.1.10",
          "form": "전 2층도 좋아하는데 1층 가까이서 보니까 확실히 표정도 보이고 더 좋긴 하더라구요",
          "original_form": "전 2층도 좋아하는데 1층 가까이서 보니까 확실히 표정도 보이고 더 좋긴 하더라구요",
          "speaker_id": "2",
          "time": "20210826 16:36"
        },
        {
          "id": "MDRW2100016561.1.11",
          "form": "현장감이 확실히 다를것가타효",
          "original_form": "현장감이 확실히 다를것가타효",
          "speaker_id": "1",
          "time": "20210826 16:38"
        },
        {
          "id": "MDRW2100016561.1.12",
          "form": "프레스콜 영상 올라오는 거랑 비교하면 현장에서 보는 게 훨씬 좋아요",
          "original_form": "프레스콜 영상 올라오는 거랑 비교하면 현장에서 보는 게 훨씬 좋아요",
          "speaker_id": "2",
          "time": "20210826 16:40"
        },
        {
          "id": "MDRW2100016561.1.13",
          "form": "아 꼭가봐야겠어요",
          "original_form": "아 꼭가봐야겠어요",
          "speaker_id": "1",
          "time": "20210826 16:43"
        },
        {
          "id": "MDRW2100016561.1.14",
          "form": "헤드윅은 저도 아직 못 봤는데 꼭 한 번 보고싶어요",
          "original_form": "헤드윅은 저도 아직 못 봤는데 꼭 한 번 보고싶어요",
          "speaker_id": "2",
          "time": "20210826 16:44"
        },
        {
          "id": "MDRW2100016561.1.15",
          "form": "명작이죠 진짜",
          "original_form": "명작이죠 진짜",
          "speaker_id": "1",
          "time": "20210826 16:46"
        }
      ]
