      "utterance": [
        {
          "id": "MDRW2100038217.1.1",
          "form": "지금 내가 18000원석이 잇는데",
          "original_form": "지금 내가 18000원석이 잇는데",
          "speaker_id": "1",
          "time": "20210902 01:01"
        },
        {
          "id": "MDRW2100038217.1.2",
          "form": "대충 90연 치려면 14000 든다고 했을때",
          "original_form": "대충 90연 치려면 14000 든다고 했을때",
          "speaker_id": "1",
          "time": "20210902 01:01"
        },
        {
          "id": "MDRW2100038217.1.3",
          "form": "4천이 남으니까",
          "original_form": "4천이 남으니까",
          "speaker_id": "1",
          "time": "20210902 01:01"
        },
        {
          "id": "MDRW2100038217.1.4",
          "form": "7200+4000인데",
          "original_form": "7200+4000인데",
          "speaker_id": "1",
          "time": "20210902 01:01"
        },
        {
          "id": "MDRW2100038217.1.5",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "1",
          "time": "20210902 01:01"
        },
        {
          "id": "MDRW2100038217.1.6",
          "form": "상자깡이랑 퀘스트 열심히 해도",
          "original_form": "상자깡이랑 퀘스트 열심히 해도",
          "speaker_id": "1",
          "time": "20210902 01:01"
        },
        {
          "id": "MDRW2100038217.1.7",
          "form": "애매......",
          "original_form": "애매......",
          "speaker_id": "2",
          "time": "20210902 01:01"
        },
        {
          "id": "MDRW2100038217.1.8",
          "form": "1천장+절반정도가",
          "original_form": "1천장+절반정도가",
          "speaker_id": "1",
          "time": "20210902 01:01"
        },
        {
          "id": "MDRW2100038217.1.9",
          "form": "최대일거같은데...",
          "original_form": "최대일거같은데...",
          "speaker_id": "1",
          "time": "20210902 01:01"
        },
        {
          "id": "MDRW2100038217.1.10",
          "form": "일단 돌려보고",
          "original_form": "일단 돌려보고",
          "speaker_id": "2",
          "time": "20210902 01:02"
        },
        {
          "id": "MDRW2100038217.1.11",
          "form": "또 100원석씩 주는 이벤트 달라고",
          "original_form": "또 100원석씩 주는 이벤트 달라고",
          "speaker_id": "2",
          "time": "20210902 01:02"
        },
        {
          "id": "MDRW2100038217.1.12",
          "form": "기도하기...?",
          "original_form": "기도하기...?",
          "speaker_id": "2",
          "time": "20210902 01:02"
        },
        {
          "id": "MDRW2100038217.1.13",
          "form": "ㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 01:03"
        },
        {
          "id": "MDRW2100038217.1.14",
          "form": "아 새 버전이니까 리딤코드도 주겟지...?",
          "original_form": "아 새 버전이니까 리딤코드도 주겟지...?",
          "speaker_id": "1",
          "time": "20210902 01:04"
        },
        {
          "id": "MDRW2100038217.1.15",
          "form": "근데 그거 고작 300원석이라",
          "original_form": "근데 그거 고작 300원석이라",
          "speaker_id": "1",
          "time": "20210902 01:04"
        },
        {
          "id": "MDRW2100038217.1.16",
          "form": "아 어음",
          "original_form": "아 어음",
          "speaker_id": "2",
          "time": "20210902 01:04"
        },
        {
          "id": "MDRW2100038217.1.17",
          "form": "그렇겠지....?",
          "original_form": "그렇겠지....?",
          "speaker_id": "2",
          "time": "20210902 01:04"
        },
        {
          "id": "MDRW2100038217.1.18",
          "form": "2연차...",
          "original_form": "2연차...",
          "speaker_id": "1",
          "time": "20210902 01:04"
        },
        {
          "id": "MDRW2100038217.1.19",
          "form": "일단",
          "original_form": "일단",
          "speaker_id": "2",
          "time": "20210902 01:04"
        },
        {
          "id": "MDRW2100038217.1.20",
          "form": "영끌해봐야지 뭐",
          "original_form": "영끌해봐야지 뭐",
          "speaker_id": "2",
          "time": "20210902 01:04"
        },
        {
          "id": "MDRW2100038217.1.21",
          "form": "석상 풀렙 찍고 업적+상자 짜잘짜잘하게 다 긁어모아야 저정도일듯..",
          "original_form": "석상 풀렙 찍고 업적+상자 짜잘짜잘하게 다 긁어모아야 저정도일듯..",
          "speaker_id": "1",
          "time": "20210902 01:04"
        },
        {
          "id": "MDRW2100038217.1.22",
          "form": "1천장+절반이면",
          "original_form": "1천장+절반이면",
          "speaker_id": "1",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100038217.1.23",
          "form": "내 생각은",
          "original_form": "내 생각은",
          "speaker_id": "2",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100038217.1.24",
          "form": "2만원석인데",
          "original_form": "2만원석인데",
          "speaker_id": "1",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100038217.1.25",
          "form": "라이덴 뽑아두면",
          "original_form": "라이덴 뽑아두면",
          "speaker_id": "2",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100038217.1.26",
          "form": "저 소는 신포도다 하지만",
          "original_form": "저 소는 신포도다 하지만",
          "speaker_id": "2",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100038217.1.27",
          "form": "안뽑고 감우 알베도면",
          "original_form": "안뽑고 감우 알베도면",
          "speaker_id": "2",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100038217.1.28",
          "form": "눈물의 원신 계정폭파쇼",
          "original_form": "눈물의 원신 계정폭파쇼",
          "speaker_id": "2",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100038217.1.29",
          "form": "ㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100038217.1.30",
          "form": "ㅎ ㅏ",
          "original_form": "ㅎ ㅏ",
          "speaker_id": "1",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100038217.1.31",
          "form": "아닐까",
          "original_form": "아닐까",
          "speaker_id": "2",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100038217.1.32",
          "form": "복각 누구할건지",
          "original_form": "복각 누구할건지",
          "speaker_id": "1",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100038217.1.33",
          "form": "코코미로 넘어가기전에",
          "original_form": "코코미로 넘어가기전에",
          "speaker_id": "1",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100038217.1.34",
          "form": "일단 라이덴도 기간 좀 남았으니까",
          "original_form": "일단 라이덴도 기간 좀 남았으니까",
          "speaker_id": "2",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100038217.1.35",
          "form": "대충 알려달라고....",
          "original_form": "대충 알려달라고....",
          "speaker_id": "1",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100038217.1.36",
          "form": "평 좀",
          "original_form": "평 좀",
          "speaker_id": "2",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100038217.1.37",
          "form": "더 봐바",
          "original_form": "더 봐바",
          "speaker_id": "2",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100038217.1.38",
          "form": "글킨 한데",
          "original_form": "글킨 한데",
          "speaker_id": "1",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100038217.1.39",
          "form": "사실 성능 따질거면",
          "original_form": "사실 성능 따질거면",
          "speaker_id": "1",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100038217.1.40",
          "form": "라이덴 말고 감우 뽑았겠지...",
          "original_form": "라이덴 말고 감우 뽑았겠지...",
          "speaker_id": "1",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100038217.1.41",
