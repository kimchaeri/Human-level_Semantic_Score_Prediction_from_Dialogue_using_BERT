      "utterance": [
        {
          "id": "MDRW2100043887.1.1",
          "form": "운동 하시는거있어요?",
          "original_form": "운동 하시는거있어요?",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043887.1.2",
          "form": "저 이제 하려고요 ㅠㅠ 체중이 불어서...",
          "original_form": "저 이제 하려고요 ㅠㅠ 체중이 불어서...",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043887.1.3",
          "form": "저도요 코로나땜에 배달음식 늘이다보니",
          "original_form": "저도요 코로나땜에 배달음식 늘이다보니",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043887.1.4",
          "form": "근데 시작하기 쉽지않네요",
          "original_form": "근데 시작하기 쉽지않네요",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043887.1.5",
          "form": "그쵸 다들 그런가봐요.. 외출을 잘 안하다보니..",
          "original_form": "그쵸 다들 그런가봐요.. 외출을 잘 안하다보니..",
          "speaker_id": "2",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043887.1.6",
          "form": "맞아요 저희가족 다 쪘답니다",
          "original_form": "맞아요 저희가족 다 쪘답니다",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043887.1.7",
          "form": "요가 등록할려고 생각만 하고있네요",
          "original_form": "요가 등록할려고 생각만 하고있네요",
          "speaker_id": "1",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043887.1.8",
          "form": "ㅋㅋㅋ 저는 홈트 하려고 하는데 생각만 10일째",
          "original_form": "ㅋㅋㅋ 저는 홈트 하려고 하는데 생각만 10일째",
          "speaker_id": "2",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043887.1.9",
          "form": "저도 작년부터랍니다ㅋㅋ",
          "original_form": "저도 작년부터랍니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043887.1.10",
          "form": "유툽보고하시게요?",
          "original_form": "유툽보고하시게요?",
          "speaker_id": "1",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043887.1.11",
          "form": "네 그러려고요 ㅋㅋ 구독만 해놨어요 ㅋㅋㅋ",
          "original_form": "네 그러려고요 ㅋㅋ 구독만 해놨어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043887.1.12",
          "form": "아 누구요?",
          "original_form": "아 누구요?",
          "speaker_id": "1",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100043887.1.13",
          "form": "저 땅끄부부 구독해놨어요 ㅋㅋ",
          "original_form": "저 땅끄부부 구독해놨어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043887.1.14",
          "form": "아 알죠알죠",
          "original_form": "아 알죠알죠",
          "speaker_id": "1",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100043887.1.15",
          "form": "유쾌하신 분들ㅋㅋ",
          "original_form": "유쾌하신 분들ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043887.1.16",
          "form": "표정만 보면 저도 따라할수 있을거 같아요 ㅋㅋㅋ",
          "original_form": "표정만 보면 저도 따라할수 있을거 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100043887.1.17",
          "form": "초보자가 부담없이입문하기",
          "original_form": "초보자가 부담없이입문하기",
