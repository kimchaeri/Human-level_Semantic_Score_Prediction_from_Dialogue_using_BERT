        {
          "id": "MDRW2100030726.1.1",
          "form": "Call of Duty edition books Indigo",
          "original_form": "Call of Duty edition books Indigo",
          "speaker_id": "2",
          "time": "20210904 11:55"
        },
        {
          "id": "MDRW2100030726.1.2",
          "form": "구두도 없어요",
          "original_form": "구두도 없어요",
          "speaker_id": "1",
          "time": "20210904 11:55"
        },
        {
          "id": "MDRW2100030726.1.3",
          "form": "영어를 잘 하네요",
          "original_form": "영어를 잘 하네요",
          "speaker_id": "1",
          "time": "20210904 11:55"
        },
        {
          "id": "MDRW2100030726.1.4",
          "form": "영어 자판 해 놓으니 영어로 찍히네요",
          "original_form": "영어 자판 해 놓으니 영어로 찍히네요",
          "speaker_id": "2",
          "time": "20210904 11:55"
        },
        {
          "id": "MDRW2100030726.1.5",
          "form": "번역이 된다고요",
          "original_form": "번역이 된다고요",
          "speaker_id": "1",
          "time": "20210904 11:56"
        },
        {
          "id": "MDRW2100030726.1.6",
          "form": "신발은 충격을 잘 흡수하고 발을 편안하게 해 주는게 중요한 것 같아요",
          "original_form": "신발은 충격을 잘 흡수하고 발을 편안하게 해 주는게 중요한 것 같아요",
          "speaker_id": "2",
          "time": "20210904 11:56"
        },
        {
          "id": "MDRW2100030726.1.7",
          "form": "웃기다",
          "original_form": "웃기다",
          "speaker_id": "1",
          "time": "20210904 11:56"
        },
        {
          "id": "MDRW2100030726.1.8",
          "form": "신발 두 개뿐인 사람 받나요",
          "original_form": "신발 두 개뿐인 사람 받나요",
          "speaker_id": "1",
          "time": "20210904 11:56"
        },
        {
          "id": "MDRW2100030726.1.9",
          "form": "저는 저런 말을 한 적이 없어요",
          "original_form": "저는 저런 말을 한 적이 없어요",
          "speaker_id": "2",
          "time": "20210904 11:56"
        },
        {
          "id": "MDRW2100030726.1.10",
          "form": "네 저예요",
          "original_form": "네 저예요",
          "speaker_id": "1",
          "time": "20210904 11:56"
        },
        {
          "id": "MDRW2100030726.1.11",
          "form": "웃음소리는 번역이 안 되네요",
          "original_form": "웃음소리는 번역이 안 되네요",
          "speaker_id": "2",
          "time": "20210904 11:56"
        },
        {
          "id": "MDRW2100030726.1.12",
          "form": "양말은 세켤레 있지요",
          "original_form": "양말은 세켤레 있지요",
          "speaker_id": "1",
          "time": "20210904 11:56"
        },
        {
          "id": "MDRW2100030726.1.13",
          "form": "세 개 정도 돌려 신습니다",
          "original_form": "세 개 정도 돌려 신습니다",
          "speaker_id": "2",
          "time": "20210904 11:56"
        },
        {
          "id": "MDRW2100030726.1.14",
          "form": "양말은 소식 켤레가 있는데 왜 매번 짝이 다를까요",
          "original_form": "양말은 소식 켤레가 있는데 왜 매번 짝이 다를까요",
          "speaker_id": "2",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100030726.1.15",
          "form": "매일 빨아야 해요",
          "original_form": "매일 빨아야 해요",
          "speaker_id": "1",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100030726.1.16",
          "form": "속옷도 세 장 있어요",
          "original_form": "속옷도 세 장 있어요",
          "speaker_id": "1",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100030726.1.17",
          "form": "그래서 살 때마다 아예 같은 디자인 같은 색깔로 한 가득 삽니다",
          "original_form": "그래서 살 때마다 아예 같은 디자인 같은 색깔로 한 가득 삽니다",
          "speaker_id": "2",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100030726.1.18",
          "form": "샤워할때마다 빨아요",
          "original_form": "샤워할때마다 빨아요",
          "speaker_id": "1",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100030726.1.19",
          "form": "비자발적 미니멀리스트",
          "original_form": "비자발적 미니멀리스트",
          "speaker_id": "1",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100030726.1.20",
          "form": "한번 크게 편하게 입으니 작은 건 입을 수가 없더라고요",
          "original_form": "한번 크게 편하게 입으니 작은 건 입을 수가 없더라고요",
          "speaker_id": "2",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100030726.1.21",
          "form": "긴팔은 네장 있어요",
          "original_form": "긴팔은 네장 있어요",
          "speaker_id": "1",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100030726.1.22",
          "form": "부지런히지고 좋은 습관을 기르기 되네요",
          "original_form": "부지런히지고 좋은 습관을 기르기 되네요",
          "speaker_id": "2",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100030726.1.23",
          "form": "가을 오면 매일 빨아야 돼요",
          "original_form": "가을 오면 매일 빨아야 돼요",
