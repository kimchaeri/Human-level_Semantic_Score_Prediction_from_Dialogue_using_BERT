      "utterance": [
        {
          "id": "MDRW2100003080.1.1",
          "form": "포천이면 너무 멀다",
          "original_form": "포천이면 너무 멀다",
          "speaker_id": "2",
          "time": "20210820 18:23"
        },
        {
          "id": "MDRW2100003080.1.2",
          "form": "그렇긴하지 허브아일랜드라고 있어",
          "original_form": "그렇긴하지 허브아일랜드라고 있어",
          "speaker_id": "1",
          "time": "20210820 18:24"
        },
        {
          "id": "MDRW2100003080.1.3",
          "form": "공원같은건가?",
          "original_form": "공원같은건가?",
          "speaker_id": "2",
          "time": "20210820 18:24"
        },
        {
          "id": "MDRW2100003080.1.4",
          "form": "응 여기도 불빛축제인데 콘텐츠가 많아",
          "original_form": "응 여기도 불빛축제인데 콘텐츠가 많아",
          "speaker_id": "1",
          "time": "20210820 18:25"
        },
        {
          "id": "MDRW2100003080.1.5",
          "form": "그럼 볼거리는 많겠다",
          "original_form": "그럼 볼거리는 많겠다",
          "speaker_id": "2",
          "time": "20210820 18:26"
        },
        {
          "id": "MDRW2100003080.1.6",
          "form": "허브존이랑 산타마을 만들기체험 등 많네",
          "original_form": "허브존이랑 산타마을 만들기체험 등 많네",
          "speaker_id": "1",
          "time": "20210820 18:27"
        },
        {
          "id": "MDRW2100003080.1.7",
          "form": "만들기 체험? 뭐 만드는데",
          "original_form": "만들기 체험? 뭐 만드는데",
          "speaker_id": "2",
          "time": "20210820 18:28"
        },
        {
          "id": "MDRW2100003080.1.8",
          "form": "불빛향기방향제만들기",
          "original_form": "불빛향기방향제만들기",
          "speaker_id": "1",
          "time": "20210820 18:28"
        },
        {
          "id": "MDRW2100003080.1.9",
          "form": "에게? 평범하게",
          "original_form": "에게? 평범하게",
          "speaker_id": "2",
          "time": "20210820 18:28"
        },
        {
          "id": "MDRW2100003080.1.10",
          "form": "다른 핀란드산타하우스에서는 체험도 많다",
          "original_form": "다른 핀란드산타하우스에서는 체험도 많다",
          "speaker_id": "1",
          "time": "20210820 18:29"
        },
        {
          "id": "MDRW2100003080.1.11",
          "form": "산타하우스ㅋㅋ",
          "original_form": "산타하우스ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:29"
        },
        {
          "id": "MDRW2100003080.1.12",
          "form": "촛대만들기랑 뱅쇼만들기도 하네",
          "original_form": "촛대만들기랑 뱅쇼만들기도 하네",
