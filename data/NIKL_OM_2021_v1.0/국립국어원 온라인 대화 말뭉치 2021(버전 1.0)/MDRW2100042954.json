        {
          "id": "MDRW2100042954.1.2",
          "form": "안녕하세여!",
          "original_form": "안녕하세여!",
          "speaker_id": "2",
          "time": "20210907 12:06"
        },
        {
          "id": "MDRW2100042954.1.3",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 12:49"
        },
        {
          "id": "MDRW2100042954.1.4",
          "form": "예전엔 조금씩 했었는데 요즘은 안해요!",
          "original_form": "예전엔 조금씩 했었는데 요즘은 안해요!",
          "speaker_id": "2",
          "time": "20210907 13:33"
        },
        {
          "id": "MDRW2100042954.1.5",
          "form": "게임 좋아하시나여",
          "original_form": "게임 좋아하시나여",
          "speaker_id": "2",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100042954.1.6",
          "form": "자 요즘 예전에 하던 메이플 다시 하고 있어요 ㅋㅋㅋ",
          "original_form": "자 요즘 예전에 하던 메이플 다시 하고 있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:11"
        },
        {
          "id": "MDRW2100042954.1.7",
          "form": "아 ㅋㅋㅋ메이플 가끔씩 복귀하는 친규들 있더라구요",
          "original_form": "아 ㅋㅋㅋ메이플 가끔씩 복귀하는 친규들 있더라구요",
          "speaker_id": "2",
          "time": "20210907 19:27"
        },
        {
          "id": "MDRW2100042954.1.8",
          "form": "제가 그 친굽니다!ㅋㅋㅋㅋㅋ",
          "original_form": "제가 그 친굽니다!ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:28"
        },
        {
          "id": "MDRW2100042954.1.9",
          "form": "저 220 찍었어요",
          "original_form": "저 220 찍었어요",
          "speaker_id": "1",
          "time": "20210907 19:28"
        },
        {
          "id": "MDRW2100042954.1.10",
          "form": "한 5달동안 219였는데 ㅋㅋㅋ",
          "original_form": "한 5달동안 219였는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:28"
        },
        {
          "id": "MDRW2100042954.1.11",
          "form": "ㅋㅋㅋㅋㅋ열심히 하시는 중이신가봐여!",
          "original_form": "ㅋㅋㅋㅋㅋ열심히 하시는 중이신가봐여!",
          "speaker_id": "2",
          "time": "20210907 19:30"
        },
        {
          "id": "MDRW2100042954.1.12",
          "form": "네 열심히 했어요..ㅋㅋㅋ오랜만에 하니까 너무 재밌는거 있죠",
          "original_form": "네 열심히 했어요..ㅋㅋㅋ오랜만에 하니까 너무 재밌는거 있죠",
          "speaker_id": "1",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100042954.1.13",
          "form": "name2님도 해보셨죠?",
          "original_form": "&name2&님도 해보셨죠?",
          "speaker_id": "1",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100042954.1.14",
          "form": "ㅋㅋㅋ사실 저는 메이플 안해봤어요",
          "original_form": "ㅋㅋㅋ사실 저는 메이플 안해봤어요",
          "speaker_id": "2",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100042954.1.15",
          "form": "애들하는 거 구경만하궄ㅋㅋ",
          "original_form": "애들하는 거 구경만하궄ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100042954.1.16",
          "form": "아 진짜요??ㅋㅋㅋㅋ거의 말하면 다 본인도 해봤다고 해서 해보셨을줄 알았어요 ㅋㅋㅋ 그럴수 있죠",
          "original_form": "아 진짜요??ㅋㅋㅋㅋ거의 말하면 다 본인도 해봤다고 해서 해보셨을줄 알았어요 ㅋㅋㅋ 그럴수 있죠",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100042954.1.17",
          "form": "그럼 휴대폰 게임도 안하세요?",
          "original_form": "그럼 휴대폰 게임도 안하세요?",
          "speaker_id": "1",
