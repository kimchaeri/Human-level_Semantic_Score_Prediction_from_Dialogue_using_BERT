      "utterance": [
        {
          "id": "MDRW2100028655.1.1",
          "form": "아 이건 정말 우리가 할 수 있는 얘기죠",
          "original_form": "아 이건 정말 우리가 할 수 있는 얘기죠",
          "speaker_id": "2",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100028655.1.2",
          "form": "그렇죠 후후",
          "original_form": "그렇죠 후후",
          "speaker_id": "1",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100028655.1.3",
          "form": "가장 최근에 어떤 책을 사셨나요",
          "original_form": "가장 최근에 어떤 책을 사셨나요",
          "speaker_id": "2",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100028655.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100028655.1.5",
          "form": "가장최근... 오늘 리디에서 사마귀가친구에게 몇화치 결제했어요",
          "original_form": "가장최근... 오늘 리디에서 사마귀가친구에게 몇화치 결제했어요",
          "speaker_id": "1",
          "time": "20210902 21:14"
        },
        {
          "id": "MDRW2100028655.1.6",
          "form": "이북단행본은 아마 발밑의황제!",
          "original_form": "이북단행본은 아마 발밑의황제!",
          "speaker_id": "1",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100028655.1.7",
          "form": "사친 연재분 모으시는구나",
          "original_form": "사친 연재분 모으시는구나",
          "speaker_id": "2",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100028655.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100028655.1.9",
          "form": "발밑의 황제는 처음들어보는데",
          "original_form": "발밑의 황제는 처음들어보는데",
          "speaker_id": "2",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100028655.1.10",
          "form": "여공남수물인가요ㅋㅋㅋㅋㅋ",
          "original_form": "여공남수물인가요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028655.1.11",
          "form": "어떻게아셨지",
          "original_form": "어떻게아셨지",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028655.1.12",
          "form": "정답입니다",
          "original_form": "정답입니다",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028655.1.13",
          "form": "name2님은 요새 뭐읽으셨죠",
          "original_form": "&name2&님은 요새 뭐읽으셨죠",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028655.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028655.1.15",
          "form": "발밑의 여자황제이면 name1님이 싫어하실거 같아서..",
          "original_form": "발밑의 여자황제이면 &name1&님이 싫어하실거 같아서..",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028655.1.16",
          "form": "아 진짜웃겨",
          "original_form": "아 진짜웃겨",
          "speaker_id": "1",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028655.1.17",
          "form": "구해주세요 공주님",
          "original_form": "구해주세요 공주님",
          "speaker_id": "2",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028655.1.18",
          "form": "최근에 읽었습니다",
          "original_form": "최근에 읽었습니다",
          "speaker_id": "2",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028655.1.19",
          "form": "좋았어요 ㅎㅎ",
          "original_form": "좋았어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028655.1.20",
          "form": "오 구공도 많이 들어봤어요!",
          "original_form": "오 구공도 많이 들어봤어요!",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100028655.1.21",
          "form": "전 요새 각잡고 단행본을 못읽고 연재만 챙겨보는 것 같아요",
          "original_form": "전 요새 각잡고 단행본을 못읽고 연재만 챙겨보는 것 같아요",
          "speaker_id": "1",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028655.1.22",
          "form": "맨날 6시 땡하면 데못죽만 열심히 챙겨읽구..",
          "original_form": "맨날 6시 땡하면 데못죽만 열심히 챙겨읽구..",
          "speaker_id": "1",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028655.1.23",
          "form": "모험물 좋아하시면 입맛에 맞으실거 같아요",
          "original_form": "모험물 좋아하시면 입맛에 맞으실거 같아요",
          "speaker_id": "2",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028655.1.24",
          "form": "앜ㅋㅋㅋ 저도 연재만 거의 봐요ㅠ",
          "original_form": "앜ㅋㅋㅋ 저도 연재만 거의 봐요ㅠ",
          "speaker_id": "2",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028655.1.25",
          "form": "조아라 무연 중심으로 보느라 요즘은 비엘을 보게 되더라고요",
          "original_form": "조아라 무연 중심으로 보느라 요즘은 비엘을 보게 되더라고요",
          "speaker_id": "2",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100028655.1.26",
          "form": "모험물 좋지요 (모험하면 떠오르는 세라이야기..)",
          "original_form": "모험물 좋지요 (모험하면 떠오르는 세라이야기..)",
          "speaker_id": "1",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100028655.1.27",
          "form": "비엘! 추천하실만한 작품있으신가요",
          "original_form": "비엘! 추천하실만한 작품있으신가요",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028655.1.28",
          "form": "세라이야기...",
          "original_form": "세라이야기...",
          "speaker_id": "2",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100028655.1.29",
          "form": "사랑하는 세라이야기...",
          "original_form": "사랑하는 세라이야기...",
          "speaker_id": "2",
          "time": "20210902 21:23"
        }
      ]
    }
