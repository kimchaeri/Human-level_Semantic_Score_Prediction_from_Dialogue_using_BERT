      "utterance": [
        {
          "id": "MDRW2100004450.1.1",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 16:34"
        },
        {
          "id": "MDRW2100004450.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210821 16:36"
        },
        {
          "id": "MDRW2100004450.1.3",
          "form": "요새 무엇에 관심 많으세요?",
          "original_form": "요새 무엇에 관심 많으세요?",
          "speaker_id": "1",
          "time": "20210821 16:36"
        },
        {
          "id": "MDRW2100004450.1.4",
          "form": "전 오늘 치과 갔다왔는데 지금 너무 아프네요",
          "original_form": "전 오늘 치과 갔다왔는데 지금 너무 아프네요",
          "speaker_id": "1",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100004450.1.5",
          "form": "치아관리를 잘해야 하는데 워낙 잇몸이 약해요",
          "original_form": "치아관리를 잘해야 하는데 워낙 잇몸이 약해요",
          "speaker_id": "1",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100004450.1.6",
          "form": "아이고...",
          "original_form": "아이고...",
          "speaker_id": "2",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100004450.1.7",
          "form": "잘 안씹어지네요",
          "original_form": "잘 안씹어지네요",
          "speaker_id": "1",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100004450.1.8",
          "form": "치과 치료 받고나면 진짜 힘든데",
          "original_form": "치과 치료 받고나면 진짜 힘든데",
          "speaker_id": "2",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100004450.1.9",
          "form": "오늘도 50만원 결재 했어요",
          "original_form": "오늘도 50만원 결재 했어요",
          "speaker_id": "1",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100004450.1.10",
          "form": "막 심하면 몸살도 나더라고요",
          "original_form": "막 심하면 몸살도 나더라고요",
          "speaker_id": "2",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004450.1.11",
          "form": "네 ㅜㅜ진짜 후가 더 아프네요",
          "original_form": "네 ㅜㅜ진짜 후가 더 아프네요",
          "speaker_id": "1",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004450.1.12",
          "form": "치과치료 비용도 부담스럽고ㅠ",
          "original_form": "치과치료 비용도 부담스럽고ㅠ",
          "speaker_id": "2",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004450.1.13",
          "form": "맞아요 사랑니는 뽑다 죽을뻔했네요",
          "original_form": "맞아요 사랑니는 뽑다 죽을뻔했네요",
          "speaker_id": "1",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004450.1.14",
          "form": "얼른 괜찮아지시길!!",
          "original_form": "얼른 괜찮아지시길!!",
          "speaker_id": "2",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004450.1.15",
          "form": "네 정말 비싸요",
          "original_form": "네 정말 비싸요",
          "speaker_id": "1",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004450.1.16",
          "form": "무서워못가겠어요",
          "original_form": "무서워못가겠어요",
          "speaker_id": "1",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004450.1.17",
          "form": "냉찜질을 해보셔요",
          "original_form": "냉찜질을 해보셔요",
          "speaker_id": "2",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004450.1.18",
          "form": "네 ㅜㅜ",
          "original_form": "네 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004450.1.19",
          "form": "진통제 챙겨드시고ㅠ",
          "original_form": "진통제 챙겨드시고ㅠ",
          "speaker_id": "2",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004450.1.20",
          "form": "네 진통제를 안주셨네요 ㅡㅡ",
          "original_form": "네 진통제를 안주셨네요 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210821 16:39"
        },
        {
          "id": "MDRW2100004450.1.21",
          "form": "안먹어도 된다고",
          "original_form": "안먹어도 된다고",
          "speaker_id": "1",
          "time": "20210821 16:39"
        },
        {
          "id": "MDRW2100004450.1.22",
          "form": "운동에 관심있으세요?",
          "original_form": "운동에 관심있으세요?",
          "speaker_id": "1",
          "time": "20210821 16:39"
        },
        {
          "id": "MDRW2100004450.1.23",
          "form": "전 이제 다시 시작하려고요",
          "original_form": "전 이제 다시 시작하려고요",
          "speaker_id": "1",
          "time": "20210821 16:39"
        },
        {
          "id": "MDRW2100004450.1.24",
          "form": "막 엄청 하다가 관두니 지금 요요가 심해요",
          "original_form": "막 엄청 하다가 관두니 지금 요요가 심해요",
          "speaker_id": "1",
          "time": "20210821 16:40"
        },
        {
          "id": "MDRW2100004450.1.25",
          "form": "네 저는 일주일에 두세번 필라테스 하고 있어요",
          "original_form": "네 저는 일주일에 두세번 필라테스 하고 있어요",
          "speaker_id": "2",
          "time": "20210821 16:40"
        },
        {
          "id": "MDRW2100004450.1.26",
          "form": "오 필라테스 해보고 싶어요",
          "original_form": "오 필라테스 해보고 싶어요",
          "speaker_id": "1",
          "time": "20210821 16:40"
        },
        {
          "id": "MDRW2100004450.1.27",
          "form": "운동 잘 못하고 흥미도 별로 없는 편인데",
          "original_form": "운동 잘 못하고 흥미도 별로 없는 편인데",
          "speaker_id": "2",
          "time": "20210821 16:40"
        },
        {
          "id": "MDRW2100004450.1.28",
          "form": "저 무지 뻣뻣한데 그래도 되나요?",
          "original_form": "저 무지 뻣뻣한데 그래도 되나요?",
          "speaker_id": "1",
          "time": "20210821 16:40"
        },
        {
          "id": "MDRW2100004450.1.29",
          "form": "저도 흥미 제로요",
          "original_form": "저도 흥미 제로요",
          "speaker_id": "1",
          "time": "20210821 16:40"
        },
        {
          "id": "MDRW2100004450.1.30",
          "form": "어렵지 않아요?",
          "original_form": "어렵지 않아요?",
          "speaker_id": "1",
          "time": "20210821 16:40"
        },
        {
          "id": "MDRW2100004450.1.31",
          "form": "저도 엄청 뻣뻣해요",
          "original_form": "저도 엄청 뻣뻣해요",
          "speaker_id": "2",
          "time": "20210821 16:40"
        },
        {
          "id": "MDRW2100004450.1.32",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004450.1.33",
          "form": "잘 못따라갈까 걱정요",
          "original_form": "잘 못따라갈까 걱정요",
          "speaker_id": "1",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004450.1.34",
          "form": "그래도 하다보니 점점 동작 따라가고",
          "original_form": "그래도 하다보니 점점 동작 따라가고",
          "speaker_id": "2",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004450.1.35",
          "form": "요가는 못하겠더라구요",
          "original_form": "요가는 못하겠더라구요",
          "speaker_id": "1",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004450.1.36",
          "form": "자세도 좀 좋아져요",
          "original_form": "자세도 좀 좋아져요",
          "speaker_id": "2",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004450.1.37",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004450.1.38",
          "form": "자세에 좋다 더라구요",
          "original_form": "자세에 좋다 더라구요",
          "speaker_id": "1",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004450.1.39",
          "form": "무엇보다 몸이 좀 가벼워지고",
          "original_form": "무엇보다 몸이 좀 가벼워지고",
          "speaker_id": "2",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004450.1.40",
          "form": "요가 곡소리 나던데 ㅋㅋ",
          "original_form": "요가 곡소리 나던데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004450.1.41",
          "form": "바로 때려 쳤어요 ㅋㅋ",
          "original_form": "바로 때려 쳤어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004450.1.42",
          "form": "쑤신 데가 없어지니까 좋더라고요",
          "original_form": "쑤신 데가 없어지니까 좋더라고요",
          "speaker_id": "2",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100004450.1.43",
          "form": "요가가 더 힘들다고 하더라고요",
          "original_form": "요가가 더 힘들다고 하더라고요",
          "speaker_id": "2",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004450.1.44",
          "form": "아 이제 적응 하셨나봐요",
          "original_form": "아 이제 적응 하셨나봐요",
          "speaker_id": "1",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004450.1.45",
          "form": "제 친구가 요가 갔는데",
          "original_form": "제 친구가 요가 갔는데",
          "speaker_id": "2",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004450.1.46",
          "form": "전 잘못 택한거죠",
          "original_form": "전 잘못 택한거죠",
          "speaker_id": "1",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004450.1.47",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004450.1.48",
          "form": "다 기인들만 모여있는 것 같다고ㅋㅋ",
          "original_form": "다 기인들만 모여있는 것 같다고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004450.1.49",
          "form": "맞아요 저만 나무 토막이예요 ㅋㅋ",
          "original_form": "맞아요 저만 나무 토막이예요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004450.1.50",
          "form": "챙피합니다",
          "original_form": "챙피합니다",
          "speaker_id": "1",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004450.1.51",
          "form": "막 물구나무 서고 몸을 접고 그런다고 하더라고요",
          "original_form": "막 물구나무 서고 몸을 접고 그런다고 하더라고요",
          "speaker_id": "2",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004450.1.52",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004450.1.53",
          "form": "그러니까요 ㅋㅋ",
          "original_form": "그러니까요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 16:42"
        },
        {
          "id": "MDRW2100004450.1.54",
          "form": "신기해요 인도같아요",
          "original_form": "신기해요 인도같아요",
          "speaker_id": "1",
