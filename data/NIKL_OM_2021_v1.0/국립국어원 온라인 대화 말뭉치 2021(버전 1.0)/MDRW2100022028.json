      "utterance": [
        {
          "id": "MDRW2100022028.1.1",
          "form": "잡지, 도서ㅎ",
          "original_form": "잡지, 도서ㅎ",
          "speaker_id": "1",
          "time": "20210829 01:20"
        },
        {
          "id": "MDRW2100022028.1.2",
          "form": "우와 내가 잘 않읽는거네요ㅎㅎ",
          "original_form": "우와 내가 잘 않읽는거네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 01:20"
        },
        {
          "id": "MDRW2100022028.1.3",
          "form": "그래도 수다떨수있음ㅎㅎ 최근에 읽으신 책있어요?",
          "original_form": "그래도 수다떨수있음ㅎㅎ 최근에 읽으신 책있어요?",
          "speaker_id": "1",
          "time": "20210829 01:20"
        },
        {
          "id": "MDRW2100022028.1.4",
          "form": "저 요즘 토지 다시 읽고 있어요 ㅋㅋ",
          "original_form": "저 요즘 토지 다시 읽고 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 01:21"
        },
        {
          "id": "MDRW2100022028.1.5",
          "form": "책 잘 안 읽으세요?",
          "original_form": "책 잘 안 읽으세요?",
          "speaker_id": "2",
          "time": "20210829 01:21"
        },
        {
          "id": "MDRW2100022028.1.6",
          "form": "우왐ㅎ 토지ㅎ",
          "original_form": "우왐ㅎ 토지ㅎ",
          "speaker_id": "1",
          "time": "20210829 01:22"
        },
        {
          "id": "MDRW2100022028.1.7",
          "form": "저는 제테크, 투자책위주로 읽어요",
          "original_form": "저는 제테크, 투자책위주로 읽어요",
          "speaker_id": "1",
          "time": "20210829 01:22"
        },
        {
          "id": "MDRW2100022028.1.8",
          "form": "갬성적인 책들도 가끔읽는데",
          "original_form": "갬성적인 책들도 가끔읽는데",
          "speaker_id": "1",
          "time": "20210829 01:22"
        },
        {
          "id": "MDRW2100022028.1.9",
          "form": "토지는 안읽어봤어요ㅍ",
          "original_form": "토지는 안읽어봤어요ㅍ",
          "speaker_id": "1",
          "time": "20210829 01:22"
        },
        {
          "id": "MDRW2100022028.1.10",
          "form": "저는 비문학은 잘 안 읽고 문학 좋아해여 ㅋㅋ",
          "original_form": "저는 비문학은 잘 안 읽고 문학 좋아해여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 01:23"
        },
        {
          "id": "MDRW2100022028.1.11",
          "form": "갬성적인 책이라면 어떤?",
          "original_form": "갬성적인 책이라면 어떤?",
          "speaker_id": "2",
          "time": "20210829 01:23"
        },
        {
          "id": "MDRW2100022028.1.12",
          "form": "에세이 류 인가요?",
          "original_form": "에세이 류 인가요?",
          "speaker_id": "2",
          "time": "20210829 01:23"
        },
        {
          "id": "MDRW2100022028.1.13",
          "form": "이름이 생각안나요ㅠ 시베리아 횡단열차에서.. 일어난 일인제",
          "original_form": "이름이 생각안나요ㅠ 시베리아 횡단열차에서.. 일어난 일인제",
          "speaker_id": "1",
          "time": "20210829 01:24"
        },
        {
          "id": "MDRW2100022028.1.14",
          "form": "소설 종류요ㅎㅎ",
          "original_form": "소설 종류요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 01:24"
        },
        {
          "id": "MDRW2100022028.1.15",
          "form": "우와 문학을 좋아하시다니ㅠ",
          "original_form": "우와 문학을 좋아하시다니ㅠ",
          "speaker_id": "1",
          "time": "20210829 01:24"
        },
        {
          "id": "MDRW2100022028.1.16",
          "form": "공부 잘하신분이다~!!",
          "original_form": "공부 잘하신분이다~!!",
          "speaker_id": "1",
          "time": "20210829 01:24"
        },
        {
          "id": "MDRW2100022028.1.17",
          "form": "엌 아녀 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "엌 아녀 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 01:25"
        },
        {
          "id": "MDRW2100022028.1.18",
          "form": "그냥 뭐 소설책 좋아하는 거죠 재미있잖아요",
          "original_form": "그냥 뭐 소설책 좋아하는 거죠 재미있잖아요",
          "speaker_id": "2",
          "time": "20210829 01:25"
        },
        {
          "id": "MDRW2100022028.1.19",
          "form": "근데 책읽는거 예전에 좋아했어요ㅎ",
          "original_form": "근데 책읽는거 예전에 좋아했어요ㅎ",
          "speaker_id": "1",
          "time": "20210829 01:25"
        },
        {
          "id": "MDRW2100022028.1.20",
          "form": "교과서를 너무 사랑했어요ㅎ",
          "original_form": "교과서를 너무 사랑했어요ㅎ",
          "speaker_id": "1",
          "time": "20210829 01:26"
        },
        {
          "id": "MDRW2100022028.1.21",
          "form": "근데 왜 성적이 잘안나왔을까요?",
          "original_form": "근데 왜 성적이 잘안나왔을까요?",
          "speaker_id": "1",
          "time": "20210829 01:26"
        },
        {
          "id": "MDRW2100022028.1.22",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 01:26"
        },
        {
          "id": "MDRW2100022028.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ짝사랑이었네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ짝사랑이었네요",
          "speaker_id": "2",
          "time": "20210829 01:26"
        },
        {
          "id": "MDRW2100022028.1.24",
          "form": "저도 예전에는 진짜 많이 읽었어요",
          "original_form": "저도 예전에는 진짜 많이 읽었어요",
          "speaker_id": "2",
          "time": "20210829 01:27"
        },
        {
          "id": "MDRW2100022028.1.25",
          "form": "하루에 다섯권도 읽고 그랬는데",
          "original_form": "하루에 다섯권도 읽고 그랬는데",
          "speaker_id": "2",
          "time": "20210829 01:27"
        },
        {
          "id": "MDRW2100022028.1.26",
          "form": "요즘은 한달에 한 권도 가끔 힘드네요 ㅋㅋ",
          "original_form": "요즘은 한달에 한 권도 가끔 힘드네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 01:27"
        },
        {
          "id": "MDRW2100022028.1.27",
          "form": "우와ㅎㅎ 대단하시다ㅎ",
          "original_form": "우와ㅎㅎ 대단하시다ㅎ",
          "speaker_id": "1",
          "time": "20210829 01:27"
        },
        {
          "id": "MDRW2100022028.1.28",
          "form": "교과서도 하루에 다섯권은 잠와서 못읽어요ㅎ",
          "original_form": "교과서도 하루에 다섯권은 잠와서 못읽어요ㅎ",
          "speaker_id": "1",
          "time": "20210829 01:27"
        },
        {
          "id": "MDRW2100022028.1.29",
          "form": "근데 그럼 작가중에 좋아하신분있으세요?",
          "original_form": "근데 그럼 작가중에 좋아하신분있으세요?",
          "speaker_id": "1",
          "time": "20210829 01:28"
        },
        {
          "id": "MDRW2100022028.1.30",
          "form": "베스트 셀러?",
          "original_form": "베스트 셀러?",
          "speaker_id": "1",
          "time": "20210829 01:28"
        },
        {
          "id": "MDRW2100022028.1.31",
          "form": "박경리 선생님이요",
          "original_form": "박경리 선생님이요",
          "speaker_id": "2",
          "time": "20210829 01:28"
        },
        {
          "id": "MDRW2100022028.1.32",
          "form": "박완서 선생님도 너무 좋아해요",
          "original_form": "박완서 선생님도 너무 좋아해요",
          "speaker_id": "2",
          "time": "20210829 01:28"
        },
        {
          "id": "MDRW2100022028.1.33",
          "form": "최근 작가중엔 구병모 작가님",
          "original_form": "최근 작가중엔 구병모 작가님",
          "speaker_id": "2",
          "time": "20210829 01:28"
        },
        {
          "id": "MDRW2100022028.1.34",
          "form": "박경리 선생님은 토지 작가님이시죠?ㅎ",
          "original_form": "박경리 선생님은 토지 작가님이시죠?ㅎ",
          "speaker_id": "1",
          "time": "20210829 01:29"
        },
        {
          "id": "MDRW2100022028.1.35",
          "form": "아닌가?ㅎ 두분은 잘모르겟네요ㅍ",
          "original_form": "아닌가?ㅎ 두분은 잘모르겟네요ㅍ",
          "speaker_id": "1",
          "time": "20210829 01:29"
        },
        {
          "id": "MDRW2100022028.1.36",
          "form": "역시 지적이시네요ㅠ 전 대화가 안될듯ㅠ",
          "original_form": "역시 지적이시네요ㅠ 전 대화가 안될듯ㅠ",
          "speaker_id": "1",
          "time": "20210829 01:30"
        },
        {
          "id": "MDRW2100022028.1.37",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210829 01:30"
        },
        {
          "id": "MDRW2100022028.1.38",
          "form": "토지 쓰신 분이 박경리 작가님이구",
          "original_form": "토지 쓰신 분이 박경리 작가님이구",
          "speaker_id": "2",
          "time": "20210829 01:30"
        },
        {
          "id": "MDRW2100022028.1.39",
          "form": "그많던 싱아는 누가 먹었을까 아세요?",
          "original_form": "그많던 싱아는 누가 먹었을까 아세요?",
          "speaker_id": "2",
          "time": "20210829 01:30"
        },
        {
          "id": "MDRW2100022028.1.40",
          "form": "이게 박완서 작가님ㅋㅋ",
          "original_form": "이게 박완서 작가님ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 01:30"
        },
        {
          "id": "MDRW2100022028.1.41",
          "form": "교과서에 많이 나와용",
          "original_form": "교과서에 많이 나와용",
          "speaker_id": "2",
          "time": "20210829 01:30"
        },
        {
          "id": "MDRW2100022028.1.42",
          "form": "오ㅎㅎ 그유명한ㅎ",
          "original_form": "오ㅎㅎ 그유명한ㅎ",
          "speaker_id": "1",
          "time": "20210829 01:31"
        },
        {
          "id": "MDRW2100022028.1.43",
          "form": "도둑맞은 가난 나목...",
          "original_form": "도둑맞은 가난 나목...",
          "speaker_id": "2",
          "time": "20210829 01:31"
        },
        {
          "id": "MDRW2100022028.1.44",
          "form": "아앙아아아아ㅏ",
          "original_form": "아앙아아아아ㅏ",
          "speaker_id": "1",
          "time": "20210829 01:31"
        },
        {
          "id": "MDRW2100022028.1.45",
          "form": "알아요ㅎㅎㅎ",
          "original_form": "알아요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 01:31"
        },
        {
          "id": "MDRW2100022028.1.46",
          "form": "교과서를 사랑한 제가 모를수가있나요?ㅎ",
          "original_form": "교과서를 사랑한 제가 모를수가있나요?ㅎ",
          "speaker_id": "1",
          "time": "20210829 01:31"
        },
        {
          "id": "MDRW2100022028.1.47",
          "form": "그쵸?? ㅋㅋㅋ 정규교육 받는 사람이면 다 알걸요~",
          "original_form": "그쵸?? ㅋㅋㅋ 정규교육 받는 사람이면 다 알걸요~",
          "speaker_id": "2",
          "time": "20210829 01:37"
        },
        {
          "id": "MDRW2100022028.1.48",
          "form": "ㅎㅎ 너무 부럽네요ㅎ",
          "original_form": "ㅎㅎ 너무 부럽네요ㅎ",
          "speaker_id": "1",
          "time": "20210829 01:39"
        },
        {
