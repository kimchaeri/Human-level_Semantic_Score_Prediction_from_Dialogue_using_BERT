      "utterance": [
        {
          "id": "MDRW2100009542.1.1",
          "form": "온라인 커뮤니티 해보셨어요?",
          "original_form": "온라인 커뮤니티 해보셨어요?",
          "speaker_id": "1",
          "time": "20210823 08:51"
        },
        {
          "id": "MDRW2100009542.1.2",
          "form": "커뮤니티 활동을 잘안해서 궁금해요ㅎㄹ",
          "original_form": "커뮤니티 활동을 잘안해서 궁금해요ㅎㄹ",
          "speaker_id": "1",
          "time": "20210823 08:51"
        },
        {
          "id": "MDRW2100009542.1.3",
          "form": "저도 온라인커뮤니티는 어릴때만 한거 같아요 ㅎㅎ",
          "original_form": "저도 온라인커뮤니티는 어릴때만 한거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 09:53"
        },
        {
          "id": "MDRW2100009542.1.4",
          "form": "어릴때 게임 좋아해서 게임관련 ㅎㅎㅎ\r\n그거외엔 딱히 없는거 같아요",
          "original_form": "어릴때 게임 좋아해서 게임관련 ㅎㅎㅎ\r\n그거외엔 딱히 없는거 같아요",
          "speaker_id": "2",
          "time": "20210823 09:54"
        },
        {
          "id": "MDRW2100009542.1.5",
          "form": "오ㅎㅎ 게임좋아하시는구나ㅎ",
          "original_form": "오ㅎㅎ 게임좋아하시는구나ㅎ",
          "speaker_id": "1",
          "time": "20210824 22:01"
        },
        {
          "id": "MDRW2100009542.1.6",
          "form": "전 모바일밖에안해서ㅎㅎ 정모나 이런것도 하셨어요?ㅎ 무슨게임 이었어요?",
          "original_form": "전 모바일밖에안해서ㅎㅎ 정모나 이런것도 하셨어요?ㅎ 무슨게임 이었어요?",
          "speaker_id": "1",
          "time": "20210824 22:02"
        },
        {
          "id": "MDRW2100009542.1.7",
          "form": "음 제일 마지막으로 한건 블레이드앤 소울요 ㅎㅎ",
          "original_form": "음 제일 마지막으로 한건 블레이드앤 소울요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:31"
        },
        {
          "id": "MDRW2100009542.1.8",
          "form": "ㅎㅎ 블러드앤소울 좋죠ㅎㅎ",
          "original_form": "ㅎㅎ 블러드앤소울 좋죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 22:40"
        },
        {
          "id": "MDRW2100009542.1.9",
          "form": "초보는 아니셨을것 같은데요ㅎㅎㅎ",
          "original_form": "초보는 아니셨을것 같은데요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 22:40"
        },
        {
          "id": "MDRW2100009542.1.10",
          "form": "왠지... 고수분이셨을듯",
          "original_form": "왠지... 고수분이셨을듯",
          "speaker_id": "1",
          "time": "20210824 22:40"
        },
        {
          "id": "MDRW2100009542.1.11",
          "form": "음 ㅎㅎ 고수까진 아녀도 1.5배는 했어요 ㅎㅎ",
          "original_form": "음 ㅎㅎ 고수까진 아녀도 1.5배는 했어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:41"
        },
        {
          "id": "MDRW2100009542.1.12",
          "form": "친목이라 재밌어서 한게 커요",
          "original_form": "친목이라 재밌어서 한게 커요",
          "speaker_id": "2",
          "time": "20210824 22:42"
        },
        {
          "id": "MDRW2100009542.1.13",
          "form": "글죠ㅎㅎ 하시는거보면 알죠ㄹㄹ",
          "original_form": "글죠ㅎㅎ 하시는거보면 알죠ㄹㄹ",
          "speaker_id": "1",
          "time": "20210824 22:43"
        },
        {
          "id": "MDRW2100009542.1.14",
          "form": "디테일하시고 꾸준하시니깐ㅎㅎ",
          "original_form": "디테일하시고 꾸준하시니깐ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 22:43"
        },
        {
          "id": "MDRW2100009542.1.15",
          "form": "그럼 온라인에서 현실로 만나신거에요? 친목으로?ㅎㅎ",
          "original_form": "그럼 온라인에서 현실로 만나신거에요? 친목으로?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100009542.1.16",
          "form": "네네 ㅎㅎ",
          "original_form": "네네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:46"
        },
        {
          "id": "MDRW2100009542.1.17",
          "form": "밥도 먹고 겜도 하고  재밌긴 햿어요\r\n다행히 망나니?가 없어서 ㅎㅎ",
          "original_form": "밥도 먹고 겜도 하고  재밌긴 햿어요\r\n다행히 망나니?가 없어서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:47"
        },
        {
          "id": "MDRW2100009542.1.18",
          "form": "가끔 딴 길드 보면\r\n이상한사람들 있어서 해체도 되고 그랬었거든용",
          "original_form": "가끔 딴 길드 보면\r\n이상한사람들 있어서 해체도 되고 그랬었거든용",
          "speaker_id": "2",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100009542.1.19",
          "form": "ㅎㅎ역시 재밌게하셨네요 ㅣㅎ",
          "original_form": "ㅎㅎ역시 재밌게하셨네요 ㅣㅎ",
          "speaker_id": "1",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100009542.1.20",
          "form": "한번빠지면 계속몰입하는스타일이신가요?ㅎ",
          "original_form": "한번빠지면 계속몰입하는스타일이신가요?ㅎ",
          "speaker_id": "1",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100009542.1.21",
          "form": "아뇨 ㅎㅎ 게임은 2시간이상 안했어요",
          "original_form": "아뇨 ㅎㅎ 게임은 2시간이상 안했어요",
          "speaker_id": "2",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100009542.1.22",
          "form": "더하면 피곤 ㅎㅎㅎ",
          "original_form": "더하면 피곤 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100009542.1.23",
          "form": "그정도밖에요?ㅎ",
          "original_form": "그정도밖에요?ㅎ",
          "speaker_id": "1",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100009542.1.24",
          "form": "전 온라인은 안좋아하고 오프라인게임만 즐겨했는데... 저혼자 맨날 밤샜어요ㅎㅎㅎ",
          "original_form": "전 온라인은 안좋아하고 오프라인게임만 즐겨했는데... 저혼자 맨날 밤샜어요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100009542.1.25",
          "form": "이게 시간 제한 안두면 뒷날 몽롱하더라구요 ㅎㅎ",
          "original_form": "이게 시간 제한 안두면 뒷날 몽롱하더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100009542.1.26",
          "form": "누구랑 대화하는것도아니고 나혼자 축구게임하면서ㅎㅎ 참 어리석었었죠ㅎ",
          "original_form": "누구랑 대화하는것도아니고 나혼자 축구게임하면서ㅎㅎ 참 어리석었었죠ㅎ",
          "speaker_id": "1",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100009542.1.27",
          "form": "맞아요ㅎ 시간제한둬야하는데...",
          "original_form": "맞아요ㅎ 시간제한둬야하는데...",
          "speaker_id": "1",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100009542.1.28",
          "form": "잘안되죠 그게ㅎㅎ",
          "original_form": "잘안되죠 그게ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100009542.1.29",
          "form": "그래도 뭔가 미치도록 하고싶은게 있읆때가 좋은거 같아요 ㅎㅎ",
          "original_form": "그래도 뭔가 미치도록 하고싶은게 있읆때가 좋은거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100009542.1.30",
          "form": "온라인 커뮤니티하니깐 생각나네요ㅎ 예전에 거상하세요?",
          "original_form": "온라인 커뮤니티하니깐 생각나네요ㅎ 예전에 거상하세요?",
          "speaker_id": "1",
