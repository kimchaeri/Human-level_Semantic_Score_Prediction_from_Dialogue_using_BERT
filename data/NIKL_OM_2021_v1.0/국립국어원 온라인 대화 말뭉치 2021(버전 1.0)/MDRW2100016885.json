      "utterance": [
        {
          "id": "MDRW2100016885.1.1",
          "form": "스포츠좋아하세요~?",
          "original_form": "스포츠좋아하세요~?",
          "speaker_id": "1",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016885.1.2",
          "form": "네 스포츠 하는 것보단 보는 것 좋아해요",
          "original_form": "네 스포츠 하는 것보단 보는 것 좋아해요",
          "speaker_id": "2",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016885.1.3",
          "form": "저도 보는것만 ㅎㅎ",
          "original_form": "저도 보는것만 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 16:17"
        },
        {
          "id": "MDRW2100016885.1.4",
          "form": "어떤 종목 좋아하세요?",
          "original_form": "어떤 종목 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100016885.1.5",
          "form": "챙겨서보는종목은없는데 ! 요번에올림픽할때",
          "original_form": "챙겨서보는종목은없는데 ! 요번에올림픽할때",
          "speaker_id": "1",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100016885.1.6",
          "form": "배구랑 양궁봤어요!",
          "original_form": "배구랑 양궁봤어요!",
          "speaker_id": "1",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100016885.1.7",
          "form": "오 저도 여자배구랑 양궁 봤어요",
          "original_form": "오 저도 여자배구랑 양궁 봤어요",
          "speaker_id": "2",
          "time": "20210826 17:12"
        },
        {
          "id": "MDRW2100016885.1.8",
          "form": "정말 열심히 응원했죠",
          "original_form": "정말 열심히 응원했죠",
          "speaker_id": "2",
          "time": "20210826 17:12"
        },
        {
          "id": "MDRW2100016885.1.9",
          "form": "선수들정말 멋있더라구요ㅠㅠ!!",
          "original_form": "선수들정말 멋있더라구요ㅠㅠ!!",
          "speaker_id": "1",
          "time": "20210826 17:12"
        },
        {
          "id": "MDRW2100016885.1.10",
          "form": "네 열심히 경기하니 저도 열심히 응원하게 되고ㅠㅠ",
          "original_form": "네 열심히 경기하니 저도 열심히 응원하게 되고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100016885.1.11",
          "form": "전 안산선수는몰랐는데! 요번에 아주기억에남앗어요!",
          "original_form": "전 안산선수는몰랐는데! 요번에 아주기억에남앗어요!",
          "speaker_id": "1",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100016885.1.12",
          "form": "네네 이번 올림픽 때 정말 대단했죠",
          "original_form": "네네 이번 올림픽 때 정말 대단했죠",
          "speaker_id": "2",
          "time": "20210826 17:16"
        },
        {
          "id": "MDRW2100016885.1.13",
          "form": "옛날엔 야구장도 자주갔는데",
          "original_form": "옛날엔 야구장도 자주갔는데",
          "speaker_id": "1",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100016885.1.14",
          "form": "벌써...6~7년된얘기네요ㅠ",
          "original_form": "벌써...6~7년된얘기네요ㅠ",
          "speaker_id": "1",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100016885.1.15",
          "form": "저도 야구장 종종 갔었는데",
          "original_form": "저도 야구장 종종 갔었는데",
          "speaker_id": "2",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100016885.1.16",
          "form": "어느 팀을 응원하세요?",
          "original_form": "어느 팀을 응원하세요?",
          "speaker_id": "2",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100016885.1.17",
          "form": "서울에있을때 갔었는데 그때 같이 다니던언니가",
          "original_form": "서울에있을때 갔었는데 그때 같이 다니던언니가",
          "speaker_id": "1",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100016885.1.18",
          "form": "부산이라 ㅎ 롯데찐팬이욨어요 ㅋㅋ 전 롯데석에 앉아서 그냥 맥주먹고 ㅎㅎ 응원하고 ㅎㅎ",
          "original_form": "부산이라 ㅎ 롯데찐팬이욨어요 ㅋㅋ 전 롯데석에 앉아서 그냥 맥주먹고 ㅎㅎ 응원하고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100016885.1.19",
          "form": "아하ㅎㅎ 맞아요 야구장은 치맥하는 재미죠!",
          "original_form": "아하ㅎㅎ 맞아요 야구장은 치맥하는 재미죠!",
          "speaker_id": "2",
          "time": "20210826 17:29"
        }
      ]
