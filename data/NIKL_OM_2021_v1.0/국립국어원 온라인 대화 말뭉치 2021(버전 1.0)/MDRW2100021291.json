        {
          "id": "MDRW2100021291.1.1",
          "form": "맛집조아하시나요",
          "original_form": "맛집조아하시나요",
          "speaker_id": "1",
          "time": "20210828 17:20"
        },
        {
          "id": "MDRW2100021291.1.2",
          "form": "맛집 좋아는 하는데, 웨이팅시간 긴걸 안 좋아해서 자주는 안 가요",
          "original_form": "맛집 좋아는 하는데, 웨이팅시간 긴걸 안 좋아해서 자주는 안 가요",
          "speaker_id": "2",
          "time": "20210828 17:20"
        },
        {
          "id": "MDRW2100021291.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 17:20"
        },
        {
          "id": "MDRW2100021291.1.4",
          "form": "저는 좋아해요!!",
          "original_form": "저는 좋아해요!!",
          "speaker_id": "1",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100021291.1.5",
          "form": "맛집 많이 찾아다니시나봐요!",
          "original_form": "맛집 많이 찾아다니시나봐요!",
          "speaker_id": "2",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100021291.1.6",
          "form": "네네 돈까스조아해요",
          "original_form": "네네 돈까스조아해요",
          "speaker_id": "1",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100021291.1.7",
          "form": "돈까스 맛있죠!",
          "original_form": "돈까스 맛있죠!",
          "speaker_id": "2",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100021291.1.8",
          "form": "아우 침고이네요",
          "original_form": "아우 침고이네요",
          "speaker_id": "1",
          "time": "20210828 17:28"
        },
        {
          "id": "MDRW2100021291.1.9",
          "form": "돈까스 먹은지 넘 오래되었어요ㅠㅠ",
          "original_form": "돈까스 먹은지 넘 오래되었어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 17:28"
        },
        {
          "id": "MDRW2100021291.1.10",
          "form": "크레이지카츠 함 가보세요!",
          "original_form": "크레이지카츠 함 가보세요!",
          "speaker_id": "1",
          "time": "20210828 17:30"
        },
        {
          "id": "MDRW2100021291.1.11",
          "form": "크레이지카츠라 기억해둬야겠네요",
          "original_form": "크레이지카츠라 기억해둬야겠네요",
          "speaker_id": "2",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100021291.1.12",
          "form": "검색한번 해보세요ㅎㅎ",
          "original_form": "검색한번 해보세요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100021291.1.13",
          "form": "네네 그럴게욯ㅎ",
          "original_form": "네네 그럴게욯ㅎ",
          "speaker_id": "2",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100021291.1.14",
          "form": "특로스카츠 넘 맛있었네요",
          "original_form": "특로스카츠 넘 맛있었네요",
          "speaker_id": "1",
          "time": "20210828 17:35"
        },
        {
          "id": "MDRW2100021291.1.15",
          "form": "검색해보니 넘 땡기는걸요 \r\n사진으로만봐도 바삭바삭 맛있어보여요",
