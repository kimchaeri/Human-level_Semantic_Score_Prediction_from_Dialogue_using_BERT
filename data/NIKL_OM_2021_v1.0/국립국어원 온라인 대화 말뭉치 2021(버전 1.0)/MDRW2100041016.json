      "utterance": [
        {
          "id": "MDRW2100041016.1.1",
          "form": "점저는 닭볶음탕을 먹을 것 같아요!!!!",
          "original_form": "점저는 닭볶음탕을 먹을 것 같아요!!!!",
          "speaker_id": "1",
          "time": "20210905 15:14"
        },
        {
          "id": "MDRW2100041016.1.2",
          "form": "매콤하게!!",
          "original_form": "매콤하게!!",
          "speaker_id": "1",
          "time": "20210905 15:14"
        },
        {
          "id": "MDRW2100041016.1.3",
          "form": "우왕 맛있겠어요..!",
          "original_form": "우왕 맛있겠어요..!",
          "speaker_id": "2",
          "time": "20210905 15:23"
        },
        {
          "id": "MDRW2100041016.1.4",
          "form": "저는 아마도 치킨을 먹을거 같아요 ㅋㅋ",
          "original_form": "저는 아마도 치킨을 먹을거 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:23"
        },
        {
          "id": "MDRW2100041016.1.5",
          "form": "배달로 시킬실건가요?",
          "original_form": "배달로 시킬실건가요?",
          "speaker_id": "2",
          "time": "20210905 15:24"
        },
        {
          "id": "MDRW2100041016.1.6",
          "form": "우와아아 오늘 그러면 네네치킨 드시는 건가요?!",
          "original_form": "우와아아 오늘 그러면 네네치킨 드시는 건가요?!",
          "speaker_id": "1",
          "time": "20210905 15:24"
        },
        {
          "id": "MDRW2100041016.1.7",
          "form": "아 저는 엄마가 오셔서 요리하고 계세요 ㅋㅋ맛있는 냄새 솔솔",
          "original_form": "아 저는 엄마가 오셔서 요리하고 계세요 ㅋㅋ맛있는 냄새 솔솔",
          "speaker_id": "1",
          "time": "20210905 15:24"
        },
        {
          "id": "MDRW2100041016.1.8",
          "form": "이른 저녁이지만 좋으시겟어요 ㅎㅎ",
          "original_form": "이른 저녁이지만 좋으시겟어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 15:26"
        },
        {
          "id": "MDRW2100041016.1.9",
          "form": "네!! 네네치킨을 시킬거 같아요ㅋㅋㅋㅋ",
          "original_form": "네!! 네네치킨을 시킬거 같아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:26"
        },
        {
          "id": "MDRW2100041016.1.10",
          "form": "유명한 카페나 베이커리 아는곳 있으신가요?",
          "original_form": "유명한 카페나 베이커리 아는곳 있으신가요?",
          "speaker_id": "2",
          "time": "20210905 15:27"
        },
        {
          "id": "MDRW2100041016.1.11",
          "form": "음 서울에서 동네마다 유명하다고해서 가본곳들 몇번 있는데",
          "original_form": "음 서울에서 동네마다 유명하다고해서 가본곳들 몇번 있는데",
          "speaker_id": "1",
          "time": "20210905 15:46"
        },
        {
          "id": "MDRW2100041016.1.12",
          "form": "최근에는 망원동에 있는",
          "original_form": "최근에는 망원동에 있는",
          "speaker_id": "1",
          "time": "20210905 15:46"
        },
        {
          "id": "MDRW2100041016.1.13",
          "form": "블랑제리코팡에서 빵 사다먹었는데 맛있었어요!!",
          "original_form": "블랑제리코팡에서 빵 사다먹었는데 맛있었어요!!",
          "speaker_id": "1",
          "time": "20210905 15:47"
        },
        {
          "id": "MDRW2100041016.1.14",
          "form": "첨 들어봐요!!!! 어떤 빵이 맛있었나요?",
          "original_form": "첨 들어봐요!!!! 어떤 빵이 맛있었나요?",
          "speaker_id": "2",
          "time": "20210905 15:56"
        },
        {
          "id": "MDRW2100041016.1.15",
          "form": "음 거기 바게트가 되게 유명하대요 ㅋㅋ 바게트도 맛있었고 건강빵류 다 맛있었어요 ㅋㅋㅋ",
          "original_form": "음 거기 바게트가 되게 유명하대요 ㅋㅋ 바게트도 맛있었고 건강빵류 다 맛있었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:00"
        },
        {
          "id": "MDRW2100041016.1.16",
          "form": "맛있는 빵집 아시나요?! ㅋㅋㅋ",
          "original_form": "맛있는 빵집 아시나요?! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:00"
        },
        {
          "id": "MDRW2100041016.1.17",
          "form": "어.. 문래동에 러스트베이커리 케잌 맛있었구요",
          "original_form": "어.. 문래동에 러스트베이커리 케잌 맛있었구요",
          "speaker_id": "2",
          "time": "20210905 19:16"
        },
        {
          "id": "MDRW2100041016.1.18",
          "form": "역시 문래동인데 마이스페이스라는 카페의 크라상도 맛있었어오",
          "original_form": "역시 문래동인데 마이스페이스라는 카페의 크라상도 맛있었어오",
          "speaker_id": "2",
          "time": "20210905 19:17"
        },
        {
          "id": "MDRW2100041016.1.19",
          "form": "오 안그래도 저 문래동에 양키통닭??이라는곳이",
          "original_form": "오 안그래도 저 문래동에 양키통닭??이라는곳이",
          "speaker_id": "1",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100041016.1.20",
          "form": "유명해서 한번 가보려했는데 그때 들러봐야겠군요!!ㅋㅋㅋㅋㅋㅋ",
          "original_form": "유명해서 한번 가보려했는데 그때 들러봐야겠군요!!ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100041016.1.21",
          "form": "양키통닭 유명해서 사람이 많으니 다른곳도 후보로 올려놓으시거나",
          "original_form": "양키통닭 유명해서 사람이 많으니 다른곳도 후보로 올려놓으시거나",
          "speaker_id": "2",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100041016.1.22",
          "form": "일찍 가시는거 추천드려요 ㅎㅎㅎ",
          "original_form": "일찍 가시는거 추천드려요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100041016.1.23",
          "form": "맞아요 오픈시간 맞춰서 가도 이미 사람들이 줄서있다고 들었어요 ㅠㅠㅠㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 오픈시간 맞춰서 가도 이미 사람들이 줄서있다고 들었어요 ㅠㅠㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041016.1.24",
          "form": "양키통닭 주변에도 맛집이 있으니 걱정마시구요 ㅎㅎ",
          "original_form": "양키통닭 주변에도 맛집이 있으니 걱정마시구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 21:04"
        },
        {
          "id": "MDRW2100041016.1.25",
          "form": "오 문래동 맛집 또 추천해주세요!!!! 저 문래동은 처음이거든요",
          "original_form": "오 문래동 맛집 또 추천해주세요!!!! 저 문래동은 처음이거든요",
          "speaker_id": "1",
          "time": "20210905 21:11"
        }
      ]
