      "utterance": [
        {
          "id": "MDRW2100030087.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210903 21:30"
        },
        {
          "id": "MDRW2100030087.1.2",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100030087.1.3",
          "form": "키우고 싶긴한데",
          "original_form": "키우고 싶긴한데",
          "speaker_id": "2",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100030087.1.4",
          "form": "지금 사는 곳이",
          "original_form": "지금 사는 곳이",
          "speaker_id": "2",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100030087.1.5",
          "form": "동물 금지라서;;",
          "original_form": "동물 금지라서;;",
          "speaker_id": "2",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100030087.1.6",
          "form": "뭐 키우시나요?",
          "original_form": "뭐 키우시나요?",
          "speaker_id": "2",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100030087.1.7",
          "form": "저는 구피를 키웁니다 ㅋㅋ",
          "original_form": "저는 구피를 키웁니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100030087.1.8",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "2",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100030087.1.9",
          "form": "물고기 키울라면",
          "original_form": "물고기 키울라면",
          "speaker_id": "2",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100030087.1.10",
          "form": "뭐 나름대로 준비해야 되는거 많지 않나요",
          "original_form": "뭐 나름대로 준비해야 되는거 많지 않나요",
          "speaker_id": "2",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100030087.1.11",
          "form": "물 관리가 힘듭니다",
          "original_form": "물 관리가 힘듭니다",
          "speaker_id": "1",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100030087.1.12",
          "form": "네네 그렇게 들었어요",
          "original_form": "네네 그렇게 들었어요",
          "speaker_id": "2",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100030087.1.13",
          "form": "일이에요",
          "original_form": "일이에요",
          "speaker_id": "1",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100030087.1.14",
          "form": "솔직히 예전에는",
          "original_form": "솔직히 예전에는",
          "speaker_id": "2",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100030087.1.15",
          "form": "그냥 관상용(?)이라고만",
          "original_form": "그냥 관상용(?)이라고만",
          "speaker_id": "2",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100030087.1.16",
          "form": "생각해서",
          "original_form": "생각해서",
          "speaker_id": "2",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100030087.1.17",
          "form": "진짜 손 안 가는 줄 알았는데",
          "original_form": "진짜 손 안 가는 줄 알았는데",
          "speaker_id": "2",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100030087.1.18",
          "form": "아니더라고요",
          "original_form": "아니더라고요",
          "speaker_id": "2",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100030087.1.19",
          "form": "케어가 힘들어요",
          "original_form": "케어가 힘들어요",
          "speaker_id": "1",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100030087.1.20",
          "form": "막 보니까",
          "original_form": "막 보니까",
          "speaker_id": "2",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100030087.1.21",
          "form": "산소라던가",
          "original_form": "산소라던가",
          "speaker_id": "2",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100030087.1.22",
          "form": "수질이라던가",
          "original_form": "수질이라던가",
          "speaker_id": "2",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100030087.1.23",
          "form": "의외로 신경 쓸 게 많더라고요",
          "original_form": "의외로 신경 쓸 게 많더라고요",
          "speaker_id": "2",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100030087.1.24",
          "form": "사람보더 다 신경 쓰여요",
          "original_form": "사람보더 다 신경 쓰여요",
          "speaker_id": "1",
          "time": "20210903 21:45"
        },
        {
          "id": "MDRW2100030087.1.25",
          "form": "그러고보니",
          "original_form": "그러고보니",
          "speaker_id": "2",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100030087.1.26",
          "form": "구피는",
          "original_form": "구피는",
          "speaker_id": "2",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100030087.1.27",
          "form": "사람 알아보나요?",
          "original_form": "사람 알아보나요?",
          "speaker_id": "2",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100030087.1.28",
          "form": "아니여",
          "original_form": "아니여",
          "speaker_id": "1",
          "time": "20210903 21:47"
        }
      ]
