      "utterance": [
        {
          "id": "MDRW2100038222.1.1",
          "form": "쏜살칰이슬퍼하구있어요..",
          "original_form": "쏜살칰이슬퍼하구있어요..",
          "speaker_id": "2",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100038222.1.2",
          "form": "근데식어도맛있더라구요",
          "original_form": "근데식어도맛있더라구요",
          "speaker_id": "2",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100038222.1.3",
          "form": "ㄱㄱㅋㅋㄱㅋㅈㄱㅈㄱ",
          "original_form": "ㄱㄱㅋㅋㄱㅋㅈㄱㅈㄱ",
          "speaker_id": "2",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100038222.1.4",
          "form": "ㅎㅎ 이제 부대찌개 고지가 보여요!",
          "original_form": "ㅎㅎ 이제 부대찌개 고지가 보여요!",
          "speaker_id": "1",
          "time": "20210902 01:05"
        },
        {
          "id": "MDRW2100038222.1.5",
          "form": "쏜살치킨 드셔보셨나요?",
          "original_form": "쏜살치킨 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100038222.1.6",
          "form": "부대찌개는 식당포장? 배달? 밀키트?",
          "original_form": "부대찌개는 식당포장? 배달? 밀키트?",
          "speaker_id": "2",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100038222.1.7",
          "form": "네쏜살먹어봤어요!!!!",
          "original_form": "네쏜살먹어봤어요!!!!",
          "speaker_id": "2",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100038222.1.8",
          "form": "저렴하구\r\n뻑뻑살천지라좋아합니다",
          "original_form": "저렴하구\r\n뻑뻑살천지라좋아합니다",
          "speaker_id": "2",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100038222.1.9",
          "form": "요기요서 gs25 포장 만원 사면 오천원 할인하더라구요ㅎ",
          "original_form": "요기요서 gs25 포장 만원 사면 오천원 할인하더라구요ㅎ",
          "speaker_id": "1",
          "time": "20210902 01:07"
        },
        {
          "id": "MDRW2100038222.1.10",
          "form": "헐 요새합니깍!!!!!",
          "original_form": "헐 요새합니깍!!!!!",
          "speaker_id": "2",
          "time": "20210902 01:07"
        },
        {
          "id": "MDRW2100038222.1.11",
          "form": "마침 2000쿠폰 있어서 퇴근길에 3000원에 업어옴ㅋ",
          "original_form": "마침 2000쿠폰 있어서 퇴근길에 3000원에 업어옴ㅋ",
          "speaker_id": "1",
          "time": "20210902 01:07"
        },
        {
          "id": "MDRW2100038222.1.12",
          "form": "오ㆍ...엌...가야겠어요...",
          "original_form": "오ㆍ...엌...가야겠어요...",
          "speaker_id": "2",
          "time": "20210902 01:07"
        },
        {
          "id": "MDRW2100038222.1.13",
          "form": "오늘도 하는듯 한데요ㅎㅎ",
          "original_form": "오늘도 하는듯 한데요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100038222.1.14",
          "form": "배달은 말구요ㅎ",
          "original_form": "배달은 말구요ㅎ",
          "speaker_id": "1",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100038222.1.15",
          "form": "오늘 돈 아꼈으니 내일은 비싼거 먹어도 가책이 없겠어요ㅎ",
          "original_form": "오늘 돈 아꼈으니 내일은 비싼거 먹어도 가책이 없겠어요ㅎ",
          "speaker_id": "1",
          "time": "20210902 01:08"
        },
        {
          "id": "MDRW2100038222.1.16",
          "form": "맞아요 번갈아가면서먹어주면 또이또이되니까 괜찮음",
          "original_form": "맞아요 번갈아가면서먹어주면 또이또이되니까 괜찮음",
          "speaker_id": "2",
          "time": "20210902 01:10"
        },
        {
          "id": "MDRW2100038222.1.17",
          "form": "ㅋㅋㅋㄱㅈㅋㅈㅋ",
          "original_form": "ㅋㅋㅋㄱㅈㅋㅈㅋ",
          "speaker_id": "2",
          "time": "20210902 01:10"
        },
        {
          "id": "MDRW2100038222.1.18",
          "form": "포장찾아봐야겠어요 으흐흐 정보감사합니다",
          "original_form": "포장찾아봐야겠어요 으흐흐 정보감사합니다",
          "speaker_id": "2",
          "time": "20210902 01:10"
        },
        {
          "id": "MDRW2100038222.1.19",
          "form": "ㅎㅎ 지금도 오천원 할인 뜨네요!",
          "original_form": "ㅎㅎ 지금도 오천원 할인 뜨네요!",
          "speaker_id": "1",
          "time": "20210902 01:10"
        },
        {
