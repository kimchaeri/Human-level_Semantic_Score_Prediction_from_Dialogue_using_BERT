        {
          "id": "MDRW2100037000.1.1",
          "form": "name1님은 저녁에 뭐 드실 계획인가요?",
          "original_form": "&name1&님은 저녁에 뭐 드실 계획인가요?",
          "speaker_id": "2",
          "time": "20210831 16:45"
        },
        {
          "id": "MDRW2100037000.1.2",
          "form": "저는 닭가슴살이요 ㅋㅋ",
          "original_form": "저는 닭가슴살이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 16:45"
        },
        {
          "id": "MDRW2100037000.1.3",
          "form": "저녁 메뉴 정하셨어요?",
          "original_form": "저녁 메뉴 정하셨어요?",
          "speaker_id": "1",
          "time": "20210831 16:45"
        },
        {
          "id": "MDRW2100037000.1.4",
          "form": "닭가슴살이면 다이어트용인가요?",
          "original_form": "닭가슴살이면 다이어트용인가요?",
          "speaker_id": "2",
          "time": "20210831 16:46"
        },
        {
          "id": "MDRW2100037000.1.5",
          "form": "음...허닭에서 샀는데",
          "original_form": "음...허닭에서 샀는데",
          "speaker_id": "1",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100037000.1.6",
          "form": "치즈퐁듀 닭가슴살 샀거든요",
          "original_form": "치즈퐁듀 닭가슴살 샀거든요",
          "speaker_id": "1",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100037000.1.7",
          "form": "그걸로 그냥 떼우려구요 ㅎ",
          "original_form": "그걸로 그냥 떼우려구요 ㅎ",
          "speaker_id": "1",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100037000.1.8",
          "form": "치즈퐁듀라니.. 우리딸이 들으면 엄청 먹고싶어하겠네요..ㅎㅎ",
          "original_form": "치즈퐁듀라니.. 우리딸이 들으면 엄청 먹고싶어하겠네요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 16:50"
        },
        {
          "id": "MDRW2100037000.1.9",
          "form": "우리집은 오늘 아마도 부침개쪽으로 먹지 않을까 생각해요",
          "original_form": "우리집은 오늘 아마도 부침개쪽으로 먹지 않을까 생각해요",
          "speaker_id": "2",
          "time": "20210831 16:50"
        },
        {
          "id": "MDRW2100037000.1.10",
          "form": "저도 맛이 어떤지는 모르겠어요",
          "original_form": "저도 맛이 어떤지는 모르겠어요",
          "speaker_id": "1",
          "time": "20210831 16:50"
        },
        {
          "id": "MDRW2100037000.1.11",
          "form": "궁금해서 사봤거든요",
          "original_form": "궁금해서 사봤거든요",
          "speaker_id": "1",
          "time": "20210831 16:50"
        },
        {
          "id": "MDRW2100037000.1.12",
          "form": "오 비오는날 부침개",
          "original_form": "오 비오는날 부침개",
          "speaker_id": "1",
          "time": "20210831 16:50"
        },
        {
          "id": "MDRW2100037000.1.13",
          "form": "딱 좋네요",
          "original_form": "딱 좋네요",
          "speaker_id": "1",
          "time": "20210831 16:50"
        },
        {
          "id": "MDRW2100037000.1.14",
          "form": "장마라 전을 자주 먹네요..ㅎㅎ",
          "original_form": "장마라 전을 자주 먹네요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 16:51"
        },
        {
          "id": "MDRW2100037000.1.15",
          "form": "전은 계속 먹어도 맛날것같아요",
          "original_form": "전은 계속 먹어도 맛날것같아요",
          "speaker_id": "1",
          "time": "20210831 16:55"
        },
        {
          "id": "MDRW2100037000.1.16",
          "form": "밀가루와 기름의 만남은 항상 좋죠..하하",
          "original_form": "밀가루와 기름의 만남은 항상 좋죠..하하",
          "speaker_id": "2",
          "time": "20210831 16:55"
        },
        {
          "id": "MDRW2100037000.1.17",
          "form": "소주한잔이랑 같이 곁들이면 최상의 궁합 ㅋㅋ",
          "original_form": "소주한잔이랑 같이 곁들이면 최상의 궁합 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 16:55"
        },
        {
          "id": "MDRW2100037000.1.18",
          "form": "그럼 닭가슴살 그대로 먹는건가요?",
          "original_form": "그럼 닭가슴살 그대로 먹는건가요?",
          "speaker_id": "2",
          "time": "20210831 16:56"
        },
        {
          "id": "MDRW2100037000.1.19",
          "form": "뭐 따로 요리 할거 없이?",
          "original_form": "뭐 따로 요리 할거 없이?",
          "speaker_id": "2",
          "time": "20210831 16:56"
        },
        {
          "id": "MDRW2100037000.1.20",
          "form": "치즈퐁듀 안에 버무려져있는거같아요",
          "original_form": "치즈퐁듀 안에 버무려져있는거같아요",
          "speaker_id": "1",
          "time": "20210831 16:57"
        },
        {
          "id": "MDRW2100037000.1.21",
          "form": "그럼 렌지에만 돌려서 간단히 섭취ㅎㅎ",
          "original_form": "그럼 렌지에만 돌려서 간단히 섭취ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 16:57"
        },
        {
          "id": "MDRW2100037000.1.22",
          "form": "간단식이군요",
          "original_form": "간단식이군요",
          "speaker_id": "2",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100037000.1.23",
          "form": "간편하게 먹을 수 있는 제품들이 많이 나와서",
          "original_form": "간편하게 먹을 수 있는 제품들이 많이 나와서",
          "speaker_id": "2",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100037000.1.24",
          "form": "편하게 주문해서 먹응ㄹ 수 있는거 같아요",
          "original_form": "편하게 주문해서 먹응ㄹ 수 있는거 같아요",
          "speaker_id": "2",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100037000.1.25",
          "form": "저는 주로 마트나 시장에서 장 보거든요",
          "original_form": "저는 주로 마트나 시장에서 장 보거든요",
          "speaker_id": "2",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100037000.1.26",
          "form": "저는 인터넷으로 주로봐요",
          "original_form": "저는 인터넷으로 주로봐요",
          "speaker_id": "1",
          "time": "20210831 17:02"
        },
        {
          "id": "MDRW2100037000.1.27",
          "form": "안그럼 충동구매가 많아지더라구요",
          "original_form": "안그럼 충동구매가 많아지더라구요",
          "speaker_id": "1",
          "time": "20210831 17:03"
        },
        {
          "id": "MDRW2100037000.1.28",
          "form": "그렇군요.. 온라인 쇼핑이 익숙치 않아서..",
          "original_form": "그렇군요.. 온라인 쇼핑이 익숙치 않아서..",
          "speaker_id": "2",
          "time": "20210831 17:06"
        },
        {
          "id": "MDRW2100037000.1.29",
          "form": "주로 뭘 구매하나요?",
          "original_form": "주로 뭘 구매하나요?",
          "speaker_id": "2",
          "time": "20210831 17:06"
        },
        {
          "id": "MDRW2100037000.1.30",
          "form": "고기류? 채소류?",
          "original_form": "고기류? 채소류?",
          "speaker_id": "2",
          "time": "20210831 17:06"
        },
        {
          "id": "MDRW2100037000.1.31",
          "form": "종류별로 그때그때 다른거같아요",
          "original_form": "종류별로 그때그때 다른거같아요",
          "speaker_id": "1",
          "time": "20210831 17:06"
        },
        {
          "id": "MDRW2100037000.1.32",
          "form": "저는이제 퇴근해야겠네요",
          "original_form": "저는이제 퇴근해야겠네요",
          "speaker_id": "1",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100037000.1.33",
