      "utterance": [
        {
          "id": "MDRW2100044357.1.1",
          "form": "ㅋㅋㅋ 걔네들듀 크규 싶겠지ㅠㅠ",
          "original_form": "ㅋㅋㅋ 걔네들듀 크규 싶겠지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044357.1.2",
          "form": "유전자가 글케 된걸 어쩌겠어ㅠㅠ",
          "original_form": "유전자가 글케 된걸 어쩌겠어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044357.1.3",
          "form": "다이어트하느라 그런것도 있겠다ㅋㅋㅋㅋ 개웃겨",
          "original_form": "다이어트하느라 그런것도 있겠다ㅋㅋㅋㅋ 개웃겨",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044357.1.4",
          "form": "마자마자",
          "original_form": "마자마자",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044357.1.5",
          "form": "위로맞냐고ㅋㅋㅋ",
          "original_form": "위로맞냐고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044357.1.6",
          "form": "약간 조롱같나?ㅋㅋㅋ 진심이야ㅠㅠ",
          "original_form": "약간 조롱같나?ㅋㅋㅋ 진심이야ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044357.1.7",
          "form": "응ㅋㅋ진심조롱ㅠㅠㅋㅋ",
          "original_form": "응ㅋㅋ진심조롱ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044357.1.8",
          "form": "근데 진짜 오샌 환경호르몬 영향도 많이받고 그래서 더 그럴수도 있어",
          "original_form": "근데 진짜 오샌 환경호르몬 영향도 많이받고 그래서 더 그럴수도 있어",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044357.1.9",
          "form": "그리고 그거있잖아",
          "original_form": "그리고 그거있잖아",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044357.1.10",
          "form": "어떤거?",
          "original_form": "어떤거?",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044357.1.11",
          "form": "찌꺼기? 그..\n그 무슨 법칙 하면서 알지?",
          "original_form": "찌꺼기? 그..\n그 무슨 법칙 하면서 알지?",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044357.1.12",
          "form": "스위트홈말하는거지?",
          "original_form": "스위트홈말하는거지?",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044357.1.13",
          "form": "응ㅋㅋ 뭐였지?",
          "original_form": "응ㅋㅋ 뭐였지?",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
