      "utterance": [
        {
          "id": "MDRW2100039117.1.1",
          "form": "이젠 맑아졌어요",
          "original_form": "이젠 맑아졌어요",
          "speaker_id": "2",
          "time": "20210903 10:42"
        },
        {
          "id": "MDRW2100039117.1.2",
          "form": "해도 보여요",
          "original_form": "해도 보여요",
          "speaker_id": "2",
          "time": "20210903 10:42"
        },
        {
          "id": "MDRW2100039117.1.3",
          "form": "드디어 빨래를 걷었습니다",
          "original_form": "드디어 빨래를 걷었습니다",
          "speaker_id": "1",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100039117.1.4",
          "form": "며칠동안 안마르던 걸.. 드디어",
          "original_form": "며칠동안 안마르던 걸.. 드디어",
          "speaker_id": "1",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100039117.1.5",
          "form": "ㅋㅋ 빨래가 구원되었군요",
          "original_form": "ㅋㅋ 빨래가 구원되었군요",
          "speaker_id": "2",
          "time": "20210903 11:08"
        },
        {
          "id": "MDRW2100039117.1.6",
          "form": "네.. 당분간 해 그림이 있던데 오늘 또 빨래해야죠..ㅎㅎ",
          "original_form": "네.. 당분간 해 그림이 있던데 오늘 또 빨래해야죠..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 11:11"
        },
        {
          "id": "MDRW2100039117.1.7",
          "form": "오늘같은 가을 날씨가 당분간 지속되면 좋겠네요",
          "original_form": "오늘같은 가을 날씨가 당분간 지속되면 좋겠네요",
          "speaker_id": "1",
          "time": "20210903 11:11"
        },
        {
          "id": "MDRW2100039117.1.8",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210903 11:57"
        },
        {
          "id": "MDRW2100039117.1.9",
          "form": "오늘 하늘도 이쁘네요",
          "original_form": "오늘 하늘도 이쁘네요",
          "speaker_id": "2",
          "time": "20210903 11:57"
        },
        {
          "id": "MDRW2100039117.1.10",
          "form": "이게 가을하늘이죠",
          "original_form": "이게 가을하늘이죠",
          "speaker_id": "1",
          "time": "20210903 12:50"
        },
        {
          "id": "MDRW2100039117.1.11",
          "form": "그동안 너무 우중충했어요",
          "original_form": "그동안 너무 우중충했어요",
          "speaker_id": "1",
          "time": "20210903 12:50"
        },
        {
          "id": "MDRW2100039117.1.12",
          "form": "대신 온도가 가을이 아니네요",
          "original_form": "대신 온도가 가을이 아니네요",
          "speaker_id": "2",
          "time": "20210903 12:50"
        },
        {
          "id": "MDRW2100039117.1.13",
          "form": "여기 지금 30도에요",
          "original_form": "여기 지금 30도에요",
          "speaker_id": "2",
          "time": "20210903 12:50"
        },
        {
          "id": "MDRW2100039117.1.14",
          "form": "그쵸.. 아직 늦여름 초가을",
          "original_form": "그쵸.. 아직 늦여름 초가을",
          "speaker_id": "1",
          "time": "20210903 12:58"
        },
        {
          "id": "MDRW2100039117.1.15",
          "form": "낮엔 여름 같을 거지만 일단 해가 났다는 데서 기분이 좋네요",
          "original_form": "낮엔 여름 같을 거지만 일단 해가 났다는 데서 기분이 좋네요",
          "speaker_id": "1",
          "time": "20210903 12:58"
        },
        {
          "id": "MDRW2100039117.1.16",
          "form": "바람도 제법 선선해졌구요",
          "original_form": "바람도 제법 선선해졌구요",
          "speaker_id": "1",
          "time": "20210903 12:58"
        },
        {
          "id": "MDRW2100039117.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210903 15:00"
        },
        {
          "id": "MDRW2100039117.1.18",
          "form": "그리고 습하지 않아요",
          "original_form": "그리고 습하지 않아요",
          "speaker_id": "2",
          "time": "20210903 15:00"
        },
        {
          "id": "MDRW2100039117.1.19",
          "form": "그게 좋아요",
          "original_form": "그게 좋아요",
          "speaker_id": "1",
          "time": "20210903 19:29"
        },
        {
          "id": "MDRW2100039117.1.20",
          "form": "그동안 너무 습했어요",
          "original_form": "그동안 너무 습했어요",
          "speaker_id": "1",
          "time": "20210903 19:29"
        },
        {
          "id": "MDRW2100039117.1.21",
          "form": "덥고 습하니 찝찝했죠",
          "original_form": "덥고 습하니 찝찝했죠",
          "speaker_id": "2",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100039117.1.22",
          "form": "담주에 비소식이 또 있긴 하지만 가을비겠죠?",
          "original_form": "담주에 비소식이 또 있긴 하지만 가을비겠죠?",
          "speaker_id": "1",
          "time": "20210903 20:48"
        },
        {
          "id": "MDRW2100039117.1.23",
          "form": "장마는 끝난 것이길 바래봅니다",
          "original_form": "장마는 끝난 것이길 바래봅니다",
          "speaker_id": "1",
          "time": "20210903 20:48"
        },
        {
          "id": "MDRW2100039117.1.24",
          "form": "잠깐 내리고 끝나겠죠",
          "original_form": "잠깐 내리고 끝나겠죠",
          "speaker_id": "2",
          "time": "20210903 20:49"
        },
        {
          "id": "MDRW2100039117.1.25",
          "form": "...제발요",
          "original_form": "...제발요",
          "speaker_id": "2",
          "time": "20210903 20:49"
        },
        {
          "id": "MDRW2100039117.1.26",
          "form": "이제는 비가 오더라도 저녁에 오고 낮에는 파란 하늘을 많이 보여줬으면 좋겠어요",
          "original_form": "이제는 비가 오더라도 저녁에 오고 낮에는 파란 하늘을 많이 보여줬으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100039117.1.27",
          "form": "높고 파란 하늘이 너무너무 그리워요",
          "original_form": "높고 파란 하늘이 너무너무 그리워요",
          "speaker_id": "1",
          "time": "20210903 20:54"
        }
      ]
    }
