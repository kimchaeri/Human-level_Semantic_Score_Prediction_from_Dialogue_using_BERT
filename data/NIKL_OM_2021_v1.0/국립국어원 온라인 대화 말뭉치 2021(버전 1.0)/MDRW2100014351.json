        },
        {
          "id": "MDRW2100014351.1.2",
          "form": "주제가 뭔데",
          "original_form": "주제가 뭔데",
          "speaker_id": "2",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100014351.1.3",
          "form": "여름특집이래",
          "original_form": "여름특집이래",
          "speaker_id": "1",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100014351.1.4",
          "form": "아직도 여름특집이네",
          "original_form": "아직도 여름특집이네",
          "speaker_id": "2",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100014351.1.5",
          "form": "엉 추리특급으로 범인은 누구라는데",
          "original_form": "엉 추리특급으로 범인은 누구라는데",
          "speaker_id": "1",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100014351.1.6",
          "form": "뭔 특집이 2개나 ㅋㅋ",
          "original_form": "뭔 특집이 2개나 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100014351.1.7",
          "form": "그러게 웃긴다 여름특집에 추리특급 ㅋㅋ",
          "original_form": "그러게 웃긴다 여름특집에 추리특급 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100014351.1.8",
          "form": "이제 하나?",
          "original_form": "이제 하나?",
          "speaker_id": "2",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100014351.1.9",
          "form": "아니 아직 광고중 광고가 길다",
          "original_form": "아니 아직 광고중 광고가 길다",
          "speaker_id": "1",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100014351.1.10",
          "form": "광고때문에 기다리는데 지쳐 다른데 보겠다",
          "original_form": "광고때문에 기다리는데 지쳐 다른데 보겠다",
          "speaker_id": "2",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100014351.1.11",
          "form": "내말이 그래도 볼려면 기다려야지뭐",
          "original_form": "내말이 그래도 볼려면 기다려야지뭐",
          "speaker_id": "1",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100014351.1.12",
          "form": "응 어쩔수없지",
          "original_form": "응 어쩔수없지",
          "speaker_id": "2",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100014351.1.13",
          "form": "근데 광고시작한지 이제 3분이다 아직 많이 남은듯",
          "original_form": "근데 광고시작한지 이제 3분이다 아직 많이 남은듯",
          "speaker_id": "1",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100014351.1.14",
          "form": "그럼 다른데 보다 틀어봐라",
          "original_form": "그럼 다른데 보다 틀어봐라",
          "speaker_id": "2",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100014351.1.15",
          "form": "그래야하나 갑자기 변덕이 ㅋㅋ",
          "original_form": "그래야하나 갑자기 변덕이 ㅋㅋ",
