      "utterance": [
        {
          "id": "MDRW2100009987.1.1",
          "form": "엄마랑 조성진 공연 보러가려고",
          "original_form": "엄마랑 조성진 공연 보러가려고",
          "speaker_id": "1",
          "time": "20210823 12:50"
        },
        {
          "id": "MDRW2100009987.1.2",
          "form": "티켓팅 준비할까 했는데 엄마가 안간대요ㅋㅋ",
          "original_form": "티켓팅 준비할까 했는데 엄마가 안간대요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 12:50"
        },
        {
          "id": "MDRW2100009987.1.3",
          "form": "ㅋ 왠일로 공연을 마다하지",
          "original_form": "ㅋ 왠일로 공연을 마다하지",
          "speaker_id": "2",
          "time": "20210823 13:05"
        },
        {
          "id": "MDRW2100009987.1.4",
          "form": "코로나로 극장, 콘서트 안간지 너무 오래됐당",
          "original_form": "코로나로 극장, 콘서트 안간지 너무 오래됐당",
          "speaker_id": "2",
          "time": "20210823 13:06"
        },
        {
          "id": "MDRW2100009987.1.5",
          "form": "조만간 영화관이라도 가고 싶은데..보고픈 영화도 없고",
          "original_form": "조만간 영화관이라도 가고 싶은데..보고픈 영화도 없고",
          "speaker_id": "2",
          "time": "20210823 13:06"
        },
        {
          "id": "MDRW2100009987.1.6",
          "form": "지난달..서촌 전시회가 가장 최근 관람한 공연이군",
          "original_form": "지난달..서촌 전시회가 가장 최근 관람한 공연이군",
          "speaker_id": "2",
          "time": "20210823 13:07"
        },
        {
          "id": "MDRW2100009987.1.7",
          "form": "지금 볼 정신? 이 없다고ㅋㅋㅋ",
          "original_form": "지금 볼 정신? 이 없다고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 14:05"
        },
        {
          "id": "MDRW2100009987.1.8",
          "form": "어머니 바쁘시니 ㅋ",
          "original_form": "어머니 바쁘시니 ㅋ",
          "speaker_id": "2",
          "time": "20210823 14:11"
        },
        {
          "id": "MDRW2100009987.1.9",
          "form": "불현듯 ㅋ2년전  너 수능 앞두고 조성진공연가서 사인받은거 생각나는군 ㅋ",
          "original_form": "불현듯 ㅋ2년전  너 수능 앞두고 조성진공연가서 사인받은거 생각나는군 ㅋ",
          "speaker_id": "2",
          "time": "20210823 14:12"
        },
        {
          "id": "MDRW2100009987.1.10",
          "form": "그런가봐요",
          "original_form": "그런가봐요",
          "speaker_id": "1",
          "time": "20210823 15:58"
        },
        {
          "id": "MDRW2100009987.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:58"
        },
        {
          "id": "MDRW2100009987.1.12",
          "form": "그때 프로그램 다 좋아하는건데ㅜ",
          "original_form": "그때 프로그램 다 좋아하는건데ㅜ",
          "speaker_id": "1",
          "time": "20210823 15:59"
        },
        {
          "id": "MDRW2100009987.1.13",
          "form": "아쉽아쉽 지금은 그닥 안좋아하는걸로 리사이틀하더라구여ㅜ",
          "original_form": "아쉽아쉽 지금은 그닥 안좋아하는걸로 리사이틀하더라구여ㅜ",
          "speaker_id": "1",
          "time": "20210823 15:59"
        },
        {
          "id": "MDRW2100009987.1.14",
          "form": "그래서 공연 안 보겠다고 하는걸수도 ㅋ",
          "original_form": "그래서 공연 안 보겠다고 하는걸수도 ㅋ",
          "speaker_id": "2",
          "time": "20210823 16:00"
        },
        {
          "id": "MDRW2100009987.1.15",
          "form": "그냥 다른거 찾아봐야겠네요",
          "original_form": "그냥 다른거 찾아봐야겠네요",
          "speaker_id": "1",
          "time": "20210823 16:00"
        },
        {
          "id": "MDRW2100009987.1.16",
          "form": "요즘은 백신맞으면 할인해주던데ㅋㅋㅋㅋ",
          "original_form": "요즘은 백신맞으면 할인해주던데ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:01"
        },
        {
          "id": "MDRW2100009987.1.17",
          "form": "아..공연도 할인되는거야,",
          "original_form": "아..공연도 할인되는거야,",
          "speaker_id": "2",
          "time": "20210823 16:01"
        },
        {
          "id": "MDRW2100009987.1.18",
          "form": "넹 한 만원정도?",
          "original_form": "넹 한 만원정도?",
          "speaker_id": "1",
          "time": "20210823 19:17"
        },
        {
          "id": "MDRW2100009987.1.19",
          "form": "밥값굳어요",
          "original_form": "밥값굳어요",
          "speaker_id": "1",
          "time": "20210823 19:17"
        },
        {
          "id": "MDRW2100009987.1.20",
          "form": "오~~좋은정보",
          "original_form": "오~~좋은정보",
          "speaker_id": "2",
          "time": "20210823 19:20"
        },
        {
          "id": "MDRW2100009987.1.21",
          "form": "요즘 쇼팽 쳐서 쇼팽 연주 듣고싶네용",
          "original_form": "요즘 쇼팽 쳐서 쇼팽 연주 듣고싶네용",
          "speaker_id": "1",
          "time": "20210824 21:38"
        },
        {
          "id": "MDRW2100009987.1.22",
          "form": "혼자 볼 생각은 없구나ㅋ",
          "original_form": "혼자 볼 생각은 없구나ㅋ",
          "speaker_id": "2",
          "time": "20210824 21:41"
        },
        {
          "id": "MDRW2100009987.1.23",
          "form": "ㅎㅎ혼자서도 볼 맘은 있어요",
          "original_form": "ㅎㅎ혼자서도 볼 맘은 있어요",
          "speaker_id": "1",
          "time": "20210824 21:46"
        }
      ]
