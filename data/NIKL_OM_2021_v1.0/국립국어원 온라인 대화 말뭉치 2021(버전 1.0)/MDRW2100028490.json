      "utterance": [
        {
          "id": "MDRW2100028490.1.1",
          "form": "완전 싫어~",
          "original_form": "완전 싫어~",
          "speaker_id": "2",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100028490.1.2",
          "form": "서류 작업 듣기만 해도 이골이 난다..",
          "original_form": "서류 작업 듣기만 해도 이골이 난다..",
          "speaker_id": "2",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100028490.1.3",
          "form": "근데 난 서류는",
          "original_form": "근데 난 서류는",
          "speaker_id": "1",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100028490.1.4",
          "form": "메뉴얼만 주면 잘 해",
          "original_form": "메뉴얼만 주면 잘 해",
          "speaker_id": "1",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100028490.1.5",
          "form": "글치 보통 메뉴얼대로 보고 따라하면 서류는 금방 끝나니까",
          "original_form": "글치 보통 메뉴얼대로 보고 따라하면 서류는 금방 끝나니까",
          "speaker_id": "2",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028490.1.6",
          "form": "없으니까 멘붕이지",
          "original_form": "없으니까 멘붕이지",
          "speaker_id": "2",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028490.1.7",
          "form": "글구 진짜 일에 가이드라인 없으니까 힘들더라",
          "original_form": "글구 진짜 일에 가이드라인 없으니까 힘들더라",
          "speaker_id": "1",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028490.1.8",
          "form": "인수인계 안하고 도망가는 사람 왜 뭐라하는지 알겠어",
          "original_form": "인수인계 안하고 도망가는 사람 왜 뭐라하는지 알겠어",
          "speaker_id": "1",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028490.1.9",
          "form": "욕 먹어도 할 말 없어 진짜",
          "original_form": "욕 먹어도 할 말 없어 진짜",
          "speaker_id": "2",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028490.1.10",
          "form": "무책임해 진심으로",
          "original_form": "무책임해 진심으로",
          "speaker_id": "2",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100028490.1.11",
          "form": "인수인계 안해도 돼",
          "original_form": "인수인계 안해도 돼",
          "speaker_id": "2",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100028490.1.12",
          "form": "다만, 자료를 확실하게 만들어놔야 인정이야",
          "original_form": "다만, 자료를 확실하게 만들어놔야 인정이야",
          "speaker_id": "2",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100028490.1.13",
          "form": "그건맞아",
          "original_form": "그건맞아",
          "speaker_id": "1",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028490.1.14",
          "form": "내가 못해주면 자료라도 있어야지",
          "original_form": "내가 못해주면 자료라도 있어야지",
          "speaker_id": "1",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028490.1.15",
          "form": "너 그래서 나올때도 고생했잖아",
          "original_form": "너 그래서 나올때도 고생했잖아",
          "speaker_id": "1",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028490.1.16",
          "form": "나는 내가 너무 탈출하고 싶으니까..",
          "original_form": "나는 내가 너무 탈출하고 싶으니까..",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028490.1.17",
          "form": "책임지고 인수인계 자료 처음부터 끝까지 만들고 가겠다...",
          "original_form": "책임지고 인수인계 자료 처음부터 끝까지 만들고 가겠다...",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028490.1.18",
          "form": "사람 안구해져서 인수인계 못해도 보고 바로 할 수 있게",
          "original_form": "사람 안구해져서 인수인계 못해도 보고 바로 할 수 있게",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028490.1.19",
          "form": "자료 만들어두겠다.......",
          "original_form": "자료 만들어두겠다.......",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028490.1.20",
          "form": "이러고 엄청 붙잡아서 스트레스 대박이었지",
          "original_form": "이러고 엄청 붙잡아서 스트레스 대박이었지",
          "speaker_id": "2",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028490.1.21",
          "form": "일을 잘해도 스트레스구나",
          "original_form": "일을 잘해도 스트레스구나",
          "speaker_id": "1",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100028490.1.22",
          "form": "마치 나와 비슷하군",
          "original_form": "마치 나와 비슷하군",
          "speaker_id": "1",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100028490.1.23",
          "form": "하지만 니가 훨씬 힘들었어",
          "original_form": "하지만 니가 훨씬 힘들었어",
          "speaker_id": "1",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100028490.1.24",
          "form": "일을 너무 잘하면 안돼..",
          "original_form": "일을 너무 잘하면 안돼..",
          "speaker_id": "2",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100028490.1.25",
          "form": "적당히 못하는 척 하고 아방하게 굴어야하는데",
          "original_form": "적당히 못하는 척 하고 아방하게 굴어야하는데",
          "speaker_id": "2",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100028490.1.26",
          "form": "그걸 못하는 타입임...",
          "original_form": "그걸 못하는 타입임...",
          "speaker_id": "2",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100028490.1.27",
          "form": "약간 일이 진도 안 나가면",
          "original_form": "약간 일이 진도 안 나가면",
          "speaker_id": "1",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100028490.1.28",
          "form": "답답해서 어쩔수없음",
          "original_form": "답답해서 어쩔수없음",
          "speaker_id": "1",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100028490.1.29",
          "form": "이래서 급해 죽겠는 사람만 손해보나봐",
          "original_form": "이래서 급해 죽겠는 사람만 손해보나봐",
          "speaker_id": "2",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100028490.1.30",
          "form": "될대로 되던가 해야하는데",
          "original_form": "될대로 되던가 해야하는데",
          "speaker_id": "2",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100028490.1.31",
          "form": "눈물이 흐르네",
          "original_form": "눈물이 흐르네",
          "speaker_id": "1",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100028490.1.32",
          "form": "급한 사람이 손해",
          "original_form": "급한 사람이 손해",
          "speaker_id": "1",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028490.1.33",
          "form": "언제나 위로하는 건 하나야",
          "original_form": "언제나 위로하는 건 하나야",
          "speaker_id": "2",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028490.1.34",
          "form": "모든 세상사 다 사람이 관여해서 굴러간다는거",
          "original_form": "모든 세상사 다 사람이 관여해서 굴러간다는거",
          "speaker_id": "2",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028490.1.35",
          "form": "결국 다 사람이 일처리 하니 얼렁뚱땅으로 지나가는구나 하는",
          "original_form": "결국 다 사람이 일처리 하니 얼렁뚱땅으로 지나가는구나 하는",
          "speaker_id": "2",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028490.1.36",
          "form": "맞아 그래도 다 어떻게 굴러가니까",
          "original_form": "맞아 그래도 다 어떻게 굴러가니까",
          "speaker_id": "1",
          "time": "20210902 20:37"
        }
      ]
    }
  ]
