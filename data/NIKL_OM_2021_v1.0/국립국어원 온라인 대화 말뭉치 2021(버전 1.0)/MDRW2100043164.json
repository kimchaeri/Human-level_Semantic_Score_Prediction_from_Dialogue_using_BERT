      "utterance": [
        {
          "id": "MDRW2100043164.1.1",
          "form": "쇼핑조아하시나요",
          "original_form": "쇼핑조아하시나요",
          "speaker_id": "1",
          "time": "20210907 15:09"
        },
        {
          "id": "MDRW2100043164.1.2",
          "form": "쇼핑좋아합니다. 오늘 해초샀어요",
          "original_form": "쇼핑좋아합니다. 오늘 해초샀어요",
          "speaker_id": "2",
          "time": "20210907 15:09"
        },
        {
          "id": "MDRW2100043164.1.3",
          "form": "ㅋㅋ저는 출근하자마자 가을니트삿어요",
          "original_form": "ㅋㅋ저는 출근하자마자 가을니트삿어요",
          "speaker_id": "1",
          "time": "20210907 15:10"
        },
        {
          "id": "MDRW2100043164.1.4",
          "form": "옼ㅋㅋㅋ 이제진짜 니트살때가왔어요",
          "original_form": "옼ㅋㅋㅋ 이제진짜 니트살때가왔어요",
          "speaker_id": "2",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100043164.1.5",
          "form": "가을옷!!!!",
          "original_form": "가을옷!!!!",
          "speaker_id": "1",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100043164.1.6",
          "form": "맞아요 ㅋㅋㅋ 가을겨울옷 맨트맨사고싶어요",
          "original_form": "맞아요 ㅋㅋㅋ 가을겨울옷 맨트맨사고싶어요",
          "speaker_id": "2",
          "time": "20210907 15:13"
        },
        {
          "id": "MDRW2100043164.1.7",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100043164.1.8",
          "form": "저두요!!!!!!!!!!",
          "original_form": "저두요!!!!!!!!!!",
          "speaker_id": "1",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100043164.1.9",
          "form": "맨투맨!!!!",
          "original_form": "맨투맨!!!!",
          "speaker_id": "1",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100043164.1.10",
          "form": "맨투맨 어디서사시나요? 쿰척..",
          "original_form": "맨투맨 어디서사시나요? 쿰척..",
          "speaker_id": "2",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043164.1.11",
          "form": "어유",
          "original_form": "어유",
          "speaker_id": "1",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043164.1.12",
          "form": "제가 여쭙고싶어유",
          "original_form": "제가 여쭙고싶어유",
          "speaker_id": "1",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043164.1.13",
          "form": "좋아하는브랜드잇으신가여",
          "original_form": "좋아하는브랜드잇으신가여",
          "speaker_id": "1",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043164.1.14",
          "form": "저는..그냥 다입습니다... 특히 회사다닐때입을 옷은 절대 비싼 거 안사요 ㅋㅋㅋ",
          "original_form": "저는..그냥 다입습니다... 특히 회사다닐때입을 옷은 절대 비싼 거 안사요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:20"
        },
        {
          "id": "MDRW2100043164.1.15",
          "form": "ㅋㅋ저는 유니폼입고 일해서 츄리닝입고 댕겨요",
          "original_form": "ㅋㅋ저는 유니폼입고 일해서 츄리닝입고 댕겨요",
          "speaker_id": "1",
          "time": "20210907 15:21"
        },
        {
          "id": "MDRW2100043164.1.16",
          "form": "아진짜요?ㅋㅋㅋ 유니폼완전편하죠",
          "original_form": "아진짜요?ㅋㅋㅋ 유니폼완전편하죠",
          "speaker_id": "2",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100043164.1.17",
          "form": "네ㅋㅋ그래서 넘 좋아요",
          "original_form": "네ㅋㅋ그래서 넘 좋아요",
          "speaker_id": "1",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100043164.1.18",
          "form": "저도유니폼입고싶어요",
          "original_form": "저도유니폼입고싶어요",
          "speaker_id": "2",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100043164.1.19",
          "form": "출근복장 고민안해도되서 겁나 편해요",
