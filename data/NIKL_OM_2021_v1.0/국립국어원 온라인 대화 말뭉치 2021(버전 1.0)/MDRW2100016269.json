      "utterance": [
        {
          "id": "MDRW2100016269.1.1",
          "form": "여행좋아하셔요?",
          "original_form": "여행좋아하셔요?",
          "speaker_id": "1",
          "time": "20210826 11:34"
        },
        {
          "id": "MDRW2100016269.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210826 11:37"
        },
        {
          "id": "MDRW2100016269.1.3",
          "form": "제주도 가고싶네요",
          "original_form": "제주도 가고싶네요",
          "speaker_id": "2",
          "time": "20210826 11:37"
        },
        {
          "id": "MDRW2100016269.1.4",
          "form": "요즘은 제주도가 최선이죠ㅠ",
          "original_form": "요즘은 제주도가 최선이죠ㅠ",
          "speaker_id": "1",
          "time": "20210826 11:38"
        },
        {
          "id": "MDRW2100016269.1.5",
          "form": "공항만 가도 기분이좋죠",
          "original_form": "공항만 가도 기분이좋죠",
          "speaker_id": "2",
          "time": "20210826 11:39"
        },
        {
          "id": "MDRW2100016269.1.6",
          "form": "맞아요 멀리 비행기만 봐도요",
          "original_form": "맞아요 멀리 비행기만 봐도요",
          "speaker_id": "1",
          "time": "20210826 11:48"
        },
        {
          "id": "MDRW2100016269.1.7",
          "form": "계획은 세우시는 편인가요?",
          "original_form": "계획은 세우시는 편인가요?",
          "speaker_id": "2",
          "time": "20210826 11:58"
        },
        {
          "id": "MDRW2100016269.1.8",
          "form": "전 사실 즉흥이 좋아요",
          "original_form": "전 사실 즉흥이 좋아요",
          "speaker_id": "1",
          "time": "20210826 12:06"
        },
        {
          "id": "MDRW2100016269.1.9",
          "form": "자주 즉흥적으로 떠나셨나요?",
          "original_form": "자주 즉흥적으로 떠나셨나요?",
          "speaker_id": "2",
          "time": "20210826 12:09"
        },
        {
          "id": "MDRW2100016269.1.10",
          "form": "날짜와장소는 계획이고 가서는 자유롭게 다녀요",
          "original_form": "날짜와장소는 계획이고 가서는 자유롭게 다녀요",
          "speaker_id": "1",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016269.1.11",
          "form": "기억남는게 있으시나요?",
          "original_form": "기억남는게 있으시나요?",
          "speaker_id": "2",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016269.1.12",
          "form": "여행지 야경도 좋아해요",
          "original_form": "여행지 야경도 좋아해요",
          "speaker_id": "1",
          "time": "20210826 12:11"
        },
        {
          "id": "MDRW2100016269.1.13",
          "form": "어디가 좋았나요?",
          "original_form": "어디가 좋았나요?",
          "speaker_id": "2",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100016269.1.14",
          "form": "지금 하나 꼽으라고 하니까 생각이 잘 안나네요 ㅋㅋ 그래도 대부분 다 좋았던 기억이라",
          "original_form": "지금 하나 꼽으라고 하니까 생각이 잘 안나네요 ㅋㅋ 그래도 대부분 다 좋았던 기억이라",
