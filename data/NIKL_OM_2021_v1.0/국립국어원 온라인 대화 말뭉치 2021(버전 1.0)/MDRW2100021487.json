      "utterance": [
        {
          "id": "MDRW2100021487.1.1",
          "form": "인생영화 있으세요?",
          "original_form": "인생영화 있으세요?",
          "speaker_id": "1",
          "time": "20210828 19:31"
        },
        {
          "id": "MDRW2100021487.1.2",
          "form": "답이 늦었습니다",
          "original_form": "답이 늦었습니다",
          "speaker_id": "2",
          "time": "20210829 07:42"
        },
        {
          "id": "MDRW2100021487.1.3",
          "form": "저 코코 좋아합니다",
          "original_form": "저 코코 좋아합니다",
          "speaker_id": "2",
          "time": "20210829 07:42"
        },
        {
          "id": "MDRW2100021487.1.4",
          "form": "코코 예전에 친구랑 심야영화로 봤었네요",
          "original_form": "코코 예전에 친구랑 심야영화로 봤었네요",
          "speaker_id": "1",
          "time": "20210829 10:18"
        },
        {
          "id": "MDRW2100021487.1.5",
          "form": "어떠셨나요",
          "original_form": "어떠셨나요",
          "speaker_id": "2",
          "time": "20210829 10:32"
        },
        {
          "id": "MDRW2100021487.1.6",
          "form": "되게 화려하고 후반쯤에 오열했어요ㅠ",
          "original_form": "되게 화려하고 후반쯤에 오열했어요ㅠ",
          "speaker_id": "1",
          "time": "20210829 12:15"
        },
        {
          "id": "MDRW2100021487.1.7",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 13:24"
        },
        {
          "id": "MDRW2100021487.1.8",
          "form": "처음에 보면 이게 뭐야 유치하네 ㅎ 했다가 우는 저의 모습을 발겻",
          "original_form": "처음에 보면 이게 뭐야 유치하네 ㅎ 했다가 우는 저의 모습을 발겻",
          "speaker_id": "2",
          "time": "20210829 13:24"
        },
        {
          "id": "MDRW2100021487.1.9",
          "form": "리멤버미~",
          "original_form": "리멤버미~",
          "speaker_id": "2",
          "time": "20210829 13:24"
        },
        {
          "id": "MDRW2100021487.1.10",
          "form": "어떤 장면이 좋았나요",
          "original_form": "어떤 장면이 좋았나요",
          "speaker_id": "2",
          "time": "20210829 13:24"
        },
        {
          "id": "MDRW2100021487.1.11",
          "form": "후반부에 아빠와 대화하는 부분이었나?? 그부분이 인상깊어요ㅠ 너무 슬펐어서ㅠㅠ",
          "original_form": "후반부에 아빠와 대화하는 부분이었나?? 그부분이 인상깊어요ㅠ 너무 슬펐어서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 13:37"
        },
        {
          "id": "MDRW2100021487.1.12",
          "form": "저도 그 부분이 인상적이었어오ㅡㅠㅠ",
          "original_form": "저도 그 부분이 인상적이었어오ㅡㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 13:38"
        },
        {
          "id": "MDRW2100021487.1.13",
          "form": "다시생각해도 눈물이..",
          "original_form": "다시생각해도 눈물이..",
          "speaker_id": "2",
          "time": "20210829 13:38"
        },
        {
          "id": "MDRW2100021487.1.14",
          "form": "name1의 인생영화는 무엇이었나요",
          "original_form": "&name1&의 인생영화는 무엇이었나요",
          "speaker_id": "2",
          "time": "20210829 13:38"
        },
        {
          "id": "MDRW2100021487.1.15",
          "form": "저는 다크나이트랑 중경삼림이요",
          "original_form": "저는 다크나이트랑 중경삼림이요",
          "speaker_id": "1",
          "time": "20210829 13:44"
        },
        {
          "id": "MDRW2100021487.1.16",
          "form": "다크나이트는 어떤점이 인상적이었나요",
          "original_form": "다크나이트는 어떤점이 인상적이었나요",
          "speaker_id": "2",
          "time": "20210829 13:45"
        },
        {
          "id": "MDRW2100021487.1.17",
          "form": "일단 화면연출이 세련된게 제 취향이고요 조커의 악역 연기가 멋지죠",
          "original_form": "일단 화면연출이 세련된게 제 취향이고요 조커의 악역 연기가 멋지죠",
          "speaker_id": "1",
          "time": "20210829 13:55"
        },
        {
          "id": "MDRW2100021487.1.18",
          "form": "오 아무래도 조커가 임팩트있죠",
          "original_form": "오 아무래도 조커가 임팩트있죠",
          "speaker_id": "2",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100021487.1.19",
          "form": "아무래도 계단내려오는 그 장면 가장 인상적이었어요",
          "original_form": "아무래도 계단내려오는 그 장면 가장 인상적이었어요",
          "speaker_id": "2",
          "time": "20210829 14:12"
        },
        {
          "id": "MDRW2100021487.1.20",
          "form": "영화 조커도 보셧나요?",
          "original_form": "영화 조커도 보셧나요?",
          "speaker_id": "1",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100021487.1.21",
          "form": "네 조커좋아해요",
          "original_form": "네 조커좋아해요",
          "speaker_id": "2",
          "time": "20210829 14:15"
        }
      ]
