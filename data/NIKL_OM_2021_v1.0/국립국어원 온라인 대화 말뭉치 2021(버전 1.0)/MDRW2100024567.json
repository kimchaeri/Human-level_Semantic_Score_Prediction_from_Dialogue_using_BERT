      "utterance": [
        {
          "id": "MDRW2100024567.1.1",
          "form": "콜라보 제품 구매해보셨나요?",
          "original_form": "콜라보 제품 구매해보셨나요?",
          "speaker_id": "1",
          "time": "20210831 14:21"
        },
        {
          "id": "MDRW2100024567.1.2",
          "form": "박하스 젤리 먹어봤어요",
          "original_form": "박하스 젤리 먹어봤어요",
          "speaker_id": "2",
          "time": "20210831 14:27"
        },
        {
          "id": "MDRW2100024567.1.3",
          "form": "와우 대박...어떠셨나요",
          "original_form": "와우 대박...어떠셨나요",
          "speaker_id": "1",
          "time": "20210831 14:32"
        },
        {
          "id": "MDRW2100024567.1.4",
          "form": "진짜 그냥 박하스 맛이에요ㅋㅋ",
          "original_form": "진짜 그냥 박하스 맛이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:38"
        },
        {
          "id": "MDRW2100024567.1.5",
          "form": "크..넘 신기해요 곰표는요?",
          "original_form": "크..넘 신기해요 곰표는요?",
          "speaker_id": "1",
          "time": "20210831 14:47"
        },
        {
          "id": "MDRW2100024567.1.6",
          "form": "곰표는 사본게 없네요",
          "original_form": "곰표는 사본게 없네요",
          "speaker_id": "2",
          "time": "20210831 14:49"
        },
        {
          "id": "MDRW2100024567.1.7",
          "form": "대부분 맥주 얘기하시던데 제가 워낙 술찌라...",
          "original_form": "대부분 맥주 얘기하시던데 제가 워낙 술찌라...",
          "speaker_id": "2",
          "time": "20210831 14:49"
        },
        {
          "id": "MDRW2100024567.1.8",
          "form": "저는 아이스크림 먹어봤어요 ㅎ",
          "original_form": "저는 아이스크림 먹어봤어요 ㅎ",
          "speaker_id": "1",
          "time": "20210831 14:49"
        },
        {
          "id": "MDRW2100024567.1.9",
          "form": "아이스크림은 처음 들어봐요",
          "original_form": "아이스크림은 처음 들어봐요",
          "speaker_id": "2",
          "time": "20210831 14:50"
        },
        {
          "id": "MDRW2100024567.1.10",
          "form": "네 근데 재구매는 안할듯요 ㅎ",
          "original_form": "네 근데 재구매는 안할듯요 ㅎ",
          "speaker_id": "1",
          "time": "20210831 14:51"
        },
        {
          "id": "MDRW2100024567.1.11",
          "form": "아하... 맛이 없으셨나요...",
          "original_form": "아하... 맛이 없으셨나요...",
          "speaker_id": "2",
          "time": "20210831 14:51"
        },
        {
          "id": "MDRW2100024567.1.12",
          "form": "네 느끼하고 눅눅했어요 ㅠ",
          "original_form": "네 느끼하고 눅눅했어요 ㅠ",
          "speaker_id": "1",
          "time": "20210831 14:52"
        },
        {
          "id": "MDRW2100024567.1.13",
          "form": "ㅠ 왜 그렇게 만들었을까요...",
          "original_form": "ㅠ 왜 그렇게 만들었을까요...",
          "speaker_id": "2",
          "time": "20210831 14:53"
        },
        {
          "id": "MDRW2100024567.1.14",
          "form": "제 입맛에만 그랬을지도용.. ㅋ",
          "original_form": "제 입맛에만 그랬을지도용.. ㅋ",
          "speaker_id": "1",
          "time": "20210831 14:54"
        },
        {
          "id": "MDRW2100024567.1.15",
          "form": "근처에서 파는 것도 본적이 없으니 맛 없는게 맞을거예요",
          "original_form": "근처에서 파는 것도 본적이 없으니 맛 없는게 맞을거예요",
          "speaker_id": "2",
          "time": "20210831 14:58"
        },
        {
          "id": "MDRW2100024567.1.16",
