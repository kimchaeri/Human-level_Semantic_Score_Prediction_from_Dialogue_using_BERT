      "utterance": [
        {
          "id": "MDRW2100002406.1.1",
          "form": "이번 올림픽 보셨나요?",
          "original_form": "이번 올림픽 보셨나요?",
          "speaker_id": "1",
          "time": "20210820 11:18"
        },
        {
          "id": "MDRW2100002406.1.2",
          "form": "봤어요!",
          "original_form": "봤어요!",
          "speaker_id": "2",
          "time": "20210820 11:40"
        },
        {
          "id": "MDRW2100002406.1.3",
          "form": "양궁이랑 배구 진짜 최고였어요",
          "original_form": "양궁이랑 배구 진짜 최고였어요",
          "speaker_id": "2",
          "time": "20210820 11:40"
        },
        {
          "id": "MDRW2100002406.1.4",
          "form": "어떤 종목 재미있게 보셨어요?",
          "original_form": "어떤 종목 재미있게 보셨어요?",
          "speaker_id": "2",
          "time": "20210820 11:41"
        },
        {
          "id": "MDRW2100002406.1.5",
          "form": "저도 양궁이랑 배구요",
          "original_form": "저도 양궁이랑 배구요",
          "speaker_id": "1",
          "time": "20210820 11:41"
        },
        {
          "id": "MDRW2100002406.1.6",
          "form": "특히 배구는 매순간이 감동이었어요",
          "original_form": "특히 배구는 매순간이 감동이었어요",
          "speaker_id": "1",
          "time": "20210820 11:41"
        },
        {
          "id": "MDRW2100002406.1.7",
          "form": "진짜 선수들 최선을 다해서 싸워준게 너무 고마웠고 좋았어요",
          "original_form": "진짜 선수들 최선을 다해서 싸워준게 너무 고마웠고 좋았어요",
          "speaker_id": "1",
          "time": "20210820 11:41"
        },
        {
          "id": "MDRW2100002406.1.8",
          "form": "배구 진짜 최고였어요",
          "original_form": "배구 진짜 최고였어요",
          "speaker_id": "2",
          "time": "20210820 11:41"
        },
        {
          "id": "MDRW2100002406.1.9",
          "form": "공중파 중계도 안해줘서 다른데서 봤는데",
          "original_form": "공중파 중계도 안해줘서 다른데서 봤는데",
          "speaker_id": "2",
          "time": "20210820 11:42"
        },
        {
          "id": "MDRW2100002406.1.10",
          "form": "한일전 너무 재미있게 봤어요 저는",
          "original_form": "한일전 너무 재미있게 봤어요 저는",
          "speaker_id": "2",
          "time": "20210820 11:42"
        },
        {
          "id": "MDRW2100002406.1.11",
          "form": "맞아요ㅡㅡ 어떻게 공중파를 안해줄 수 있는지",
          "original_form": "맞아요ㅡㅡ 어떻게 공중파를 안해줄 수 있는지",
          "speaker_id": "1",
          "time": "20210820 11:42"
        },
        {
          "id": "MDRW2100002406.1.12",
          "form": "그 때 축구했던가 그랬잖아요",
          "original_form": "그 때 축구했던가 그랬잖아요",
          "speaker_id": "1",
          "time": "20210820 11:42"
        },
        {
          "id": "MDRW2100002406.1.13",
          "form": "하 야구랑 축구는 다 처참하게 지고",
          "original_form": "하 야구랑 축구는 다 처참하게 지고",
          "speaker_id": "2",
          "time": "20210820 11:42"
        },
        {
          "id": "MDRW2100002406.1.14",
          "form": "맞아요!!!!",
          "original_form": "맞아요!!!!",
          "speaker_id": "2",
          "time": "20210820 11:42"
        },
        {
          "id": "MDRW2100002406.1.15",
          "form": "배구가 훨씬 재밌었는데!!!!",
          "original_form": "배구가 훨씬 재밌었는데!!!!",
          "speaker_id": "1",
          "time": "20210820 11:42"
        },
        {
          "id": "MDRW2100002406.1.16",
          "form": "야구  축구도 잠깐씩 봤는데 속터지고..",
          "original_form": "야구  축구도 잠깐씩 봤는데 속터지고..",
          "speaker_id": "2",
          "time": "20210820 11:42"
        },
        {
          "id": "MDRW2100002406.1.17",
          "form": "배구 보길 진짜 잘했어요",
          "original_form": "배구 보길 진짜 잘했어요",
          "speaker_id": "2",
          "time": "20210820 11:42"
        },
        {
          "id": "MDRW2100002406.1.18",
          "form": "진짜 공중파들 양심도 없지!!",
          "original_form": "진짜 공중파들 양심도 없지!!",
          "speaker_id": "1",
          "time": "20210820 11:42"
        },
        {
          "id": "MDRW2100002406.1.19",
          "form": "근데 너무 떨리지 않았어요? ㅌㅌㅋㅋㅋㅋ",
          "original_form": "근데 너무 떨리지 않았어요? ㅌㅌㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 11:43"
        },
        {
          "id": "MDRW2100002406.1.20",
          "form": "저 손 모으고 봤잖아요",
          "original_form": "저 손 모으고 봤잖아요",
          "speaker_id": "1",
          "time": "20210820 11:43"
        },
        {
          "id": "MDRW2100002406.1.21",
          "form": "저 양궁보는데도 제가 안산선수보다 더 떨었어요..",
          "original_form": "저 양궁보는데도 제가 안산선수보다 더 떨었어요..",
          "speaker_id": "2",
          "time": "20210820 11:43"
        },
        {
          "id": "MDRW2100002406.1.22",
          "form": "양궁도 진짜 제발 제발 하면서 보고",
          "original_form": "양궁도 진짜 제발 제발 하면서 보고",
          "speaker_id": "1",
          "time": "20210820 11:43"
        },
        {
          "id": "MDRW2100002406.1.23",
          "form": "양궁 너무 재밌어요. 나중에 양궁카페도 가보려고요",
          "original_form": "양궁 너무 재밌어요. 나중에 양궁카페도 가보려고요",
          "speaker_id": "2",
          "time": "20210820 11:43"
        },
        {
          "id": "MDRW2100002406.1.24",
          "form": "진짜 선수들 너무 멋있어요ㅠㅠ",
          "original_form": "진짜 선수들 너무 멋있어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 11:43"
        },
        {
          "id": "MDRW2100002406.1.25",
          "form": "오죽하면 상대팀 쏠 때 선풍기틀었잖아요",
          "original_form": "오죽하면 상대팀 쏠 때 선풍기틀었잖아요",
          "speaker_id": "1",
          "time": "20210820 11:43"
        }
      ]
    }
