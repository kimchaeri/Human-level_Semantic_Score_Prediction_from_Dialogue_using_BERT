      "utterance": [
        {
          "id": "MDRW2100015629.1.1",
          "form": "요즘 온라인 피트니스가 핫하대",
          "original_form": "요즘 온라인 피트니스가 핫하대",
          "speaker_id": "2",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100015629.1.2",
          "form": "요즘 유튜브로 운동 많이하더라!",
          "original_form": "요즘 유튜브로 운동 많이하더라!",
          "speaker_id": "1",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100015629.1.3",
          "form": "나도 요즘 해",
          "original_form": "나도 요즘 해",
          "speaker_id": "2",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100015629.1.4",
          "form": "어떤 유투버 주로 봐?",
          "original_form": "어떤 유투버 주로 봐?",
          "speaker_id": "1",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100015629.1.5",
          "form": "나는 주로 기구없이 맨몸운동을 주로하는편이거든",
          "original_form": "나는 주로 기구없이 맨몸운동을 주로하는편이거든",
          "speaker_id": "2",
          "time": "20210825 23:53"
        },
        {
          "id": "MDRW2100015629.1.6",
          "form": "오 그럼 유튜브로 운동하기 적합하다",
          "original_form": "오 그럼 유튜브로 운동하기 적합하다",
          "speaker_id": "1",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100015629.1.7",
          "form": "그치? 빅씨스라는 유튜버 추천할게",
          "original_form": "그치? 빅씨스라는 유튜버 추천할게",
          "speaker_id": "2",
          "time": "20210825 23:54"
        },
        {
          "id": "MDRW2100015629.1.8",
          "form": "오 고마워 나도 유튜브 보면서 운동해봐야겠다",
          "original_form": "오 고마워 나도 유튜브 보면서 운동해봐야겠다",
          "speaker_id": "1",
          "time": "20210825 23:55"
        },
        {
          "id": "MDRW2100015629.1.9",
          "form": "그 사람 영상 특이한 점이있어",
          "original_form": "그 사람 영상 특이한 점이있어",
          "speaker_id": "2",
          "time": "20210825 23:55"
        },
        {
          "id": "MDRW2100015629.1.10",
          "form": "빅씨스란 유튜버가? 뭐야?",
          "original_form": "빅씨스란 유튜버가? 뭐야?",
          "speaker_id": "1",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100015629.1.11",
          "form": "운동을 집에서 하는데 배경이 무척 이뻐",
          "original_form": "운동을 집에서 하는데 배경이 무척 이뻐",
          "speaker_id": "2",
          "time": "20210825 23:56"
        },
        {
          "id": "MDRW2100015629.1.12",
          "form": "오 운동할 맛 나겠다",
          "original_form": "오 운동할 맛 나겠다",
          "speaker_id": "1",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015629.1.13",
          "form": "그리고 노래를 틀어놓고 운동할수있어서 온라인 피트니스가 좋아",
          "original_form": "그리고 노래를 틀어놓고 운동할수있어서 온라인 피트니스가 좋아",
          "speaker_id": "2",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015629.1.14",
          "form": "맞아 좋아하는 노래 들으면서 운동할수 있는건 진짜 좋다",
          "original_form": "맞아 좋아하는 노래 들으면서 운동할수 있는건 진짜 좋다",
          "speaker_id": "1",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015629.1.15",
          "form": "편하고 참 좋아",
          "original_form": "편하고 참 좋아",
          "speaker_id": "2",
          "time": "20210825 23:59"
        }
      ]
