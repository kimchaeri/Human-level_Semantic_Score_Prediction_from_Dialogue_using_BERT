      "utterance": [
        {
          "id": "MDRW2100025488.1.1",
          "form": "음료는 멀좋아하시나요?",
          "original_form": "음료는 멀좋아하시나요?",
          "speaker_id": "2",
          "time": "20210901 12:13"
        },
        {
          "id": "MDRW2100025488.1.2",
          "form": "저는 아침햇살 좋아해요",
          "original_form": "저는 아침햇살 좋아해요",
          "speaker_id": "1",
          "time": "20210901 12:15"
        },
        {
          "id": "MDRW2100025488.1.3",
          "form": "어떤거 좋아하세요?",
          "original_form": "어떤거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 12:16"
        },
        {
          "id": "MDRW2100025488.1.4",
          "form": "저는 알로애나",
          "original_form": "저는 알로애나",
          "speaker_id": "2",
          "time": "20210901 12:17"
        },
        {
          "id": "MDRW2100025488.1.5",
          "form": "포카리??",
          "original_form": "포카리??",
          "speaker_id": "2",
          "time": "20210901 12:17"
        },
        {
          "id": "MDRW2100025488.1.6",
          "form": "이온음료가 조터라구요!",
          "original_form": "이온음료가 조터라구요!",
          "speaker_id": "2",
          "time": "20210901 12:17"
        },
        {
          "id": "MDRW2100025488.1.7",
          "form": "레몬 녹차도 좋아해요",
          "original_form": "레몬 녹차도 좋아해요",
          "speaker_id": "2",
          "time": "20210901 12:18"
        },
        {
          "id": "MDRW2100025488.1.8",
          "form": "탄산은 좋아하세요?",
          "original_form": "탄산은 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 12:18"
        },
        {
          "id": "MDRW2100025488.1.9",
          "form": "좋아하긴하는데",
          "original_form": "좋아하긴하는데",
          "speaker_id": "2",
          "time": "20210901 12:18"
        },
        {
          "id": "MDRW2100025488.1.10",
          "form": "가끔먹어요살찔까바...ㅎ",
          "original_form": "가끔먹어요살찔까바...ㅎ",
          "speaker_id": "2",
          "time": "20210901 12:18"
        },
        {
          "id": "MDRW2100025488.1.11",
          "form": "제로칼로리 요즘 많이 나오던데 걱정되시면 그건 어때요?",
          "original_form": "제로칼로리 요즘 많이 나오던데 걱정되시면 그건 어때요?",
          "speaker_id": "1",
          "time": "20210901 12:19"
        },
        {
          "id": "MDRW2100025488.1.12",
          "form": "맞아요 그런거 조타하더라구요 근데 찾아먹을만큼좋아하는건아니라서요!",
          "original_form": "맞아요 그런거 조타하더라구요 근데 찾아먹을만큼좋아하는건아니라서요!",
          "speaker_id": "2",
          "time": "20210901 12:50"
        },
        {
          "id": "MDRW2100025488.1.13",
          "form": "그런거보다 코코팜 알로애 이런거찾아먹어요!",
          "original_form": "그런거보다 코코팜 알로애 이런거찾아먹어요!",
          "speaker_id": "2",
          "time": "20210901 12:50"
        },
        {
          "id": "MDRW2100025488.1.14",
          "form": "아하! 코코팜 저는 젤리만들때 자주 썼어요",
          "original_form": "아하! 코코팜 저는 젤리만들때 자주 썼어요",
          "speaker_id": "1",
          "time": "20210901 13:55"
        },
        {
          "id": "MDRW2100025488.1.15",
          "form": "요리 잘하세요?",
          "original_form": "요리 잘하세요?",
          "speaker_id": "2",
          "time": "20210901 13:57"
        },
        {
          "id": "MDRW2100025488.1.16",
          "form": "저는 똥손이거든요 ㅠ",
          "original_form": "저는 똥손이거든요 ㅠ",
          "speaker_id": "2",
          "time": "20210901 13:57"
        },
        {
          "id": "MDRW2100025488.1.17",
          "form": "저는 자취용정도 해요",
          "original_form": "저는 자취용정도 해요",
          "speaker_id": "1",
          "time": "20210901 14:21"
        },
        {
          "id": "MDRW2100025488.1.18",
          "form": "딱 먹고살수만있게....",
          "original_form": "딱 먹고살수만있게....",
          "speaker_id": "1",
          "time": "20210901 14:21"
        },
        {
          "id": "MDRW2100025488.1.19",
          "form": "아 혼자 사시는구나",
          "original_form": "아 혼자 사시는구나",
          "speaker_id": "2",
          "time": "20210901 14:49"
        },
        {
          "id": "MDRW2100025488.1.20",
          "form": "그래도 하시는게어딘가요!",
          "original_form": "그래도 하시는게어딘가요!",
          "speaker_id": "2",
          "time": "20210901 14:49"
        },
        {
          "id": "MDRW2100025488.1.21",
          "form": "그렇긴하죠! 소스와 조미료 백종원만있다면...",
          "original_form": "그렇긴하죠! 소스와 조미료 백종원만있다면...",
          "speaker_id": "1",
          "time": "20210901 15:12"
        },
        {
          "id": "MDRW2100025488.1.22",
          "form": "충분합니다",
          "original_form": "충분합니다",
          "speaker_id": "1",
          "time": "20210901 15:12"
        },
        {
          "id": "MDRW2100025488.1.23",
          "form": "백종원이 짱이깅하네요 ㅋㅋㅋ",
          "original_form": "백종원이 짱이깅하네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:16"
        }
      ]
    }
