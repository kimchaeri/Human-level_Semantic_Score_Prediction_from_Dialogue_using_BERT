      "utterance": [
        {
          "id": "MDRW2100039429.1.1",
          "form": "토끼 키우고싶어",
          "original_form": "토끼 키우고싶어",
          "speaker_id": "2",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100039429.1.2",
          "form": "개키우고싶어",
          "original_form": "개키우고싶어",
          "speaker_id": "1",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100039429.1.3",
          "form": "난 요즘 개는 별론 거 같아",
          "original_form": "난 요즘 개는 별론 거 같아",
          "speaker_id": "2",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100039429.1.4",
          "form": "나의 프라이버시 존중이 없어(?)",
          "original_form": "나의 프라이버시 존중이 없어(?)",
          "speaker_id": "2",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100039429.1.5",
          "form": "어째서... 개가 얼마나 사랑스러운데요",
          "original_form": "어째서... 개가 얼마나 사랑스러운데요",
          "speaker_id": "1",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100039429.1.6",
          "form": "그점이 귀여운거야ㅠㅠ 개가 꼬랑지흔드는거 짱귀여운데",
          "original_form": "그점이 귀여운거야ㅠㅠ 개가 꼬랑지흔드는거 짱귀여운데",
          "speaker_id": "1",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100039429.1.7",
          "form": "전 저한테 집착하는 다정광공견보단",
          "original_form": "전 저한테 집착하는 다정광공견보단",
          "speaker_id": "2",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100039429.1.8",
          "form": "저한테 관심없는 싸가지묘가 더 끌리는 거 같아요",
          "original_form": "저한테 관심없는 싸가지묘가 더 끌리는 거 같아요",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039429.1.9",
          "form": "귀여운 강아지를 광공같은 수식어 붙이지 말아주새요",
          "original_form": "귀여운 강아지를 광공같은 수식어 붙이지 말아주새요",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039429.1.10",
          "form": "근데 고양이 알러지있음",
          "original_form": "근데 고양이 알러지있음",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039429.1.11",
          "form": "토끼는 없구?",
          "original_form": "토끼는 없구?",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039429.1.12",
          "form": "토끼도 묘자를 써요",
          "original_form": "토끼도 묘자를 써요",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039429.1.13",
          "form": "자축인묘",
          "original_form": "자축인묘",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039429.1.14",
          "form": "글쵸 토끼..전 너무 키우기 힘들어보여서 별루",
          "original_form": "글쵸 토끼..전 너무 키우기 힘들어보여서 별루",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039429.1.15",
          "form": "토끼 알러지는 업냐는 얘기엿구나",
          "original_form": "토끼 알러지는 업냐는 얘기엿구나",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100039429.1.16",
          "form": "토끼알러지검사는 녹십자에서만 가능하대서 못받아봣어",
          "original_form": "토끼알러지검사는 녹십자에서만 가능하대서 못받아봣어",
          "speaker_id": "2",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100039429.1.17",
          "form": "키우기 어렵긴한데",
          "original_form": "키우기 어렵긴한데",
          "speaker_id": "2",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100039429.1.18",
          "form": "러블리",
          "original_form": "러블리",
          "speaker_id": "2",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100039429.1.19",
          "form": "아니 왜 녹십자에서만..? 의외다 녹십자가 토끼지분있나",
          "original_form": "아니 왜 녹십자에서만..? 의외다 녹십자가 토끼지분있나",
          "speaker_id": "1",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100039429.1.20",
          "form": "정확히는 녹십자에서 나오는 검사키트에만 토끼가 잇다함",
          "original_form": "정확히는 녹십자에서 나오는 검사키트에만 토끼가 잇다함",
          "speaker_id": "2",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100039429.1.21",
          "form": "토끼 근데 개보단 키우기 쉬울걸요 산책도 필요없고",
          "original_form": "토끼 근데 개보단 키우기 쉬울걸요 산책도 필요없고",
          "speaker_id": "2",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100039429.1.22",
          "form": "먹이도 풀만 먹고... 물도 알아서 먹고...",
          "original_form": "먹이도 풀만 먹고... 물도 알아서 먹고...",
          "speaker_id": "2",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100039429.1.23",
          "form": "신기하네요 토끼특허라도 있나",
          "original_form": "신기하네요 토끼특허라도 있나",
          "speaker_id": "1",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100039429.1.24",
          "form": "쉬야도 크면서 잘 가리고...",
          "original_form": "쉬야도 크면서 잘 가리고...",
          "speaker_id": "2",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100039429.1.25",
          "form": "끙아는 코코볼이구",
          "original_form": "끙아는 코코볼이구",
          "speaker_id": "2",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100039429.1.26",
          "form": "개도 쉬 잘 가려... 가르치면 돼...",
          "original_form": "개도 쉬 잘 가려... 가르치면 돼...",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100039429.1.27",
          "form": "산책가야하자나요",
          "original_form": "산책가야하자나요",
          "speaker_id": "2",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100039429.1.28",
          "form": "체력도 조아서 터그놀이같은거 해조야하구",
          "original_form": "체력도 조아서 터그놀이같은거 해조야하구",
          "speaker_id": "2",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100039429.1.29",
          "form": "가면 되죠 주인 건강에도 좋아",
          "original_form": "가면 되죠 주인 건강에도 좋아",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100039429.1.30",
          "form": "톡기가 힘든 점은",
          "original_form": "톡기가 힘든 점은",
          "speaker_id": "2",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100039429.1.31",
          "form": "터레기뿐이야!!!",
          "original_form": "터레기뿐이야!!!",
          "speaker_id": "2",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100039429.1.32",
          "form": "귀엽잖아요 터그놀이 얼마나 기여어",
          "original_form": "귀엽잖아요 터그놀이 얼마나 기여어",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100039429.1.33",
          "form": "전 쬐깐한게 성질부리면 싫을거같아서...",
          "original_form": "전 쬐깐한게 성질부리면 싫을거같아서...",
          "speaker_id": "1",
          "time": "20210903 17:37"
        },
        {
          "id": "MDRW2100039429.1.34",
          "form": "산책을 애초에 장점이라구 생각해서 그런가... 운동부족은 없을 느낌",
          "original_form": "산책을 애초에 장점이라구 생각해서 그런가... 운동부족은 없을 느낌",
          "speaker_id": "1",
          "time": "20210903 17:37"
        },
        {
          "id": "MDRW2100039429.1.35",
          "form": "같이 놀고싶은사람과 거리가 필요한 사람의 반려동물 취향차이",
          "original_form": "같이 놀고싶은사람과 거리가 필요한 사람의 반려동물 취향차이",
          "speaker_id": "1",
          "time": "20210903 17:38"
        }
      ]
    }
