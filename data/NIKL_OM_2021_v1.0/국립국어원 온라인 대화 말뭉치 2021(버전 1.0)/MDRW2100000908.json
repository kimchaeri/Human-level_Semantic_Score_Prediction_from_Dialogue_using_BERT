      "utterance": [
        {
          "id": "MDRW2100000908.1.1",
          "form": "보이나..? 나 머리 자르고싶다고",
          "original_form": "보이나..? 나 머리 자르고싶다고",
          "speaker_id": "1",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000908.1.2",
          "form": "알지알지",
          "original_form": "알지알지",
          "speaker_id": "2",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000908.1.3",
          "form": "뭘 알아ㅠㅋㅋㅋㅋㅋ",
          "original_form": "뭘 알아ㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000908.1.4",
          "form": "머리 길면 거추장스러워서",
          "original_form": "머리 길면 거추장스러워서",
          "speaker_id": "2",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000908.1.5",
          "form": "자르고싶은데 ㅠ",
          "original_form": "자르고싶은데 ㅠ",
          "speaker_id": "2",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000908.1.6",
          "form": "맞아ㅠ 근데 겨울엔 긴머리해여돼",
          "original_form": "맞아ㅠ 근데 겨울엔 긴머리해여돼",
          "speaker_id": "1",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000908.1.7",
          "form": "거울보면 긴머리 예쁘더라",
          "original_form": "거울보면 긴머리 예쁘더라",
          "speaker_id": "2",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000908.1.8",
          "form": "추우니까..",
          "original_form": "추우니까..",
          "speaker_id": "1",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000908.1.9",
          "form": "맞아 목시려",
          "original_form": "맞아 목시려",
          "speaker_id": "2",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000908.1.10",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000908.1.11",
          "form": "파마도 하고싶은데",
          "original_form": "파마도 하고싶은데",
          "speaker_id": "2",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000908.1.12",
          "form": "파마도 하고싶은데 비싸고 구ㅏ찮아",
          "original_form": "파마도 하고싶은데 비싸고 구ㅏ찮아",
          "speaker_id": "1",
          "time": "20210819 20:33"
        },
        {
          "id": "MDRW2100000908.1.13",
          "form": "에 나랑 똑같네",
          "original_form": "에 나랑 똑같네",
          "speaker_id": "1",
          "time": "20210819 20:33"
        },
        {
          "id": "MDRW2100000908.1.14",
          "form": "그니까 그래도 염색보다는",
          "original_form": "그니까 그래도 염색보다는",
          "speaker_id": "2",
          "time": "20210819 20:33"
        },
        {
          "id": "MDRW2100000908.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ관리하기 편해서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ관리하기 편해서",
          "speaker_id": "2",
          "time": "20210819 20:33"
        },
        {
          "id": "MDRW2100000908.1.16",
          "form": "염색 나 셀프로하는데 그 색 좀 지겹다",
          "original_form": "염색 나 셀프로하는데 그 색 좀 지겹다",
          "speaker_id": "1",
          "time": "20210819 20:33"
        },
        {
          "id": "MDRW2100000908.1.17",
          "form": "염색은 뿌염진짜 노답이야",
          "original_form": "염색은 뿌염진짜 노답이야",
          "speaker_id": "2",
          "time": "20210819 20:33"
        },
        {
          "id": "MDRW2100000908.1.18",
          "form": "아직 한 통 남아서 할 수 있긴 하다",
          "original_form": "아직 한 통 남아서 할 수 있긴 하다",
          "speaker_id": "1",
          "time": "20210819 20:33"
        },
        {
          "id": "MDRW2100000908.1.19",
          "form": "와 부지런하다",
          "original_form": "와 부지런하다",
          "speaker_id": "2",
          "time": "20210819 20:33"
        },
        {
          "id": "MDRW2100000908.1.20",
          "form": "나 요즘 수분크림이랑 로션이 다 떨어져 가.",
