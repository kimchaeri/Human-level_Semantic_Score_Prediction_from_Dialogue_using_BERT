      "utterance": [
        {
          "id": "MDRW2100014776.1.1",
          "form": "동물 좋아하세요?",
          "original_form": "동물 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014776.1.2",
          "form": "네네! 혹시 동물 키우세요?",
          "original_form": "네네! 혹시 동물 키우세요?",
          "speaker_id": "2",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014776.1.3",
          "form": "저도 동물 좋아하는데 키우진 못 해요ㅠ",
          "original_form": "저도 동물 좋아하는데 키우진 못 해요ㅠ",
          "speaker_id": "1",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014776.1.4",
          "form": "알레르기 같은 거 있으세용??",
          "original_form": "알레르기 같은 거 있으세용??",
          "speaker_id": "2",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014776.1.5",
          "form": "아 그건 아닌데",
          "original_form": "아 그건 아닌데",
          "speaker_id": "1",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014776.1.6",
          "form": "건물주가 반려동물 안 된다고 해서요ㅠ",
          "original_form": "건물주가 반려동물 안 된다고 해서요ㅠ",
          "speaker_id": "1",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014776.1.7",
          "form": "앗 ㅠㅜ 나쁜 건물주.. 되면 어떤동뮬 키우고 싶으세요??",
          "original_form": "앗 ㅠㅜ 나쁜 건물주.. 되면 어떤동뮬 키우고 싶으세요??",
          "speaker_id": "2",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014776.1.8",
          "form": "음 강아지나 고양이 다 좋을 것 같어여",
          "original_form": "음 강아지나 고양이 다 좋을 것 같어여",
          "speaker_id": "1",
          "time": "20210825 20:58"
        },
        {
          "id": "MDRW2100014776.1.9",
          "form": "예전에 강아지 키웟는데",
          "original_form": "예전에 강아지 키웟는데",
          "speaker_id": "1",
          "time": "20210825 20:58"
        },
        {
          "id": "MDRW2100014776.1.10",
          "form": "무지개다리 건넜고든요 그래서 한 동안 강아지 못 보겠단데 요즘은 다시 귀엽고 예쁘더라구여",
          "original_form": "무지개다리 건넜고든요 그래서 한 동안 강아지 못 보겠단데 요즘은 다시 귀엽고 예쁘더라구여",
          "speaker_id": "1",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014776.1.11",
          "form": "그런 사연이 있으셨군요 ㅠㅠ 멍뭉이 좋은 곳 갔을거에요!",
          "original_form": "그런 사연이 있으셨군요 ㅠㅠ 멍뭉이 좋은 곳 갔을거에요!",
          "speaker_id": "2",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014776.1.12",
          "form": "name2님은 반려동물 키우시나요?",
          "original_form": "&name2&님은 반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014776.1.13",
          "form": "아뇨 저는 저 챙기기도 힘들더라구용 ㅠㅠ",
          "original_form": "아뇨 저는 저 챙기기도 힘들더라구용 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014776.1.14",
          "form": "대신 반려동물인형... ㅎㅎ 이랑 같이 살아요",
          "original_form": "대신 반려동물인형... ㅎㅎ 이랑 같이 살아요",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014776.1.15",
          "form": "반려동물 인형 ㅋㅋㅋㅋ 귀여워요",
          "original_form": "반려동물 인형 ㅋㅋㅋㅋ 귀여워요",
          "speaker_id": "1",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014776.1.16",
          "form": "어떤 동물 인형인가요",
          "original_form": "어떤 동물 인형인가요",
          "speaker_id": "1",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100014776.1.17",
          "form": "저는 곰 덕후랍니다.. 보기만 해도 행복해져요..",
          "original_form": "저는 곰 덕후랍니다.. 보기만 해도 행복해져요..",
          "speaker_id": "2",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014776.1.18",
          "form": "뚱뚱해도 귀엽고.. 가만히만 있어도 규ㅣ엽구...",
          "original_form": "뚱뚱해도 귀엽고.. 가만히만 있어도 규ㅣ엽구...",
          "speaker_id": "2",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014776.1.19",
          "form": "헐 곰 귀엽죠",
          "original_form": "헐 곰 귀엽죠",
          "speaker_id": "1",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100014776.1.20",
          "form": "라이언 캐릭터 좋아하세요?",
          "original_form": "라이언 캐릭터 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100014776.1.21",
          "form": "아 얜 곰이 어닌가요",
          "original_form": "아 얜 곰이 어닌가요",
          "speaker_id": "1",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100014776.1.22",
          "form": "저희 아부지께서 곰이라고 하셔요",
          "original_form": "저희 아부지께서 곰이라고 하셔요",
          "speaker_id": "2",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100014776.1.23",
          "form": "그러니 곰이라서 조아합니다 ㅎㅎㅎㅎ",
          "original_form": "그러니 곰이라서 조아합니다 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:37"
        }
      ]
    }
  ]
