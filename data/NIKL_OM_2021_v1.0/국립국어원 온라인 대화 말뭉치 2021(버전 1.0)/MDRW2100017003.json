      "utterance": [
        {
          "id": "MDRW2100017003.1.1",
          "form": "그러고보니 한국 프게머들",
          "original_form": "그러고보니 한국 프게머들",
          "speaker_id": "2",
          "time": "20210826 16:48"
        },
        {
          "id": "MDRW2100017003.1.2",
          "form": "다들 거북일자목에 라운드숄더던데",
          "original_form": "다들 거북일자목에 라운드숄더던데",
          "speaker_id": "2",
          "time": "20210826 16:49"
        },
        {
          "id": "MDRW2100017003.1.3",
          "form": "건강관리 해야하는거 아닌가",
          "original_form": "건강관리 해야하는거 아닌가",
          "speaker_id": "2",
          "time": "20210826 16:49"
        },
        {
          "id": "MDRW2100017003.1.4",
          "form": "구단이.. 알아서 하겄지ㅋㅋㅋㅋ",
          "original_form": "구단이.. 알아서 하겄지ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:50"
        },
        {
          "id": "MDRW2100017003.1.5",
          "form": "ㅋㅋ근데 타국선수들은 안그런대",
          "original_form": "ㅋㅋ근데 타국선수들은 안그런대",
          "speaker_id": "2",
          "time": "20210826 16:51"
        },
        {
          "id": "MDRW2100017003.1.6",
          "form": "한국만그래서",
          "original_form": "한국만그래서",
          "speaker_id": "2",
          "time": "20210826 16:51"
        },
        {
          "id": "MDRW2100017003.1.7",
          "form": "허리로 고생하고나니 남일같지 않네",
          "original_form": "허리로 고생하고나니 남일같지 않네",
          "speaker_id": "2",
          "time": "20210826 16:52"
        },
        {
          "id": "MDRW2100017003.1.8",
          "form": "에궁...다들 허리피고 게임해...",
          "original_form": "에궁...다들 허리피고 게임해...",
          "speaker_id": "1",
          "time": "20210826 16:52"
        },
        {
          "id": "MDRW2100017003.1.9",
          "form": "저번리그에 카트 선수 하나도",
          "original_form": "저번리그에 카트 선수 하나도",
          "speaker_id": "1",
          "time": "20210826 16:52"
        },
        {
          "id": "MDRW2100017003.1.10",
          "form": "리그 내내 손목 아프다고 했는데",
          "original_form": "리그 내내 손목 아프다고 했는데",
          "speaker_id": "1",
          "time": "20210826 16:53"
        },
        {
          "id": "MDRW2100017003.1.11",
          "form": "그래서 롤 프게머중에 건강때문에 스무살도 안됐는데 은퇴하는애들많대",
          "original_form": "그래서 롤 프게머중에 건강때문에 스무살도 안됐는데 은퇴하는애들많대",
          "speaker_id": "2",
          "time": "20210826 16:53"
        },
        {
          "id": "MDRW2100017003.1.12",
          "form": "그건 너무 심각하네ㅋㅋㅋ",
          "original_form": "그건 너무 심각하네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:53"
        },
        {
          "id": "MDRW2100017003.1.13",
          "form": "ㄹㅇ... 쉬운 일이없다",
          "original_form": "ㄹㅇ... 쉬운 일이없다",
          "speaker_id": "2",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100017003.1.14",
          "form": "진짜 아프다거 하면 철렁한다고..",
          "original_form": "진짜 아프다거 하면 철렁한다고..",
          "speaker_id": "1",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100017003.1.15",
          "form": "내가 말한애는 갠전 우승 맨날 코앞에서 놓치던 애라",
          "original_form": "내가 말한애는 갠전 우승 맨날 코앞에서 놓치던 애라",
          "speaker_id": "1",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100017003.1.16",
          "form": "근데 별로 챙겨주는데가 없는듯",
          "original_form": "근데 별로 챙겨주는데가 없는듯",
          "speaker_id": "2",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100017003.1.17",
          "form": "진짜 아까움ㅠㅠㅠ",
          "original_form": "진짜 아까움ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100017003.1.18",
          "form": "월급이 쟈가서 그런가",
          "original_form": "월급이 쟈가서 그런가",
          "speaker_id": "2",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100017003.1.19",
          "form": "그런가바...",
          "original_form": "그런가바...",
          "speaker_id": "1",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100017003.1.20",
          "form": "스포츠건 e스포츠건",
          "original_form": "스포츠건 e스포츠건",
          "speaker_id": "2",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100017003.1.21",
          "form": "몸이 망가지네",
          "original_form": "몸이 망가지네",
          "speaker_id": "2",
