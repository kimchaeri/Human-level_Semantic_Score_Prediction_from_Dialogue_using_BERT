      "utterance": [
        {
          "id": "MDRW2100018695.1.1",
          "form": "둘이 심심하지 않겠다..",
          "original_form": "둘이 심심하지 않겠다..",
          "speaker_id": "2",
          "time": "20210827 10:51"
        },
        {
          "id": "MDRW2100018695.1.2",
          "form": "한마리 키움 약간 가여워요",
          "original_form": "한마리 키움 약간 가여워요",
          "speaker_id": "2",
          "time": "20210827 10:52"
        },
        {
          "id": "MDRW2100018695.1.3",
          "form": "맞아요ㅠ 근데 고양이가 독립적이라 그런지 따로 지내더라구요",
          "original_form": "맞아요ㅠ 근데 고양이가 독립적이라 그런지 따로 지내더라구요",
          "speaker_id": "1",
          "time": "20210827 10:52"
        },
        {
          "id": "MDRW2100018695.1.4",
          "form": "놀때도 같이 안놀고ㅠ",
          "original_form": "놀때도 같이 안놀고ㅠ",
          "speaker_id": "1",
          "time": "20210827 10:52"
        },
        {
          "id": "MDRW2100018695.1.5",
          "form": "애기때부터 같이 키웠는데도요?",
          "original_form": "애기때부터 같이 키웠는데도요?",
          "speaker_id": "2",
          "time": "20210827 10:53"
        },
        {
          "id": "MDRW2100018695.1.6",
          "form": "네 커가면서 서로 자리라던지 생기더라구요",
          "original_form": "네 커가면서 서로 자리라던지 생기더라구요",
          "speaker_id": "1",
          "time": "20210827 10:53"
        },
        {
          "id": "MDRW2100018695.1.7",
          "form": "함께하기도 하지만 개별으로 활동해요",
          "original_form": "함께하기도 하지만 개별으로 활동해요",
          "speaker_id": "1",
          "time": "20210827 10:54"
        },
        {
          "id": "MDRW2100018695.1.8",
          "form": "주인한테 질투는 안해요?",
          "original_form": "주인한테 질투는 안해요?",
          "speaker_id": "2",
          "time": "20210827 10:55"
        },
        {
          "id": "MDRW2100018695.1.9",
          "form": "한쪽에 관심주면 한쪽에서 거리감 두긴하더라구요",
          "original_form": "한쪽에 관심주면 한쪽에서 거리감 두긴하더라구요",
          "speaker_id": "1",
          "time": "20210827 10:58"
        },
        {
          "id": "MDRW2100018695.1.10",
          "form": "대화가 안되니 속상해요ㅜ",
          "original_form": "대화가 안되니 속상해요ㅜ",
          "speaker_id": "1",
          "time": "20210827 10:58"
        },
        {
          "id": "MDRW2100018695.1.11",
          "form": "ㅡㅡ 너무 안타깝네요... \n얼렁 고양이 번역기 나왓으면..",
          "original_form": "ㅡㅡ 너무 안타깝네요... \n얼렁 고양이 번역기 나왓으면..",
          "speaker_id": "2",
          "time": "20210827 10:59"
        },
        {
          "id": "MDRW2100018695.1.12",
          "form": "맞아요ㅋㅋ 어플 해봤는데 맞는건지 안맞는건지 싶더라구요",
          "original_form": "맞아요ㅋㅋ 어플 해봤는데 맞는건지 안맞는건지 싶더라구요",
          "speaker_id": "1",
          "time": "20210827 11:00"
        },
        {
          "id": "MDRW2100018695.1.13",
          "form": "지금나오는건 다 사기같아요..ㅋㅋㅋ \n다들 아픈덴 엄서여?",
          "original_form": "지금나오는건 다 사기같아요..ㅋㅋㅋ \n다들 아픈덴 엄서여?",
          "speaker_id": "2",
          "time": "20210827 11:02"
        },
        {
          "id": "MDRW2100018695.1.14",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:03"
        },
        {
          "id": "MDRW2100018695.1.15",
          "form": "넵! 건강히 지내고 있어요~",
          "original_form": "넵! 건강히 지내고 있어요~",
          "speaker_id": "1",
          "time": "20210827 11:03"
        },
        {
          "id": "MDRW2100018695.1.16",
          "form": "병원을 싫어해서 최대한 지금은 안갈수 있도록 아프지않게 지켜보고있어요",
          "original_form": "병원을 싫어해서 최대한 지금은 안갈수 있도록 아프지않게 지켜보고있어요",
          "speaker_id": "1",
          "time": "20210827 11:04"
        },
        {
          "id": "MDRW2100018695.1.17",
          "form": "다행이네요.. 건강이.. \n동물은 진짜 병원비 넘 비싸요..",
          "original_form": "다행이네요.. 건강이.. \n동물은 진짜 병원비 넘 비싸요..",
          "speaker_id": "2",
          "time": "20210827 11:07"
        },
        {
          "id": "MDRW2100018695.1.18",
          "form": "맞아요 보험이 많이 생기면 좋겠어요",
          "original_form": "맞아요 보험이 많이 생기면 좋겠어요",
          "speaker_id": "1",
          "time": "20210827 11:08"
        },
        {
          "id": "MDRW2100018695.1.19",
          "form": "사제보험은 없나요?",
          "original_form": "사제보험은 없나요?",
          "speaker_id": "2",
          "time": "20210827 11:09"
        },
        {
          "id": "MDRW2100018695.1.20",
          "form": "없었던거로 알고있어요",
          "original_form": "없었던거로 알고있어요",
          "speaker_id": "1",
          "time": "20210827 11:11"
        },
        {
          "id": "MDRW2100018695.1.21",
          "form": "최근엔 안찾아봐서 정확히는 모르겠어요ㅠ",
          "original_form": "최근엔 안찾아봐서 정확히는 모르겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210827 11:11"
        }
      ]
    }
