      "utterance": [
        {
          "id": "MDRW2100022706.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022706.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210829 17:11"
        },
        {
          "id": "MDRW2100022706.1.3",
          "form": "다이어트 해보신적 있나요",
          "original_form": "다이어트 해보신적 있나요",
          "speaker_id": "1",
          "time": "20210829 17:12"
        },
        {
          "id": "MDRW2100022706.1.4",
          "form": "급하게 일주일 정도 해본 적은 있어요 ㅠㅠㅋㅋ",
          "original_form": "급하게 일주일 정도 해본 적은 있어요 ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:18"
        },
        {
          "id": "MDRW2100022706.1.5",
          "form": "어떤 다이어트 하셨어요?",
          "original_form": "어떤 다이어트 하셨어요?",
          "speaker_id": "1",
          "time": "20210829 17:19"
        },
        {
          "id": "MDRW2100022706.1.6",
          "form": "두유 감자 샐러드 계란 닭가슴살이랑 과일 먹으면서 땅끄부부 운동 따라했어요!",
          "original_form": "두유 감자 샐러드 계란 닭가슴살이랑 과일 먹으면서 땅끄부부 운동 따라했어요!",
          "speaker_id": "2",
          "time": "20210829 17:19"
        },
        {
          "id": "MDRW2100022706.1.7",
          "form": "오 식단이랑 같이하셨군요 효과가 있었나요?",
          "original_form": "오 식단이랑 같이하셨군요 효과가 있었나요?",
          "speaker_id": "1",
          "time": "20210829 17:20"
        },
        {
          "id": "MDRW2100022706.1.8",
          "form": "제가 원래 살이 잘 안빠지고 안찌는 체질이라",
          "original_form": "제가 원래 살이 잘 안빠지고 안찌는 체질이라",
          "speaker_id": "2",
          "time": "20210829 17:20"
        },
        {
          "id": "MDRW2100022706.1.9",
          "form": "일주일 해서 1kg 빠졌더라구요",
          "original_form": "일주일 해서 1kg 빠졌더라구요",
          "speaker_id": "2",
          "time": "20210829 17:20"
        },
        {
          "id": "MDRW2100022706.1.10",
          "form": "다이어트 해보신 적 있으세요?",
          "original_form": "다이어트 해보신 적 있으세요?",
          "speaker_id": "2",
          "time": "20210829 17:21"
        },
        {
          "id": "MDRW2100022706.1.11",
          "form": "일주일에 1kg라도 빼서 대단하시네요",
          "original_form": "일주일에 1kg라도 빼서 대단하시네요",
          "speaker_id": "1",
          "time": "20210829 17:21"
        },
        {
          "id": "MDRW2100022706.1.12",
          "form": "저는 다이어트해서 8kg 뺐었는데",
          "original_form": "저는 다이어트해서 8kg 뺐었는데",
          "speaker_id": "1",
          "time": "20210829 17:21"
        },
        {
          "id": "MDRW2100022706.1.13",
          "form": "요요와서 9kg 다시쪘습니다",
          "original_form": "요요와서 9kg 다시쪘습니다",
          "speaker_id": "1",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022706.1.14",
          "form": "앗....",
          "original_form": "앗....",
          "speaker_id": "2",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022706.1.15",
          "form": "다시 운동하려하는데",
          "original_form": "다시 운동하려하는데",
          "speaker_id": "1",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100022706.1.16",
          "form": "의지가 안생기네요..",
          "original_form": "의지가 안생기네요..",
          "speaker_id": "1",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100022706.1.17",
          "form": "저도 사진 찍느라고 다이어트 했는데",
          "original_form": "저도 사진 찍느라고 다이어트 했는데",
          "speaker_id": "2",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100022706.1.18",
          "form": "계속 참으니까 끝나고 단 게 엄청 땡기더라구요...ㅠㅠ",
          "original_form": "계속 참으니까 끝나고 단 게 엄청 땡기더라구요...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100022706.1.19",
          "form": "단거 엄청 좋아해서",
          "original_form": "단거 엄청 좋아해서",
          "speaker_id": "1",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022706.1.20",
          "form": "힘들죠ㅠㅠ",
          "original_form": "힘들죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022706.1.21",
          "form": "저는 극단적으로 살뺏었는데",
          "original_form": "저는 극단적으로 살뺏었는데",
          "speaker_id": "1",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022706.1.22",
          "form": "부작용이 많았어요",
          "original_form": "부작용이 많았어요",
          "speaker_id": "1",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022706.1.23",
          "form": "요즘 코로나 때문에 계속 집에 있고",
          "original_form": "요즘 코로나 때문에 계속 집에 있고",
          "speaker_id": "2",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022706.1.24",
          "form": "누워있으니까 살이 더 찌네요",
          "original_form": "누워있으니까 살이 더 찌네요",
          "speaker_id": "2",
          "time": "20210829 17:24"
        },
        {
          "id": "MDRW2100022706.1.25",
          "form": "그렇죠.. 빨리 운동해야하는데",
          "original_form": "그렇죠.. 빨리 운동해야하는데",
          "speaker_id": "1",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022706.1.26",
          "form": "혹시 코로나 백신 맞으셨나요?",
          "original_form": "혹시 코로나 백신 맞으셨나요?",
          "speaker_id": "2",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022706.1.27",
          "form": "저번주에 1차 접종했어서",
          "original_form": "저번주에 1차 접종했어서",
          "speaker_id": "1",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022706.1.28",
          "form": "일주일간 운동 안해서",
          "original_form": "일주일간 운동 안해서",
          "speaker_id": "1",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022706.1.29",
          "form": "살이 더쪘습니다 ㅎ허러",
          "original_form": "살이 더쪘습니다 ㅎ허러",
          "speaker_id": "1",
          "time": "20210829 17:25"
        },
        {
          "id": "MDRW2100022706.1.30",
          "form": "저는 화이자 2차 접종까지 했는데",
          "original_form": "저는 화이자 2차 접종까지 했는데",
          "speaker_id": "2",
          "time": "20210829 17:26"
        },
        {
          "id": "MDRW2100022706.1.31",
          "form": "팔에 근육통 온 것처럼 아프더라구요",
          "original_form": "팔에 근육통 온 것처럼 아프더라구요",
          "speaker_id": "2",
          "time": "20210829 17:27"
        },
        {
          "id": "MDRW2100022706.1.32",
          "form": "2차에 부작용 많다던데",
          "original_form": "2차에 부작용 많다던데",
          "speaker_id": "1",
          "time": "20210829 17:27"
        },
        {
          "id": "MDRW2100022706.1.33",
          "form": "근육통만 왔었나요?",
          "original_form": "근육통만 왔었나요?",
          "speaker_id": "1",
          "time": "20210829 17:27"
        },
        {
          "id": "MDRW2100022706.1.34",
          "form": "2차 접종 부작용은 없었나요?",
          "original_form": "2차 접종 부작용은 없었나요?",
          "speaker_id": "1",
          "time": "20210829 17:27"
        }
      ]
