      "utterance": [
        {
          "id": "MDRW2100036243.1.1",
          "form": "돈만 생기면 쓰고 싶어 큰일이에요ㅠ",
          "original_form": "돈만 생기면 쓰고 싶어 큰일이에요ㅠ",
          "speaker_id": "2",
          "time": "20210830 20:01"
        },
        {
          "id": "MDRW2100036243.1.2",
          "form": "사고싶은거 뭐있으세요?",
          "original_form": "사고싶은거 뭐있으세요?",
          "speaker_id": "1",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100036243.1.3",
          "form": "저는 반지",
          "original_form": "저는 반지",
          "speaker_id": "1",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100036243.1.4",
          "form": "가방",
          "original_form": "가방",
          "speaker_id": "1",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100036243.1.5",
          "form": "후드티",
          "original_form": "후드티",
          "speaker_id": "1",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100036243.1.6",
          "form": "바막",
          "original_form": "바막",
          "speaker_id": "1",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100036243.1.7",
          "form": "신발 오븐 가디건",
          "original_form": "신발 오븐 가디건",
          "speaker_id": "2",
          "time": "20210830 20:16"
        },
        {
          "id": "MDRW2100036243.1.8",
          "form": "악세사리 재료요!",
          "original_form": "악세사리 재료요!",
          "speaker_id": "2",
          "time": "20210830 20:16"
        },
        {
          "id": "MDRW2100036243.1.9",
          "form": "신발은 어떤건가요?!",
          "original_form": "신발은 어떤건가요?!",
          "speaker_id": "1",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100036243.1.10",
          "form": "나이스꺼요!!",
          "original_form": "나이스꺼요!!",
          "speaker_id": "2",
          "time": "20210830 20:22"
        },
        {
          "id": "MDRW2100036243.1.11",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 20:22"
        },
        {
          "id": "MDRW2100036243.1.12",
          "form": "저는 뉴발꺼요!",
          "original_form": "저는 뉴발꺼요!",
          "speaker_id": "1",
          "time": "20210830 20:22"
        },
        {
          "id": "MDRW2100036243.1.13",
          "form": "품절인데 재입고가 안되네요ㅠ",
          "original_form": "품절인데 재입고가 안되네요ㅠ",
          "speaker_id": "1",
          "time": "20210830 20:23"
        },
        {
          "id": "MDRW2100036243.1.14",
          "form": "아.. 인기가 많은가보네요ㅠㅜ",
          "original_form": "아.. 인기가 많은가보네요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210830 20:27"
        },
        {
          "id": "MDRW2100036243.1.15",
          "form": "재입고는 빠딱빠딱 해줘야하는데ㅠ",
          "original_form": "재입고는 빠딱빠딱 해줘야하는데ㅠ",
          "speaker_id": "2",
          "time": "20210830 20:27"
        },
        {
          "id": "MDRW2100036243.1.16",
          "form": "장사꾼들,..",
          "original_form": "장사꾼들,..",
          "speaker_id": "1",
          "time": "20210830 20:28"
        },
        {
          "id": "MDRW2100036243.1.17",
          "form": "가디건은 무슨색 사고싶으세요?!",
          "original_form": "가디건은 무슨색 사고싶으세요?!",
          "speaker_id": "1",
          "time": "20210830 20:28"
        },
        {
          "id": "MDRW2100036243.1.18",
          "form": "저는 검정색!",
          "original_form": "저는 검정색!",
          "speaker_id": "1",
          "time": "20210830 20:28"
        },
        {
          "id": "MDRW2100036243.1.19",
          "form": "저는 검정하고 베이지요!",
          "original_form": "저는 검정하고 베이지요!",
          "speaker_id": "2",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100036243.1.20",
          "form": "맘같아서는 색별로 쟁이고 싶슴다",
          "original_form": "맘같아서는 색별로 쟁이고 싶슴다",
          "speaker_id": "2",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100036243.1.21",
          "form": "아 그쵸",
          "original_form": "아 그쵸",
          "speaker_id": "1",
          "time": "20210830 20:30"
        },
        {
          "id": "MDRW2100036243.1.22",
          "form": "날마다 기분이 다른데",
          "original_form": "날마다 기분이 다른데",
          "speaker_id": "1",
          "time": "20210830 20:31"
        },
        {
          "id": "MDRW2100036243.1.23",
          "form": "다있어야,,",
          "original_form": "다있어야,,",
          "speaker_id": "1",
          "time": "20210830 20:31"
        },
        {
          "id": "MDRW2100036243.1.24",
          "form": "맞아요.. 똑같은 색 매일은 아쉽거등요",
          "original_form": "맞아요.. 똑같은 색 매일은 아쉽거등요",
          "speaker_id": "2",
          "time": "20210830 20:35"
        },
        {
          "id": "MDRW2100036243.1.25",
          "form": "아 베이지도 이쁠것같아요",
          "original_form": "아 베이지도 이쁠것같아요",
          "speaker_id": "1",
          "time": "20210830 20:42"
        },
        {
          "id": "MDRW2100036243.1.26",
          "form": "검정슬랙스에,,.",
          "original_form": "검정슬랙스에,,.",
          "speaker_id": "1",
          "time": "20210830 20:42"
        },
        {
          "id": "MDRW2100036243.1.27",
          "form": "아 좋죠",
          "original_form": "아 좋죠",
          "speaker_id": "2",
          "time": "20210830 20:45"
        },
        {
          "id": "MDRW2100036243.1.28",
          "form": "벌써 즐거워요",
          "original_form": "벌써 즐거워요",
          "speaker_id": "2",
          "time": "20210830 20:45"
        },
        {
          "id": "MDRW2100036243.1.29",
          "form": "ㅋㄱㅋㄱㄱㄱㄱㅋ아직 사지도않았는데,,,!",
          "original_form": "ㅋㄱㅋㄱㄱㄱㄱㅋ아직 사지도않았는데,,,!",
          "speaker_id": "1",
          "time": "20210830 20:46"
        }
      ]
    }
