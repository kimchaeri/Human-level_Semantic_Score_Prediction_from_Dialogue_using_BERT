      "utterance": [
        {
          "id": "MDRW2100005448.1.1",
          "form": "운동해요?",
          "original_form": "운동해요?",
          "speaker_id": "1",
          "time": "20210821 22:26"
        },
        {
          "id": "MDRW2100005448.1.2",
          "form": "몇달전까지 3시간씩 했는데 요새 안해요",
          "original_form": "몇달전까지 3시간씩 했는데 요새 안해요",
          "speaker_id": "2",
          "time": "20210821 22:30"
        },
        {
          "id": "MDRW2100005448.1.3",
          "form": "오늘부터 살살해요",
          "original_form": "오늘부터 살살해요",
          "speaker_id": "2",
          "time": "20210821 22:30"
        },
        {
          "id": "MDRW2100005448.1.4",
          "form": "운동 하세요?",
          "original_form": "운동 하세요?",
          "speaker_id": "2",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005448.1.5",
          "form": "홈트요",
          "original_form": "홈트요",
          "speaker_id": "1",
          "time": "20210822 08:51"
        },
        {
          "id": "MDRW2100005448.1.6",
          "form": "뭐 보고 따라하세요",
          "original_form": "뭐 보고 따라하세요",
          "speaker_id": "2",
          "time": "20210822 09:22"
        },
        {
          "id": "MDRW2100005448.1.7",
          "form": "잘보는 방송 있으세요?",
          "original_form": "잘보는 방송 있으세요?",
          "speaker_id": "2",
          "time": "20210822 09:22"
        },
        {
          "id": "MDRW2100005448.1.8",
          "form": "땅크요",
          "original_form": "땅크요",
          "speaker_id": "1",
          "time": "20210822 09:22"
        },
        {
          "id": "MDRW2100005448.1.9",
          "form": "다들 땅끄 많이 보던데",
          "original_form": "다들 땅끄 많이 보던데",
          "speaker_id": "2",
          "time": "20210822 09:22"
        },
        {
          "id": "MDRW2100005448.1.10",
          "form": "케미가",
          "original_form": "케미가",
          "speaker_id": "1",
          "time": "20210822 09:22"
        },
        {
          "id": "MDRW2100005448.1.11",
          "form": "칼소폭 이런건 도저히 못하겠어요",
          "original_form": "칼소폭 이런건 도저히 못하겠어요",
          "speaker_id": "2",
          "time": "20210822 09:22"
        },
        {
          "id": "MDRW2100005448.1.12",
          "form": "두분 넘좋아 보이죠",
          "original_form": "두분 넘좋아 보이죠",
          "speaker_id": "2",
          "time": "20210822 09:23"
        },
        {
          "id": "MDRW2100005448.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 09:23"
        },
        {
          "id": "MDRW2100005448.1.14",
          "form": "운동 잘하게 뿜뿜 해주시던데",
          "original_form": "운동 잘하게 뿜뿜 해주시던데",
          "speaker_id": "2",
          "time": "20210822 09:23"
        },
        {
          "id": "MDRW2100005448.1.15",
          "form": "전 쉬운거만 따라했어요",
          "original_form": "전 쉬운거만 따라했어요",
          "speaker_id": "2",
          "time": "20210822 09:23"
        },
        {
          "id": "MDRW2100005448.1.16",
          "form": "고난이도도 해보셨어요?",
          "original_form": "고난이도도 해보셨어요?",
          "speaker_id": "2",
          "time": "20210822 09:23"
        },
        {
          "id": "MDRW2100005448.1.17",
          "form": "저도 쉬운거만",
          "original_form": "저도 쉬운거만",
          "speaker_id": "1",
          "time": "20210822 09:24"
        },
        {
          "id": "MDRW2100005448.1.18",
          "form": "그렇죠 저만 그런게 아니군요 ㅋㅋ",
          "original_form": "그렇죠 저만 그런게 아니군요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:24"
        },
        {
          "id": "MDRW2100005448.1.19",
          "form": "쉬운것도 배 엄청 땡겨요 운동 돼요",
          "original_form": "쉬운것도 배 엄청 땡겨요 운동 돼요",
          "speaker_id": "2",
          "time": "20210822 09:25"
        },
        {
          "id": "MDRW2100005448.1.20",
          "form": "숨차고",
          "original_form": "숨차고",
          "speaker_id": "2",
          "time": "20210822 09:25"
        },
        {
          "id": "MDRW2100005448.1.21",
          "form": "땀이 쏟아지는 조금만 해도",
          "original_form": "땀이 쏟아지는 조금만 해도",
          "speaker_id": "1",
