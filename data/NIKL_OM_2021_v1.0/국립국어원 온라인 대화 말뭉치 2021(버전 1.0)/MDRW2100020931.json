      "utterance": [
        {
          "id": "MDRW2100020931.1.1",
          "form": "그런 육아전담하시는거에요?",
          "original_form": "그런 육아전담하시는거에요?",
          "speaker_id": "1",
          "time": "20210828 14:16"
        },
        {
          "id": "MDRW2100020931.1.2",
          "form": "거의 그렇게됐죠",
          "original_form": "거의 그렇게됐죠",
          "speaker_id": "2",
          "time": "20210828 14:17"
        },
        {
          "id": "MDRW2100020931.1.3",
          "form": "자기 시간 1도 엄겟어요? 아직 갓난아기 일거같은뎅..",
          "original_form": "자기 시간 1도 엄겟어요? 아직 갓난아기 일거같은뎅..",
          "speaker_id": "1",
          "time": "20210828 14:18"
        },
        {
          "id": "MDRW2100020931.1.4",
          "form": "5 6살이요 ㅎ",
          "original_form": "5 6살이요 ㅎ",
          "speaker_id": "2",
          "time": "20210828 14:18"
        },
        {
          "id": "MDRW2100020931.1.5",
          "form": "제일 많이 사고칠때져?",
          "original_form": "제일 많이 사고칠때져?",
          "speaker_id": "1",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100020931.1.6",
          "form": "맞아요  말도많고",
          "original_form": "맞아요  말도많고",
          "speaker_id": "2",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100020931.1.7",
          "form": "신기한것도 많고",
          "original_form": "신기한것도 많고",
          "speaker_id": "2",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100020931.1.8",
          "form": "그.. 질문의 무한루프..",
          "original_form": "그.. 질문의 무한루프..",
          "speaker_id": "1",
          "time": "20210828 14:21"
        },
        {
          "id": "MDRW2100020931.1.9",
          "form": "엄아 그건 왜? 그래서 왜? 왜.. ㅋㅋㅋ",
          "original_form": "엄아 그건 왜? 그래서 왜? 왜.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:21"
        },
        {
          "id": "MDRW2100020931.1.10",
          "form": "왜요 반복 ㅋ",
          "original_form": "왜요 반복 ㅋ",
          "speaker_id": "2",
          "time": "20210828 14:21"
        },
        {
          "id": "MDRW2100020931.1.11",
          "form": "밤에 잠은 잘자요?",
          "original_form": "밤에 잠은 잘자요?",
          "speaker_id": "1",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100020931.1.12",
          "form": "네 잠은 잘자요",
          "original_form": "네 잠은 잘자요",
          "speaker_id": "2",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100020931.1.13",
          "form": "그나마 다행이네요.. 밤에 안자는 애기들 있음.. 으으윽..",
          "original_form": "그나마 다행이네요.. 밤에 안자는 애기들 있음.. 으으윽..",
          "speaker_id": "1",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020931.1.14",
          "form": "그래도 나름 내시간만들어서 운동도다니고",
          "original_form": "그래도 나름 내시간만들어서 운동도다니고",
          "speaker_id": "2",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020931.1.15",
          "form": "그래요 ㅎㅎ",
          "original_form": "그래요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020931.1.16",
          "form": "9시되면 불다꺼요 ㅋ",
          "original_form": "9시되면 불다꺼요 ㅋ",
          "speaker_id": "2",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020931.1.17",
          "form": "그럼 책읽어주면 책다보고 자요",
          "original_form": "그럼 책읽어주면 책다보고 자요",
          "speaker_id": "2",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020931.1.18",
          "form": "ㅋㅋㅋ 착하네.. 아마도 딸?이라서 얌전한건가요,?",
          "original_form": "ㅋㅋㅋ 착하네.. 아마도 딸?이라서 얌전한건가요,?",
          "speaker_id": "1",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020931.1.19",
          "form": "딸 아들 순서예요",
          "original_form": "딸 아들 순서예요",
          "speaker_id": "2",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100020931.1.20",
          "form": "저는 세종도착해서 이제 일좀봐야겠네요",
          "original_form": "저는 세종도착해서 이제 일좀봐야겠네요",
          "speaker_id": "2",
          "time": "20210828 14:26"
        },
