      "utterance": [
        {
          "id": "MDRW2100036301.1.1",
          "form": "닭탕매울꺼같아 ㅇ",
          "original_form": "닭탕매울꺼같아 ㅇ",
          "speaker_id": "2",
          "time": "20210830 21:04"
        },
        {
          "id": "MDRW2100036301.1.2",
          "form": "오빠못먹으면어쩌노",
          "original_form": "오빠못먹으면어쩌노",
          "speaker_id": "2",
          "time": "20210830 21:04"
        },
        {
          "id": "MDRW2100036301.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:04"
        },
        {
          "id": "MDRW2100036301.1.4",
          "form": "집으로 다 퍼졋을꺼여..",
          "original_form": "집으로 다 퍼졋을꺼여..",
          "speaker_id": "2",
          "time": "20210831 15:29"
        },
        {
          "id": "MDRW2100036301.1.5",
          "form": "내가진짜 방충망에 기피제라도 뿌려야하나",
          "original_form": "내가진짜 방충망에 기피제라도 뿌려야하나",
          "speaker_id": "2",
          "time": "20210831 15:29"
        },
        {
          "id": "MDRW2100036301.1.6",
          "form": "막생각하고잇엇는데",
          "original_form": "막생각하고잇엇는데",
          "speaker_id": "2",
          "time": "20210831 15:30"
        },
        {
          "id": "MDRW2100036301.1.7",
          "form": "ㅋㅋㅋㅋ애꿎은 기피제만 쓸뻘",
          "original_form": "ㅋㅋㅋㅋ애꿎은 기피제만 쓸뻘",
          "speaker_id": "1",
          "time": "20210831 15:30"
        },
        {
          "id": "MDRW2100036301.1.8",
          "form": "근데 다행인지 모기눈 없엇나봐 ㅋㅋ안물림 ㅋㅋㅋㅋㅋㅋㅋ아오",
          "original_form": "근데 다행인지 모기눈 없엇나봐 ㅋㅋ안물림 ㅋㅋㅋㅋㅋㅋㅋ아오",
          "speaker_id": "2",
          "time": "20210831 15:30"
        },
        {
          "id": "MDRW2100036301.1.9",
          "form": "그럼 얼마나웃겻을꼬 ㅋㅋㅋㅋ",
          "original_form": "그럼 얼마나웃겻을꼬 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:30"
        },
        {
          "id": "MDRW2100036301.1.10",
          "form": "ㅋㅋㅋ진짜 다행 ㅋㅋㅋ 근데 우리집에 모기 들어왔었나봐 내동생 다리 두 방이나 물렸더라 ㅋㅋ",
          "original_form": "ㅋㅋㅋ진짜 다행 ㅋㅋㅋ 근데 우리집에 모기 들어왔었나봐 내동생 다리 두 방이나 물렸더라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:31"
        },
        {
          "id": "MDRW2100036301.1.11",
          "form": "에이 ㅠㅠ 엘베타고따라왓당",
          "original_form": "에이 ㅠㅠ 엘베타고따라왓당",
          "speaker_id": "2",
          "time": "20210831 15:31"
        },
        {
          "id": "MDRW2100036301.1.12",
          "form": "간지러어쩌노ㅠ",
          "original_form": "간지러어쩌노ㅠ",
          "speaker_id": "2",
          "time": "20210831 15:31"
        },
        {
          "id": "MDRW2100036301.1.13",
          "form": "ㅋㅋ맛있는 피였나봄",
          "original_form": "ㅋㅋ맛있는 피였나봄",
          "speaker_id": "1",
          "time": "20210831 15:32"
        },
        {
          "id": "MDRW2100036301.1.14",
          "form": "헉 ㅋㅋㅋㅋㅋ징그러ㅠㅠㅠ",
          "original_form": "헉 ㅋㅋㅋㅋㅋ징그러ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 15:32"
        },
        {
          "id": "MDRW2100036301.1.15",
          "form": "오늘 일찍 출근하느라 아침도 못먹고 왔겠는디?!!",
          "original_form": "오늘 일찍 출근하느라 아침도 못먹고 왔겠는디?!!",
          "speaker_id": "1",
          "time": "20210831 15:32"
        },
        {
          "id": "MDRW2100036301.1.16",
          "form": "진짜 더욱 신경쓰겟어 ㅋㅋㅋ",
          "original_form": "진짜 더욱 신경쓰겟어 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:32"
        },
        {
          "id": "MDRW2100036301.1.17",
          "form": "한시간졸앗더니",
          "original_form": "한시간졸앗더니",
          "speaker_id": "2",
          "time": "20210831 15:32"
        },
        {
          "id": "MDRW2100036301.1.18",
          "form": "9시에 눈뜨고 바로왓징",
          "original_form": "9시에 눈뜨고 바로왓징",
          "speaker_id": "2",
          "time": "20210831 15:33"
        },
        {
          "id": "MDRW2100036301.1.19",
          "form": "김밥먹음 ㅋㅋ",
          "original_form": "김밥먹음 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:33"
        },
        {
          "id": "MDRW2100036301.1.20",
          "form": "여기 오전언니가 아아도 사다쥬고감 ㅎ",
          "original_form": "여기 오전언니가 아아도 사다쥬고감 ㅎ",
          "speaker_id": "2",
          "time": "20210831 15:33"
        },
        {
          "id": "MDRW2100036301.1.21",
          "form": "오 언니 센스있으시구먼ㅋㅋㅋ",
          "original_form": "오 언니 센스있으시구먼ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:34"
        },
        {
          "id": "MDRW2100036301.1.22",
          "form": "먼저 자기가 사주겟다고 뭐마시고싶냐고 물어보더라구!",
          "original_form": "먼저 자기가 사주겟다고 뭐마시고싶냐고 물어보더라구!",
          "speaker_id": "2",
          "time": "20210831 15:34"
        },
        {
          "id": "MDRW2100036301.1.23",
          "form": "갬동이엿어 ㅋㅋㅋ",
          "original_form": "갬동이엿어 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:34"
        },
        {
          "id": "MDRW2100036301.1.24",
          "form": "ㅋㅋ진짜 약간 천사신가?ㅋㅋㅋ",
          "original_form": "ㅋㅋ진짜 약간 천사신가?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 15:35"
        },
        {
          "id": "MDRW2100036301.1.25",
          "form": "벌써 얼음은 다 사라졋네 ㅋㅋㅋ더벤티사다줌 ㅋㅋㅋ",
          "original_form": "벌써 얼음은 다 사라졋네 ㅋㅋㅋ더벤티사다줌 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:35"
        },
        {
          "id": "MDRW2100036301.1.26",
          "form": "여기앞에 컴포즈는 현수막잇으면 아메처넌이다",
          "original_form": "여기앞에 컴포즈는 현수막잇으면 아메처넌이다",
          "speaker_id": "2",
          "time": "20210831 15:35"
        },
        {
          "id": "MDRW2100036301.1.27",
          "form": "적립은 안되고!",
          "original_form": "적립은 안되고!",
          "speaker_id": "2",
          "time": "20210831 15:35"
        },
        {
          "id": "MDRW2100036301.1.28",
          "form": "ㅋㅋ현수막은 그럼 그떄마다 다른거야?",
          "original_form": "ㅋㅋ현수막은 그럼 그떄마다 다른거야?",
          "speaker_id": "1",
          "time": "20210831 15:35"
        },
        {
          "id": "MDRW2100036301.1.29",
          "form": "걸린날이 있고 없고?",
          "original_form": "걸린날이 있고 없고?",
          "speaker_id": "1",
          "time": "20210831 15:35"
        },
        {
          "id": "MDRW2100036301.1.30",
          "form": "베너가 잇다없다하는거같아 ㅋㅋㅋ",
          "original_form": "베너가 잇다없다하는거같아 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:35"
        },
        {
          "id": "MDRW2100036301.1.31",
          "form": "그컴포즈옆에 떡집생김",
          "original_form": "그컴포즈옆에 떡집생김",
          "speaker_id": "2",
          "time": "20210831 15:36"
        },
        {
          "id": "MDRW2100036301.1.32",
          "form": "ㅋㅋ오픈했나요 ?",
          "original_form": "ㅋㅋ오픈했나요 ?",
          "speaker_id": "1",
          "time": "20210831 15:36"
        },
        {
          "id": "MDRW2100036301.1.33",
          "form": "공사중 !! ㅋㅋㅋ9월에한데",
          "original_form": "공사중 !! ㅋㅋㅋ9월에한데",
          "speaker_id": "2",
          "time": "20210831 15:36"
        },
