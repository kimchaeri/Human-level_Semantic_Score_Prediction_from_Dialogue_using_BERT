      "utterance": [
        {
          "id": "MDRW2100042963.1.1",
          "form": "안녕하세뇨! 게임 하고 있는 거 있으신가요?",
          "original_form": "안녕하세뇨! 게임 하고 있는 거 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 12:06"
        },
        {
          "id": "MDRW2100042963.1.2",
          "form": "안녕하세요 저는 요즘 오버워치를 해요",
          "original_form": "안녕하세요 저는 요즘 오버워치를 해요",
          "speaker_id": "2",
          "time": "20210907 12:07"
        },
        {
          "id": "MDRW2100042963.1.3",
          "form": "name1님도 오버워치 하시나요?아니면 다른거?",
          "original_form": "&name1&님도 오버워치 하시나요?아니면 다른거?",
          "speaker_id": "2",
          "time": "20210907 12:07"
        },
        {
          "id": "MDRW2100042963.1.4",
          "form": "헉 저는 사람들이 많이 하는 거 잘 안 하게 되더라구요ㅋㅋ",
          "original_form": "헉 저는 사람들이 많이 하는 거 잘 안 하게 되더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 12:08"
        },
        {
          "id": "MDRW2100042963.1.5",
          "form": "간잽만 하다 끝났습니다..",
          "original_form": "간잽만 하다 끝났습니다..",
          "speaker_id": "1",
          "time": "20210907 12:08"
        },
        {
          "id": "MDRW2100042963.1.6",
          "form": "ㅋㅋㅋㅋㄱ아ㅠ마이너의 가혹한운명인가요",
          "original_form": "ㅋㅋㅋㅋㄱ아ㅠ마이너의 가혹한운명인가요",
          "speaker_id": "2",
          "time": "20210907 12:09"
        },
        {
          "id": "MDRW2100042963.1.7",
          "form": "그럼 다른겜 안하시나요",
          "original_form": "그럼 다른겜 안하시나요",
          "speaker_id": "2",
          "time": "20210907 12:09"
        },
        {
          "id": "MDRW2100042963.1.8",
          "form": "ㅋㅋ테런 가끔씩 해요ㅋㅋㅋ",
          "original_form": "ㅋㅋ테런 가끔씩 해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 12:12"
        },
        {
          "id": "MDRW2100042963.1.9",
          "form": "ㅋㅋㅋㅋ추억의겜이죠",
          "original_form": "ㅋㅋㅋㅋ추억의겜이죠",
          "speaker_id": "2",
          "time": "20210907 12:14"
        },
        {
          "id": "MDRW2100042963.1.10",
          "form": "저는 옵치하기전에",
          "original_form": "저는 옵치하기전에",
          "speaker_id": "2",
          "time": "20210907 12:14"
        },
        {
          "id": "MDRW2100042963.1.11",
          "form": "테런에 인생을 갈아넣고",
          "original_form": "테런에 인생을 갈아넣고",
          "speaker_id": "2",
          "time": "20210907 12:15"
        },
        {
          "id": "MDRW2100042963.1.12",
          "form": "했어요.,",
          "original_form": "했어요.,",
          "speaker_id": "2",
          "time": "20210907 12:15"
        },
        {
          "id": "MDRW2100042963.1.13",
          "form": "테런 누구좋아하시나요",
          "original_form": "테런 누구좋아하시나요",
          "speaker_id": "2",
          "time": "20210907 12:15"
        },
        {
          "id": "MDRW2100042963.1.14",
          "form": "아악ㅋㅋㅋㅋㅋㅋㅋㅋ테런...재밌죠...",
          "original_form": "아악ㅋㅋㅋㅋㅋㅋㅋㅋ테런...재밌죠...",
          "speaker_id": "1",
          "time": "20210907 12:20"
        },
        {
          "id": "MDRW2100042963.1.15",
          "form": "베라 좋아해요,,//",
          "original_form": "베라 좋아해요,,//",
          "speaker_id": "1",
          "time": "20210907 12:20"
        },
        {
          "id": "MDRW2100042963.1.16",
          "form": "ㅋㅋㅋㅋㄱ베라너무귀여워요",
          "original_form": "ㅋㅋㅋㅋㄱ베라너무귀여워요",
          "speaker_id": "2",
          "time": "20210907 12:21"
        },
        {
          "id": "MDRW2100042963.1.17",
          "form": "그 리뉴얼?변신?",
          "original_form": "그 리뉴얼?변신?",
          "speaker_id": "2",
          "time": "20210907 12:21"
        },
        {
          "id": "MDRW2100042963.1.18",
          "form": "그때진심, .아 이겜..갓겜이네ㅋㅋㅋ",
          "original_form": "그때진심, .아 이겜..갓겜이네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:22"
        },
        {
          "id": "MDRW2100042963.1.19",
          "form": "햇네요",
          "original_form": "햇네요",
          "speaker_id": "2",
          "time": "20210907 12:22"
        },
        {
          "id": "MDRW2100042963.1.20",
          "form": "맞아요ㅋㅋㅋㅋ 그 뱀파이어였나 짱 예쁘더라구요",
          "original_form": "맞아요ㅋㅋㅋㅋ 그 뱀파이어였나 짱 예쁘더라구요",
          "speaker_id": "1",
          "time": "20210907 12:22"
        },
        {
          "id": "MDRW2100042963.1.21",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210907 12:26"
        },
        {
          "id": "MDRW2100042963.1.22",
          "form": "진짜 모델링하나는",
          "original_form": "진짜 모델링하나는",
          "speaker_id": "2",
          "time": "20210907 12:27"
        },
        {
          "id": "MDRW2100042963.1.23",
          "form": "깔쌈하게 하는것같아요",
          "original_form": "깔쌈하게 하는것같아요",
          "speaker_id": "2",
          "time": "20210907 12:27"
        },
        {
          "id": "MDRW2100042963.1.24",
          "form": "맞아요ㅋㅋ 요즘 일러도 진짜 예쁘더라구요",
          "original_form": "맞아요ㅋㅋ 요즘 일러도 진짜 예쁘더라구요",
          "speaker_id": "1",
          "time": "20210907 12:30"
        },
        {
          "id": "MDRW2100042963.1.25",
          "form": "이번에 리오넬?",
          "original_form": "이번에 리오넬?",
          "speaker_id": "2",
          "time": "20210907 12:34"
        },
        {
          "id": "MDRW2100042963.1.26",
          "form": "그광공캐너무 웃기고 잘생겻어요",
          "original_form": "그광공캐너무 웃기고 잘생겻어요",
          "speaker_id": "2",
          "time": "20210907 12:34"
        },
        {
          "id": "MDRW2100042963.1.27",
          "form": "러빗",
          "original_form": "러빗",
          "speaker_id": "2",
          "time": "20210907 12:34"
        },
        {
          "id": "MDRW2100042963.1.28",
          "form": "아ㅠㅠㅜㅠ걔ㅠㅠㅠㅠ너무 웃겨요.. 근데 잘생겼더라구요.. 플레이를 안 해봤네요",
          "original_form": "아ㅠㅠㅜㅠ걔ㅠㅠㅠㅠ너무 웃겨요.. 근데 잘생겼더라구요.. 플레이를 안 해봤네요",
          "speaker_id": "1",
          "time": "20210907 12:38"
        },
        {
          "id": "MDRW2100042963.1.29",
          "form": "ㅋㅋㅋㅋㄱ전걍 점프하는거보고",
          "original_form": "ㅋㅋㅋㅋㄱ전걍 점프하는거보고",
