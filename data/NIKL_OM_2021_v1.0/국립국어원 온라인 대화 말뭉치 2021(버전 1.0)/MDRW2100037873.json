      "utterance": [
        {
          "id": "MDRW2100037873.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210901 20:18"
        },
        {
          "id": "MDRW2100037873.1.2",
          "form": "넹 안녕하세요",
          "original_form": "넹 안녕하세요",
          "speaker_id": "2",
          "time": "20210901 20:18"
        },
        {
          "id": "MDRW2100037873.1.3",
          "form": "요즘 사신 거 있으세요?",
          "original_form": "요즘 사신 거 있으세요?",
          "speaker_id": "2",
          "time": "20210901 20:18"
        },
        {
          "id": "MDRW2100037873.1.4",
          "form": "먹을거 샀어요",
          "original_form": "먹을거 샀어요",
          "speaker_id": "1",
          "time": "20210901 20:19"
        },
        {
          "id": "MDRW2100037873.1.5",
          "form": "식자재하고 과일 올리브유 샀어요ㅎㅎ",
          "original_form": "식자재하고 과일 올리브유 샀어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 20:19"
        },
        {
          "id": "MDRW2100037873.1.6",
          "form": "오 요리하세요?",
          "original_form": "오 요리하세요?",
          "speaker_id": "2",
          "time": "20210901 20:19"
        },
        {
          "id": "MDRW2100037873.1.7",
          "form": "우와 마트 다녀오셨나봐요",
          "original_form": "우와 마트 다녀오셨나봐요",
          "speaker_id": "2",
          "time": "20210901 20:19"
        },
        {
          "id": "MDRW2100037873.1.8",
          "form": "홈플러스에서 온라인 첫구매하면",
          "original_form": "홈플러스에서 온라인 첫구매하면",
          "speaker_id": "1",
          "time": "20210901 20:19"
        },
        {
          "id": "MDRW2100037873.1.9",
          "form": "4만원사면 2만원 할인해주더라구요",
          "original_form": "4만원사면 2만원 할인해주더라구요",
          "speaker_id": "1",
          "time": "20210901 20:20"
        },
        {
          "id": "MDRW2100037873.1.10",
          "form": "온라인구매했어요ㅎㅎ",
          "original_form": "온라인구매했어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 20:20"
        },
        {
          "id": "MDRW2100037873.1.11",
          "form": "낮에시켰는데 7시에 받았어요",
          "original_form": "낮에시켰는데 7시에 받았어요",
          "speaker_id": "1",
          "time": "20210901 20:20"
        },
        {
          "id": "MDRW2100037873.1.12",
          "form": "헉 대박 저는 이마트만 이용하거든요~ 유플러스 인터넷 신규가입하면 십몇만원 상품권 줘요",
          "original_form": "헉 대박 저는 이마트만 이용하거든요~ 유플러스 인터넷 신규가입하면 십몇만원 상품권 줘요",
          "speaker_id": "2",
          "time": "20210901 20:20"
        },
        {
          "id": "MDRW2100037873.1.13",
          "form": "그거 쓰느라 요즘 바빠요",
          "original_form": "그거 쓰느라 요즘 바빠요",
          "speaker_id": "2",
          "time": "20210901 20:20"
        },
        {
          "id": "MDRW2100037873.1.14",
          "form": "ㅎㅎ 이마트상품권 받았어요?",
          "original_form": "ㅎㅎ 이마트상품권 받았어요?",
          "speaker_id": "1",
          "time": "20210901 20:20"
        },
        {
          "id": "MDRW2100037873.1.15",
          "form": "네 홈플이랑 이마트 고르라는데 이마트 골랐어요 가까워서ㅋㅋ",
          "original_form": "네 홈플이랑 이마트 고르라는데 이마트 골랐어요 가까워서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:21"
        },
        {
          "id": "MDRW2100037873.1.16",
          "form": "가까운게최고죠",
          "original_form": "가까운게최고죠",
          "speaker_id": "1",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100037873.1.17",
          "form": "요즘 뭐사셨어요?",
          "original_form": "요즘 뭐사셨어요?",
          "speaker_id": "1",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100037873.1.18",
          "form": "홈플러스는 너무 멀어서 안 가게 되더라구요~ 마트들이 다 뻔해서 가까운 곳만 가게 돼요",
          "original_form": "홈플러스는 너무 멀어서 안 가게 되더라구요~ 마트들이 다 뻔해서 가까운 곳만 가게 돼요",
          "speaker_id": "2",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100037873.1.19",
          "form": "추천템 있나요?",
          "original_form": "추천템 있나요?",
          "speaker_id": "1",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100037873.1.20",
          "form": "저 오늘 책상 샀어요ㅋㅋ 이사왔거든요",
          "original_form": "저 오늘 책상 샀어요ㅋㅋ 이사왔거든요",
          "speaker_id": "2",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100037873.1.21",
          "form": "책상 컴프프로 샀어요 각도조절 되는 걸루",
          "original_form": "책상 컴프프로 샀어요 각도조절 되는 걸루",
          "speaker_id": "2",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100037873.1.22",
          "form": "ㅎㅎ 각도가조절돼요??",
          "original_form": "ㅎㅎ 각도가조절돼요??",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100037873.1.23",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100037873.1.24",
          "form": "혹시 높이도 조절돼요?",
          "original_form": "혹시 높이도 조절돼요?",
          "speaker_id": "1",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100037873.1.25",
          "form": "네네! 그게 대만에서 들여와서 특허받은거라",
          "original_form": "네네! 그게 대만에서 들여와서 특허받은거라",
          "speaker_id": "2",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100037873.1.26",
          "form": "거기서만 파는 거라나 아무튼 그래서 거기서 샀어요 ㅋㅋ",
          "original_form": "거기서만 파는 거라나 아무튼 그래서 거기서 샀어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100037873.1.27",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100037873.1.28",
          "form": "신기하군요",
          "original_form": "신기하군요",
          "speaker_id": "1",
          "time": "20210901 20:26"
        },
        {
          "id": "MDRW2100037873.1.29",
          "form": "이사축하해요! 새로운곳에서 좋은일 많이 있기른 바래요",
          "original_form": "이사축하해요! 새로운곳에서 좋은일 많이 있기른 바래요",
          "speaker_id": "1",
          "time": "20210901 20:27"
        },
        {
          "id": "MDRW2100037873.1.30",
          "form": "원래 책상은 식탁으로 쓰던 거라 불편해서 하나 샀는데 좋더라구요ㅋㅋ",
          "original_form": "원래 책상은 식탁으로 쓰던 거라 불편해서 하나 샀는데 좋더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:27"
        },
        {
          "id": "MDRW2100037873.1.31",
          "form": "감사합니다 ㅎㅎ 이사하니까 솔직히 다 바꾸고 싶어서 엄청 사요 ㅋㅋ",
          "original_form": "감사합니다 ㅎㅎ 이사하니까 솔직히 다 바꾸고 싶어서 엄청 사요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:27"
        },
        {
          "id": "MDRW2100037873.1.32",
          "form": "ㅎㅎ 좋으시겠어요",
          "original_form": "ㅎㅎ 좋으시겠어요",
          "speaker_id": "1",
          "time": "20210901 20:27"
        },
        {
          "id": "MDRW2100037873.1.33",
          "form": "새 물건들이랑 같이 있으니 더 좋은 것 같아요ㅋㅋㅋ",
          "original_form": "새 물건들이랑 같이 있으니 더 좋은 것 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:27"
        },
        {
          "id": "MDRW2100037873.1.34",
          "form": "자취하시는거예요?",
          "original_form": "자취하시는거예요?",
          "speaker_id": "1",
          "time": "20210901 20:27"
        },
        {
          "id": "MDRW2100037873.1.35",
          "form": "인테리어도해야죠ㅋㅋ집꾸미기",
          "original_form": "인테리어도해야죠ㅋㅋ집꾸미기",
          "speaker_id": "1",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100037873.1.36",
          "form": "원래 자취했는데 다시 본가쪽으로 왔어요~",
          "original_form": "원래 자취했는데 다시 본가쪽으로 왔어요~",
          "speaker_id": "2",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100037873.1.37",
          "form": "내공간있다는게 진짜 좋은것같아요",
          "original_form": "내공간있다는게 진짜 좋은것같아요",
          "speaker_id": "1",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100037873.1.38",
          "form": "아 인테리어는 진짜 골아프더라구요.. 하려니까ㅠㅠ",
          "original_form": "아 인테리어는 진짜 골아프더라구요.. 하려니까ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100037873.1.39",
          "form": "맞아요 내 방이 있어야 든든하고 내가 살 것도 사게 되고 그래요",
          "original_form": "맞아요 내 방이 있어야 든든하고 내가 살 것도 사게 되고 그래요",
          "speaker_id": "2",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100037873.1.40",
          "form": "ㅎㅎ어떤스타일 좋아해요?",
          "original_form": "ㅎㅎ어떤스타일 좋아해요?",
          "speaker_id": "1",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100037873.1.41",
          "form": "저는 심플 모던 스타일이좋더라구요",
          "original_form": "저는 심플 모던 스타일이좋더라구요",
          "speaker_id": "1",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100037873.1.42",
          "form": "저도 그냥 무던한게 좋더라고요~~",
          "original_form": "저도 그냥 무던한게 좋더라고요~~",
          "speaker_id": "2",
          "time": "20210901 20:29"
        }
      ]
