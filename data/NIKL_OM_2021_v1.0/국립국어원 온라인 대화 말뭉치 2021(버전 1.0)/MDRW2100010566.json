      "utterance": [
        {
          "id": "MDRW2100010566.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210823 19:39"
        },
        {
          "id": "MDRW2100010566.1.2",
          "form": "좋아하는 음식 있으신가요?",
          "original_form": "좋아하는 음식 있으신가요?",
          "speaker_id": "1",
          "time": "20210823 19:39"
        },
        {
          "id": "MDRW2100010566.1.3",
          "form": "저 짬뽕 좋아해요",
          "original_form": "저 짬뽕 좋아해요",
          "speaker_id": "2",
          "time": "20210823 19:40"
        },
        {
          "id": "MDRW2100010566.1.4",
          "form": "아 짜장파 짬뽕파 중에 짬뽕파시군요",
          "original_form": "아 짜장파 짬뽕파 중에 짬뽕파시군요",
          "speaker_id": "1",
          "time": "20210823 19:40"
        },
        {
          "id": "MDRW2100010566.1.5",
          "form": "짬뽕 얼큰하고 맛있어서 좋은 것같아요",
          "original_form": "짬뽕 얼큰하고 맛있어서 좋은 것같아요",
          "speaker_id": "1",
          "time": "20210823 19:41"
        },
        {
          "id": "MDRW2100010566.1.6",
          "form": "맛아요 얼큰한게 최고!!",
          "original_form": "맛아요 얼큰한게 최고!!",
          "speaker_id": "2",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010566.1.7",
          "form": "name1은 짬뽕파?짜장파?",
          "original_form": "&name1&은 짬뽕파?짜장파?",
          "speaker_id": "2",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010566.1.8",
          "form": "저는...결정장애라...",
          "original_form": "저는...결정장애라...",
          "speaker_id": "1",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010566.1.9",
          "form": "맨날 고민하다 짬짜면 시켜요",
          "original_form": "맨날 고민하다 짬짜면 시켜요",
          "speaker_id": "1",
          "time": "20210823 19:43"
        },
        {
          "id": "MDRW2100010566.1.10",
          "form": "왘ㅋㅋ 제일 좋네요",
          "original_form": "왘ㅋㅋ 제일 좋네요",
          "speaker_id": "2",
          "time": "20210823 19:43"
        },
        {
          "id": "MDRW2100010566.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ근데 왠지 짬짜면은 양이 적은 것같아서 아쉬워요ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ근데 왠지 짬짜면은 양이 적은 것같아서 아쉬워요ㅠ",
          "speaker_id": "1",
          "time": "20210823 19:44"
        },
        {
          "id": "MDRW2100010566.1.12",
          "form": "그래서 한 사람 더 모아서 짜장 짬뽕 하나씩 시킬때도 있어요ㅋㅋㅋㅋ",
          "original_form": "그래서 한 사람 더 모아서 짜장 짬뽕 하나씩 시킬때도 있어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 19:44"
        },
        {
          "id": "MDRW2100010566.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 잘드시는 편이군요!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 잘드시는 편이군요!",
          "speaker_id": "2",
          "time": "20210823 19:45"
        },
        {
          "id": "MDRW2100010566.1.14",
          "form": "두명이서 다른거 하나씩 시키는게 최고죠",
          "original_form": "두명이서 다른거 하나씩 시키는게 최고죠",
          "speaker_id": "2",
          "time": "20210823 19:45"
        },
        {
          "id": "MDRW2100010566.1.15",
          "form": "둘다 너무 맛있어요ㅠㅋㅋㅋㅋㅋ",
          "original_form": "둘다 너무 맛있어요ㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 19:45"
        },
        {
          "id": "MDRW2100010566.1.16",
          "form": "저는 면류면 다 좋아해요",
          "original_form": "저는 면류면 다 좋아해요",
          "speaker_id": "1",
          "time": "20210823 19:46"
        },
        {
          "id": "MDRW2100010566.1.17",
          "form": "콩국수 빼고!",
          "original_form": "콩국수 빼고!",
          "speaker_id": "1",
          "time": "20210823 19:46"
        },
        {
          "id": "MDRW2100010566.1.18",
          "form": "아! 저도 콩국수 별로예요",
          "original_form": "아! 저도 콩국수 별로예요",
          "speaker_id": "2",
          "time": "20210823 19:47"
        },
        {
          "id": "MDRW2100010566.1.19",
          "form": "그리고 팥칼국수도! 팥 별로 안좋아하거든요",
          "original_form": "그리고 팥칼국수도! 팥 별로 안좋아하거든요",
          "speaker_id": "2",
          "time": "20210823 19:48"
        },
        {
          "id": "MDRW2100010566.1.20",
          "form": "오오 저두요",
          "original_form": "오오 저두요",
          "speaker_id": "1",
          "time": "20210823 19:49"
        },
        {
          "id": "MDRW2100010566.1.21",
          "form": "팥 붕어빵까진 수용 가능인데",
          "original_form": "팥 붕어빵까진 수용 가능인데",
          "speaker_id": "1",
          "time": "20210823 19:49"
        },
        {
          "id": "MDRW2100010566.1.22",
          "form": "팥칼국수는 별루",
          "original_form": "팥칼국수는 별루",
          "speaker_id": "1",
          "time": "20210823 19:50"
        },
        {
          "id": "MDRW2100010566.1.23",
          "form": "와 저도요! 간식같은 붕어빵은 괜찮은데 식사류는 별로더라고요",
          "original_form": "와 저도요! 간식같은 붕어빵은 괜찮은데 식사류는 별로더라고요",
          "speaker_id": "2",
          "time": "20210823 19:51"
        },
        {
          "id": "MDRW2100010566.1.24",
          "form": "팥죽이나 호박죽도 별로",
          "original_form": "팥죽이나 호박죽도 별로",
          "speaker_id": "2",
          "time": "20210823 19:51"
        },
        {
          "id": "MDRW2100010566.1.25",
          "form": "전 죽 중에 전복죽이 젤 좋아요",
          "original_form": "전 죽 중에 전복죽이 젤 좋아요",
          "speaker_id": "1",
          "time": "20210823 19:52"
        },
        {
          "id": "MDRW2100010566.1.26",
          "form": "전복죽 맛있죠!",
          "original_form": "전복죽 맛있죠!",
          "speaker_id": "2",
          "time": "20210823 21:30"
        },
        {
          "id": "MDRW2100010566.1.27",
          "form": "죽얘기하니까 먹고 싶어졌어요",
          "original_form": "죽얘기하니까 먹고 싶어졌어요",
          "speaker_id": "2",
          "time": "20210823 21:30"
        },
        {
          "id": "MDRW2100010566.1.28",
          "form": "ㅋㅋㅋㅋㅋ입맛없을때 죽먹으면 입맛이 살아나는 것같아요",
          "original_form": "ㅋㅋㅋㅋㅋ입맛없을때 죽먹으면 입맛이 살아나는 것같아요",
          "speaker_id": "1",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100010566.1.29",
          "form": "죽 먹기 편해서 좋아요 소화도 잘되고 ㅋㅋ",
          "original_form": "죽 먹기 편해서 좋아요 소화도 잘되고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 12:43"
        },
        {
          "id": "MDRW2100010566.1.30",
          "form": "죽 만들어드시는 편은 아닌가요?",
          "original_form": "죽 만들어드시는 편은 아닌가요?",
          "speaker_id": "2",
          "time": "20210824 12:43"
        },
        {
          "id": "MDRW2100010566.1.31",
          "form": "못나가는 상황이거나 부모님이 집에 있으실땐",
          "original_form": "못나가는 상황이거나 부모님이 집에 있으실땐",
          "speaker_id": "1",
          "time": "20210824 12:45"
        },
        {
          "id": "MDRW2100010566.1.32",
          "form": "만들어먹긴 하는데 사와서 먹는게 더 맛있더라고요ㅋㅋㅋ",
          "original_form": "만들어먹긴 하는데 사와서 먹는게 더 맛있더라고요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:45"
        },
        {
          "id": "MDRW2100010566.1.33",
          "form": "아 그건 맞아요",
          "original_form": "아 그건 맞아요",
          "speaker_id": "2",
          "time": "20210824 12:48"
        },
        {
          "id": "MDRW2100010566.1.34",
          "form": "만들어 먹는건 뭔가 좀 부족한 느낌",
          "original_form": "만들어 먹는건 뭔가 좀 부족한 느낌",
          "speaker_id": "2",
          "time": "20210824 12:48"
        }
      ]
    }
