      "utterance": [
        {
          "id": "MDRW2100033553.1.1",
          "form": "이혼하고 부모님집에 합가하려했는데 엄마가 튕군 남자 썰 읽음 아까",
          "original_form": "이혼하고 부모님집에 합가하려했는데 엄마가 튕군 남자 썰 읽음 아까",
          "speaker_id": "2",
          "time": "20210824 13:56"
        },
        {
          "id": "MDRW2100033553.1.2",
          "form": "엄마가 다시 아들 밥차리고 빨래해야하니 기찮겠지",
          "original_form": "엄마가 다시 아들 밥차리고 빨래해야하니 기찮겠지",
          "speaker_id": "2",
          "time": "20210824 13:56"
        },
        {
          "id": "MDRW2100033553.1.3",
          "form": "허허 괜찮은 남자들도 많을거야 세상에는 ㅜㅜ",
          "original_form": "허허 괜찮은 남자들도 많을거야 세상에는 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 14:03"
        },
        {
          "id": "MDRW2100033553.1.4",
          "form": "꼭 그런사람 말고ㅜ",
          "original_form": "꼭 그런사람 말고ㅜ",
          "speaker_id": "1",
          "time": "20210824 14:03"
        },
        {
          "id": "MDRW2100033553.1.5",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210824 14:06"
        },
        {
          "id": "MDRW2100033553.1.6",
          "form": "좋은 파트너가 있을꺼야",
          "original_form": "좋은 파트너가 있을꺼야",
          "speaker_id": "2",
          "time": "20210824 14:07"
        },
        {
          "id": "MDRW2100033553.1.7",
          "form": "평소에 그 아들이 엄마를 별로 안도와줬나봄",
          "original_form": "평소에 그 아들이 엄마를 별로 안도와줬나봄",
          "speaker_id": "1",
          "time": "20210824 14:11"
        },
        {
          "id": "MDRW2100033553.1.8",
          "form": "아님 엄마가 혼자 사는게 좋으셨던가 ㅋㅋ",
          "original_form": "아님 엄마가 혼자 사는게 좋으셨던가 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:13"
        },
        {
          "id": "MDRW2100033553.1.9",
          "form": "아들도 알아서 앞가림해야지 모 ㅎㅎㅎ",
          "original_form": "아들도 알아서 앞가림해야지 모 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100033553.1.10",
          "form": "엄마힘들오ㅜㅜ",
          "original_form": "엄마힘들오ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100033553.1.11",
          "form": "또 엄마랑 딸이랑 엄마랑 아들은 다를지도요",
          "original_form": "또 엄마랑 딸이랑 엄마랑 아들은 다를지도요",
          "speaker_id": "1",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100033553.1.12",
          "form": "음 이혼한 부인은 친가 들어갔다구하네 애들 데리고",
          "original_form": "음 이혼한 부인은 친가 들어갔다구하네 애들 데리고",
          "speaker_id": "2",
          "time": "20210824 14:24"
        },
        {
          "id": "MDRW2100033553.1.13",
          "form": "받아주나바 ㅋㅋ",
          "original_form": "받아주나바 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100033553.1.14",
          "form": "애초에 정말 좋은 남자면 이혼 안했을거임 ㅋㅋ",
          "original_form": "애초에 정말 좋은 남자면 이혼 안했을거임 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:26"
        },
        {
          "id": "MDRW2100033553.1.15",
          "form": "뻔한거지만",
          "original_form": "뻔한거지만",
          "speaker_id": "1",
          "time": "20210824 14:26"
        },
        {
          "id": "MDRW2100033553.1.16",
          "form": "근데 그건 누가 올린거임 사연?",
          "original_form": "근데 그건 누가 올린거임 사연?",
          "speaker_id": "1",
          "time": "20210824 14:26"
        },
        {
          "id": "MDRW2100033553.1.17",
          "form": "ㅇㅇ사연읽음",
          "original_form": "ㅇㅇ사연읽음",
          "speaker_id": "2",
          "time": "20210824 14:28"
        },
        {
          "id": "MDRW2100033553.1.18",
          "form": "양육비 매달 30 만원주기로했대",
          "original_form": "양육비 매달 30 만원주기로했대",
          "speaker_id": "2",
          "time": "20210824 14:28"
        },
        {
          "id": "MDRW2100033553.1.19",
          "form": "애가 둘인데",
          "original_form": "애가 둘인데",
          "speaker_id": "2",
          "time": "20210824 14:28"
        },
        {
          "id": "MDRW2100033553.1.20",
          "form": "아들 본인이 사연보낸거? 엄마가보낸거?",
          "original_form": "아들 본인이 사연보낸거? 엄마가보낸거?",
          "speaker_id": "1",
          "time": "20210824 14:29"
        },
        {
          "id": "MDRW2100033553.1.21",
          "form": "아들이 자기 붕 떳다고 하소연하는 글이었음ㅋ",
          "original_form": "아들이 자기 붕 떳다고 하소연하는 글이었음ㅋ",
          "speaker_id": "2",
          "time": "20210824 14:29"
        },
        {
          "id": "MDRW2100033553.1.22",
          "form": "아니 하소연 할거면 엄마가 거둬가 ㅋㅋ",
          "original_form": "아니 하소연 할거면 엄마가 거둬가 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:32"
        },
        {
          "id": "MDRW2100033553.1.23",
          "form": "웃기네 ㅋㅋㅋ",
          "original_form": "웃기네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:32"
        },
        {
          "id": "MDRW2100033553.1.24",
          "form": "아내는 친가갔는데 자기는 합가인한다고 혼자됐다고 징징글이었음",
          "original_form": "아내는 친가갔는데 자기는 합가인한다고 혼자됐다고 징징글이었음",
          "speaker_id": "2",
          "time": "20210824 14:34"
        },
        {
          "id": "MDRW2100033553.1.25",
          "form": "아 아들이 하소연했다는거네",
          "original_form": "아 아들이 하소연했다는거네",
          "speaker_id": "1",
          "time": "20210824 15:37"
        },
        {
          "id": "MDRW2100033553.1.26",
          "form": "지가 사연올리고",
          "original_form": "지가 사연올리고",
          "speaker_id": "1",
          "time": "20210824 15:37"
        },
        {
          "id": "MDRW2100033553.1.27",
          "form": "그게 쪽팔릴일인줄도 모르니",
          "original_form": "그게 쪽팔릴일인줄도 모르니",
          "speaker_id": "1",
          "time": "20210824 15:38"
        },
        {
          "id": "MDRW2100033553.1.28",
          "form": "이혼당하는거임",
          "original_form": "이혼당하는거임",
          "speaker_id": "1",
          "time": "20210824 15:38"
        },
        {
          "id": "MDRW2100033553.1.29",
          "form": "내동생 드디어 애인생김!",
          "original_form": "내동생 드디어 애인생김!",
          "speaker_id": "1",
          "time": "20210828 17:29"
        }
      ]
