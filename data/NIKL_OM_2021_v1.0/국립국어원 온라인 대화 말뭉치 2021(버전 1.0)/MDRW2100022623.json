      "utterance": [
        {
          "id": "MDRW2100022623.1.1",
          "form": "곰표 상품 사보셨어요?",
          "original_form": "곰표 상품 사보셨어요?",
          "speaker_id": "1",
          "time": "20210829 16:26"
        },
        {
          "id": "MDRW2100022623.1.2",
          "form": "팝콘만 먹어봤어요! ㅋㅋ",
          "original_form": "팝콘만 먹어봤어요! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:27"
        },
        {
          "id": "MDRW2100022623.1.3",
          "form": "아하 저는 곰표젤리 먹어봤어요 ㅎㅎ",
          "original_form": "아하 저는 곰표젤리 먹어봤어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 16:30"
        },
        {
          "id": "MDRW2100022623.1.4",
          "form": "그거 먹어보고싶었는데! 민트 쎄요?",
          "original_form": "그거 먹어보고싶었는데! 민트 쎄요?",
          "speaker_id": "2",
          "time": "20210829 16:32"
        },
        {
          "id": "MDRW2100022623.1.5",
          "form": "네 민트쎄요 ㅋㅋㅋㅋ",
          "original_form": "네 민트쎄요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:33"
        },
        {
          "id": "MDRW2100022623.1.6",
          "form": "민트맛만 나요?",
          "original_form": "민트맛만 나요?",
          "speaker_id": "2",
          "time": "20210829 16:34"
        },
        {
          "id": "MDRW2100022623.1.7",
          "form": "넹넹 ㅎㅎㅎ",
          "original_form": "넹넹 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 16:35"
        },
        {
          "id": "MDRW2100022623.1.8",
          "form": "여튼 좀 세요 ㅎㅎ",
          "original_form": "여튼 좀 세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 16:36"
        },
        {
          "id": "MDRW2100022623.1.9",
          "form": "곰표나쵸도 맛있다던데!",
          "original_form": "곰표나쵸도 맛있다던데!",
          "speaker_id": "2",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022623.1.10",
          "form": "오호 그렇군요",
          "original_form": "오호 그렇군요",
          "speaker_id": "1",
          "time": "20210829 16:42"
        },
        {
          "id": "MDRW2100022623.1.11",
          "form": "근데 그뒤에 구두표 맥주",
          "original_form": "근데 그뒤에 구두표 맥주",
          "speaker_id": "1",
          "time": "20210829 16:42"
        },
        {
          "id": "MDRW2100022623.1.12",
          "form": "매직 음료수",
          "original_form": "매직 음료수",
          "speaker_id": "1",
          "time": "20210829 16:42"
        },
        {
          "id": "MDRW2100022623.1.13",
          "form": "이런건 개오바같아요",
          "original_form": "이런건 개오바같아요",
          "speaker_id": "1",
          "time": "20210829 16:42"
        },
        {
          "id": "MDRW2100022623.1.14",
          "form": "아 진짜 그런건 사고위험있어보여요 ㅜㅜ",
          "original_form": "아 진짜 그런건 사고위험있어보여요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 16:42"
        },
        {
          "id": "MDRW2100022623.1.15",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210829 16:43"
        },
        {
          "id": "MDRW2100022623.1.16",
          "form": "너무 매출만 생각한거같아요",
          "original_form": "너무 매출만 생각한거같아요",
          "speaker_id": "1",
          "time": "20210829 16:43"
        },
        {
          "id": "MDRW2100022623.1.17",
          "form": "매출은 잘 나오나 싶긴해요 ㅋㅋㅋ 사먹는사람 못봤어요 ㅋㅋㅋ",
          "original_form": "매출은 잘 나오나 싶긴해요 ㅋㅋㅋ 사먹는사람 못봤어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022623.1.18",
          "form": "악인이 망한거 보는거마냥ㄱ",
          "original_form": "악인이 망한거 보는거마냥ㄱ",
          "speaker_id": "1",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022623.1.19",
          "form": "기분좋네요",
