      "utterance": [
        {
          "id": "MDRW2100041737.1.1",
          "form": "안녕하세요~~ 점심 드셨나요 ㅎㅎ",
          "original_form": "안녕하세요~~ 점심 드셨나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 15:14"
        },
        {
          "id": "MDRW2100041737.1.2",
          "form": "앗 오랜만이에요 name1님!!!",
          "original_form": "앗 오랜만이에요 &name1&님!!!",
          "speaker_id": "2",
          "time": "20210906 15:16"
        },
        {
          "id": "MDRW2100041737.1.3",
          "form": "네네 저는 오늘도 도시락을 먹었답니다!",
          "original_form": "네네 저는 오늘도 도시락을 먹었답니다!",
          "speaker_id": "2",
          "time": "20210906 15:16"
        },
        {
          "id": "MDRW2100041737.1.4",
          "form": "두부랑... 콩나물 무침을 먹었어요~~~",
          "original_form": "두부랑... 콩나물 무침을 먹었어요~~~",
          "speaker_id": "2",
          "time": "20210906 15:16"
        },
        {
          "id": "MDRW2100041737.1.5",
          "form": "해일님은요?",
          "original_form": "해일님은요?",
          "speaker_id": "2",
          "time": "20210906 15:16"
        },
        {
          "id": "MDRW2100041737.1.6",
          "form": "저는 짜장밥 먹었어요ㅋㅋㅋ 지금은 백신 맞으러 왔어요! 두근두근",
          "original_form": "저는 짜장밥 먹었어요ㅋㅋㅋ 지금은 백신 맞으러 왔어요! 두근두근",
          "speaker_id": "1",
          "time": "20210906 15:16"
        },
        {
          "id": "MDRW2100041737.1.7",
          "form": "집가는길에 빵사갈 생각에 설렙니다 후후",
          "original_form": "집가는길에 빵사갈 생각에 설렙니다 후후",
          "speaker_id": "1",
          "time": "20210906 15:17"
        },
        {
          "id": "MDRW2100041737.1.8",
          "form": "와 그러면",
          "original_form": "와 그러면",
          "speaker_id": "2",
          "time": "20210906 15:25"
        },
        {
          "id": "MDRW2100041737.1.9",
          "form": "백신 맞고 빵 사갖고 퇴근하시는거네요!!!",
          "original_form": "백신 맞고 빵 사갖고 퇴근하시는거네요!!!",
          "speaker_id": "2",
          "time": "20210906 15:25"
        },
        {
          "id": "MDRW2100041737.1.10",
          "form": "무슨 빵 사시나요?!?!",
          "original_form": "무슨 빵 사시나요?!?!",
          "speaker_id": "2",
          "time": "20210906 15:25"
        },
        {
          "id": "MDRW2100041737.1.11",
          "form": "저 아마 제가 좋아하는",
          "original_form": "저 아마 제가 좋아하는",
          "speaker_id": "1",
          "time": "20210906 15:26"
        },
        {
          "id": "MDRW2100041737.1.12",
          "form": "빵 맛 빵...",
          "original_form": "빵 맛 빵...",
          "speaker_id": "1",
          "time": "20210906 15:26"
        },
        {
          "id": "MDRW2100041737.1.13",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100041737.1.14",
          "form": "아무 맛 안나는 심심한거 좋아하세요?",
          "original_form": "아무 맛 안나는 심심한거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100041737.1.15",
          "form": "모닝롤 같은?",
          "original_form": "모닝롤 같은?",
          "speaker_id": "2",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100041737.1.16",
          "form": "심심이라니! 담백하고 고소한 빵 좋아해요ㅋㅋㅋ 사실 심심한 맛 맞지만",
          "original_form": "심심이라니! 담백하고 고소한 빵 좋아해요ㅋㅋㅋ 사실 심심한 맛 맞지만",
          "speaker_id": "1",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100041737.1.17",
          "form": "스콘 크로와상 베이글 이런 거 좋아해요... 대충 느낌오시죠!",
          "original_form": "스콘 크로와상 베이글 이런 거 좋아해요... 대충 느낌오시죠!",
          "speaker_id": "1",
          "time": "20210906 15:49"
        },
        {
          "id": "MDRW2100041737.1.18",
          "form": "오!!!!!!",
          "original_form": "오!!!!!!",
          "speaker_id": "2",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100041737.1.19",
          "form": "심심하다고 해서 죄송합니다 이 세상의 온갖 빵님덜,,,",
          "original_form": "심심하다고 해서 죄송합니다 이 세상의 온갖 빵님덜,,,",
          "speaker_id": "2",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100041737.1.20",
          "form": "네네네 완전 느낌 와요!!!",
          "original_form": "네네네 완전 느낌 와요!!!",
          "speaker_id": "2",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100041737.1.21",
          "form": "씹을수록 맛있는 것들이요!!!",
          "original_form": "씹을수록 맛있는 것들이요!!!",
          "speaker_id": "2",
          "time": "20210906 15:50"
        },
        {
          "id": "MDRW2100041737.1.22",
          "form": "맞아요맞아요!!! 입에서 그 화학적ㅋㅋㅋ 조화가 느껴지는 빵들!!!!!",
          "original_form": "맞아요맞아요!!! 입에서 그 화학적ㅋㅋㅋ 조화가 느껴지는 빵들!!!!!",
          "speaker_id": "1",
          "time": "20210906 15:53"
        },
        {
          "id": "MDRW2100041737.1.23",
          "form": "화학 맞죠?!",
          "original_form": "화학 맞죠?!",
          "speaker_id": "1",
          "time": "20210906 15:53"
        },
        {
          "id": "MDRW2100041737.1.24",
          "form": "빵... 발효...",
          "original_form": "빵... 발효...",
          "speaker_id": "1",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100041737.1.25",
          "form": "알죠알죠!!!!!",
          "original_form": "알죠알죠!!!!!",
          "speaker_id": "2",
          "time": "20210906 15:57"
        },
        {
          "id": "MDRW2100041737.1.26",
          "form": "아낰ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아낰ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:57"
        },
        {
          "id": "MDRW2100041737.1.27",
          "form": "아니 진짜 먹고싶네요",
          "original_form": "아니 진짜 먹고싶네요",
          "speaker_id": "2",
          "time": "20210906 15:57"
        },
        {
          "id": "MDRW2100041737.1.28",
          "form": "저는 빵이라면 그냥 다 좋아요",
          "original_form": "저는 빵이라면 그냥 다 좋아요",
          "speaker_id": "2",
          "time": "20210906 15:57"
        },
        {
          "id": "MDRW2100041737.1.29",
          "form": "그런데 선택권이 있으면 좀 자극적인 걸 좋아하는 편 같아요 확실히!!!",
          "original_form": "그런데 선택권이 있으면 좀 자극적인 걸 좋아하는 편 같아요 확실히!!!",
          "speaker_id": "2",
          "time": "20210906 15:57"
        },
        {
          "id": "MDRW2100041737.1.30",
          "form": "자극적인거라면? 소세지 이런거 올라간 거...?",
          "original_form": "자극적인거라면? 소세지 이런거 올라간 거...?",
          "speaker_id": "1",
          "time": "20210906 16:02"
        },
        {
          "id": "MDRW2100041737.1.31",
          "form": "말씀하시는 건가요",
          "original_form": "말씀하시는 건가요",
          "speaker_id": "1",
          "time": "20210906 16:02"
        },
        {
          "id": "MDRW2100041737.1.32",
          "form": "네네네!!! 아니면",
          "original_form": "네네네!!! 아니면",
          "speaker_id": "2",
          "time": "20210906 16:03"
