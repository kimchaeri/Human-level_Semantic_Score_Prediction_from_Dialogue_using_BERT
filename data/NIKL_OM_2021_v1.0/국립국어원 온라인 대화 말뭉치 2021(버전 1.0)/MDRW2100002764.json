      "utterance": [
        {
          "id": "MDRW2100002764.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 10:52"
        },
        {
          "id": "MDRW2100002764.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210824 10:53"
        },
        {
          "id": "MDRW2100002764.1.3",
          "form": "스포츠 좋아하시나요?",
          "original_form": "스포츠 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210824 11:01"
        },
        {
          "id": "MDRW2100002764.1.4",
          "form": "네 좋아하죠!",
          "original_form": "네 좋아하죠!",
          "speaker_id": "2",
          "time": "20210824 11:04"
        },
        {
          "id": "MDRW2100002764.1.5",
          "form": "축구를 특히 좋아합니다!",
          "original_form": "축구를 특히 좋아합니다!",
          "speaker_id": "2",
          "time": "20210824 11:04"
        },
        {
          "id": "MDRW2100002764.1.6",
          "form": "어떤팀 좋아하나요?",
          "original_form": "어떤팀 좋아하나요?",
          "speaker_id": "1",
          "time": "20210824 11:11"
        },
        {
          "id": "MDRW2100002764.1.7",
          "form": "저는 EPL에는 레스터",
          "original_form": "저는 EPL에는 레스터",
          "speaker_id": "2",
          "time": "20210824 11:46"
        },
        {
          "id": "MDRW2100002764.1.8",
          "form": "라리가는 AT 마드리드",
          "original_form": "라리가는 AT 마드리드",
          "speaker_id": "2",
          "time": "20210824 11:46"
        },
        {
          "id": "MDRW2100002764.1.9",
          "form": "이렇게 좋아해요",
          "original_form": "이렇게 좋아해요",
          "speaker_id": "2",
          "time": "20210824 11:47"
        },
        {
          "id": "MDRW2100002764.1.10",
          "form": "레스터? 새로올라온팀인가요?",
          "original_form": "레스터? 새로올라온팀인가요?",
          "speaker_id": "1",
          "time": "20210824 11:55"
        },
        {
          "id": "MDRW2100002764.1.11",
          "form": "저는 토트넘좋아해요ㅋㅋㅋ",
          "original_form": "저는 토트넘좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:55"
        },
        {
          "id": "MDRW2100002764.1.12",
          "form": "아하 토트넘좋아하시는구나",
          "original_form": "아하 토트넘좋아하시는구나",
          "speaker_id": "2",
          "time": "20210824 11:56"
        },
        {
          "id": "MDRW2100002764.1.13",
          "form": "레스터 커뮤니티쉴드 우승도하고 저번해에 우승또했던 팀이라",
          "original_form": "레스터 커뮤니티쉴드 우승도하고 저번해에 우승또했던 팀이라",
          "speaker_id": "2",
          "time": "20210824 11:56"
        },
        {
          "id": "MDRW2100002764.1.14",
          "form": "나름 유명하긴해용",
          "original_form": "나름 유명하긴해용",
          "speaker_id": "2",
          "time": "20210824 11:56"
        },
        {
          "id": "MDRW2100002764.1.15",
          "form": "아 저는 한창 박지성할때봐가지고 그때팀들이 익숙해서 요즘팀들은",
          "original_form": "아 저는 한창 박지성할때봐가지고 그때팀들이 익숙해서 요즘팀들은",
          "speaker_id": "1",
          "time": "20210824 12:16"
        },
        {
          "id": "MDRW2100002764.1.16",
          "form": "잘모르겠네요ㅠ",
          "original_form": "잘모르겠네요ㅠ",
          "speaker_id": "1",
          "time": "20210824 12:17"
        },
        {
          "id": "MDRW2100002764.1.17",
          "form": "이번 리그부터 다시보는중이에요ㅋㅋㅋ",
          "original_form": "이번 리그부터 다시보는중이에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:17"
        },
        {
          "id": "MDRW2100002764.1.18",
          "form": "아하 이번 리그 재밌어요!",
          "original_form": "아하 이번 리그 재밌어요!",
          "speaker_id": "2",
          "time": "20210824 12:21"
        },
        {
          "id": "MDRW2100002764.1.19",
          "form": "토트넘은 기분좋게 2승하긴했는데",
          "original_form": "토트넘은 기분좋게 2승하긴했는데",
          "speaker_id": "2",
          "time": "20210824 12:21"
        },
        {
          "id": "MDRW2100002764.1.20",
          "form": "손흥민 부상 얘기 돌아서 걱정이긴하겠네요",
          "original_form": "손흥민 부상 얘기 돌아서 걱정이긴하겠네요",
          "speaker_id": "2",
          "time": "20210824 12:21"
        },
        {
          "id": "MDRW2100002764.1.21",
          "form": "ㅋㅋㅋ 2번째경기는 꾸역꾸역이겨가지고ㅠ",
          "original_form": "ㅋㅋㅋ 2번째경기는 꾸역꾸역이겨가지고ㅠ",
          "speaker_id": "1",
          "time": "20210824 12:25"
        },
        {
          "id": "MDRW2100002764.1.22",
          "form": "맞아요 햄스트링ㅠ",
          "original_form": "맞아요 햄스트링ㅠ",
          "speaker_id": "1",
          "time": "20210824 12:25"
        },
        {
          "id": "MDRW2100002764.1.23",
          "form": "레스터에는 누가있나요? 스트라이커?!",
          "original_form": "레스터에는 누가있나요? 스트라이커?!",
          "speaker_id": "1",
          "time": "20210824 12:25"
        },
        {
          "id": "MDRW2100002764.1.24",
          "form": "제이미 바디!",
          "original_form": "제이미 바디!",
          "speaker_id": "2",
          "time": "20210824 12:25"
        },
        {
          "id": "MDRW2100002764.1.25",
          "form": "아직까지도 현역이죠",
          "original_form": "아직까지도 현역이죠",
          "speaker_id": "2",
          "time": "20210824 12:26"
        },
        {
          "id": "MDRW2100002764.1.26",
          "form": "좋네요ㅋㅋㅋ",
          "original_form": "좋네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:27"
        },
        {
          "id": "MDRW2100002764.1.27",
          "form": "요즘꺼 챙겨보세요?",
          "original_form": "요즘꺼 챙겨보세요?",
          "speaker_id": "1",
          "time": "20210824 12:27"
        },
        {
          "id": "MDRW2100002764.1.28",
          "form": "새벽시간대인데",
          "original_form": "새벽시간대인데",
          "speaker_id": "1",
          "time": "20210824 12:27"
        },
        {
          "id": "MDRW2100002764.1.29",
          "form": "저녁이나 새벽 1~2시껀 보고",
          "original_form": "저녁이나 새벽 1~2시껀 보고",
          "speaker_id": "2",
          "time": "20210824 12:28"
        },
        {
          "id": "MDRW2100002764.1.30",
          "form": "나머지 3시 이런건 하이라이트로 봐요",
          "original_form": "나머지 3시 이런건 하이라이트로 봐요",
          "speaker_id": "2",
          "time": "20210824 12:28"
        }
      ]
    }
