      "utterance": [
        {
          "id": "MDRW2100002729.1.1",
          "form": "응 너무 힘들다 수강심청",
          "original_form": "응 너무 힘들다 수강심청",
          "speaker_id": "2",
          "time": "20210820 15:03"
        },
        {
          "id": "MDRW2100002729.1.2",
          "form": "나는 저번주였어",
          "original_form": "나는 저번주였어",
          "speaker_id": "2",
          "time": "20210820 15:03"
        },
        {
          "id": "MDRW2100002729.1.3",
          "form": "너는 학고다닐때 수강심청을 잘하는 편이었니?",
          "original_form": "너는 학고다닐때 수강심청을 잘하는 편이었니?",
          "speaker_id": "2",
          "time": "20210820 15:03"
        },
        {
          "id": "MDRW2100002729.1.4",
          "form": "학교. 응 나는 잘하는 편이었어.",
          "original_form": "학교. 응 나는 잘하는 편이었어.",
          "speaker_id": "1",
          "time": "20210820 15:03"
        },
        {
          "id": "MDRW2100002729.1.5",
          "form": "내가 기운을 좀 줄 걸 그랬다.",
          "original_form": "내가 기운을 좀 줄 걸 그랬다.",
          "speaker_id": "1",
          "time": "20210820 15:04"
        },
        {
          "id": "MDRW2100002729.1.6",
          "form": "그러면 개강은 안제야?",
          "original_form": "그러면 개강은 안제야?",
          "speaker_id": "1",
          "time": "20210820 15:04"
        },
        {
          "id": "MDRW2100002729.1.7",
          "form": "그러게 기운좀 받을껄",
          "original_form": "그러게 기운좀 받을껄",
          "speaker_id": "2",
          "time": "20210820 15:04"
        },
        {
          "id": "MDRW2100002729.1.8",
          "form": "나는 9월에 개강해",
          "original_form": "나는 9월에 개강해",
          "speaker_id": "2",
          "time": "20210820 15:04"
        },
        {
          "id": "MDRW2100002729.1.9",
          "form": "아직 비대면인지 아닌지도 몰라",
          "original_form": "아직 비대면인지 아닌지도 몰라",
          "speaker_id": "2",
          "time": "20210820 15:04"
        },
        {
          "id": "MDRW2100002729.1.10",
          "form": "학교는 일처리가 왜이렇게 느릴까",
          "original_form": "학교는 일처리가 왜이렇게 느릴까",
          "speaker_id": "2",
          "time": "20210820 15:04"
        },
        {
          "id": "MDRW2100002729.1.11",
          "form": "개강.",
          "original_form": "개강.",
          "speaker_id": "1",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002729.1.12",
          "form": "곧이구나.",
          "original_form": "곧이구나.",
          "speaker_id": "1",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002729.1.13",
          "form": "그러게 행정처리가 느리네",
          "original_form": "그러게 행정처리가 느리네",
          "speaker_id": "1",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002729.1.14",
          "form": "국립대라서 더 그런가?",
          "original_form": "국립대라서 더 그런가?",
          "speaker_id": "2",
          "time": "20210820 15:06"
        },
        {
          "id": "MDRW2100002729.1.15",
          "form": "학교는 대면수업을 원하는거같아",
          "original_form": "학교는 대면수업을 원하는거같아",
          "speaker_id": "2",
          "time": "20210820 15:06"
        },
        {
          "id": "MDRW2100002729.1.16",
          "form": "사립대도 마찬가지야.",
          "original_form": "사립대도 마찬가지야.",
          "speaker_id": "1",
          "time": "20210820 15:06"
        },
        {
          "id": "MDRW2100002729.1.17",
          "form": "오 모든 학교의 특징같아",
          "original_form": "오 모든 학교의 특징같아",
          "speaker_id": "1",
          "time": "20210820 15:06"
        },
        {
          "id": "MDRW2100002729.1.18",
          "form": "너는 학교가 좋니?",
          "original_form": "너는 학교가 좋니?",
          "speaker_id": "1",
          "time": "20210820 15:06"
        },
        {
          "id": "MDRW2100002729.1.19",
          "form": "학교 사람들은 어때?",
          "original_form": "학교 사람들은 어때?",
          "speaker_id": "1",
          "time": "20210820 15:06"
        },
        {
          "id": "MDRW2100002729.1.20",
          "form": "나는 그저그랴",
          "original_form": "나는 그저그랴",
          "speaker_id": "2",
          "time": "20210820 15:06"
        },
        {
          "id": "MDRW2100002729.1.21",
          "form": "친구는 사귀었니?",
          "original_form": "친구는 사귀었니?",
          "speaker_id": "1",
          "time": "20210820 15:06"
        },
        {
          "id": "MDRW2100002729.1.22",
          "form": "비대면 수업했으면 좋겠어",
          "original_form": "비대면 수업했으면 좋겠어",
          "speaker_id": "2",
          "time": "20210820 15:06"
        },
        {
          "id": "MDRW2100002729.1.23",
          "form": "아무래도 친규가 없으니까.",
          "original_form": "아무래도 친규가 없으니까.",
          "speaker_id": "2",
          "time": "20210820 15:07"
        },
        {
          "id": "MDRW2100002729.1.24",
          "form": "비대몀 좋아.",
          "original_form": "비대몀 좋아.",
          "speaker_id": "1",
          "time": "20210820 15:07"
        },
        {
          "id": "MDRW2100002729.1.25",
          "form": "안타까워",
          "original_form": "안타까워",
          "speaker_id": "1",
          "time": "20210820 15:07"
        },
        {
          "id": "MDRW2100002729.1.26",
          "form": "친구가 있어도 비대면이 좋을 거 같아",
          "original_form": "친구가 있어도 비대면이 좋을 거 같아",
          "speaker_id": "1",
          "time": "20210820 15:07"
        },
        {
          "id": "MDRW2100002729.1.27",
          "form": "그건그래 학교는 너무 귀찮어",
          "original_form": "그건그래 학교는 너무 귀찮어",
          "speaker_id": "2",
          "time": "20210820 15:07"
        },
        {
          "id": "MDRW2100002729.1.28",
          "form": "맞아. 가는 길이 귀찮지.",
          "original_form": "맞아. 가는 길이 귀찮지.",
          "speaker_id": "1",
          "time": "20210820 15:07"
        },
        {
          "id": "MDRW2100002729.1.29",
          "form": "아침수업이 있으면 더해",
          "original_form": "아침수업이 있으면 더해",
          "speaker_id": "2",
          "time": "20210820 15:07"
        }
      ]
    }
  ]
