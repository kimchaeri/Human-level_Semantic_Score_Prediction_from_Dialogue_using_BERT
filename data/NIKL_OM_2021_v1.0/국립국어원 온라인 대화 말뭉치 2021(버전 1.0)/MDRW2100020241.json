      "utterance": [
        {
          "id": "MDRW2100020241.1.1",
          "form": "세상엔 이제",
          "original_form": "세상엔 이제",
          "speaker_id": "2",
          "time": "20210828 00:37"
        },
        {
          "id": "MDRW2100020241.1.2",
          "form": "계산천재가 너무많아서",
          "original_form": "계산천재가 너무많아서",
          "speaker_id": "2",
          "time": "20210828 00:37"
        },
        {
          "id": "MDRW2100020241.1.3",
          "form": "아녀",
          "original_form": "아녀",
          "speaker_id": "1",
          "time": "20210828 00:37"
        },
        {
          "id": "MDRW2100020241.1.4",
          "form": "그냥 평범 계산정도",
          "original_form": "그냥 평범 계산정도",
          "speaker_id": "2",
          "time": "20210828 00:37"
        },
        {
          "id": "MDRW2100020241.1.5",
          "form": "평범하개라도",
          "original_form": "평범하개라도",
          "speaker_id": "1",
          "time": "20210828 00:37"
        },
        {
          "id": "MDRW2100020241.1.6",
          "form": "할줄아는게",
          "original_form": "할줄아는게",
          "speaker_id": "1",
          "time": "20210828 00:37"
        },
        {
          "id": "MDRW2100020241.1.7",
          "form": "인생이 편한거같어",
          "original_form": "인생이 편한거같어",
          "speaker_id": "1",
          "time": "20210828 00:37"
        },
        {
          "id": "MDRW2100020241.1.8",
          "form": "그럼그럼",
          "original_form": "그럼그럼",
          "speaker_id": "2",
          "time": "20210828 00:37"
        },
        {
          "id": "MDRW2100020241.1.9",
          "form": "사실",
          "original_form": "사실",
          "speaker_id": "2",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100020241.1.10",
          "form": "평범하기가 세상에서",
          "original_form": "평범하기가 세상에서",
          "speaker_id": "2",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100020241.1.11",
          "form": "제일 어려운듯",
          "original_form": "제일 어려운듯",
          "speaker_id": "2",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100020241.1.12",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100020241.1.13",
          "form": "정말 맞말이야",
          "original_form": "정말 맞말이야",
          "speaker_id": "1",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100020241.1.14",
          "form": "평범평범 퍙범2!",
          "original_form": "평범평범 퍙범2!",
          "speaker_id": "2",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100020241.1.15",
          "form": "평범하게만 살아도",
          "original_form": "평범하게만 살아도",
          "speaker_id": "1",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100020241.1.16",
          "form": "학교를 그렇게",
          "original_form": "학교를 그렇게",
          "speaker_id": "1",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100020241.1.17",
          "form": "안싫어했을거야",
          "original_form": "안싫어했을거야",
          "speaker_id": "1",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100020241.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 맞말",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 맞말",
          "speaker_id": "2",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100020241.1.19",
          "form": "내가",
          "original_form": "내가",
          "speaker_id": "2",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100020241.1.20",
          "form": "성적 b+는 가볒게받고",
          "original_form": "성적 b+는 가볒게받고",
          "speaker_id": "2",
          "time": "20210828 00:38"
        },
        {
          "id": "MDRW2100020241.1.21",
          "form": "적당히 웃으면서 학교다닐수잇엇으면",
          "original_form": "적당히 웃으면서 학교다닐수잇엇으면",
          "speaker_id": "2",
          "time": "20210828 00:39"
        },
        {
          "id": "MDRW2100020241.1.22",
          "form": "학교를 사랑햇을거다",
          "original_form": "학교를 사랑햇을거다",
          "speaker_id": "2",
          "time": "20210828 00:39"
        },
        {
          "id": "MDRW2100020241.1.23",
          "form": "중고등학교도",
          "original_form": "중고등학교도",
          "speaker_id": "2",
          "time": "20210828 00:39"
        },
        {
          "id": "MDRW2100020241.1.24",
          "form": "최소 4등급 나왓으면",
          "original_form": "최소 4등급 나왓으면",
          "speaker_id": "2",
          "time": "20210828 00:39"
        },
        {
          "id": "MDRW2100020241.1.25",
          "form": "정맣 행복이엇을듯",
          "original_form": "정맣 행복이엇을듯",
          "speaker_id": "2",
          "time": "20210828 00:39"
        },
        {
          "id": "MDRW2100020241.1.26",
          "form": "막 등록금 걱정도 안 하고",
          "original_form": "막 등록금 걱정도 안 하고",
          "speaker_id": "1",
          "time": "20210828 00:39"
        },
        {
          "id": "MDRW2100020241.1.27",
          "form": "ㄱㄴㄲ",
          "original_form": "ㄱㄴㄲ",
          "speaker_id": "2",
          "time": "20210828 00:39"
        },
        {
          "id": "MDRW2100020241.1.28",
          "form": "성적그리고",
          "original_form": "성적그리고",
          "speaker_id": "1",
          "time": "20210828 00:39"
        },
        {
          "id": "MDRW2100020241.1.29",
          "form": "몇점 떨어지는걸로",
          "original_form": "몇점 떨어지는걸로",
          "speaker_id": "1",
          "time": "20210828 00:39"
        },
        {
          "id": "MDRW2100020241.1.30",
          "form": "스트레스안받아도",
          "original_form": "스트레스안받아도",
          "speaker_id": "1",
          "time": "20210828 00:39"
        },
        {
          "id": "MDRW2100020241.1.31",
          "form": "상관없으면 그렇게 다녓나?!",
          "original_form": "상관없으면 그렇게 다녓나?!",
          "speaker_id": "1",
          "time": "20210828 00:39"
        },
        {
          "id": "MDRW2100020241.1.32",
          "form": "ㄷㄴㄲ",
          "original_form": "ㄷㄴㄲ",
          "speaker_id": "2",
          "time": "20210828 00:39"
        },
        {
          "id": "MDRW2100020241.1.33",
          "form": "막",
          "original_form": "막",
          "speaker_id": "2",
          "time": "20210828 00:39"
        },
        {
          "id": "MDRW2100020241.1.34",
          "form": "내가 대학교 열심히는 안 다녔어도",
          "original_form": "내가 대학교 열심히는 안 다녔어도",
          "speaker_id": "1",
          "time": "20210828 00:40"
        },
        {
          "id": "MDRW2100020241.1.35",
          "form": "생활비 걱정도 없고",
          "original_form": "생활비 걱정도 없고",
          "speaker_id": "2",
          "time": "20210828 00:40"
        },
        {
          "id": "MDRW2100020241.1.36",
          "form": "국장 받을정도는 유지했다..",
          "original_form": "국장 받을정도는 유지했다..",
          "speaker_id": "1",
          "time": "20210828 00:40"
        },
        {
          "id": "MDRW2100020241.1.37",
          "form": "학교 기숙사나",
          "original_form": "학교 기숙사나",
          "speaker_id": "2",
          "time": "20210828 00:40"
        },
        {
          "id": "MDRW2100020241.1.38",
          "form": "통학 안히곤하게 평범",
          "original_form": "통학 안히곤하게 평범",
          "speaker_id": "2",
          "time": "20210828 00:40"
        },
        {
          "id": "MDRW2100020241.1.39",
          "form": "ㄱ거리에 살기만햇어도",
          "original_form": "ㄱ거리에 살기만햇어도",
          "speaker_id": "2",
          "time": "20210828 00:40"
        },
        {
          "id": "MDRW2100020241.1.40",
          "form": "행복이엇을듯",
          "original_form": "행복이엇을듯",
          "speaker_id": "2",
          "time": "20210828 00:40"
        },
        {
          "id": "MDRW2100020241.1.41",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "1",
          "time": "20210828 00:40"
        },
        {
          "id": "MDRW2100020241.1.42",
          "form": "다 양보해서",
          "original_form": "다 양보해서",
          "speaker_id": "1",
          "time": "20210828 00:40"
        },
        {
          "id": "MDRW2100020241.1.43",
          "form": "밥이라도 제때 먹을수있었다면",
          "original_form": "밥이라도 제때 먹을수있었다면",
          "speaker_id": "1",
          "time": "20210828 00:40"
        },
        {
          "id": "MDRW2100020241.1.44",
          "form": "얼마나 나았을까",
          "original_form": "얼마나 나았을까",
