      "utterance": [
        {
          "id": "MDRW2100047162.1.1",
          "form": "반려동물키우시나요?",
          "original_form": "반려동물키우시나요?",
          "speaker_id": "2",
          "time": "20210901 15:17"
        },
        {
          "id": "MDRW2100047162.1.2",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "1",
          "time": "20210901 15:17"
        },
        {
          "id": "MDRW2100047162.1.3",
          "form": "간데 좋아는해요",
          "original_form": "간데 좋아는해요",
          "speaker_id": "1",
          "time": "20210901 15:17"
        },
        {
          "id": "MDRW2100047162.1.4",
          "form": "유독좋아하시는 동물 있으세요?",
          "original_form": "유독좋아하시는 동물 있으세요?",
          "speaker_id": "2",
          "time": "20210901 15:18"
        },
        {
          "id": "MDRW2100047162.1.5",
          "form": "저 하는다람쥐요",
          "original_form": "저 하는다람쥐요",
          "speaker_id": "1",
          "time": "20210901 15:23"
        },
        {
          "id": "MDRW2100047162.1.6",
          "form": "너무 기엽더라구요 ㅠ",
          "original_form": "너무 기엽더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 15:23"
        },
        {
          "id": "MDRW2100047162.1.7",
          "form": "헉 그친구 너무 귀엽죠ㅠㅠㅠㅠ",
          "original_form": "헉 그친구 너무 귀엽죠ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 15:26"
        },
        {
          "id": "MDRW2100047162.1.8",
          "form": "하늘다람쥐말하시는거죠?",
          "original_form": "하늘다람쥐말하시는거죠?",
          "speaker_id": "2",
          "time": "20210901 15:26"
        },
        {
          "id": "MDRW2100047162.1.9",
          "form": "맞아요!!주인손에 꼬옥안기는게 너무 치명적이예요",
          "original_form": "맞아요!!주인손에 꼬옥안기는게 너무 치명적이예요",
          "speaker_id": "1",
          "time": "20210901 15:35"
        },
        {
          "id": "MDRW2100047162.1.10",
          "form": "너무 귀여워요ㅠㅠ 반려동물로는 적합하지않겠지만",
          "original_form": "너무 귀여워요ㅠㅠ 반려동물로는 적합하지않겠지만",
          "speaker_id": "2",
          "time": "20210901 15:36"
        },
        {
          "id": "MDRW2100047162.1.11",
          "form": "멀리서 구경하고 싶어요",
          "original_form": "멀리서 구경하고 싶어요",
          "speaker_id": "2",
          "time": "20210901 15:36"
        },
        {
          "id": "MDRW2100047162.1.12",
          "form": "맞아요 그냥 보는것만으로도 힐링이예요",
          "original_form": "맞아요 그냥 보는것만으로도 힐링이예요",
          "speaker_id": "1",
          "time": "20210901 15:37"
        },
        {
          "id": "MDRW2100047162.1.13",
          "form": "음..약간 작은 동물들이 기엽더라구요..",
          "original_form": "음..약간 작은 동물들이 기엽더라구요..",
          "speaker_id": "1",
          "time": "20210901 15:37"
        },
        {
          "id": "MDRW2100047162.1.14",
          "form": "소동물귀엽죠...",
          "original_form": "소동물귀엽죠...",
          "speaker_id": "2",
          "time": "20210901 15:39"
        },
        {
          "id": "MDRW2100047162.1.15",
          "form": "만약 키우신다면 어떤 동물 키우고싶으세요?",
          "original_form": "만약 키우신다면 어떤 동물 키우고싶으세요?",
          "speaker_id": "2",
          "time": "20210901 15:39"
        },
        {
          "id": "MDRW2100047162.1.16",
          "form": "음 머든좋을거같아요",
          "original_form": "음 머든좋을거같아요",
          "speaker_id": "1",
          "time": "20210901 15:44"
        },
        {
          "id": "MDRW2100047162.1.17",
          "form": "물고기도 조쿠요!",
          "original_form": "물고기도 조쿠요!",
          "speaker_id": "1",
          "time": "20210901 15:44"
        },
        {
          "id": "MDRW2100047162.1.18",
          "form": "열대어도 좋아하시나요?",
          "original_form": "열대어도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210901 15:46"
        },
        {
          "id": "MDRW2100047162.1.19",
          "form": "나쁘지않은거같아요",
          "original_form": "나쁘지않은거같아요",
          "speaker_id": "1",
          "time": "20210901 15:47"
        },
        {
          "id": "MDRW2100047162.1.20",
          "form": "저는 고양이 키우고싶네요",
          "original_form": "저는 고양이 키우고싶네요",
          "speaker_id": "2",
          "time": "20210901 15:47"
        }
      ]
