      "utterance": [
        {
          "id": "MDRW2100047241.1.1",
          "form": "지에스 라방때 반값구매도 사서 편상은 쓸모가 있어서 좋아요 ㅋ",
          "original_form": "지에스 라방때 반값구매도 사서 편상은 쓸모가 있어서 좋아요 ㅋ",
          "speaker_id": "1",
          "time": "20210826 01:11"
        },
        {
          "id": "MDRW2100047241.1.2",
          "form": "전주로 사진않고 당첨이예요ㅎ",
          "original_form": "전주로 사진않고 당첨이예요ㅎ",
          "speaker_id": "2",
          "time": "20210826 01:13"
        },
        {
          "id": "MDRW2100047241.1.3",
          "form": "이벤트응모로ㅎ",
          "original_form": "이벤트응모로ㅎ",
          "speaker_id": "2",
          "time": "20210826 01:13"
        },
        {
          "id": "MDRW2100047241.1.4",
          "form": "와~~~  멋져요~",
          "original_form": "와~~~  멋져요~",
          "speaker_id": "1",
          "time": "20210826 01:14"
        },
        {
          "id": "MDRW2100047241.1.5",
          "form": "거의 백퍼주거나 그런건 부모님껏도하고 ㅋ",
          "original_form": "거의 백퍼주거나 그런건 부모님껏도하고 ㅋ",
          "speaker_id": "2",
          "time": "20210826 01:16"
        },
        {
          "id": "MDRW2100047241.1.6",
          "form": "정보다팔아서",
          "original_form": "정보다팔아서",
          "speaker_id": "2",
          "time": "20210826 01:16"
        },
        {
          "id": "MDRW2100047241.1.7",
          "form": "그래서 물건싸게 사고 하는거죠~^^",
          "original_form": "그래서 물건싸게 사고 하는거죠~^^",
          "speaker_id": "1",
          "time": "20210826 01:17"
        },
        {
          "id": "MDRW2100047241.1.8",
          "form": "네 아빠가 빵 아스크림좋아해서",
          "original_form": "네 아빠가 빵 아스크림좋아해서",
          "speaker_id": "2",
          "time": "20210826 01:17"
        },
        {
          "id": "MDRW2100047241.1.9",
          "form": "배라 사주고",
          "original_form": "배라 사주고",
          "speaker_id": "2",
          "time": "20210826 01:17"
        },
        {
          "id": "MDRW2100047241.1.10",
          "form": "파바사주고 하니",
          "original_form": "파바사주고 하니",
          "speaker_id": "2",
          "time": "20210826 01:17"
        },
        {
          "id": "MDRW2100047241.1.11",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210826 01:17"
        },
        {
          "id": "MDRW2100047241.1.12",
          "form": "뿌듯하시겠어요~",
          "original_form": "뿌듯하시겠어요~",
          "speaker_id": "1",
          "time": "20210826 01:18"
        },
        {
          "id": "MDRW2100047241.1.13",
          "form": "그래서 참여도가 좋아요ㅎㅎ",
          "original_form": "그래서 참여도가 좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 01:20"
        },
        {
          "id": "MDRW2100047241.1.14",
          "form": "인증번호 잘보내주고",
          "original_form": "인증번호 잘보내주고",
          "speaker_id": "2",
          "time": "20210826 01:20"
        },
        {
          "id": "MDRW2100047241.1.15",
          "form": "깁콘문자옴 바로보내주고",
          "original_form": "깁콘문자옴 바로보내주고",
          "speaker_id": "2",
          "time": "20210826 01:20"
        },
        {
          "id": "MDRW2100047241.1.16",
          "form": "좋네요~ 저희신랑은 뭐싸게 산다고 해도 감흥이 별로 없어요",
          "original_form": "좋네요~ 저희신랑은 뭐싸게 산다고 해도 감흥이 별로 없어요",
          "speaker_id": "1",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100047241.1.17",
          "form": "남자들은 싫어하나봐요",
          "original_form": "남자들은 싫어하나봐요",
          "speaker_id": "2",
          "time": "20210826 01:36"
        },
        {
          "id": "MDRW2100047241.1.18",
          "form": "궁상맞게보고",
          "original_form": "궁상맞게보고",
          "speaker_id": "2",
          "time": "20210826 01:36"
        },
        {
          "id": "MDRW2100047241.1.19",
          "form": "맞아요. 그게해서 얼마나 싸게산다고 이러면서",
          "original_form": "맞아요. 그게해서 얼마나 싸게산다고 이러면서",
          "speaker_id": "1",
          "time": "20210826 01:43"
        },
        {
          "id": "MDRW2100047241.1.20",
          "form": "빙고 ㅎ 다들 그런가봐요 주변 얘기들어보면",
          "original_form": "빙고 ㅎ 다들 그런가봐요 주변 얘기들어보면",
          "speaker_id": "2",
          "time": "20210826 01:48"
        },
        {
          "id": "MDRW2100047241.1.21",
          "form": "싸다고 쟁이면 이거 다어디두냐고 뭐라하고",
          "original_form": "싸다고 쟁이면 이거 다어디두냐고 뭐라하고",
          "speaker_id": "1",
          "time": "20210826 01:48"
        },
        {
          "id": "MDRW2100047241.1.22",
          "form": "근데 사놓음 또 본인이 다먹는다던데요기",
          "original_form": "근데 사놓음 또 본인이 다먹는다던데요기",
          "speaker_id": "2",
          "time": "20210826 01:49"
        },
        {
          "id": "MDRW2100047241.1.23",
          "form": "사면 잔소리하다가ㅋ",
          "original_form": "사면 잔소리하다가ㅋ",
          "speaker_id": "2",
          "time": "20210826 01:49"
        }
      ]
