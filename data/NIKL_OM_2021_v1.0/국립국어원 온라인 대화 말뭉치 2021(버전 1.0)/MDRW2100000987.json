      "utterance": [
        {
          "id": "MDRW2100000987.1.1",
          "form": "지금 슬의해요~  이거 보세요?",
          "original_form": "지금 슬의해요~  이거 보세요?",
          "speaker_id": "2",
          "time": "20210819 21:03"
        },
        {
          "id": "MDRW2100000987.1.2",
          "form": "아 본방송 하나요?",
          "original_form": "아 본방송 하나요?",
          "speaker_id": "1",
          "time": "20210819 21:04"
        },
        {
          "id": "MDRW2100000987.1.3",
          "form": "네 본방이요",
          "original_form": "네 본방이요",
          "speaker_id": "2",
          "time": "20210819 21:04"
        },
        {
          "id": "MDRW2100000987.1.4",
          "form": "맛남의 광장 틀어놨어요",
          "original_form": "맛남의 광장 틀어놨어요",
          "speaker_id": "1",
          "time": "20210819 21:04"
        },
        {
          "id": "MDRW2100000987.1.5",
          "form": "전 모기나오는거보다 틀었어요.  딸램이 틀어달라고해서",
          "original_form": "전 모기나오는거보다 틀었어요.  딸램이 틀어달라고해서",
          "speaker_id": "2",
          "time": "20210819 21:04"
        },
        {
          "id": "MDRW2100000987.1.6",
          "form": "지금 몇회예요? 언제하는지 몰라서 맨날 재방봐요",
          "original_form": "지금 몇회예요? 언제하는지 몰라서 맨날 재방봐요",
          "speaker_id": "1",
          "time": "20210819 21:05"
        },
        {
          "id": "MDRW2100000987.1.7",
          "form": "9회요",
          "original_form": "9회요",
          "speaker_id": "2",
          "time": "20210819 21:06"
        },
        {
          "id": "MDRW2100000987.1.8",
          "form": "큰애는 안보고있어요. 첨에못봤다고 나중에 몰아서본데요",
          "original_form": "큰애는 안보고있어요. 첨에못봤다고 나중에 몰아서본데요",
          "speaker_id": "2",
          "time": "20210819 21:07"
        },
        {
          "id": "MDRW2100000987.1.9",
          "form": "저도 중간중간봐서 내용을 모르겠더라구요",
          "original_form": "저도 중간중간봐서 내용을 모르겠더라구요",
          "speaker_id": "1",
          "time": "20210819 21:07"
        },
        {
          "id": "MDRW2100000987.1.10",
          "form": "정경호랑은 왜헤어진거예요 ?",
          "original_form": "정경호랑은 왜헤어진거예요 ?",
          "speaker_id": "1",
          "time": "20210819 21:07"
        },
        {
          "id": "MDRW2100000987.1.11",
          "form": "아퍼서요",
          "original_form": "아퍼서요",
          "speaker_id": "2",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100000987.1.12",
          "form": "아 정경호는 모르는거죠?",
          "original_form": "아 정경호는 모르는거죠?",
          "speaker_id": "1",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100000987.1.13",
          "form": "그때 사고나서 검사하고 유학하다들어왔어요",
          "original_form": "그때 사고나서 검사하고 유학하다들어왔어요",
          "speaker_id": "2",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100000987.1.14",
          "form": "네.  그냥 이별당했죠",
          "original_form": "네.  그냥 이별당했죠",
          "speaker_id": "2",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100000987.1.15",
          "form": "오빠도 알고있는거예요? 조정석이",
          "original_form": "오빠도 알고있는거예요? 조정석이",
          "speaker_id": "1",
          "time": "20210819 21:09"
        },
        {
          "id": "MDRW2100000987.1.16",
          "form": "네.  그때 절때로 정경호한테 말하지 말라고해서 눈치로 알아챘어요",
          "original_form": "네.  그때 절때로 정경호한테 말하지 말라고해서 눈치로 알아챘어요",
          "speaker_id": "2",
          "time": "20210819 21:10"
        },
        {
          "id": "MDRW2100000987.1.17",
          "form": "아 둘이 사귄거 조정석이 알았어요?",
          "original_form": "아 둘이 사귄거 조정석이 알았어요?",
          "speaker_id": "1",
          "time": "20210819 21:10"
        },
        {
          "id": "MDRW2100000987.1.18",
          "form": "네. 그런거같아요.",
          "original_form": "네. 그런거같아요.",
          "speaker_id": "2",
          "time": "20210819 21:11"
        },
        {
          "id": "MDRW2100000987.1.19",
          "form": "저도 나중에 몰아서 봐야겠어요ㅎ 산부인과 둘은 잘될거같죠?ㅋ",
          "original_form": "저도 나중에 몰아서 봐야겠어요ㅎ 산부인과 둘은 잘될거같죠?ㅋ",
          "speaker_id": "1",
          "time": "20210819 21:12"
        },
        {
          "id": "MDRW2100000987.1.20",
          "form": "네~~ 둘이 귀여워요",
          "original_form": "네~~ 둘이 귀여워요",
          "speaker_id": "2",
          "time": "20210819 21:13"
        },
        {
          "id": "MDRW2100000987.1.21",
          "form": "몇부작인지 모르시죠~",
          "original_form": "몇부작인지 모르시죠~",
          "speaker_id": "1",
          "time": "20210819 21:14"
        },
        {
          "id": "MDRW2100000987.1.22",
          "form": "16부아닐까요?",
          "original_form": "16부아닐까요?",
          "speaker_id": "2",
          "time": "20210819 21:14"
        },
        {
          "id": "MDRW2100000987.1.23",
          "form": "거의 그런거같은데",
          "original_form": "거의 그런거같은데",
          "speaker_id": "2",
          "time": "20210819 21:14"
        },
        {
          "id": "MDRW2100000987.1.24",
          "form": "이제 중반부왔군요",
          "original_form": "이제 중반부왔군요",
          "speaker_id": "1",
          "time": "20210819 21:14"
        },
        {
          "id": "MDRW2100000987.1.25",
          "form": "조정석 넘 웃긴거같아요",
          "original_form": "조정석 넘 웃긴거같아요",
          "speaker_id": "2",
          "time": "20210819 21:21"
        },
        {
          "id": "MDRW2100000987.1.26",
          "form": "드라마에서 넘 웃기게나와요ㅎ 저런친구들 있음 좋을거같아요",
          "original_form": "드라마에서 넘 웃기게나와요ㅎ 저런친구들 있음 좋을거같아요",
          "speaker_id": "1",
          "time": "20210819 21:22"
        }
      ]
    }
