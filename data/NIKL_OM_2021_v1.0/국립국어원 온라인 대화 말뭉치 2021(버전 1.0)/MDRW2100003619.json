      "utterance": [
        {
          "id": "MDRW2100003619.1.1",
          "form": "플립은 안써요?",
          "original_form": "플립은 안써요?",
          "speaker_id": "1",
          "time": "20210821 08:49"
        },
        {
          "id": "MDRW2100003619.1.2",
          "form": "폴드 플립같이 나오던데",
          "original_form": "폴드 플립같이 나오던데",
          "speaker_id": "1",
          "time": "20210821 08:49"
        },
        {
          "id": "MDRW2100003619.1.3",
          "form": "플립이 이번에 이쁘게 나왔는데",
          "original_form": "플립이 이번에 이쁘게 나왔는데",
          "speaker_id": "2",
          "time": "20210821 08:49"
        },
        {
          "id": "MDRW2100003619.1.4",
          "form": "플립은 취향이 아닌가?",
          "original_form": "플립은 취향이 아닌가?",
          "speaker_id": "1",
          "time": "20210821 08:49"
        },
        {
          "id": "MDRW2100003619.1.5",
          "form": "전 화면이 큰 폴드로",
          "original_form": "전 화면이 큰 폴드로",
          "speaker_id": "2",
          "time": "20210821 08:49"
        },
        {
          "id": "MDRW2100003619.1.6",
          "form": "가려구요",
          "original_form": "가려구요",
          "speaker_id": "2",
          "time": "20210821 08:49"
        },
        {
          "id": "MDRW2100003619.1.7",
          "form": "근데 폴드도 그립톡 같은거 붙혀요?",
          "original_form": "근데 폴드도 그립톡 같은거 붙혀요?",
          "speaker_id": "1",
          "time": "20210821 08:50"
        },
        {
          "id": "MDRW2100003619.1.8",
          "form": "붙힐 수 있나",
          "original_form": "붙힐 수 있나",
          "speaker_id": "1",
          "time": "20210821 08:50"
        },
        {
          "id": "MDRW2100003619.1.9",
          "form": "네 붙히는데",
          "original_form": "네 붙히는데",
          "speaker_id": "2",
          "time": "20210821 08:50"
        },
        {
          "id": "MDRW2100003619.1.10",
          "form": "저는 그립톡은  그냥 일반폰에만",
          "original_form": "저는 그립톡은  그냥 일반폰에만",
          "speaker_id": "2",
          "time": "20210821 08:50"
        },
        {
          "id": "MDRW2100003619.1.11",
          "form": "붙여서 써보고",
          "original_form": "붙여서 써보고",
          "speaker_id": "2",
          "time": "20210821 08:51"
        },
        {
          "id": "MDRW2100003619.1.12",
          "form": "폴드쓰면서부터는",
          "original_form": "폴드쓰면서부터는",
          "speaker_id": "2",
          "time": "20210821 08:51"
        },
        {
          "id": "MDRW2100003619.1.13",
          "form": "안써봤어요",
          "original_form": "안써봤어요",
          "speaker_id": "2",
          "time": "20210821 08:51"
        },
        {
          "id": "MDRW2100003619.1.14",
          "form": "그쵸 아무래도 폴드에 붙이기는 좀ㅎ",
          "original_form": "그쵸 아무래도 폴드에 붙이기는 좀ㅎ",
          "speaker_id": "1",
          "time": "20210821 08:51"
        },
        {
          "id": "MDRW2100003619.1.15",
          "form": "name1님은 최근에 뭐 산거 있나요?",
          "original_form": "&name1&님은 최근에 뭐 산거 있나요?",
          "speaker_id": "2",
          "time": "20210821 08:51"
        },
        {
          "id": "MDRW2100003619.1.16",
          "form": "전 물욕이 없어서..",
          "original_form": "전 물욕이 없어서..",
          "speaker_id": "1",
          "time": "20210821 08:51"
        },
        {
          "id": "MDRW2100003619.1.17",
          "form": "최근 산거라곤 배달음식..ㅎ",
          "original_form": "최근 산거라곤 배달음식..ㅎ",
          "speaker_id": "1",
          "time": "20210821 08:51"
        },
        {
          "id": "MDRW2100003619.1.18",
          "form": "여자분들은",
          "original_form": "여자분들은",
          "speaker_id": "2",
          "time": "20210821 08:52"
        },
        {
          "id": "MDRW2100003619.1.19",
          "form": "옷 신발 가방",
          "original_form": "옷 신발 가방",
          "speaker_id": "2",
          "time": "20210821 08:52"
        },
        {
          "id": "MDRW2100003619.1.20",
          "form": "화장품",
          "original_form": "화장품",
          "speaker_id": "2",
          "time": "20210821 08:52"
        },
        {
          "id": "MDRW2100003619.1.21",
          "form": "에이 너무",
          "original_form": "에이 너무",
          "speaker_id": "1",
          "time": "20210821 08:52"
        },
        {
          "id": "MDRW2100003619.1.22",
          "form": "이런거 사지 않나요 ㅋㅋ",
          "original_form": "이런거 사지 않나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 08:52"
        },
        {
          "id": "MDRW2100003619.1.23",
          "form": "편협한 생각이시다",
          "original_form": "편협한 생각이시다",
          "speaker_id": "1",
          "time": "20210821 08:52"
        },
        {
          "id": "MDRW2100003619.1.24",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 08:52"
        },
        {
          "id": "MDRW2100003619.1.25",
          "form": "그것도 사람 나름인거고",
          "original_form": "그것도 사람 나름인거고",
          "speaker_id": "1",
          "time": "20210821 08:52"
        },
        {
          "id": "MDRW2100003619.1.26",
          "form": "전 가방을 안써서",
          "original_form": "전 가방을 안써서",
          "speaker_id": "2",
          "time": "20210821 08:53"
        },
        {
          "id": "MDRW2100003619.1.27",
          "form": "전 지갑도 안들고다녀요",
          "original_form": "전 지갑도 안들고다녀요",
          "speaker_id": "1",
          "time": "20210821 08:53"
        },
        {
          "id": "MDRW2100003619.1.28",
          "form": "귀찮아서",
          "original_form": "귀찮아서",
          "speaker_id": "1",
          "time": "20210821 08:53"
        },
        {
          "id": "MDRW2100003619.1.29",
          "form": "삼성페이로",
          "original_form": "삼성페이로",
          "speaker_id": "2",
          "time": "20210821 08:53"
        },
        {
          "id": "MDRW2100003619.1.30",
          "form": "해결하시나요?",
          "original_form": "해결하시나요?",
          "speaker_id": "2",
          "time": "20210821 08:53"
        },
        {
          "id": "MDRW2100003619.1.31",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 08:53"
        },
        {
          "id": "MDRW2100003619.1.32",
          "form": "네 삼페너무 편해요 그래서 갤럭시 못버리겠..",
          "original_form": "네 삼페너무 편해요 그래서 갤럭시 못버리겠..",
