      "utterance": [
        {
          "id": "MDRW2100008186.1.1",
          "form": "그래도말야 요즘은",
          "original_form": "그래도말야 요즘은",
          "speaker_id": "1",
          "time": "20210822 13:55"
        },
        {
          "id": "MDRW2100008186.1.2",
          "form": "운동을 잘 못해서 그런가 몸상태가 영 별로라",
          "original_form": "운동을 잘 못해서 그런가 몸상태가 영 별로라",
          "speaker_id": "1",
          "time": "20210822 13:55"
        },
        {
          "id": "MDRW2100008186.1.3",
          "form": "강제적으로라도 운동을 해야할까봐",
          "original_form": "강제적으로라도 운동을 해야할까봐",
          "speaker_id": "1",
          "time": "20210822 13:55"
        },
        {
          "id": "MDRW2100008186.1.4",
          "form": "무슨 운동 하시게여",
          "original_form": "무슨 운동 하시게여",
          "speaker_id": "2",
          "time": "20210822 13:56"
        },
        {
          "id": "MDRW2100008186.1.5",
          "form": "저도 운동의 필요성은 느끼지만",
          "original_form": "저도 운동의 필요성은 느끼지만",
          "speaker_id": "2",
          "time": "20210822 13:56"
        },
        {
          "id": "MDRW2100008186.1.6",
          "form": "뭔가 마음먹기가 힘들어요",
          "original_form": "뭔가 마음먹기가 힘들어요",
          "speaker_id": "2",
          "time": "20210822 13:56"
        },
        {
          "id": "MDRW2100008186.1.7",
          "form": "우선은 가벼운 유산소을 해야하지 않을까?",
          "original_form": "우선은 가벼운 유산소을 해야하지 않을까?",
          "speaker_id": "1",
          "time": "20210822 13:57"
        },
        {
          "id": "MDRW2100008186.1.8",
          "form": "근력운동이 중요한건 잘 알지만",
          "original_form": "근력운동이 중요한건 잘 알지만",
          "speaker_id": "1",
          "time": "20210822 13:57"
        },
        {
          "id": "MDRW2100008186.1.9",
          "form": "안한 텀이 넘 길어서 갑자기 했다가는 다칠까봐 무서워",
          "original_form": "안한 텀이 넘 길어서 갑자기 했다가는 다칠까봐 무서워",
          "speaker_id": "1",
          "time": "20210822 13:58"
        },
        {
          "id": "MDRW2100008186.1.10",
          "form": "전...그래서 솔직히",
          "original_form": "전...그래서 솔직히",
          "speaker_id": "2",
          "time": "20210822 13:58"
        },
        {
          "id": "MDRW2100008186.1.11",
          "form": "링피트를 탐내하고 있어요",
          "original_form": "링피트를 탐내하고 있어요",
          "speaker_id": "2",
          "time": "20210822 13:58"
        },
        {
          "id": "MDRW2100008186.1.12",
          "form": "링피트와 함께라면 나의 체계적인 운동을 링피트가 책임져줄거 같아서 ㅎ",
          "original_form": "링피트와 함께라면 나의 체계적인 운동을 링피트가 책임져줄거 같아서 ㅎ{emoji:…}",
          "speaker_id": "2",
          "time": "20210822 13:59"
        },
        {
          "id": "MDRW2100008186.1.13",
          "form": "ㅋㅋㅋㅋㅋ 운동을 홀로 할 자신은 없고",
          "original_form": "ㅋㅋㅋㅋㅋ 운동을 홀로 할 자신은 없고",
          "speaker_id": "1",
          "time": "20210822 13:59"
        },
        {
          "id": "MDRW2100008186.1.14",
          "form": "피티를 끊기엔 여건이 좋지 않으니",
          "original_form": "피티를 끊기엔 여건이 좋지 않으니",
          "speaker_id": "1",
          "time": "20210822 13:59"
        },
        {
          "id": "MDRW2100008186.1.15",
          "form": "전문 성우가 붙은 링이 나를 독촉해주기를 바라는거구만",
          "original_form": "전문 성우가 붙은 링이 나를 독촉해주기를 바라는거구만",
          "speaker_id": "1",
          "time": "20210822 13:59"
        },
        {
          "id": "MDRW2100008186.1.16",
          "form": "맞습니다...",
          "original_form": "맞습니다...",
          "speaker_id": "2",
          "time": "20210822 14:01"
        },
        {
          "id": "MDRW2100008186.1.17",
          "form": "그리고 링피트를 사면 산 돈이 아까워서라도",
          "original_form": "그리고 링피트를 사면 산 돈이 아까워서라도",
          "speaker_id": "2",
          "time": "20210822 14:01"
        },
        {
          "id": "MDRW2100008186.1.18",
          "form": "내 자신이 알아서 운동을 하지 않을까 하는 생각도 있어요 ㅋㅋㅋㅋ",
          "original_form": "내 자신이 알아서 운동을 하지 않을까 하는 생각도 있어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:01"
        },
        {
          "id": "MDRW2100008186.1.19",
          "form": "ㅋㅋㅋ 그래도 조심하래.. 내 친구중에 물리치료사 하는 애 있는데",
          "original_form": "ㅋㅋㅋ 그래도 조심하래.. 내 친구중에 물리치료사 하는 애 있는데",
          "speaker_id": "1",
          "time": "20210822 14:02"
        },
        {
          "id": "MDRW2100008186.1.20",
          "form": "요즘 이삼십대 링피트 하다가 무릎 관절 ㅂㅂ해서",
          "original_form": "요즘 이삼십대 링피트 하다가 무릎 관절 ㅂㅂ해서",
          "speaker_id": "1",
          "time": "20210822 14:02"
        },
        {
          "id": "MDRW2100008186.1.21",
          "form": "오는애들 엄청 많대",
          "original_form": "오는애들 엄청 많대",
          "speaker_id": "1",
          "time": "20210822 14:02"
        },
        {
          "id": "MDRW2100008186.1.22",
          "form": "헐 진짜요??? 그건 또 몰랐네요",
          "original_form": "헐 진짜요??? 그건 또 몰랐네요",
          "speaker_id": "2",
          "time": "20210822 14:03"
        },
        {
          "id": "MDRW2100008186.1.23",
          "form": "이번에 링피트 체험 해봤는데 확실히",
          "original_form": "이번에 링피트 체험 해봤는데 확실히",
          "speaker_id": "2",
          "time": "20210822 14:03"
        },
        {
          "id": "MDRW2100008186.1.24",
          "form": "스쿼트를 많이 시키긴 하더라구요 점프를 스쿼트로 해야해서 ㅋㅋㅋㅋ",
          "original_form": "스쿼트를 많이 시키긴 하더라구요 점프를 스쿼트로 해야해서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:03"
        },
        {
          "id": "MDRW2100008186.1.25",
          "form": "응응 그게 제대로 할줄 아는 사람이면 문제가 없는데",
          "original_form": "응응 그게 제대로 할줄 아는 사람이면 문제가 없는데",
          "speaker_id": "1",
          "time": "20210822 14:04"
        },
        {
          "id": "MDRW2100008186.1.26",
          "form": "운동이랑 연이 없던 사람은 뭐가 바른자세인지 모르니까",
          "original_form": "운동이랑 연이 없던 사람은 뭐가 바른자세인지 모르니까",
          "speaker_id": "1",
          "time": "20210822 14:04"
        },
        {
          "id": "MDRW2100008186.1.27",
          "form": "오히려 몸 다치기만 한다더라구",
          "original_form": "오히려 몸 다치기만 한다더라구",
          "speaker_id": "1",
          "time": "20210822 14:04"
        },
        {
          "id": "MDRW2100008186.1.28",
          "form": "ㅇㅎ... 아이고 ㅠㅠㅠㅠㅠ",
          "original_form": "ㅇㅎ... 아이고 ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 14:06"
        },
        {
          "id": "MDRW2100008186.1.29",
          "form": "전 근데 뭐...다리근육은 기본이 돼 있어서",
          "original_form": "전 근데 뭐...다리근육은 기본이 돼 있어서",
          "speaker_id": "2",
          "time": "20210822 14:07"
        },
        {
          "id": "MDRW2100008186.1.30",
          "form": "그런 걱정은 잘 안해욬ㅋㅋㅋㅋㅋ",
          "original_form": "그런 걱정은 잘 안해욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:07"
        },
        {
          "id": "MDRW2100008186.1.31",
          "form": "근육과... 관절은 다른 문제니까",
          "original_form": "근육과... 관절은 다른 문제니까",
          "speaker_id": "1",
          "time": "20210822 14:08"
        },
        {
          "id": "MDRW2100008186.1.32",
          "form": "조심은 하자...",
          "original_form": "조심은 하자...",
          "speaker_id": "1",
          "time": "20210822 14:08"
        },
        {
          "id": "MDRW2100008186.1.33",
          "form": "왕창 뛰당기다가 비오면 오른쪽 무릎이 쑤시는 사람이.. 말합니다..",
          "original_form": "왕창 뛰당기다가 비오면 오른쪽 무릎이 쑤시는 사람이.. 말합니다..",
          "speaker_id": "1",
          "time": "20210822 14:09"
        },
        {
          "id": "MDRW2100008186.1.34",
          "form": "ㅇㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:10"
        },
        {
          "id": "MDRW2100008186.1.35",
          "form": "그건...그렇죠...",
          "original_form": "그건...그렇죠...",
          "speaker_id": "2",
          "time": "20210822 14:10"
        },
        {
          "id": "MDRW2100008186.1.36",
          "form": "하지만 중요한 것은 링피트가 저에게 없다는 겁니다 짜란~",
          "original_form": "하지만 중요한 것은 링피트가 저에게 없다는 겁니다 짜란~",
          "speaker_id": "2",
          "time": "20210822 14:11"
