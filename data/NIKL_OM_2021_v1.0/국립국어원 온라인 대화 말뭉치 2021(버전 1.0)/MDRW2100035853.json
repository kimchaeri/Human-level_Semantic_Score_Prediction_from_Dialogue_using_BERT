      "utterance": [
        {
          "id": "MDRW2100035853.1.1",
          "form": "서울로보냄",
          "original_form": "서울로보냄",
          "speaker_id": "2",
          "time": "20210830 09:11"
        },
        {
          "id": "MDRW2100035853.1.2",
          "form": "난안먹엉....ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "난안먹엉....ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 09:11"
        },
        {
          "id": "MDRW2100035853.1.3",
          "form": "고민하다 케이준이랑 쉬림프햇당 ㅋㅋ",
          "original_form": "고민하다 케이준이랑 쉬림프햇당 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 09:11"
        },
        {
          "id": "MDRW2100035853.1.4",
          "form": "ㅋㅋㅋ아ㅋㅋ아부지가 그만 드시고 싶다한 목소리가 들리네요",
          "original_form": "ㅋㅋㅋ아ㅋㅋ아부지가 그만 드시고 싶다한 목소리가 들리네요",
          "speaker_id": "1",
          "time": "20210830 09:11"
        },
        {
          "id": "MDRW2100035853.1.5",
          "form": "어제는 족발시켜드림 ㅋㅋㅋ",
          "original_form": "어제는 족발시켜드림 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 09:12"
        },
        {
          "id": "MDRW2100035853.1.6",
          "form": "ㅋㅋㅋ오 족발 맛있쥐!!!",
          "original_form": "ㅋㅋㅋ오 족발 맛있쥐!!!",
          "speaker_id": "1",
          "time": "20210830 09:13"
        },
        {
          "id": "MDRW2100035853.1.7",
          "form": "사람들후기좋길래 시켜봣는데 ㅋㅋ동네에 새로생긴집이라고 알더라고  ㅋㅋㅋ",
          "original_form": "사람들후기좋길래 시켜봣는데 ㅋㅋ동네에 새로생긴집이라고 알더라고  ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 09:13"
        },
        {
          "id": "MDRW2100035853.1.8",
          "form": "젊은 남자애들이 한다면서 ㅋㅋ 맛도괜찮앗뎅",
          "original_form": "젊은 남자애들이 한다면서 ㅋㅋ 맛도괜찮앗뎅",
          "speaker_id": "2",
          "time": "20210830 09:13"
        },
        {
          "id": "MDRW2100035853.1.9",
          "form": "오홍ㅋㅋ 족발은 진짜 할인해도 비싸",
          "original_form": "오홍ㅋㅋ 족발은 진짜 할인해도 비싸",
          "speaker_id": "1",
          "time": "20210830 09:14"
        },
        {
          "id": "MDRW2100035853.1.10",
          "form": "맞아유 ㅋㅋㅋㅋ 할인두읍서용 ㅠㅠ",
          "original_form": "맞아유 ㅋㅋㅋㅋ 할인두읍서용 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 09:14"
        },
        {
          "id": "MDRW2100035853.1.11",
          "form": "ㅠㅠ난 어제 파김치에 대패삼겹살 먹었는데 꿀맛!!!",
          "original_form": "ㅠㅠ난 어제 파김치에 대패삼겹살 먹었는데 꿀맛!!!",
          "speaker_id": "1",
          "time": "20210830 09:18"
        },
        {
          "id": "MDRW2100035853.1.12",
          "form": "헐 ㅠㅠ",
          "original_form": "헐 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 09:18"
        },
        {
          "id": "MDRW2100035853.1.13",
          "form": "맛잇겟다ㅠㅠ",
          "original_form": "맛잇겟다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 09:18"
        },
        {
          "id": "MDRW2100035853.1.14",
          "form": "어제 저녁엔 맛난거드셨나유,!!!",
          "original_form": "어제 저녁엔 맛난거드셨나유,!!!",
          "speaker_id": "1",
          "time": "20210830 09:19"
        },
        {
          "id": "MDRW2100035853.1.15",
          "form": "뭐먹엇더라",
          "original_form": "뭐먹엇더라",
          "speaker_id": "2",
          "time": "20210830 09:22"
        },
        {
          "id": "MDRW2100035853.1.16",
          "form": "그냥집밥먹음 ㅋㅋㅋㅋ",
          "original_form": "그냥집밥먹음 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 09:22"
        },
        {
          "id": "MDRW2100035853.1.17",
          "form": "그러고 11시엔 둘이 냉동식품 에어해먹고 ㅋㅋㅋ",
          "original_form": "그러고 11시엔 둘이 냉동식품 에어해먹고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 09:22"
        },
        {
          "id": "MDRW2100035853.1.18",
          "form": "내가 생초코렛삿다고말햇쥬? 별로....야...ㅡㅡ",
          "original_form": "내가 생초코렛삿다고말햇쥬? 별로....야...ㅡㅡ",
          "speaker_id": "2",
          "time": "20210830 09:22"
        },
        {
          "id": "MDRW2100035853.1.19",
          "form": "헐! 왜 별로죠!!!",
          "original_form": "헐! 왜 별로죠!!!",
          "speaker_id": "1",
          "time": "20210830 09:23"
        },
        {
          "id": "MDRW2100035853.1.20",
          "form": "아몬드들어간건데",
          "original_form": "아몬드들어간건데",
          "speaker_id": "2",
          "time": "20210830 09:23"
        },
        {
          "id": "MDRW2100035853.1.21",
