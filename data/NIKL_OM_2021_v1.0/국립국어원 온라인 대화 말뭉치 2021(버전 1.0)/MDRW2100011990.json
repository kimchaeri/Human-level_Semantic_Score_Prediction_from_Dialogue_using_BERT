      "utterance": [
        {
          "id": "MDRW2100011990.1.1",
          "form": "와 바디워시도 자몽이네요",
          "original_form": "와 바디워시도 자몽이네요",
          "speaker_id": "2",
          "time": "20210824 18:14"
        },
        {
          "id": "MDRW2100011990.1.2",
          "form": "전 자몽을 무지 좋아합니다",
          "original_form": "전 자몽을 무지 좋아합니다",
          "speaker_id": "1",
          "time": "20210824 18:14"
        },
        {
          "id": "MDRW2100011990.1.3",
          "form": "자몽향이 취향에 맞나봐요!",
          "original_form": "자몽향이 취향에 맞나봐요!",
          "speaker_id": "2",
          "time": "20210824 18:14"
        },
        {
          "id": "MDRW2100011990.1.4",
          "form": "원래 라임향을 좋아했는데",
          "original_form": "원래 라임향을 좋아했는데",
          "speaker_id": "1",
          "time": "20210824 18:14"
        },
        {
          "id": "MDRW2100011990.1.5",
          "form": "라임도 좋죠",
          "original_form": "라임도 좋죠",
          "speaker_id": "2",
          "time": "20210824 18:14"
        },
        {
          "id": "MDRW2100011990.1.6",
          "form": "애경인가 거기꺼 쓰고 나서 바꼈어요",
          "original_form": "애경인가 거기꺼 쓰고 나서 바꼈어요",
          "speaker_id": "1",
          "time": "20210824 18:14"
        },
        {
          "id": "MDRW2100011990.1.7",
          "form": "거기 라임 바디워시를 샀었는데 모기향이 나는거있죠",
          "original_form": "거기 라임 바디워시를 샀었는데 모기향이 나는거있죠",
          "speaker_id": "1",
          "time": "20210824 18:14"
        },
        {
          "id": "MDRW2100011990.1.8",
          "form": "아하 기억해둬야겠네요",
          "original_form": "아하 기억해둬야겠네요",
          "speaker_id": "2",
          "time": "20210824 18:15"
        },
        {
          "id": "MDRW2100011990.1.9",
          "form": "그래서 라임과 멀어졌습니다",
          "original_form": "그래서 라임과 멀어졌습니다",
          "speaker_id": "1",
          "time": "20210824 18:15"
        },
        {
          "id": "MDRW2100011990.1.10",
          "form": "모기향이라니 세상에",
          "original_form": "모기향이라니 세상에",
          "speaker_id": "2",
          "time": "20210824 18:15"
        },
        {
          "id": "MDRW2100011990.1.11",
          "form": "기억하세요 절대 눈길도 주지마세요",
          "original_form": "기억하세요 절대 눈길도 주지마세요",
          "speaker_id": "1",
          "time": "20210824 18:15"
        },
        {
          "id": "MDRW2100011990.1.12",
          "form": "애경이 이간질한거요",
          "original_form": "애경이 이간질한거요",
          "speaker_id": "2",
          "time": "20210824 18:15"
        },
        {
          "id": "MDRW2100011990.1.13",
          "form": "거예요",
          "original_form": "거예요",
          "speaker_id": "2",
          "time": "20210824 18:15"
        },
        {
          "id": "MDRW2100011990.1.14",
          "form": "라임과 지원님 사이를",
          "original_form": "라임과 지원님 사이를",
          "speaker_id": "2",
          "time": "20210824 18:15"
        },
        {
          "id": "MDRW2100011990.1.15",
          "form": "그런걸까요",
          "original_form": "그런걸까요",
          "speaker_id": "1",
          "time": "20210824 18:15"
        },
        {
          "id": "MDRW2100011990.1.16",
          "form": "아 그런 의도가 있었다니 제가 너무 둔했군요",
          "original_form": "아 그런 의도가 있었다니 제가 너무 둔했군요",
          "speaker_id": "1",
          "time": "20210824 18:15"
        },
        {
          "id": "MDRW2100011990.1.17",
          "form": "자몽향 바디워시는 만족하시나요?",
          "original_form": "자몽향 바디워시는 만족하시나요?",
          "speaker_id": "2",
          "time": "20210824 18:15"
        },
        {
          "id": "MDRW2100011990.1.18",
          "form": "아 처음에 만족한다고 하셨구나",
          "original_form": "아 처음에 만족한다고 하셨구나",
          "speaker_id": "2",
          "time": "20210824 18:16"
        },
        {
          "id": "MDRW2100011990.1.19",
          "form": "그냥저냥 나쁘지않은 거 같아요",
          "original_form": "그냥저냥 나쁘지않은 거 같아요",
          "speaker_id": "1",
          "time": "20210824 18:16"
        }
      ]
