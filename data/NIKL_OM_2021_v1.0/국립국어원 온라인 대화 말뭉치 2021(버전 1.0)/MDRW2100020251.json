      "utterance": [
        {
          "id": "MDRW2100020251.1.1",
          "form": "결혼 하셨나요!?",
          "original_form": "결혼 하셨나요!?",
          "speaker_id": "1",
          "time": "20210828 00:41"
        },
        {
          "id": "MDRW2100020251.1.2",
          "form": "아아ㅎㅎ 결혼했어요ㅎ",
          "original_form": "아아ㅎㅎ 결혼했어요ㅎ",
          "speaker_id": "2",
          "time": "20210828 00:42"
        },
        {
          "id": "MDRW2100020251.1.3",
          "form": "아이가 한명있습니다ㅎ",
          "original_form": "아이가 한명있습니다ㅎ",
          "speaker_id": "2",
          "time": "20210828 00:42"
        },
        {
          "id": "MDRW2100020251.1.4",
          "form": "오..",
          "original_form": "오..",
          "speaker_id": "1",
          "time": "20210828 00:42"
        },
        {
          "id": "MDRW2100020251.1.5",
          "form": "결혼한지 몇년되셧나요??",
          "original_form": "결혼한지 몇년되셧나요??",
          "speaker_id": "1",
          "time": "20210828 00:42"
        },
        {
          "id": "MDRW2100020251.1.6",
          "form": "3년차됬어요ㅎㅎ",
          "original_form": "3년차됬어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020251.1.7",
          "form": "결혼하셨어요?",
          "original_form": "결혼하셨어요?",
          "speaker_id": "2",
          "time": "20210828 00:43"
        },
        {
          "id": "MDRW2100020251.1.8",
          "form": "아뇨 ㅎ 전 연애도 안하고 결혼도 안한 솔로입니다",
          "original_form": "아뇨 ㅎ 전 연애도 안하고 결혼도 안한 솔로입니다",
          "speaker_id": "1",
          "time": "20210828 00:44"
        },
        {
          "id": "MDRW2100020251.1.9",
          "form": "3년차면 한창 좋을때아닌가요 ㅎㅎ",
          "original_form": "3년차면 한창 좋을때아닌가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 00:44"
        },
        {
          "id": "MDRW2100020251.1.10",
          "form": "아아ㅎㅎ 상담해드릴까요?",
          "original_form": "아아ㅎㅎ 상담해드릴까요?",
          "speaker_id": "2",
          "time": "20210828 00:46"
        },
        {
          "id": "MDRW2100020251.1.11",
          "form": "이벤트도 할겸?",
          "original_form": "이벤트도 할겸?",
          "speaker_id": "2",
          "time": "20210828 00:46"
        },
        {
          "id": "MDRW2100020251.1.12",
          "form": "글쎄요 ㅎㅎ제가 상담할거리가 있을까요..",
          "original_form": "글쎄요 ㅎㅎ제가 상담할거리가 있을까요..",
          "speaker_id": "1",
          "time": "20210828 00:47"
        },
        {
          "id": "MDRW2100020251.1.13",
          "form": "3년차면 한창 좋을때라 생각하시는거에요?ㅎ 아이가있어서ㅎ",
          "original_form": "3년차면 한창 좋을때라 생각하시는거에요?ㅎ 아이가있어서ㅎ",
          "speaker_id": "2",
          "time": "20210828 00:47"
        },
        {
          "id": "MDRW2100020251.1.14",
          "form": "서로 왠수입니다ㅎㅎㅎ",
          "original_form": "서로 왠수입니다ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:47"
        },
        {
          "id": "MDRW2100020251.1.15",
          "form": "3년차면 신혼아닌가요",
          "original_form": "3년차면 신혼아닌가요",
          "speaker_id": "1",
          "time": "20210828 00:47"
        },
        {
          "id": "MDRW2100020251.1.16",
          "form": "아이는 몇살이에요??",
          "original_form": "아이는 몇살이에요??",
          "speaker_id": "1",
          "time": "20210828 00:47"
        },
        {
          "id": "MDRW2100020251.1.17",
          "form": "결혼에대한 궁금한거 있으시면 말씀하세요ㅎ",
          "original_form": "결혼에대한 궁금한거 있으시면 말씀하세요ㅎ",
          "speaker_id": "2",
          "time": "20210828 00:48"
        },
        {
          "id": "MDRW2100020251.1.18",
          "form": "아이 4살이요ㅎㅎ",
          "original_form": "아이 4살이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:48"
        },
        {
          "id": "MDRW2100020251.1.19",
          "form": "속도위반으러 결혼해서ㅎㅎ",
          "original_form": "속도위반으러 결혼해서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:48"
        },
        {
          "id": "MDRW2100020251.1.20",
          "form": "아아 ㅋㅋㅋ속도위반하면",
          "original_form": "아아 ㅋㅋㅋ속도위반하면",
          "speaker_id": "1",
          "time": "20210828 00:48"
        },
        {
          "id": "MDRW2100020251.1.21",
          "form": "양쪽 부모님 반응은 어땠나요..",
          "original_form": "양쪽 부모님 반응은 어땠나요..",
          "speaker_id": "1",
          "time": "20210828 00:48"
        },
        {
          "id": "MDRW2100020251.1.22",
          "form": "좋아하셬ㅅ아요",
          "original_form": "좋아하셬ㅅ아요",
          "speaker_id": "2",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100020251.1.23",
          "form": "처음에는 싫어하셨는데",
          "original_form": "처음에는 싫어하셨는데",
          "speaker_id": "2",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100020251.1.24",
          "form": "나중에는 너무 좋아하시더라구요",
          "original_form": "나중에는 너무 좋아하시더라구요",
          "speaker_id": "2",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100020251.1.25",
          "form": "저희집이면 아마 몽둥이로 뚜들겨 맞고 쫓겨날것같은데",
          "original_form": "저희집이면 아마 몽둥이로 뚜들겨 맞고 쫓겨날것같은데",
          "speaker_id": "1",
          "time": "20210828 00:51"
        },
        {
          "id": "MDRW2100020251.1.26",
          "form": "요즘엔 엄마도 손주보고싶어하더라구요",
          "original_form": "요즘엔 엄마도 손주보고싶어하더라구요",
          "speaker_id": "1",
          "time": "20210828 00:52"
        },
        {
          "id": "MDRW2100020251.1.27",
          "form": "에이 안그래요 막상 그상황되면",
          "original_form": "에이 안그래요 막상 그상황되면",
          "speaker_id": "2",
          "time": "20210828 00:52"
        },
        {
          "id": "MDRW2100020251.1.28",
          "form": "그래도 속도위반은 못참으시는것 같아요",
          "original_form": "그래도 속도위반은 못참으시는것 같아요",
          "speaker_id": "1",
          "time": "20210828 00:52"
        },
        {
          "id": "MDRW2100020251.1.29",
          "form": "오히려 건강하게 더 챙겨주세요",
          "original_form": "오히려 건강하게 더 챙겨주세요",
          "speaker_id": "2",
          "time": "20210828 00:52"
        }
      ]
    }
