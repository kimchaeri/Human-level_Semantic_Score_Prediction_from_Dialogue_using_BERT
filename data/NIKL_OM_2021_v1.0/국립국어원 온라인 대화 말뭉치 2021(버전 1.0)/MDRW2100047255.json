      "utterance": [
        {
          "id": "MDRW2100047255.1.1",
          "form": "백신 맞으셨나요?!",
          "original_form": "백신 맞으셨나요?!",
          "speaker_id": "2",
          "time": "20210826 01:33"
        },
        {
          "id": "MDRW2100047255.1.2",
          "form": "아뇨! 저는 9월달에 예약했어요!",
          "original_form": "아뇨! 저는 9월달에 예약했어요!",
          "speaker_id": "1",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100047255.1.3",
          "form": "헉 그러시구나 전 어제 잔여백신 잡아서 맞고왔어요!!!",
          "original_form": "헉 그러시구나 전 어제 잔여백신 잡아서 맞고왔어요!!!",
          "speaker_id": "2",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100047255.1.4",
          "form": "우헤헤...",
          "original_form": "우헤헤...",
          "speaker_id": "2",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100047255.1.5",
          "form": "헐 대박",
          "original_form": "헐 대박",
          "speaker_id": "1",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100047255.1.6",
          "form": "팁좀요!",
          "original_form": "팁좀요!",
          "speaker_id": "1",
          "time": "20210826 01:35"
        },
        {
          "id": "MDRW2100047255.1.7",
          "form": "앗..",
          "original_form": "앗..",
          "speaker_id": "2",
          "time": "20210826 01:35"
        },
        {
          "id": "MDRW2100047255.1.8",
          "form": "저 진짜 얼떨결에 잡았어요",
          "original_form": "저 진짜 얼떨결에 잡았어요",
          "speaker_id": "2",
          "time": "20210826 01:35"
        },
        {
          "id": "MDRW2100047255.1.9",
          "form": "타이밍이 딱 맞았나요?",
          "original_form": "타이밍이 딱 맞았나요?",
          "speaker_id": "1",
          "time": "20210826 01:35"
        },
        {
          "id": "MDRW2100047255.1.10",
          "form": "드라마보다가 눌렀는데 되어서ㅠㅋㅋㅋㅋ 이게 팁이랄게 없더라고요...",
          "original_form": "드라마보다가 눌렀는데 되어서ㅠㅋㅋㅋㅋ 이게 팁이랄게 없더라고요...",
          "speaker_id": "2",
          "time": "20210826 01:35"
        },
        {
          "id": "MDRW2100047255.1.11",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210826 01:35"
        },
        {
          "id": "MDRW2100047255.1.12",
          "form": "그냥 지이이잉 하고 울려서",
          "original_form": "그냥 지이이잉 하고 울려서",
          "speaker_id": "2",
          "time": "20210826 01:35"
        },
        {
          "id": "MDRW2100047255.1.13",
          "form": "눌렀는데 됐어요",
          "original_form": "눌렀는데 됐어요",
          "speaker_id": "2",
          "time": "20210826 01:36"
        },
        {
          "id": "MDRW2100047255.1.14",
          "form": "헐 그걸로 돼요?",
          "original_form": "헐 그걸로 돼요?",
          "speaker_id": "1",
          "time": "20210826 01:36"
        },
        {
          "id": "MDRW2100047255.1.15",
          "form": "카카오톡??",
          "original_form": "카카오톡??",
          "speaker_id": "1",
          "time": "20210826 01:36"
        },
        {
          "id": "MDRW2100047255.1.16",
          "form": "네네!!!",
          "original_form": "네네!!!",
          "speaker_id": "2",
          "time": "20210826 01:37"
        },
        {
          "id": "MDRW2100047255.1.17",
          "form": "다들 그 반응을 하시더라고요!!!!",
          "original_form": "다들 그 반응을 하시더라고요!!!!",
          "speaker_id": "2",
          "time": "20210826 01:37"
        },
        {
          "id": "MDRW2100047255.1.18",
          "form": "알람이 울렸는데 어떻게 잡았냐는...그 반응..",
          "original_form": "알람이 울렸는데 어떻게 잡았냐는...그 반응..",
          "speaker_id": "2",
          "time": "20210826 01:37"
        },
        {
          "id": "MDRW2100047255.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:37"
        },
        {
          "id": "MDRW2100047255.1.20",
          "form": "당연하죠!",
          "original_form": "당연하죠!",
          "speaker_id": "1",
          "time": "20210826 01:37"
        },
        {
          "id": "MDRW2100047255.1.21",
          "form": "제가 알기로는 그거 발생하고 5초후에 알림온댔는데..?",
          "original_form": "제가 알기로는 그거 발생하고 5초후에 알림온댔는데..?",
          "speaker_id": "1",
          "time": "20210826 01:37"
        },
        {
          "id": "MDRW2100047255.1.22",
          "form": "헐 그래요?",
          "original_form": "헐 그래요?",
          "speaker_id": "2",
          "time": "20210826 01:38"
        },
        {
          "id": "MDRW2100047255.1.23",
          "form": "저 심지어 늦게 들어갔는데도 잡아서 놀랐는데....",
          "original_form": "저 심지어 늦게 들어갔는데도 잡아서 놀랐는데....",
          "speaker_id": "2",
          "time": "20210826 01:38"
        },
        {
          "id": "MDRW2100047255.1.24",
          "form": "신기하네요 근데 후기보면 다들 얼떨결에 잡았다고 써서",
          "original_form": "신기하네요 근데 후기보면 다들 얼떨결에 잡았다고 써서",
          "speaker_id": "2",
          "time": "20210826 01:38"
        },
        {
          "id": "MDRW2100047255.1.25",
          "form": "진짜 될놈될",
          "original_form": "진짜 될놈될",
          "speaker_id": "1",
          "time": "20210826 01:39"
        },
        {
          "id": "MDRW2100047255.1.26",
          "form": "저도 좀 어이없었는데...그렇게 잡아서...저도 팁이랄게...없다는 점이........",
          "original_form": "저도 좀 어이없었는데...그렇게 잡아서...저도 팁이랄게...없다는 점이........",
          "speaker_id": "2",
          "time": "20210826 01:39"
        },
        {
          "id": "MDRW2100047255.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아니에요!!! name1님도 내일 우연히 집게 될 걸료?!?!?!?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아니에요!!! &name1&님도 내일 우연히 집게 될 걸료?!?!?!?",
          "speaker_id": "2",
          "time": "20210826 01:39"
        },
        {
          "id": "MDRW2100047255.1.28",
          "form": "제 기운 드립니다",
          "original_form": "제 기운 드립니다",
          "speaker_id": "2",
          "time": "20210826 01:39"
        },
        {
          "id": "MDRW2100047255.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 와 그랬으면 좋겠네요!!! 저도  잡으면 막 자랑해야지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 와 그랬으면 좋겠네요!!! 저도  잡으면 막 자랑해야지",
          "speaker_id": "1",
          "time": "20210826 01:39"
        },
        {
          "id": "MDRW2100047255.1.30",
          "form": "저 요즘 여드름 너무 심해요.",
