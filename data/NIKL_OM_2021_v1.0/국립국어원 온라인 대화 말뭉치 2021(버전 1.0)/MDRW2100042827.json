      "utterance": [
        {
          "id": "MDRW2100042827.1.1",
          "form": "제주도 해장국 되게 유명하잖아요",
          "original_form": "제주도 해장국 되게 유명하잖아요",
          "speaker_id": "2",
          "time": "20210907 09:45"
        },
        {
          "id": "MDRW2100042827.1.2",
          "form": "해장국과 술을 부르는 날씨랄까여",
          "original_form": "해장국과 술을 부르는 날씨랄까여",
          "speaker_id": "1",
          "time": "20210907 09:45"
        },
        {
          "id": "MDRW2100042827.1.3",
          "form": "그 은이네해장국 맞나 그거 수도권에 체인점 많아서 먹어봤은데",
          "original_form": "그 은이네해장국 맞나 그거 수도권에 체인점 많아서 먹어봤은데",
          "speaker_id": "2",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042827.1.4",
          "form": "오으 완전 제스타일",
          "original_form": "오으 완전 제스타일",
          "speaker_id": "2",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042827.1.5",
          "form": "흐린날엔 국물이져",
          "original_form": "흐린날엔 국물이져",
          "speaker_id": "2",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042827.1.6",
          "form": "맞아여",
          "original_form": "맞아여",
          "speaker_id": "1",
          "time": "20210907 09:48"
        },
        {
          "id": "MDRW2100042827.1.7",
          "form": "집 근처에 본점이라고 들었어요",
          "original_form": "집 근처에 본점이라고 들었어요",
          "speaker_id": "1",
          "time": "20210907 09:48"
        },
        {
          "id": "MDRW2100042827.1.8",
          "form": "아직 제주는 반팔티 입는데 님 계신곳은 어떠신가요",
          "original_form": "아직 제주는 반팔티 입는데 님 계신곳은 어떠신가요",
          "speaker_id": "1",
          "time": "20210907 09:48"
        },
        {
          "id": "MDRW2100042827.1.9",
          "form": "세상에 본점이라니",
          "original_form": "세상에 본점이라니",
          "speaker_id": "2",
          "time": "20210907 09:49"
        },
        {
          "id": "MDRW2100042827.1.10",
          "form": "가보시고 후기들려주새요 ㅋㅋㅋㅋ",
          "original_form": "가보시고 후기들려주새요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:49"
        },
        {
          "id": "MDRW2100042827.1.11",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210907 09:49"
        },
        {
          "id": "MDRW2100042827.1.12",
          "form": "우선 비가 어제부터 계속 오구요",
          "original_form": "우선 비가 어제부터 계속 오구요",
          "speaker_id": "2",
          "time": "20210907 09:49"
        },
        {
          "id": "MDRW2100042827.1.13",
          "form": "제주는 제가 가고 나서 쨍하게 좋은 날이 얼마 없더라구요",
          "original_form": "제주는 제가 가고 나서 쨍하게 좋은 날이 얼마 없더라구요",
          "speaker_id": "1",
          "time": "20210907 09:49"
        },
        {
          "id": "MDRW2100042827.1.14",
          "form": "저는 급 쌀쌀해져서 반팔에 긴팔 입엉ㅅ",
          "original_form": "저는 급 쌀쌀해져서 반팔에 긴팔 입엉ㅅ",
          "speaker_id": "2",
          "time": "20210907 09:49"
        },
        {
          "id": "MDRW2100042827.1.15",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "1",
          "time": "20210907 09:50"
        },
        {
          "id": "MDRW2100042827.1.16",
          "form": "저도 이제 가을옷 준비해야게네요",
          "original_form": "저도 이제 가을옷 준비해야게네요",
          "speaker_id": "1",
          "time": "20210907 09:50"
        },
        {
          "id": "MDRW2100042827.1.17",
          "form": "해 쨍할때 또 제주도 참 예쁘잖아요",
          "original_form": "해 쨍할때 또 제주도 참 예쁘잖아요",
          "speaker_id": "2",
          "time": "20210907 09:56"
        },
        {
          "id": "MDRW2100042827.1.18",
          "form": "그냥 오름에 올라서 앉아만 잇어도 좋을거같아요 ㅠㅠ",
          "original_form": "그냥 오름에 올라서 앉아만 잇어도 좋을거같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 09:56"
        },
        {
          "id": "MDRW2100042827.1.19",
          "form": "맞아요 저 후리스 하나 장만하려구요",
          "original_form": "맞아요 저 후리스 하나 장만하려구요",
          "speaker_id": "2",
          "time": "20210907 09:56"
        },
        {
          "id": "MDRW2100042827.1.20",
          "form": "저는 가디건을 ㅎㅎ",
          "original_form": "저는 가디건을 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 09:57"
        }
