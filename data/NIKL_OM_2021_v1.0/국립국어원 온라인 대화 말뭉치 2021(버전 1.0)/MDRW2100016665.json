      "utterance": [
        {
          "id": "MDRW2100016665.1.1",
          "form": "어떤 연예인 좋아하세요?",
          "original_form": "어떤 연예인 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 14:36"
        },
        {
          "id": "MDRW2100016665.1.2",
          "form": "저 다양하게 좋아해여ㅋㅋㅋㅋ",
          "original_form": "저 다양하게 좋아해여ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:37"
        },
        {
          "id": "MDRW2100016665.1.3",
          "form": "누구 좋아하세요?",
          "original_form": "누구 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 14:37"
        },
        {
          "id": "MDRW2100016665.1.4",
          "form": "요즘은 sf9 찬희요! 아시나요?",
          "original_form": "요즘은 sf9 찬희요! 아시나요?",
          "speaker_id": "1",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100016665.1.5",
          "form": "아아 알아요 ㅋㅋㅋㅋ 스카이캐슬나왓잖아요!",
          "original_form": "아아 알아요 ㅋㅋㅋㅋ 스카이캐슬나왓잖아요!",
          "speaker_id": "2",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100016665.1.6",
          "form": "sf9팬이세요??",
          "original_form": "sf9팬이세요??",
          "speaker_id": "2",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100016665.1.7",
          "form": "아뇨 찬희만 좀 호감있어요 ㅎㅎ 좋아하는 배우 있으세요?",
          "original_form": "아뇨 찬희만 좀 호감있어요 ㅎㅎ 좋아하는 배우 있으세요?",
          "speaker_id": "1",
          "time": "20210826 15:17"
        },
        {
          "id": "MDRW2100016665.1.8",
          "form": "김선호 좋아해요!",
          "original_form": "김선호 좋아해요!",
          "speaker_id": "2",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100016665.1.9",
          "form": "이번주에 새로운 드라마 하더라구요!",
          "original_form": "이번주에 새로운 드라마 하더라구요!",
          "speaker_id": "2",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100016665.1.10",
          "form": "입덕작품이 뭔가요?",
          "original_form": "입덕작품이 뭔가요?",
          "speaker_id": "1",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100016665.1.11",
          "form": "그 이유영 나오는 단편드라마? 다들 아마 거기서 많이 입덕햇을거에요 ㅋㅋㅋㅋ",
          "original_form": "그 이유영 나오는 단편드라마? 다들 아마 거기서 많이 입덕햇을거에요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:50"
        },
        {
          "id": "MDRW2100016665.1.12",
          "form": "맞아요 인터넷서 핫하더라고요 ㅎㅎ",
          "original_form": "맞아요 인터넷서 핫하더라고요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016665.1.13",
          "form": "넷플릭스 구독 하세여?",
          "original_form": "넷플릭스 구독 하세여?",
          "speaker_id": "1",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016665.1.14",
          "form": "네네 구독중이에요 ㅋㅋㅋㅋ근데 자주보지는 않아요",
          "original_form": "네네 구독중이에요 ㅋㅋㅋㅋ근데 자주보지는 않아요",
          "speaker_id": "2",
          "time": "20210826 15:52"
        },
        {
          "id": "MDRW2100016665.1.15",
          "form": "넷플 많이보세요??",
          "original_form": "넷플 많이보세요??",
          "speaker_id": "2",
          "time": "20210826 15:52"
        },
        {
          "id": "MDRW2100016665.1.16",
          "form": "가끔봐요 ㅎㅎ 돌싱글즈 보고있어요!",
          "original_form": "가끔봐요 ㅎㅎ 돌싱글즈 보고있어요!",
          "speaker_id": "1",
          "time": "20210826 15:53"
        },
        {
          "id": "MDRW2100016665.1.17",
          "form": "아아 그거요즘하더라구요ㅋㅋㅋ",
          "original_form": "아아 그거요즘하더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:56"
        },
        {
          "id": "MDRW2100016665.1.18",
          "form": "티빙은 보세요?",
          "original_form": "티빙은 보세요?",
          "speaker_id": "2",
          "time": "20210826 15:56"
        },
        {
          "id": "MDRW2100016665.1.19",
          "form": "아뇨! 티빙 보세요?",
          "original_form": "아뇨! 티빙 보세요?",
          "speaker_id": "1",
          "time": "20210826 15:56"
        },
        {
          "id": "MDRW2100016665.1.20",
          "form": "네네 ㅋㅋㅋㅋ그 네이버플러스멤버십 이용해서요!",
          "original_form": "네네 ㅋㅋㅋㅋ그 네이버플러스멤버십 이용해서요!",
          "speaker_id": "2",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016665.1.21",
          "form": "환승연애 재밋더라구요",
          "original_form": "환승연애 재밋더라구요",
          "speaker_id": "2",
          "time": "20210826 15:58"
        }
