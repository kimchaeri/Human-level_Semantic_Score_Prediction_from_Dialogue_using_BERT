      "utterance": [
        {
          "id": "MDRW2100033838.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 11:41"
        },
        {
          "id": "MDRW2100033838.1.2",
          "form": "안녕하세요 점심 뭐 드실건가요?",
          "original_form": "안녕하세요 점심 뭐 드실건가요?",
          "speaker_id": "2",
          "time": "20210825 12:16"
        },
        {
          "id": "MDRW2100033838.1.3",
          "form": "저 이미먹었답니다!",
          "original_form": "저 이미먹었답니다!",
          "speaker_id": "1",
          "time": "20210825 12:19"
        },
        {
          "id": "MDRW2100033838.1.4",
          "form": "일찍 드셨네요? 뭐 드셨나요??",
          "original_form": "일찍 드셨네요? 뭐 드셨나요??",
          "speaker_id": "2",
          "time": "20210825 12:24"
        },
        {
          "id": "MDRW2100033838.1.5",
          "form": "저 치킨이요ㅋㅋㅋ",
          "original_form": "저 치킨이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:30"
        },
        {
          "id": "MDRW2100033838.1.6",
          "form": "점심 치킨이라니 좋겠어요",
          "original_form": "점심 치킨이라니 좋겠어요",
          "speaker_id": "2",
          "time": "20210825 12:45"
        },
        {
          "id": "MDRW2100033838.1.7",
          "form": "ㅋㅋㅋ 양념치킨이었답니다",
          "original_form": "ㅋㅋㅋ 양념치킨이었답니다",
          "speaker_id": "1",
          "time": "20210825 12:55"
        },
        {
          "id": "MDRW2100033838.1.8",
          "form": "양념치킨 좋죠ㅋㅋ 갑자기 먹고싶네요",
          "original_form": "양념치킨 좋죠ㅋㅋ 갑자기 먹고싶네요",
          "speaker_id": "2",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100033838.1.9",
          "form": "뿌링클vs스노윙?!",
          "original_form": "뿌링클vs스노윙?!",
          "speaker_id": "1",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100033838.1.10",
          "form": "저는 뿌링클이요",
          "original_form": "저는 뿌링클이요",
          "speaker_id": "2",
          "time": "20210825 13:49"
        },
        {
          "id": "MDRW2100033838.1.11",
          "form": "피자좋아하세요?",
          "original_form": "피자좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 13:57"
        },
        {
          "id": "MDRW2100033838.1.12",
          "form": "피자도 좋아하죠",
          "original_form": "피자도 좋아하죠",
          "speaker_id": "2",
          "time": "20210825 14:54"
        },
        {
          "id": "MDRW2100033838.1.13",
          "form": "좋아하는 피자 브랜드 있으세요?",
          "original_form": "좋아하는 피자 브랜드 있으세요?",
          "speaker_id": "2",
          "time": "20210825 14:54"
        },
        {
          "id": "MDRW2100033838.1.14",
          "form": "전 피자헛이요ㅋㅋㅋ",
          "original_form": "전 피자헛이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100033838.1.15",
          "form": "님ㅈ은요?",
          "original_form": "님ㅈ은요?",
          "speaker_id": "1",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100033838.1.16",
          "form": "저는 피자스쿨이요",
          "original_form": "저는 피자스쿨이요",
          "speaker_id": "2",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100033838.1.17",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210825 15:20"
        },
        {
          "id": "MDRW2100033838.1.18",
          "form": "고구마인가요?",
          "original_form": "고구마인가요?",
          "speaker_id": "1",
          "time": "20210825 15:20"
        },
        {
          "id": "MDRW2100033838.1.19",
          "form": "고구마든 치즈든 다 좋아해요",
          "original_form": "고구마든 치즈든 다 좋아해요",
          "speaker_id": "2",
          "time": "20210825 16:32"
        },
        {
          "id": "MDRW2100033838.1.20",
          "form": "ㅋㅋㅋ피자스쿨",
          "original_form": "ㅋㅋㅋ피자스쿨",
          "speaker_id": "1",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100033838.1.21",
          "form": "동네에없어져서 아쉬워요",
          "original_form": "동네에없어져서 아쉬워요",
