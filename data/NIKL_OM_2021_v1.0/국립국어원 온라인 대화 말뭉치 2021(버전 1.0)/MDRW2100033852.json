      "utterance": [
        {
          "id": "MDRW2100033852.1.1",
          "form": "태리로제는 드셔보셨어요?",
          "original_form": "태리로제는 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210825 12:57"
        },
        {
          "id": "MDRW2100033852.1.2",
          "form": "당연하죠!ㅋㅋㅋㅋ",
          "original_form": "당연하죠!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:03"
        },
        {
          "id": "MDRW2100033852.1.3",
          "form": "태리로제는 맛있었어요?",
          "original_form": "태리로제는 맛있었어요?",
          "speaker_id": "2",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100033852.1.4",
          "form": "ㅋㅋㅋ 사실",
          "original_form": "ㅋㅋㅋ 사실",
          "speaker_id": "1",
          "time": "20210825 13:26"
        },
        {
          "id": "MDRW2100033852.1.5",
          "form": "떡군이네가 젤존맛이예요",
          "original_form": "떡군이네가 젤존맛이예요",
          "speaker_id": "1",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100033852.1.6",
          "form": "아 떡군이네 점찍어두고 있었는데",
          "original_form": "아 떡군이네 점찍어두고 있었는데",
          "speaker_id": "2",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100033852.1.7",
          "form": "먹어봐야겠네요",
          "original_form": "먹어봐야겠네요",
          "speaker_id": "2",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100033852.1.8",
          "form": "스파게티면",
          "original_form": "스파게티면",
          "speaker_id": "1",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100033852.1.9",
          "form": "무조건추가입니다",
          "original_form": "무조건추가입니다",
          "speaker_id": "1",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100033852.1.10",
          "form": "이건 무조건이예요!",
          "original_form": "이건 무조건이예요!",
          "speaker_id": "1",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100033852.1.11",
          "form": "오 참고할게요ㅋㅋㅋㅋ",
          "original_form": "오 참고할게요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:36"
        },
        {
          "id": "MDRW2100033852.1.12",
          "form": "달토끼 투움바떡볶이 드셔보셨어요?",
          "original_form": "달토끼 투움바떡볶이 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210825 13:36"
        },
        {
          "id": "MDRW2100033852.1.13",
          "form": "오우 먹어봤죠",
          "original_form": "오우 먹어봤죠",
          "speaker_id": "1",
          "time": "20210825 13:46"
        },
        {
          "id": "MDRW2100033852.1.14",
          "form": "먹어봤나요?",
          "original_form": "먹어봤나요?",
          "speaker_id": "1",
          "time": "20210825 13:46"
        },
        {
          "id": "MDRW2100033852.1.15",
          "form": "존맛",
          "original_form": "존맛",
          "speaker_id": "1",
          "time": "20210825 13:46"
        },
        {
          "id": "MDRW2100033852.1.16",
          "form": "네 먹어봤어요 완전 맛있죠",
          "original_form": "네 먹어봤어요 완전 맛있죠",
          "speaker_id": "2",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100033852.1.17",
          "form": "전 스파게티 들어간 떡볶이는 처음 먹어봤는데 잘 어울리더라고요",
          "original_form": "전 스파게티 들어간 떡볶이는 처음 먹어봤는데 잘 어울리더라고요",
          "speaker_id": "2",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100033852.1.18",
          "form": "그니까요ㅋㅋㅋㅋ",
          "original_form": "그니까요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100033852.1.19",
          "form": "그래서 제가 떡군이네를 먹는겁니다",
          "original_form": "그래서 제가 떡군이네를 먹는겁니다",
          "speaker_id": "1",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100033852.1.20",
          "form": "배떡엔 스파게티면추가가 없거든요",
          "original_form": "배떡엔 스파게티면추가가 없거든요",
          "speaker_id": "1",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100033852.1.21",
          "form": "아 그렇군요 중국당면은 좋아하세요?",
          "original_form": "아 그렇군요 중국당면은 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100033852.1.22",
          "form": "좋아하죠ㅋㅋㅋ",
          "original_form": "좋아하죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:41"
        },
        {
          "id": "MDRW2100033852.1.23",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210825 14:41"
        },
        {
          "id": "MDRW2100033852.1.24",
          "form": "저도요 중국당면 토핑이 제일 맛있어요",
          "original_form": "저도요 중국당면 토핑이 제일 맛있어요",
          "speaker_id": "2",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100033852.1.25",
          "form": "ㅋㅋㅋ 저도요",
          "original_form": "ㅋㅋㅋ 저도요",
          "speaker_id": "1",
          "time": "20210825 15:17"
        }
