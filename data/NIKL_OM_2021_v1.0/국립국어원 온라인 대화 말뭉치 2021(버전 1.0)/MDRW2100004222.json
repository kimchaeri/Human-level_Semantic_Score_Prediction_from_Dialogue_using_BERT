      "utterance": [
        {
          "id": "MDRW2100004222.1.1",
          "form": "동물 키우세요?",
          "original_form": "동물 키우세요?",
          "speaker_id": "2",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004222.1.2",
          "form": "아니요..ㅠㅠ",
          "original_form": "아니요..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004222.1.3",
          "form": "저희집은 동물 별로 안좋아해서",
          "original_form": "저희집은 동물 별로 안좋아해서",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004222.1.4",
          "form": "부모님이 특히 안좋아하셔서 못키우네요",
          "original_form": "부모님이 특히 안좋아하셔서 못키우네요",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004222.1.5",
          "form": "키우시는 반려동물 있나요?",
          "original_form": "키우시는 반려동물 있나요?",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004222.1.6",
          "form": "아니요 없어요",
          "original_form": "아니요 없어요",
          "speaker_id": "2",
          "time": "20210821 17:47"
        },
        {
          "id": "MDRW2100004222.1.7",
          "form": "좋아하는 동물은 있습니더 ㅎ",
          "original_form": "좋아하는 동물은 있습니더 ㅎ",
          "speaker_id": "2",
          "time": "20210821 17:48"
        },
        {
          "id": "MDRW2100004222.1.8",
          "form": "어떤 동물 좋아하세요?",
          "original_form": "어떤 동물 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 17:55"
        },
        {
          "id": "MDRW2100004222.1.9",
          "form": "전 고양이 좋아합니다",
          "original_form": "전 고양이 좋아합니다",
          "speaker_id": "2",
          "time": "20210821 17:58"
        },
        {
          "id": "MDRW2100004222.1.10",
          "form": "좋아하는 동물 있으세요?",
          "original_form": "좋아하는 동물 있으세요?",
          "speaker_id": "2",
          "time": "20210821 17:58"
        },
        {
          "id": "MDRW2100004222.1.11",
          "form": "저도 고양이 좋아해요!",
          "original_form": "저도 고양이 좋아해요!",
          "speaker_id": "1",
          "time": "20210821 18:09"
        },
        {
          "id": "MDRW2100004222.1.12",
          "form": "고양이 너무 귀엽죠ㅠㅠ",
          "original_form": "고양이 너무 귀엽죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 18:10"
        },
        {
          "id": "MDRW2100004222.1.13",
          "form": "네 ㅠㅠ 제 최애 동물입니다",
          "original_form": "네 ㅠㅠ 제 최애 동물입니다",
          "speaker_id": "2",
          "time": "20210821 18:10"
        },
        {
          "id": "MDRW2100004222.1.14",
          "form": "저는 자주는 아니지만",
          "original_form": "저는 자주는 아니지만",
          "speaker_id": "1",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004222.1.15",
          "form": "가끔 집근처 고양이들한테 밥주고 그러네요...",
          "original_form": "가끔 집근처 고양이들한테 밥주고 그러네요...",
          "speaker_id": "1",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004222.1.16",
          "form": "그래두 동네마다 캣맘있으셔서 다들 돌보시는 것 같더라구요",
          "original_form": "그래두 동네마다 캣맘있으셔서 다들 돌보시는 것 같더라구요",
          "speaker_id": "1",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004222.1.17",
          "form": "전 물만 줘봤어요!",
          "original_form": "전 물만 줘봤어요!",
          "speaker_id": "2",
          "time": "20210821 18:13"
        },
        {
          "id": "MDRW2100004222.1.18",
          "form": "저희 동네도 신경 써주시는 분이 계시더라구요",
          "original_form": "저희 동네도 신경 써주시는 분이 계시더라구요",
          "speaker_id": "2",
          "time": "20210821 18:13"
        },
        {
          "id": "MDRW2100004222.1.19",
          "form": "그쵸 그쵸",
          "original_form": "그쵸 그쵸",
          "speaker_id": "1",
          "time": "20210821 18:13"
        },
        {
          "id": "MDRW2100004222.1.20",
          "form": "요새 길고양이들이 되게 많이 늘었더라구요",
          "original_form": "요새 길고양이들이 되게 많이 늘었더라구요",
          "speaker_id": "2",
          "time": "20210821 18:14"
        },
        {
          "id": "MDRW2100004222.1.21",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 18:14"
        },
        {
          "id": "MDRW2100004222.1.22",
          "form": "고양이 보면 키우고싶은데",
          "original_form": "고양이 보면 키우고싶은데",
          "speaker_id": "1",
          "time": "20210821 18:14"
        },
        {
          "id": "MDRW2100004222.1.23",
          "form": "자취하더라도 혼자 감당 할수있을지 모르겠고",
          "original_form": "자취하더라도 혼자 감당 할수있을지 모르겠고",
          "speaker_id": "1",
          "time": "20210821 18:14"
        },
        {
          "id": "MDRW2100004222.1.24",
          "form": "좀 망설여지더라구요",
          "original_form": "좀 망설여지더라구요",
          "speaker_id": "1",
          "time": "20210821 18:14"
        },
        {
          "id": "MDRW2100004222.1.25",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 18:16"
        },
        {
          "id": "MDRW2100004222.1.26",
          "form": "저도 그 책임감 때문에",
          "original_form": "저도 그 책임감 때문에",
          "speaker_id": "2",
          "time": "20210821 18:16"
        }
      ]
    }
