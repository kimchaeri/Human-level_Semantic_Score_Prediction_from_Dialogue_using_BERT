      "utterance": [
        {
          "id": "MDRW2100038245.1.1",
          "form": "국수하나",
          "original_form": "국수하나",
          "speaker_id": "2",
          "time": "20210902 01:50"
        },
        {
          "id": "MDRW2100038245.1.2",
          "form": "그런가봐 긇는 물에 국수를 삶네",
          "original_form": "그런가봐 긇는 물에 국수를 삶네",
          "speaker_id": "1",
          "time": "20210902 01:51"
        },
        {
          "id": "MDRW2100038245.1.3",
          "form": "4명인데 2인분을",
          "original_form": "4명인데 2인분을",
          "speaker_id": "2",
          "time": "20210902 01:51"
        },
        {
          "id": "MDRW2100038245.1.4",
          "form": "연예인이니 많이 안먹을듯",
          "original_form": "연예인이니 많이 안먹을듯",
          "speaker_id": "1",
          "time": "20210902 01:52"
        },
        {
          "id": "MDRW2100038245.1.5",
          "form": "그래도 너무 적다",
          "original_form": "그래도 너무 적다",
          "speaker_id": "2",
          "time": "20210902 01:52"
        },
        {
          "id": "MDRW2100038245.1.6",
          "form": "그렇긴 하지만 국수가 없으니 조금씩 먹어야지",
          "original_form": "그렇긴 하지만 국수가 없으니 조금씩 먹어야지",
          "speaker_id": "1",
          "time": "20210902 01:52"
        },
        {
          "id": "MDRW2100038245.1.7",
          "form": "메밀국수네",
          "original_form": "메밀국수네",
          "speaker_id": "2",
          "time": "20210902 01:52"
        },
        {
          "id": "MDRW2100038245.1.8",
          "form": "2인분을 그릇4개에 담으니 엄청 작음",
          "original_form": "2인분을 그릇4개에 담으니 엄청 작음",
          "speaker_id": "1",
          "time": "20210902 01:53"
        },
        {
          "id": "MDRW2100038245.1.9",
          "form": "음식점이니 파는걸로 곁들여서",
          "original_form": "음식점이니 파는걸로 곁들여서",
          "speaker_id": "2",
          "time": "20210902 01:53"
        },
        {
          "id": "MDRW2100038245.1.10",
          "form": "맞네 그러면 되겠네",
          "original_form": "맞네 그러면 되겠네",
          "speaker_id": "1",
          "time": "20210902 01:53"
        },
        {
          "id": "MDRW2100038245.1.11",
          "form": "불고기덮밥하네",
          "original_form": "불고기덮밥하네",
          "speaker_id": "2",
          "time": "20210902 01:53"
        },
        {
          "id": "MDRW2100038245.1.12",
          "form": "맛있겠네 다 외국나가면 그렇게 한식이 맛있더라",
          "original_form": "맛있겠네 다 외국나가면 그렇게 한식이 맛있더라",
          "speaker_id": "1",
          "time": "20210902 01:54"
        },
