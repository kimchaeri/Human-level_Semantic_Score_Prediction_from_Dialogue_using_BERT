        {
          "id": "MDRW2100000873.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210819 20:15"
        },
        {
          "id": "MDRW2100000873.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210819 20:15"
        },
        {
          "id": "MDRW2100000873.1.3",
          "form": "최근에.구매한.물건이.무엇이신가요",
          "original_form": "최근에.구매한.물건이.무엇이신가요",
          "speaker_id": "1",
          "time": "20210819 20:22"
        },
        {
          "id": "MDRW2100000873.1.4",
          "form": "당근마켓에서  가방을  구입했어요",
          "original_form": "당근마켓에서  가방을  구입했어요",
          "speaker_id": "2",
          "time": "20210819 20:26"
        },
        {
          "id": "MDRW2100000873.1.5",
          "form": "저렴하게 득템하셨겠네요",
          "original_form": "저렴하게 득템하셨겠네요",
          "speaker_id": "1",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000873.1.6",
          "form": "네~   중고가격  확인해보니   싸게  구입했더라고요",
          "original_form": "네~   중고가격  확인해보니   싸게  구입했더라고요",
          "speaker_id": "2",
          "time": "20210819 20:33"
        },
        {
          "id": "MDRW2100000873.1.7",
          "form": "저도 가끔 중고 구매하는댜",
          "original_form": "저도 가끔 중고 구매하는댜",
          "speaker_id": "1",
          "time": "20210819 20:33"
        },
        {
          "id": "MDRW2100000873.1.8",
          "form": "꼭 새거 안사도 괜찮더라구요",
          "original_form": "꼭 새거 안사도 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210819 20:33"
        },
        {
          "id": "MDRW2100000873.1.9",
          "form": "저도   그래서  중고거래  즐겨하고  있어요",
          "original_form": "저도   그래서  중고거래  즐겨하고  있어요",
          "speaker_id": "2",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000873.1.10",
          "form": "저도 중고거래 좋아해요",
          "original_form": "저도 중고거래 좋아해요",
          "speaker_id": "1",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000873.1.11",
          "form": "할인도 좋아해요",
          "original_form": "할인도 좋아해요",
          "speaker_id": "1",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000873.1.12",
          "form": "저도  쿠폰  이용하는거  좋아합니다~",
          "original_form": "저도  쿠폰  이용하는거  좋아합니다~",
          "speaker_id": "2",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000873.1.13",
          "form": "전 핫딜같은것도 좋아하구요",
          "original_form": "전 핫딜같은것도 좋아하구요",
          "speaker_id": "1",
          "time": "20210819 20:35"
        },
        {
          "id": "MDRW2100000873.1.14",
          "form": "뭔가 그렇게 구매하면 재밌어요",
          "original_form": "뭔가 그렇게 구매하면 재밌어요",
          "speaker_id": "1",
          "time": "20210819 20:36"
        },
        {
          "id": "MDRW2100000873.1.15",
          "form": "저랑  남편은   즐겨해요~",
          "original_form": "저랑  남편은   즐겨해요~",
          "speaker_id": "2",
          "time": "20210819 20:36"
        },
        {
          "id": "MDRW2100000873.1.16",
          "form": "어디에 정보가 많이올라오나요",
          "original_form": "어디에 정보가 많이올라오나요",
          "speaker_id": "1",
          "time": "20210819 20:36"
        },
        {
          "id": "MDRW2100000873.1.17",
          "form": "디젤매니아라는  곳이  있는데   할인혜택을  많이  올려주더라고요",
          "original_form": "디젤매니아라는  곳이  있는데   할인혜택을  많이  올려주더라고요",
          "speaker_id": "2",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000873.1.18",
          "form": "아 그런가요?",
          "original_form": "아 그런가요?",
          "speaker_id": "1",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000873.1.19",
          "form": "네이버카페  이름이예요~",
          "original_form": "네이버카페  이름이예요~",
          "speaker_id": "2",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000873.1.20",
          "form": "감사합니다 물건구매할때가 뭔가 젤 좋읃거같아요",
          "original_form": "감사합니다 물건구매할때가 뭔가 젤 좋읃거같아요",
          "speaker_id": "1",
          "time": "20210819 20:41"
