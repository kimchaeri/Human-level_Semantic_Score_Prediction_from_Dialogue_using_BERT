      "utterance": [
        {
          "id": "MDRW2100006206.1.1",
          "form": "그때 완전 언터쳐블이었어서",
          "original_form": "그때 완전 언터쳐블이었어서",
          "speaker_id": "2",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100006206.1.2",
          "form": "기대했는데",
          "original_form": "기대했는데",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100006206.1.3",
          "form": "그래도 큰 부상은 아닌 것 같아서 다행이에요",
          "original_form": "그래도 큰 부상은 아닌 것 같아서 다행이에요",
          "speaker_id": "1",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100006206.1.4",
          "form": "팔꿈치를 삔거라네요",
          "original_form": "팔꿈치를 삔거라네요",
          "speaker_id": "2",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100006206.1.5",
          "form": "류현진은 또 승수 올렸더라구요",
          "original_form": "류현진은 또 승수 올렸더라구요",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100006206.1.6",
          "form": "어깨부상 이후에도 잘 하는 거 보면 괴물은 괴물인가봐요",
          "original_form": "어깨부상 이후에도 잘 하는 거 보면 괴물은 괴물인가봐요",
          "speaker_id": "1",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100006206.1.7",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100006206.1.8",
          "form": "저 류현진 유니폼 샀어요",
          "original_form": "저 류현진 유니폼 샀어요",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100006206.1.9",
          "form": "국내복귀하면",
          "original_form": "국내복귀하면",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100006206.1.10",
          "form": "입고 응원가려구요",
          "original_form": "입고 응원가려구요",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100006206.1.11",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100006206.1.12",
          "form": "토론토 유니폼이요?",
          "original_form": "토론토 유니폼이요?",
          "speaker_id": "1",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100006206.1.13",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100006206.1.14",
          "form": "미국직구",
          "original_form": "미국직구",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100006206.1.15",
          "form": "미국가서 응원하고 싶었는데",
          "original_form": "미국가서 응원하고 싶었는데",
          "speaker_id": "2",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100006206.1.16",
          "form": "코로나땜에ㅜㅜ",
          "original_form": "코로나땜에ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100006206.1.17",
          "form": "메이저리그 구장 가보고 싶었는데 시기를 못 맞춰서 미국 갔을 때 경기 못 보고 왔었어요",
          "original_form": "메이저리그 구장 가보고 싶었는데 시기를 못 맞춰서 미국 갔을 때 경기 못 보고 왔었어요",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100006206.1.18",
          "form": "언젠가 한 번 가보고 싶네요 ㅎㅎ",
          "original_form": "언젠가 한 번 가보고 싶네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100006206.1.19",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100006206.1.20",
          "form": "미국 가셨구나",
          "original_form": "미국 가셨구나",
          "speaker_id": "2",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100006206.1.21",
          "form": "저도 가보고 싶어요",
          "original_form": "저도 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100006206.1.22",
          "form": "지나가면서 경기장 외관만 슬쩍 봤었어요ㅋㅋ",
          "original_form": "지나가면서 경기장 외관만 슬쩍 봤었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100006206.1.23",
          "form": "일본 갔을 때 고시엔 가서 한신 경기 본 적은 있어요",
          "original_form": "일본 갔을 때 고시엔 가서 한신 경기 본 적은 있어요",
          "speaker_id": "1",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100006206.1.24",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100006206.1.25",
          "form": "진짜 야구 좋아하시는구나",
          "original_form": "진짜 야구 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100006206.1.26",
          "form": "전 일본가서 야구 볼 생각은 못했는데",
          "original_form": "전 일본가서 야구 볼 생각은 못했는데",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100006206.1.27",
          "form": "상대팀 투수 잘한다 생각했었는데 나와서 찾아보니 오타니였어요ㅋㅋ",
          "original_form": "상대팀 투수 잘한다 생각했었는데 나와서 찾아보니 오타니였어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100006206.1.28",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100006206.1.29",
          "form": "오타니 지금 메이저리그 폭격중이자나요",
          "original_form": "오타니 지금 메이저리그 폭격중이자나요",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100006206.1.30",
          "form": "오타니를 보시다니",
          "original_form": "오타니를 보시다니",
          "speaker_id": "2",
          "time": "20210907 21:53"
        }
      ]
