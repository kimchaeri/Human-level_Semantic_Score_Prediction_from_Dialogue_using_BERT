      "utterance": [
        {
          "id": "MDRW2100043325.1.1",
          "form": "이상형 있오??",
          "original_form": "이상형 있오??",
          "speaker_id": "1",
          "time": "20210907 17:41"
        },
        {
          "id": "MDRW2100043325.1.2",
          "form": "능력있는착한남자",
          "original_form": "능력있는착한남자",
          "speaker_id": "2",
          "time": "20210907 17:43"
        },
        {
          "id": "MDRW2100043325.1.3",
          "form": "없지... ㅋㅋ",
          "original_form": "없지... ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:43"
        },
        {
          "id": "MDRW2100043325.1.4",
          "form": "우앙",
          "original_form": "우앙",
          "speaker_id": "1",
          "time": "20210907 17:43"
        },
        {
          "id": "MDRW2100043325.1.5",
          "form": "찾기힘들어 그치ㅠㅠ",
          "original_form": "찾기힘들어 그치ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 17:43"
        },
        {
          "id": "MDRW2100043325.1.6",
          "form": "연애는 커녕 생존문제ㅋㅋ",
          "original_form": "연애는 커녕 생존문제ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:43"
        },
        {
          "id": "MDRW2100043325.1.7",
          "form": "일본도 사건 봤어?",
          "original_form": "일본도 사건 봤어?",
          "speaker_id": "2",
          "time": "20210907 17:43"
        },
        {
          "id": "MDRW2100043325.1.8",
          "form": "아니 그게 뭐여???",
          "original_form": "아니 그게 뭐여???",
          "speaker_id": "1",
          "time": "20210907 17:43"
        },
        {
          "id": "MDRW2100043325.1.9",
          "form": "이혼소송중 아내 일본도로 찔러죽임ㅜ 장인이 말리는데도",
          "original_form": "이혼소송중 아내 일본도로 찔러죽임ㅜ 장인이 말리는데도",
          "speaker_id": "2",
          "time": "20210907 17:45"
        },
        {
          "id": "MDRW2100043325.1.10",
          "form": "헝 뭐야 미쳤나ㅠㅠㅠㅠ",
          "original_form": "헝 뭐야 미쳤나ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 17:49"
        },
        {
          "id": "MDRW2100043325.1.11",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "1",
          "time": "20210907 17:49"
        },
        {
          "id": "MDRW2100043325.1.12",
          "form": "남자 잘 만나여해 친구야ㅠㅠ",
          "original_form": "남자 잘 만나여해 친구야ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 17:50"
        },
        {
          "id": "MDRW2100043325.1.13",
          "form": "남자 잘못 만남 저세상 가... ㅜㅋㅋ안만나면 조금 잔소리만 듣지만",
          "original_form": "남자 잘못 만남 저세상 가... ㅜㅋㅋ안만나면 조금 잔소리만 듣지만",
          "speaker_id": "2",
          "time": "20210907 18:33"
        },
        {
          "id": "MDRW2100043325.1.14",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100043325.1.15",
          "form": "조심해야해 남자는 항상",
          "original_form": "조심해야해 남자는 항상",
          "speaker_id": "1",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100043325.1.16",
          "form": "아오 진짜 겉만 봐서는 모르니까ㅜ",
          "original_form": "아오 진짜 겉만 봐서는 모르니까ㅜ",
          "speaker_id": "2",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100043325.1.17",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100043325.1.18",
          "form": "잘생긴거 그런ㄱ 생각하면 필요넚어ㅠㅠ",
          "original_form": "잘생긴거 그런ㄱ 생각하면 필요넚어ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100043325.1.19",
          "form": "비혼친구들 많이많이 생겨서 함께 늙었으면ㅋㅋ",
          "original_form": "비혼친구들 많이많이 생겨서 함께 늙었으면ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100043325.1.20",
          "form": "앜ㅋㅋㅋ맞아 난 이미 많아",
          "original_form": "앜ㅋㅋㅋ맞아 난 이미 많아",
          "speaker_id": "1",
