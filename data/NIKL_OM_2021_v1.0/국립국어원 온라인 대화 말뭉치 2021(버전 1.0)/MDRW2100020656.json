      "utterance": [
        {
          "id": "MDRW2100020656.1.1",
          "form": "포카칩이 얇다락해서....",
          "original_form": "포카칩이 얇다락해서....",
          "speaker_id": "2",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020656.1.2",
          "form": "얇은것만",
          "original_form": "얇은것만",
          "speaker_id": "1",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020656.1.3",
          "form": "아셨구나",
          "original_form": "아셨구나",
          "speaker_id": "1",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020656.1.4",
          "form": "초록은 어니언입니다 (단호)",
          "original_form": "초록은 어니언입니다 (단호)",
          "speaker_id": "1",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020656.1.5",
          "form": "담에 먹을 땐 향을 한번 음미해보죠...",
          "original_form": "담에 먹을 땐 향을 한번 음미해보죠...",
          "speaker_id": "2",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020656.1.6",
          "form": "그럼 혹시",
          "original_form": "그럼 혹시",
          "speaker_id": "1",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020656.1.7",
          "form": "꼬깔콘도",
          "original_form": "꼬깔콘도",
          "speaker_id": "1",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020656.1.8",
          "form": "비슷하게 느끼시나요",
          "original_form": "비슷하게 느끼시나요",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020656.1.9",
          "form": "꼬깔콘은...!",
          "original_form": "꼬깔콘은...!",
          "speaker_id": "2",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020656.1.10",
          "form": "꼬깔콘은 조금 달라요",
          "original_form": "꼬깔콘은 조금 달라요",
          "speaker_id": "2",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020656.1.11",
          "form": "설마..!",
          "original_form": "설마..!",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020656.1.12",
          "form": "다행이다",
          "original_form": "다행이다",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020656.1.13",
          "form": "어떤 맛 좋아하세요",
          "original_form": "어떤 맛 좋아하세요",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020656.1.14",
          "form": "꼬깔콘은 오리지널이죠 역시",
          "original_form": "꼬깔콘은 오리지널이죠 역시",
          "speaker_id": "2",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020656.1.15",
          "form": "아니 근데",
          "original_form": "아니 근데",
          "speaker_id": "2",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020656.1.16",
          "form": "꼭오리지널이 아니어도",
          "original_form": "꼭오리지널이 아니어도",
          "speaker_id": "2",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020656.1.17",
          "form": "빨강색 그쵸",
          "original_form": "빨강색 그쵸",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020656.1.18",
          "form": "맛은 있어요",
          "original_form": "맛은 있어요",
          "speaker_id": "2",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020656.1.19",
          "form": "사실 그래요 다 맛있어요",
          "original_form": "사실 그래요 다 맛있어요",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020656.1.20",
          "form": "아 그거 말고 뭔 깐풍맛인가",
          "original_form": "아 그거 말고 뭔 깐풍맛인가",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020656.1.21",
          "form": "그거 개별로..",
          "original_form": "그거 개별로..",
          "speaker_id": "1",
          "time": "20210828 10:52"
        },
        {
          "id": "MDRW2100020656.1.22",
          "form": "아니 근데 금방 네이버쳐봤는네",
          "original_form": "아니 근데 금방 네이버쳐봤는네",
          "speaker_id": "2",
          "time": "20210828 10:52"
        },
        {
          "id": "MDRW2100020656.1.23",
          "form": "맛이 왜이렇게 늘었죠...?",
          "original_form": "맛이 왜이렇게 늘었죠...?",
          "speaker_id": "2",
          "time": "20210828 10:52"
        },
        {
          "id": "MDRW2100020656.1.24",
          "form": "저는 군옥수수 파랍니다..",
          "original_form": "저는 군옥수수 파랍니다..",
          "speaker_id": "2",
          "time": "20210828 10:52"
        },
        {
          "id": "MDRW2100020656.1.25",
          "form": "겁나많죠",
          "original_form": "겁나많죠",
          "speaker_id": "1",
          "time": "20210828 10:52"
        },
        {
          "id": "MDRW2100020656.1.26",
          "form": "군옥수수도 맛있어요",
          "original_form": "군옥수수도 맛있어요",
          "speaker_id": "1",
          "time": "20210828 10:52"
        },
        {
          "id": "MDRW2100020656.1.27",
          "form": "아니 우리 슈퍼엔 많아봤자 두종류였는데",
          "original_form": "아니 우리 슈퍼엔 많아봤자 두종류였는데",
          "speaker_id": "2",
          "time": "20210828 10:52"
        },
        {
          "id": "MDRW2100020656.1.28",
          "form": "뭔가 좀 더 진한맛",
          "original_form": "뭔가 좀 더 진한맛",
          "speaker_id": "1",
          "time": "20210828 10:52"
        }
      ]
    }
  ]
