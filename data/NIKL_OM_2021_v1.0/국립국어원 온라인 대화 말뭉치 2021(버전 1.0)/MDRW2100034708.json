      "utterance": [
        {
          "id": "MDRW2100034708.1.1",
          "form": "요즘 게임 뭐하고 계신가요?",
          "original_form": "요즘 게임 뭐하고 계신가요?",
          "speaker_id": "1",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100034708.1.2",
          "form": "디즈니 매직킹덤 이라고 모바일 게임 하나해요!",
          "original_form": "디즈니 매직킹덤 이라고 모바일 게임 하나해요!",
          "speaker_id": "2",
          "time": "20210828 18:06"
        },
        {
          "id": "MDRW2100034708.1.3",
          "form": "앜ㅋㄱㄲ저희 이 얘기 했나봐요",
          "original_form": "앜ㅋㄱㄲ저희 이 얘기 했나봐요",
          "speaker_id": "1",
          "time": "20210828 18:10"
        },
        {
          "id": "MDRW2100034708.1.4",
          "form": "전 꿈의집하곸ㅋㄱㅋ",
          "original_form": "전 꿈의집하곸ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210828 18:10"
        },
        {
          "id": "MDRW2100034708.1.5",
          "form": "다른 게임은 안하세용?",
          "original_form": "다른 게임은 안하세용?",
          "speaker_id": "1",
          "time": "20210828 18:10"
        },
        {
          "id": "MDRW2100034708.1.6",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:11"
        },
        {
          "id": "MDRW2100034708.1.7",
          "form": "저 그 전에는 길 건너 친구들 해봤어요 ㅋㅋㅋㅋㅋ",
          "original_form": "저 그 전에는 길 건너 친구들 해봤어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:12"
        },
        {
          "id": "MDRW2100034708.1.8",
          "form": "기억 안나서 게임 목록 보고 왔습니다 ㅎㅎ",
          "original_form": "기억 안나서 게임 목록 보고 왔습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 18:12"
        },
        {
          "id": "MDRW2100034708.1.9",
          "form": "헐 길건너친구들",
          "original_form": "헐 길건너친구들",
          "speaker_id": "1",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100034708.1.10",
          "form": "뭔가 추억이네요",
          "original_form": "뭔가 추억이네요",
          "speaker_id": "1",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100034708.1.11",
          "form": "ㅋㅋㄱㅋㄱㅋ",
          "original_form": "ㅋㅋㄱㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100034708.1.12",
          "form": "귀여운데ㅋㄱㅋ",
          "original_form": "귀여운데ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100034708.1.13",
          "form": "생각해보니 이것도 캐릭터 모으는 게임이었네요",
          "original_form": "생각해보니 이것도 캐릭터 모으는 게임이었네요",
          "speaker_id": "2",
          "time": "20210828 18:17"
        },
        {
          "id": "MDRW2100034708.1.14",
          "form": "저 왜 이렇게 캐릭터에 집착하는 거 같죠 ㅋㅋㅋㅋ",
          "original_form": "저 왜 이렇게 캐릭터에 집착하는 거 같죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:17"
        },
        {
          "id": "MDRW2100034708.1.15",
          "form": "앜ㅋㅋㄱㅋㅋㅋㅋㅋㅋ캐릭터 콜렉터 같은 느낌....!!",
          "original_form": "앜ㅋㅋㄱㅋㅋㅋㅋㅋㅋ캐릭터 콜렉터 같은 느낌....!!",
          "speaker_id": "1",
          "time": "20210828 18:18"
        },
        {
          "id": "MDRW2100034708.1.16",
          "form": "근데 이런게임이 뭔가 뿌듯하고 그렇긴해욬ㅋㄱㅋ",
          "original_form": "근데 이런게임이 뭔가 뿌듯하고 그렇긴해욬ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210828 18:18"
        },
        {
          "id": "MDRW2100034708.1.17",
          "form": "눈에 결과가 쌓여서 보이는 걸 좋아하나봐요 ㅋㅋㅋㅋ",
          "original_form": "눈에 결과가 쌓여서 보이는 걸 좋아하나봐요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:19"
        },
        {
          "id": "MDRW2100034708.1.18",
          "form": "단계 클리어하는 게임 좋아하시나요?",
          "original_form": "단계 클리어하는 게임 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210828 18:19"
        },
        {
          "id": "MDRW2100034708.1.19",
          "form": "네네 저는 그런게 재밌더라구욥!!! 한단계한단계 클리어할때 쾌감이 좋아요ㅋㄱㅋ",
          "original_form": "네네 저는 그런게 재밌더라구욥!!! 한단계한단계 클리어할때 쾌감이 좋아요ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210828 18:20"
        },
        {
          "id": "MDRW2100034708.1.20",
          "form": "name2님은 그런 게임은 안하시나용ㅎㅎ?",
          "original_form": "&name2&님은 그런 게임은 안하시나용ㅎㅎ?",
          "speaker_id": "1",
          "time": "20210828 18:20"
        },
        {
          "id": "MDRW2100034708.1.21",
          "form": "저는 그런 게임을 시작하면 끝을 봐야해서ㅠ",
          "original_form": "저는 그런 게임을 시작하면 끝을 봐야해서ㅠ",
          "speaker_id": "2",
          "time": "20210828 18:25"
        },
        {
          "id": "MDRW2100034708.1.22",
          "form": "옛날에 애니팡 유행 할 때 일상생활이 안되더라구요 ㅎㅎ",
          "original_form": "옛날에 애니팡 유행 할 때 일상생활이 안되더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 18:26"
        },
        {
          "id": "MDRW2100034708.1.23",
          "form": "그래서 그 이후로 시작을 못해요ㅠ",
          "original_form": "그래서 그 이후로 시작을 못해요ㅠ",
          "speaker_id": "2",
          "time": "20210828 18:26"
        },
        {
          "id": "MDRW2100034708.1.24",
          "form": "앜ㅋㅋㅋㅋㅋ그래서 아예 안하시는거구나!",
          "original_form": "앜ㅋㅋㅋㅋㅋ그래서 아예 안하시는거구나!",
          "speaker_id": "1",
          "time": "20210828 18:27"
        },
        {
          "id": "MDRW2100034708.1.25",
          "form": "어릴땐 카트라이더나 크아같은 컴퓨터 게임 했었는데 name2님도 하셨어요?",
          "original_form": "어릴땐 카트라이더나 크아같은 컴퓨터 게임 했었는데 &name2&님도 하셨어요?",
          "speaker_id": "1",
          "time": "20210828 18:27"
        },
        {
          "id": "MDRW2100034708.1.26",
          "form": "크아 좋아했어요!",
          "original_form": "크아 좋아했어요!",
          "speaker_id": "2",
          "time": "20210828 18:28"
        },
        {
          "id": "MDRW2100034708.1.27",
          "form": "카트는 보고 있으면 어지러워서 많이 못해봤어요ㅠ",
          "original_form": "카트는 보고 있으면 어지러워서 많이 못해봤어요ㅠ",
          "speaker_id": "2",
          "time": "20210828 18:28"
        },
        {
          "id": "MDRW2100034708.1.28",
          "form": "핳 맞아요 카트는 좀 오래하면 멀미나요ㅠㅡㅠ",
          "original_form": "핳 맞아요 카트는 좀 오래하면 멀미나요ㅠㅡㅠ",
