      "utterance": [
        {
          "id": "MDRW2100026455.1.1",
          "form": "저녁에 밥 안먹고 수육만 몇점 먹었더니 입이 심심하네",
          "original_form": "저녁에 밥 안먹고 수육만 몇점 먹었더니 입이 심심하네",
          "speaker_id": "1",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026455.1.2",
          "form": "수육 맛있겠다...",
          "original_form": "수육 맛있겠다...",
          "speaker_id": "2",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026455.1.3",
          "form": "비계 넘 없어서 쩜 퍽퍽하드라ㅋㅋㅋ",
          "original_form": "비계 넘 없어서 쩜 퍽퍽하드라ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026455.1.4",
          "form": "넌 저녁 뭐 먹었니",
          "original_form": "넌 저녁 뭐 먹었니",
          "speaker_id": "1",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026455.1.5",
          "form": "마져 비계랑 같이 춱춱해야 ㅋㅋ",
          "original_form": "마져 비계랑 같이 춱춱해야 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026455.1.6",
          "form": "넌 저녁에 전어회 먹었니",
          "original_form": "넌 저녁에 전어회 먹었니",
          "speaker_id": "1",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026455.1.7",
          "form": "귀찬아서 안감...",
          "original_form": "귀찬아서 안감...",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026455.1.8",
          "form": "사장님 내일 뵐게여...",
          "original_form": "사장님 내일 뵐게여...",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100026455.1.9",
          "form": "ㅋㅋㅋ 그래 낼은 가라 비 안온다더라ㅋㅋ",
          "original_form": "ㅋㅋㅋ 그래 낼은 가라 비 안온다더라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026455.1.10",
          "form": "응 그럴라고 다섯시 땡 하면 ㅋㅋㅋ",
          "original_form": "응 그럴라고 다섯시 땡 하면 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026455.1.11",
          "form": "낸 저녁은 진짜로 전어회를 먹겠구먼ㅋㅋ",
          "original_form": "낸 저녁은 진짜로 전어회를 먹겠구먼ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026455.1.12",
          "form": "반찬통도 들고가겠습니다",
          "original_form": "반찬통도 들고가겠습니다",
          "speaker_id": "2",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026455.1.13",
          "form": "용기있는 녀자였구나ㅋㅋㅋ",
          "original_form": "용기있는 녀자였구나ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100026455.1.14",
          "form": "처음으로  ㅋㅋㅋㅋ",
          "original_form": "처음으로  ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100026455.1.15",
          "form": "마트는 쩜 그른데 시장은 용기 들고가면 좋긴하겠다ㅋ 번데기살때 용기 들고가볼까ㅋㅋ",
          "original_form": "마트는 쩜 그른데 시장은 용기 들고가면 좋긴하겠다ㅋ 번데기살때 용기 들고가볼까ㅋㅋ",
          "speaker_id": "1",
