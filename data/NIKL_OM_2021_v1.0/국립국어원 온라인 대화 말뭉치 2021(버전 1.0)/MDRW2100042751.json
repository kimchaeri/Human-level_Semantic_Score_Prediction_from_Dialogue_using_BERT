      "utterance": [
        {
          "id": "MDRW2100042751.1.1",
          "form": "해물믹스조합 꼭해야겟어요",
          "original_form": "해물믹스조합 꼭해야겟어요",
          "speaker_id": "2",
          "time": "20210907 08:22"
        },
        {
          "id": "MDRW2100042751.1.2",
          "form": "완전꿀팁",
          "original_form": "완전꿀팁",
          "speaker_id": "2",
          "time": "20210907 08:22"
        },
        {
          "id": "MDRW2100042751.1.3",
          "form": "네 해물이 적게들어서 넣음 맛나용ㅎ",
          "original_form": "네 해물이 적게들어서 넣음 맛나용ㅎ",
          "speaker_id": "1",
          "time": "20210907 08:23"
        },
        {
          "id": "MDRW2100042751.1.4",
          "form": "중고거래해보셧나용",
          "original_form": "중고거래해보셧나용",
          "speaker_id": "2",
          "time": "20210907 08:37"
        },
        {
          "id": "MDRW2100042751.1.5",
          "form": "저는 귀차니즘이심해서",
          "original_form": "저는 귀차니즘이심해서",
          "speaker_id": "2",
          "time": "20210907 08:37"
        },
        {
          "id": "MDRW2100042751.1.6",
          "form": "당근딱한번해보고 끝햇어요",
          "original_form": "당근딱한번해보고 끝햇어요",
          "speaker_id": "2",
          "time": "20210907 08:37"
        },
        {
          "id": "MDRW2100042751.1.7",
          "form": "저는 파는건 종종해요  동네분이라 거래도 쉽고",
          "original_form": "저는 파는건 종종해요  동네분이라 거래도 쉽고",
          "speaker_id": "1",
          "time": "20210907 08:38"
        },
        {
          "id": "MDRW2100042751.1.8",
          "form": "오호 그러시군용",
          "original_form": "오호 그러시군용",
          "speaker_id": "2",
          "time": "20210907 08:45"
        },
        {
          "id": "MDRW2100042751.1.9",
          "form": "빠른거래 ㅋㅋㅋ젤좋아요 ㅎ",
          "original_form": "빠른거래 ㅋㅋㅋ젤좋아요 ㅎ",
          "speaker_id": "1",
          "time": "20210907 08:46"
        },
        {
          "id": "MDRW2100042751.1.10",
          "form": "주변에보니까 육아템 당근에서 많이하더라구요",
          "original_form": "주변에보니까 육아템 당근에서 많이하더라구요",
          "speaker_id": "2",
          "time": "20210907 08:48"
        },
        {
          "id": "MDRW2100042751.1.11",
          "form": "애들꺼는 잠깐쓰고 안쓰게 되는건 팔면 좋죠",
          "original_form": "애들꺼는 잠깐쓰고 안쓰게 되는건 팔면 좋죠",
          "speaker_id": "1",
          "time": "20210907 08:49"
        },
        {
          "id": "MDRW2100042751.1.12",
          "form": "위시템잇으신가요",
          "original_form": "위시템잇으신가요",
          "speaker_id": "2",
          "time": "20210907 08:56"
        },
        {
          "id": "MDRW2100042751.1.13",
          "form": "요즘 나오는 플립폰 ㅠㅠ 갖고싶네유",
          "original_form": "요즘 나오는 플립폰 ㅠㅠ 갖고싶네유",
          "speaker_id": "1",
          "time": "20210907 08:57"
        },
        {
          "id": "MDRW2100042751.1.14",
          "form": "아ㅜㅜㅜㅜㅜ",
          "original_form": "아ㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 09:06"
        },
        {
          "id": "MDRW2100042751.1.15",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210907 09:06"
        },
        {
          "id": "MDRW2100042751.1.16",
          "form": "플립 ㅜㅜㅜㅜ",
          "original_form": "플립 ㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 09:06"
        },
        {
          "id": "MDRW2100042751.1.17",
          "form": "미치겟어요",
          "original_form": "미치겟어요",
          "speaker_id": "2",
          "time": "20210907 09:06"
        },
        {
          "id": "MDRW2100042751.1.18",
          "form": "노트10  말짱한데 엉엉",
          "original_form": "노트10  말짱한데 엉엉",
          "speaker_id": "2",
          "time": "20210907 09:06"
        },
        {
          "id": "MDRW2100042751.1.19",
          "form": "아 전9 ㅠㅠ\r\n멀쩡하진 않는데 ㅠ 그래도 2년 지났는데",
          "original_form": "아 전9 ㅠㅠ\r\n멀쩡하진 않는데 ㅠ 그래도 2년 지났는데",
