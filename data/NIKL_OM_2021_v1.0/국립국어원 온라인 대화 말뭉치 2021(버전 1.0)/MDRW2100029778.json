      "utterance": [
        {
          "id": "MDRW2100029778.1.1",
          "form": "영화말고 전시나 연극은 보세요?",
          "original_form": "영화말고 전시나 연극은 보세요?",
          "speaker_id": "1",
          "time": "20210903 13:21"
        },
        {
          "id": "MDRW2100029778.1.2",
          "form": "연극 한번 본적읶어요!",
          "original_form": "연극 한번 본적읶어요!",
          "speaker_id": "2",
          "time": "20210903 14:25"
        },
        {
          "id": "MDRW2100029778.1.3",
          "form": "진짜 완전 옛날 거의10년전?!",
          "original_form": "진짜 완전 옛날 거의10년전?!",
          "speaker_id": "2",
          "time": "20210903 14:26"
        },
        {
          "id": "MDRW2100029778.1.4",
          "form": "와, 뭐 보셨는지 기억나세요?",
          "original_form": "와, 뭐 보셨는지 기억나세요?",
          "speaker_id": "1",
          "time": "20210903 14:27"
        },
        {
          "id": "MDRW2100029778.1.5",
          "form": "무슨 웃긴거였는데...",
          "original_form": "무슨 웃긴거였는데...",
          "speaker_id": "2",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100029778.1.6",
          "form": "이름도 기억이 잘 안나요ㅋㅋㅋㅋㅋ",
          "original_form": "이름도 기억이 잘 안나요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100029778.1.7",
          "form": "코엑스몰에서 본거같은데",
          "original_form": "코엑스몰에서 본거같은데",
          "speaker_id": "2",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100029778.1.8",
          "form": "웃긴거라고 남들 다웃는데",
          "original_form": "웃긴거라고 남들 다웃는데",
          "speaker_id": "2",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100029778.1.9",
          "form": "진짜진짜 제가다민망할정도로",
          "original_form": "진짜진짜 제가다민망할정도로",
          "speaker_id": "2",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100029778.1.10",
          "form": "전 하나도 안 웃긴거있죠..",
          "original_form": "전 하나도 안 웃긴거있죠..",
          "speaker_id": "2",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100029778.1.11",
          "form": "아이고 저런...",
          "original_form": "아이고 저런...",
          "speaker_id": "1",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100029778.1.12",
          "form": "그럴때 있죠. 군중속의 고독.... ㅎㅎㅎ",
          "original_form": "그럴때 있죠. 군중속의 고독.... ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:10"
        },
        {
          "id": "MDRW2100029778.1.13",
          "form": "그래서 그후로는 안봤어요",
          "original_form": "그래서 그후로는 안봤어요",
          "speaker_id": "2",
          "time": "20210904 08:52"
        },
        {
          "id": "MDRW2100029778.1.14",
          "form": "지방살아서",
          "original_form": "지방살아서",
          "speaker_id": "2",
          "time": "20210904 08:52"
        },
        {
          "id": "MDRW2100029778.1.15",
          "form": "연극이나 전시회보려면 서울가야하고",
          "original_form": "연극이나 전시회보려면 서울가야하고",
          "speaker_id": "2",
          "time": "20210904 08:53"
        },
        {
          "id": "MDRW2100029778.1.16",
          "form": "귀차나서",
          "original_form": "귀차나서",
          "speaker_id": "2",
          "time": "20210904 08:53"
        },
        {
          "id": "MDRW2100029778.1.17",
          "form": "아하 그러시군요.",
          "original_form": "아하 그러시군요.",
          "speaker_id": "1",
          "time": "20210904 09:02"
        },
        {
          "id": "MDRW2100029778.1.18",
          "form": "전 대학와서 처음? 봤어요.",
          "original_form": "전 대학와서 처음? 봤어요.",
          "speaker_id": "1",
          "time": "20210904 09:02"
        },
        {
          "id": "MDRW2100029778.1.19",
          "form": "사실 처음은 아닐것같은데 기억에 남는건 그때네요.",
          "original_form": "사실 처음은 아닐것같은데 기억에 남는건 그때네요.",
          "speaker_id": "1",
          "time": "20210904 09:03"
        },
        {
          "id": "MDRW2100029778.1.20",
          "form": "그때 전 너무 좋아서 그 뒤로도 종종 내돈내산하곤 합니다 ㅎㅎ",
          "original_form": "그때 전 너무 좋아서 그 뒤로도 종종 내돈내산하곤 합니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 09:03"
        },
        {
          "id": "MDRW2100029778.1.21",
          "form": "어떤 작품이",
          "original_form": "어떤 작품이",
          "speaker_id": "2",
          "time": "20210904 11:54"
        },
        {
          "id": "MDRW2100029778.1.22",
          "form": "그렇게 마음에 드셨나요?!",
          "original_form": "그렇게 마음에 드셨나요?!",
          "speaker_id": "2",
          "time": "20210904 11:54"
        },
        {
          "id": "MDRW2100029778.1.23",
          "form": "제 기억에 남는 첫 뮤지컬은 모차르트였어요",
          "original_form": "제 기억에 남는 첫 뮤지컬은 모차르트였어요",
          "speaker_id": "1",
          "time": "20210904 12:05"
        },
        {
          "id": "MDRW2100029778.1.24",
          "form": "그때 신영숙 배우님의 황금별이 너무 쩔어서 뮤지컬에 대한 호감이 급격히 생겼죠",
          "original_form": "그때 신영숙 배우님의 황금별이 너무 쩔어서 뮤지컬에 대한 호감이 급격히 생겼죠",
          "speaker_id": "1",
          "time": "20210904 12:06"
        },
        {
          "id": "MDRW2100029778.1.25",
          "form": "오...유명한 작품이었네요",
          "original_form": "오...유명한 작품이었네요",
          "speaker_id": "2",
          "time": "20210904 12:30"
        },
        {
          "id": "MDRW2100029778.1.26",
          "form": "뮤지컬 자주 보세요?",
          "original_form": "뮤지컬 자주 보세요?",
          "speaker_id": "2",
          "time": "20210904 12:30"
        },
        {
          "id": "MDRW2100029778.1.27",
          "form": "음... 네. 달에 서너번...? ㅎㅎ",
          "original_form": "음... 네. 달에 서너번...? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 12:31"
        },
        {
          "id": "MDRW2100029778.1.28",
          "form": "오..... 가장 최근에 본 건 뭐예요?",
          "original_form": "오..... 가장 최근에 본 건 뭐예요?",
          "speaker_id": "2",
          "time": "20210904 12:31"
        },
        {
          "id": "MDRW2100029778.1.29",
          "form": "연극까지 포함이면 일리아드 라는 연극을 지난주에 봤구요",
          "original_form": "연극까지 포함이면 일리아드 라는 연극을 지난주에 봤구요",
          "speaker_id": "1",
          "time": "20210904 13:13"
        },
        {
          "id": "MDRW2100029778.1.30",
          "form": "뮤지컬은 스프링어웨이크닝을 지지난주에 봤어요",
          "original_form": "뮤지컬은 스프링어웨이크닝을 지지난주에 봤어요",
          "speaker_id": "1",
          "time": "20210904 13:14"
        },
        {
          "id": "MDRW2100029778.1.31",
          "form": "오..... 제가 뮤지컬쪽은",
          "original_form": "오..... 제가 뮤지컬쪽은",
          "speaker_id": "2",
          "time": "20210904 19:40"
        },
        {
          "id": "MDRW2100029778.1.32",
          "form": "아예몰라서 처음들어요ㅎㅎ",
          "original_form": "아예몰라서 처음들어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 19:40"
        }
      ]
