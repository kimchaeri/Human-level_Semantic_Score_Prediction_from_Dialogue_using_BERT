      "utterance": [
        {
          "id": "MDRW2100006132.1.1",
          "form": "위드코로나 진짜 하려나보던데",
          "original_form": "위드코로나 진짜 하려나보던데",
          "speaker_id": "1",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100006132.1.2",
          "form": "근데 계속 재난문자 와서 어디 방문한 사람 검사받으라고 하고 그런거 보면 무서워",
          "original_form": "근데 계속 재난문자 와서 어디 방문한 사람 검사받으라고 하고 그런거 보면 무서워",
          "speaker_id": "2",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100006132.1.3",
          "form": "방금도 의정부 음식점?방문한사람 검사받으래",
          "original_form": "방금도 의정부 음식점?방문한사람 검사받으래",
          "speaker_id": "1",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100006132.1.4",
          "form": "나도 받았음",
          "original_form": "나도 받았음",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100006132.1.5",
          "form": "우리 학교에 애들 코로나 걸려서 어제도 갑자기 하교시켯는데",
          "original_form": "우리 학교에 애들 코로나 걸려서 어제도 갑자기 하교시켯는데",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100006132.1.6",
          "form": "이거 위드코로나 하면 술 못먹어서 환장한 사람들 장난 아닐듯 한데",
          "original_form": "이거 위드코로나 하면 술 못먹어서 환장한 사람들 장난 아닐듯 한데",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100006132.1.7",
          "form": "갑자기 사람들 폭발해서",
          "original_form": "갑자기 사람들 폭발해서",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100006132.1.8",
          "form": "의료붕괴나는거 아냐?",
          "original_form": "의료붕괴나는거 아냐?",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100006132.1.9",
          "form": "내말이",
          "original_form": "내말이",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100006132.1.10",
          "form": "사실이될수도",
          "original_form": "사실이될수도",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100006132.1.11",
          "form": "좀 무섭긴 해",
          "original_form": "좀 무섭긴 해",
          "speaker_id": "2",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100006132.1.12",
          "form": "근데 언제까지 이렇게 살수도 없고",
          "original_form": "근데 언제까지 이렇게 살수도 없고",
          "speaker_id": "2",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100006132.1.13",
          "form": "자영업자들 다 죽어나가는데ㅜㅜ",
          "original_form": "자영업자들 다 죽어나가는데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100006132.1.14",
          "form": "나도 그건 그렇게 생각",
          "original_form": "나도 그건 그렇게 생각",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100006132.1.15",
          "form": "우리 극장 못간지도 벌써 2년 되간다",
          "original_form": "우리 극장 못간지도 벌써 2년 되간다",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100006132.1.16",
          "form": "극장에서 코로나 확진자 나왔단 소리도 못들어봤거늘...",
          "original_form": "극장에서 코로나 확진자 나왔단 소리도 못들어봤거늘...",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100006132.1.17",
          "form": "무서우니까 못갔는데",
          "original_form": "무서우니까 못갔는데",
          "speaker_id": "1",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100006132.1.18",
          "form": "이제 슬슬 가도 되지 않으까?",
          "original_form": "이제 슬슬 가도 되지 않으까?",
          "speaker_id": "1",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100006132.1.19",
          "form": "응 우리도 이제 마스크 잘하고",
          "original_form": "응 우리도 이제 마스크 잘하고",
          "speaker_id": "2",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100006132.1.20",
          "form": "못했던것들 해도 되지 않을까 싶어",
          "original_form": "못했던것들 해도 되지 않을까 싶어",
          "speaker_id": "2",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100006132.1.21",
          "form": "나 백신 2차도 맞았고 ㅋㅋㅋ",
          "original_form": "나 백신 2차도 맞았고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100006132.1.22",
          "form": "자긴 아니지만",
          "original_form": "자긴 아니지만",
          "speaker_id": "2",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100006132.1.23",
          "form": "나도 한달뒤 2차 ㅎㅎ",
          "original_form": "나도 한달뒤 2차 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100006132.1.24",
          "form": "다른나라들 보니까 어쩔수 없는거 같애",
          "original_form": "다른나라들 보니까 어쩔수 없는거 같애",
          "speaker_id": "2",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100006132.1.25",
          "form": "언제까지 이렇게 살순 없으니",
          "original_form": "언제까지 이렇게 살순 없으니",
          "speaker_id": "2",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100006132.1.26",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210907 20:51"
        }
