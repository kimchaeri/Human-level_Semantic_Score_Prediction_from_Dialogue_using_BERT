        {
          "id": "MDRW2100014601.1.2",
          "form": "어떤 고기요?",
          "original_form": "어떤 고기요?",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014601.1.3",
          "form": "소고기요!",
          "original_form": "소고기요!",
          "speaker_id": "2",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014601.1.4",
          "form": "소고기를 좋아하시나봐요",
          "original_form": "소고기를 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100014601.1.5",
          "form": "네 ㅎㅎ 고기 다 좋아해요",
