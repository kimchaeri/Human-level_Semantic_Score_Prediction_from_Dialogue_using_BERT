        {
          "id": "MDRW2100016242.1.1",
          "form": "안녕하세요? 반려동물 키우시나요?",
          "original_form": "안녕하세요? 반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210826 11:18"
        },
        {
          "id": "MDRW2100016242.1.2",
          "form": "아뇨  안키워요 키우시나요?",
          "original_form": "아뇨  안키워요 키우시나요?",
          "speaker_id": "2",
          "time": "20210826 11:19"
        },
        {
          "id": "MDRW2100016242.1.3",
          "form": "구피 키워요",
          "original_form": "구피 키워요",
          "speaker_id": "1",
          "time": "20210826 11:19"
        },
        {
          "id": "MDRW2100016242.1.4",
          "form": "오 귀엽겠네요",
          "original_form": "오 귀엽겠네요",
          "speaker_id": "2",
          "time": "20210826 11:20"
        },
        {
          "id": "MDRW2100016242.1.5",
          "form": "이름은 뭔가요?",
          "original_form": "이름은 뭔가요?",
          "speaker_id": "2",
          "time": "20210826 11:20"
        },
        {
          "id": "MDRW2100016242.1.6",
          "form": "많아서 그냥 키워요",
          "original_form": "많아서 그냥 키워요",
          "speaker_id": "1",
          "time": "20210826 11:21"
        },
        {
          "id": "MDRW2100016242.1.7",
          "form": "색깔은 뭔가요?",
          "original_form": "색깔은 뭔가요?",
          "speaker_id": "2",
          "time": "20210826 11:22"
        },
        {
          "id": "MDRW2100016242.1.8",
          "form": "흰색 검은색이에요",
          "original_form": "흰색 검은색이에요",
          "speaker_id": "1",
          "time": "20210826 11:22"
        },
        {
          "id": "MDRW2100016242.1.9",
          "form": "수조관리는 얼마나하시나요?",
          "original_form": "수조관리는 얼마나하시나요?",
          "speaker_id": "2",
          "time": "20210826 11:23"
        },
        {
          "id": "MDRW2100016242.1.10",
          "form": "한달에 한번 씻어요",
          "original_form": "한달에 한번 씻어요",
          "speaker_id": "1",
          "time": "20210826 11:23"
        },
        {
          "id": "MDRW2100016242.1.11",
          "form": "어렵나요?",
          "original_form": "어렵나요?",
          "speaker_id": "2",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100016242.1.12",
          "form": "귀찮아요ㅠ",
          "original_form": "귀찮아요ㅠ",
          "speaker_id": "1",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100016242.1.13",
          "form": "쉬운건 없네요",
