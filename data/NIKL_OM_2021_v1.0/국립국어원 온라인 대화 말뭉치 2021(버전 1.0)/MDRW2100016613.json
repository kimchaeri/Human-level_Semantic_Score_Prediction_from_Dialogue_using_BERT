      "utterance": [
        {
          "id": "MDRW2100016613.1.1",
          "form": "조혈모세포 신청하고 받은 데일밴드",
          "original_form": "조혈모세포 신청하고 받은 데일밴드",
          "speaker_id": "2",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100016613.1.2",
          "form": "음청고급이었어",
          "original_form": "음청고급이었어",
          "speaker_id": "2",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100016613.1.3",
          "form": "ㅋㅋㅋ 왼팔 나도",
          "original_form": "ㅋㅋㅋ 왼팔 나도",
          "speaker_id": "2",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100016613.1.4",
          "form": "물론 왼팔이 쬐끔이지만 혈관이 낫기도함",
          "original_form": "물론 왼팔이 쬐끔이지만 혈관이 낫기도함",
          "speaker_id": "1",
          "time": "20210826 14:04"
        },
        {
          "id": "MDRW2100016613.1.5",
          "form": "그래도 링거는 구멍이 작으니까",
          "original_form": "그래도 링거는 구멍이 작으니까",
          "speaker_id": "1",
          "time": "20210826 14:04"
        },
        {
          "id": "MDRW2100016613.1.6",
          "form": "어떻게든 오른쪽에 잘 꽂아줄거라 믿고 ㅋ",
          "original_form": "어떻게든 오른쪽에 잘 꽂아줄거라 믿고 ㅋ",
          "speaker_id": "1",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100016613.1.7",
          "form": "맘이편해 왼팔이 더",
          "original_form": "맘이편해 왼팔이 더",
          "speaker_id": "2",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100016613.1.8",
          "form": "오른팔 일해야대 ㅋ",
          "original_form": "오른팔 일해야대 ㅋ",
          "speaker_id": "2",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100016613.1.9",
          "form": "근데 고작 데일밴드주다니 ㅋㅋ",
          "original_form": "근데 고작 데일밴드주다니 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100016613.1.10",
          "form": "좀더 좋은거주지 ㅋㅋㅋ",
          "original_form": "좀더 좋은거주지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100016613.1.11",
          "form": "다른거도 줫는데 기억안나 달럭?",
          "original_form": "다른거도 줫는데 기억안나 달럭?",
          "speaker_id": "2",
          "time": "20210826 14:06"
        },
        {
          "id": "MDRW2100016613.1.12",
          "form": "력",
          "original_form": "력",
          "speaker_id": "2",
          "time": "20210826 14:07"
        },
        {
          "id": "MDRW2100016613.1.13",
          "form": "헌혈보상 솔직히 너무빈약항",
          "original_form": "헌혈보상 솔직히 너무빈약항",
          "speaker_id": "1",
          "time": "20210826 14:08"
        },
        {
          "id": "MDRW2100016613.1.14",
          "form": "외국에비해",
          "original_form": "외국에비해",
          "speaker_id": "1",
          "time": "20210826 14:08"
        },
        {
          "id": "MDRW2100016613.1.15",
          "form": "외국머줌?",
          "original_form": "외국머줌?",
          "speaker_id": "2",
          "time": "20210826 14:08"
        },
        {
          "id": "MDRW2100016613.1.16",
          "form": "이만원정도준다고본듷",
          "original_form": "이만원정도준다고본듷",
          "speaker_id": "1",
          "time": "20210826 14:12"
        },
        {
          "id": "MDRW2100016613.1.17",
          "form": "듯 미국",
          "original_form": "듯 미국",
          "speaker_id": "1",
          "time": "20210826 14:12"
        },
        {
          "id": "MDRW2100016613.1.18",
          "form": "돈은 위험하지않나",
          "original_form": "돈은 위험하지않나",
          "speaker_id": "2",
          "time": "20210826 14:12"
        },
        {
          "id": "MDRW2100016613.1.19",
          "form": "가난한사람들 피팔면 위험한데",
          "original_form": "가난한사람들 피팔면 위험한데",
          "speaker_id": "2",
          "time": "20210826 14:13"
        },
        {
          "id": "MDRW2100016613.1.20",
          "form": "울나란 티켓두장주니까 26000원인데 따지면ㅋㅋㅋㅋ",
          "original_form": "울나란 티켓두장주니까 26000원인데 따지면ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:23"
        },
        {
          "id": "MDRW2100016613.1.21",
          "form": "에이 걔들도 포인트로줌 문상같이",
          "original_form": "에이 걔들도 포인트로줌 문상같이",
          "speaker_id": "1",
          "time": "20210826 14:32"
        },
        {
          "id": "MDRW2100016613.1.22",
          "form": "솔직히 문상으로 계산해야져",
          "original_form": "솔직히 문상으로 계산해야져",
          "speaker_id": "1",
          "time": "20210826 14:33"
        },
        {
          "id": "MDRW2100016613.1.23",
          "form": "만원임 만원",
          "original_form": "만원임 만원",
          "speaker_id": "1",
          "time": "20210826 14:33"
        },
        {
          "id": "MDRW2100016613.1.24",
          "form": "그것도 이벤트때 ㅋㄱ",
          "original_form": "그것도 이벤트때 ㅋㄱ",
          "speaker_id": "1",
          "time": "20210826 14:33"
        },
        {
          "id": "MDRW2100016613.1.25",
          "form": "문상 오처넌두장인데",
          "original_form": "문상 오처넌두장인데",
          "speaker_id": "2",
          "time": "20210826 14:34"
        },
        {
          "id": "MDRW2100016613.1.26",
          "form": "마넌이고 영화티켓은 이만육처넌인데",
          "original_form": "마넌이고 영화티켓은 이만육처넌인데",
          "speaker_id": "2",
          "time": "20210826 14:34"
        },
        {
          "id": "MDRW2100016613.1.27",
          "form": "넘 차이남",
          "original_form": "넘 차이남",
          "speaker_id": "1",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100016613.1.28",
          "form": "솔직히 문상만원은줘야지",
          "original_form": "솔직히 문상만원은줘야지",
          "speaker_id": "1",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100016613.1.29",
          "form": "오 지금 두장이니까 마넌아님?",
          "original_form": "오 지금 두장이니까 마넌아님?",
          "speaker_id": "2",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100016613.1.30",
          "form": "아니 그냥도 ㅋㅋ 만원은줘야지",
          "original_form": "아니 그냥도 ㅋㅋ 만원은줘야지",
          "speaker_id": "1",
          "time": "20210826 18:30"
        },
        {
          "id": "MDRW2100016613.1.31",
          "form": "그래야 이만원 이만육천원",
          "original_form": "그래야 이만원 이만육천원",
          "speaker_id": "1",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100016613.1.32",
          "form": "아님만원 만삼처넌이되지 ㅋㅋ",
          "original_form": "아님만원 만삼처넌이되지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100016613.1.33",
          "form": "ㅋㅋ 조으네 피뺐는데 밥한끼는 무야제",
          "original_form": "ㅋㅋ 조으네 피뺐는데 밥한끼는 무야제",
          "speaker_id": "2",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100016613.1.34",
          "form": "맨날 피 모자라서 결국 외국서 사온다는데",
          "original_form": "맨날 피 모자라서 결국 외국서 사온다는데",
          "speaker_id": "1",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100016613.1.35",
          "form": "아 그정도구나",
          "original_form": "아 그정도구나",
          "speaker_id": "2",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100016613.1.36",
          "form": "그돈 차라리우리한테 투자해린",
          "original_form": "그돈 차라리우리한테 투자해린",
          "speaker_id": "1",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100016613.1.37",
          "form": "근데 내주위는 여자들인지 싫어함 헌혈",
          "original_form": "근데 내주위는 여자들인지 싫어함 헌혈",
