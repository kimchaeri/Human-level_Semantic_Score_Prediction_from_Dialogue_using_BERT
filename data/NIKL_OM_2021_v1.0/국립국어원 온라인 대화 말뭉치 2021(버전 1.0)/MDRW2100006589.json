        {
          "id": "MDRW2100006589.1.1",
          "form": "응원하는.프로팀 있나요",
          "original_form": "응원하는.프로팀 있나요",
          "speaker_id": "1",
          "time": "20210908 02:53"
        },
        {
          "id": "MDRW2100006589.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100006589.1.3",
          "form": "전 국대경기 봐요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 국대경기 봐요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:54"
        },
        {
          "id": "MDRW2100006589.1.4",
          "form": "오늘 보셨나요",
          "original_form": "오늘 보셨나요",
          "speaker_id": "1",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100006589.1.5",
          "form": "축구요?",
          "original_form": "축구요?",
          "speaker_id": "2",
          "time": "20210908 02:57"
        },
        {
          "id": "MDRW2100006589.1.6",
          "form": "네 국대경기 아니었나요?",
          "original_form": "네 국대경기 아니었나요?",
          "speaker_id": "1",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100006589.1.7",
          "form": "오늘은 못봤어욬ㅋㅋㅋㅋ",
          "original_form": "오늘은 못봤어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:59"
        },
        {
          "id": "MDRW2100006589.1.8",
          "form": "경기보셨나요?",
          "original_form": "경기보셨나요?",
          "speaker_id": "2",
          "time": "20210908 02:59"
        },
        {
          "id": "MDRW2100006589.1.9",
          "form": "저도 못봤습니다..ㅎㅎ 아쉽",
          "original_form": "저도 못봤습니다..ㅎㅎ 아쉽",
          "speaker_id": "1",
          "time": "20210908 03:00"
        },
        {
          "id": "MDRW2100006589.1.10",
          "form": "8시에 바빳어요",
          "original_form": "8시에 바빳어요",
          "speaker_id": "2",
          "time": "20210908 03:01"
        },
        {
          "id": "MDRW2100006589.1.11",
          "form": "이겼네요ㅋㅋㅋㅋ",
          "original_form": "이겼네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:01"
        },
        {
          "id": "MDRW2100006589.1.12",
          "form": "프로축구 보세요?",
          "original_form": "프로축구 보세요?",
          "speaker_id": "2",
          "time": "20210908 03:01"
        },
        {
          "id": "MDRW2100006589.1.13",
          "form": "그랬군요 저도 휴대폰 붙잡는다고 못봤어요",
          "original_form": "그랬군요 저도 휴대폰 붙잡는다고 못봤어요",
          "speaker_id": "1",
          "time": "20210908 03:02"
        },
        {
          "id": "MDRW2100006589.1.14",
          "form": "프로야구를 보시나요",
          "original_form": "프로야구를 보시나요",
          "speaker_id": "2",
          "time": "20210908 03:03"
        },
        {
          "id": "MDRW2100006589.1.15",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210908 03:03"
        },
        {
          "id": "MDRW2100006589.1.16",
          "form": "저는 사실 프로팀 경기를.잘 안봅니다..ㅎㅎ",
          "original_form": "저는 사실 프로팀 경기를.잘 안봅니다..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 03:04"
        },
        {
          "id": "MDRW2100006589.1.17",
          "form": "그럼 해외리그 보시나요?",
          "original_form": "그럼 해외리그 보시나요?",
          "speaker_id": "2",
          "time": "20210908 03:05"
        },
        {
