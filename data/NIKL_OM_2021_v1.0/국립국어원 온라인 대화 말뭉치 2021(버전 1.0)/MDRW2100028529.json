      "utterance": [
        {
          "id": "MDRW2100028529.1.1",
          "form": "저녁 뭐먹었어요",
          "original_form": "저녁 뭐먹었어요",
          "speaker_id": "2",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100028529.1.2",
          "form": "청년다방 떡볶이 먹었어요",
          "original_form": "청년다방 떡볶이 먹었어요",
          "speaker_id": "1",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100028529.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100028529.1.4",
          "form": "오 저도 땡기네요",
          "original_form": "오 저도 땡기네요",
          "speaker_id": "2",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028529.1.5",
          "form": "전 닭볶음탕이요 ㅋㅋ",
          "original_form": "전 닭볶음탕이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028529.1.6",
          "form": "청년다방 메뉴 뭐 좋아해요?",
          "original_form": "청년다방 메뉴 뭐 좋아해요?",
          "speaker_id": "2",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028529.1.7",
          "form": "차돌박이 들어간 떡볶이요ㅎㅎ",
          "original_form": "차돌박이 들어간 떡볶이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028529.1.8",
          "form": "닭볶음탕 맛있었겠어요",
          "original_form": "닭볶음탕 맛있었겠어요",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028529.1.9",
          "form": "맛있었죠 ㅎㅎ",
          "original_form": "맛있었죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028529.1.10",
          "form": "평소에 간식 같은거 잘 먹어요?",
          "original_form": "평소에 간식 같은거 잘 먹어요?",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028529.1.11",
          "form": "네ㅎㅎ 간식이라면 정신을 못차립니다",
          "original_form": "네ㅎㅎ 간식이라면 정신을 못차립니다",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028529.1.12",
          "form": "뭐 먹어요??",
          "original_form": "뭐 먹어요??",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028529.1.13",
          "form": "좋아하는거ㅋㅋ",
          "original_form": "좋아하는거ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028529.1.14",
          "form": "마카롱도 좋아하고",
          "original_form": "마카롱도 좋아하고",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028529.1.15",
          "form": "과자 좋아해요",
          "original_form": "과자 좋아해요",
          "speaker_id": "1",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028529.1.16",
          "form": "센베이 과자요",
          "original_form": "센베이 과자요",
          "speaker_id": "1",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028529.1.17",
          "form": "오.. 옛날거 요즘거 골고루 좋아하네요",
          "original_form": "오.. 옛날거 요즘거 골고루 좋아하네요",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028529.1.18",
          "form": "마카롱 당 땡길 때 좋죠",
          "original_form": "마카롱 당 땡길 때 좋죠",
          "speaker_id": "2",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028529.1.19",
          "form": "맞아요 단거하면 마카롱!",
          "original_form": "맞아요 단거하면 마카롱!",
          "speaker_id": "1",
          "time": "20210902 21:11"
        },
        {
          "id": "MDRW2100028529.1.20",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100028529.1.21",
          "form": "뚱카롱 좋아해요 아니면 그냥 일반 좋아해요?",
          "original_form": "뚱카롱 좋아해요 아니면 그냥 일반 좋아해요?",
          "speaker_id": "2",
          "time": "20210902 21:13"
        },
        {
          "id": "MDRW2100028529.1.22",
          "form": "저는 일반이요",
          "original_form": "저는 일반이요",
          "speaker_id": "1",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100028529.1.23",
          "form": "뚱카롱은 바삭한 맛이 너무 죽어요ㅠ",
          "original_form": "뚱카롱은 바삭한 맛이 너무 죽어요ㅠ",
          "speaker_id": "1",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100028529.1.24",
          "form": "저도 그래요 ㅋㅋㅋ 전 그 가운데 든걸 그닥 안좋아해서",
          "original_form": "저도 그래요 ㅋㅋㅋ 전 그 가운데 든걸 그닥 안좋아해서",
          "speaker_id": "2",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100028529.1.25",
          "form": "조금 든 게 좋더라구요",
          "original_form": "조금 든 게 좋더라구요",
          "speaker_id": "2",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100028529.1.26",
          "form": "좋아하는 맛 있나요",
          "original_form": "좋아하는 맛 있나요",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028529.1.27",
          "form": "저는 민트부터 찾는데ㅎㅎ",
          "original_form": "저는 민트부터 찾는데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028529.1.28",
          "form": "전 초코여..ㅋㅋㅋㅋㅋ 레몬 민트 이런게 차애예요",
          "original_form": "전 초코여..ㅋㅋㅋㅋㅋ 레몬 민트 이런게 차애예요",
          "speaker_id": "2",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028529.1.29",
          "form": "저는 레몬은 안좋아해요ㅠ",
          "original_form": "저는 레몬은 안좋아해요ㅠ",
          "speaker_id": "1",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028529.1.30",
          "form": "신거에 약해서ㅠ",
          "original_form": "신거에 약해서ㅠ",
          "speaker_id": "1",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028529.1.31",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100028529.1.32",
          "form": "그럼 요거트도 별로예요?",
          "original_form": "그럼 요거트도 별로예요?",
          "speaker_id": "2",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100028529.1.33",
          "form": "요거트까지는 그럭저럭 먹어요",
          "original_form": "요거트까지는 그럭저럭 먹어요",
