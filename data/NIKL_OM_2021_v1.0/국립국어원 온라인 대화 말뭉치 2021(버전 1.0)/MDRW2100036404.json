      "utterance": [
        {
          "id": "MDRW2100036404.1.1",
          "form": "헐 두부볶음밥이요?",
          "original_form": "헐 두부볶음밥이요?",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036404.1.2",
          "form": "그건 좀 신기한뎁쇼!",
          "original_form": "그건 좀 신기한뎁쇼!",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036404.1.3",
          "form": "어떻게 만드는거에요?",
          "original_form": "어떻게 만드는거에요?",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036404.1.4",
          "form": "그냥 양파랑 작게 뭉갠 두부랑 계란이랑해서 소금넣고 볶고",
          "original_form": "그냥 양파랑 작게 뭉갠 두부랑 계란이랑해서 소금넣고 볶고",
          "speaker_id": "1",
          "time": "20210831 11:36"
        },
        {
          "id": "MDRW2100036404.1.5",
          "form": "간장 쪼끔 넣어서 먹었어요",
          "original_form": "간장 쪼끔 넣어서 먹었어요",
          "speaker_id": "1",
          "time": "20210831 11:37"
        },
        {
          "id": "MDRW2100036404.1.6",
          "form": "그럭저럭 맛난 ㅋㅋㅋㅋ",
          "original_form": "그럭저럭 맛난 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:37"
        },
        {
          "id": "MDRW2100036404.1.7",
          "form": "홀 진짜 맛있겠어욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "홀 진짜 맛있겠어욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 13:18"
        },
        {
          "id": "MDRW2100036404.1.8",
          "form": "두부 하니까 순두부찌개가 땡기는군요",
          "original_form": "두부 하니까 순두부찌개가 땡기는군요",
          "speaker_id": "2",
          "time": "20210831 13:18"
        },
        {
          "id": "MDRW2100036404.1.9",
          "form": "옴마 순두부찌개 너무 좋죠",
          "original_form": "옴마 순두부찌개 너무 좋죠",
          "speaker_id": "1",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100036404.1.10",
          "form": "계란 탁 ㅠ",
          "original_form": "계란 탁 ㅠ",
          "speaker_id": "1",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100036404.1.11",
          "form": "크으으 얼큰한거 너무좋어요",
          "original_form": "크으으 얼큰한거 너무좋어요",
          "speaker_id": "2",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100036404.1.12",
          "form": "비오는데 너무 잘어울리는느낌...",
          "original_form": "비오는데 너무 잘어울리는느낌...",
          "speaker_id": "2",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100036404.1.13",
          "form": "맞아요 ㅠㅠㅠ 아아 너무 땡긴다",
          "original_form": "맞아요 ㅠㅠㅠ 아아 너무 땡긴다",
          "speaker_id": "1",
          "time": "20210831 22:55"
        },
        {
          "id": "MDRW2100036404.1.14",
          "form": "후딱 사라져요",
          "original_form": "후딱 사라져요",
          "speaker_id": "1",
          "time": "20210831 22:55"
        },
        {
          "id": "MDRW2100036404.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 왜 맛있는건 항상 금방 없어질까요ㅜ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 왜 맛있는건 항상 금방 없어질까요ㅜ",
          "speaker_id": "2",
          "time": "20210901 08:39"
        },
        {
          "id": "MDRW2100036404.1.16",
          "form": "그러니까요 ㅠ",
          "original_form": "그러니까요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 10:45"
        },
        {
          "id": "MDRW2100036404.1.17",
          "form": "음미하랴는데..",
          "original_form": "음미하랴는데..",
          "speaker_id": "1",
          "time": "20210901 10:45"
        },
        {
          "id": "MDRW2100036404.1.18",
          "form": "없어져있어",
          "original_form": "없어져있어",
          "speaker_id": "1",
          "time": "20210901 10:45"
        },
        {
          "id": "MDRW2100036404.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 크나큰 공감....",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 크나큰 공감....",
          "speaker_id": "2",
          "time": "20210901 13:22"
        },
        {
          "id": "MDRW2100036404.1.20",
          "form": "있었는데요",
          "original_form": "있었는데요",
          "speaker_id": "2",
          "time": "20210901 13:22"
        },
        {
          "id": "MDRW2100036404.1.21",
          "form": "없었습니다",
          "original_form": "없었습니다",
          "speaker_id": "2",
          "time": "20210901 13:22"
        },
        {
          "id": "MDRW2100036404.1.22",
          "form": "이게 마술이지..",
          "original_form": "이게 마술이지..",
          "speaker_id": "1",
          "time": "20210901 18:55"
        },
        {
          "id": "MDRW2100036404.1.23",
          "form": "저녁은 드셨나용",
          "original_form": "저녁은 드셨나용",
          "speaker_id": "1",
          "time": "20210901 18:55"
        },
        {
          "id": "MDRW2100036404.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 네네 삼계탕 먹었습죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 네네 삼계탕 먹었습죠",
          "speaker_id": "2",
          "time": "20210901 19:33"
        },
        {
          "id": "MDRW2100036404.1.25",
          "form": "요새 기력이 쇠해요...ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "요새 기력이 쇠해요...ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:34"
        },
        {
          "id": "MDRW2100036404.1.26",
          "form": "아아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100036404.1.27",
          "form": "삼계탕이 직빵이긴하죠",
          "original_form": "삼계탕이 직빵이긴하죠",
          "speaker_id": "1",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100036404.1.28",
          "form": "백신 맞으셨어요ㅡ",
          "original_form": "백신 맞으셨어요ㅡ",
          "speaker_id": "1",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100036404.1.29",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100036404.1.30",
          "form": "앗 아뇨 ㅋㅋㅋㅋㅋ 그냥 체력이 그지라 먹었습니닼ㅋㅋㅋㅋ",
          "original_form": "앗 아뇨 ㅋㅋㅋㅋㅋ 그냥 체력이 그지라 먹었습니닼ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100036404.1.31",
          "form": "아아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100036404.1.32",
          "form": "밥 잘챙겨드세요",
          "original_form": "밥 잘챙겨드세요",
          "speaker_id": "1",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100036404.1.33",
          "form": "한국인은 역시 밥심 ㅋㅋㅋㅋ",
          "original_form": "한국인은 역시 밥심 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:20"
        },
        {
          "id": "MDRW2100036404.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 암요암요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 암요암요",
          "speaker_id": "2",
          "time": "20210902 13:20"
        },
        {
          "id": "MDRW2100036404.1.35",
          "form": "역시 밥심이죠",
          "original_form": "역시 밥심이죠",
          "speaker_id": "2",
          "time": "20210902 13:20"
        },
        {
          "id": "MDRW2100036404.1.36",
          "form": "점심 드시나요!",
          "original_form": "점심 드시나요!",
          "speaker_id": "2",
          "time": "20210902 13:21"
        },
        {
          "id": "MDRW2100036404.1.37",
          "form": "점심식사 저는 패스할 생각임다 ㅎ...",
          "original_form": "점심식사 저는 패스할 생각임다 ㅎ...",
          "speaker_id": "2",
