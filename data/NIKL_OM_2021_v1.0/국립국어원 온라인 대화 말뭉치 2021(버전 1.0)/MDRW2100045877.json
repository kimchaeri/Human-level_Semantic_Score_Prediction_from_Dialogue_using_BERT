      "utterance": [
        {
          "id": "MDRW2100045877.1.1",
          "form": "배고파",
          "original_form": "배고파",
          "speaker_id": "1",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100045877.1.2",
          "form": "뭐 맛있는게 있을까?",
          "original_form": "뭐 맛있는게 있을까?",
          "speaker_id": "1",
          "time": "20210908 08:18"
        },
        {
          "id": "MDRW2100045877.1.3",
          "form": "요거트라도 먹어",
          "original_form": "요거트라도 먹어",
          "speaker_id": "2",
          "time": "20210908 08:27"
        },
        {
          "id": "MDRW2100045877.1.4",
          "form": "그래야하나?",
          "original_form": "그래야하나?",
          "speaker_id": "1",
          "time": "20210908 08:32"
        },
        {
          "id": "MDRW2100045877.1.5",
          "form": "근데 먹기 구찮어",
          "original_form": "근데 먹기 구찮어",
          "speaker_id": "1",
          "time": "20210908 08:32"
        },
        {
          "id": "MDRW2100045877.1.6",
          "form": "오늘도 힘내",
          "original_form": "오늘도 힘내",
          "speaker_id": "1",
          "time": "20210908 08:32"
        },
        {
          "id": "MDRW2100045877.1.7",
          "form": "어이없다",
          "original_form": "어이없다",
          "speaker_id": "2",
          "time": "20210908 08:32"
        },
        {
          "id": "MDRW2100045877.1.8",
          "form": "난 아침에 요거트 먹고 나왔는데",
          "original_form": "난 아침에 요거트 먹고 나왔는데",
          "speaker_id": "2",
          "time": "20210908 08:32"
        },
        {
          "id": "MDRW2100045877.1.9",
          "form": "오늘 구내식당 맛있어보이더라",
          "original_form": "오늘 구내식당 맛있어보이더라",
          "speaker_id": "2",
          "time": "20210908 08:32"
        },
        {
          "id": "MDRW2100045877.1.10",
          "form": "구내식당 메뉴 중 뭐 먹을거야??",
          "original_form": "구내식당 메뉴 중 뭐 먹을거야??",
          "speaker_id": "1",
          "time": "20210908 08:39"
        },
        {
          "id": "MDRW2100045877.1.11",
          "form": "저녁에 콩나물국 일 듯",
          "original_form": "저녁에 콩나물국 일 듯",
          "speaker_id": "1",
          "time": "20210908 08:40"
        },
        {
          "id": "MDRW2100045877.1.12",
          "form": "두부 굽굽하고",
          "original_form": "두부 굽굽하고",
          "speaker_id": "1",
          "time": "20210908 08:40"
        },
        {
          "id": "MDRW2100045877.1.13",
          "form": "에비동 나온다길래 그거먹을라구",
          "original_form": "에비동 나온다길래 그거먹을라구",
          "speaker_id": "2",
          "time": "20210908 08:42"
        },
        {
          "id": "MDRW2100045877.1.14",
          "form": "오 볶음김치는?",
          "original_form": "오 볶음김치는?",
          "speaker_id": "2",
          "time": "20210908 08:42"
        },
        {
          "id": "MDRW2100045877.1.15",
          "form": "볶음김치 원해??",
          "original_form": "볶음김치 원해??",
          "speaker_id": "1",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100045877.1.16",
          "form": "에비동?  맛나겠다",
          "original_form": "에비동?  맛나겠다",
          "speaker_id": "1",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100045877.1.17",
          "form": "당근빳따루 원해",
          "original_form": "당근빳따루 원해",
          "speaker_id": "2",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045877.1.18",
          "form": "오키",
          "original_form": "오키",
          "speaker_id": "1",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045877.1.19",
          "form": "웅 맛있어보여서 먹어볼라고",
          "original_form": "웅 맛있어보여서 먹어볼라고",
          "speaker_id": "2",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045877.1.20",
          "form": "ㅇㅇ 맛있었으면 좋겠다",
          "original_form": "ㅇㅇ 맛있었으면 좋겠다",
          "speaker_id": "1",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045877.1.21",
          "form": "ㅋㅋ 근데 사실 구내식당이라 별 기대안해",
          "original_form": "ㅋㅋ 근데 사실 구내식당이라 별 기대안해",
          "speaker_id": "2",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100045877.1.22",
          "form": "ㅋㅋ 그래도  혹시 하는 기대 한 스푼",
          "original_form": "ㅋㅋ 그래도  혹시 하는 기대 한 스푼",
          "speaker_id": "1",
          "time": "20210908 08:55"
        },
        {
          "id": "MDRW2100045877.1.23",
          "form": "으 ㅋㅋㅋ 먼 혹시야~~! 회사식당에 기대하는 사람이 어딨어",
          "original_form": "으 ㅋㅋㅋ 먼 혹시야~~! 회사식당에 기대하는 사람이 어딨어",
          "speaker_id": "2",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100045877.1.24",
          "form": "ㅋㅋ 여기있어 남의 밥 먹는게 얼마나 좋게",
          "original_form": "ㅋㅋ 여기있어 남의 밥 먹는게 얼마나 좋게",
          "speaker_id": "1",
          "time": "20210908 08:59"
        }
      ]
