      "utterance": [
        {
          "id": "MDRW2100035483.1.1",
          "form": "놀면뭐하니보세요?",
          "original_form": "놀면뭐하니보세요?",
          "speaker_id": "2",
          "time": "20210829 18:05"
        },
        {
          "id": "MDRW2100035483.1.2",
          "form": "요즘엔 안봐요",
          "original_form": "요즘엔 안봐요",
          "speaker_id": "1",
          "time": "20210829 18:05"
        },
        {
          "id": "MDRW2100035483.1.3",
          "form": "요즘엔 런닝맨이랑",
          "original_form": "요즘엔 런닝맨이랑",
          "speaker_id": "1",
          "time": "20210829 18:05"
        },
        {
          "id": "MDRW2100035483.1.4",
          "form": "노는언니. 골때리는그녀들",
          "original_form": "노는언니. 골때리는그녀들",
          "speaker_id": "1",
          "time": "20210829 18:05"
        },
        {
          "id": "MDRW2100035483.1.5",
          "form": "봅니다ㅎㅎ",
          "original_form": "봅니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 18:05"
        },
        {
          "id": "MDRW2100035483.1.6",
          "form": "런닝맨!",
          "original_form": "런닝맨!",
          "speaker_id": "2",
          "time": "20210829 18:07"
        },
        {
          "id": "MDRW2100035483.1.7",
          "form": "지금 보고계세요?",
          "original_form": "지금 보고계세요?",
          "speaker_id": "2",
          "time": "20210829 18:07"
        },
        {
          "id": "MDRW2100035483.1.8",
          "form": "아 또 본방은 잘 안봐요ㅋㅋㅋㅋㅋ",
          "original_form": "아 또 본방은 잘 안봐요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:14"
        },
        {
          "id": "MDRW2100035483.1.9",
          "form": "밥먹거나할때 웨이브로",
          "original_form": "밥먹거나할때 웨이브로",
          "speaker_id": "1",
          "time": "20210829 18:14"
        },
        {
          "id": "MDRW2100035483.1.10",
          "form": "봅니다",
          "original_form": "봅니다",
          "speaker_id": "1",
          "time": "20210829 18:14"
        },
        {
          "id": "MDRW2100035483.1.11",
          "form": "아ㅋㅋ 그러시군요",
          "original_form": "아ㅋㅋ 그러시군요",
          "speaker_id": "2",
          "time": "20210829 18:28"
        },
        {
          "id": "MDRW2100035483.1.12",
          "form": "플랫폼 나오고 본방 잘 안보게 되긴해요ㅋㅋ",
          "original_form": "플랫폼 나오고 본방 잘 안보게 되긴해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:29"
        },
        {
          "id": "MDRW2100035483.1.13",
          "form": "플랫폼 어떤거 쓰세요??",
          "original_form": "플랫폼 어떤거 쓰세요??",
          "speaker_id": "1",
          "time": "20210829 18:29"
        },
        {
          "id": "MDRW2100035483.1.14",
          "form": "웨이브 넷플 네이버 티빙이요!",
          "original_form": "웨이브 넷플 네이버 티빙이요!",
          "speaker_id": "2",
          "time": "20210829 18:37"
        },
        {
          "id": "MDRW2100035483.1.15",
          "form": "친구랑 노나서 쓰고 있어서 많아졌어요ㅋㅋ",
          "original_form": "친구랑 노나서 쓰고 있어서 많아졌어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:37"
        },
        {
          "id": "MDRW2100035483.1.16",
          "form": "ㅋㄱㅋㄱㅋㄱㄱ다쓰시네요?!ㅋㄱㅋㅋㄱㄱ",
          "original_form": "ㅋㄱㅋㄱㅋㄱㄱ다쓰시네요?!ㅋㄱㅋㅋㄱㄱ",
          "speaker_id": "1",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100035483.1.17",
          "form": "친구들이랑 팟구해서 하면 좋죠ㅠ",
          "original_form": "친구들이랑 팟구해서 하면 좋죠ㅠ",
          "speaker_id": "1",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100035483.1.18",
          "form": "넹ㅋㅋ 왓챠만 못써봤어요",
          "original_form": "넹ㅋㅋ 왓챠만 못써봤어요",
          "speaker_id": "2",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100035483.1.19",
          "form": "어디 쓰세요?",
          "original_form": "어디 쓰세요?",
          "speaker_id": "2",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100035483.1.20",
          "form": "저는 웨이브만 써요!",
          "original_form": "저는 웨이브만 써요!",
          "speaker_id": "1",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100035483.1.21",
          "form": "제가돈내고 가족들이랑보느라ㅠ",
          "original_form": "제가돈내고 가족들이랑보느라ㅠ",
          "speaker_id": "1",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100035483.1.22",
          "form": "앗ㅠ 그렇군요",
          "original_form": "앗ㅠ 그렇군요",
          "speaker_id": "2",
          "time": "20210829 19:00"
        },
        {
          "id": "MDRW2100035483.1.23",
          "form": "전 개인적으로 웨이브가 젤 많이 써요ㅋㅋ",
          "original_form": "전 개인적으로 웨이브가 젤 많이 써요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:00"
        },
        {
          "id": "MDRW2100035483.1.24",
          "form": "그쵸!! 웨이브가 제일 편하더라고요 저는",
          "original_form": "그쵸!! 웨이브가 제일 편하더라고요 저는",
          "speaker_id": "1",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100035483.1.25",
          "form": "넷플왓차한번씩다돌고",
          "original_form": "넷플왓차한번씩다돌고",
          "speaker_id": "1",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100035483.1.26",
          "form": "웨이브정착ㅋㅋ",
          "original_form": "웨이브정착ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:04"
        },
        {
          "id": "MDRW2100035483.1.27",
          "form": "맞아요ㅋㅋㄲ",
          "original_form": "맞아요ㅋㅋㄲ",
          "speaker_id": "2",
          "time": "20210829 19:07"
        },
        {
          "id": "MDRW2100035483.1.28",
          "form": "하나만 고르라면 웨이브를 선택할거에요ㅋㅋㅋ",
          "original_form": "하나만 고르라면 웨이브를 선택할거에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:07"
        }
      ]
