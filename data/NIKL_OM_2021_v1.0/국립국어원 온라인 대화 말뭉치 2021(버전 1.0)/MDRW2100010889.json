      "utterance": [
        {
          "id": "MDRW2100010889.1.1",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 22:40"
        },
        {
          "id": "MDRW2100010889.1.2",
          "form": "운동은 별로 안 좋아하는데",
          "original_form": "운동은 별로 안 좋아하는데",
          "speaker_id": "2",
          "time": "20210823 22:41"
        },
        {
          "id": "MDRW2100010889.1.3",
          "form": "요즘 수영 다니고 있어요",
          "original_form": "요즘 수영 다니고 있어요",
          "speaker_id": "2",
          "time": "20210823 22:41"
        },
        {
          "id": "MDRW2100010889.1.4",
          "form": "살려면 운동해야죠...ㅋㅋ \r\n수영은 얼마나 햇어요?",
          "original_form": "살려면 운동해야죠...ㅋㅋ \r\n수영은 얼마나 햇어요?",
          "speaker_id": "1",
          "time": "20210823 22:44"
        },
        {
          "id": "MDRW2100010889.1.5",
          "form": "이제 한 달이요!",
          "original_form": "이제 한 달이요!",
          "speaker_id": "2",
          "time": "20210823 22:45"
        },
        {
          "id": "MDRW2100010889.1.6",
          "form": "그럼 아직 발장구?",
          "original_form": "그럼 아직 발장구?",
          "speaker_id": "1",
          "time": "20210823 22:46"
        },
        {
          "id": "MDRW2100010889.1.7",
          "form": "발장구 하고",
          "original_form": "발장구 하고",
          "speaker_id": "2",
          "time": "20210823 22:46"
        },
        {
          "id": "MDRW2100010889.1.8",
          "form": "이제 팔 돌리는 거 해요!",
          "original_form": "이제 팔 돌리는 거 해요!",
          "speaker_id": "2",
          "time": "20210823 22:46"
        },
        {
          "id": "MDRW2100010889.1.9",
          "form": "그거 판들고요?",
          "original_form": "그거 판들고요?",
          "speaker_id": "1",
          "time": "20210823 22:47"
        },
        {
          "id": "MDRW2100010889.1.10",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210823 22:48"
        },
        {
          "id": "MDRW2100010889.1.11",
          "form": "판 안 들고 하면",
          "original_form": "판 안 들고 하면",
          "speaker_id": "2",
          "time": "20210823 22:48"
        },
        {
          "id": "MDRW2100010889.1.12",
          "form": "꼬르륵 하고 가라앉아요...",
          "original_form": "꼬르륵 하고 가라앉아요...",
          "speaker_id": "2",
          "time": "20210823 22:48"
        },
        {
          "id": "MDRW2100010889.1.13",
          "form": "어깨 힘빼는게 진짜 어렵지 않나요?",
          "original_form": "어깨 힘빼는게 진짜 어렵지 않나요?",
          "speaker_id": "1",
          "time": "20210823 22:49"
        },
        {
          "id": "MDRW2100010889.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210823 22:49"
        },
        {
          "id": "MDRW2100010889.1.15",
          "form": "완전 어려워요",
          "original_form": "완전 어려워요",
          "speaker_id": "2",
          "time": "20210823 22:49"
        },
        {
          "id": "MDRW2100010889.1.16",
          "form": "힘만 빼는 건데 왜 이랗게 어려운지ㅠㅠ",
          "original_form": "힘만 빼는 건데 왜 이랗게 어려운지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 22:50"
        },
        {
          "id": "MDRW2100010889.1.17",
          "form": "저도 근육질이라 영 물에 안뜨더라구요.. ㅋㅋㅋ",
          "original_form": "저도 근육질이라 영 물에 안뜨더라구요.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100010889.1.18",
          "form": "2달 배우고 포기..",
          "original_form": "2달 배우고 포기..",
          "speaker_id": "1",
          "time": "20210823 22:51"
        },
        {
          "id": "MDRW2100010889.1.19",
          "form": "운동을 열심히 하시나 봐요",
          "original_form": "운동을 열심히 하시나 봐요",
          "speaker_id": "2",
          "time": "20210823 22:52"
        },
        {
          "id": "MDRW2100010889.1.20",
          "form": "ㅋㅋㅋ 홈트랑 등산 정도해요.. \r\n여유생김 다시 수영배울려고요..",
          "original_form": "ㅋㅋㅋ 홈트랑 등산 정도해요.. \r\n여유생김 다시 수영배울려고요..",
          "speaker_id": "1",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100010889.1.21",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100010889.1.22",
          "form": "저는 등산하다가 근육통이 너무 와서",
          "original_form": "저는 등산하다가 근육통이 너무 와서",
          "speaker_id": "2",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100010889.1.23",
          "form": "그 뒤로는 안 가요",
          "original_form": "그 뒤로는 안 가요",
          "speaker_id": "2",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100010889.1.24",
          "form": "경상도에 산 진짜 좋잖아요.. \r\n영남알프스라고..",
          "original_form": "경상도에 산 진짜 좋잖아요.. \r\n영남알프스라고..",
          "speaker_id": "1",
          "time": "20210823 22:54"
        }
      ]
