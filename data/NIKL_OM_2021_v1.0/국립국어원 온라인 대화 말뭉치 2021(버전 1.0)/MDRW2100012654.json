      "utterance": [
        {
          "id": "MDRW2100012654.1.1",
          "form": "운동 하시나요",
          "original_form": "운동 하시나요",
          "speaker_id": "2",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100012654.1.2",
          "form": "근력운동해요... \n집에 턱걸이 기계가 있어서.. ㅎㅎㅎ",
          "original_form": "근력운동해요... \n집에 턱걸이 기계가 있어서.. ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100012654.1.3",
          "form": "오 홈트네요",
          "original_form": "오 홈트네요",
          "speaker_id": "2",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100012654.1.4",
          "form": "유툽으로 보는 사람 있나요?",
          "original_form": "유툽으로 보는 사람 있나요?",
          "speaker_id": "2",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100012654.1.5",
          "form": "운동벚 같은건 안보고 가끔씩 부상당하면 재활관련 찾아봐요..",
          "original_form": "운동벚 같은건 안보고 가끔씩 부상당하면 재활관련 찾아봐요..",
          "speaker_id": "1",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100012654.1.6",
          "form": "운동 유투브 많이 보세요?",
          "original_form": "운동 유투브 많이 보세요?",
          "speaker_id": "1",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100012654.1.7",
          "form": "아뇨 그건 아닌데",
          "original_form": "아뇨 그건 아닌데",
          "speaker_id": "2",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100012654.1.8",
          "form": "전 땅끄부부보고 홈트해요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 땅끄부부보고 홈트해요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100012654.1.9",
          "form": "그거 유명하죠..",
          "original_form": "그거 유명하죠..",
          "speaker_id": "1",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100012654.1.10",
          "form": "채널 대기업급이죠?",
          "original_form": "채널 대기업급이죠?",
          "speaker_id": "1",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100012654.1.11",
          "form": "ㅋㅋㅋㅋㅋ네",
          "original_form": "ㅋㅋㅋㅋㅋ네",
          "speaker_id": "2",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100012654.1.12",
          "form": "정말 힘들더라구요",
          "original_form": "정말 힘들더라구요",
          "speaker_id": "2",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100012654.1.13",
          "form": "마라맛이에요",
          "original_form": "마라맛이에요",
          "speaker_id": "2",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100012654.1.14",
          "form": "ㅋㅋㅋ 어찌됫든 몸만 움직이면 칼로리는 소모되이까..",
          "original_form": "ㅋㅋㅋ 어찌됫든 몸만 움직이면 칼로리는 소모되이까..",
          "speaker_id": "1",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100012654.1.15",
          "form": "땅크 한번봣는데 저도 못따라가겟더라구요",
          "original_form": "땅크 한번봣는데 저도 못따라가겟더라구요",
          "speaker_id": "1",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100012654.1.16",
          "form": "네.... 허벅지가 불타더라구요",
          "original_form": "네.... 허벅지가 불타더라구요",
          "speaker_id": "2",
          "time": "20210824 23:56"
        },
        {
          "id": "MDRW2100012654.1.17",
          "form": "노페인 노머슬..",
          "original_form": "노페인 노머슬..",
          "speaker_id": "1",
          "time": "20210824 23:56"
        },
        {
          "id": "MDRW2100012654.1.18",
          "form": "고통온만큼.. 결과가.. ㅎㅎㅎ",
          "original_form": "고통온만큼.. 결과가.. ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 23:56"
        },
        {
          "id": "MDRW2100012654.1.19",
          "form": "전 주말만 해서 그런지",
          "original_form": "전 주말만 해서 그런지",
          "speaker_id": "2",
          "time": "20210824 23:57"
        },
        {
          "id": "MDRW2100012654.1.20",
          "form": "효과 좀 흑 ㅠ",
          "original_form": "효과 좀 흑 ㅠ",
          "speaker_id": "2",
          "time": "20210824 23:57"
        },
        {
          "id": "MDRW2100012654.1.21",
          "form": "5일 쉬면 다시 알베지 않나여?",
          "original_form": "5일 쉬면 다시 알베지 않나여?",
          "speaker_id": "1",
          "time": "20210824 23:57"
        },
        {
          "id": "MDRW2100012654.1.22",
          "form": "전 4일이상쉬면 리셋되는듯..",
          "original_form": "전 4일이상쉬면 리셋되는듯..",
          "speaker_id": "1",
          "time": "20210824 23:57"
        }
      ]
