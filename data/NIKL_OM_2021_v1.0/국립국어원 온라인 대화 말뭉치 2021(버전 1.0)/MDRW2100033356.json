        },
        {
          "id": "MDRW2100033356.1.2",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "2",
          "time": "20210906 22:48"
        },
        {
          "id": "MDRW2100033356.1.3",
          "form": "종교가 있으세요?",
          "original_form": "종교가 있으세요?",
          "speaker_id": "1",
          "time": "20210906 22:48"
        },
        {
          "id": "MDRW2100033356.1.4",
          "form": "아뇨 무교예요",
          "original_form": "아뇨 무교예요",
          "speaker_id": "2",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100033356.1.5",
          "form": "저도 무교",
          "original_form": "저도 무교",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100033356.1.6",
          "form": "부모님도 무교세요?",
          "original_form": "부모님도 무교세요?",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100033356.1.7",
          "form": "세례명이 있기는 하신데 사실상 무교세요",
          "original_form": "세례명이 있기는 하신데 사실상 무교세요",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100033356.1.8",
          "form": "저흰 불교쪽인데 강요안하세요",
          "original_form": "저흰 불교쪽인데 강요안하세요",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100033356.1.9",
          "form": "그럼 교회 가보셨어요?",
          "original_form": "그럼 교회 가보셨어요?",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100033356.1.10",
          "form": "요즘은 종교 거의 강요 안하시는거같아요",
          "original_form": "요즘은 종교 거의 강요 안하시는거같아요",
          "speaker_id": "2",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100033356.1.11",
          "form": "아뇨 교회는 안가봣어요",
          "original_form": "아뇨 교회는 안가봣어요",
          "speaker_id": "2",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100033356.1.12",
          "form": "친구따라 전 가봤거든요",
          "original_form": "친구따라 전 가봤거든요",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100033356.1.13",
          "form": "어떠셨어요?",
          "original_form": "어떠셨어요?",
          "speaker_id": "2",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100033356.1.14",
          "form": "성탄절에 선물준대서",
          "original_form": "성탄절에 선물준대서",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100033356.1.15",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100033356.1.16",
          "form": "뭐 받으셨나요?",
          "original_form": "뭐 받으셨나요?",
          "speaker_id": "2",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100033356.1.17",
          "form": "통성기도 아세요?",
          "original_form": "통성기도 아세요?",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100033356.1.18",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100033356.1.19",
          "form": "너무 충격받아서ㅋㅋ",
          "original_form": "너무 충격받아서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100033356.1.20",
          "form": "선물은 기억이 나지않네요ㅎㅎ",
          "original_form": "선물은 기억이 나지않네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100033356.1.21",
          "form": "나중에 인터넷으로 찾아보시면ㅜㅠㅋㅋ",
