      "utterance": [
        {
          "id": "MDRW2100027792.1.1",
          "form": "나 배고파",
          "original_form": "나 배고파",
          "speaker_id": "1",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100027792.1.2",
          "form": "엄청 맛잇는 만두먹고싶은데 올라가도 같이 먹을사람이 없어서 슬픜거 잇지",
          "original_form": "엄청 맛잇는 만두먹고싶은데 올라가도 같이 먹을사람이 없어서 슬픜거 잇지",
          "speaker_id": "1",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100027792.1.3",
          "form": "아 그 봤어 혜화쪽에 만두",
          "original_form": "아 그 봤어 혜화쪽에 만두",
          "speaker_id": "2",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100027792.1.4",
          "form": "어젠가부터 알티한 혜화 만두맛집..",
          "original_form": "어젠가부터 알티한 혜화 만두맛집..",
          "speaker_id": "1",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100027792.1.5",
          "form": "거기 맛있겠더라",
          "original_form": "거기 맛있겠더라",
          "speaker_id": "2",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100027792.1.6",
          "form": "너 나올수잇니",
          "original_form": "너 나올수잇니",
          "speaker_id": "1",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027792.1.7",
          "form": "지금?????????",
          "original_form": "지금?????????",
          "speaker_id": "2",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027792.1.8",
          "form": "지금이겟냐",
          "original_form": "지금이겟냐",
          "speaker_id": "1",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027792.1.9",
          "form": "백신안맞아서",
          "original_form": "백신안맞아서",
          "speaker_id": "1",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027792.1.10",
          "form": "못나오지",
          "original_form": "못나오지",
          "speaker_id": "1",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027792.1.11",
          "form": "나 1차 맞았어",
          "original_form": "나 1차 맞았어",
          "speaker_id": "2",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027792.1.12",
          "form": "저런.....",
          "original_form": "저런.....",
          "speaker_id": "2",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027792.1.13",
          "form": "아 근데 거기 만두가",
          "original_form": "아 근데 거기 만두가",
          "speaker_id": "2",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027792.1.14",
          "form": "그래서 돌려말햇어",
          "original_form": "그래서 돌려말햇어",
          "speaker_id": "1",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027792.1.15",
          "form": "진짜 맛있어 보여서",
          "original_form": "진짜 맛있어 보여서",
          "speaker_id": "2",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027792.1.16",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027792.1.17",
          "form": "나 너무... 가고싶고먹고싶고 눈물이나고",
          "original_form": "나 너무... 가고싶고먹고싶고 눈물이나고",
          "speaker_id": "1",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027792.1.18",
          "form": "지금 탐라에 문익점푸딩이랑 술이랑 다 파는 카페?",
          "original_form": "지금 탐라에 문익점푸딩이랑 술이랑 다 파는 카페?",
          "speaker_id": "1",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027792.1.19",
          "form": "전부 광주더라",
          "original_form": "전부 광주더라",
          "speaker_id": "1",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027792.1.20",
          "form": "내 고향이 그리워 졌죠",
          "original_form": "내 고향이 그리워 졌죠",
          "speaker_id": "1",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027792.1.21",
          "form": "문익점 푸딩 개웃겨",
          "original_form": "문익점 푸딩 개웃겨",
          "speaker_id": "2",
          "time": "20210902 15:13"
        },
        {
          "id": "MDRW2100027792.1.22",
          "form": "광주에도되게 디저트 많아",
          "original_form": "광주에도되게 디저트 많아",
          "speaker_id": "2",
          "time": "20210902 15:13"
        },
        {
          "id": "MDRW2100027792.1.23",
          "form": "맛있어보이는거",
          "original_form": "맛있어보이는거",
          "speaker_id": "2",
          "time": "20210902 15:13"
        },
        {
          "id": "MDRW2100027792.1.24",
          "form": "광주는 사실 살때 별 생각안했어",
          "original_form": "광주는 사실 살때 별 생각안했어",
