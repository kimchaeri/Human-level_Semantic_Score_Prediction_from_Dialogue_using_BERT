        },
        {
          "id": "MDRW2100003010.1.2",
          "form": "아니요 처음들어요",
          "original_form": "아니요 처음들어요",
          "speaker_id": "2",
          "time": "20210820 17:31"
        },
        {
          "id": "MDRW2100003010.1.3",
          "form": "그건 어떤거에요?",
          "original_form": "그건 어떤거에요?",
          "speaker_id": "2",
          "time": "20210820 17:31"
        },
        {
          "id": "MDRW2100003010.1.4",
          "form": "ai시대라 막 체형같은거 다",
          "original_form": "ai시대라 막 체형같은거 다",
          "speaker_id": "1",
          "time": "20210820 17:31"
        },
        {
          "id": "MDRW2100003010.1.5",
          "form": "진단해주고 운동에 도움된대요",
          "original_form": "진단해주고 운동에 도움된대요",
          "speaker_id": "1",
          "time": "20210820 17:31"
        },
        {
          "id": "MDRW2100003010.1.6",
          "form": "아 들어본거같아요",
          "original_form": "아 들어본거같아요",
          "speaker_id": "2",
          "time": "20210820 17:31"
        },
        {
          "id": "MDRW2100003010.1.7",
          "form": "운동은 언제해도 힘들어요 ㅠ",
          "original_form": "운동은 언제해도 힘들어요 ㅠ",
          "speaker_id": "1",
          "time": "20210820 17:32"
        },
        {
          "id": "MDRW2100003010.1.8",
          "form": "신체 사진 찍으면 분석해서 알려주는 그거",
          "original_form": "신체 사진 찍으면 분석해서 알려주는 그거",
          "speaker_id": "2",
          "time": "20210820 17:32"
        },
        {
          "id": "MDRW2100003010.1.9",
          "form": "맞아용",
          "original_form": "맞아용",
          "speaker_id": "1",
          "time": "20210820 17:32"
        },
        {
          "id": "MDRW2100003010.1.10",
          "form": "맞아요 운동은 꾸준히 해야하는 것도 있잖아요",
          "original_form": "맞아요 운동은 꾸준히 해야하는 것도 있잖아요",
          "speaker_id": "2",
          "time": "20210820 17:32"
        },
        {
          "id": "MDRW2100003010.1.11",
          "form": "인바디도 알아서 측정해주곤 한다네여",
          "original_form": "인바디도 알아서 측정해주곤 한다네여",
          "speaker_id": "1",
          "time": "20210820 17:32"
        },
        {
          "id": "MDRW2100003010.1.12",
          "form": "뭔가 시간과 노력을 들여먀하는",
          "original_form": "뭔가 시간과 노력을 들여먀하는",
          "speaker_id": "2",
          "time": "20210820 17:32"
        },
        {
          "id": "MDRW2100003010.1.13",
          "form": "인바디도요?",
          "original_form": "인바디도요?",
          "speaker_id": "2",
          "time": "20210820 17:33"
        },
        {
          "id": "MDRW2100003010.1.14",
          "form": "진짜 스마트한 시대네요",
          "original_form": "진짜 스마트한 시대네요",
          "speaker_id": "2",
          "time": "20210820 17:33"
        },
        {
          "id": "MDRW2100003010.1.15",
          "form": "그쵸 ㅋㅋㅋㅋㅋ진짜 앞으로가 더 기대되요",
