          "speaker_id": "2",
          "time": "20210906 14:25"
        },
        {
          "id": "MDRW2100041679.1.3",
          "form": "그 이정재나오는 잔인한것두",
          "original_form": "그 이정재나오는 잔인한것두",
          "speaker_id": "2",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100041679.1.4",
          "form": "기대중이에요",
          "original_form": "기대중이에요",
          "speaker_id": "2",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100041679.1.5",
          "form": "오 그건또뭐예요ㅡ???!!!!!",
          "original_form": "오 그건또뭐예요ㅡ???!!!!!",
          "speaker_id": "1",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100041679.1.6",
          "form": "오 알려주세요!",
          "original_form": "오 알려주세요!",
          "speaker_id": "1",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100041679.1.7",
          "form": "저도 잘몰라욥 언뜻본거같애서 ㅎㅎㅎㅎ",
          "original_form": "저도 잘몰라욥 언뜻본거같애서 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100041679.1.8",
          "form": "제목이",
          "original_form": "제목이",
          "speaker_id": "2",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100041679.1.9",
          "form": "오징어게임이래유",
          "original_form": "오징어게임이래유",
          "speaker_id": "2",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100041679.1.10",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100041679.1.11",
          "form": "오 ㅋㅋㅋ잔인한거  어...막 잘린거보여주면 못보는데",
          "original_form": "오 ㅋㅋㅋ잔인한거  어...막 잘린거보여주면 못보는데",
          "speaker_id": "1",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100041679.1.12",
          "form": "그상황자체는좋아한다구해야하나",
          "original_form": "그상황자체는좋아한다구해야하나",
          "speaker_id": "1",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100041679.1.13",
          "form": "싸움ㅋㅋㅋㅋ그런거ㅋㄱㅈㅋ",
          "original_form": "싸움ㅋㅋㅋㅋ그런거ㅋㄱㅈㅋ",
          "speaker_id": "1",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100041679.1.14",
          "form": "기대되네요오우\n오징어게임...",
          "original_form": "기대되네요오우\n오징어게임...",
          "speaker_id": "1",
          "time": "20210906 14:31"
        },
        {
          "id": "MDRW2100041679.1.15",
          "form": "ㅋㅋㅋ저는 잔인한거 완전 조아해요",
          "original_form": "ㅋㅋㅋ저는 잔인한거 완전 조아해요",
          "speaker_id": "2",
          "time": "20210906 14:33"
        },
        {
          "id": "MDRW2100041679.1.16",
          "form": "이정재님좋아해서 기대중이에요",
          "original_form": "이정재님좋아해서 기대중이에요",
          "speaker_id": "2",
          "time": "20210906 14:33"
        },
        {
          "id": "MDRW2100041679.1.17",
          "form": "아 세상에 취향저격작품이네요ㅋㅋㄲㅋ",
          "original_form": "아 세상에 취향저격작품이네요ㅋㅋㄲㅋ",
          "speaker_id": "1",
          "time": "20210906 14:34"
        },
        {
          "id": "MDRW2100041679.1.18",
          "form": "이정재님까지 완벽",
