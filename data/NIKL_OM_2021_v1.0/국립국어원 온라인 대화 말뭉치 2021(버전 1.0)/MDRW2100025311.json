          "time": "20210901 09:03"
        },
        {
          "id": "MDRW2100025311.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 09:03"
        },
        {
          "id": "MDRW2100025311.1.25",
          "form": "맞는말이네...",
          "original_form": "맞는말이네...",
          "speaker_id": "2",
          "time": "20210901 09:04"
        },
        {
          "id": "MDRW2100025311.1.26",
