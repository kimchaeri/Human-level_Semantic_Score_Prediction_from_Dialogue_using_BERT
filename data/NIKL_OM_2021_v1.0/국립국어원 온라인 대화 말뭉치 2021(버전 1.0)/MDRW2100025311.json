      "utterance": [
        {
          "id": "MDRW2100025311.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ김성규도 작품 많이 하면 좋을텐데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ김성규도 작품 많이 하면 좋을텐데",
          "speaker_id": "2",
          "time": "20210901 09:02"
        },
        {
          "id": "MDRW2100025311.1.2",
          "form": "아 그니까",
          "original_form": "아 그니까",
          "speaker_id": "1",
          "time": "20210901 09:02"
        },
        {
          "id": "MDRW2100025311.1.3",
          "form": "킹덤 대박난거 치곤 생각보다 많이 안보이네",
          "original_form": "킹덤 대박난거 치곤 생각보다 많이 안보이네",
          "speaker_id": "2",
          "time": "20210901 09:02"
        },
        {
          "id": "MDRW2100025311.1.4",
          "form": "내가 못 찾는건지..",
          "original_form": "내가 못 찾는건지..",
          "speaker_id": "2",
          "time": "20210901 09:02"
        },
        {
          "id": "MDRW2100025311.1.5",
          "form": "킹덤3그러고보니",
          "original_form": "킹덤3그러고보니",
          "speaker_id": "1",
          "time": "20210901 09:02"
        },
        {
          "id": "MDRW2100025311.1.6",
          "form": "내년에나온다더라",
          "original_form": "내년에나온다더라",
          "speaker_id": "1",
          "time": "20210901 09:02"
        },
        {
          "id": "MDRW2100025311.1.7",
          "form": "영신이얘기가 주가 될느낌이던데",
          "original_form": "영신이얘기가 주가 될느낌이던데",
          "speaker_id": "1",
          "time": "20210901 09:02"
        },
        {
          "id": "MDRW2100025311.1.8",
          "form": "김은희작가얘기하는거보니",
          "original_form": "김은희작가얘기하는거보니",
          "speaker_id": "1",
          "time": "20210901 09:02"
        },
        {
          "id": "MDRW2100025311.1.9",
          "form": "오? 빠르네",
          "original_form": "오? 빠르네",
          "speaker_id": "2",
          "time": "20210901 09:02"
        },
        {
          "id": "MDRW2100025311.1.10",
          "form": "영신... 죽지마......ㅠㅠ",
          "original_form": "영신... 죽지마......ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 09:03"
        },
        {
          "id": "MDRW2100025311.1.11",
          "form": "하지만 죽이겠지.....",
          "original_form": "하지만 죽이겠지.....",
          "speaker_id": "2",
          "time": "20210901 09:03"
        },
        {
          "id": "MDRW2100025311.1.12",
          "form": "ㅇㅇ영신이 죽으면안돼.....",
          "original_form": "ㅇㅇ영신이 죽으면안돼.....",
          "speaker_id": "1",
          "time": "20210901 09:03"
        },
        {
          "id": "MDRW2100025311.1.13",
          "form": "김은희라면......",
          "original_form": "김은희라면......",
          "speaker_id": "2",
          "time": "20210901 09:03"
        },
        {
          "id": "MDRW2100025311.1.14",
          "form": "아 근데 영신이하니까 그 창귀",
          "original_form": "아 근데 영신이하니까 그 창귀",
          "speaker_id": "1",
          "time": "20210901 09:03"
        },
        {
          "id": "MDRW2100025311.1.15",
          "form": "그거생각나네여",
          "original_form": "그거생각나네여",
          "speaker_id": "1",
          "time": "20210901 09:03"
        },
        {
          "id": "MDRW2100025311.1.16",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210901 09:03"
        },
        {
          "id": "MDRW2100025311.1.17",
          "form": "아?",
          "original_form": "아?",
          "speaker_id": "2",
          "time": "20210901 09:03"
        },
        {
          "id": "MDRW2100025311.1.18",
          "form": "ㅋㅋㄱㅋㅋㅋ호랑이잡으러다닌 영신...",
          "original_form": "ㅋㅋㄱㅋㅋㅋ호랑이잡으러다닌 영신...",
          "speaker_id": "1",
          "time": "20210901 09:03"
        },
        {
          "id": "MDRW2100025311.1.19",
          "form": "범을 잡는다 거드럭대다가",
          "original_form": "범을 잡는다 거드럭대다가",
          "speaker_id": "2",
          "time": "20210901 09:03"
        },
        {
          "id": "MDRW2100025311.1.20",
          "form": "목숨을 잃었소만ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "목숨을 잃었소만ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 09:03"
        },
        {
          "id": "MDRW2100025311.1.21",
          "form": "영신이가 호랑이잡으러 다니다가",
          "original_form": "영신이가 호랑이잡으러 다니다가",
          "speaker_id": "1",
          "time": "20210901 09:03"
        },
        {
          "id": "MDRW2100025311.1.22",
          "form": "좀비한테 목숨을잃을뻔",
          "original_form": "좀비한테 목숨을잃을뻔",
          "speaker_id": "1",
          "time": "20210901 09:03"
        },
        {
          "id": "MDRW2100025311.1.23",
          "form": "ㅋㅋㅋㄱㅋㄱㅋㅋㄱㅋㄱ",
