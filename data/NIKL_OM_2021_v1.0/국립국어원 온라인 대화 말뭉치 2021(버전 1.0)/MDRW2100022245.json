      "utterance": [
        {
          "id": "MDRW2100022245.1.1",
          "form": "국밥은 그냥 건더기 먹는 느낌이라 먹네요",
          "original_form": "국밥은 그냥 건더기 먹는 느낌이라 먹네요",
          "speaker_id": "2",
          "time": "20210829 11:34"
        },
        {
          "id": "MDRW2100022245.1.2",
          "form": "그러고보니 저 순대국밥 만들어먹으려고 순대 샀어요 어제",
          "original_form": "그러고보니 저 순대국밥 만들어먹으려고 순대 샀어요 어제",
          "speaker_id": "2",
          "time": "20210829 11:34"
        },
        {
          "id": "MDRW2100022245.1.3",
          "form": "진짜요",
          "original_form": "진짜요",
          "speaker_id": "1",
          "time": "20210829 11:34"
        },
        {
          "id": "MDRW2100022245.1.4",
          "form": "어디서사셨어요",
          "original_form": "어디서사셨어요",
          "speaker_id": "1",
          "time": "20210829 11:35"
        },
        {
          "id": "MDRW2100022245.1.5",
          "form": "그 인터넷 쇼핑몰이요 ㅎㅎ",
          "original_form": "그 인터넷 쇼핑몰이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 11:35"
        },
        {
          "id": "MDRW2100022245.1.6",
          "form": "병천순대 아세요?",
          "original_form": "병천순대 아세요?",
          "speaker_id": "2",
          "time": "20210829 11:35"
        },
        {
          "id": "MDRW2100022245.1.7",
          "form": "네 알아요",
          "original_form": "네 알아요",
          "speaker_id": "1",
          "time": "20210829 11:35"
        },
        {
          "id": "MDRW2100022245.1.8",
          "form": "체인점으로 유명하죠?",
          "original_form": "체인점으로 유명하죠?",
          "speaker_id": "2",
          "time": "20210829 11:35"
        },
        {
          "id": "MDRW2100022245.1.9",
          "form": "거기순대 맛있어요",
          "original_form": "거기순대 맛있어요",
          "speaker_id": "1",
          "time": "20210829 11:35"
        },
        {
          "id": "MDRW2100022245.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210829 11:35"
        },
        {
          "id": "MDRW2100022245.1.11",
          "form": "맞아요 그래서 저 그거 샀어요 ㅎㅎ",
          "original_form": "맞아요 그래서 저 그거 샀어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 11:35"
        },
        {
          "id": "MDRW2100022245.1.12",
          "form": "병천순대에 납품하는 거래요",
          "original_form": "병천순대에 납품하는 거래요",
          "speaker_id": "2",
          "time": "20210829 11:35"
        },
        {
          "id": "MDRW2100022245.1.13",
          "form": "기본순대만사셨어요?",
          "original_form": "기본순대만사셨어요?",
          "speaker_id": "1",
          "time": "20210829 11:35"
        },
        {
          "id": "MDRW2100022245.1.14",
          "form": "전 지금까지 병천순대가 직접 수제로 만드는 줄..",
          "original_form": "전 지금까지 병천순대가 직접 수제로 만드는 줄..",
          "speaker_id": "2",
          "time": "20210829 11:35"
        },
        {
          "id": "MDRW2100022245.1.15",
          "form": "기본순대만 사려고 했더니 무료배송 안돼서..",
          "original_form": "기본순대만 사려고 했더니 무료배송 안돼서..",
          "speaker_id": "2",
          "time": "20210829 11:35"
        },
        {
          "id": "MDRW2100022245.1.16",
          "form": "백순대도 같이 샀어요 ㅎㅎ",
          "original_form": "백순대도 같이 샀어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 11:35"
        },
        {
          "id": "MDRW2100022245.1.17",
          "form": "백순대 볶음이요?",
          "original_form": "백순대 볶음이요?",
          "speaker_id": "1",
          "time": "20210829 11:35"
        },
        {
          "id": "MDRW2100022245.1.18",
          "form": "키트 사신건가",
          "original_form": "키트 사신건가",
          "speaker_id": "1",
          "time": "20210829 11:35"
        },
        {
          "id": "MDRW2100022245.1.19",
          "form": "저도 그건줄알았는데",
          "original_form": "저도 그건줄알았는데",
          "speaker_id": "2",
          "time": "20210829 11:36"
        },
        {
          "id": "MDRW2100022245.1.20",
          "form": "말 그대로 그냥 하얀순대더라구요?",
          "original_form": "말 그대로 그냥 하얀순대더라구요?",
          "speaker_id": "2",
          "time": "20210829 11:36"
        },
        {
          "id": "MDRW2100022245.1.21",
          "form": "이건 처음먹어봐서 좀 기대돼요 ㅎㅎ",
          "original_form": "이건 처음먹어봐서 좀 기대돼요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 11:36"
        },
        {
          "id": "MDRW2100022245.1.22",
          "form": "아앗.. 걍 백순대구나",
          "original_form": "아앗.. 걍 백순대구나",
          "speaker_id": "1",
          "time": "20210829 11:36"
        },
        {
          "id": "MDRW2100022245.1.23",
          "form": "먹어봤어요?",
          "original_form": "먹어봤어요?",
          "speaker_id": "2",
          "time": "20210829 11:36"
        },
        {
          "id": "MDRW2100022245.1.24",
          "form": "이름에 충실한 상품인편",
          "original_form": "이름에 충실한 상품인편",
          "speaker_id": "1",
          "time": "20210829 11:36"
        },
        {
          "id": "MDRW2100022245.1.25",
          "form": "아니요ㅋㅋㅋㅋ",
          "original_form": "아니요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:36"
        },
        {
          "id": "MDRW2100022245.1.26",
          "form": "ㅋㅋㅋㅋ그니까요 ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋ그니까요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 11:36"
        }
      ]
