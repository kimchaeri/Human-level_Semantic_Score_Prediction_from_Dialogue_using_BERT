      "utterance": [
        {
          "id": "MDRW2100016356.1.1",
          "form": "해산물 좋아해요!",
          "original_form": "해산물 좋아해요!",
          "speaker_id": "2",
          "time": "20210826 12:13"
        },
        {
          "id": "MDRW2100016356.1.2",
          "form": "오옹 저도 해산물 좋아해요!",
          "original_form": "오옹 저도 해산물 좋아해요!",
          "speaker_id": "1",
          "time": "20210826 13:38"
        },
        {
          "id": "MDRW2100016356.1.3",
          "form": "어떤 해산물이 제일 좋으세요?",
          "original_form": "어떤 해산물이 제일 좋으세요?",
          "speaker_id": "1",
          "time": "20210826 13:38"
        },
        {
          "id": "MDRW2100016356.1.4",
          "form": "갑각류도 좋구요 회도 좋아해요",
          "original_form": "갑각류도 좋구요 회도 좋아해요",
          "speaker_id": "2",
          "time": "20210826 13:44"
        },
        {
          "id": "MDRW2100016356.1.5",
          "form": "뭐 좋아하세요?",
          "original_form": "뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 13:44"
        },
        {
          "id": "MDRW2100016356.1.6",
          "form": "저는 회나 탕을 좋아해요",
          "original_form": "저는 회나 탕을 좋아해요",
          "speaker_id": "1",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016356.1.7",
          "form": "면류도 좋아해요",
          "original_form": "면류도 좋아해요",
          "speaker_id": "1",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016356.1.8",
          "form": "탕종류중에 좋아하는 메뉴는 무엇인가요?",
          "original_form": "탕종류중에 좋아하는 메뉴는 무엇인가요?",
          "speaker_id": "2",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100016356.1.9",
          "form": "연포탕도 좋아하고 해물탕도 좋아해요",
          "original_form": "연포탕도 좋아하고 해물탕도 좋아해요",
          "speaker_id": "1",
          "time": "20210826 14:27"
        },
        {
          "id": "MDRW2100016356.1.10",
          "form": "원탑을 고르기엔 너무 맛있는게 많아요ㅠㅋㅋㅋㅋ",
          "original_form": "원탑을 고르기엔 너무 맛있는게 많아요ㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:27"
        },
        {
          "id": "MDRW2100016356.1.11",
          "form": "해물탕 좋죠ㅠㅠㅠㅠ",
          "original_form": "해물탕 좋죠ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 14:47"
        },
        {
          "id": "MDRW2100016356.1.12",
          "form": "면류는 어떤거 좋아하세요?",
          "original_form": "면류는 어떤거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 14:47"
        },
        {
          "id": "MDRW2100016356.1.13",
          "form": "면이면 다 좋아해요!",
          "original_form": "면이면 다 좋아해요!",
          "speaker_id": "1",
          "time": "20210826 14:48"
        },
        {
          "id": "MDRW2100016356.1.14",
          "form": "콩국수 빼고 가리지 않아요",
          "original_form": "콩국수 빼고 가리지 않아요",
          "speaker_id": "1",
          "time": "20210826 14:48"
        },
        {
          "id": "MDRW2100016356.1.15",
          "form": "저두요!",
          "original_form": "저두요!",
          "speaker_id": "2",
          "time": "20210826 14:48"
        },
        {
          "id": "MDRW2100016356.1.16",
          "form": "탕, 면 저도 다 너무 좋아해요",
          "original_form": "탕, 면 저도 다 너무 좋아해요",
          "speaker_id": "2",
          "time": "20210826 14:48"
        },
        {
          "id": "MDRW2100016356.1.17",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "1",
          "time": "20210826 14:50"
        },
        {
          "id": "MDRW2100016356.1.18",
          "form": "네 간단히 계란볶음밥 먹었어요",
          "original_form": "네 간단히 계란볶음밥 먹었어요",
          "speaker_id": "2",
          "time": "20210826 14:52"
        },
        {
          "id": "MDRW2100016356.1.19",
          "form": "맛있겠네용",
          "original_form": "맛있겠네용",
          "speaker_id": "1",
          "time": "20210826 14:55"
        },
        {
          "id": "MDRW2100016356.1.20",
          "form": "마라탕을 자주 드시는 편인가요?",
          "original_form": "마라탕을 자주 드시는 편인가요?",
          "speaker_id": "1",
          "time": "20210826 14:55"
        },
        {
          "id": "MDRW2100016356.1.21",
          "form": "아니요 취향이 아니예요",
          "original_form": "아니요 취향이 아니예요",
          "speaker_id": "2",
          "time": "20210826 14:57"
        },
        {
          "id": "MDRW2100016356.1.22",
          "form": "마라가 향이랑 맛이 독특해서 호불호가 갈리는 것같아요",
          "original_form": "마라가 향이랑 맛이 독특해서 호불호가 갈리는 것같아요",
          "speaker_id": "1",
