      "utterance": [
        {
          "id": "MDRW2100029084.1.1",
          "form": "하루가 금방이네요",
          "original_form": "하루가 금방이네요",
          "speaker_id": "2",
          "time": "20210906 21:03"
        },
        {
          "id": "MDRW2100029084.1.2",
          "form": "육퇴하셨나요?",
          "original_form": "육퇴하셨나요?",
          "speaker_id": "2",
          "time": "20210906 21:03"
        },
        {
          "id": "MDRW2100029084.1.3",
          "form": "네 육퇴요",
          "original_form": "네 육퇴요",
          "speaker_id": "1",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100029084.1.4",
          "form": "축하드립니다.\n저도 이른 육퇴네요ㅋ",
          "original_form": "축하드립니다.\n저도 이른 육퇴네요ㅋ",
          "speaker_id": "2",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100029084.1.5",
          "form": "원래 늦게까지 안자는데\n오늘 계탔습니다.",
          "original_form": "원래 늦게까지 안자는데\n오늘 계탔습니다.",
          "speaker_id": "2",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100029084.1.6",
          "form": "오~~신나는 육아^^ 끝~~",
          "original_form": "오~~신나는 육아^^ 끝~~",
          "speaker_id": "1",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100029084.1.7",
          "form": "하하 하루가 금방인데 너무길게느껴지기도..\n연애와 결혼인데\n육아의 대화군요ㅋ",
          "original_form": "하하 하루가 금방인데 너무길게느껴지기도..\n연애와 결혼인데\n육아의 대화군요ㅋ",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100029084.1.8",
          "form": "아이들이 있으면,\n일하고 퇴근후\n그냥 애들 보다가 하루가 가는..",
          "original_form": "아이들이 있으면,\n일하고 퇴근후\n그냥 애들 보다가 하루가 가는..",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100029084.1.9",
          "form": "결혼하니 힘들지만 행복",
          "original_form": "결혼하니 힘들지만 행복",
          "speaker_id": "1",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100029084.1.10",
          "form": "맞아요 근데 더 싸우긴하네요ㅋ",
          "original_form": "맞아요 근데 더 싸우긴하네요ㅋ",
          "speaker_id": "2",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100029084.1.11",
          "form": "저도 맨날싸우는듯 휴 내맘같지않으니",
          "original_form": "저도 맨날싸우는듯 휴 내맘같지않으니",
          "speaker_id": "1",
          "time": "20210906 21:35"
        },
        {
          "id": "MDRW2100029084.1.12",
          "form": "육아로 싸우니 더 답답합니다",
          "original_form": "육아로 싸우니 더 답답합니다",
          "speaker_id": "2",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100029084.1.13",
          "form": "서로 불만이 있겠지만..",
          "original_form": "서로 불만이 있겠지만..",
          "speaker_id": "2",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100029084.1.14",
          "form": "맞춰가면 좋을텐데..\n일방적으로 짜증내니..\n참고 참다..터질지도..ㅋ",
          "original_form": "맞춰가면 좋을텐데..\n일방적으로 짜증내니..\n참고 참다..터질지도..ㅋ",
          "speaker_id": "2",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100029084.1.15",
          "form": "ㅋ 저희도 육아로 싸우죠",
          "original_form": "ㅋ 저희도 육아로 싸우죠",
          "speaker_id": "1",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100029084.1.16",
          "form": "아이가 어리시죠?",
          "original_form": "아이가 어리시죠?",
          "speaker_id": "2",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100029084.1.17",
          "form": "크면 더싸웁니다",
          "original_form": "크면 더싸웁니다",
          "speaker_id": "2",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100029084.1.18",
          "form": "학업문제로ㅋ",
          "original_form": "학업문제로ㅋ",
          "speaker_id": "2",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100029084.1.19",
          "form": "ㅋ 아 학업 그건 그냥 둬야죠",
          "original_form": "ㅋ 아 학업 그건 그냥 둬야죠",
          "speaker_id": "1",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100029084.1.20",
          "form": "놔두라니 집착을..",
          "original_form": "놔두라니 집착을..",
          "speaker_id": "2",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100029084.1.21",
          "form": "괜히 화풀이하는걸로보여요ㅋ",
          "original_form": "괜히 화풀이하는걸로보여요ㅋ",
          "speaker_id": "2",
          "time": "20210906 21:54"
        }
