      "utterance": [
        {
          "id": "MDRW2100038855.1.1",
          "form": "보리숭어는 첨 들어봐용!!",
          "original_form": "보리숭어는 첨 들어봐용!!",
          "speaker_id": "1",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100038855.1.2",
          "form": "붉은혈압육이 있는 생선이예요",
          "original_form": "붉은혈압육이 있는 생선이예요",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100038855.1.3",
          "form": "씹는맛이 있어요",
          "original_form": "씹는맛이 있어요",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100038855.1.4",
          "form": "우아 뭔가 되게 비싸보여요 ㅠㅠ",
          "original_form": "우아 뭔가 되게 비싸보여요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100038855.1.5",
          "form": "싸요",
          "original_form": "싸요",
          "speaker_id": "2",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100038855.1.6",
          "form": "ㅋㅋㄱㅋㅋㅋㄱㅋㅋ",
          "original_form": "ㅋㅋㄱㅋㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100038855.1.7",
          "form": "가성비좋아서 좋아해요...",
          "original_form": "가성비좋아서 좋아해요...",
          "speaker_id": "2",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100038855.1.8",
          "form": "광어와 더불어 가성비생선이예요",
          "original_form": "광어와 더불어 가성비생선이예요",
          "speaker_id": "2",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100038855.1.9",
          "form": "오오 저는 무조건 연어를 먹어요!! ㅋㅋㅋ",
          "original_form": "오오 저는 무조건 연어를 먹어요!! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100038855.1.10",
          "form": "우와 광어 ㅠㅠ",
          "original_form": "우와 광어 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100038855.1.11",
          "form": "무조건 연어파시군요!",
          "original_form": "무조건 연어파시군요!",
          "speaker_id": "2",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100038855.1.12",
          "form": "연어초밥 연어회 연어덮밥 연어장 가리지않으시나요?",
          "original_form": "연어초밥 연어회 연어덮밥 연어장 가리지않으시나요?",
          "speaker_id": "2",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100038855.1.13",
          "form": "오오 너무 좋죠 ㅠㅠ 다 맛있어요 ㅠㅜ",
          "original_form": "오오 너무 좋죠 ㅠㅠ 다 맛있어요 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100038855.1.14",
          "form": "연어장 진짜 밥도둑이에요",
          "original_form": "연어장 진짜 밥도둑이에요",
          "speaker_id": "1",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100038855.1.15",
          "form": "저는 연어 사먹고 혹시 조금 남을때면",
          "original_form": "저는 연어 사먹고 혹시 조금 남을때면",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100038855.1.16",
          "form": "연어장을 해먹는데 진짜 밥한공기 뚝딱이예요",
          "original_form": "연어장을 해먹는데 진짜 밥한공기 뚝딱이예요",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100038855.1.17",
          "form": "너무 맛있죠ㅠㅠ",
          "original_form": "너무 맛있죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100038855.1.18",
          "form": "유아 ㅜㅜ 만들줄 아세용??,?",
          "original_form": "유아 ㅜㅜ 만들줄 아세용??,?",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100038855.1.19",
          "form": "대충 간장 올리고당 식초 넣구 만들어요",
          "original_form": "대충 간장 올리고당 식초 넣구 만들어요",
          "speaker_id": "2",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100038855.1.20",
          "form": "우아 그래도 비율이 중요하지않아요?? 저는 요리 완전 못해서 ㅋㅋ",
          "original_form": "우아 그래도 비율이 중요하지않아요?? 저는 요리 완전 못해서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:20"
        },
        {
          "id": "MDRW2100038855.1.21",
          "form": "중간중간 맛봐가면서 달짝지근하게 만들어요",
          "original_form": "중간중간 맛봐가면서 달짝지근하게 만들어요",
          "speaker_id": "2",
          "time": "20210902 22:20"
        },
        {
          "id": "MDRW2100038855.1.22",
          "form": "사먹는게 확실히 맛있죠!",
          "original_form": "사먹는게 확실히 맛있죠!",
          "speaker_id": "2",
          "time": "20210902 22:21"
        },
        {
          "id": "MDRW2100038855.1.23",
          "form": "간장게장도 너무 맛있어요 ㅠㅠ",
          "original_form": "간장게장도 너무 맛있어요 ㅠㅠ",
