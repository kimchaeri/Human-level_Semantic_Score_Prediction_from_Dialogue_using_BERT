      "utterance": [
        {
          "id": "MDRW2100031393.1.1",
          "form": "귀여운거는 사는 편이에요 ㅋㅋ",
          "original_form": "귀여운거는 사는 편이에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:17"
        },
        {
          "id": "MDRW2100031393.1.2",
          "form": "저도요 귀여운 캐릭터에 약해요",
          "original_form": "저도요 귀여운 캐릭터에 약해요",
          "speaker_id": "1",
          "time": "20210905 09:26"
        },
        {
          "id": "MDRW2100031393.1.3",
          "form": "ㅎㅎ 어떤거 사보셨나요",
          "original_form": "ㅎㅎ 어떤거 사보셨나요",
          "speaker_id": "2",
          "time": "20210905 13:21"
        },
        {
          "id": "MDRW2100031393.1.4",
          "form": "쿠션&담요 잡지 틴케이스 에그볼 인형 향초 그 외에도 많아요",
          "original_form": "쿠션&담요 잡지 틴케이스 에그볼 인형 향초 그 외에도 많아요",
          "speaker_id": "1",
          "time": "20210905 15:21"
        },
        {
          "id": "MDRW2100031393.1.5",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210905 15:21"
        },
        {
          "id": "MDRW2100031393.1.6",
          "form": "저는 주로 컵 종류? 를 많이 사는거 같네요",
          "original_form": "저는 주로 컵 종류? 를 많이 사는거 같네요",
          "speaker_id": "2",
          "time": "20210905 15:55"
        },
        {
          "id": "MDRW2100031393.1.7",
          "form": "캐릭터나 브랜드 쪽인가요?",
          "original_form": "캐릭터나 브랜드 쪽인가요?",
          "speaker_id": "1",
          "time": "20210905 15:59"
        },
        {
          "id": "MDRW2100031393.1.8",
          "form": "네 카카오 같은거요!",
          "original_form": "네 카카오 같은거요!",
          "speaker_id": "2",
          "time": "20210905 16:00"
        },
        {
          "id": "MDRW2100031393.1.9",
          "form": "카카오 누구 좋아하세요? 전 라이언이요",
          "original_form": "카카오 누구 좋아하세요? 전 라이언이요",
          "speaker_id": "1",
          "time": "20210905 16:01"
        },
        {
          "id": "MDRW2100031393.1.10",
          "form": "저는 라이언이랑 어피치 작은애요 ㅋㅋ",
          "original_form": "저는 라이언이랑 어피치 작은애요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 16:33"
        },
        {
          "id": "MDRW2100031393.1.11",
          "form": "리틀 어피치요?",
          "original_form": "리틀 어피치요?",
          "speaker_id": "1",
          "time": "20210905 18:13"
        },
        {
          "id": "MDRW2100031393.1.12",
          "form": "네 ㅋㅋ 귀엽게 생겼더라고요 ㅎㅎ",
          "original_form": "네 ㅋㅋ 귀엽게 생겼더라고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 18:38"
        },
        {
          "id": "MDRW2100031393.1.13",
          "form": "맞아요 리틀이 더 귀여워요 라이언은 기본이 더 귀엽고요",
          "original_form": "맞아요 리틀이 더 귀여워요 라이언은 기본이 더 귀엽고요",
          "speaker_id": "1",
          "time": "20210905 19:13"
        },
        {
          "id": "MDRW2100031393.1.14",
          "form": "맞아요 라이언은 클수록 더 귀여운거 같아요 인형도 ㅋㅋ",
          "original_form": "맞아요 라이언은 클수록 더 귀여운거 같아요 인형도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:14"
        },
        {
          "id": "MDRW2100031393.1.15",
          "form": "크니까 왕 귀여워요 ㅋㅋ 카카오는 콜라보 자주해서 구매하기 참 좋아요",
          "original_form": "크니까 왕 귀여워요 ㅋㅋ 카카오는 콜라보 자주해서 구매하기 참 좋아요",
          "speaker_id": "1",
          "time": "20210905 19:15"
        },
        {
          "id": "MDRW2100031393.1.16",
          "form": "그러게요 그 브랜드에 맞게끔 특색 살려서 잘 나오는거 같아요",
          "original_form": "그러게요 그 브랜드에 맞게끔 특색 살려서 잘 나오는거 같아요",
          "speaker_id": "2",
          "time": "20210905 19:17"
        }
      ]
