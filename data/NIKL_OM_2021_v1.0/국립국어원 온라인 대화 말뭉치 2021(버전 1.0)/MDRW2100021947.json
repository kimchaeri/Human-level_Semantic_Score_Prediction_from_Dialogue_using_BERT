      "utterance": [
        {
          "id": "MDRW2100021947.1.1",
          "form": "저 냉면 진짜 좋아하긴 하는데ㅋㅋ 비빔냉면 좋아해요",
          "original_form": "저 냉면 진짜 좋아하긴 하는데ㅋㅋ 비빔냉면 좋아해요",
          "speaker_id": "1",
          "time": "20210829 00:03"
        },
        {
          "id": "MDRW2100021947.1.2",
          "form": "아 비냉파시구나!",
          "original_form": "아 비냉파시구나!",
          "speaker_id": "2",
          "time": "20210829 00:03"
        },
        {
          "id": "MDRW2100021947.1.3",
          "form": "넵 각종 비빔면 파!",
          "original_form": "넵 각종 비빔면 파!",
          "speaker_id": "1",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100021947.1.4",
          "form": "혹시 그럼",
          "original_form": "혹시 그럼",
          "speaker_id": "2",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100021947.1.5",
          "form": "틈새볶음면",
          "original_form": "틈새볶음면",
          "speaker_id": "2",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100021947.1.6",
          "form": "드셔보셨어요?",
          "original_form": "드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100021947.1.7",
          "form": "아 그거 먹어봤어요",
          "original_form": "아 그거 먹어봤어요",
          "speaker_id": "1",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100021947.1.8",
          "form": "jmt...",
          "original_form": "jmt...",
          "speaker_id": "2",
          "time": "20210829 00:05"
        },
        {
          "id": "MDRW2100021947.1.9",
          "form": "그거 매콤 짭짤하니 맛나더라고요",
          "original_form": "그거 매콤 짭짤하니 맛나더라고요",
          "speaker_id": "1",
          "time": "20210829 00:05"
        },
        {
          "id": "MDRW2100021947.1.10",
          "form": "근데 왠지 몇 번 먹고 다시 불닭으로 돌아왔어요",
          "original_form": "근데 왠지 몇 번 먹고 다시 불닭으로 돌아왔어요",
          "speaker_id": "1",
          "time": "20210829 00:05"
        },
        {
          "id": "MDRW2100021947.1.11",
          "form": "불닭을 더 좋아하시는구나!",
          "original_form": "불닭을 더 좋아하시는구나!",
          "speaker_id": "2",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100021947.1.12",
          "form": "넵 저는 불닭 파!",
          "original_form": "넵 저는 불닭 파!",
          "speaker_id": "1",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100021947.1.13",
          "form": "저는 불닭 자체보다는 짜장라면이랑 섞는게 좋더라구요",
          "original_form": "저는 불닭 자체보다는 짜장라면이랑 섞는게 좋더라구요",
          "speaker_id": "2",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100021947.1.14",
          "form": "아 그것도 진짜 맛있죠",
          "original_form": "아 그것도 진짜 맛있죠",
          "speaker_id": "1",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100021947.1.15",
          "form": "불파게티!!",
          "original_form": "불파게티!!",
          "speaker_id": "1",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100021947.1.16",
          "form": "짱맛꿀맛!!!",
          "original_form": "짱맛꿀맛!!!",
          "speaker_id": "2",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100021947.1.17",
          "form": "매운거 못먹는데 너무 좋아해요",
          "original_form": "매운거 못먹는데 너무 좋아해요",
          "speaker_id": "2",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100021947.1.18",
          "form": "마침 딱 볶으면 하나 먹기 좋은 시간이네요",
          "original_form": "마침 딱 볶으면 하나 먹기 좋은 시간이네요",
          "speaker_id": "1",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100021947.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100021947.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021947.1.21",
          "form": "하 침나오네요",
          "original_form": "하 침나오네요",
          "speaker_id": "2",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021947.1.22",
          "form": "그거 아세요 볶음면의 딜레마라고",
          "original_form": "그거 아세요 볶음면의 딜레마라고",
          "speaker_id": "1",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100021947.1.23",
          "form": "엗 그게머죠",
          "original_form": "엗 그게머죠",
          "speaker_id": "2",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100021947.1.24",
          "form": "한 개 끓이면 0.7인분 같고",
          "original_form": "한 개 끓이면 0.7인분 같고",
          "speaker_id": "1",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100021947.1.25",
          "form": "두개 끓이면 또 확 불어나는ㅋㅋㅋㅋ",
          "original_form": "두개 끓이면 또 확 불어나는ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100021947.1.26",
          "form": "팔도네요",
          "original_form": "팔도네요",
          "speaker_id": "2",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100021947.1.27",
          "form": "팔도비빔면 정말..",
          "original_form": "팔도비빔면 정말..",
          "speaker_id": "2",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100021947.1.28",
          "form": "애매해요",
          "original_form": "애매해요",
          "speaker_id": "2",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100021947.1.29",
          "form": "맞아요 진짜 둘이서 세개 먹으면 딱 맞는데",
          "original_form": "맞아요 진짜 둘이서 세개 먹으면 딱 맞는데",
          "speaker_id": "1",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021947.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021947.1.31",
          "form": "둘이서 세개 셋이서 네개..  이게 딱 맞는 것 같아요",
          "original_form": "둘이서 세개 셋이서 네개..  이게 딱 맞는 것 같아요",
          "speaker_id": "1",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021947.1.32",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "2",
          "time": "20210829 00:13"
        },
        {
          "id": "MDRW2100021947.1.33",
          "form": "비빔칼국수도 맛있어요",
          "original_form": "비빔칼국수도 맛있어요",
          "speaker_id": "2",
          "time": "20210829 00:13"
        },
        {
          "id": "MDRW2100021947.1.34",
          "form": "어 비빔 칼국수는 저 안 먹어 봤어요",
          "original_form": "어 비빔 칼국수는 저 안 먹어 봤어요",
          "speaker_id": "1",
          "time": "20210829 00:13"
        },
        {
          "id": "MDRW2100021947.1.35",
          "form": "라면 종류예요?",
          "original_form": "라면 종류예요?",
          "speaker_id": "1",
          "time": "20210829 00:14"
        },
        {
          "id": "MDRW2100021947.1.36",
          "form": "아녀 가끔 식당들에서 팔아요!!",
          "original_form": "아녀 가끔 식당들에서 팔아요!!",
          "speaker_id": "2",
          "time": "20210829 00:14"
        },
        {
          "id": "MDRW2100021947.1.37",
          "form": "식당을 알려주세요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "식당을 알려주세요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100021947.1.38",
          "form": "비빔칼국수 식당 알려주세요!",
          "original_form": "비빔칼국수 식당 알려주세요!",
