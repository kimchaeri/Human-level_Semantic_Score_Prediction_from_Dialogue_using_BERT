        },
        {
          "id": "MDRW2100020222.1.5",
          "form": "갤럭시 9플러스여ㅎㅎ",
          "original_form": "갤럭시 9플러스여ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:27"
        },
        {
          "id": "MDRW2100020222.1.6",
          "form": "이제 바꺼야해여ㅎㅎ",
          "original_form": "이제 바꺼야해여ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:27"
        },
        {
          "id": "MDRW2100020222.1.7",
          "form": "보내줘야함ㅠ 용량도 다차고 느려요ㅠ",
          "original_form": "보내줘야함ㅠ 용량도 다차고 느려요ㅠ",
          "speaker_id": "2",
          "time": "20210828 00:27"
        },
        {
          "id": "MDRW2100020222.1.8",
          "form": "z플립 가나요",
          "original_form": "z플립 가나요",
          "speaker_id": "1",
          "time": "20210828 00:27"
        },
        {
          "id": "MDRW2100020222.1.9",
          "form": "둘중에 고민중이라 아마 사전예약했는데",
          "original_form": "둘중에 고민중이라 아마 사전예약했는데",
          "speaker_id": "2",
          "time": "20210828 00:27"
        },
        {
          "id": "MDRW2100020222.1.10",
          "form": "다 놓칠것같애요ㅎ",
          "original_form": "다 놓칠것같애요ㅎ",
          "speaker_id": "2",
          "time": "20210828 00:27"
        },
        {
          "id": "MDRW2100020222.1.11",
          "form": "사전예약이 오히려 이득같아요",
          "original_form": "사전예약이 오히려 이득같아요",
          "speaker_id": "1",
          "time": "20210828 00:28"
        },
        {
          "id": "MDRW2100020222.1.12",
          "form": "주는게 많고 혜택이",
          "original_form": "주는게 많고 혜택이",
          "speaker_id": "1",
          "time": "20210828 00:28"
        },
        {
          "id": "MDRW2100020222.1.13",
          "form": "전 노트 20 울트라 쓰는데",
          "original_form": "전 노트 20 울트라 쓰는데",
          "speaker_id": "1",
          "time": "20210828 00:28"
        },
        {
          "id": "MDRW2100020222.1.14",
          "form": "저에비하면ㅎㅎ최신폰이네요ㅎㅎ",
          "original_form": "저에비하면ㅎㅎ최신폰이네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:29"
        },
        {
          "id": "MDRW2100020222.1.15",
          "form": "사전예약때 버즈라이브랑  살균되는 충전기랑",
          "original_form": "사전예약때 버즈라이브랑  살균되는 충전기랑",
          "speaker_id": "1",
          "time": "20210828 00:29"
        },
        {
          "id": "MDRW2100020222.1.16",
          "form": "사전예약 놓칠듯요ㅠ",
          "original_form": "사전예약 놓칠듯요ㅠ",
          "speaker_id": "2",
          "time": "20210828 00:29"
        },
        {
          "id": "MDRW2100020222.1.17",
          "form": "우잉 왜요",
          "original_form": "우잉 왜요",
          "speaker_id": "1",
          "time": "20210828 00:29"
        },
        {
          "id": "MDRW2100020222.1.18",
          "form": "아직 하는거 아니에요?",
          "original_form": "아직 하는거 아니에요?",
          "speaker_id": "1",
          "time": "20210828 00:29"
        },
        {
          "id": "MDRW2100020222.1.19",
