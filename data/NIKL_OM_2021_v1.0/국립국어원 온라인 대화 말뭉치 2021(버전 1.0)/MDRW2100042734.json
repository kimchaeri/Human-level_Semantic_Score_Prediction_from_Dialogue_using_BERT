      "utterance": [
        {
          "id": "MDRW2100042734.1.1",
          "form": "오늘날씨 어때",
          "original_form": "오늘날씨 어때",
          "speaker_id": "1",
          "time": "20210907 08:04"
        },
        {
          "id": "MDRW2100042734.1.2",
          "form": "오늘 비가 조금씩 오네 어제보단 좀 적게 오는 것 같아!",
          "original_form": "오늘 비가 조금씩 오네 어제보단 좀 적게 오는 것 같아!",
          "speaker_id": "2",
          "time": "20210907 08:05"
        },
        {
          "id": "MDRW2100042734.1.3",
          "form": "아 우리는 흐리고 비는 안와",
          "original_form": "아 우리는 흐리고 비는 안와",
          "speaker_id": "1",
          "time": "20210907 08:05"
        },
        {
          "id": "MDRW2100042734.1.4",
          "form": "며칠내내 너무 흐리기만 하다ㅠㅠ",
          "original_form": "며칠내내 너무 흐리기만 하다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 08:05"
        },
        {
          "id": "MDRW2100042734.1.5",
          "form": "와 좋겠다 그래도 우산 안들어서",
          "original_form": "와 좋겠다 그래도 우산 안들어서",
          "speaker_id": "2",
          "time": "20210907 08:06"
        },
        {
          "id": "MDRW2100042734.1.6",
          "form": "맞아 구름보기가 쉽지 않네 ㅠㅠ",
          "original_form": "맞아 구름보기가 쉽지 않네 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 08:06"
        },
        {
          "id": "MDRW2100042734.1.7",
          "form": "덥진 않아서 좋은데 바람불면 너무 추워",
          "original_form": "덥진 않아서 좋은데 바람불면 너무 추워",
          "speaker_id": "2",
          "time": "20210907 08:06"
        },
        {
          "id": "MDRW2100042734.1.8",
          "form": "아 맞아 바람 진짜 세게 불어",
          "original_form": "아 맞아 바람 진짜 세게 불어",
          "speaker_id": "1",
          "time": "20210907 08:06"
        },
        {
          "id": "MDRW2100042734.1.9",
          "form": "어제 날아갈뻔 했어",
          "original_form": "어제 날아갈뻔 했어",
          "speaker_id": "1",
          "time": "20210907 08:06"
        },
        {
          "id": "MDRW2100042734.1.10",
          "form": "에고 출퇴근할때 많이 힘들었겠다 ㅠㅠ",
          "original_form": "에고 출퇴근할때 많이 힘들었겠다 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 08:08"
        },
        {
          "id": "MDRW2100042734.1.11",
          "form": "출퇴근은 기왕이면 날씨가 맑았으면 좋겠는데 말이야 ㅠㅠ",
          "original_form": "출퇴근은 기왕이면 날씨가 맑았으면 좋겠는데 말이야 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 08:08"
        },
        {
          "id": "MDRW2100042734.1.12",
          "form": "맞아 비오는날 최악이야 ㅋㅋ",
          "original_form": "맞아 비오는날 최악이야 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:12"
        },
        {
          "id": "MDRW2100042734.1.13",
          "form": "맞아 난 그래서 비오는 날에는 샌들을 신어",
          "original_form": "맞아 난 그래서 비오는 날에는 샌들을 신어",
          "speaker_id": "2",
          "time": "20210907 10:53"
        },
        {
          "id": "MDRW2100042734.1.14",
          "form": "신발 젖는게 너무 싫더라구 ㅠㅠ",
          "original_form": "신발 젖는게 너무 싫더라구 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 10:53"
        },
        {
          "id": "MDRW2100042734.1.15",
          "form": "나도 장화는 안사?",
          "original_form": "나도 장화는 안사?",
          "speaker_id": "1",
          "time": "20210907 12:08"
        },
        {
          "id": "MDRW2100042734.1.16",
          "form": "엉 장화는 둘곳도 없기도 하고 샌들로도 괜찮을 것 같아서 그냥 샌들 신고있어",
          "original_form": "엉 장화는 둘곳도 없기도 하고 샌들로도 괜찮을 것 같아서 그냥 샌들 신고있어",
          "speaker_id": "2",
          "time": "20210907 15:03"
        },
        {
          "id": "MDRW2100042734.1.17",
          "form": "겨울에는 발 추우니까 비올때 워커같은거신어",
          "original_form": "겨울에는 발 추우니까 비올때 워커같은거신어",
          "speaker_id": "2",
          "time": "20210907 15:04"
        },
        {
          "id": "MDRW2100042734.1.18",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210907 15:05"
        },
        {
          "id": "MDRW2100042734.1.19",
          "form": "난 작년에 롱부츠 사고는 신지도못했어",
          "original_form": "난 작년에 롱부츠 사고는 신지도못했어",
          "speaker_id": "1",
          "time": "20210907 15:05"
        },
        {
          "id": "MDRW2100042734.1.20",
          "form": "더위와 추위사이에서 고민하다가 ㅋㅋㅋㅋ",
          "original_form": "더위와 추위사이에서 고민하다가 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:05"
        },
        {
          "id": "MDRW2100042734.1.21",
          "form": "시즌 놓침",
          "original_form": "시즌 놓침",
          "speaker_id": "1",
          "time": "20210907 15:05"
        },
        {
          "id": "MDRW2100042734.1.22",
          "form": "아 그래도 올해 열심히 신으면 되지~",
          "original_form": "아 그래도 올해 열심히 신으면 되지~",
          "speaker_id": "2",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100042734.1.23",
          "form": "맞아 망설이다보면 어느새 따뜻해져있고 그러더라 ㅠㅠ",
          "original_form": "맞아 망설이다보면 어느새 따뜻해져있고 그러더라 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100042734.1.24",
          "form": "맞아 올해는 시즌 안놓치고 신을거야",
          "original_form": "맞아 올해는 시즌 안놓치고 신을거야",
          "speaker_id": "1",
          "time": "20210907 15:14"
        }
      ]
    }
