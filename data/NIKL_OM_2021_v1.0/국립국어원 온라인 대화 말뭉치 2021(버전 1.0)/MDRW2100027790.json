      "utterance": [
        {
          "id": "MDRW2100027790.1.1",
          "form": "책 뭐읽으시나요?",
          "original_form": "책 뭐읽으시나요?",
          "speaker_id": "1",
          "time": "20210902 15:09"
        },
        {
          "id": "MDRW2100027790.1.2",
          "form": "지금은없어요 육아서적 구매할까생각만이요",
          "original_form": "지금은없어요 육아서적 구매할까생각만이요",
          "speaker_id": "2",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027790.1.3",
          "form": "뭐보세요",
          "original_form": "뭐보세요",
          "speaker_id": "2",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027790.1.4",
          "form": "저도요 아니면 요리책 ㅜ뭐해먹지고민하는데 책 들춰봐요",
          "original_form": "저도요 아니면 요리책 ㅜ뭐해먹지고민하는데 책 들춰봐요",
          "speaker_id": "1",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027790.1.5",
          "form": "듣는 독서로 책 읽다가 요즘은 끊었어요 윌라이런거",
          "original_form": "듣는 독서로 책 읽다가 요즘은 끊었어요 윌라이런거",
          "speaker_id": "1",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027790.1.6",
          "form": "네 요리책은 이상하게 안보게되더라구요 사기만하구",
          "original_form": "네 요리책은 이상하게 안보게되더라구요 사기만하구",
          "speaker_id": "2",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027790.1.7",
          "form": "온라인책보는거요?",
          "original_form": "온라인책보는거요?",
          "speaker_id": "2",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027790.1.8",
          "form": "전 집앞에 스마트독서박스가 있어서 도서관은 먼데 거기서 가끔 빌려와요",
          "original_form": "전 집앞에 스마트독서박스가 있어서 도서관은 먼데 거기서 가끔 빌려와요",
          "speaker_id": "1",
          "time": "20210902 15:13"
        },
        {
          "id": "MDRW2100027790.1.9",
          "form": "빌려보는것도 좋죠",
          "original_form": "빌려보는것도 좋죠",
          "speaker_id": "2",
          "time": "20210902 15:13"
        },
        {
          "id": "MDRW2100027790.1.10",
          "form": "재밋는책 추천좀부탁드려요",
          "original_form": "재밋는책 추천좀부탁드려요",
          "speaker_id": "2",
          "time": "20210902 15:14"
        },
        {
          "id": "MDRW2100027790.1.11",
          "form": "이게사과일까인가 그거 일본작가 그림책 신박해요",
          "original_form": "이게사과일까인가 그거 일본작가 그림책 신박해요",
          "speaker_id": "1",
          "time": "20210902 15:14"
        },
        {
          "id": "MDRW2100027790.1.12",
          "form": "아네 도서실가몀 한번볼께요",
          "original_form": "아네 도서실가몀 한번볼께요",
          "speaker_id": "2",
          "time": "20210902 15:16"
        },
        {
          "id": "MDRW2100027790.1.13",
          "form": "육아서는 뭐보세요?",
          "original_form": "육아서는 뭐보세요?",
          "speaker_id": "1",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100027790.1.14",
          "form": "찾아보고있어요 지금11살아이키우고있어서요",
          "original_form": "찾아보고있어요 지금11살아이키우고있어서요",
          "speaker_id": "2",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100027790.1.15",
          "form": "아하 육아서적 공부법이런거 많이 책 보시겠네요",
          "original_form": "아하 육아서적 공부법이런거 많이 책 보시겠네요",
          "speaker_id": "1",
          "time": "20210902 15:20"
        },
        {
          "id": "MDRW2100027790.1.16",
          "form": "네 키우는노하우나..그런거에관해서요 ㅎ",
          "original_form": "네 키우는노하우나..그런거에관해서요 ㅎ",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027790.1.17",
          "form": "전 해리포터시리즈 아직도 가끔 읽는데 넘 재밌네요 어릴때 읽던건데요",
          "original_form": "전 해리포터시리즈 아직도 가끔 읽는데 넘 재밌네요 어릴때 읽던건데요",
          "speaker_id": "1",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027790.1.18",
          "form": "베스트셀러죠",
          "original_form": "베스트셀러죠",
          "speaker_id": "1",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027790.1.19",
          "form": "맞아요 ㅎ 저도 아이사줄려고요 아직주문을 못했네요",
          "original_form": "맞아요 ㅎ 저도 아이사줄려고요 아직주문을 못했네요",
          "speaker_id": "2",
          "time": "20210902 15:23"
        },
        {
          "id": "MDRW2100027790.1.20",
          "form": "요즘영화로 해리포터 잘보더라구여",
          "original_form": "요즘영화로 해리포터 잘보더라구여",
          "speaker_id": "2",
          "time": "20210902 15:23"
        },
        {
          "id": "MDRW2100027790.1.21",
          "form": "11살이면 잘보겠어요",
          "original_form": "11살이면 잘보겠어요",
          "speaker_id": "1",
          "time": "20210902 15:23"
        },
        {
          "id": "MDRW2100027790.1.22",
          "form": "기억의밤 그 소설도 넘잼나여",
          "original_form": "기억의밤 그 소설도 넘잼나여",
          "speaker_id": "1",
          "time": "20210902 15:24"
        }
      ]
    }
