      "utterance": [
        {
          "id": "MDRW2100047528.1.1",
          "form": "그체 나갔다 왔는데 덥드라",
          "original_form": "그체 나갔다 왔는데 덥드라",
          "speaker_id": "2",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100047528.1.2",
          "form": "또 더워지니 더 더운거 같다",
          "original_form": "또 더워지니 더 더운거 같다",
          "speaker_id": "1",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100047528.1.3",
          "form": "어제는 그리 비가 오더만은",
          "original_form": "어제는 그리 비가 오더만은",
          "speaker_id": "2",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100047528.1.4",
          "form": "낼 또 다른 장마온다는데 덥네",
          "original_form": "낼 또 다른 장마온다는데 덥네",
          "speaker_id": "1",
          "time": "20210826 19:23"
        },
        {
          "id": "MDRW2100047528.1.5",
          "form": "예보는 주말에 온다더만 내일 온대?",
          "original_form": "예보는 주말에 온다더만 내일 온대?",
          "speaker_id": "2",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100047528.1.6",
          "form": "응 아까 날씨예보 봤는데 낼 다시 비온다고 하던데",
          "original_form": "응 아까 날씨예보 봤는데 낼 다시 비온다고 하던데",
          "speaker_id": "1",
          "time": "20210826 19:24"
        },
        {
          "id": "MDRW2100047528.1.7",
          "form": "차라리 비 많이 오면 사람 작게 오니까 더 좋다ㅋ",
          "original_form": "차라리 비 많이 오면 사람 작게 오니까 더 좋다ㅋ",
          "speaker_id": "2",
          "time": "20210826 19:25"
        },
        {
          "id": "MDRW2100047528.1.8",
          "form": "날씨예보는 실시간으로 봐야한다 ㅋㅋ",
          "original_form": "날씨예보는 실시간으로 봐야한다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:25"
        },
        {
          "id": "MDRW2100047528.1.9",
          "form": "그래 전에도 맑음에서 순식간에 비로 바낌",
          "original_form": "그래 전에도 맑음에서 순식간에 비로 바낌",
          "speaker_id": "2",
          "time": "20210826 19:26"
        },
        {
          "id": "MDRW2100047528.1.10",
          "form": "나도 일할때 비오면 그렇게 좋더라 ㅋㅋ",
          "original_form": "나도 일할때 비오면 그렇게 좋더라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:26"
        },
        {
          "id": "MDRW2100047528.1.11",
          "form": "맞제..  사람이 좀 적게 오자나ㅋ",
          "original_form": "맞제..  사람이 좀 적게 오자나ㅋ",
          "speaker_id": "2",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100047528.1.12",
          "form": "엉 하루 몇명 안와서 날씨가 돕는다",
          "original_form": "엉 하루 몇명 안와서 날씨가 돕는다",
          "speaker_id": "1",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100047528.1.13",
          "form": "출퇴근 길에는 비가 안오고 일할때만 오면 참 좋은데",
          "original_form": "출퇴근 길에는 비가 안오고 일할때만 오면 참 좋은데",
          "speaker_id": "1",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100047528.1.14",
          "form": "그러게 내일도 차라리 비 많이 왔음 좋겠다",
          "original_form": "그러게 내일도 차라리 비 많이 왔음 좋겠다",
          "speaker_id": "2",
          "time": "20210826 19:28"
        }
