      "utterance": [
        {
          "id": "MDRW2100032989.1.1",
          "form": "이미 다양하게 먹구 왔어요",
          "original_form": "이미 다양하게 먹구 왔어요",
          "speaker_id": "2",
          "time": "20210906 12:11"
        },
        {
          "id": "MDRW2100032989.1.2",
          "form": "매일매일이 파티 해요",
          "original_form": "매일매일이 파티 해요",
          "speaker_id": "1",
          "time": "20210906 12:11"
        },
        {
          "id": "MDRW2100032989.1.3",
          "form": "고향가면 그렇더라고요",
          "original_form": "고향가면 그렇더라고요",
          "speaker_id": "1",
          "time": "20210906 12:11"
        },
        {
          "id": "MDRW2100032989.1.4",
          "form": "ㅎㅎ 아침 생선은 어떤거 드셨어요",
          "original_form": "ㅎㅎ 아침 생선은 어떤거 드셨어요",
          "speaker_id": "2",
          "time": "20210906 12:12"
        },
        {
          "id": "MDRW2100032989.1.5",
          "form": "곧 전어철인데",
          "original_form": "곧 전어철인데",
          "speaker_id": "2",
          "time": "20210906 12:12"
        },
        {
          "id": "MDRW2100032989.1.6",
          "form": "고등어랑 고등어 비슷한 생선이요",
          "original_form": "고등어랑 고등어 비슷한 생선이요",
          "speaker_id": "1",
          "time": "20210906 12:12"
        },
        {
          "id": "MDRW2100032989.1.7",
          "form": "부모님이 아무래도 자식들 오면 신경많이 쓰시죠",
          "original_form": "부모님이 아무래도 자식들 오면 신경많이 쓰시죠",
          "speaker_id": "2",
          "time": "20210906 12:12"
        },
        {
          "id": "MDRW2100032989.1.8",
          "form": "비슷한 생선이라뇨 ㅋㅋㅋ",
          "original_form": "비슷한 생선이라뇨 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 12:12"
        },
        {
          "id": "MDRW2100032989.1.9",
          "form": "삼치인가",
          "original_form": "삼치인가",
          "speaker_id": "2",
          "time": "20210906 12:12"
        },
        {
          "id": "MDRW2100032989.1.10",
          "form": "제주도생선이라 이름들어도 모르겠어요ㅜ",
          "original_form": "제주도생선이라 이름들어도 모르겠어요ㅜ",
          "speaker_id": "1",
          "time": "20210906 12:12"
        },
        {
          "id": "MDRW2100032989.1.11",
          "form": "각재기?",
          "original_form": "각재기?",
          "speaker_id": "1",
          "time": "20210906 12:12"
        },
        {
          "id": "MDRW2100032989.1.12",
          "form": "아 ㅎㅎ",
          "original_form": "아 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 12:12"
        },
        {
          "id": "MDRW2100032989.1.13",
          "form": "저는 처음들어봐여",
          "original_form": "저는 처음들어봐여",
          "speaker_id": "1",
          "time": "20210906 12:12"
        },
        {
          "id": "MDRW2100032989.1.14",
          "form": "장대국도 맛있던데",
          "original_form": "장대국도 맛있던데",
          "speaker_id": "2",
          "time": "20210906 12:13"
        },
        {
          "id": "MDRW2100032989.1.15",
          "form": "못 알아듣는 음식 이름이 많아요",
          "original_form": "못 알아듣는 음식 이름이 많아요",
          "speaker_id": "1",
          "time": "20210906 12:13"
        },
        {
          "id": "MDRW2100032989.1.16",
          "form": "무를 놈삐라고 하시더라구요ㄷㄷ",
          "original_form": "무를 놈삐라고 하시더라구요ㄷㄷ",
          "speaker_id": "1",
          "time": "20210906 12:13"
        },
        {
          "id": "MDRW2100032989.1.17",
          "form": "놈삐국 먹자고 ㅎ",
          "original_form": "놈삐국 먹자고 ㅎ",
          "speaker_id": "1",
          "time": "20210906 12:13"
        },
        {
          "id": "MDRW2100032989.1.18",
          "form": "아 무슨 성질나서 화내시는 이름 같아요",
          "original_form": "아 무슨 성질나서 화내시는 이름 같아요",
          "speaker_id": "2",
          "time": "20210906 12:14"
        },
        {
          "id": "MDRW2100032989.1.19",
          "form": "몬딱묵으라~",
          "original_form": "몬딱묵으라~",
          "speaker_id": "1",
          "time": "20210906 12:14"
        },
        {
          "id": "MDRW2100032989.1.20",
