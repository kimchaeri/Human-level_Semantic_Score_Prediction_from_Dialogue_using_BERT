      "utterance": [
        {
          "id": "MDRW2100014127.1.1",
          "form": "그렇지 시원하고",
          "original_form": "그렇지 시원하고",
          "speaker_id": "2",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100014127.1.2",
          "form": "응 문경에 융추게곡이라고 있어",
          "original_form": "응 문경에 융추게곡이라고 있어",
          "speaker_id": "1",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014127.1.3",
          "form": "들어는 본거 같다",
          "original_form": "들어는 본거 같다",
          "speaker_id": "2",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014127.1.4",
          "form": "문경8경에 속한다네",
          "original_form": "문경8경에 속한다네",
          "speaker_id": "1",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014127.1.5",
          "form": "그럼 엄청 유명하겠다",
          "original_form": "그럼 엄청 유명하겠다",
          "speaker_id": "2",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100014127.1.6",
          "form": "용이 승천했다는 계곡이고 하는데 진짜 보면 용의 비늘 작국이 선명하데",
          "original_form": "용이 승천했다는 계곡이고 하는데 진짜 보면 용의 비늘 작국이 선명하데",
          "speaker_id": "1",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014127.1.7",
          "form": "공룡인가?",
          "original_form": "공룡인가?",
          "speaker_id": "2",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014127.1.8",
          "form": "글쎄 일단 용의비늘이고 하니 용이 맞겠지",
          "original_form": "글쎄 일단 용의비늘이고 하니 용이 맞겠지",
          "speaker_id": "1",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014127.1.9",
          "form": "응 중요하지 않다",
          "original_form": "응 중요하지 않다",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014127.1.10",
          "form": "응 이 계곡이 문경 대야신에 있어",
          "original_form": "응 이 계곡이 문경 대야신에 있어",
          "speaker_id": "1",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014127.1.11",
          "form": "대야산 알았어",
          "original_form": "대야산 알았어",
          "speaker_id": "2",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014127.1.12",
          "form": "계곡모양이 여러가지로 내려오는데 생각보다 깊고해서 튜브가 필수",
          "original_form": "계곡모양이 여러가지로 내려오는데 생각보다 깊고해서 튜브가 필수",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100014127.1.13",
          "form": "조심해야겠다",
          "original_form": "조심해야겠다",
          "speaker_id": "2",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100014127.1.14",
          "form": "한탕강이라고 알어",
          "original_form": "한탕강이라고 알어",
          "speaker_id": "1",
          "time": "20210825 17:29"
        }
