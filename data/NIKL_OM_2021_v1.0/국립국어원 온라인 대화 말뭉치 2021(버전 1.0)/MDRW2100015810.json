      "utterance": [
        {
          "id": "MDRW2100015810.1.1",
          "form": "과일또 뭐좋아하세요?",
          "original_form": "과일또 뭐좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 01:22"
        },
        {
          "id": "MDRW2100015810.1.2",
          "form": "귤도 엄청 좋아하구요",
          "original_form": "귤도 엄청 좋아하구요",
          "speaker_id": "2",
          "time": "20210826 01:23"
        },
        {
          "id": "MDRW2100015810.1.3",
          "form": "딸기나 복숭아도 굉장히 좋아해요 ㅋㅋㅋㅋ",
          "original_form": "딸기나 복숭아도 굉장히 좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:23"
        },
        {
          "id": "MDRW2100015810.1.4",
          "form": "저도 딸기 수박 복숭아ㅎ",
          "original_form": "저도 딸기 수박 복숭아ㅎ",
          "speaker_id": "1",
          "time": "20210826 01:24"
        },
        {
          "id": "MDRW2100015810.1.5",
          "form": "망고는 비싸서못먹고요",
          "original_form": "망고는 비싸서못먹고요",
          "speaker_id": "1",
          "time": "20210826 01:24"
        },
        {
          "id": "MDRW2100015810.1.6",
          "form": "체리 넘좋아하는데",
          "original_form": "체리 넘좋아하는데",
          "speaker_id": "1",
          "time": "20210826 01:24"
        },
        {
          "id": "MDRW2100015810.1.7",
          "form": "이것도 넘비싼ㅠㅠ",
          "original_form": "이것도 넘비싼ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 01:24"
        },
        {
          "id": "MDRW2100015810.1.8",
          "form": "맞아요 체리 너무 맛잇어요 ㅠㅠ",
          "original_form": "맞아요 체리 너무 맛잇어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 01:25"
        },
        {
          "id": "MDRW2100015810.1.9",
          "form": "아 메론도 좋아하는데 메론도 비쌈...",
          "original_form": "아 메론도 좋아하는데 메론도 비쌈...",
          "speaker_id": "2",
          "time": "20210826 01:25"
        },
        {
          "id": "MDRW2100015810.1.10",
          "form": "맛난건 다비싸죠 ㅎㅎ",
          "original_form": "맛난건 다비싸죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 01:26"
        },
        {
          "id": "MDRW2100015810.1.11",
          "form": "귤이 참 착한가격ㅎ",
          "original_form": "귤이 참 착한가격ㅎ",
          "speaker_id": "1",
          "time": "20210826 01:26"
        },
        {
          "id": "MDRW2100015810.1.12",
          "form": "맞아요 ㅋㅋㅋㅋ그래서 손이 노래지도록 먹습니다 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ그래서 손이 노래지도록 먹습니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:27"
        },
        {
          "id": "MDRW2100015810.1.13",
          "form": "황금향 레드향도 맛나요",
          "original_form": "황금향 레드향도 맛나요",
          "speaker_id": "1",
          "time": "20210826 01:27"
        },
        {
          "id": "MDRW2100015810.1.14",
          "form": "맞아요 레드향 너무 맛있더라구요",
          "original_form": "맞아요 레드향 너무 맛있더라구요",
          "speaker_id": "2",
          "time": "20210826 01:30"
        },
        {
          "id": "MDRW2100015810.1.15",
          "form": "별개다나오더라구요",
          "original_form": "별개다나오더라구요",
          "speaker_id": "1",
          "time": "20210826 01:30"
        },
        {
          "id": "MDRW2100015810.1.16",
          "form": "황금향은안먹어봣어요 ㅋㅋㅋ",
          "original_form": "황금향은안먹어봣어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:30"
        },
        {
          "id": "MDRW2100015810.1.17",
          "form": "다비슷비슷해요 레드향이랑",
          "original_form": "다비슷비슷해요 레드향이랑",
          "speaker_id": "1",
          "time": "20210826 01:30"
        },
        {
          "id": "MDRW2100015810.1.18",
          "form": "좀더단거같기도하고",
          "original_form": "좀더단거같기도하고",
          "speaker_id": "1",
          "time": "20210826 01:31"
        },
        {
          "id": "MDRW2100015810.1.19",
          "form": "껍질이 얇아서 신기햇던것같아요 레드향",
          "original_form": "껍질이 얇아서 신기햇던것같아요 레드향",
          "speaker_id": "2",
          "time": "20210826 01:32"
        },
        {
          "id": "MDRW2100015810.1.20",
          "form": "황금향도 ㅎ",
          "original_form": "황금향도 ㅎ",
          "speaker_id": "1",
          "time": "20210826 01:33"
        },
        {
          "id": "MDRW2100015810.1.21",
          "form": "시기가있더라구요",
          "original_form": "시기가있더라구요",
          "speaker_id": "1",
          "time": "20210826 01:33"
        },
        {
          "id": "MDRW2100015810.1.22",
          "form": "순서대로 나오던데 사실 다놓고보면 구분도 못할거같아요ㅋ",
          "original_form": "순서대로 나오던데 사실 다놓고보면 구분도 못할거같아요ㅋ",
          "speaker_id": "1",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100015810.1.23",
          "form": "뭐가 레드향이고 황금향이고",
          "original_form": "뭐가 레드향이고 황금향이고",
          "speaker_id": "1",
          "time": "20210826 01:34"
        },
        {
          "id": "MDRW2100015810.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아요 비교해가며 먹지않고 그냥주면 모를것같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아요 비교해가며 먹지않고 그냥주면 모를것같아요",
          "speaker_id": "2",
          "time": "20210826 01:35"
        },
        {
          "id": "MDRW2100015810.1.25",
          "form": "맛있음 된거죠ㅎㅎ",
          "original_form": "맛있음 된거죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 01:36"
        },
        {
          "id": "MDRW2100015810.1.26",
          "form": "맞아요 ㅋㅋㅋㅋㅋ 오랜만에 귤먹고싶네요",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ 오랜만에 귤먹고싶네요",
          "speaker_id": "2",
          "time": "20210826 01:37"
        },
        {
          "id": "MDRW2100015810.1.27",
          "form": "귤은 요즘 하우스로도 나오니 ㅎ",
          "original_form": "귤은 요즘 하우스로도 나오니 ㅎ",
          "speaker_id": "1",
          "time": "20210826 01:46"
        },
        {
          "id": "MDRW2100015810.1.28",
          "form": "한동안 여름과일만 먹어서 귤을 안먹엇거든요 ㅋㅋㅋ",
          "original_form": "한동안 여름과일만 먹어서 귤을 안먹엇거든요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:48"
        },
        {
          "id": "MDRW2100015810.1.29",
          "form": "이제 슬슬 하우스귤 나올거같아요",
          "original_form": "이제 슬슬 하우스귤 나올거같아요",
