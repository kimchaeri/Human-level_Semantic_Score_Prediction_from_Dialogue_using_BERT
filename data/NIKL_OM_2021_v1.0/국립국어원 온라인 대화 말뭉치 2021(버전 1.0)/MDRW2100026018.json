        },
        {
          "id": "MDRW2100026018.1.14",
          "form": "ㅁㅊ 회옴",
          "original_form": "ㅁㅊ 회옴",
          "speaker_id": "1",
          "time": "20210901 18:59"
        },
        {
          "id": "MDRW2100026018.1.15",
          "form": "그보샘 빨리오잖늠",
          "original_form": "그보샘 빨리오잖늠",
          "speaker_id": "2",
          "time": "20210901 18:59"
        },
        {
          "id": "MDRW2100026018.1.16",
          "form": "아 오징어회 시킬까...",
          "original_form": "아 오징어회 시킬까...",
          "speaker_id": "2",
          "time": "20210901 19:00"
        },
        {
          "id": "MDRW2100026018.1.17",
          "form": "숭어랑 오징어 고민하다가 오징오시킴",
          "original_form": "숭어랑 오징어 고민하다가 오징오시킴",
          "speaker_id": "2",
          "time": "20210901 19:12"
        },
        {
          "id": "MDRW2100026018.1.18",
          "form": "비싸네요ㅠ 2.3마넌",
          "original_form": "비싸네요ㅠ 2.3마넌",
          "speaker_id": "2",
          "time": "20210901 19:12"
        },
        {
          "id": "MDRW2100026018.1.19",
          "form": "오징어맛잇겟다ㅠ",
          "original_form": "오징어맛잇겟다ㅠ",
          "speaker_id": "1",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100026018.1.20",
          "form": "배안참",
          "original_form": "배안참",
          "speaker_id": "1",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100026018.1.21",
          "form": "목살+육개장+오징어회ㅋㅋㅋ",
          "original_form": "목살+육개장+오징어회ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:53"
        },
        {
          "id": "MDRW2100026018.1.22",
          "form": "아배불러",
          "original_form": "아배불러",
          "speaker_id": "2",
          "time": "20210901 19:53"
        },
        {
          "id": "MDRW2100026018.1.23",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210901 19:56"
        },
        {
          "id": "MDRW2100026018.1.24",
          "form": "배터지게먹엇네",
          "original_form": "배터지게먹엇네",
          "speaker_id": "1",
          "time": "20210901 19:56"
        },
        {
          "id": "MDRW2100026018.1.25",
          "form": "ㅋㅋㅋ어쩌다보니 일케된",
          "original_form": "ㅋㅋㅋ어쩌다보니 일케된",
          "speaker_id": "2",
          "time": "20210901 19:57"
        },
        {
          "id": "MDRW2100026018.1.26",
          "form": "너무과한거아닙니까?",
          "original_form": "너무과한거아닙니까?",
          "speaker_id": "1",
          "time": "20210901 19:58"
        },
        {
          "id": "MDRW2100026018.1.27",
          "form": "ㄹㅇ 과함ㅠ 울집 가끔 이래요",
          "original_form": "ㄹㅇ 과함ㅠ 울집 가끔 이래요",
          "speaker_id": "2",
          "time": "20210901 20:01"
        },
        {
          "id": "MDRW2100026018.1.28",
          "form": "정줄놓음",
          "original_form": "정줄놓음",
          "speaker_id": "2",
          "time": "20210901 20:01"
        },
        {
          "id": "MDRW2100026018.1.29",
          "form": "그래도 아부지가 회를 좋아해서",
          "original_form": "그래도 아부지가 회를 좋아해서",
          "speaker_id": "2",
          "time": "20210901 20:02"
        },
        {
          "id": "MDRW2100026018.1.30",
          "form": "에라 모르겠다가 됨",
          "original_form": "에라 모르겠다가 됨",
          "speaker_id": "2",
          "time": "20210901 20:02"
        },
        {
          "id": "MDRW2100026018.1.31",
          "form": "각자가 먹고싶은거 대충 요리한 느낌?",
          "original_form": "각자가 먹고싶은거 대충 요리한 느낌?",
          "speaker_id": "2",
          "time": "20210901 20:02"
        },
        {
          "id": "MDRW2100026018.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:02"
        },
        {
          "id": "MDRW2100026018.1.33",
          "form": "가족이 화목하니좋네요",
          "original_form": "가족이 화목하니좋네요",
          "speaker_id": "1",
          "time": "20210901 20:02"
        },
        {
          "id": "MDRW2100026018.1.34",
          "form": "그래서 문제.. ㅋㅋㅋㅋㅋ",
          "original_form": "그래서 문제.. ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:03"
        },
        {
          "id": "MDRW2100026018.1.35",
          "form": "ㅋㅋㅋㅋ뭐가문제에요 좋구만",
          "original_form": "ㅋㅋㅋㅋ뭐가문제에요 좋구만",
          "speaker_id": "1",
          "time": "20210901 20:09"
        },
        {
          "id": "MDRW2100026018.1.36",
          "form": "거 그런게 있슴다",
          "original_form": "거 그런게 있슴다",
          "speaker_id": "2",
          "time": "20210901 20:28"
        },
        {
          "id": "MDRW2100026018.1.37",
          "form": "인생은 멀리서 보면 희극이고 가까이서 보면 비극인거임",
          "original_form": "인생은 멀리서 보면 희극이고 가까이서 보면 비극인거임",
          "speaker_id": "2",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100026018.1.38",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100026018.1.39",
          "form": "아 배부르니",
          "original_form": "아 배부르니",
          "speaker_id": "1",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100026018.1.40",
          "form": "잠도오네요",
          "original_form": "잠도오네요",
          "speaker_id": "1",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100026018.1.41",
          "form": "넷플봐야돠는데",
          "original_form": "넷플봐야돠는데",
          "speaker_id": "1",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100026018.1.42",
          "form": "뭐보세요",
          "original_form": "뭐보세요",
          "speaker_id": "2",
          "time": "20210901 20:30"
        },
        {
          "id": "MDRW2100026018.1.43",
          "form": "디피 달릴라구여",
          "original_form": "디피 달릴라구여",
          "speaker_id": "1",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100026018.1.44",
          "form": "아 먼 군대를",
          "original_form": "아 먼 군대를",
          "speaker_id": "2",
          "time": "20210901 20:38"
        },
        {
          "id": "MDRW2100026018.1.45",
          "form": "졸쟘이라눈데",
          "original_form": "졸쟘이라눈데",
          "speaker_id": "1",
          "time": "20210901 20:38"
        },
        {
          "id": "MDRW2100026018.1.46",
          "form": "그러게요 인기많던데",
          "original_form": "그러게요 인기많던데",
          "speaker_id": "2",
          "time": "20210901 20:38"
        },
