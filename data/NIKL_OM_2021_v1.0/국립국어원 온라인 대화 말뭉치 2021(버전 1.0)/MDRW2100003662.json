      "utterance": [
        {
          "id": "MDRW2100003662.1.1",
          "form": "점심은 뭐 먹을까?",
          "original_form": "점심은 뭐 먹을까?",
          "speaker_id": "2",
          "time": "20210821 11:52"
        },
        {
          "id": "MDRW2100003662.1.2",
          "form": "훌 벌써 점심시간이네 ㅎㅎ",
          "original_form": "훌 벌써 점심시간이네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 11:53"
        },
        {
          "id": "MDRW2100003662.1.3",
          "form": "ㅇㅇ 오전 시간 금방 가네 ㅋㅋ",
          "original_form": "ㅇㅇ 오전 시간 금방 가네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 11:53"
        },
        {
          "id": "MDRW2100003662.1.4",
          "form": "불고기랑 미역국 먹을까?",
          "original_form": "불고기랑 미역국 먹을까?",
          "speaker_id": "1",
          "time": "20210821 11:53"
        },
        {
          "id": "MDRW2100003662.1.5",
          "form": "오키 내가 해줄께",
          "original_form": "오키 내가 해줄께",
          "speaker_id": "2",
          "time": "20210821 11:53"
        },
        {
          "id": "MDRW2100003662.1.6",
          "form": "괜찮아 내가 하면 돼 ㅎㅎ",
          "original_form": "괜찮아 내가 하면 돼 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 11:53"
        },
        {
          "id": "MDRW2100003662.1.7",
          "form": "주말엔 내가 해줄께~ 푹 쉬어",
          "original_form": "주말엔 내가 해줄께~ 푹 쉬어",
          "speaker_id": "2",
          "time": "20210821 11:54"
        },
        {
          "id": "MDRW2100003662.1.8",
          "form": "ㅎㅎ 그럼 부탁해 불고기는 냉동실에 있음",
          "original_form": "ㅎㅎ 그럼 부탁해{emoji:♡} 불고기는 냉동실에 있음",
          "speaker_id": "1",
          "time": "20210821 11:54"
        },
        {
          "id": "MDRW2100003662.1.9",
          "form": "미역은 전에 쓰던 거 있지?",
          "original_form": "미역은 전에 쓰던 거 있지?",
          "speaker_id": "2",
          "time": "20210821 11:54"
        },
        {
          "id": "MDRW2100003662.1.10",
          "form": "아니 새로 사놨어. 씽크대 서랍 열면 있을거야",
          "original_form": "아니 새로 사놨어. 씽크대 서랍 열면 있을거야",
          "speaker_id": "1",
          "time": "20210821 11:54"
        },
        {
          "id": "MDRW2100003662.1.11",
          "form": "오키 맛있게 해줄께 ㅎㅎ",
          "original_form": "오키 맛있게 해줄께 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 11:55"
        },
        {
          "id": "MDRW2100003662.1.12",
          "form": "기대만땅 ㅎㅎ",
          "original_form": "기대만땅 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 11:55"
        },
        {
          "id": "MDRW2100003662.1.13",
          "form": "불고기에 새송이 버섯이랑 넣어야겠다",
          "original_form": "불고기에 새송이 버섯이랑 넣어야겠다",
          "speaker_id": "2",
          "time": "20210821 11:55"
        },
        {
          "id": "MDRW2100003662.1.14",
          "form": "좋지 ㅎㅎ 어제 산 실리콘 볶음 주걱로 볶아",
          "original_form": "좋지 ㅎㅎ 어제 산 실리콘 볶음 주걱로 볶아",
          "speaker_id": "1",
          "time": "20210821 11:56"
        },
        {
          "id": "MDRW2100003662.1.15",
          "form": "그건 또 언제 샀댕 ㅋㅋ",
          "original_form": "그건 또 언제 샀댕 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 11:56"
        },
        {
          "id": "MDRW2100003662.1.16",
          "form": "어제 ㅎㅎ 코팅 후라이팬도 안 벗겨지고 잘 볶여서 좋아",
          "original_form": "어제 ㅎㅎ 코팅 후라이팬도 안 벗겨지고 잘 볶여서 좋아",
          "speaker_id": "1",
          "time": "20210821 11:56"
        },
        {
          "id": "MDRW2100003662.1.17",
          "form": "오오 실리콘 재질이라 부드러운가보네",
          "original_form": "오오 실리콘 재질이라 부드러운가보네",
          "speaker_id": "2",
          "time": "20210821 11:56"
        },
        {
          "id": "MDRW2100003662.1.18",
          "form": "ㅇㅇ 색깔도 예쁘고 잘 볶아져",
          "original_form": "ㅇㅇ 색깔도 예쁘고 잘 볶아져",
          "speaker_id": "1",
          "time": "20210821 11:56"
        },
        {
          "id": "MDRW2100003662.1.19",
          "form": "함 써봐",
          "original_form": "함 써봐",
          "speaker_id": "1",
          "time": "20210821 11:57"
        },
        {
          "id": "MDRW2100003662.1.20",
          "form": "ㅇㅇ 알았어",
          "original_form": "ㅇㅇ 알았어",
          "speaker_id": "2",
          "time": "20210821 11:57"
        },
        {
          "id": "MDRW2100003662.1.21",
