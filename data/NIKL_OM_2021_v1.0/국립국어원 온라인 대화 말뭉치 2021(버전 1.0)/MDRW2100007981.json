        {
          "id": "MDRW2100007981.1.1",
          "form": "아직이요 허쉬초코하나 마시고있어요",
          "original_form": "아직이요 허쉬초코하나 마시고있어요",
          "speaker_id": "2",
          "time": "20210822 12:26"
        },
        {
          "id": "MDRW2100007981.1.2",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210822 12:26"
        },
        {
          "id": "MDRW2100007981.1.3",
          "form": "국밥 먹었어요",
          "original_form": "국밥 먹었어요",
          "speaker_id": "1",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007981.1.4",
          "form": "무슨국밥이요?",
          "original_form": "무슨국밥이요?",
          "speaker_id": "2",
          "time": "20210822 12:28"
        },
        {
          "id": "MDRW2100007981.1.5",
          "form": "돼지요",
          "original_form": "돼지요",
          "speaker_id": "1",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007981.1.6",
          "form": "배달시켜드시나요?",
          "original_form": "배달시켜드시나요?",
          "speaker_id": "2",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007981.1.7",
          "form": "가서 먹었어요",
          "original_form": "가서 먹었어요",
          "speaker_id": "1",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007981.1.8",
          "form": "아 직접갔군요 맛집인가요?",
          "original_form": "아 직접갔군요 맛집인가요?",
          "speaker_id": "2",
          "time": "20210822 12:31"
        },
        {
          "id": "MDRW2100007981.1.9",
          "form": "아니요 그냥 땡겨서 사람없는 아침에 후딱",
          "original_form": "아니요 그냥 땡겨서 사람없는 아침에 후딱",
          "speaker_id": "1",
          "time": "20210822 12:32"
        },
        {
          "id": "MDRW2100007981.1.10",
          "form": "돼지국밥이면 뭐뭐 들어가요?",
          "original_form": "돼지국밥이면 뭐뭐 들어가요?",
          "speaker_id": "2",
          "time": "20210822 12:33"
        },
        {
          "id": "MDRW2100007981.1.11",
          "form": "부추 새우젓 넣으면 좋죠",
          "original_form": "부추 새우젓 넣으면 좋죠",
          "speaker_id": "1",
          "time": "20210822 12:47"
        },
        {
          "id": "MDRW2100007981.1.12",
          "form": "내장도 들어감 맛있을듯",
          "original_form": "내장도 들어감 맛있을듯",
          "speaker_id": "2",
          "time": "20210822 13:02"
        },
        {
          "id": "MDRW2100007981.1.13",
          "form": "섞어도 좋죠",
          "original_form": "섞어도 좋죠",
          "speaker_id": "1",
          "time": "20210822 13:02"
        },
        {
          "id": "MDRW2100007981.1.14",
