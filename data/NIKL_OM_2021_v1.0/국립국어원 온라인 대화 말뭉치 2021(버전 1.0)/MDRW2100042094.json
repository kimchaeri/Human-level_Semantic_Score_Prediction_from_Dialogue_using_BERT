      "utterance": [
        {
          "id": "MDRW2100042094.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210906 19:36"
        },
        {
          "id": "MDRW2100042094.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100042094.1.3",
          "form": "좋아하는 음식 있으세요?",
          "original_form": "좋아하는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210906 19:39"
        },
        {
          "id": "MDRW2100042094.1.4",
          "form": "거의 다 안가리고 잘먹습니당",
          "original_form": "거의 다 안가리고 잘먹습니당",
          "speaker_id": "2",
          "time": "20210906 19:41"
        },
        {
          "id": "MDRW2100042094.1.5",
          "form": "지금은 마라탕이 먹고싶네요ㅋㅋ",
          "original_form": "지금은 마라탕이 먹고싶네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:41"
        },
        {
          "id": "MDRW2100042094.1.6",
          "form": "전 마라탕 안 먹어봤어요 한번 먹으면 다들 중독되길래 전 중독되기 싫어서 굳이 안 먹고 있어요ㅋㅋㅋ",
          "original_form": "전 마라탕 안 먹어봤어요 한번 먹으면 다들 중독되길래 전 중독되기 싫어서 굳이 안 먹고 있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:42"
        },
        {
          "id": "MDRW2100042094.1.7",
          "form": "ㅋㅋㅋㅋ그러게요",
          "original_form": "ㅋㅋㅋㅋ그러게요",
          "speaker_id": "2",
          "time": "20210906 19:47"
        },
        {
          "id": "MDRW2100042094.1.8",
          "form": "주기적으로생각나는거같아요",
          "original_form": "주기적으로생각나는거같아요",
          "speaker_id": "2",
          "time": "20210906 19:47"
        },
        {
          "id": "MDRW2100042094.1.9",
          "form": "머좋아하세요?",
          "original_form": "머좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 19:48"
        },
        {
          "id": "MDRW2100042094.1.10",
          "form": "전 떡볶이 좋아해여",
          "original_form": "전 떡볶이 좋아해여",
          "speaker_id": "1",
          "time": "20210906 19:49"
        },
        {
          "id": "MDRW2100042094.1.11",
          "form": "와 진짜맛있죠",
          "original_form": "와 진짜맛있죠",
          "speaker_id": "2",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100042094.1.12",
          "form": "갑자기먹고싶네요ㅋㅋ",
          "original_form": "갑자기먹고싶네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100042094.1.13",
          "form": "ㅋㅋㅋ좋아하는 떡볶이 브랜드있으세요?",
          "original_form": "ㅋㅋㅋ좋아하는 떡볶이 브랜드있으세요?",
          "speaker_id": "1",
          "time": "20210906 19:51"
        },
        {
          "id": "MDRW2100042094.1.14",
          "form": "신전좋아해요!",
          "original_form": "신전좋아해요!",
          "speaker_id": "2",
          "time": "20210906 19:52"
        },
        {
          "id": "MDRW2100042094.1.15",
          "form": "요즘은 태리로제떡볶이도 많이시켜먹어요ㅋㅋ",
          "original_form": "요즘은 태리로제떡볶이도 많이시켜먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:52"
        },
        {
          "id": "MDRW2100042094.1.16",
          "form": "저 태리에서는 안 먹어봤어요",
          "original_form": "저 태리에서는 안 먹어봤어요",
          "speaker_id": "1",
          "time": "20210906 19:52"
        },
        {
          "id": "MDRW2100042094.1.17",
          "form": "신전이랑 떡군이네 먹어봤었는데 둘 다 괜찮았어요",
          "original_form": "신전이랑 떡군이네 먹어봤었는데 둘 다 괜찮았어요",
          "speaker_id": "1",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100042094.1.18",
          "form": "떡군이네 먹어보고싶은데",
          "original_form": "떡군이네 먹어보고싶은데",
          "speaker_id": "2",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100042094.1.19",
          "form": "주변에 매장이 없더라구요ㅜㅜ",
          "original_form": "주변에 매장이 없더라구요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100042094.1.20",
          "form": "스파게티면추가가 필수라던ㅋㅋ",
          "original_form": "스파게티면추가가 필수라던ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:53"
        },
        {
          "id": "MDRW2100042094.1.21",
          "form": "로제 떡볶이 완전 맛있어요 전 스파게티면도 좋지만 넙적당면이 더 좋더라고요",
          "original_form": "로제 떡볶이 완전 맛있어요 전 스파게티면도 좋지만 넙적당면이 더 좋더라고요",
          "speaker_id": "1",
          "time": "20210906 19:54"
        },
        {
          "id": "MDRW2100042094.1.22",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210906 19:55"
        },
        {
          "id": "MDRW2100042094.1.23",
          "form": "역시 근본이죠 당면",
          "original_form": "역시 근본이죠 당면",
          "speaker_id": "2",
          "time": "20210906 19:55"
        },
        {
          "id": "MDRW2100042094.1.24",
          "form": "꼭먹어보고싶네요ㅜㅜ",
          "original_form": "꼭먹어보고싶네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 19:55"
        }
      ]
