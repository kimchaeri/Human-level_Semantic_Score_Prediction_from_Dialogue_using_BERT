      "utterance": [
        {
          "id": "MDRW2100019461.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210827 19:32"
        },
        {
          "id": "MDRW2100019461.1.2",
          "form": "혹시 최근에 아파본 적 있어요?",
          "original_form": "혹시 최근에 아파본 적 있어요?",
          "speaker_id": "1",
          "time": "20210827 19:32"
        },
        {
          "id": "MDRW2100019461.1.3",
          "form": "음 아뇨아뇨",
          "original_form": "음 아뇨아뇨",
          "speaker_id": "2",
          "time": "20210827 19:33"
        },
        {
          "id": "MDRW2100019461.1.4",
          "form": "name1님은요??",
          "original_form": "&name1&님은요??",
          "speaker_id": "2",
          "time": "20210827 19:33"
        },
        {
          "id": "MDRW2100019461.1.5",
          "form": "저는 바로 어제 알러지성 비염 왔어요 ㅜㅜ",
          "original_form": "저는 바로 어제 알러지성 비염 왔어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 19:34"
        },
        {
          "id": "MDRW2100019461.1.6",
          "form": "일하고 있는데 갑작스러워서 어찌나 당황스럽던지요..",
          "original_form": "일하고 있는데 갑작스러워서 어찌나 당황스럽던지요..",
          "speaker_id": "1",
          "time": "20210827 19:34"
        },
        {
          "id": "MDRW2100019461.1.7",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100019461.1.8",
          "form": "저도 비염 조금 있는데 ㅠ",
          "original_form": "저도 비염 조금 있는데 ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100019461.1.9",
          "form": "심한 날엔 잘 때 힘들어요 코막혀서",
          "original_form": "심한 날엔 잘 때 힘들어요 코막혀서",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100019461.1.10",
          "form": "ㅠㅜ",
          "original_form": "ㅠㅜ",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100019461.1.11",
          "form": "ㅠㅠ 맞죠 차라리 코 막히고 재채기 하면 다행인데",
          "original_form": "ㅠㅠ 맞죠 차라리 코 막히고 재채기 하면 다행인데",
          "speaker_id": "1",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100019461.1.12",
          "form": "어떤 날엔 심하면 맑은 콧물까지 나니 미치겠더라구요",
          "original_form": "어떤 날엔 심하면 맑은 콧물까지 나니 미치겠더라구요",
          "speaker_id": "1",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100019461.1.13",
          "form": "아 또 그렇게 생각하니까",
          "original_form": "아 또 그렇게 생각하니까",
          "speaker_id": "2",
          "time": "20210828 18:07"
        },
        {
          "id": "MDRW2100019461.1.14",
          "form": "차라리 막히는 게 낫네요..",
          "original_form": "차라리 막히는 게 낫네요..",
          "speaker_id": "2",
          "time": "20210828 18:07"
        },
        {
          "id": "MDRW2100019461.1.15",
          "form": "그쵸 ㅜㅜ 코로나 때문에 눈총 받고 오해 받기 쉬운 것 같아요",
          "original_form": "그쵸 ㅜㅜ 코로나 때문에 눈총 받고 오해 받기 쉬운 것 같아요",
          "speaker_id": "1",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100019461.1.16",
          "form": "비염의 서러움... ㅜㅜ",
          "original_form": "비염의 서러움... ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100019461.1.17",
          "form": "ㅠㅜ 요즘 무슨 약 나왔던데",
          "original_form": "ㅠㅜ 요즘 무슨 약 나왔던데",
          "speaker_id": "2",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100019461.1.18",
          "form": "코 안에 바르는 거",
          "original_form": "코 안에 바르는 거",
          "speaker_id": "2",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100019461.1.19",
          "form": "그런 것도 있어요?",
          "original_form": "그런 것도 있어요?",
          "speaker_id": "1",
          "time": "20210828 20:26"
        },
        {
          "id": "MDRW2100019461.1.20",
          "form": "저 예전에 비강 스프레이? 이런거 써봤는데 전혀 효과 없던데요 ㅜㅜ",
          "original_form": "저 예전에 비강 스프레이? 이런거 써봤는데 전혀 효과 없던데요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 20:26"
        },
        {
          "id": "MDRW2100019461.1.21",
          "form": "아 정말요???",
          "original_form": "아 정말요???",
          "speaker_id": "2",
          "time": "20210830 11:52"
        },
        {
          "id": "MDRW2100019461.1.22",
          "form": "근데 정말 심한 날은 정말 꽉 막히더라구요",
          "original_form": "근데 정말 심한 날은 정말 꽉 막히더라구요",
          "speaker_id": "2",
          "time": "20210830 11:52"
        },
        {
          "id": "MDRW2100019461.1.23",
          "form": "너무 불편해요 ㅠ",
          "original_form": "너무 불편해요 ㅠ",
          "speaker_id": "2",
          "time": "20210830 11:52"
        },
        {
          "id": "MDRW2100019461.1.24",
          "form": "알러지 원인이 뭐에요?",
          "original_form": "알러지 원인이 뭐에요?",
          "speaker_id": "1",
          "time": "20210830 19:17"
        },
        {
          "id": "MDRW2100019461.1.25",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210831 20:48"
        },
        {
          "id": "MDRW2100019461.1.26",
          "form": "약간 건조하면 막히더라구용 ㅠ",
          "original_form": "약간 건조하면 막히더라구용 ㅠ",
          "speaker_id": "2",
          "time": "20210831 20:48"
        },
        {
          "id": "MDRW2100019461.1.27",
          "form": "아 ㅠㅠ 저도 그런 이유일 수도 있겠네요",
          "original_form": "아 ㅠㅠ 저도 그런 이유일 수도 있겠네요",
          "speaker_id": "1",
          "time": "20210831 20:53"
        },
        {
          "id": "MDRW2100019461.1.28",
          "form": "제가 워낙 습도에 둔감해서 ㅋㅋㅋㅋㅋ",
          "original_form": "제가 워낙 습도에 둔감해서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 20:53"
        }
      ]
