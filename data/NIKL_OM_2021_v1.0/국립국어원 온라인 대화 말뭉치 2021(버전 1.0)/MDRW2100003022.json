      "utterance": [
        {
          "id": "MDRW2100003022.1.1",
          "form": "점심 뭐드셨나요ㅎㅎ",
          "original_form": "점심 뭐드셨나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 17:38"
        },
        {
          "id": "MDRW2100003022.1.2",
          "form": "신전떡볶이 먹었습니다",
          "original_form": "신전떡볶이 먹었습니다",
