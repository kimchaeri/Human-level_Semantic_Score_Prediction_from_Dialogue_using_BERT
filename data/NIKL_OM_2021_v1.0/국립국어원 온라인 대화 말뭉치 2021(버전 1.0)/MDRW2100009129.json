      "utterance": [
        {
          "id": "MDRW2100009129.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210822 21:33"
        },
        {
          "id": "MDRW2100009129.1.2",
          "form": "봉사활동경험있으신가요?",
          "original_form": "봉사활동경험있으신가요?",
          "speaker_id": "1",
          "time": "20210822 21:34"
        },
        {
          "id": "MDRW2100009129.1.3",
          "form": "음 초등학교 도서관봉사랑",
          "original_form": "음 초등학교 도서관봉사랑",
          "speaker_id": "2",
          "time": "20210822 21:34"
        },
        {
          "id": "MDRW2100009129.1.4",
          "form": "저는 일년정도 인도에 해외봉사 다녀왔었어요",
          "original_form": "저는 일년정도 인도에 해외봉사 다녀왔었어요",
          "speaker_id": "1",
          "time": "20210822 21:34"
        },
        {
          "id": "MDRW2100009129.1.5",
          "form": "구연동화 봉사요",
          "original_form": "구연동화 봉사요",
          "speaker_id": "2",
          "time": "20210822 21:34"
        },
        {
          "id": "MDRW2100009129.1.6",
          "form": "오 좋은 봉사 하셨네요",
          "original_form": "오 좋은 봉사 하셨네요",
          "speaker_id": "1",
          "time": "20210822 21:34"
        },
        {
          "id": "MDRW2100009129.1.7",
          "form": "어머 세상에",
          "original_form": "어머 세상에",
          "speaker_id": "2",
          "time": "20210822 21:34"
        },
        {
          "id": "MDRW2100009129.1.8",
          "form": "구연동화!!!ㅇ와",
          "original_form": "구연동화!!!ㅇ와",
          "speaker_id": "1",
          "time": "20210822 21:35"
        },
        {
          "id": "MDRW2100009129.1.9",
          "form": "재능있으시다",
          "original_form": "재능있으시다",
          "speaker_id": "1",
          "time": "20210822 21:35"
        },
        {
          "id": "MDRW2100009129.1.10",
          "form": "물이나 음식도 안맞아 힘들다던데",
          "original_form": "물이나 음식도 안맞아 힘들다던데",
          "speaker_id": "2",
          "time": "20210822 21:35"
        },
        {
          "id": "MDRW2100009129.1.11",
          "form": "ㅋㅋ목소리가 좀 특이해요",
          "original_form": "ㅋㅋ목소리가 좀 특이해요",
          "speaker_id": "2",
          "time": "20210822 21:35"
        },
        {
          "id": "MDRW2100009129.1.12",
          "form": "멋지시네요 구연동화 저두어릴때 많이 들었어요",
          "original_form": "멋지시네요 구연동화 저두어릴때 많이 들었어요",
          "speaker_id": "1",
          "time": "20210822 21:35"
        },
        {
          "id": "MDRW2100009129.1.13",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 21:35"
        },
        {
          "id": "MDRW2100009129.1.14",
          "form": "잼나요",
          "original_form": "잼나요",
          "speaker_id": "2",
          "time": "20210822 21:35"
        },
        {
          "id": "MDRW2100009129.1.15",
          "form": "개성을 잘 살리셨군요",
          "original_form": "개성을 잘 살리셨군요",
          "speaker_id": "1",
          "time": "20210822 21:35"
        },
        {
          "id": "MDRW2100009129.1.16",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 21:35"
        },
        {
          "id": "MDRW2100009129.1.17",
          "form": "무슨 봉사세요?",
          "original_form": "무슨 봉사세요?",
          "speaker_id": "2",
          "time": "20210822 21:35"
        },
        {
          "id": "MDRW2100009129.1.18",
          "form": "저도 구연동화봉사해주시는 선생님들덕분에 재미난 어린시절보냈네요",
          "original_form": "저도 구연동화봉사해주시는 선생님들덕분에 재미난 어린시절보냈네요",
          "speaker_id": "1",
          "time": "20210822 21:36"
        },
        {
          "id": "MDRW2100009129.1.19",
          "form": "아 그렇다니 다행이네요",
          "original_form": "아 그렇다니 다행이네요",
          "speaker_id": "2",
          "time": "20210822 21:36"
        },
        {
          "id": "MDRW2100009129.1.20",
          "form": "인도에 천민동네 학교 유치원 지원하는봉사였어요",
          "original_form": "인도에 천민동네 학교 유치원 지원하는봉사였어요",
          "speaker_id": "1",
          "time": "20210822 21:36"
        },
        {
          "id": "MDRW2100009129.1.21",
          "form": "아이들이 분장도 같이 하니 좋아했어요",
          "original_form": "아이들이 분장도 같이 하니 좋아했어요",
          "speaker_id": "2",
          "time": "20210822 21:36"
        },
        {
          "id": "MDRW2100009129.1.22",
          "form": "지금은 난리가나서못가겠네요 ㅋㅋ",
          "original_form": "지금은 난리가나서못가겠네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:36"
        },
        {
          "id": "MDRW2100009129.1.23",
          "form": "아 정말요",
          "original_form": "아 정말요",
          "speaker_id": "2",
          "time": "20210822 21:36"
        },
        {
          "id": "MDRW2100009129.1.24",
          "form": "와 요샌 진짜 다양한거 하네 학교에서",
          "original_form": "와 요샌 진짜 다양한거 하네 학교에서",
          "speaker_id": "1",
          "time": "20210822 21:36"
        },
        {
          "id": "MDRW2100009129.1.25",
          "form": "재밌었겠다",
          "original_form": "재밌었겠다",
          "speaker_id": "1",
          "time": "20210822 21:37"
        },
        {
          "id": "MDRW2100009129.1.26",
          "form": "진짜 아이들 동보는거는 아이를 진짜 좋아해야 할수있어요",
          "original_form": "진짜 아이들 동보는거는 아이를 진짜 좋아해야 할수있어요",
          "speaker_id": "2",
          "time": "20210822 21:37"
        },
        {
          "id": "MDRW2100009129.1.27",
          "form": "네 근데 코로나로 ㅜㅜ",
          "original_form": "네 근데 코로나로 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 21:37"
        },
        {
          "id": "MDRW2100009129.1.28",
          "form": "인도 학교에서도 그런거 해보면 좋았겠어요",
          "original_form": "인도 학교에서도 그런거 해보면 좋았겠어요",
          "speaker_id": "1",
          "time": "20210822 21:37"
        },
        {
          "id": "MDRW2100009129.1.29",
          "form": "아 글쵸 코로나로 학교가 안여니까",
          "original_form": "아 글쵸 코로나로 학교가 안여니까",
          "speaker_id": "1",
          "time": "20210822 21:37"
        },
        {
          "id": "MDRW2100009129.1.30",
          "form": "중지된지 2년 다됐어요 ㅜㅜ",
          "original_form": "중지된지 2년 다됐어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 21:37"
        },
        {
          "id": "MDRW2100009129.1.31",
          "form": "재미도 못보고",
          "original_form": "재미도 못보고",
          "speaker_id": "1",
          "time": "20210822 21:37"
        },
        {
          "id": "MDRW2100009129.1.32",
          "form": "애들도 답답하겠어요",
          "original_form": "애들도 답답하겠어요",
          "speaker_id": "1",
          "time": "20210822 21:37"
        },
        {
          "id": "MDRW2100009129.1.33",
          "form": "애들도 많은걸 경험 못하고 맘아파요",
          "original_form": "애들도 많은걸 경험 못하고 맘아파요",
          "speaker_id": "2",
          "time": "20210822 21:37"
        },
        {
          "id": "MDRW2100009129.1.34",
          "form": "생활시 어려운점이 많을텐데",
          "original_form": "생활시 어려운점이 많을텐데",
          "speaker_id": "2",
          "time": "20210822 21:38"
        },
        {
          "id": "MDRW2100009129.1.35",
          "form": "진짜 봉사정신이 대단하신가봐요",
          "original_form": "진짜 봉사정신이 대단하신가봐요",
          "speaker_id": "2",
          "time": "20210822 21:38"
        },
        {
          "id": "MDRW2100009129.1.36",
          "form": "예전에 노인요양원 에서 잠깐 청소 봉사했는데",
          "original_form": "예전에 노인요양원 에서 잠깐 청소 봉사했는데",
          "speaker_id": "2",
          "time": "20210822 21:39"
        },
        {
          "id": "MDRW2100009129.1.37",
          "form": "보니까 어르신들 케어 보통일 아니더라구요",
          "original_form": "보니까 어르신들 케어 보통일 아니더라구요",
          "speaker_id": "2",
          "time": "20210822 21:39"
        },
        {
          "id": "MDRW2100009129.1.38",
          "form": "아 또 좋은봉사를 하셨군요",
          "original_form": "아 또 좋은봉사를 하셨군요",
          "speaker_id": "1",
          "time": "20210822 21:40"
        },
        {
          "id": "MDRW2100009129.1.39",
          "form": "어렵진 않았어요",
          "original_form": "어렵진 않았어요",
          "speaker_id": "2",
          "time": "20210822 21:40"
        },
        {
          "id": "MDRW2100009129.1.40",
          "form": "어르신들이 몸이 불편하시니까 손이 많이 가더라구요",
          "original_form": "어르신들이 몸이 불편하시니까 손이 많이 가더라구요",
          "speaker_id": "1",
          "time": "20210822 21:40"
        },
        {
          "id": "MDRW2100009129.1.41",
          "form": "전 청소라",
          "original_form": "전 청소라",
          "speaker_id": "2",
          "time": "20210822 21:40"
        },
        {
          "id": "MDRW2100009129.1.42",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 21:40"
        },
        {
          "id": "MDRW2100009129.1.43",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210822 21:40"
        },
        {
          "id": "MDRW2100009129.1.44",
          "form": "전 초보라 ㅋㅋ",
          "original_form": "전 초보라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:40"
        },
        {
          "id": "MDRW2100009129.1.45",
          "form": "저도 인도에 켈커타에서 노인시설봉사도 했었는데요",
          "original_form": "저도 인도에 켈커타에서 노인시설봉사도 했었는데요",
          "speaker_id": "1",
          "time": "20210822 21:41"
        }
      ]
