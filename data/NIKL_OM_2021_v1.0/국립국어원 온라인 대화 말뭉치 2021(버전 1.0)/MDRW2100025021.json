          "time": "20210831 22:21"
        },
        {
          "id": "MDRW2100025021.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:21"
        },
        {
          "id": "MDRW2100025021.1.18",
          "form": "직접가서먹어용",
