      "utterance": [
        {
          "id": "MDRW2100002574.1.1",
          "form": "누나는 혹시 좋아하는",
          "original_form": "누나는 혹시 좋아하는",
          "speaker_id": "1",
          "time": "20210820 13:40"
        },
        {
          "id": "MDRW2100002574.1.2",
          "form": "음식이나 음료같은거 있어?",
          "original_form": "음식이나 음료같은거 있어?",
          "speaker_id": "1",
          "time": "20210820 13:40"
        },
        {
          "id": "MDRW2100002574.1.3",
          "form": "좋아하는 음식은 계란이랑 음료는 토마토주스!",
          "original_form": "좋아하는 음식은 계란이랑 음료는 토마토주스!",
          "speaker_id": "2",
          "time": "20210820 13:41"
        },
        {
          "id": "MDRW2100002574.1.4",
          "form": "계란도 맛있지",
          "original_form": "계란도 맛있지",
          "speaker_id": "1",
          "time": "20210820 13:42"
        },
        {
          "id": "MDRW2100002574.1.5",
          "form": "계란을 이용한 요리 많던데",
          "original_form": "계란을 이용한 요리 많던데",
          "speaker_id": "1",
          "time": "20210820 13:42"
        },
        {
          "id": "MDRW2100002574.1.6",
          "form": "스크램블 에그라던지",
          "original_form": "스크램블 에그라던지",
          "speaker_id": "1",
          "time": "20210820 13:42"
        },
        {
          "id": "MDRW2100002574.1.7",
          "form": "계란찜",
          "original_form": "계란찜",
          "speaker_id": "1",
          "time": "20210820 13:42"
        },
        {
          "id": "MDRW2100002574.1.8",
          "form": "계란말이",
          "original_form": "계란말이",
          "speaker_id": "2",
          "time": "20210820 13:42"
        },
        {
          "id": "MDRW2100002574.1.9",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210820 13:42"
        },
        {
          "id": "MDRW2100002574.1.10",
          "form": "또 계란토스트",
          "original_form": "또 계란토스트",
          "speaker_id": "2",
          "time": "20210820 13:42"
        },
        {
          "id": "MDRW2100002574.1.11",
          "form": "토스트도 맛있지",
          "original_form": "토스트도 맛있지",
          "speaker_id": "1",
          "time": "20210820 13:42"
        },
        {
          "id": "MDRW2100002574.1.12",
          "form": "개인적으로 나는 토스트 햄에 계란 그리고 케챱 넣어서",
          "original_form": "개인적으로 나는 토스트 햄에 계란 그리고 케챱 넣어서",
          "speaker_id": "1",
          "time": "20210820 13:42"
        },
        {
          "id": "MDRW2100002574.1.13",
          "form": "먹으면",
          "original_form": "먹으면",
          "speaker_id": "1",
          "time": "20210820 13:42"
        },
        {
          "id": "MDRW2100002574.1.14",
          "form": "맛있더라",
          "original_form": "맛있더라",
          "speaker_id": "1",
          "time": "20210820 13:43"
        },
        {
          "id": "MDRW2100002574.1.15",
          "form": "갑자기 이삭 토스트먹고싶어졌다",
          "original_form": "갑자기 이삭 토스트먹고싶어졌다",
          "speaker_id": "2",
          "time": "20210820 13:43"
        },
        {
          "id": "MDRW2100002574.1.16",
          "form": "이삭토스트가 맛집이긴하지",
          "original_form": "이삭토스트가 맛집이긴하지",
          "speaker_id": "1",
          "time": "20210820 13:43"
        },
        {
          "id": "MDRW2100002574.1.17",
          "form": "토스트 거의 원탑급",
          "original_form": "토스트 거의 원탑급",
          "speaker_id": "1",
          "time": "20210820 13:43"
        },
        {
          "id": "MDRW2100002574.1.18",
          "form": "특히 계란에 옥수수알갱이도있고",
          "original_form": "특히 계란에 옥수수알갱이도있고",
          "speaker_id": "2",
          "time": "20210820 13:43"
        },
        {
          "id": "MDRW2100002574.1.19",
          "form": "맞아맞아 엄청 맛있지",
          "original_form": "맞아맞아 엄청 맛있지",
          "speaker_id": "1",
          "time": "20210820 13:44"
        },
        {
          "id": "MDRW2100002574.1.20",
          "form": "그 난 토스트중에 치즈 늘어나는거 들어간게 제일좋더라",
          "original_form": "그 난 토스트중에 치즈 늘어나는거 들어간게 제일좋더라",
          "speaker_id": "2",
          "time": "20210820 13:45"
        },
        {
          "id": "MDRW2100002574.1.21",
          "form": "치즈도 맛있지",
          "original_form": "치즈도 맛있지",
          "speaker_id": "1",
          "time": "20210820 13:46"
        },
        {
          "id": "MDRW2100002574.1.22",
          "form": "이삭토스트에서 스페셜 들어간건 다 먹어봐여되",
          "original_form": "이삭토스트에서 스페셜 들어간건 다 먹어봐여되",
          "speaker_id": "1",
          "time": "20210820 13:46"
        },
        {
          "id": "MDRW2100002574.1.23",
          "form": "양 엄청 나!",
          "original_form": "양 엄청 나!",
          "speaker_id": "1",
          "time": "20210820 13:46"
        },
        {
          "id": "MDRW2100002574.1.24",
          "form": "아 오늘당장 토스트 먹으러가야겠다",
          "original_form": "아 오늘당장 토스트 먹으러가야겠다",
          "speaker_id": "2",
          "time": "20210820 13:49"
        },
        {
          "id": "MDRW2100002574.1.25",
          "form": "당장 가자 고고씽",
          "original_form": "당장 가자 고고씽",
