      "utterance": [
        {
          "id": "MDRW2100015020.1.1",
          "form": "안녕하세용ㅎㅎ",
          "original_form": "안녕하세용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100015020.1.2",
          "form": "운동좋아하시나요?",
          "original_form": "운동좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100015020.1.3",
          "form": "안녕하세용!!",
          "original_form": "안녕하세용!!",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100015020.1.4",
          "form": "저는 축구 보는거 좋아해요ㅎㅎ",
          "original_form": "저는 축구 보는거 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100015020.1.5",
          "form": "좋아하시는 스포츠 있으세요?",
          "original_form": "좋아하시는 스포츠 있으세요?",
          "speaker_id": "2",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100015020.1.6",
          "form": "저도 축구보는건 좋아해요!",
          "original_form": "저도 축구보는건 좋아해요!",
          "speaker_id": "1",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100015020.1.7",
          "form": "저는 야구랑 농구좋아해요ㅎㅎ",
          "original_form": "저는 야구랑 농구좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100015020.1.8",
          "form": "오오 야구랑 농구하는걸 좋아하시는거에요?",
          "original_form": "오오 야구랑 농구하는걸 좋아하시는거에요?",
          "speaker_id": "2",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100015020.1.9",
          "form": "네 야구랑 농구는 하는것도 좋아하구 보는것도 좋아해요!",
          "original_form": "네 야구랑 농구는 하는것도 좋아하구 보는것도 좋아해요!",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100015020.1.10",
          "form": "우와,, 멋지세요!",
          "original_form": "우와,, 멋지세요!",
          "speaker_id": "2",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100015020.1.11",
          "form": "혹시 직관도 해보셨나요?",
          "original_form": "혹시 직관도 해보셨나요?",
          "speaker_id": "2",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100015020.1.12",
          "form": "네 직관도 꽤 해봤죠!ㅎㅎ",
          "original_form": "네 직관도 꽤 해봤죠!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100015020.1.13",
          "form": "축구 직관가보셨나요?",
          "original_form": "축구 직관가보셨나요?",
          "speaker_id": "1",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100015020.1.14",
          "form": "저는 축구는 한번도 직관안해봤어요!",
          "original_form": "저는 축구는 한번도 직관안해봤어요!",
          "speaker_id": "1",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100015020.1.15",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100015020.1.16",
          "form": "저는 많이는 못해보고 세네번 정도 해봤어요",
          "original_form": "저는 많이는 못해보고 세네번 정도 해봤어요",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100015020.1.17",
          "form": "근데 직관 재밌더라구요ㅋㅋ",
          "original_form": "근데 직관 재밌더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100015020.1.18",
          "form": "오 그러시구낭 축구 어디서 보셨어요?",
          "original_form": "오 그러시구낭 축구 어디서 보셨어요?",
          "speaker_id": "1",
          "time": "20210825 21:40"
        },
        {
          "id": "MDRW2100015020.1.19",
          "form": "직괌 재미있죠!ㅎㅎ",
          "original_form": "직괌 재미있죠!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:40"
        },
        {
          "id": "MDRW2100015020.1.20",
          "form": "서울월드컵경기장이라고 아실려나용",
          "original_form": "서울월드컵경기장이라고 아실려나용",
          "speaker_id": "2",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100015020.1.21",
          "form": "딴데는 멀어서 여기만 가봤네요ㅋㅋㅠㅠ",
          "original_form": "딴데는 멀어서 여기만 가봤네요ㅋㅋㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015020.1.22",
          "form": "서울사시나보네요!?",
          "original_form": "서울사시나보네요!?",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015020.1.23",
          "form": "저도 전라도사람이라서",
          "original_form": "저도 전라도사람이라서",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100015020.1.24",
          "form": "야구는 광주에서만 보고 농구는 전주랑 군산에서만 봐밨어요!ㅋㅋㅋㅋ",
          "original_form": "야구는 광주에서만 보고 농구는 전주랑 군산에서만 봐밨어요!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100015020.1.25",
          "form": "우와 그래도 여러군데에서 보셨네요ㅋㅋ",
          "original_form": "우와 그래도 여러군데에서 보셨네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100015020.1.26",
          "form": "야구랑 농구도 요즘 무관중인가요?",
          "original_form": "야구랑 농구도 요즘 무관중인가요?",
          "speaker_id": "2",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100015020.1.27",
          "form": "네 직관이 매력이 있죠ㅎㅎ",
          "original_form": "네 직관이 매력이 있죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015020.1.28",
          "form": "야구는 지금 관중있는곳도 있고 무관중인곳도 있는걸로 알고있어요!",
          "original_form": "야구는 지금 관중있는곳도 있고 무관중인곳도 있는걸로 알고있어요!",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015020.1.29",
          "form": "농구는 지금 시즌이 끝나서 다음시즌에 어떻게 될지 잘 모르겠네요ㅠ",
          "original_form": "농구는 지금 시즌이 끝나서 다음시즌에 어떻게 될지 잘 모르겠네요ㅠ",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015020.1.30",
          "form": "축구 어느팀 좋아하세요?",
          "original_form": "축구 어느팀 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015020.1.31",
          "form": "아 그렇군요ㅠㅠ",
          "original_form": "아 그렇군요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100015020.1.32",
          "form": "저는 토트넘 좋아해요ㅎㅎ",
          "original_form": "저는 토트넘 좋아해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015020.1.33",
          "form": "손흥민 선수 응원하거든요ㅋㅋ",
          "original_form": "손흥민 선수 응원하거든요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015020.1.34",
          "form": "오 그러시구나!",
          "original_form": "오 그러시구나!",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015020.1.35",
          "form": "손흥민선수 진짜 꾸준히 잘해서 한국인으로서 너무 기분 좋아요ㅠㅠ",
          "original_form": "손흥민선수 진짜 꾸준히 잘해서 한국인으로서 너무 기분 좋아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015020.1.36",
          "form": "저는 어릴때 박지성선수 응원하려고 맨유경기도 암청 많이 봤었어요!ㅋㅋㅋㅋ",
          "original_form": "저는 어릴때 박지성선수 응원하려고 맨유경기도 암청 많이 봤었어요!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100015020.1.37",
          "form": "그쵸ㅠㅠ 한국선수 있는 팀은 다 응원하게 되더라구요ㅋㅋ",
          "original_form": "그쵸ㅠㅠ 한국선수 있는 팀은 다 응원하게 되더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100015020.1.38",
          "form": "오 박지성 선수 맨유시절도 보셨구나 부러워요ㅋㅋ",
          "original_form": "오 박지성 선수 맨유시절도 보셨구나 부러워요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015020.1.39",
          "form": "그러게요!ㅎㅎ",
          "original_form": "그러게요!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:55"
        },
        {
          "id": "MDRW2100015020.1.40",
          "form": "박지성선수 맨유시절에 진짜 재미있었어요ㅠㅠ",
          "original_form": "박지성선수 맨유시절에 진짜 재미있었어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 21:56"
        },
        {
          "id": "MDRW2100015020.1.41",
          "form": "ㅠㅠ전 그시절을 못봤거든요 너무 아쉬워요",
          "original_form": "ㅠㅠ전 그시절을 못봤거든요 너무 아쉬워요",
          "speaker_id": "2",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100015020.1.42",
          "form": "어릴때부터 스포츠를 좋아하셨나용?",
          "original_form": "어릴때부터 스포츠를 좋아하셨나용?",
          "speaker_id": "2",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100015020.1.43",
          "form": "아쉽긴하지만 앞으로도 더 좋은선수도 많이 나오겠죠!ㅎㅎ",
          "original_form": "아쉽긴하지만 앞으로도 더 좋은선수도 많이 나오겠죠!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100015020.1.44",
          "form": "네 어릴때부터 이것저갓 좋아했어요!",
          "original_form": "네 어릴때부터 이것저갓 좋아했어요!",
          "speaker_id": "1",
          "time": "20210825 21:59"
        }
      ]
