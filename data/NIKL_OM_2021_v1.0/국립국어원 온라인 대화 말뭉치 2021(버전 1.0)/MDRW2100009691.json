      "utterance": [
        {
          "id": "MDRW2100009691.1.1",
          "form": "홈트하세요?",
          "original_form": "홈트하세요?",
          "speaker_id": "1",
          "time": "20210823 04:09"
        },
        {
          "id": "MDRW2100009691.1.2",
          "form": "운동이라면 숨쉬기만 하는데...",
          "original_form": "운동이라면 숨쉬기만 하는데...",
          "speaker_id": "2",
          "time": "20210823 04:10"
        },
        {
          "id": "MDRW2100009691.1.3",
          "form": "걷기라도 하시지",
          "original_form": "걷기라도 하시지",
          "speaker_id": "1",
          "time": "20210823 04:10"
        },
        {
          "id": "MDRW2100009691.1.4",
          "form": "출근하는걸로도 충분해",
          "original_form": "출근하는걸로도 충분해",
          "speaker_id": "2",
          "time": "20210823 04:10"
        },
        {
          "id": "MDRW2100009691.1.5",
          "form": "아마도?",
          "original_form": "아마도?",
          "speaker_id": "2",
          "time": "20210823 04:10"
        },
        {
          "id": "MDRW2100009691.1.6",
          "form": "절대 아닐걸요",
          "original_form": "절대 아닐걸요",
          "speaker_id": "1",
          "time": "20210823 04:10"
        },
        {
          "id": "MDRW2100009691.1.7",
          "form": "내 배가 그럴다고는 하네",
          "original_form": "내 배가 그럴다고는 하네",
          "speaker_id": "2",
          "time": "20210823 04:11"
        },
        {
          "id": "MDRW2100009691.1.8",
          "form": "지방층이요?",
          "original_form": "지방층이요?",
          "speaker_id": "1",
          "time": "20210823 04:11"
        },
        {
          "id": "MDRW2100009691.1.9",
          "form": "근육이여",
          "original_form": "근육이여",
          "speaker_id": "2",
          "time": "20210823 04:11"
        },
        {
          "id": "MDRW2100009691.1.10",
          "form": "물결치는?",
          "original_form": "물결치는?",
          "speaker_id": "1",
          "time": "20210823 04:11"
        },
        {
          "id": "MDRW2100009691.1.11",
          "form": "근육도 물결칠수 있어",
          "original_form": "근육도 물결칠수 있어",
          "speaker_id": "2",
          "time": "20210823 04:11"
        },
        {
          "id": "MDRW2100009691.1.12",
          "form": "그건 좀 징그러울것 같은데요",
          "original_form": "그건 좀 징그러울것 같은데요",
          "speaker_id": "1",
          "time": "20210823 04:11"
        },
        {
          "id": "MDRW2100009691.1.13",
          "form": "선수들은 그렇지 않을까?",
          "original_form": "선수들은 그렇지 않을까?",
          "speaker_id": "2",
          "time": "20210823 04:12"
        },
        {
          "id": "MDRW2100009691.1.14",
          "form": "오히려 체지방율이 없어서 안 흔들릴것 같은데요",
          "original_form": "오히려 체지방율이 없어서 안 흔들릴것 같은데요",
          "speaker_id": "1",
          "time": "20210823 04:12"
        },
        {
          "id": "MDRW2100009691.1.15",
          "form": "내 뱃살은 흔들리니까 그렇게 믿을래",
          "original_form": "내 뱃살은 흔들리니까 그렇게 믿을래",
          "speaker_id": "2",
          "time": "20210823 04:13"
        },
        {
          "id": "MDRW2100009691.1.16",
          "form": "전형적인 자기 합리화네요",
          "original_form": "전형적인 자기 합리화네요",
          "speaker_id": "1",
          "time": "20210823 04:13"
        },
        {
          "id": "MDRW2100009691.1.17",
          "form": "간단한 홈트 기구라도 사서하세요",
          "original_form": "간단한 홈트 기구라도 사서하세요",
          "speaker_id": "1",
          "time": "20210823 04:13"
        },
        {
          "id": "MDRW2100009691.1.18",
          "form": "옷걸이라면 이미 많아",
          "original_form": "옷걸이라면 이미 많아",
          "speaker_id": "2",
          "time": "20210823 04:13"
        },
        {
          "id": "MDRW2100009691.1.19",
          "form": "먼지도 조금 많지만",
          "original_form": "먼지도 조금 많지만",
          "speaker_id": "2",
          "time": "20210823 04:13"
        }
      ]
