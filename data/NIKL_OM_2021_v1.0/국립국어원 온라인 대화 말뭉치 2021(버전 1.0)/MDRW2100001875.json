      "utterance": [
        {
          "id": "MDRW2100001875.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210819 21:52"
        },
        {
          "id": "MDRW2100001875.1.2",
          "form": "저는 최근에 가을옷 샀어요ㅎ",
          "original_form": "저는 최근에 가을옷 샀어요ㅎ",
          "speaker_id": "2",
          "time": "20210819 21:52"
        },
        {
          "id": "MDRW2100001875.1.3",
          "form": "오~ 이제 정말 가을이 왔으니",
          "original_form": "오~ 이제 정말 가을이 왔으니",
          "speaker_id": "1",
          "time": "20210819 21:54"
        },
        {
          "id": "MDRW2100001875.1.4",
          "form": "옷을 준비해야겠네용",
          "original_form": "옷을 준비해야겠네용",
          "speaker_id": "1",
          "time": "20210819 21:54"
        },
        {
          "id": "MDRW2100001875.1.5",
          "form": "갑자기 날씨가 선선해져서 샀어요ㅎ",
          "original_form": "갑자기 날씨가 선선해져서 샀어요ㅎ",
          "speaker_id": "2",
          "time": "20210819 21:55"
        },
        {
          "id": "MDRW2100001875.1.6",
          "form": "최근에 관심있는 물건있으신가요?",
          "original_form": "최근에 관심있는 물건있으신가요?",
          "speaker_id": "2",
          "time": "20210819 21:55"
        },
        {
          "id": "MDRW2100001875.1.7",
          "form": "휴대폰 갖고픈데 관심만ㅋ",
          "original_form": "휴대폰 갖고픈데 관심만ㅋ",
          "speaker_id": "1",
          "time": "20210819 21:55"
        },
        {
          "id": "MDRW2100001875.1.8",
          "form": "휴대폰도 신상은 다 갖고싶어요ㅎ",
          "original_form": "휴대폰도 신상은 다 갖고싶어요ㅎ",
          "speaker_id": "2",
          "time": "20210819 21:56"
        },
        {
          "id": "MDRW2100001875.1.9",
          "form": "접었다폈다ㅡ하고싶어요ㅡㅜㅠ",
          "original_form": "접었다폈다ㅡ하고싶어요ㅡㅜㅠ",
          "speaker_id": "1",
          "time": "20210819 21:57"
        },
        {
          "id": "MDRW2100001875.1.10",
          "form": "저도 보고나니 사고싶더라구요\r\n접었다폈다ㅜㅋㅋ",
          "original_form": "저도 보고나니 사고싶더라구요\r\n접었다폈다ㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:58"
        },
        {
          "id": "MDRW2100001875.1.11",
          "form": "그쳥ㅋㅋ저2년 넘었는데 너무비싸서 못사고있어요ㅠㅠ",
          "original_form": "그쳥ㅋㅋ저2년 넘었는데 너무비싸서 못사고있어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 22:04"
        },
        {
          "id": "MDRW2100001875.1.12",
          "form": "휴대폰이 너무 비싸요ㅜㅜ",
          "original_form": "휴대폰이 너무 비싸요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210819 22:05"
        },
        {
          "id": "MDRW2100001875.1.13",
          "form": "ㅠㅠ 매번 좋은게 나오니ㅎㅎ 사고싶은데 현실은..ㅠ",
          "original_form": "ㅠㅠ 매번 좋은게 나오니ㅎㅎ 사고싶은데 현실은..ㅠ",
          "speaker_id": "1",
          "time": "20210819 22:06"
        },
        {
          "id": "MDRW2100001875.1.14",
          "form": "2년정도쓰니 바꿔달라고 신호를 보내더라구요ㅜㅜ",
          "original_form": "2년정도쓰니 바꿔달라고 신호를 보내더라구요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210819 22:07"
        },
        {
          "id": "MDRW2100001875.1.15",
          "form": "저도 신호를 보내는데ㅠㅠ무시하고있어요",
          "original_form": "저도 신호를 보내는데ㅠㅠ무시하고있어요",
          "speaker_id": "1",
          "time": "20210819 22:10"
        },
        {
          "id": "MDRW2100001875.1.16",
