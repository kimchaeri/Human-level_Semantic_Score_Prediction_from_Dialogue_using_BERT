      "utterance": [
        {
          "id": "MDRW2100032633.1.1",
          "form": "본가에 살다보니 할일이 잘 없어요",
          "original_form": "본가에 살다보니 할일이 잘 없어요",
          "speaker_id": "2",
          "time": "20210906 06:45"
        },
        {
          "id": "MDRW2100032633.1.2",
          "form": "그래서 못하는 ㅜ.ㅜ\r\nname1님은요?",
          "original_form": "그래서 못하는 ㅜ.ㅜ\r\n&name1&님은요?",
          "speaker_id": "2",
          "time": "20210906 06:45"
        },
        {
          "id": "MDRW2100032633.1.3",
          "form": "저도 엄마가 해주셔서 ㅎㅎ",
          "original_form": "저도 엄마가 해주셔서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 11:49"
        },
        {
          "id": "MDRW2100032633.1.4",
          "form": "집밥이 제일 맛있어요",
          "original_form": "집밥이 제일 맛있어요",
          "speaker_id": "1",
          "time": "20210906 11:49"
        },
        {
          "id": "MDRW2100032633.1.5",
          "form": "그쵸? 배달음식은 곧잘 질려요",
          "original_form": "그쵸? 배달음식은 곧잘 질려요",
          "speaker_id": "2",
          "time": "20210906 11:52"
        },
        {
          "id": "MDRW2100032633.1.6",
          "form": "맞어요 ㅜㅜㅜ 가끔 귀찮을때 시키는데",
          "original_form": "맞어요 ㅜㅜㅜ 가끔 귀찮을때 시키는데",
          "speaker_id": "1",
          "time": "20210906 11:53"
        },
        {
          "id": "MDRW2100032633.1.7",
          "form": "배달비도 비싸구 ㅠㅜ",
          "original_form": "배달비도 비싸구 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210906 11:53"
        },
        {
          "id": "MDRW2100032633.1.8",
          "form": "그쵸? 요즘 배달팁도 비싸요",
          "original_form": "그쵸? 요즘 배달팁도 비싸요",
          "speaker_id": "2",
          "time": "20210906 11:54"
        },
        {
          "id": "MDRW2100032633.1.9",
          "form": "닭발 먹고싶네요 ㅠㅠ",
          "original_form": "닭발 먹고싶네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 11:56"
        },
        {
          "id": "MDRW2100032633.1.10",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 11:56"
        },
        {
          "id": "MDRW2100032633.1.11",
          "form": "닭발은 쏘쏘예요ㅋㅋㅋ 양에 비해 가격이 좀 있더라구요",
          "original_form": "닭발은 쏘쏘예요ㅋㅋㅋ 양에 비해 가격이 좀 있더라구요",
          "speaker_id": "2",
          "time": "20210906 11:58"
        },
        {
          "id": "MDRW2100032633.1.12",
          "form": "맞아여 ㅠㅠ",
          "original_form": "맞아여 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 11:59"
        },
        {
          "id": "MDRW2100032633.1.13",
          "form": "계란찜 좋아하세요?",
          "original_form": "계란찜 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 11:59"
        },
        {
          "id": "MDRW2100032633.1.14",
          "form": "좋아하죠! 어제도 오늘도 먹었어요",
          "original_form": "좋아하죠! 어제도 오늘도 먹었어요",
          "speaker_id": "2",
          "time": "20210906 12:07"
        },
        {
          "id": "MDRW2100032633.1.15",
          "form": "우아~~ 저는 계란 너무 좋아해요",
          "original_form": "우아~~ 저는 계란 너무 좋아해요",
