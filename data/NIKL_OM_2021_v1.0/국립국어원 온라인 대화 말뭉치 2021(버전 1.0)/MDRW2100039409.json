      "utterance": [
        {
          "id": "MDRW2100039409.1.1",
          "form": "ㅇㅇ 예전에 걸어간거 기억하나오",
          "original_form": "ㅇㅇ 예전에 걸어간거 기억하나오",
          "speaker_id": "2",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100039409.1.2",
          "form": "광사에서",
          "original_form": "광사에서",
          "speaker_id": "2",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100039409.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100039409.1.4",
          "form": "기억합니다",
          "original_form": "기억합니다",
          "speaker_id": "1",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100039409.1.5",
          "form": "그때는 젋었죠",
          "original_form": "그때는 젋었죠",
          "speaker_id": "2",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100039409.1.6",
          "form": "걸어서 노브랜드까지",
          "original_form": "걸어서 노브랜드까지",
          "speaker_id": "1",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100039409.1.7",
          "form": "지금도 젊어오",
          "original_form": "지금도 젊어오",
          "speaker_id": "1",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100039409.1.8",
          "form": "쇼핑을 위해서라면 어디든 갈 수 있어오",
          "original_form": "쇼핑을 위해서라면 어디든 갈 수 있어오",
          "speaker_id": "1",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100039409.1.9",
          "form": "요새 마트 자주감??",
          "original_form": "요새 마트 자주감??",
          "speaker_id": "1",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100039409.1.10",
          "form": "아뇨 새마을 시장 갑니댜",
          "original_form": "아뇨 새마을 시장 갑니댜",
          "speaker_id": "2",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100039409.1.11",
          "form": "멀어...",
          "original_form": "멀어...",
          "speaker_id": "2",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100039409.1.12",
          "form": "킴스 곧 문닫으니 이번주 가구ㅋㅋㅋ",
          "original_form": "킴스 곧 문닫으니 이번주 가구ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039409.1.13",
          "form": "오 새마을 시장 요새 어때???",
          "original_form": "오 새마을 시장 요새 어때???",
          "speaker_id": "1",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039409.1.14",
          "form": "리뉴얼 됨???",
          "original_form": "리뉴얼 됨???",
          "speaker_id": "1",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039409.1.15",
          "form": "앗 고별전 문자오더니",
          "original_form": "앗 고별전 문자오더니",
          "speaker_id": "1",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039409.1.16",
          "form": "진짜 문닫는구나",
          "original_form": "진짜 문닫는구나",
          "speaker_id": "1",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039409.1.17",
          "form": "혹시 이천일 아울렛 전체 폐점이야???",
          "original_form": "혹시 이천일 아울렛 전체 폐점이야???",
          "speaker_id": "1",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100039409.1.18",
          "form": "ㄴㄴ 시장 그대로 인거같ㅋㅋ",
          "original_form": "ㄴㄴ 시장 그대로 인거같ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100039409.1.19",
          "form": "ㅇㅇ 문닫고 뭐 주상복합이니 뭐니 들어온다는데",
          "original_form": "ㅇㅇ 문닫고 뭐 주상복합이니 뭐니 들어온다는데",
          "speaker_id": "2",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100039409.1.20",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100039409.1.21",
          "form": "일단 안에 다 빠지는지 세일 스티커 다 붙여놓음",
          "original_form": "일단 안에 다 빠지는지 세일 스티커 다 붙여놓음",
          "speaker_id": "2",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100039409.1.22",
          "form": "그냥 이천일 아울렛 하지 아쉽",
          "original_form": "그냥 이천일 아울렛 하지 아쉽",
          "speaker_id": "1",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100039409.1.23",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "1",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100039409.1.24",
          "form": "세일 많이 함???",
          "original_form": "세일 많이 함???",
          "speaker_id": "1",
          "time": "20210903 17:22"
        },
        {
          "id": "MDRW2100039409.1.25",
          "form": "30퍼 세일 많이 하는거 같은데 식품보다는",
          "original_form": "30퍼 세일 많이 하는거 같은데 식품보다는",
          "speaker_id": "2",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100039409.1.26",
          "form": "1층이 난리ㅋㅋ 옷이며 운동화며ㅋㅋ",
          "original_form": "1층이 난리ㅋㅋ 옷이며 운동화며ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100039409.1.27",
          "form": "할인 되게 많이 해주네 ㄷㄷㄷ",
          "original_form": "할인 되게 많이 해주네 ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100039409.1.28",
          "form": "지를 타이밍이다....",
          "original_form": "지를 타이밍이다....",
          "speaker_id": "1",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100039409.1.29",
          "form": "브랜드 많아서 건질 거 많을 거 같은데",
          "original_form": "브랜드 많아서 건질 거 많을 거 같은데",
          "speaker_id": "1",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100039409.1.30",
          "form": "헐 오나오",
          "original_form": "헐 오나오",
          "speaker_id": "2",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100039409.1.31",
          "form": "2층도 세일 하긴 하더라",
          "original_form": "2층도 세일 하긴 하더라",
          "speaker_id": "2",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100039409.1.32",
          "form": "가고싶은데 못갈 거 같아",
          "original_form": "가고싶은데 못갈 거 같아",
          "speaker_id": "1",
          "time": "20210903 17:30"
        },
        {
          "id": "MDRW2100039409.1.33",
          "form": "가을옷 장만할 수 있겠다 ㅋㅋㅋ",
          "original_form": "가을옷 장만할 수 있겠다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:30"
        },
        {
          "id": "MDRW2100039409.1.34",
          "form": "역시 일본 가야 살 수 있는건가",
          "original_form": "역시 일본 가야 살 수 있는건가",
          "speaker_id": "1",
          "time": "20210903 19:19"
        }
      ]
