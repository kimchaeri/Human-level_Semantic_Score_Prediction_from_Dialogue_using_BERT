      "utterance": [
        {
          "id": "MDRW2100039146.1.1",
          "form": "점심시간이다ㅋㅋ",
          "original_form": "점심시간이다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100039146.1.2",
          "form": "오늘도 바쁘나ㅋㅋ",
          "original_form": "오늘도 바쁘나ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100039146.1.3",
          "form": "오늘은 좀 낫네ㅋㅋㅋㅋ",
          "original_form": "오늘은 좀 낫네ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:54"
        },
        {
          "id": "MDRW2100039146.1.4",
          "form": "다행이네",
          "original_form": "다행이네",
          "speaker_id": "2",
          "time": "20210903 11:54"
        },
        {
          "id": "MDRW2100039146.1.5",
          "form": "어젠 바쁜데 손님도 진짜 많이 왔음ㅠ",
          "original_form": "어젠 바쁜데 손님도 진짜 많이 왔음ㅠ",
          "speaker_id": "1",
          "time": "20210903 11:54"
        },
        {
          "id": "MDRW2100039146.1.6",
          "form": "오늘은 비와서",
          "original_form": "오늘은 비와서",
          "speaker_id": "2",
          "time": "20210903 11:54"
        },
        {
          "id": "MDRW2100039146.1.7",
          "form": "오늘은 비 오니까 없네ㅋㅋㅋ",
          "original_form": "오늘은 비 오니까 없네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:54"
        },
        {
          "id": "MDRW2100039146.1.8",
          "form": "덜오는거아이가",
          "original_form": "덜오는거아이가",
          "speaker_id": "2",
          "time": "20210903 11:54"
        },
        {
          "id": "MDRW2100039146.1.9",
          "form": "그러이ㅋㅋ",
          "original_form": "그러이ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:54"
        },
        {
          "id": "MDRW2100039146.1.10",
          "form": "좋다 좋다ㅋㅋㅋ",
          "original_form": "좋다 좋다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:54"
        },
        {
          "id": "MDRW2100039146.1.11",
          "form": "태풍 아 불어라ㅋㅋㅋ",
          "original_form": "태풍 아 불어라ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:54"
        },
        {
          "id": "MDRW2100039146.1.12",
          "form": "ㅋㅋㅋㅋㅋ웃겨ㅋㅋㅋ",
