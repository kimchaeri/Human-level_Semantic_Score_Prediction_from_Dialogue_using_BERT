      "utterance": [
        {
          "id": "MDRW2100036425.1.1",
          "form": "진짜 솔직히 말하면",
          "original_form": "진짜 솔직히 말하면",
          "speaker_id": "1",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100036425.1.2",
          "form": "오..",
          "original_form": "오..",
          "speaker_id": "2",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100036425.1.3",
          "form": "맥주보다는",
          "original_form": "맥주보다는",
          "speaker_id": "1",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100036425.1.4",
          "form": "좀 맛이 떨어지긴하는데",
          "original_form": "좀 맛이 떨어지긴하는데",
          "speaker_id": "1",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100036425.1.5",
          "form": "보리차 탄산..?",
          "original_form": "보리차 탄산..?",
          "speaker_id": "2",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100036425.1.6",
          "form": "그래도 대체용으로는 나쁘지않다~",
          "original_form": "그래도 대체용으로는 나쁘지않다~",
          "speaker_id": "1",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100036425.1.7",
          "form": "싶어요ㅋㅋㅋ",
          "original_form": "싶어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100036425.1.8",
          "form": "보리차 탄산ㅋㅋㅋㅋ",
          "original_form": "보리차 탄산ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100036425.1.9",
          "form": "고정도까진 아니고 그것보다는 맛있어요!",
          "original_form": "고정도까진 아니고 그것보다는 맛있어요!",
          "speaker_id": "1",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100036425.1.10",
          "form": "아 그런가요 ㅋㅋㅋㅋ",
          "original_form": "아 그런가요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100036425.1.11",
          "form": "함 먹어봐야겠네요",
          "original_form": "함 먹어봐야겠네요",
          "speaker_id": "2",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100036425.1.12",
          "form": "탄산 보리차 맛일줄 알고 안먹어봣어요",
          "original_form": "탄산 보리차 맛일줄 알고 안먹어봣어요",
          "speaker_id": "2",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100036425.1.13",
          "form": "카스같이 라거류를 그닥 좋아하지않아서",
          "original_form": "카스같이 라거류를 그닥 좋아하지않아서",
          "speaker_id": "2",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100036425.1.14",
          "form": "드셔보세용! 안취하는데 맥주맛 나서 약간 이상합니다ㅋㅋㅋ",
          "original_form": "드셔보세용! 안취하는데 맥주맛 나서 약간 이상합니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100036425.1.15",
          "form": "잌ㅋㅋㅋ 궁금하네요 안취하는맥주",
          "original_form": "잌ㅋㅋㅋ 궁금하네요 안취하는맥주",
          "speaker_id": "2",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100036425.1.16",
          "form": "어쩌다 무알콜맥주를 드셧어요?",
          "original_form": "어쩌다 무알콜맥주를 드셧어요?",
          "speaker_id": "2",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100036425.1.17",
          "form": "그거 먹는사람 거의없는거같던데",
          "original_form": "그거 먹는사람 거의없는거같던데",
          "speaker_id": "2",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100036425.1.18",
          "form": "아 제가 원래는",
          "original_form": "아 제가 원래는",
          "speaker_id": "1",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100036425.1.19",
          "form": "술을 제일 잘 마셨을때는",
          "original_form": "술을 제일 잘 마셨을때는",
          "speaker_id": "1",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100036425.1.20",
          "form": "소주 두병까지도 마셨었거든요",
          "original_form": "소주 두병까지도 마셨었거든요",
          "speaker_id": "1",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100036425.1.21",
          "form": "오 주당이셧네요",
          "original_form": "오 주당이셧네요",
          "speaker_id": "2",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100036425.1.22",
          "form": "근데 점점 이것도 안마시고 거의 일년에 한두번 먹기 시작하더니",
          "original_form": "근데 점점 이것도 안마시고 거의 일년에 한두번 먹기 시작하더니",
          "speaker_id": "1",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100036425.1.23",
          "form": "아 맞아요 맞아요",
          "original_form": "아 맞아요 맞아요",
          "speaker_id": "2",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100036425.1.24",
          "form": "맥주 두캔에도 취하려고 하더라구요ㅋㅋㅋㅋ",
          "original_form": "맥주 두캔에도 취하려고 하더라구요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100036425.1.25",
          "form": "나이들수록 술 취하는것도 피곤하고..",
          "original_form": "나이들수록 술 취하는것도 피곤하고..",
          "speaker_id": "2",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100036425.1.26",
          "form": "조금만 마셔도 알딸딸해요 ㅋㅋㅋ",
          "original_form": "조금만 마셔도 알딸딸해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:50"
        },
        {
          "id": "MDRW2100036425.1.27",
          "form": "그래서 음 가볍게만 즐기고싶은데 뭐 없을까 하다가 알게되었어요!",
          "original_form": "그래서 음 가볍게만 즐기고싶은데 뭐 없을까 하다가 알게되었어요!",
          "speaker_id": "1",
          "time": "20210830 22:50"
        },
        {
          "id": "MDRW2100036425.1.28",
          "form": "네네ㅠㅠ",
          "original_form": "네네ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 22:50"
        },
        {
          "id": "MDRW2100036425.1.29",
          "form": "취한몸 이끄는게ㅋㅋㅋㅋ",
          "original_form": "취한몸 이끄는게ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:50"
        }
      ]
    }
