      "utterance": [
        {
          "id": "MDRW2100040919.1.1",
          "form": "게임얘기에이어,",
          "original_form": "게임얘기에이어,",
          "speaker_id": "1",
          "time": "20210905 12:32"
        },
        {
          "id": "MDRW2100040919.1.2",
          "form": "최근에 구매하신거있으셔용",
          "original_form": "최근에 구매하신거있으셔용",
          "speaker_id": "1",
          "time": "20210905 12:32"
        },
        {
          "id": "MDRW2100040919.1.3",
          "form": "와플메이커 구매해서 열심히 떡 구워먹고 있지요",
          "original_form": "와플메이커 구매해서 열심히 떡 구워먹고 있지요",
          "speaker_id": "2",
          "time": "20210905 13:27"
        },
        {
          "id": "MDRW2100040919.1.4",
          "form": "와 대세템 구매하셨네요ㅋㅋ \r\n사용하긴편한가용?",
          "original_form": "와 대세템 구매하셨네요ㅋㅋ \r\n사용하긴편한가용?",
          "speaker_id": "1",
          "time": "20210905 14:18"
        },
        {
          "id": "MDRW2100040919.1.5",
          "form": "저는 전기가 아니라 가스불이용하는 걸로 사서 그런지 좋아요",
          "original_form": "저는 전기가 아니라 가스불이용하는 걸로 사서 그런지 좋아요",
          "speaker_id": "2",
          "time": "20210905 16:56"
        },
        {
          "id": "MDRW2100040919.1.6",
          "form": "씻기도 편하고요",
          "original_form": "씻기도 편하고요",
          "speaker_id": "2",
          "time": "20210905 16:56"
        },
        {
          "id": "MDRW2100040919.1.7",
          "form": "생선구이팬이랑 비슷해요",
          "original_form": "생선구이팬이랑 비슷해요",
          "speaker_id": "2",
          "time": "20210905 16:57"
        },
        {
          "id": "MDRW2100040919.1.8",
          "form": "오 가스불이용이더편할거같긴한데 조절하시기 힘들진않으셔용?",
          "original_form": "오 가스불이용이더편할거같긴한데 조절하시기 힘들진않으셔용?",
          "speaker_id": "1",
          "time": "20210905 18:01"
        },
        {
          "id": "MDRW2100040919.1.9",
          "form": "글쎼요.. 생선구이도 해봤기 때문에 어렵다고는 안느껴지네요",
          "original_form": "글쎼요.. 생선구이도 해봤기 때문에 어렵다고는 안느껴지네요",
          "speaker_id": "2",
          "time": "20210905 20:01"
        },
        {
          "id": "MDRW2100040919.1.10",
          "form": "아하.. 역시 다른거 해보신분이 잘하시네요ㅎㅁㅎ",
          "original_form": "아하.. 역시 다른거 해보신분이 잘하시네요ㅎㅁㅎ",
          "speaker_id": "1",
          "time": "20210905 20:04"
        },
        {
          "id": "MDRW2100040919.1.11",
          "form": "전기연결 와플팬은 세척이 어렵다네요",
          "original_form": "전기연결 와플팬은 세척이 어렵다네요",
          "speaker_id": "2",
          "time": "20210905 20:18"
        },
        {
          "id": "MDRW2100040919.1.12",
          "form": "비싸고",
          "original_form": "비싸고",
          "speaker_id": "2",
          "time": "20210905 20:18"
        },
        {
          "id": "MDRW2100040919.1.13",
          "form": "네 저도 한번본적있는데 가격이 꽤하더라구요ㅎㅎ\r\n세척까지생각해보지않앗는데\r\n완전중요한건데...!!",
          "original_form": "네 저도 한번본적있는데 가격이 꽤하더라구요ㅎㅎ\r\n세척까지생각해보지않앗는데\r\n완전중요한건데...!!",
          "speaker_id": "1",
          "time": "20210905 20:28"
        },
        {
          "id": "MDRW2100040919.1.14",
          "form": "그래서 당근마켓에 중고로 종종 나와요",
          "original_form": "그래서 당근마켓에 중고로 종종 나와요",
          "speaker_id": "2",
          "time": "20210905 21:03"
        },
        {
          "id": "MDRW2100040919.1.15",
          "form": "우리집 것은 팬처럼 생긴거라서 세척도 아주 쉬워서 좋아요",
          "original_form": "우리집 것은 팬처럼 생긴거라서 세척도 아주 쉬워서 좋아요",
          "speaker_id": "2",
          "time": "20210905 21:03"
        },
        {
          "id": "MDRW2100040919.1.16",
          "form": "그게훨씬좋겠네용ㅋㅋㅋ잘고르셨네용\r\n아 당근에도 꽤올라오겠네요",
          "original_form": "그게훨씬좋겠네용ㅋㅋㅋ잘고르셨네용\r\n아 당근에도 꽤올라오겠네요",
          "speaker_id": "1",
          "time": "20210905 21:23"
        },
        {
          "id": "MDRW2100040919.1.17",
          "form": "굳이 비싼 거 살 필요 없어요",
          "original_form": "굳이 비싼 거 살 필요 없어요",
          "speaker_id": "2",
          "time": "20210905 21:26"
        },
        {
          "id": "MDRW2100040919.1.18",
          "form": "일단 팬형식으로 사도 되고",
          "original_form": "일단 팬형식으로 사도 되고",
          "speaker_id": "2",
          "time": "20210905 21:26"
        },
        {
          "id": "MDRW2100040919.1.19",
          "form": "전기제품은 중고도 도전할만해요",
          "original_form": "전기제품은 중고도 도전할만해요",
          "speaker_id": "2",
          "time": "20210905 21:26"
        },
        {
          "id": "MDRW2100040919.1.20",
          "form": "사람들이 몇번 안쓰고 넣어놓게 된다고 하니까요",
          "original_form": "사람들이 몇번 안쓰고 넣어놓게 된다고 하니까요",
          "speaker_id": "2",
          "time": "20210905 21:27"
        },
        {
          "id": "MDRW2100040919.1.21",
          "form": "뭔가 잘 몰라가지고 선뜻 중고로 사기가 두렵더라구요ㅎㅎ",
          "original_form": "뭔가 잘 몰라가지고 선뜻 중고로 사기가 두렵더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 21:29"
        }
      ]
    }
