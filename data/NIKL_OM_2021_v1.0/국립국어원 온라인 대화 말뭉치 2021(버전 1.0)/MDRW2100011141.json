        {
          "id": "MDRW2100011141.1.1",
          "form": "ㅋㅋ쇼핑 좋아하세요~?",
          "original_form": "ㅋㅋ쇼핑 좋아하세요~?",
          "speaker_id": "2",
          "time": "20210824 01:39"
        },
        {
          "id": "MDRW2100011141.1.2",
          "form": "옷산지는 좀 오래고 요즘은 주로 먹을거사요",
          "original_form": "옷산지는 좀 오래고 요즘은 주로 먹을거사요",
          "speaker_id": "1",
          "time": "20210824 01:39"
        },
        {
          "id": "MDRW2100011141.1.3",
          "form": "남자분들은 주로 뭐사시나요?ㅎ",
          "original_form": "남자분들은 주로 뭐사시나요?ㅎ",
          "speaker_id": "1",
          "time": "20210824 01:40"
        },
        {
          "id": "MDRW2100011141.1.4",
          "form": "저도 식료품 보통ㅋㅋ",
          "original_form": "저도 식료품 보통ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 01:43"
        },
        {
          "id": "MDRW2100011141.1.5",
          "form": "할인한다 싶으면 사요 보톳",
          "original_form": "할인한다 싶으면 사요 보톳",
          "speaker_id": "2",
          "time": "20210824 01:43"
        },
        {
          "id": "MDRW2100011141.1.6",
          "form": "통",
          "original_form": "통",
          "speaker_id": "2",
          "time": "20210824 01:43"
        },
        {
          "id": "MDRW2100011141.1.7",
          "form": "아 와이프분이 안사시고요?",
          "original_form": "아 와이프분이 안사시고요?",
          "speaker_id": "1",
          "time": "20210824 01:43"
        },
        {
          "id": "MDRW2100011141.1.8",
          "form": "음 카드실적 채워야되는 사람시",
          "original_form": "음 카드실적 채워야되는 사람시",
          "speaker_id": "2",
          "time": "20210824 01:46"
        },
        {
          "id": "MDRW2100011141.1.9",
          "form": "이 사요 ㅎㅎ",
          "original_form": "이 사요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 01:46"
        },
        {
          "id": "MDRW2100011141.1.10",
          "form": "아하ㅎ 그런이유가",
          "original_form": "아하ㅎ 그런이유가",
          "speaker_id": "1",
          "time": "20210824 01:47"
        },
        {
          "id": "MDRW2100011141.1.11",
          "form": "주로 어디서 사세요?",
          "original_form": "주로 어디서 사세요?",
          "speaker_id": "1",
          "time": "20210824 01:47"
        },
        {
          "id": "MDRW2100011141.1.12",
          "form": "11번가랑 지마켓이요 보통",
          "original_form": "11번가랑 지마켓이요 보통",
          "speaker_id": "2",
          "time": "20210824 01:48"
        },
        {
          "id": "MDRW2100011141.1.13",
          "form": "근데 특가 뜨는곳",
          "original_form": "근데 특가 뜨는곳",
          "speaker_id": "2",
          "time": "20210824 01:48"
        },
        {
          "id": "MDRW2100011141.1.14",
          "form": "위주인거 같아요 ㅎㅎ",
          "original_form": "위주인거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 01:48"
        },
        {
          "id": "MDRW2100011141.1.15",
          "form": "할인정보는 어디서 ㅎ 그냥 사이트들어가세요?",
          "original_form": "할인정보는 어디서 ㅎ 그냥 사이트들어가세요?",
          "speaker_id": "1",
          "time": "20210824 01:49"
        },
        {
          "id": "MDRW2100011141.1.16",
          "form": "네 가끔 들어가서 보다가",
          "original_form": "네 가끔 들어가서 보다가",
          "speaker_id": "2",
          "time": "20210824 01:50"
        },
        {
          "id": "MDRW2100011141.1.17",
          "form": "어?하면 사요ㅋㅋ",
          "original_form": "어?하면 사요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 01:50"
        },
        {
          "id": "MDRW2100011141.1.18",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210824 01:50"
        },
        {
          "id": "MDRW2100011141.1.19",
          "form": "아 전 카페에 정보뜨거나 카톡알림올때 핫딜있음 사요ㅎ",
          "original_form": "아 전 카페에 정보뜨거나 카톡알림올때 핫딜있음 사요ㅎ",
          "speaker_id": "1",
          "time": "20210824 01:51"
        },
        {
          "id": "MDRW2100011141.1.20",
          "form": "신규가입 이벤트로도 사고",
          "original_form": "신규가입 이벤트로도 사고",
          "speaker_id": "1",
          "time": "20210824 01:51"
        },
        {
          "id": "MDRW2100011141.1.21",
          "form": "쿠폰안줌 이제 장 못보겠더라구요",
          "original_form": "쿠폰안줌 이제 장 못보겠더라구요",
          "speaker_id": "1",
          "time": "20210824 01:52"
        },
        {
          "id": "MDRW2100011141.1.22",
          "form": "맞아요 물가가 흑흑",
          "original_form": "맞아요 물가가 흑흑",
          "speaker_id": "2",
          "time": "20210824 01:53"
        },
        {
          "id": "MDRW2100011141.1.23",
          "form": "계란은 손떨려서 못사겠던데요ㅎ",
          "original_form": "계란은 손떨려서 못사겠던데요ㅎ",
          "speaker_id": "1",
          "time": "20210824 01:54"
        },
        {
          "id": "MDRW2100011141.1.24",
          "form": "금계란",
