      "utterance": [
        {
          "id": "MDRW2100019790.1.1",
          "form": "전참시 보시나요?",
          "original_form": "전참시 보시나요?",
          "speaker_id": "1",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100019790.1.2",
          "form": "요즘도 이영자 나오는 지 모르겠는데",
          "original_form": "요즘도 이영자 나오는 지 모르겠는데",
          "speaker_id": "2",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100019790.1.3",
          "form": "예전에 이영자 때문에 한동안 꾸준히 봤어요 ㅋㅋ",
          "original_form": "예전에 이영자 때문에 한동안 꾸준히 봤어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100019790.1.4",
          "form": "요즘도 나오더라고요ㅋㅋ",
          "original_form": "요즘도 나오더라고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:29"
        },
        {
          "id": "MDRW2100019790.1.5",
          "form": "재밌어요",
          "original_form": "재밌어요",
          "speaker_id": "1",
          "time": "20210827 22:29"
        },
        {
          "id": "MDRW2100019790.1.6",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "2",
          "time": "20210828 10:21"
        },
        {
          "id": "MDRW2100019790.1.7",
          "form": "이영자 먹는 거에 진심인 것 같아요 ㅋㅋ",
          "original_form": "이영자 먹는 거에 진심인 것 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:21"
        },
        {
          "id": "MDRW2100019790.1.8",
          "form": "넹ㅋㅋ 맞아요",
          "original_form": "넹ㅋㅋ 맞아요",
          "speaker_id": "1",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100019790.1.9",
          "form": "말하는거 듣고 있으면 저도모르게 집중이!",
          "original_form": "말하는거 듣고 있으면 저도모르게 집중이!",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100019790.1.10",
          "form": "그 뒤로 소떡소떡이랑 명랑핫도그 등등 불티나게 팔렸다잖아여 ㅎㅎ",
          "original_form": "그 뒤로 소떡소떡이랑 명랑핫도그 등등 불티나게 팔렸다잖아여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100019790.1.11",
          "form": "어쩜 묘사를 그렇게 찰지게 잘하시는 지...",
          "original_form": "어쩜 묘사를 그렇게 찰지게 잘하시는 지...",
          "speaker_id": "2",
          "time": "20210828 10:56"
        },
        {
          "id": "MDRW2100019790.1.12",
          "form": "맞아요 그것도 진짜 능력이다 싶더라구요",
          "original_form": "맞아요 그것도 진짜 능력이다 싶더라구요",
          "speaker_id": "1",
          "time": "20210828 10:56"
        },
        {
          "id": "MDRW2100019790.1.13",
          "form": "인터넷에 이영자맛집 치면 리스트 쫙 나오더라구요 ㅎㅎ",
          "original_form": "인터넷에 이영자맛집 치면 리스트 쫙 나오더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 11:01"
        },
        {
          "id": "MDRW2100019790.1.14",
          "form": "근데 코시국이라 가본 적이 없네요 ㅠㅠ",
          "original_form": "근데 코시국이라 가본 적이 없네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 11:01"
        },
        {
          "id": "MDRW2100019790.1.15",
          "form": "저도요ㅠㅠ",
          "original_form": "저도요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 11:03"
        },
        {
          "id": "MDRW2100019790.1.16",
          "form": "전참시에 나오는 연예인 중 누구 좋아하세요?",
          "original_form": "전참시에 나오는 연예인 중 누구 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 11:03"
        },
        {
          "id": "MDRW2100019790.1.17",
          "form": "리스트만 따라가도 넘칠거같은데 말이죠",
          "original_form": "리스트만 따라가도 넘칠거같은데 말이죠",
          "speaker_id": "1",
          "time": "20210828 11:03"
        },
        {
          "id": "MDRW2100019790.1.18",
          "form": "저는 전에 박성광 좋았어요!",
          "original_form": "저는 전에 박성광 좋았어요!",
          "speaker_id": "1",
          "time": "20210828 11:04"
        },
        {
          "id": "MDRW2100019790.1.19",
          "form": "ㅋㅋㅋㅋㅋ아 박성광",
          "original_form": "ㅋㅋㅋㅋㅋ아 박성광",
          "speaker_id": "2",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100019790.1.20",
          "form": "그 동글동글 안경 쓰신 분 맞죠",
          "original_form": "그 동글동글 안경 쓰신 분 맞죠",
          "speaker_id": "2",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100019790.1.21",
          "form": "지금은 출연 안해요?",
          "original_form": "지금은 출연 안해요?",
          "speaker_id": "2",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100019790.1.22",
          "form": "네! 저번에 잠깐 나오고 안나오더라구요ㅠ",
          "original_form": "네! 저번에 잠깐 나오고 안나오더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100019790.1.23",
          "form": "매니저도 바뀌고 결혼해서 그런가봐요ㅠ",
          "original_form": "매니저도 바뀌고 결혼해서 그런가봐요ㅠ",
          "speaker_id": "1",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100019790.1.24",
          "form": "ㅠㅠㅠ아 결혼 하셨구나",
          "original_form": "ㅠㅠㅠ아 결혼 하셨구나",
          "speaker_id": "2",
          "time": "20210828 11:06"
        }
      ]
    }
  ]
