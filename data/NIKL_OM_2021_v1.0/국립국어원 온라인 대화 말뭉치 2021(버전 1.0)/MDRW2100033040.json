      "utterance": [
        {
          "id": "MDRW2100033040.1.1",
          "form": "맞아요ㅠ 저희집은 근데 너무 신실해서;;;",
          "original_form": "맞아요ㅠ 저희집은 근데 너무 신실해서;;;",
          "speaker_id": "1",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100033040.1.2",
          "form": "코로나 이후에도 가요...",
          "original_form": "코로나 이후에도 가요...",
          "speaker_id": "1",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100033040.1.3",
          "form": "전에 방역 심했을 때만 안갔고",
          "original_form": "전에 방역 심했을 때만 안갔고",
          "speaker_id": "1",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100033040.1.4",
          "form": "모더나 미국 간 기사보셨어요?",
          "original_form": "모더나 미국 간 기사보셨어요?",
          "speaker_id": "2",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100033040.1.5",
          "form": "스위트룸에 묵어서",
          "original_form": "스위트룸에 묵어서",
          "speaker_id": "2",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100033040.1.6",
          "form": "헐????",
          "original_form": "헐????",
          "speaker_id": "1",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100033040.1.7",
          "form": "아니요!?!",
          "original_form": "아니요!?!",
          "speaker_id": "1",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100033040.1.8",
          "form": "돈 엄청쓰고왔다네요",
          "original_form": "돈 엄청쓰고왔다네요",
          "speaker_id": "2",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100033040.1.9",
          "form": "그리고 갈때 골프가방 가져갔다고",
          "original_form": "그리고 갈때 골프가방 가져갔다고",
          "speaker_id": "2",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100033040.1.10",
          "form": "모더나 백신이요?",
          "original_form": "모더나 백신이요?",
          "speaker_id": "1",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100033040.1.11",
          "form": "모더나가 공급안해주니까",
          "original_form": "모더나가 공급안해주니까",
          "speaker_id": "2",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100033040.1.12",
          "form": "헐 뭐지뭐지",
          "original_form": "헐 뭐지뭐지",
          "speaker_id": "1",
          "time": "20210906 14:30"
        },
        {
