      "utterance": [
        {
          "id": "MDRW2100023415.1.1",
          "form": "넷플 구독하시면 넷플에서 많이 보시겠어용",
          "original_form": "넷플 구독하시면 넷플에서 많이 보시겠어용",
          "speaker_id": "1",
          "time": "20210830 01:50"
        },
        {
          "id": "MDRW2100023415.1.2",
          "form": "아 이제 그만하려구요",
          "original_form": "아 이제 그만하려구요",
          "speaker_id": "2",
          "time": "20210830 01:50"
        },
        {
          "id": "MDRW2100023415.1.3",
          "form": "막상",
          "original_form": "막상",
          "speaker_id": "2",
          "time": "20210830 01:50"
        },
        {
          "id": "MDRW2100023415.1.4",
          "form": "등록하면",
          "original_form": "등록하면",
          "speaker_id": "2",
          "time": "20210830 01:50"
        },
        {
          "id": "MDRW2100023415.1.5",
          "form": "볼게없음",
          "original_form": "볼게없음",
          "speaker_id": "2",
          "time": "20210830 01:50"
        },
        {
          "id": "MDRW2100023415.1.6",
          "form": "다이소같은 느낌입니다",
          "original_form": "다이소같은 느낌입니다",
          "speaker_id": "2",
          "time": "20210830 01:50"
        },
        {
          "id": "MDRW2100023415.1.7",
          "form": "어? 다이소네 뭐있나볼까? 했다가 빈손으로 나오죠",
          "original_form": "어? 다이소네 뭐있나볼까? 했다가 빈손으로 나오죠",
          "speaker_id": "2",
          "time": "20210830 01:50"
        },
        {
          "id": "MDRW2100023415.1.8",
          "form": "넷플릭스도",
          "original_form": "넷플릭스도",
          "speaker_id": "2",
          "time": "20210830 01:50"
        },
        {
          "id": "MDRW2100023415.1.9",
          "form": "마찬가지로 없으면 재밌어보이는거 참 많은데",
          "original_form": "마찬가지로 없으면 재밌어보이는거 참 많은데",
          "speaker_id": "2",
          "time": "20210830 01:51"
        },
        {
          "id": "MDRW2100023415.1.10",
          "form": "결제하고 보려고하면",
          "original_form": "결제하고 보려고하면",
          "speaker_id": "2",
          "time": "20210830 01:51"
        },
        {
          "id": "MDRW2100023415.1.11",
          "form": "다 노잼같아서",
          "original_form": "다 노잼같아서",
          "speaker_id": "2",
          "time": "20210830 01:51"
        },
        {
          "id": "MDRW2100023415.1.12",
          "form": "제가 처음 넷플 한달체험했을때 느꼈던 감정이네요ㅋㅋㅋㄱ",
          "original_form": "제가 처음 넷플 한달체험했을때 느꼈던 감정이네요ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210830 01:51"
        },
        {
          "id": "MDRW2100023415.1.13",
          "form": "생각보다 볼게없다느꼈었어요",
          "original_form": "생각보다 볼게없다느꼈었어요",
          "speaker_id": "1",
          "time": "20210830 01:51"
        },
        {
          "id": "MDRW2100023415.1.14",
          "form": "스파르타쿠스 보셨어요?",
          "original_form": "스파르타쿠스 보셨어요?",
          "speaker_id": "2",
          "time": "20210830 01:51"
        },
        {
          "id": "MDRW2100023415.1.15",
          "form": "이것도 꽤 재밌어요",
          "original_form": "이것도 꽤 재밌어요",
          "speaker_id": "2",
          "time": "20210830 01:51"
        },
        {
          "id": "MDRW2100023415.1.16",
          "form": "starz 에서 방영한건데",
          "original_form": "starz 에서 방영한건데",
          "speaker_id": "2",
          "time": "20210830 01:52"
        },
        {
          "id": "MDRW2100023415.1.17",
          "form": "넷플릭스에 있어요",
          "original_form": "넷플릭스에 있어요",
          "speaker_id": "2",
          "time": "20210830 01:52"
        },
        {
          "id": "MDRW2100023415.1.18",
          "form": "야한장면이나 잔인한장면이 많긴한데요",
          "original_form": "야한장면이나 잔인한장면이 많긴한데요",
          "speaker_id": "2",
          "time": "20210830 01:52"
        },
        {
          "id": "MDRW2100023415.1.19",
          "form": "재밌어요",
          "original_form": "재밌어요",
          "speaker_id": "2",
          "time": "20210830 01:52"
        },
        {
          "id": "MDRW2100023415.1.20",
          "form": "여기서 여러 분과  대화하면서 추천받고있어요ㅋㅋㅋㅋ",
          "original_form": "여기서 여러 분과  대화하면서 추천받고있어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:53"
        },
        {
          "id": "MDRW2100023415.1.21",
          "form": "담에 구독하면보려구",
          "original_form": "담에 구독하면보려구",
          "speaker_id": "1",
          "time": "20210830 01:53"
        },
        {
          "id": "MDRW2100023415.1.22",
          "form": "아 그리고",
          "original_form": "아 그리고",
          "speaker_id": "2",
          "time": "20210830 01:54"
        },
        {
          "id": "MDRW2100023415.1.23",
          "form": "그",
          "original_form": "그",
          "speaker_id": "2",
          "time": "20210830 01:54"
        },
        {
          "id": "MDRW2100023415.1.24",
          "form": "마약카르텔에 관련된",
          "original_form": "마약카르텔에 관련된",
          "speaker_id": "2",
          "time": "20210830 01:54"
        },
        {
          "id": "MDRW2100023415.1.25",
          "form": "드라마도 재밌거든요",
          "original_form": "드라마도 재밌거든요",
          "speaker_id": "2",
          "time": "20210830 01:54"
        },
        {
          "id": "MDRW2100023415.1.26",
          "form": "제가 실화 바탕으로한것들을 좋아해서요",
          "original_form": "제가 실화 바탕으로한것들을 좋아해서요",
          "speaker_id": "2",
          "time": "20210830 01:54"
        },
        {
          "id": "MDRW2100023415.1.27",
          "form": "그게 이름이 뭐더라",
          "original_form": "그게 이름이 뭐더라",
          "speaker_id": "2",
          "time": "20210830 01:54"
        },
        {
          "id": "MDRW2100023415.1.28",
          "form": "아 나르코스",
          "original_form": "아 나르코스",
          "speaker_id": "2",
          "time": "20210830 01:54"
        },
        {
          "id": "MDRW2100023415.1.29",
          "form": "오 이건 진차 첨듣는거예요!",
          "original_form": "오 이건 진차 첨듣는거예요!",
          "speaker_id": "1",
          "time": "20210830 01:55"
        },
        {
          "id": "MDRW2100023415.1.30",
          "form": "콜롬비아에서 마약왕이라고 불리던 파블로 에스코바르 라는 인간이 있었는데요",
          "original_form": "콜롬비아에서 마약왕이라고 불리던 파블로 에스코바르 라는 인간이 있었는데요",
          "speaker_id": "2",
          "time": "20210830 01:56"
        },
        {
          "id": "MDRW2100023415.1.31",
          "form": "추천감사해요 \n저도실화바탕좋아해요!",
          "original_form": "추천감사해요 \n저도실화바탕좋아해요!",
          "speaker_id": "1",
          "time": "20210830 01:56"
        },
        {
          "id": "MDRW2100023415.1.32",
          "form": "이인간이 마약을 팔면서",
          "original_form": "이인간이 마약을 팔면서",
          "speaker_id": "2",
          "time": "20210830 01:56"
        },
        {
          "id": "MDRW2100023415.1.33",
          "form": "부자가되었는데",
          "original_form": "부자가되었는데",
          "speaker_id": "2",
          "time": "20210830 01:56"
        },
        {
          "id": "MDRW2100023415.1.34",
          "form": "돈벌리는게 감당이 안될정도였답니다",
          "original_form": "돈벌리는게 감당이 안될정도였답니다",
          "speaker_id": "2",
          "time": "20210830 01:56"
        },
        {
          "id": "MDRW2100023415.1.35",
          "form": "어우 네네",
          "original_form": "어우 네네",
          "speaker_id": "1",
          "time": "20210830 01:56"
        },
        {
          "id": "MDRW2100023415.1.36",
          "form": "그래서 지고향에 학교랑 병원도 짓고",
          "original_form": "그래서 지고향에 학교랑 병원도 짓고",
          "speaker_id": "2",
          "time": "20210830 01:56"
        },
        {
          "id": "MDRW2100023415.1.37",
          "form": "그래도 돈이 감당이 안되니까",
          "original_form": "그래도 돈이 감당이 안되니까",
          "speaker_id": "2",
          "time": "20210830 01:56"
        },
        {
          "id": "MDRW2100023415.1.38",
          "form": "다발로",
          "original_form": "다발로",
          "speaker_id": "2",
          "time": "20210830 01:57"
        },
        {
          "id": "MDRW2100023415.1.39",
          "form": "고무줄로 묶어서",
          "original_form": "고무줄로 묶어서",
          "speaker_id": "2",
          "time": "20210830 01:57"
        },
        {
          "id": "MDRW2100023415.1.40",
          "form": "땅에 파묻었데요",
          "original_form": "땅에 파묻었데요",
          "speaker_id": "2",
          "time": "20210830 01:57"
        },
        {
          "id": "MDRW2100023415.1.41",
          "form": "네네 ㅇㅁㅇ!",
          "original_form": "네네 ㅇㅁㅇ!",
          "speaker_id": "1",
          "time": "20210830 01:57"
        },
        {
          "id": "MDRW2100023415.1.42",
          "form": "고무줄값만 몇백만원어치 썼다는데",
          "original_form": "고무줄값만 몇백만원어치 썼다는데",
          "speaker_id": "2",
          "time": "20210830 01:57"
        },
        {
          "id": "MDRW2100023415.1.43",
          "form": "이러다가 미국마약범죄국에 걸려서",
          "original_form": "이러다가 미국마약범죄국에 걸려서",
          "speaker_id": "2",
          "time": "20210830 01:57"
        },
        {
          "id": "MDRW2100023415.1.44",
          "form": "총맞고 죽어요",
          "original_form": "총맞고 죽어요",
          "speaker_id": "2",
          "time": "20210830 01:57"
        },
        {
          "id": "MDRW2100023415.1.45",
          "form": "그리고 멕시코 카르텔이 크기시작하는데",
          "original_form": "그리고 멕시코 카르텔이 크기시작하는데",
          "speaker_id": "2",
          "time": "20210830 01:58"
        },
        {
          "id": "MDRW2100023415.1.46",
          "form": "걔네가 미국인 수사관을",
          "original_form": "걔네가 미국인 수사관을",
          "speaker_id": "2",
          "time": "20210830 01:58"
        },
        {
          "id": "MDRW2100023415.1.47",
          "form": "해코지해서",
          "original_form": "해코지해서",
          "speaker_id": "2",
          "time": "20210830 01:58"
        },
        {
          "id": "MDRW2100023415.1.48",
          "form": "죽거든요",
          "original_form": "죽거든요",
          "speaker_id": "2",
          "time": "20210830 01:58"
        },
        {
          "id": "MDRW2100023415.1.49",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210830 01:58"
        },
        {
          "id": "MDRW2100023415.1.50",
          "form": "그래서 미국공권력으로 해체를 시키더라구요",
          "original_form": "그래서 미국공권력으로 해체를 시키더라구요",
          "speaker_id": "2",
          "time": "20210830 01:58"
        },
        {
          "id": "MDRW2100023415.1.51",
          "form": "어찌이렇게줄거리를잘설명해주시지 오오",
          "original_form": "어찌이렇게줄거리를잘설명해주시지 오오",
          "speaker_id": "1",
          "time": "20210830 01:59"
        },
        {
          "id": "MDRW2100023415.1.52",
          "form": "그래서 멕시코에서 그이후로 미국수사관들은 안건든데요",
          "original_form": "그래서 멕시코에서 그이후로 미국수사관들은 안건든데요",
          "speaker_id": "2",
          "time": "20210830 01:59"
        },
        {
          "id": "MDRW2100023415.1.53",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210830 02:00"
        },
        {
          "id": "MDRW2100023415.1.54",
          "form": "그렇게되는거군요",
          "original_form": "그렇게되는거군요",
