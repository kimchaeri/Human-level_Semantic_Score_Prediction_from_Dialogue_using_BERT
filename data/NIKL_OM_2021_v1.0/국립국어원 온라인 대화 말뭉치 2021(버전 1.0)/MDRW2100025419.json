      "utterance": [
        {
          "id": "MDRW2100025419.1.1",
          "form": "요즘 유행하는 곰표맥주",
          "original_form": "요즘 유행하는 곰표맥주",
          "speaker_id": "1",
          "time": "20210901 11:05"
        },
        {
          "id": "MDRW2100025419.1.2",
          "form": "드셔보셨나요?",
          "original_form": "드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210901 11:06"
        },
        {
          "id": "MDRW2100025419.1.3",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210901 11:06"
        },
        {
          "id": "MDRW2100025419.1.4",
          "form": "달달하더라구요",
          "original_form": "달달하더라구요",
          "speaker_id": "2",
          "time": "20210901 11:06"
        },
        {
          "id": "MDRW2100025419.1.5",
          "form": "제가 호가든처럼",
          "original_form": "제가 호가든처럼",
          "speaker_id": "1",
          "time": "20210901 11:07"
        },
        {
          "id": "MDRW2100025419.1.6",
          "form": "밀맥주를 좋아하는데요",
          "original_form": "밀맥주를 좋아하는데요",
          "speaker_id": "1",
          "time": "20210901 11:07"
        },
        {
          "id": "MDRW2100025419.1.7",
          "form": "곰표맥주도 밀맥주여서",
          "original_form": "곰표맥주도 밀맥주여서",
          "speaker_id": "1",
          "time": "20210901 11:07"
        },
        {
          "id": "MDRW2100025419.1.8",
          "form": "마셔보고 얼마나 좋던지몰라요",
          "original_form": "마셔보고 얼마나 좋던지몰라요",
          "speaker_id": "1",
          "time": "20210901 11:07"
        },
        {
          "id": "MDRW2100025419.1.9",
          "form": "다행이 밀가루맛은 아니더라구요ㅋㄱ",
          "original_form": "다행이 밀가루맛은 아니더라구요ㅋㄱ",
          "speaker_id": "2",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100025419.1.10",
          "form": "곰표후드티이쁘던데ㅋㅋ",
          "original_form": "곰표후드티이쁘던데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100025419.1.11",
          "form": "저희 엄마는 밀가루포대기 쓴거 같대요",
          "original_form": "저희 엄마는 밀가루포대기 쓴거 같대요",
          "speaker_id": "2",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100025419.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ센스쟁이시네이",
          "original_form": "ㅋㅋㅋㅋㅋㅋ센스쟁이시네이",
          "speaker_id": "1",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100025419.1.13",
          "form": "뭔가 향수를 불러일으키면서",
          "original_form": "뭔가 향수를 불러일으키면서",
          "speaker_id": "1",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100025419.1.14",
          "form": "힙해보여서 이쁘더라구요",
          "original_form": "힙해보여서 이쁘더라구요",
          "speaker_id": "1",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100025419.1.15",
          "form": "진로 두꺼비도 예뻐요",
          "original_form": "진로 두꺼비도 예뻐요",
          "speaker_id": "1",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100025419.1.16",
          "form": "저희 집에 두꺼비 그려진 에코백이 있는데",
          "original_form": "저희 집에 두꺼비 그려진 에코백이 있는데",
          "speaker_id": "1",
          "time": "20210901 11:10"
        },
        {
          "id": "MDRW2100025419.1.17",
          "form": "볼때마다 진로 마시고 싶더라구요",
          "original_form": "볼때마다 진로 마시고 싶더라구요",
          "speaker_id": "1",
          "time": "20210901 11:10"
        },
        {
          "id": "MDRW2100025419.1.18",
          "form": "와!",
          "original_form": "와!",
          "speaker_id": "2",
          "time": "20210901 11:10"
        },
        {
          "id": "MDRW2100025419.1.19",
          "form": "에코백 이쁘겠네요!",
          "original_form": "에코백 이쁘겠네요!",
          "speaker_id": "2",
          "time": "20210901 11:10"
        },
        {
          "id": "MDRW2100025419.1.20",
          "form": "친구가 저번에",
          "original_form": "친구가 저번에",
          "speaker_id": "1",
          "time": "20210901 11:11"
        },
        {
          "id": "MDRW2100025419.1.21",
          "form": "진로 두꺼비 그려진 유리잔 선물해줬거든요",
          "original_form": "진로 두꺼비 그려진 유리잔 선물해줬거든요",
          "speaker_id": "1",
          "time": "20210901 11:11"
        },
        {
          "id": "MDRW2100025419.1.22",
          "form": "줬나여?",
          "original_form": "줬나여?",
          "speaker_id": "2",
          "time": "20210901 11:11"
        },
        {
          "id": "MDRW2100025419.1.23",
          "form": "엄청 귀여운데",
          "original_form": "엄청 귀여운데",
          "speaker_id": "1",
          "time": "20210901 11:11"
        },
        {
          "id": "MDRW2100025419.1.24",
          "form": "맥주 마실때 그잔에 따라 마시면",
          "original_form": "맥주 마실때 그잔에 따라 마시면",
          "speaker_id": "1",
          "time": "20210901 11:12"
        },
        {
          "id": "MDRW2100025419.1.25",
          "form": "정말 맛이 죽여줘요",
          "original_form": "정말 맛이 죽여줘요",
          "speaker_id": "1",
          "time": "20210901 11:12"
        },
        {
          "id": "MDRW2100025419.1.26",
          "form": "어우",
          "original_form": "어우",
          "speaker_id": "2",
          "time": "20210901 11:12"
        },
        {
          "id": "MDRW2100025419.1.27",
          "form": "소맥 마시고 싶네요",
          "original_form": "소맥 마시고 싶네요",
          "speaker_id": "2",
          "time": "20210901 11:12"
        },
        {
          "id": "MDRW2100025419.1.28",
          "form": "주말은 전어에 진로소맥잔에 소맥말아먹어야겠어요ㅋㅋ",
          "original_form": "주말은 전어에 진로소맥잔에 소맥말아먹어야겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:13"
        },
        {
          "id": "MDRW2100025419.1.29",
          "form": "그죠 역시 잔이 이뻐야 술맛도 좋아요",
          "original_form": "그죠 역시 잔이 이뻐야 술맛도 좋아요",
          "speaker_id": "1",
          "time": "20210901 11:13"
        },
        {
          "id": "MDRW2100025419.1.30",
          "form": "최근에보니까",
          "original_form": "최근에보니까",
          "speaker_id": "1",
          "time": "20210901 11:15"
        },
        {
          "id": "MDRW2100025419.1.31",
          "form": "곰표 치킨너겟 출시했더라구요",
          "original_form": "곰표 치킨너겟 출시했더라구요",
          "speaker_id": "1",
          "time": "20210901 11:15"
        },
        {
          "id": "MDRW2100025419.1.32",
          "form": "저희집 근처엔 안팔던데",
          "original_form": "저희집 근처엔 안팔던데",
          "speaker_id": "1",
          "time": "20210901 11:15"
        },
        {
          "id": "MDRW2100025419.1.33",
          "form": "있으면 사먹어보고싶어요",
          "original_form": "있으면 사먹어보고싶어요",
          "speaker_id": "1",
          "time": "20210901 11:15"
        },
        {
          "id": "MDRW2100025419.1.34",
          "form": "맛이 궁금해요 ㅋㅋㅋ",
          "original_form": "맛이 궁금해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:16"
        },
        {
          "id": "MDRW2100025419.1.35",
          "form": "곰표 맥주가 맛있어서 기대되네요 2ㅋ",
          "original_form": "곰표 맥주가 맛있어서 기대되네요 2ㅋ",
          "speaker_id": "1",
          "time": "20210901 11:16"
        },
        {
          "id": "MDRW2100025419.1.36",
          "form": "치킨너겟이요?",
          "original_form": "치킨너겟이요?",
          "speaker_id": "2",
          "time": "20210901 11:19"
        },
        {
          "id": "MDRW2100025419.1.37",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210901 11:19"
        },
        {
          "id": "MDRW2100025419.1.38",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "2",
          "time": "20210901 11:19"
        },
        {
          "id": "MDRW2100025419.1.39",
          "form": "음식도 콜라보로 나오군요",
          "original_form": "음식도 콜라보로 나오군요",
          "speaker_id": "2",
          "time": "20210901 11:19"
        },
        {
          "id": "MDRW2100025419.1.40",
          "form": "심지어",
          "original_form": "심지어",
          "speaker_id": "1",
          "time": "20210901 11:19"
        },
        {
          "id": "MDRW2100025419.1.41",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210901 11:19"
        },
        {
          "id": "MDRW2100025419.1.42",
          "form": "곰모양이랍니다",
          "original_form": "곰모양이랍니다",
          "speaker_id": "1",
          "time": "20210901 11:19"
        },
        {
          "id": "MDRW2100025419.1.43",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210901 11:19"
        },
        {
          "id": "MDRW2100025419.1.44",
          "form": "넘귀엽죠 ㅋㅋㅋㅋ",
          "original_form": "넘귀엽죠 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:20"
        },
        {
          "id": "MDRW2100025419.1.45",
          "form": "언제출시죠?",
          "original_form": "언제출시죠?",
          "speaker_id": "2",
          "time": "20210901 11:20"
        },
        {
          "id": "MDRW2100025419.1.46",
          "form": "이미출시되었을거에요",
          "original_form": "이미출시되었을거에요",
          "speaker_id": "1",
          "time": "20210901 11:20"
        }
      ]
    }
