          "time": "20210825 12:04"
        },
        {
          "id": "MDRW2100013061.1.26",
          "form": "고치던가노력하면되는거지",
          "original_form": "고치던가노력하면되는거지",
          "speaker_id": "1",
          "time": "20210825 12:04"
        },
        {
          "id": "MDRW2100013061.1.27",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210825 12:04"
        },
        {
          "id": "MDRW2100013061.1.28",
          "form": "그러면",
          "original_form": "그러면",
