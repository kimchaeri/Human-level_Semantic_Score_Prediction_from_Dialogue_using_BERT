      "utterance": [
        {
          "id": "MDRW2100013061.1.1",
          "form": "이번에 배우도 난리났잖아",
          "original_form": "이번에 배우도 난리났잖아",
          "speaker_id": "2",
          "time": "20210825 11:54"
        },
        {
          "id": "MDRW2100013061.1.2",
          "form": "누구?",
          "original_form": "누구?",
          "speaker_id": "1",
          "time": "20210825 11:54"
        },
        {
          "id": "MDRW2100013061.1.3",
          "form": "요즘소식잘모름",
          "original_form": "요즘소식잘모름",
          "speaker_id": "1",
          "time": "20210825 11:55"
        },
        {
          "id": "MDRW2100013061.1.4",
          "form": "푸른 거탑에 나온 배우인데",
          "original_form": "푸른 거탑에 나온 배우인데",
          "speaker_id": "2",
          "time": "20210825 11:55"
        },
        {
          "id": "MDRW2100013061.1.5",
          "form": "1회차에 극 본 사람이 불호 후기를 남긴거야",
          "original_form": "1회차에 극 본 사람이 불호 후기를 남긴거야",
          "speaker_id": "2",
          "time": "20210825 11:55"
        },
        {
          "id": "MDRW2100013061.1.6",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210825 11:55"
        },
        {
          "id": "MDRW2100013061.1.7",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210825 11:55"
        },
        {
          "id": "MDRW2100013061.1.8",
          "form": "노래가 부족 한 것 같다고",
          "original_form": "노래가 부족 한 것 같다고",
          "speaker_id": "2",
          "time": "20210825 11:55"
        },
        {
          "id": "MDRW2100013061.1.9",
          "form": "푸른거탑그배우가?",
          "original_form": "푸른거탑그배우가?",
          "speaker_id": "1",
          "time": "20210825 11:55"
        },
        {
          "id": "MDRW2100013061.1.10",
          "form": "ㅇㅇ 그 배우의 실력이 안좋다고",
          "original_form": "ㅇㅇ 그 배우의 실력이 안좋다고",
          "speaker_id": "2",
          "time": "20210825 11:56"
        },
        {
          "id": "MDRW2100013061.1.11",
          "form": "불안정하다고",
          "original_form": "불안정하다고",
          "speaker_id": "2",
          "time": "20210825 11:56"
        },
        {
          "id": "MDRW2100013061.1.12",
          "form": "아 ㅇㅋㅇㅋ 이해함",
          "original_form": "아 ㅇㅋㅇㅋ 이해함",
          "speaker_id": "1",
          "time": "20210825 11:56"
        },
        {
          "id": "MDRW2100013061.1.13",
          "form": "그랬더니 지 인스타에 1회차라서 컨디션이 안오른거다 그리고 어차피 시사회라 반값으로 오지 않았느냐 그리고 뮤지콜 전공 아니고 연극 전공이라 가창력 운운하면 안된다 게다가 김춘수 김선미 즉 컴퍼니 측에서 자꾸 별로라고 하는데 니들",
          "original_form": "그랬더니 지 인스타에 1회차라서 컨디션이 안오른거다 그리고 어차피 시사회라 반값으로 오지 않았느냐 그리고 뮤지콜 전공 아니고 연극 전공이라 가창력 운운하면 안된다 게다가 김춘수 김선미 즉 컴퍼니 측에서 자꾸 별로라고 하는데 니들",
          "speaker_id": "2",
          "time": "20210825 12:02"
        },
        {
          "id": "MDRW2100013061.1.14",
          "form": "뭐하는사람임",
          "original_form": "뭐하는사람임",
          "speaker_id": "1",
          "time": "20210825 12:03"
        },
        {
          "id": "MDRW2100013061.1.15",
          "form": "비판도받아들일줄알아야지",
          "original_form": "비판도받아들일줄알아야지",
          "speaker_id": "1",
          "time": "20210825 12:03"
        },
        {
          "id": "MDRW2100013061.1.16",
          "form": "뮤지컬하는데가창력이중요하지",
          "original_form": "뮤지컬하는데가창력이중요하지",
          "speaker_id": "1",
          "time": "20210825 12:03"
        },
        {
          "id": "MDRW2100013061.1.17",
          "form": "전공이무슨상관임",
          "original_form": "전공이무슨상관임",
          "speaker_id": "1",
          "time": "20210825 12:03"
        },
        {
          "id": "MDRW2100013061.1.18",
          "form": "그니까 아니 조승우 홍광호 박은태도 실수하면 불호 후기 올라오고",
          "original_form": "그니까 아니 조승우 홍광호 박은태도 실수하면 불호 후기 올라오고",
          "speaker_id": "2",
          "time": "20210825 12:03"
        },
        {
          "id": "MDRW2100013061.1.19",
          "form": "심지어 박은태 경영학과 출신임",
          "original_form": "심지어 박은태 경영학과 출신임",
          "speaker_id": "2",
          "time": "20210825 12:03"
        },
        {
          "id": "MDRW2100013061.1.20",
          "form": "맞나",
          "original_form": "맞나",
          "speaker_id": "2",
          "time": "20210825 12:04"
        },
        {
          "id": "MDRW2100013061.1.21",
          "form": "그럼",
          "original_form": "그럼",
          "speaker_id": "2",
          "time": "20210825 12:04"
        },
        {
          "id": "MDRW2100013061.1.22",
          "form": "이미",
          "original_form": "이미",
          "speaker_id": "1",
          "time": "20210825 12:04"
        },
        {
          "id": "MDRW2100013061.1.23",
          "form": "뮤지컬들어갔는데",
          "original_form": "뮤지컬들어갔는데",
          "speaker_id": "1",
          "time": "20210825 12:04"
        },
        {
          "id": "MDRW2100013061.1.24",
          "form": "전공운운하면안되지",
          "original_form": "전공운운하면안되지",
          "speaker_id": "1",
          "time": "20210825 12:04"
        },
        {
          "id": "MDRW2100013061.1.25",
          "form": "비판들으면",
          "original_form": "비판들으면",
