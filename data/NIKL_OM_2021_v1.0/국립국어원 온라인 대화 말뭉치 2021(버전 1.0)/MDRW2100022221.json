      "utterance": [
        {
          "id": "MDRW2100022221.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210829 11:16"
        },
        {
          "id": "MDRW2100022221.1.2",
          "form": "키우는 동물 있으세요?",
          "original_form": "키우는 동물 있으세요?",
          "speaker_id": "1",
          "time": "20210829 11:16"
        },
        {
          "id": "MDRW2100022221.1.3",
          "form": "아뇨 가족 중에 알러지가 있는 사람이 있어서",
          "original_form": "아뇨 가족 중에 알러지가 있는 사람이 있어서",
          "speaker_id": "2",
          "time": "20210829 11:16"
        },
        {
          "id": "MDRW2100022221.1.4",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210829 11:17"
        },
        {
          "id": "MDRW2100022221.1.5",
          "form": "전 키웠다가 죽고나서 안키워요",
          "original_form": "전 키웠다가 죽고나서 안키워요",
          "speaker_id": "1",
          "time": "20210829 11:17"
        },
        {
          "id": "MDRW2100022221.1.6",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 11:17"
        },
        {
          "id": "MDRW2100022221.1.7",
          "form": "아 그러셨구나",
          "original_form": "아 그러셨구나",
          "speaker_id": "2",
          "time": "20210829 11:18"
        },
        {
          "id": "MDRW2100022221.1.8",
          "form": "반려동물 오래 키우셨어요?",
          "original_form": "반려동물 오래 키우셨어요?",
          "speaker_id": "2",
          "time": "20210829 11:18"
        },
        {
          "id": "MDRW2100022221.1.9",
          "form": "네 10년?",
          "original_form": "네 10년?",
          "speaker_id": "1",
          "time": "20210829 11:18"
        },
        {
          "id": "MDRW2100022221.1.10",
          "form": "충격이 크더라구요",
          "original_form": "충격이 크더라구요",
          "speaker_id": "1",
          "time": "20210829 11:18"
        },
        {
          "id": "MDRW2100022221.1.11",
          "form": "언젠가 새로 키운아이도 그렇게 될텐데",
          "original_form": "언젠가 새로 키운아이도 그렇게 될텐데",
          "speaker_id": "1",
          "time": "20210829 11:19"
        },
        {
          "id": "MDRW2100022221.1.12",
          "form": "반려동물과 정이 많이 드셨겠네요",
          "original_form": "반려동물과 정이 많이 드셨겠네요",
          "speaker_id": "2",
          "time": "20210829 11:19"
        },
        {
          "id": "MDRW2100022221.1.13",
          "form": "강아지?",
          "original_form": "강아지?",
          "speaker_id": "2",
          "time": "20210829 11:19"
        },
        {
          "id": "MDRW2100022221.1.14",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210829 11:19"
        },
        {
          "id": "MDRW2100022221.1.15",
          "form": "강아지요",
          "original_form": "강아지요",
          "speaker_id": "1",
          "time": "20210829 11:19"
        },
        {
          "id": "MDRW2100022221.1.16",
          "form": "네 제 주변에도 반려동물이 무지개다리 건너고",
          "original_form": "네 제 주변에도 반려동물이 무지개다리 건너고",
          "speaker_id": "2",
          "time": "20210829 11:19"
        },
        {
          "id": "MDRW2100022221.1.17",
          "form": "힘들어하는 분들이 꽤 많더라고요ㅜ",
          "original_form": "힘들어하는 분들이 꽤 많더라고요ㅜ",
          "speaker_id": "2",
          "time": "20210829 11:19"
        },
        {
          "id": "MDRW2100022221.1.18",
          "form": "전 강아지가 집에 들어갔을때 반겨주는게",
          "original_form": "전 강아지가 집에 들어갔을때 반겨주는게",
          "speaker_id": "1",
          "time": "20210829 11:19"
        },
        {
          "id": "MDRW2100022221.1.19",
          "form": "좋더라구여",
          "original_form": "좋더라구여",
          "speaker_id": "1",
          "time": "20210829 11:19"
        },
        {
          "id": "MDRW2100022221.1.20",
          "form": "아무래도 가족같이 지내던 동물이니",
          "original_form": "아무래도 가족같이 지내던 동물이니",
          "speaker_id": "2",
          "time": "20210829 11:19"
        },
        {
          "id": "MDRW2100022221.1.21",
          "form": "고양이는 그런게 없어서",
          "original_form": "고양이는 그런게 없어서",
          "speaker_id": "1",
          "time": "20210829 11:20"
        },
        {
          "id": "MDRW2100022221.1.22",
          "form": "강아지 좋아해요",
          "original_form": "강아지 좋아해요",
          "speaker_id": "1",
          "time": "20210829 11:20"
        },
        {
          "id": "MDRW2100022221.1.23",
          "form": "저도 강아지들 꼬리 흔들고 와서 애교부리는 게 좋더라고요",
          "original_form": "저도 강아지들 꼬리 흔들고 와서 애교부리는 게 좋더라고요",
          "speaker_id": "2",
          "time": "20210829 11:20"
        },
        {
          "id": "MDRW2100022221.1.24",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100022221.1.25",
          "form": "고양이는 혼자노니까",
          "original_form": "고양이는 혼자노니까",
          "speaker_id": "1",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100022221.1.26",
          "form": "직접 키우진 못하고 랜선으로 보고 있어요",
          "original_form": "직접 키우진 못하고 랜선으로 보고 있어요",
          "speaker_id": "2",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100022221.1.27",
          "form": "저도 이젠 유튜브로...",
          "original_form": "저도 이젠 유튜브로...",
          "speaker_id": "1",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100022221.1.28",
          "form": "반려동물 기르는 데에도 책임이 필요하니까",
          "original_form": "반려동물 기르는 데에도 책임이 필요하니까",
          "speaker_id": "2",
          "time": "20210829 11:22"
        },
        {
          "id": "MDRW2100022221.1.29",
          "form": "여러마리 키우는건 제가 감당이 안될거 같아서",
          "original_form": "여러마리 키우는건 제가 감당이 안될거 같아서",
          "speaker_id": "1",
          "time": "20210829 11:22"
        },
        {
          "id": "MDRW2100022221.1.30",
          "form": "좀 더 신중하게 되는 듯해요",
          "original_form": "좀 더 신중하게 되는 듯해요",
          "speaker_id": "2",
          "time": "20210829 11:22"
        },
        {
          "id": "MDRW2100022221.1.31",
          "form": "그때 한마리만 키웠는데",
          "original_form": "그때 한마리만 키웠는데",
          "speaker_id": "1",
          "time": "20210829 11:22"
        },
        {
          "id": "MDRW2100022221.1.32",
          "form": "떠나고나니 허전하네요",
          "original_form": "떠나고나니 허전하네요",
          "speaker_id": "1",
          "time": "20210829 11:22"
        }
      ]
    }
  ]
