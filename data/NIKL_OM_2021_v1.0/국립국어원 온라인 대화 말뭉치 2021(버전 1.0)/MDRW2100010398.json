      "utterance": [
        {
          "id": "MDRW2100010398.1.1",
          "form": "안녕하세요 어떤음식 좋아하시나요?",
          "original_form": "안녕하세요 어떤음식 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210823 17:28"
        },
        {
          "id": "MDRW2100010398.1.2",
          "form": "가리지 않지만 요즘은 파스타가 좋아요",
          "original_form": "가리지 않지만 요즘은 파스타가 좋아요",
          "speaker_id": "2",
          "time": "20210823 17:28"
        },
        {
          "id": "MDRW2100010398.1.3",
          "form": "파스타 맛있죠! 오늘 점심에 뭐드셨나요?",
          "original_form": "파스타 맛있죠! 오늘 점심에 뭐드셨나요?",
          "speaker_id": "1",
          "time": "20210823 17:29"
        },
        {
          "id": "MDRW2100010398.1.4",
          "form": "오늘은 라면이요! 신라면 건면이 맛있어요",
          "original_form": "오늘은 라면이요! 신라면 건면이 맛있어요",
          "speaker_id": "2",
          "time": "20210823 17:32"
        },
        {
          "id": "MDRW2100010398.1.5",
          "form": "오 라면 맛있었겠네요 신라면 건면은 처음보는데 담에 한번 멋어봐야겠어요~",
          "original_form": "오 라면 맛있었겠네요 신라면 건면은 처음보는데 담에 한번 멋어봐야겠어요~",
          "speaker_id": "1",
          "time": "20210823 17:34"
        },
        {
          "id": "MDRW2100010398.1.6",
          "form": "안튀긴거라 담백해요!",
          "original_form": "안튀긴거라 담백해요!",
          "speaker_id": "2",
          "time": "20210823 17:35"
        },
        {
          "id": "MDRW2100010398.1.7",
          "form": "아하 그렇군요 배달음식 좋아하시나요?",
          "original_form": "아하 그렇군요 배달음식 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210823 17:36"
        },
        {
          "id": "MDRW2100010398.1.8",
          "form": "직접가는 편이지만 치킨 좋아해요",
          "original_form": "직접가는 편이지만 치킨 좋아해요",
          "speaker_id": "2",
          "time": "20210823 17:37"
        },
        {
          "id": "MDRW2100010398.1.9",
          "form": "치킨 맛있죠ㅎㅎ",
          "original_form": "치킨 맛있죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 17:39"
        },
        {
          "id": "MDRW2100010398.1.10",
          "form": "뿌링클이 제일 맛있더라구요",
          "original_form": "뿌링클이 제일 맛있더라구요",
          "speaker_id": "2",
          "time": "20210823 17:40"
        },
        {
          "id": "MDRW2100010398.1.11",
          "form": "오 저도 뿌링클 젤 좋아해요!",
          "original_form": "오 저도 뿌링클 젤 좋아해요!",
          "speaker_id": "1",
          "time": "20210823 17:42"
        },
        {
          "id": "MDRW2100010398.1.12",
          "form": "달달해서 딱 맛있어용ㅋㅋㅋ",
          "original_form": "달달해서 딱 맛있어용ㅋㅋㅋ",
          "speaker_id": "2",
