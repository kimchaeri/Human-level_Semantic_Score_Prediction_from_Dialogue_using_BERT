      "utterance": [
        {
          "id": "MDRW2100038632.1.1",
          "form": "치킨은 어디꺼 좋아하시나요",
          "original_form": "치킨은 어디꺼 좋아하시나요",
          "speaker_id": "1",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100038632.1.2",
          "form": "비비큐 황올",
          "original_form": "비비큐 황올",
          "speaker_id": "2",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100038632.1.3",
          "form": "굽네",
          "original_form": "굽네",
          "speaker_id": "2",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100038632.1.4",
          "form": "지코바요",
          "original_form": "지코바요",
          "speaker_id": "2",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100038632.1.5",
          "form": "아 황올 맛있죠!! 저는 안심으로 먹어요",
          "original_form": "아 황올 맛있죠!! 저는 안심으로 먹어요",
          "speaker_id": "1",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100038632.1.6",
          "form": "오잉 그거도잇어요?",
          "original_form": "오잉 그거도잇어요?",
          "speaker_id": "2",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100038632.1.7",
          "form": "네 천원 더 비싸던가 안심부위로 있어요",
          "original_form": "네 천원 더 비싸던가 안심부위로 있어요",
          "speaker_id": "1",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100038632.1.8",
          "form": "아항 찾아봐야지!",
          "original_form": "아항 찾아봐야지!",
          "speaker_id": "2",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100038632.1.9",
          "form": "주로 담백한 쪽을 좋아하시나봐요!",
          "original_form": "주로 담백한 쪽을 좋아하시나봐요!",
          "speaker_id": "1",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100038632.1.10",
          "form": "아 그런강 ?ㅋㅋㅋ 기름진것도 조아하긴 하는데요 ㅋㅋ",
          "original_form": "아 그런강 ?ㅋㅋㅋ 기름진것도 조아하긴 하는데요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:07"
        },
        {
          "id": "MDRW2100038632.1.11",
          "form": "치킨먹고싶은데 오늘은  약속이있군요..",
          "original_form": "치킨먹고싶은데 오늘은  약속이있군요..",
          "speaker_id": "2",
          "time": "20210902 18:07"
        },
        {
          "id": "MDRW2100038632.1.12",
          "form": "오늘 저녁은 뭐 드시나요? ㅎㅎ",
          "original_form": "오늘 저녁은 뭐 드시나요? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100038632.1.13",
          "form": "불백...",
          "original_form": "불백...",
          "speaker_id": "2",
          "time": "20210902 18:12"
        },
        {
          "id": "MDRW2100038632.1.14",
          "form": "조아하는덴 아니네용",
          "original_form": "조아하는덴 아니네용",
          "speaker_id": "2",
          "time": "20210902 18:12"
        },
        {
          "id": "MDRW2100038632.1.15",
          "form": "아 불백!! 별로 안 좋아하시나유",
          "original_form": "아 불백!! 별로 안 좋아하시나유",
          "speaker_id": "1",
          "time": "20210902 18:14"
        },
        {
          "id": "MDRW2100038632.1.16",
          "form": "네 그냥 ㅠㅠㅋㅋㅋ 안땡겨요",
          "original_form": "네 그냥 ㅠㅠㅋㅋㅋ 안땡겨요",
          "speaker_id": "2",
          "time": "20210902 18:18"
        },
        {
          "id": "MDRW2100038632.1.17",
          "form": "8신데 벌써배고파서",
          "original_form": "8신데 벌써배고파서",
          "speaker_id": "2",
          "time": "20210902 18:18"
        },
        {
          "id": "MDRW2100038632.1.18",
          "form": "머먹어야할거같은데..",
          "original_form": "머먹어야할거같은데..",
          "speaker_id": "2",
          "time": "20210902 18:18"
        },
        {
          "id": "MDRW2100038632.1.19",
          "form": "저런 ㅠㅠㅠ 헐 8시까지 어떻게 기다려요..",
          "original_form": "저런 ㅠㅠㅠ 헐 8시까지 어떻게 기다려요..",
          "speaker_id": "1",
          "time": "20210902 18:18"
        },
        {
          "id": "MDRW2100038632.1.20",
          "form": "너무 늦네요",
          "original_form": "너무 늦네요",
          "speaker_id": "1",
          "time": "20210902 18:18"
        },
        {
          "id": "MDRW2100038632.1.21",
          "form": "그죵",
          "original_form": "그죵",
          "speaker_id": "2",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100038632.1.22",
          "form": "뭐먹지 ㅋㅋ",
          "original_form": "뭐먹지 ㅋㅋ",
