      "utterance": [
        {
          "id": "MDRW2100041575.1.1",
          "form": "디즈니어때",
          "original_form": "디즈니어때",
          "speaker_id": "2",
          "time": "20210906 13:01"
        },
        {
          "id": "MDRW2100041575.1.2",
          "form": "픽사나",
          "original_form": "픽사나",
          "speaker_id": "2",
          "time": "20210906 13:01"
        },
        {
          "id": "MDRW2100041575.1.3",
          "form": "ㅋㅋㅋㄴ좋긴한데",
          "original_form": "ㅋㅋㅋㄴ좋긴한데",
          "speaker_id": "1",
          "time": "20210906 13:11"
        },
        {
          "id": "MDRW2100041575.1.4",
          "form": "우리가 거의 다 봐서",
          "original_form": "우리가 거의 다 봐서",
          "speaker_id": "1",
          "time": "20210906 13:11"
        },
        {
          "id": "MDRW2100041575.1.5",
          "form": "안본거있어?",
          "original_form": "안본거있어?",
          "speaker_id": "1",
          "time": "20210906 13:11"
        },
        {
          "id": "MDRW2100041575.1.6",
          "form": "거의없긴해",
          "original_form": "거의없긴해",
          "speaker_id": "2",
          "time": "20210906 13:35"
        },
        {
          "id": "MDRW2100041575.1.7",
          "form": "날 추워지니까 로맨스도땡겨",
          "original_form": "날 추워지니까 로맨스도땡겨",
          "speaker_id": "2",
          "time": "20210906 13:35"
        },
        {
          "id": "MDRW2100041575.1.8",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 13:35"
        },
        {
          "id": "MDRW2100041575.1.9",
          "form": "로맨스 극혐이라면서여",
          "original_form": "로맨스 극혐이라면서여",
          "speaker_id": "1",
          "time": "20210906 13:35"
        },
        {
          "id": "MDRW2100041575.1.10",
          "form": "최소 로코는 되야 보다면서여",
          "original_form": "최소 로코는 되야 보다면서여",
          "speaker_id": "1",
          "time": "20210906 13:35"
        },
        {
          "id": "MDRW2100041575.1.11",
          "form": "나 최근에 파이브피트봄",
          "original_form": "나 최근에 파이브피트봄",
          "speaker_id": "1",
          "time": "20210906 13:36"
        },
        {
          "id": "MDRW2100041575.1.12",
          "form": "그건뭐야",
          "original_form": "그건뭐야",
          "speaker_id": "2",
          "time": "20210906 13:37"
        },
        {
          "id": "MDRW2100041575.1.13",
          "form": "냐 로맨스는 그 시월애 그런거 조아해",
          "original_form": "냐 로맨스는 그 시월애 그런거 조아해",
          "speaker_id": "2",
          "time": "20210906 13:37"
        },
        {
          "id": "MDRW2100041575.1.14",
          "form": "겨울에 꼭 봐",
          "original_form": "겨울에 꼭 봐",
          "speaker_id": "2",
          "time": "20210906 13:37"
        },
        {
          "id": "MDRW2100041575.1.15",
          "form": "그거 거의 코미디도 없눈뎈ㅋㅋㅋ아닌가?",
          "original_form": "그거 거의 코미디도 없눈뎈ㅋㅋㅋ아닌가?",
          "speaker_id": "1",
          "time": "20210906 13:37"
        },
        {
          "id": "MDRW2100041575.1.16",
          "form": "시월애는 분위기가다했지",
          "original_form": "시월애는 분위기가다했지",
          "speaker_id": "1",
          "time": "20210906 13:37"
        },
        {
          "id": "MDRW2100041575.1.17",
          "form": "어맞아 되게잔잔",
          "original_form": "어맞아 되게잔잔",
          "speaker_id": "2",
          "time": "20210906 13:39"
        },
        {
          "id": "MDRW2100041575.1.18",
          "form": "name2도 그런것도 본다구~~",
          "original_form": "&name2&도 그런것도 본다구~~",
          "speaker_id": "2",
          "time": "20210906 13:39"
        },
        {
          "id": "MDRW2100041575.1.19",
          "form": "ㅋㅋㅋㅋ안보는줄",
          "original_form": "ㅋㅋㅋㅋ안보는줄",
          "speaker_id": "1",
          "time": "20210906 13:39"
        },
        {
          "id": "MDRW2100041575.1.20",
          "form": "동감도보고",
          "original_form": "동감도보고",
          "speaker_id": "2",
          "time": "20210906 13:39"
        },
        {
          "id": "MDRW2100041575.1.21",
          "form": "근데 거의 안보긴 해",
          "original_form": "근데 거의 안보긴 해",
          "speaker_id": "2",
          "time": "20210906 13:40"
        },
        {
          "id": "MDRW2100041575.1.22",
          "form": "프로포즈봐 존잼이야..",
          "original_form": "프로포즈봐 존잼이야..",
          "speaker_id": "1",
          "time": "20210906 13:40"
        },
        {
          "id": "MDRW2100041575.1.23",
          "form": "누구나오는건데",
          "original_form": "누구나오는건데",
          "speaker_id": "2",
          "time": "20210906 13:41"
        },
        {
          "id": "MDRW2100041575.1.24",
          "form": "라이언레이놀즈..",
          "original_form": "라이언레이놀즈..",
          "speaker_id": "1",
          "time": "20210906 13:41"
        },
        {
          "id": "MDRW2100041575.1.25",
          "form": "산드라블록",
          "original_form": "산드라블록",
          "speaker_id": "1",
          "time": "20210906 13:41"
        },
        {
          "id": "MDRW2100041575.1.26",
          "form": "의외로 라이언레이놀즈가 로코를 마니찍었거든여",
          "original_form": "의외로 라이언레이놀즈가 로코를 마니찍었거든여",
          "speaker_id": "1",
          "time": "20210906 13:41"
        },
        {
          "id": "MDRW2100041575.1.27",
          "form": "나의 특별한사랑이야기(번역머선일)도 잼써",
          "original_form": "나의 특별한사랑이야기(번역머선일)도 잼써",
          "speaker_id": "1",
          "time": "20210906 13:42"
        },
        {
          "id": "MDRW2100041575.1.28",
          "form": "어 라이언레이놀즈면 임정이지",
          "original_form": "어 라이언레이놀즈면 임정이지",
          "speaker_id": "2",
          "time": "20210906 13:48"
        }
      ]
    }
  ]
