      "utterance": [
        {
          "id": "MDRW2100041998.1.1",
          "form": "내가 막공티켓팅 뜨면 쓰려고  이번달월차를 최대한아껴놓고있느데",
          "original_form": "내가 막공티켓팅 뜨면 쓰려고  이번달월차를 최대한아껴놓고있느데",
          "speaker_id": "2",
          "time": "20210906 18:28"
        },
        {
          "id": "MDRW2100041998.1.2",
          "form": "언제뜨려나",
          "original_form": "언제뜨려나",
          "speaker_id": "2",
          "time": "20210906 18:29"
        },
        {
          "id": "MDRW2100041998.1.3",
          "form": "3차 4차 티켓팅은 텀 얼마 안두고 했었던 것 같은데",
          "original_form": "3차 4차 티켓팅은 텀 얼마 안두고 했었던 것 같은데",
          "speaker_id": "1",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100041998.1.4",
          "form": "5차 얘기 아직 못들은거같네여 그러고보니",
          "original_form": "5차 얘기 아직 못들은거같네여 그러고보니",
          "speaker_id": "1",
          "time": "20210906 18:32"
        },
        {
          "id": "MDRW2100041998.1.5",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100041998.1.6",
          "form": "쇼노트 팔로하고",
          "original_form": "쇼노트 팔로하고",
          "speaker_id": "2",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100041998.1.7",
          "form": "지켜보고있음",
          "original_form": "지켜보고있음",
          "speaker_id": "2",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100041998.1.8",
          "form": "막공 오픈때는",
          "original_form": "막공 오픈때는",
          "speaker_id": "1",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100041998.1.9",
          "form": "다시 8시티켓팅 해주면 안되냐",
          "original_form": "다시 8시티켓팅 해주면 안되냐",
          "speaker_id": "1",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100041998.1.10",
          "form": "부탁한다 제발",
          "original_form": "부탁한다 제발",
          "speaker_id": "1",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100041998.1.11",
          "form": "ㄹㅇ 제발",
          "original_form": "ㄹㅇ 제발",
          "speaker_id": "2",
          "time": "20210906 19:47"
        },
        {
          "id": "MDRW2100041998.1.12",
          "form": "나그날월차쓰고피방간다고",
          "original_form": "나그날월차쓰고피방간다고",
          "speaker_id": "2",
          "time": "20210906 19:47"
        },
        {
          "id": "MDRW2100041998.1.13",
          "form": "엉엉",
          "original_form": "엉엉",
          "speaker_id": "2",
          "time": "20210906 19:48"
        },
        {
          "id": "MDRW2100041998.1.14",
          "form": "막공은 진심 8시 해줘야해",
          "original_form": "막공은 진심 8시 해줘야해",
          "speaker_id": "1",
          "time": "20210906 20:02"
        },
        {
          "id": "MDRW2100041998.1.15",
          "form": "서버 또 뒤집어질텐데",
          "original_form": "서버 또 뒤집어질텐데",
          "speaker_id": "1",
          "time": "20210906 20:02"
        },
        {
          "id": "MDRW2100041998.1.16",
          "form": "아니그니까",
          "original_form": "아니그니까",
          "speaker_id": "2",
          "time": "20210906 20:16"
        },
        {
          "id": "MDRW2100041998.1.17",
          "form": "보는사람다직장인일텐데",
          "original_form": "보는사람다직장인일텐데",
          "speaker_id": "2",
          "time": "20210906 20:16"
        },
        {
          "id": "MDRW2100041998.1.18",
          "form": "2시 4시뭐냐고",
          "original_form": "2시 4시뭐냐고",
          "speaker_id": "2",
          "time": "20210906 20:16"
        },
        {
          "id": "MDRW2100041998.1.19",
          "form": "그니까ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ.....",
          "original_form": "그니까ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ.....",
          "speaker_id": "1",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100041998.1.20",
          "form": "심지어 이젠 대학생들도 개강인데요",
          "original_form": "심지어 이젠 대학생들도 개강인데요",
          "speaker_id": "1",
          "time": "20210906 21:19"
        },
        {
          "id": "MDRW2100041998.1.21",
          "form": "근까여",
          "original_form": "근까여",
          "speaker_id": "2",
          "time": "20210906 21:36"
        },
        {
          "id": "MDRW2100041998.1.22",
          "form": "아니 대체 시간누가정하는겨",
          "original_form": "아니 대체 시간누가정하는겨",
          "speaker_id": "2",
          "time": "20210906 21:37"
        },
        {
          "id": "MDRW2100041998.1.23",
          "form": "요새돌덕도티켓팅안하는데",
