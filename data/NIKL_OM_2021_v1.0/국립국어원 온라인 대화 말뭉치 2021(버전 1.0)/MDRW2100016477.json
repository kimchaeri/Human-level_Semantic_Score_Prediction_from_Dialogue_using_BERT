          "id": "MDRW2100016477.1.1",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210826 13:04"
        },
        {
          "id": "MDRW2100016477.1.2",
          "form": "좋아하죠. 요새 코로나로 잘 못가서 아쉽네요",
          "original_form": "좋아하죠. 요새 코로나로 잘 못가서 아쉽네요",
          "speaker_id": "2",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100016477.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100016477.1.4",
          "form": "저도 못가요",
          "original_form": "저도 못가요",
          "speaker_id": "1",
          "time": "20210826 17:49"
        },
        {
          "id": "MDRW2100016477.1.5",
          "form": "가장 최근 여행지가 어디인가요",
          "original_form": "가장 최근 여행지가 어디인가요",
          "speaker_id": "2",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100016477.1.6",
          "form": "남해요",
          "original_form": "남해요",
          "speaker_id": "1",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100016477.1.7",
          "form": "아 몇박으로 가셨어요?",
          "original_form": "아 몇박으로 가셨어요?",
          "speaker_id": "2",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100016477.1.8",
          "form": "2박이요",
          "original_form": "2박이요",
          "speaker_id": "1",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100016477.1.9",
          "form": "친구와 갔나요",
          "original_form": "친구와 갔나요",
          "speaker_id": "2",
          "time": "20210826 20:52"
        },
        {
          "id": "MDRW2100016477.1.10",
          "form": "가족과",
          "original_form": "가족과",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100016477.1.11",
          "form": "올해 가셨나요",
          "original_form": "올해 가셨나요",
          "speaker_id": "2",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100016477.1.12",
          "form": "네 7월이요",
          "original_form": "네 7월이요",
          "speaker_id": "1",
