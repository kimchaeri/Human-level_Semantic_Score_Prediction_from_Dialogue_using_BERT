      "utterance": [
        {
          "id": "MDRW2100040564.1.1",
          "form": "부산가시면 어묵을...어묵을 사세요!",
          "original_form": "부산가시면 어묵을...어묵을 사세요!",
          "speaker_id": "1",
          "time": "20210904 22:44"
        },
        {
          "id": "MDRW2100040564.1.2",
          "form": "어묵도 중요하죠",
          "original_form": "어묵도 중요하죠",
          "speaker_id": "2",
          "time": "20210904 22:45"
        },
        {
          "id": "MDRW2100040564.1.3",
          "form": "고래사어묵맛있어요 ㅜㅜ",
          "original_form": "고래사어묵맛있어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 22:45"
        },
        {
          "id": "MDRW2100040564.1.4",
          "form": "삼진어묵도맛있고",
          "original_form": "삼진어묵도맛있고",
          "speaker_id": "1",
          "time": "20210904 22:45"
        },
        {
          "id": "MDRW2100040564.1.5",
          "form": "둘다 유명하더라구요",
          "original_form": "둘다 유명하더라구요",
          "speaker_id": "2",
          "time": "20210904 22:45"
        },
        {
          "id": "MDRW2100040564.1.6",
          "form": "드셔보셨어요?!",
          "original_form": "드셔보셨어요?!",
          "speaker_id": "1",
          "time": "20210904 22:45"
        },
        {
          "id": "MDRW2100040564.1.7",
          "form": "네, 뭐 선물이나 그렇게",
          "original_form": "네, 뭐 선물이나 그렇게",
          "speaker_id": "2",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100040564.1.8",
          "form": "확실히 국 끓였을때 그 탱탱함이 달랐어요",
          "original_form": "확실히 국 끓였을때 그 탱탱함이 달랐어요",
          "speaker_id": "2",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100040564.1.9",
          "form": "마트어묵은 끓이고 다음날에 먹으면 좀 흐물텅해지는데",
          "original_form": "마트어묵은 끓이고 다음날에 먹으면 좀 흐물텅해지는데",
          "speaker_id": "2",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100040564.1.10",
          "form": "얘들은 안그러더라구요",
          "original_form": "얘들은 안그러더라구요",
          "speaker_id": "2",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100040564.1.11",
          "form": "삼진어묵 어묵볼을 드십시오 진짜 최애에요",
          "original_form": "삼진어묵 어묵볼을 드십시오 진짜 최애에요",
          "speaker_id": "1",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100040564.1.12",
          "form": "고로케처럼 튀겨주는 그어묵.. 너무맛있어요 ㅜ",
          "original_form": "고로케처럼 튀겨주는 그어묵.. 너무맛있어요 ㅜ",
          "speaker_id": "1",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100040564.1.13",
          "form": "앗, 적어놔야겠다!",
          "original_form": "앗, 적어놔야겠다!",
          "speaker_id": "2",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100040564.1.14",
          "form": "어묵볼, 백명란...",
          "original_form": "어묵볼, 백명란...",
          "speaker_id": "2",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100040564.1.15",
          "form": "바로먹으면 가장 맛있지만 포장해가져도 괜찮을거에요 ㅎㅎ",
          "original_form": "바로먹으면 가장 맛있지만 포장해가져도 괜찮을거에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 22:53"
        },
        {
          "id": "MDRW2100040564.1.16",
          "form": "감사해요ㅋㅋㅋ",
          "original_form": "감사해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:54"
        },
        {
          "id": "MDRW2100040564.1.17",
          "form": "꿀팁이네요ㅋㅋㅋ",
          "original_form": "꿀팁이네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:54"
        },
        {
          "id": "MDRW2100040564.1.18",
          "form": "평소에는 밥 집밥 주로 드세요?",
          "original_form": "평소에는 밥 집밥 주로 드세요?",
          "speaker_id": "1",
          "time": "20210904 22:55"
        },
        {
          "id": "MDRW2100040564.1.19",
          "form": "네.",
          "original_form": "네.",
          "speaker_id": "2",
          "time": "20210904 22:56"
        },
        {
          "id": "MDRW2100040564.1.20",
          "form": "양념같은건 거의 시제품쓰지만",
          "original_form": "양념같은건 거의 시제품쓰지만",
          "speaker_id": "2",
          "time": "20210904 22:57"
        },
        {
          "id": "MDRW2100040564.1.21",
          "form": "어쨌든 제가 요리를 하긴 하죠 ㅋㅋ",
          "original_form": "어쨌든 제가 요리를 하긴 하죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:57"
        },
        {
          "id": "MDRW2100040564.1.22",
          "form": "요리를 하셔야하는 군요!",
          "original_form": "요리를 하셔야하는 군요!",
          "speaker_id": "1",
          "time": "20210904 22:57"
        },
        {
          "id": "MDRW2100040564.1.23",
          "form": "전사실 아직 부모님과함께살아서 ㅎㅎ 잘얻어먹고있습니다",
          "original_form": "전사실 아직 부모님과함께살아서 ㅎㅎ 잘얻어먹고있습니다",
          "speaker_id": "1",
          "time": "20210904 22:58"
        },
        {
          "id": "MDRW2100040564.1.24",
          "form": "요리하시는분들은 다 대단하다고 생각해요!!",
          "original_form": "요리하시는분들은 다 대단하다고 생각해요!!",
          "speaker_id": "1",
          "time": "20210904 22:58"
        },
        {
          "id": "MDRW2100040564.1.25",
          "form": "먹고 살아야 하니까 하게 되더라구요",
          "original_form": "먹고 살아야 하니까 하게 되더라구요",
          "speaker_id": "2",
          "time": "20210904 22:59"
        },
        {
          "id": "MDRW2100040564.1.26",
          "form": "배달로 살자니 쓰레기도 감당안되고",
          "original_form": "배달로 살자니 쓰레기도 감당안되고",
          "speaker_id": "2",
          "time": "20210904 23:00"
        },
        {
          "id": "MDRW2100040564.1.27",
          "form": "돈도... ㅎ",
          "original_form": "돈도... ㅎ",
          "speaker_id": "2",
          "time": "20210904 23:00"
        }
      ]
    }
