      "utterance": [
        {
          "id": "MDRW2100011000.1.1",
          "form": "떡볶이 한때 엄청 먹었어",
          "original_form": "떡볶이 한때 엄청 먹었어",
          "speaker_id": "2",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100011000.1.2",
          "form": "맵고 맛있어.",
          "original_form": "맵고 맛있어.",
          "speaker_id": "1",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100011000.1.3",
          "form": "너는 어떤맛으로 먹니?",
          "original_form": "너는 어떤맛으로 먹니?",
          "speaker_id": "2",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100011000.1.4",
          "form": "난 마늘을 좋아하는데 마늘이 크게 갈려있어",
          "original_form": "난 마늘을 좋아하는데 마늘이 크게 갈려있어",
          "speaker_id": "1",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100011000.1.5",
          "form": "나는 덜매운맛으로 먹어.",
          "original_form": "나는 덜매운맛으로 먹어.",
          "speaker_id": "1",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100011000.1.6",
          "form": "거기 맛 단계가 다양하잖아",
          "original_form": "거기 맛 단계가 다양하잖아",
          "speaker_id": "2",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100011000.1.7",
          "form": "오리지널은 아프거든.",
          "original_form": "오리지널은 아프거든.",
          "speaker_id": "1",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100011000.1.8",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100011000.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ어디가 아프다는고야?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ어디가 아프다는고야?",
          "speaker_id": "2",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100011000.1.10",
          "form": "아. 나는 초보맛이나 착한맛은 안먹어",
          "original_form": "아. 나는 초보맛이나 착한맛은 안먹어",
          "speaker_id": "1",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100011000.1.11",
          "form": "맛없으니까",
          "original_form": "맛없으니까",
          "speaker_id": "1",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100011000.1.12",
          "form": "나도 덜매운맛으로 먹어",
          "original_form": "나도 덜매운맛으로 먹어",
          "speaker_id": "2",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100011000.1.13",
          "form": "어디가아픈진 경험자만 알 수 있는데",
          "original_form": "어디가아픈진 경험자만 알 수 있는데",
          "speaker_id": "1",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100011000.1.14",
          "form": "너 먹어본 거 맞아?",
          "original_form": "너 먹어본 거 맞아?",
          "speaker_id": "1",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100011000.1.15",
          "form": "그러게 안매운걸 먹을꺼면 엽떡을 왜먹을까",
          "original_form": "그러게 안매운걸 먹을꺼면 엽떡을 왜먹을까",
          "speaker_id": "2",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100011000.1.16",
          "form": "그러게. 난 그리고 응떡은 별로 안좋아해",
          "original_form": "그러게. 난 그리고 응떡은 별로 안좋아해",
          "speaker_id": "1",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100011000.1.17",
          "form": "당연히 알지. 그냥 떠본거야",
          "original_form": "당연히 알지. 그냥 떠본거야",
          "speaker_id": "2",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100011000.1.18",
          "form": "니맛도 내맛도아냐.",
          "original_form": "니맛도 내맛도아냐.",
          "speaker_id": "1",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100011000.1.19",
          "form": "그건 너무 달아서 분식집 떡볶이같아",
          "original_form": "그건 너무 달아서 분식집 떡볶이같아",
          "speaker_id": "2",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100011000.1.20",
          "form": "맞아. 실망스러워.",
          "original_form": "맞아. 실망스러워.",
          "speaker_id": "1",
          "time": "20210823 23:38"
        }
      ]
