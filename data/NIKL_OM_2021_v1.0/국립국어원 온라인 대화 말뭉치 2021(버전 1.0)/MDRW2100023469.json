      "utterance": [
        {
          "id": "MDRW2100023469.1.1",
          "form": "이찬원 신곡 잘 듣고 있낭?",
          "original_form": "이찬원 신곡 잘 듣고 있낭?",
          "speaker_id": "2",
          "time": "20210830 06:07"
        },
        {
          "id": "MDRW2100023469.1.2",
          "form": "이찬원 편의점이 유튜브 인기동영상 순위 2위에 올랐어",
          "original_form": "이찬원 편의점이 유튜브 인기동영상 순위 2위에 올랐어",
          "speaker_id": "1",
          "time": "20210830 06:07"
        },
        {
          "id": "MDRW2100023469.1.3",
          "form": "ㅋㅋ 제목이 펀의점이 뭐여?",
          "original_form": "ㅋㅋ 제목이 펀의점이 뭐여?",
          "speaker_id": "2",
          "time": "20210830 06:07"
        },
        {
          "id": "MDRW2100023469.1.4",
          "form": "멜론 신곡차트에도 들었어",
          "original_form": "멜론 신곡차트에도 들었어",
          "speaker_id": "1",
          "time": "20210830 06:07"
        },
        {
          "id": "MDRW2100023469.1.5",
          "form": "작명센스가 남다르다 ㅋㅋ",
          "original_form": "작명센스가 남다르다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 06:07"
        },
        {
          "id": "MDRW2100023469.1.6",
          "form": "언제나 우리 곁에 24시간 있는다는 것이야",
          "original_form": "언제나 우리 곁에 24시간 있는다는 것이야",
          "speaker_id": "1",
          "time": "20210830 06:07"
        },
        {
          "id": "MDRW2100023469.1.7",
          "form": "오오~ 그래도 인기가 많구먼~",
          "original_form": "오오~ 그래도 인기가 많구먼~",
          "speaker_id": "2",
          "time": "20210830 06:07"
        },
        {
          "id": "MDRW2100023469.1.8",
          "form": "음원차트는 힘들다",
          "original_form": "음원차트는 힘들다",
          "speaker_id": "1",
          "time": "20210830 06:08"
        },
        {
          "id": "MDRW2100023469.1.9",
          "form": "누가 듣나 했더니~ 아직 트롯 인기가 남아있군 ㅋ",
          "original_form": "누가 듣나 했더니~ 아직 트롯 인기가 남아있군 ㅋ",
          "speaker_id": "2",
          "time": "20210830 06:08"
        },
        {
          "id": "MDRW2100023469.1.10",
          "form": "이번 주에도 음방에 나온대",
          "original_form": "이번 주에도 음방에 나온대",
          "speaker_id": "1",
          "time": "20210830 06:08"
        },
        {
          "id": "MDRW2100023469.1.11",
          "form": "24시간 곁에 있기는 ㅋ 삼각김밥 라면하나~",
          "original_form": "24시간 곁에 있기는 ㅋ 삼각김밥 라면하나~",
          "speaker_id": "2",
          "time": "20210830 06:08"
        },
        {
          "id": "MDRW2100023469.1.12",
          "form": "음악중심",
          "original_form": "음악중심",
          "speaker_id": "1",
          "time": "20210830 06:08"
        },
        {
          "id": "MDRW2100023469.1.13",
          "form": "엠비씨에서 하는거야",
          "original_form": "엠비씨에서 하는거야",
          "speaker_id": "1",
          "time": "20210830 06:09"
        },
        {
          "id": "MDRW2100023469.1.14",
          "form": "지 먹고 싶은거 사러 가는 것 같은데 ㅋ",
          "original_form": "지 먹고 싶은거 사러 가는 것 같은데 ㅋ",
          "speaker_id": "2",
          "time": "20210830 06:09"
        },
        {
          "id": "MDRW2100023469.1.15",
          "form": "가장 대중적인거지",
          "original_form": "가장 대중적인거지",
          "speaker_id": "1",
          "time": "20210830 06:09"
        },
        {
          "id": "MDRW2100023469.1.16",
          "form": "왜 엠비씨만 나오낭? 어제 에스비에스 안나옴",
          "original_form": "왜 엠비씨만 나오낭? 어제 에스비에스 안나옴",
          "speaker_id": "2",
          "time": "20210830 06:09"
        },
        {
          "id": "MDRW2100023469.1.17",
          "form": "생각보다 편의점에 많이 가더라고",
          "original_form": "생각보다 편의점에 많이 가더라고",
          "speaker_id": "1",
          "time": "20210830 06:09"
        },
        {
          "id": "MDRW2100023469.1.18",
          "form": "인기가요도 나올것 같아",
          "original_form": "인기가요도 나올것 같아",
          "speaker_id": "1",
          "time": "20210830 06:09"
        },
        {
          "id": "MDRW2100023469.1.19",
          "form": "ㅋㅋ 이제 돈 많이 벌어서 안갈듯",
          "original_form": "ㅋㅋ 이제 돈 많이 벌어서 안갈듯",
          "speaker_id": "2",
          "time": "20210830 06:09"
        }
      ]
