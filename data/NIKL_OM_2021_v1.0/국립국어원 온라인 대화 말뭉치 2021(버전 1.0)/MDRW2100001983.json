      "utterance": [
        {
          "id": "MDRW2100001983.1.1",
          "form": "안녕하세요 영화 좋아하세요?",
          "original_form": "안녕하세요 영화 좋아하세요?",
          "speaker_id": "1",
          "time": "20210819 22:37"
        },
        {
          "id": "MDRW2100001983.1.2",
          "form": "네 안녕하세요 로맨스좋아해요  고구마님은요?",
          "original_form": "네 안녕하세요 로맨스좋아해요  고구마님은요?",
          "speaker_id": "2",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001983.1.3",
          "form": "저는 액션물 좋아해요",
          "original_form": "저는 액션물 좋아해요",
          "speaker_id": "1",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001983.1.4",
          "form": "로맨스 영화 중에 추천해주실 만한 영화 잇으세요?",
          "original_form": "로맨스 영화 중에 추천해주실 만한 영화 잇으세요?",
          "speaker_id": "1",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001983.1.5",
          "form": "나의 소녀시대 대만영화 재밌게봤어요.",
          "original_form": "나의 소녀시대 대만영화 재밌게봤어요.",
          "speaker_id": "2",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001983.1.6",
          "form": "액션물은 어떤거좋아하시나ㅡ",
          "original_form": "액션물은 어떤거좋아하시나ㅡ",
          "speaker_id": "2",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001983.1.7",
          "form": "최근에 극장서 재밋게 본 영화는 킬러의 보디가드2요",
          "original_form": "최근에 극장서 재밋게 본 영화는 킬러의 보디가드2요",
          "speaker_id": "1",
          "time": "20210819 22:40"
        },
        {
          "id": "MDRW2100001983.1.8",
          "form": "나의 소녀시대 너무 유명하죠",
          "original_form": "나의 소녀시대 너무 유명하죠",
          "speaker_id": "1",
          "time": "20210819 22:41"
        },
        {
          "id": "MDRW2100001983.1.9",
          "form": "저 그 대사 좋아해요 그애 소원에 나도 잇엇음 한다?",
          "original_form": "저 그 대사 좋아해요 그애 소원에 나도 잇엇음 한다?",
          "speaker_id": "1",
          "time": "20210819 22:41"
        },
        {
          "id": "MDRW2100001983.1.10",
          "form": "킬러의 보디가드 처음 들어보는데 재밌을거같아요",
          "original_form": "킬러의 보디가드 처음 들어보는데 재밌을거같아요",
          "speaker_id": "2",
          "time": "20210819 22:41"
        },
        {
          "id": "MDRW2100001983.1.11",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210819 22:41"
        },
        {
          "id": "MDRW2100001983.1.12",
          "form": "어 저도 그 대사 좋아해요",
          "original_form": "어 저도 그 대사 좋아해요",
          "speaker_id": "2",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001983.1.13",
          "form": "코믹하면서 재밋어요",
          "original_form": "코믹하면서 재밋어요",
          "speaker_id": "1",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001983.1.14",
          "form": "너무 낭만적이면서도 뭔가 슬퍼서...",
          "original_form": "너무 낭만적이면서도 뭔가 슬퍼서...",
          "speaker_id": "1",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001983.1.15",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001983.1.16",
          "form": "내 소원에 왕대륙 당신 넣어줄수잇는데...",
          "original_form": "내 소원에 왕대륙 당신 넣어줄수잇는데...",
          "speaker_id": "1",
          "time": "20210819 22:44"
        },
        {
          "id": "MDRW2100001983.1.17",
          "form": "그 얼굴 보는게 소원인데...",
          "original_form": "그 얼굴 보는게 소원인데...",
          "speaker_id": "1",
          "time": "20210819 22:44"
        },
        {
          "id": "MDRW2100001983.1.18",
          "form": "ㅎㅎ 최근에 본 영화는 뭐에요?",
          "original_form": "ㅎㅎ 최근에 본 영화는 뭐에요?",
          "speaker_id": "2",
          "time": "20210819 22:45"
        },
        {
          "id": "MDRW2100001983.1.19",
          "form": "모가디슈요!",
          "original_form": "모가디슈요!",
          "speaker_id": "1",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100001983.1.20",
          "form": "모가디슈 보셨나요?",
          "original_form": "모가디슈 보셨나요?",
          "speaker_id": "1",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100001983.1.21",
          "form": "아 보고싶었는데 코로나 때문에ㅠ",
          "original_form": "아 보고싶었는데 코로나 때문에ㅠ",
          "speaker_id": "2",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100001983.1.22",
          "form": "신파없이 재밌다더라구요",
          "original_form": "신파없이 재밌다더라구요",
          "speaker_id": "2",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100001983.1.23",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210819 22:48"
        }
      ]
    }
