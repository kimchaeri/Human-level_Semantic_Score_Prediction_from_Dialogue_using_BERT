      "utterance": [
        {
          "id": "MDRW2100019251.1.1",
          "form": "네! 전 코카콜라파에요 ㅋㅋ",
          "original_form": "네! 전 코카콜라파에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:40"
        },
        {
          "id": "MDRW2100019251.1.2",
          "form": "오 저는 콜라보단 사이다ㅋ",
          "original_form": "오 저는 콜라보단 사이다ㅋ",
          "speaker_id": "1",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019251.1.3",
          "form": "아 저두 콜라보단 사이다좋아해요 ㅋㅋ 콜라종류물어보신건줄알았어요",
          "original_form": "아 저두 콜라보단 사이다좋아해요 ㅋㅋ 콜라종류물어보신건줄알았어요",
          "speaker_id": "2",
          "time": "20210827 16:42"
        },
        {
          "id": "MDRW2100019251.1.4",
          "form": "전 콜라보단 사이다ㅋ 사이다보단 노탄산ㅋ",
          "original_form": "전 콜라보단 사이다ㅋ 사이다보단 노탄산ㅋ",
          "speaker_id": "1",
          "time": "20210827 16:44"
        },
        {
          "id": "MDRW2100019251.1.5",
          "form": "아정말요?? 전 음료중엔 에이드 좋아해요!",
          "original_form": "아정말요?? 전 음료중엔 에이드 좋아해요!",
          "speaker_id": "2",
          "time": "20210827 16:45"
        },
        {
          "id": "MDRW2100019251.1.6",
          "form": "음 전 그냥 물이 좋아요ㅋ",
          "original_form": "음 전 그냥 물이 좋아요ㅋ",
          "speaker_id": "1",
          "time": "20210827 16:47"
        },
        {
          "id": "MDRW2100019251.1.7",
          "form": "아 이런 ㅋㅋㅋ 물좋아하시는구나",
          "original_form": "아 이런 ㅋㅋㅋ 물좋아하시는구나",
          "speaker_id": "2",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019251.1.8",
          "form": "저는 물이 그렇게 잘 안들어가더라구요",
          "original_form": "저는 물이 그렇게 잘 안들어가더라구요",
          "speaker_id": "2",
          "time": "20210827 16:48"
        },
        {
          "id": "MDRW2100019251.1.9",
          "form": "헛 글쿤용ㅋ 음료수는 식혜 좋아요ㅋ",
          "original_form": "헛 글쿤용ㅋ 음료수는 식혜 좋아요ㅋ",
          "speaker_id": "1",
          "time": "20210827 16:56"
        },
        {
          "id": "MDRW2100019251.1.10",
          "form": "아하! 그럼 수정과도 좋아하시나요?",
          "original_form": "아하! 그럼 수정과도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 18:28"
        },
        {
          "id": "MDRW2100019251.1.11",
          "form": "엇 수정과 좋아해요ㅋ",
          "original_form": "엇 수정과 좋아해요ㅋ",
          "speaker_id": "1",
          "time": "20210827 21:27"
        },
        {
          "id": "MDRW2100019251.1.12",
          "form": "식혜도좋아요ㅋ",
          "original_form": "식혜도좋아요ㅋ",
          "speaker_id": "1",
          "time": "20210827 21:27"
        },
        {
          "id": "MDRW2100019251.1.13",
          "form": "아하! 전통음료 좋아하시는구나",
          "original_form": "아하! 전통음료 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100019251.1.14",
          "form": "몰라요ㅋ 쌍화차 이런 건 안 좋아하는데...ㅜㅜ",
          "original_form": "몰라요ㅋ 쌍화차 이런 건 안 좋아하는데...ㅜㅜ",
          "speaker_id": "1",
