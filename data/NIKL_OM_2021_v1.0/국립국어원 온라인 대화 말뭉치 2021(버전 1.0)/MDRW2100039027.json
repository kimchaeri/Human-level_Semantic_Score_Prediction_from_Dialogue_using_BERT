      "utterance": [
        {
          "id": "MDRW2100039027.1.1",
          "form": "요즘 뭐 보시는거잇나요",
          "original_form": "요즘 뭐 보시는거잇나요",
          "speaker_id": "1",
          "time": "20210903 02:30"
        },
        {
          "id": "MDRW2100039027.1.2",
          "form": "전 요즘 미드 왕좌의 게임 봐요ㅎㅎ",
          "original_form": "전 요즘 미드 왕좌의 게임 봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 02:36"
        },
        {
          "id": "MDRW2100039027.1.3",
          "form": "오 ㅋㅋ ㅋ 왓챠?",
          "original_form": "오 ㅋㅋ ㅋ 왓챠?",
          "speaker_id": "1",
          "time": "20210903 02:36"
        },
        {
          "id": "MDRW2100039027.1.4",
          "form": "네 왓챠써요ㅎㅎ",
          "original_form": "네 왓챠써요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 02:40"
        },
        {
          "id": "MDRW2100039027.1.5",
          "form": "시즌몇이용 ㅋㅋ",
          "original_form": "시즌몇이용 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 02:44"
        },
        {
          "id": "MDRW2100039027.1.6",
          "form": "시즌6보고있어요 제가 시즌8만 빼놓고 다봤엇거든요",
          "original_form": "시즌6보고있어요 제가 시즌8만 빼놓고 다봤엇거든요",
          "speaker_id": "2",
          "time": "20210903 02:46"
        },
        {
          "id": "MDRW2100039027.1.7",
          "form": "시즌8 바로 보기엔 기억이 안나가지고 재탕하고 있는거에요ㅋㅋ",
          "original_form": "시즌8 바로 보기엔 기억이 안나가지고 재탕하고 있는거에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 02:47"
        },
        {
          "id": "MDRW2100039027.1.8",
          "form": "ㅋㅋㅋㅋㅋ 다들...결말 똥이라해서",
          "original_form": "ㅋㅋㅋㅋㅋ 다들...결말 똥이라해서",
          "speaker_id": "1",
          "time": "20210903 02:49"
        },
        {
          "id": "MDRW2100039027.1.9",
          "form": "못보겟더라구요 전",
          "original_form": "못보겟더라구요 전",
          "speaker_id": "1",
          "time": "20210903 02:50"
        },
        {
          "id": "MDRW2100039027.1.10",
          "form": "name1님은 왕겜 보셨나요?",
          "original_form": "&name1&님은 왕겜 보셨나요?",
          "speaker_id": "2",
          "time": "20210903 02:50"
        },
        {
          "id": "MDRW2100039027.1.11",
          "form": "저 시즌 4까진가...ㅋㅋㅋㅋㅋ 본지오래됏어요",
          "original_form": "저 시즌 4까진가...ㅋㅋㅋㅋㅋ 본지오래됏어요",
          "speaker_id": "1",
          "time": "20210903 02:52"
        },
        {
          "id": "MDRW2100039027.1.12",
          "form": "대너리스참 응원햇는데 ㅋㅋ",
          "original_form": "대너리스참 응원햇는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 02:52"
        },
        {
          "id": "MDRW2100039027.1.13",
          "form": "대너리스 넘 좋죠ㅠㅠㅠㅠ 분위기도 넘 예쁘고요ㅜㅜ",
          "original_form": "대너리스 넘 좋죠ㅠㅠㅠㅠ 분위기도 넘 예쁘고요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 02:52"
        },
        {
          "id": "MDRW2100039027.1.14",
          "form": "진짜넘예뻐요 진짜 여왕재질ㅋㅋㅋㅋ",
          "original_form": "진짜넘예뻐요 진짜 여왕재질ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 02:53"
        },
        {
          "id": "MDRW2100039027.1.15",
          "form": "그쵸ㅠㅠ 왕겜이 보다보니까 진 주인공은 대너리스가 아닐까 싶을 정도로 서사도 좋더라고요ㅋㅋ",
          "original_form": "그쵸ㅠㅠ 왕겜이 보다보니까 진 주인공은 대너리스가 아닐까 싶을 정도로 서사도 좋더라고요ㅋㅋ",
