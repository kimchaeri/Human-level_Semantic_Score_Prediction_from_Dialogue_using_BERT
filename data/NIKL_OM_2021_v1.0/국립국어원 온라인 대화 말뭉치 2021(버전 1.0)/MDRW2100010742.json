      "utterance": [
        {
          "id": "MDRW2100010742.1.1",
          "form": "요리잘하세요?",
          "original_form": "요리잘하세요?",
          "speaker_id": "1",
          "time": "20210823 21:22"
        },
        {
          "id": "MDRW2100010742.1.2",
          "form": "아뇨 ㅠ",
          "original_form": "아뇨 ㅠ",
          "speaker_id": "2",
          "time": "20210823 21:22"
        },
        {
          "id": "MDRW2100010742.1.3",
          "form": "잘하시나요?",
          "original_form": "잘하시나요?",
          "speaker_id": "2",
          "time": "20210823 21:22"
        },
        {
          "id": "MDRW2100010742.1.4",
          "form": "저는 요리 못해요ㅠㅠ 배달음식 자주 드시나요?",
          "original_form": "저는 요리 못해요ㅠㅠ 배달음식 자주 드시나요?",
          "speaker_id": "1",
          "time": "20210823 21:23"
        },
        {
          "id": "MDRW2100010742.1.5",
          "form": "배달이 짱이죠",
          "original_form": "배달이 짱이죠",
          "speaker_id": "2",
          "time": "20210823 21:24"
        },
        {
          "id": "MDRW2100010742.1.6",
          "form": "갑자기 초밥이 생각나네요",
          "original_form": "갑자기 초밥이 생각나네요",
          "speaker_id": "2",
          "time": "20210823 21:24"
        },
        {
          "id": "MDRW2100010742.1.7",
          "form": "어떤거 주로 드세요?",
          "original_form": "어떤거 주로 드세요?",
          "speaker_id": "1",
          "time": "20210823 21:24"
        },
        {
          "id": "MDRW2100010742.1.8",
          "form": "족발이요 보쌈이랑",
          "original_form": "족발이요 보쌈이랑",
          "speaker_id": "2",
          "time": "20210823 21:24"
        },
        {
          "id": "MDRW2100010742.1.9",
          "form": "맛있겠네요 어떤 어플 주로 쓰세요?",
          "original_form": "맛있겠네요 어떤 어플 주로 쓰세요?",
          "speaker_id": "1",
          "time": "20210823 21:26"
        },
        {
          "id": "MDRW2100010742.1.10",
          "form": "배민이요",
          "original_form": "배민이요",
          "speaker_id": "2",
          "time": "20210823 21:26"
        },
        {
          "id": "MDRW2100010742.1.11",
          "form": "당신은?",
          "original_form": "당신은?",
          "speaker_id": "2",
          "time": "20210823 21:26"
        },
        {
          "id": "MDRW2100010742.1.12",
          "form": "저도 배민을 주로 써요 맛집 찾아다니시나요?",
          "original_form": "저도 배민을 주로 써요 맛집 찾아다니시나요?",
          "speaker_id": "1",
          "time": "20210823 21:27"
        },
        {
          "id": "MDRW2100010742.1.13",
          "form": "아뇨 기다리는게 젤싫어서요",
          "original_form": "아뇨 기다리는게 젤싫어서요",
          "speaker_id": "2",
          "time": "20210823 21:27"
        },
        {
          "id": "MDRW2100010742.1.14",
          "form": "유명 카페도 잘 안가시나요?",
          "original_form": "유명 카페도 잘 안가시나요?",
          "speaker_id": "1",
          "time": "20210823 21:29"
        },
        {
          "id": "MDRW2100010742.1.15",
          "form": "카페도 유명한게 있나요?",
          "original_form": "카페도 유명한게 있나요?",
          "speaker_id": "2",
          "time": "20210823 21:29"
        },
        {
          "id": "MDRW2100010742.1.16",
          "form": "요새 인스타에 유명한 카페들 많더라구요 그런데 저는 안가봤어요",
          "original_form": "요새 인스타에 유명한 카페들 많더라구요 그런데 저는 안가봤어요",
          "speaker_id": "1",
          "time": "20210823 21:30"
        },
        {
          "id": "MDRW2100010742.1.17",
          "form": "로제떡볶이 먹어봤어여?",
          "original_form": "로제떡볶이 먹어봤어여?",
          "speaker_id": "1",
