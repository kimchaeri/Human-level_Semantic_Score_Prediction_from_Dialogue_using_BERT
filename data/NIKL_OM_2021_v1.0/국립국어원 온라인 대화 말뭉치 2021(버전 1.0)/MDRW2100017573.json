      "utterance": [
        {
          "id": "MDRW2100017573.1.1",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "1",
          "time": "20210826 20:08"
        },
        {
          "id": "MDRW2100017573.1.2",
          "form": "네네 먹었습니다 ㅎㅎ 갈비 먹었어요",
          "original_form": "네네 먹었습니다 ㅎㅎ 갈비 먹었어요",
          "speaker_id": "2",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100017573.1.3",
          "form": "헉 갈비 맛있겠다ㅠㅜㅠ 외식 하셨어요?",
          "original_form": "헉 갈비 맛있겠다ㅠㅜㅠ 외식 하셨어요?",
          "speaker_id": "1",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100017573.1.4",
          "form": "아뇨 그냥 집에서 엄마가 해주셨어여 ㅎㅎ 저녁 뭐드셨나요?",
          "original_form": "아뇨 그냥 집에서 엄마가 해주셨어여 ㅎㅎ 저녁 뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100017573.1.5",
          "form": "저는 아직 안 먹었어요!",
          "original_form": "저는 아직 안 먹었어요!",
          "speaker_id": "1",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017573.1.6",
          "form": "점심을 늦게 먹어서요",
          "original_form": "점심을 늦게 먹어서요",
          "speaker_id": "1",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017573.1.7",
          "form": "배 안고프세요?ㅠㅠ 점심은 뭐드셨나요?",
          "original_form": "배 안고프세요?ㅠㅠ 점심은 뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100017573.1.8",
          "form": "늦었지만 지금 먹으려구요!!",
          "original_form": "늦었지만 지금 먹으려구요!!",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017573.1.9",
          "form": "점심은 김치찌개 먹었어요ㅎㅎ",
          "original_form": "점심은 김치찌개 먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017573.1.10",
          "form": "오 김치찌개 좋죠! 지금은 뭐드시려구요~?",
          "original_form": "오 김치찌개 좋죠! 지금은 뭐드시려구요~?",
          "speaker_id": "2",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017573.1.11",
          "form": "저녁에 남은 김치찌개 해치우고",
          "original_form": "저녁에 남은 김치찌개 해치우고",
          "speaker_id": "1",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100017573.1.12",
          "form": "복숭아 먹었어요!",
          "original_form": "복숭아 먹었어요!",
          "speaker_id": "1",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100017573.1.13",
          "form": "말복이랑 딱복 중에 어떤 거 좋아하세요?",
          "original_form": "말복이랑 딱복 중에 어떤 거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100017573.1.14",
          "form": "저는 말랑말랑한거요~",
          "original_form": "저는 말랑말랑한거요~",
          "speaker_id": "2",
          "time": "20210827 07:54"
        },
        {
          "id": "MDRW2100017573.1.15",
          "form": "저두요! 이번 여름에 말랑한 복숭아 엄청 먹었어요ㅋㅋㅋ",
          "original_form": "저두요! 이번 여름에 말랑한 복숭아 엄청 먹었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:44"
        },
        {
          "id": "MDRW2100017573.1.16",
          "form": "복숭아 맛있죠 ㅠㅠ 근데 전 먹으면 몸이 근지러워서 많이는 못먹어요",
          "original_form": "복숭아 맛있죠 ㅠㅠ 근데 전 먹으면 몸이 근지러워서 많이는 못먹어요",
          "speaker_id": "2",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100017573.1.17",
          "form": "헉 알레르기 있으신가봐요!ㅠㅠㅠ",
          "original_form": "헉 알레르기 있으신가봐요!ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100017573.1.18",
          "form": "그런가봐요 ㅠㅠ 원래는 안그랬는데 갑자기 게장도 알레르기 생기고..넘 슬퍼요",
          "original_form": "그런가봐요 ㅠㅠ 원래는 안그랬는데 갑자기 게장도 알레르기 생기고..넘 슬퍼요",
          "speaker_id": "2",
          "time": "20210827 18:04"
        },
        {
          "id": "MDRW2100017573.1.19",
          "form": "아이구ㅠㅜㅠ 알레르기가 커가면서 하나둘씩 생긴대요ㅠㅜㅠ",
          "original_form": "아이구ㅠㅜㅠ 알레르기가 커가면서 하나둘씩 생긴대요ㅠㅜㅠ",
          "speaker_id": "1",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100017573.1.20",
          "form": "맛있는 거 못 먹는건 진짜 슬플 거 같아요ㅠㅠ",
          "original_form": "맛있는 거 못 먹는건 진짜 슬플 거 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100017573.1.21",
          "form": "그런가봐요 ㅠㅠㅠ 그래도 알레르기 참으면서 억지로라도 먹고있어요",
          "original_form": "그런가봐요 ㅠㅠㅠ 그래도 알레르기 참으면서 억지로라도 먹고있어요",
          "speaker_id": "2",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100017573.1.22",
          "form": "알레르기 반응이 너무 심하지 않으면 그냥 먹더라구요ㅎㅎ",
          "original_form": "알레르기 반응이 너무 심하지 않으면 그냥 먹더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100017573.1.23",
          "form": "맛있어서ㅠㅠ",
          "original_form": "맛있어서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100017573.1.24",
          "form": "맞아요 ㅎㅎ 그냥 먹게되더라구요..오늘 저녁은 뭐드셧나요?!",
          "original_form": "맞아요 ㅎㅎ 그냥 먹게되더라구요..오늘 저녁은 뭐드셧나요?!",
