      "utterance": [
        {
          "id": "MDRW2100027756.1.1",
          "form": "맞아요 트렌치코트도 입고싶은데",
          "original_form": "맞아요 트렌치코트도 입고싶은데",
          "speaker_id": "1",
          "time": "20210902 14:46"
        },
        {
          "id": "MDRW2100027756.1.2",
          "form": "트렌치코트는 입을 엄두가 안나요",
          "original_form": "트렌치코트는 입을 엄두가 안나요",
          "speaker_id": "2",
          "time": "20210902 14:47"
        },
        {
          "id": "MDRW2100027756.1.3",
          "form": "안어울릴까봐. ..",
          "original_form": "안어울릴까봐. ..",
          "speaker_id": "2",
          "time": "20210902 14:47"
        },
        {
          "id": "MDRW2100027756.1.4",
          "form": "저도 사실.. 장바구니에만 있고 못샀어요ㅋㅋㅋ쿠ㅜㅜ",
          "original_form": "저도 사실.. 장바구니에만 있고 못샀어요ㅋㅋㅋ쿠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 14:50"
        },
        {
          "id": "MDRW2100027756.1.5",
          "form": "저도요... 무엇보다 입을 날도 많이 없네요",
          "original_form": "저도요... 무엇보다 입을 날도 많이 없네요",
          "speaker_id": "2",
          "time": "20210902 14:50"
        },
        {
          "id": "MDRW2100027756.1.6",
          "form": "맞아요.. 금방 롱패딩꺼내야해요ㅋㅋㅋ..",
          "original_form": "맞아요.. 금방 롱패딩꺼내야해요ㅋㅋㅋ..",
          "speaker_id": "1",
          "time": "20210902 14:51"
        },
        {
          "id": "MDRW2100027756.1.7",
          "form": "맞아요 지금 장마 이후에 비오면 바로 겨울일거 같아요",
          "original_form": "맞아요 지금 장마 이후에 비오면 바로 겨울일거 같아요",
          "speaker_id": "2",
          "time": "20210902 14:53"
        },
        {
          "id": "MDRW2100027756.1.8",
          "form": "벌써그럼 안되는데ㅠ 추운거 너무 싫어용",
          "original_form": "벌써그럼 안되는데ㅠ 추운거 너무 싫어용",
          "speaker_id": "1",
          "time": "20210902 14:57"
        },
        {
          "id": "MDRW2100027756.1.9",
          "form": "저는 겨울이 좋아서...ㅎㅎ",
          "original_form": "저는 겨울이 좋아서...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027756.1.10",
          "form": "앗 그렇군요! 저도 눈오는건 좋아해요",
          "original_form": "앗 그렇군요! 저도 눈오는건 좋아해요",
          "speaker_id": "1",
          "time": "20210902 15:09"
        },
        {
          "id": "MDRW2100027756.1.11",
          "form": "눈오는 날씨는 진짜 특별한거 같아요",
          "original_form": "눈오는 날씨는 진짜 특별한거 같아요",
          "speaker_id": "2",
          "time": "20210902 15:13"
        },
        {
          "id": "MDRW2100027756.1.12",
          "form": "포근하고 편안한게",
          "original_form": "포근하고 편안한게",
          "speaker_id": "2",
          "time": "20210902 15:13"
        },
        {
          "id": "MDRW2100027756.1.13",
          "form": "맞아요 눈올때가 제일좋아요",
          "original_form": "맞아요 눈올때가 제일좋아요",
          "speaker_id": "1",
          "time": "20210902 15:14"
        },
        {
          "id": "MDRW2100027756.1.14",
          "form": "느리게 내리는게 시간이 멈춘거 같기도 하죠",
          "original_form": "느리게 내리는게 시간이 멈춘거 같기도 하죠",
          "speaker_id": "2",
          "time": "20210902 15:14"
        },
        {
          "id": "MDRW2100027756.1.15",
          "form": "소복소복 쌓이는거 보면 포근해져요",
          "original_form": "소복소복 쌓이는거 보면 포근해져요",
          "speaker_id": "1",
          "time": "20210902 15:15"
        }
