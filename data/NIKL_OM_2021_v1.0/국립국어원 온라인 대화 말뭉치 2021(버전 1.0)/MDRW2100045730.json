      "utterance": [
        {
          "id": "MDRW2100045730.1.1",
          "form": "역시 설빙하면 딸기빙수죠!!",
          "original_form": "역시 설빙하면 딸기빙수죠!!",
          "speaker_id": "2",
          "time": "20210908 06:18"
        },
        {
          "id": "MDRW2100045730.1.2",
          "form": "크으으 맛잘알이시네요!",
          "original_form": "크으으 맛잘알이시네요!",
          "speaker_id": "1",
          "time": "20210908 06:19"
        },
        {
          "id": "MDRW2100045730.1.3",
          "form": "설빙은 과일이 싱싱해서 좋아요",
          "original_form": "설빙은 과일이 싱싱해서 좋아요",
          "speaker_id": "1",
          "time": "20210908 06:19"
        },
        {
          "id": "MDRW2100045730.1.4",
          "form": "맞아요ㅠㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:20"
        },
        {
          "id": "MDRW2100045730.1.5",
          "form": "가끔은 더 많이 넣어줬으면 좋겠어요ㅠ",
          "original_form": "가끔은 더 많이 넣어줬으면 좋겠어요ㅠ",
          "speaker_id": "2",
          "time": "20210908 06:20"
        },
        {
          "id": "MDRW2100045730.1.6",
          "form": "맞아요ㅠㅠㅠ 듬뿍 넣어주었으면!ㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠ 듬뿍 넣어주었으면!ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:22"
        },
        {
          "id": "MDRW2100045730.1.7",
          "form": "설빙 사이드도 맛있어요",
          "original_form": "설빙 사이드도 맛있어요",
          "speaker_id": "1",
          "time": "20210908 06:22"
        },
        {
          "id": "MDRW2100045730.1.8",
          "form": "가래떡에 치즈 올라간 거ㅠㅠ",
          "original_form": "가래떡에 치즈 올라간 거ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:22"
        },
        {
          "id": "MDRW2100045730.1.9",
          "form": "사이드는 안먹어봤는데 그게 그렇게 맛있나요??",
          "original_form": "사이드는 안먹어봤는데 그게 그렇게 맛있나요??",
          "speaker_id": "2",
          "time": "20210908 06:22"
        },
        {
          "id": "MDRW2100045730.1.10",
          "form": "네 점바점인데 괜찮았어요!",
          "original_form": "네 점바점인데 괜찮았어요!",
          "speaker_id": "1",
          "time": "20210908 06:23"
        },
        {
          "id": "MDRW2100045730.1.11",
          "form": "인절미 토스트도 맛있구요ㅋㅋ",
          "original_form": "인절미 토스트도 맛있구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:23"
        },
        {
          "id": "MDRW2100045730.1.12",
          "form": "그건 맛이 없을 수가 없네요ㅠㅠㅠㅠㅠ",
          "original_form": "그건 맛이 없을 수가 없네요ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:24"
        },
        {
          "id": "MDRW2100045730.1.13",
          "form": "아아ㅠㅠㅠㅠ 먹고 싶은게 점점 많아져요ㅠㅠ",
          "original_form": "아아ㅠㅠㅠㅠ 먹고 싶은게 점점 많아져요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:24"
        },
        {
          "id": "MDRW2100045730.1.14",
          "form": "그게 세상사는 재미죠!",
          "original_form": "그게 세상사는 재미죠!",
          "speaker_id": "1",
          "time": "20210908 06:25"
        },
        {
          "id": "MDRW2100045730.1.15",
          "form": "같이 맛있게 살아요ㅋㅋㅋ",
          "original_form": "같이 맛있게 살아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:25"
        },
        {
          "id": "MDRW2100045730.1.16",
          "form": "ㅋㅋㅋㅋ 좋아요ㅠㅠ 진짜 상상만으로 좋네요ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋ 좋아요ㅠㅠ 진짜 상상만으로 좋네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 06:25"
        },
        {
          "id": "MDRW2100045730.1.17",
          "form": "이제 가을이니 과일도 나오겠네요ㅠㅠ",
          "original_form": "이제 가을이니 과일도 나오겠네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:26"
        },
        {
          "id": "MDRW2100045730.1.18",
          "form": "배랑 사과ㅠㅠㅠ",
          "original_form": "배랑 사과ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:27"
        },
        {
          "id": "MDRW2100045730.1.19",
          "form": "사과 너무 기다리고 있었어요ㅠㅠ",
          "original_form": "사과 너무 기다리고 있었어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:27"
        },
        {
          "id": "MDRW2100045730.1.20",
          "form": "좀 싸게 먹으려면 추석 지나서를 가다려야겠지만요ㅠㅠ",
          "original_form": "좀 싸게 먹으려면 추석 지나서를 가다려야겠지만요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:28"
        },
        {
          "id": "MDRW2100045730.1.21",
          "form": "저도 사과 너무 너무 좋아해요ㅠㅠ",
          "original_form": "저도 사과 너무 너무 좋아해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:28"
        },
        {
          "id": "MDRW2100045730.1.22",
          "form": "아삭아삭한 식감하며ㅠㅠ",
          "original_form": "아삭아삭한 식감하며ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:28"
        },
        {
          "id": "MDRW2100045730.1.23",
          "form": "달달하면서 은은한 새콤함ㅠㅠㅠ",
          "original_form": "달달하면서 은은한 새콤함ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:29"
        },
        {
          "id": "MDRW2100045730.1.24",
          "form": "허허 얼렁 먹고싶네요ㅠㅠ",
          "original_form": "허허 얼렁 먹고싶네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 06:29"
        },
        {
          "id": "MDRW2100045730.1.25",
          "form": "크으으으ㅠㅠㅠㅠㅠ",
          "original_form": "크으으으ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:29"
        },
        {
          "id": "MDRW2100045730.1.26",
          "form": "진짜 과일의 왕이에요!",
          "original_form": "진짜 과일의 왕이에요!",
          "speaker_id": "1",
          "time": "20210908 06:29"
        }
      ]
    }
