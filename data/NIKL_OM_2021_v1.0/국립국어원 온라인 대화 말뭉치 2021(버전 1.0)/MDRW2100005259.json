        },
        {
          "id": "MDRW2100005259.1.2",
          "form": "메타버스 들어봤지",
          "original_form": "메타버스 들어봤지",
          "speaker_id": "1",
          "time": "20210821 21:32"
        },
        {
          "id": "MDRW2100005259.1.3",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100005259.1.4",
          "form": "인교세에서 맨날 들음",
          "original_form": "인교세에서 맨날 들음",
          "speaker_id": "2",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100005259.1.5",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100005259.1.6",
          "form": "아ㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005259.1.7",
          "form": "메타버스 안에 직업 생긴다는데",
          "original_form": "메타버스 안에 직업 생긴다는데",
          "speaker_id": "1",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005259.1.8",
          "form": "좀 재밌을 거 같음",
          "original_form": "좀 재밌을 거 같음",
          "speaker_id": "1",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005259.1.9",
          "form": "나 메타버스에서 치킨 장사해~",
          "original_form": "나 메타버스에서 치킨 장사해~",
          "speaker_id": "1",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005259.1.10",
          "form": "헐 신기해",
          "original_form": "헐 신기해",
          "speaker_id": "2",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005259.1.11",
          "form": "나는 지게차 운전사 할래",
          "original_form": "나는 지게차 운전사 할래",
          "speaker_id": "2",
          "time": "20210821 21:36"
        },
        {
          "id": "MDRW2100005259.1.12",
          "form": "제페토도 나이키나 브랜드 콜라보도 하고 그 안에서 디자이너로 수입 만드는 사람도 있고",
          "original_form": "제페토도 나이키나 브랜드 콜라보도 하고 그 안에서 디자이너로 수입 만드는 사람도 있고",
          "speaker_id": "1",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100005259.1.13",
          "form": "진짜 메타버스 직업만 가지는 사람도 생길 듯",
          "original_form": "진짜 메타버스 직업만 가지는 사람도 생길 듯",
          "speaker_id": "1",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005259.1.14",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005259.1.15",
          "form": "VR 시장도 다시 커졌대",
          "original_form": "VR 시장도 다시 커졌대",
          "speaker_id": "1",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005259.1.16",
          "form": "신기하다",
          "original_form": "신기하다",
          "speaker_id": "2",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005259.1.17",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "1",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005259.1.18",
          "form": "외국에서 제페토 시장 엄청 크다는데",
          "original_form": "외국에서 제페토 시장 엄청 크다는데",
          "speaker_id": "1",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005259.1.19",
          "form": "맞어",
          "original_form": "맞어",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005259.1.20",
          "form": "제페토 우익이긴 한데",
          "original_form": "제페토 우익이긴 한데",
          "speaker_id": "2",
          "time": "20210821 21:50"
        },
        {
          "id": "MDRW2100005259.1.21",
          "form": "아 진짜?",
          "original_form": "아 진짜?",
          "speaker_id": "1",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005259.1.22",
          "form": "그런 논란 있었던 거 같긴하다",
          "original_form": "그런 논란 있었던 거 같긴하다",
          "speaker_id": "1",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005259.1.23",
          "form": "관심이 없어서 잘 몰랐음",
          "original_form": "관심이 없어서 잘 몰랐음",
          "speaker_id": "1",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005259.1.24",
          "form": "그랬던 듯?",
          "original_form": "그랬던 듯?",
          "speaker_id": "2",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005259.1.25",
          "form": "제페토 접고 나서 알았었으",
          "original_form": "제페토 접고 나서 알았었으",
