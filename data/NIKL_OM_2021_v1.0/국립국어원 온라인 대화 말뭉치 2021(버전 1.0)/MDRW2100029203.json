      "utterance": [
        {
          "id": "MDRW2100029203.1.1",
          "form": "동반할인도돼서",
          "original_form": "동반할인도돼서",
          "speaker_id": "2",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100029203.1.2",
          "form": "둘째는 간 지 몇 년 몇 달 됐어요",
          "original_form": "둘째는 간 지 몇 년 몇 달 됐어요",
          "speaker_id": "1",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100029203.1.3",
          "form": "첫째 휴가나올때",
          "original_form": "첫째 휴가나올때",
          "speaker_id": "2",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100029203.1.4",
          "form": "코로나아니었으니까",
          "original_form": "코로나아니었으니까",
          "speaker_id": "2",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100029203.1.5",
          "form": "할인제도도 많나 보네요",
          "original_form": "할인제도도 많나 보네요",
          "speaker_id": "1",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100029203.1.6",
          "form": "영화도 보러다니고",
          "original_form": "영화도 보러다니고",
          "speaker_id": "2",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100029203.1.7",
          "form": "와 좋네",
          "original_form": "와 좋네",
          "speaker_id": "1",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100029203.1.8",
          "form": "둘째 제대한지 됐고",
          "original_form": "둘째 제대한지 됐고",
          "speaker_id": "2",
          "time": "20210903 00:15"
        },
        {
          "id": "MDRW2100029203.1.9",
          "form": "지금 회사다님",
          "original_form": "지금 회사다님",
          "speaker_id": "2",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100029203.1.10",
          "form": "앞 업체를 했어요 다행이다",
          "original_form": "앞 업체를 했어요 다행이다",
          "speaker_id": "1",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100029203.1.11",
          "form": "혜택이 진짜 좋음",
          "original_form": "혜택이 진짜 좋음",
          "speaker_id": "2",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100029203.1.12",
          "form": "이 특하네요 요리 잘하는 동생",
          "original_form": "이 특하네요 요리 잘하는 동생",
          "speaker_id": "1",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100029203.1.13",
          "form": "넷중3명 요리다잘함",
          "original_form": "넷중3명 요리다잘함",
          "speaker_id": "2",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100029203.1.14",
          "form": "내 명이에요 조카가",
          "original_form": "내 명이에요 조카가",
          "speaker_id": "1",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100029203.1.15",
          "form": "막내빼고 다잘함",
          "original_form": "막내빼고 다잘함",
          "speaker_id": "2",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100029203.1.16",
          "form": "사촌임ㅋㅋㅋ",
          "original_form": "사촌임ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100029203.1.17",
          "form": "애들이 군대가기전에도",
          "original_form": "애들이 군대가기전에도",
          "speaker_id": "2",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100029203.1.18",
          "form": "운동하고",
          "original_form": "운동하고",
          "speaker_id": "2",
          "time": "20210903 00:16"
        },
        {
          "id": "MDRW2100029203.1.19",
          "form": "군대가서도",
          "original_form": "군대가서도",
          "speaker_id": "2",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100029203.1.20",
          "form": "운동겁나게해서",
          "original_form": "운동겁나게해서",
          "speaker_id": "2",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100029203.1.21",
          "form": "아 사촌  좀 드리구나",
          "original_form": "아 사촌  좀 드리구나",
          "speaker_id": "1",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100029203.1.22",
          "form": "몸이좋았는데",
          "original_form": "몸이좋았는데",
          "speaker_id": "2",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100029203.1.23",
          "form": "첫째가",
          "original_form": "첫째가",
          "speaker_id": "2",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100029203.1.24",
          "form": "역삼각형",
          "original_form": "역삼각형",
          "speaker_id": "1",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100029203.1.25",
          "form": "사진찍었음",
          "original_form": "사진찍었음",
          "speaker_id": "2",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100029203.1.26",
          "form": "휴가나올때 맞촸었나",
          "original_form": "휴가나올때 맞촸었나",
          "speaker_id": "2",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100029203.1.27",
          "form": "바디프로필",
          "original_form": "바디프로필",
          "speaker_id": "2",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100029203.1.28",
          "form": "전공이 사진이에요",
          "original_form": "전공이 사진이에요",
          "speaker_id": "1",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100029203.1.29",
          "form": "야바디 프로필 멋있었겠네",
          "original_form": "야바디 프로필 멋있었겠네",
          "speaker_id": "1",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100029203.1.30",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100029203.1.31",
          "form": "그리고",
          "original_form": "그리고",
          "speaker_id": "2",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100029203.1.32",
          "form": "둘째가",
          "original_form": "둘째가",
          "speaker_id": "2",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100029203.1.33",
          "form": "군대갔을땐",
          "original_form": "군대갔을땐",
          "speaker_id": "2",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100029203.1.34",
          "form": "코로나때여서",
          "original_form": "코로나때여서",
          "speaker_id": "2",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100029203.1.35",
          "form": "마스크 쓰고 나온거였나",
          "original_form": "마스크 쓰고 나온거였나",
          "speaker_id": "2",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100029203.1.36",
          "form": "들고온게",
          "original_form": "들고온게",
          "speaker_id": "2",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100029203.1.37",
          "form": "아코라나 땜에 힘들었을 텐데",
          "original_form": "아코라나 땜에 힘들었을 텐데",
          "speaker_id": "1",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100029203.1.38",
          "form": "군대 특유무늬",
          "original_form": "군대 특유무늬",
          "speaker_id": "2",
          "time": "20210903 00:18"
        },
        {
          "id": "MDRW2100029203.1.39",
          "form": "그 마스크였는데 존예",
          "original_form": "그 마스크였는데 존예",
          "speaker_id": "2",
