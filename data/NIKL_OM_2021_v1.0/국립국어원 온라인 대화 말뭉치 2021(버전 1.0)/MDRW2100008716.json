      "utterance": [
        {
          "id": "MDRW2100008716.1.1",
          "form": "곧 개강이시겠네요?",
          "original_form": "곧 개강이시겠네요?",
          "speaker_id": "1",
          "time": "20210822 17:45"
        },
        {
          "id": "MDRW2100008716.1.2",
          "form": "마지막 방학을 즐기고 있습니다",
          "original_form": "마지막 방학을 즐기고 있습니다",
          "speaker_id": "2",
          "time": "20210822 17:50"
        },
        {
          "id": "MDRW2100008716.1.3",
          "form": "코로나 때문에 강의는 정상적으로 진행되나요?",
          "original_form": "코로나 때문에 강의는 정상적으로 진행되나요?",
          "speaker_id": "1",
          "time": "20210822 17:53"
        },
        {
          "id": "MDRW2100008716.1.4",
          "form": "4단계 동안은 전부 비대면이래요",
          "original_form": "4단계 동안은 전부 비대면이래요",
          "speaker_id": "2",
          "time": "20210822 17:54"
        },
        {
          "id": "MDRW2100008716.1.5",
          "form": "비대면하면서 등록금은 원래대로 받는 대(단한)학교...",
          "original_form": "비대면하면서 등록금은 원래대로 받는 대(단한)학교...",
          "speaker_id": "2",
          "time": "20210822 17:54"
        },
        {
          "id": "MDRW2100008716.1.6",
          "form": "정말 등록금 아깝다...",
          "original_form": "정말 등록금 아깝다...",
          "speaker_id": "1",
          "time": "20210822 17:55"
        },
        {
          "id": "MDRW2100008716.1.7",
          "form": "학교도 제대로 못가는데",
          "original_form": "학교도 제대로 못가는데",
          "speaker_id": "1",
          "time": "20210822 17:56"
        },
        {
          "id": "MDRW2100008716.1.8",
          "form": "비대면이면 집에서 인터넷강의 듣는건가요",
          "original_form": "비대면이면 집에서 인터넷강의 듣는건가요",
          "speaker_id": "1",
          "time": "20210822 17:56"
        },
        {
          "id": "MDRW2100008716.1.9",
          "form": "넵 화상 강의로 진행해요",
          "original_form": "넵 화상 강의로 진행해요",
          "speaker_id": "2",
          "time": "20210822 17:57"
        },
        {
          "id": "MDRW2100008716.1.10",
          "form": "동기들도 잘 못보겠네요",
          "original_form": "동기들도 잘 못보겠네요",
          "speaker_id": "1",
          "time": "20210822 17:57"
        },
        {
          "id": "MDRW2100008716.1.11",
          "form": "아쉽겠다",
          "original_form": "아쉽겠다",
          "speaker_id": "1",
          "time": "20210822 17:57"
        },
        {
          "id": "MDRW2100008716.1.12",
          "form": "1학기때 기껏 친구 많이 만들었더니",
          "original_form": "1학기때 기껏 친구 많이 만들었더니",
          "speaker_id": "2",
          "time": "20210822 17:59"
        },
        {
          "id": "MDRW2100008716.1.13",
          "form": "만나기가 힘드네요",
          "original_form": "만나기가 힘드네요",
          "speaker_id": "2",
          "time": "20210822 18:00"
        },
        {
          "id": "MDRW2100008716.1.14",
          "form": "안타까운일이다",
          "original_form": "안타까운일이다",
          "speaker_id": "1",
          "time": "20210822 18:01"
        },
        {
          "id": "MDRW2100008716.1.15",
          "form": "전공은 뭐에요?",
          "original_form": "전공은 뭐에요?",
          "speaker_id": "1",
          "time": "20210822 18:01"
        },
        {
          "id": "MDRW2100008716.1.16",
          "form": "디자인 전공하고 있습니다ㅎㅎ",
          "original_form": "디자인 전공하고 있습니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 18:02"
        },
        {
          "id": "MDRW2100008716.1.17",
          "form": "디자인 전공이면 시험은 과제대체하나요?",
          "original_form": "디자인 전공이면 시험은 과제대체하나요?",
          "speaker_id": "1",
          "time": "20210823 08:03"
        },
        {
          "id": "MDRW2100008716.1.18",
          "form": "이론 시험은 필기로 보고 과제 발표가 기말이에요...ㅋㅋ",
          "original_form": "이론 시험은 필기로 보고 과제 발표가 기말이에요...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 08:08"
        },
        {
          "id": "MDRW2100008716.1.19",
          "form": "산업디자인인가요?",
          "original_form": "산업디자인인가요?",
          "speaker_id": "1",
          "time": "20210823 08:12"
        },
        {
          "id": "MDRW2100008716.1.20",
          "form": "산디는 아니고 다른 디자인인데 이 이상 언급하면 학교가 특정되는거 같아서... 여기까지 하겠습니다",
          "original_form": "산디는 아니고 다른 디자인인데 이 이상 언급하면 학교가 특정되는거 같아서... 여기까지 하겠습니다",
          "speaker_id": "2",
          "time": "20210823 08:38"
        },
        {
          "id": "MDRW2100008716.1.21",
          "form": "넵ㅎㅎ 전공은 만족하세요?",
          "original_form": "넵ㅎㅎ 전공은 만족하세요?",
          "speaker_id": "1",
          "time": "20210823 08:44"
        }
