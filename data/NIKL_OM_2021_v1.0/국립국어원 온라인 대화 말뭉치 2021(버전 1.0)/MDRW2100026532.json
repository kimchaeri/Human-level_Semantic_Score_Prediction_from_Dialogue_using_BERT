      "utterance": [
        {
          "id": "MDRW2100026532.1.1",
          "form": "책  자주읽으세요?",
          "original_form": "책  자주읽으세요?",
          "speaker_id": "1",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026532.1.2",
          "form": "만화책을 그나마....",
          "original_form": "만화책을 그나마....",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026532.1.3",
          "form": "만화책도 도서 잖아요ㅋㅋ 저도 어렸을때 책방가서 엄청 읽었던 기억이 나네요",
          "original_form": "만화책도 도서 잖아요ㅋㅋ 저도 어렸을때 책방가서 엄청 읽었던 기억이 나네요",
          "speaker_id": "1",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100026532.1.4",
          "form": "요즘은 책 어쩌다 한권씩 보는거 같아요",
          "original_form": "요즘은 책 어쩌다 한권씩 보는거 같아요",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100026532.1.5",
          "form": "아무래도 영상매체가 더 시간보내기에 수월하잖아요ㅎㅎ 유튜브같은것도 엄청 편하게 멍하니 보면되니까요",
          "original_form": "아무래도 영상매체가 더 시간보내기에 수월하잖아요ㅎㅎ 유튜브같은것도 엄청 편하게 멍하니 보면되니까요",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026532.1.6",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "2",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100026532.1.7",
          "form": "그리고 일하고 오면 만사가 귀차나서ㅜㅜ",
          "original_form": "그리고 일하고 오면 만사가 귀차나서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100026532.1.8",
          "form": "그래도 저는 아이가 있어서 책읽는 엄마모습 보여주려고 노력합니다ㅎㅎ",
          "original_form": "그래도 저는 아이가 있어서 책읽는 엄마모습 보여주려고 노력합니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026532.1.9",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026532.1.10",
          "form": "최근에 읽으신책은?",
          "original_form": "최근에 읽으신책은?",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100026532.1.11",
          "form": "엄마의 말공부 라고 육아서 였어요ㅎ",
          "original_form": "엄마의 말공부 라고 육아서 였어요ㅎ",
          "speaker_id": "1",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026532.1.12",
          "form": "책을 원래 좋아하셨어요?",
          "original_form": "책을 원래 좋아하셨어요?",
          "speaker_id": "2",
          "time": "20210901 23:04"
        },
        {
          "id": "MDRW2100026532.1.13",
          "form": "그냥 시간보내기 할때 학교 도서관에서 소설책 보는걸 좋아하긴했지만 엄청 다독하지는 않았어요",
          "original_form": "그냥 시간보내기 할때 학교 도서관에서 소설책 보는걸 좋아하긴했지만 엄청 다독하지는 않았어요",
          "speaker_id": "1",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100026532.1.14",
          "form": "저도 소설만 좋아했는데 ㅋㅋㅋ",
          "original_form": "저도 소설만 좋아했는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026532.1.15",
          "form": "자기개발서적은 뭔가 쭉쭉 읽히질 않아서 ㅋㅋ",
          "original_form": "자기개발서적은 뭔가 쭉쭉 읽히질 않아서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100026532.1.16",
          "form": "만화책은 안보시죠?",
          "original_form": "만화책은 안보시죠?",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100026532.1.17",
          "form": "왜요!! 원피스 아직봅니다!!!!! ㅋㅋㅋ",
          "original_form": "왜요!! 원피스 아직봅니다!!!!! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100026532.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100026532.1.19",
          "form": "흥분하셨군여",
          "original_form": "흥분하셨군여",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100026532.1.20",
          "form": "원피스 아직도 나오는군요",
          "original_form": "원피스 아직도 나오는군요",
          "speaker_id": "2",
          "time": "20210901 23:15"
        },
        {
          "id": "MDRW2100026532.1.21",
          "form": "엌 만화책은 뭘보셨나요? 저는 나루토 원피스같은 소년만화를 좋아했어서ㅎㅎ",
          "original_form": "엌 만화책은 뭘보셨나요? 저는 나루토 원피스같은 소년만화를 좋아했어서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026532.1.22",
          "form": "전 도라에몽?ㅋㅋㅋ",
          "original_form": "전 도라에몽?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:16"
        },
        {
          "id": "MDRW2100026532.1.23",
          "form": "아ㅎ 짱구같은 스타일인가요?ㅋㅋ",
          "original_form": "아ㅎ 짱구같은 스타일인가요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100026532.1.24",
