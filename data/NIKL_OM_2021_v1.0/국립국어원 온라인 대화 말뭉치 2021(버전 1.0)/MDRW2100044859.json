      "utterance": [
        {
          "id": "MDRW2100044859.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044859.1.2",
          "form": "유튜브 평소에 종종 보시나요??",
          "original_form": "유튜브 평소에 종종 보시나요??",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044859.1.3",
          "form": "네 여러가지 봐요",
          "original_form": "네 여러가지 봐요",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044859.1.4",
          "form": "어떤 장르 주로 보시나요??",
          "original_form": "어떤 장르 주로 보시나요??",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044859.1.5",
          "form": "예능 클립 주로 보는거 같아요",
          "original_form": "예능 클립 주로 보는거 같아요",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044859.1.6",
          "form": "게임이나 먹방 유튜버도 보고... 동물채널도요",
          "original_form": "게임이나 먹방 유튜버도 보고... 동물채널도요",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044859.1.7",
          "form": "오 저도 예능 자주봐요 먹방도!!",
          "original_form": "오 저도 예능 자주봐요 먹방도!!",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044859.1.8",
          "form": "어떤 예능 클립 자주 보시나요??",
          "original_form": "어떤 예능 클립 자주 보시나요??",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044859.1.9",
          "form": "저 런닝맨이요",
          "original_form": "저 런닝맨이요",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044859.1.10",
          "form": "헛 저두 런닝맨 많이 봐요",
          "original_form": "헛 저두 런닝맨 많이 봐요",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044859.1.11",
          "form": "어떤 멤버 응원하세요??",
          "original_form": "어떤 멤버 응원하세요??",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044859.1.12",
          "form": "보통 유재석이요ㅋㅋ",
          "original_form": "보통 유재석이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044859.1.13",
          "form": "앜ㅋㅋ 저두요 유재석 송지효 투탑이에요",
          "original_form": "앜ㅋㅋ 저두요 유재석 송지효 투탑이에요",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044859.1.14",
          "form": "유재석 요즘 너무 웃겨서 더 좋아졌어요 ㅡㅋㅋ",
          "original_form": "유재석 요즘 너무 웃겨서 더 좋아졌어요 ㅡㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044859.1.15",
          "form": "맞아요ㅋㅋ 요새 멤버들 케미 다 좋져",
          "original_form": "맞아요ㅋㅋ 요새 멤버들 케미 다 좋져",
          "speaker_id": "2",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044859.1.16",
          "form": "광수한텐 미안하지만 요즘 너무 재밌어요ㅋㅋㅋ",
          "original_form": "광수한텐 미안하지만 요즘 너무 재밌어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044859.1.17",
          "form": "공포특집도 보셨나요??",
          "original_form": "공포특집도 보셨나요??",
          "speaker_id": "1",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044859.1.18",
          "form": "이번에 한거요? 봤어요",
          "original_form": "이번에 한거요? 봤어요",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044859.1.19",
          "form": "공포해도 멤버들 하는건 웃기더라고요",
          "original_form": "공포해도 멤버들 하는건 웃기더라고요",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044859.1.20",
          "form": "앜ㅋㅋ 맞아요 케미가 너무 좋아서!!!",
          "original_form": "앜ㅋㅋ 맞아요 케미가 너무 좋아서!!!",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044859.1.21",
          "form": "근데 이번껀 좀 무서웠어요ㅠㅠ 인형이라가지구..",
