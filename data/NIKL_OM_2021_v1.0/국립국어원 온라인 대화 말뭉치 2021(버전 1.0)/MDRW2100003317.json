      "utterance": [
        {
          "id": "MDRW2100003317.1.1",
          "form": "아니 좋아 ㅋㅋㅋ",
          "original_form": "아니 좋아 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 21:42"
        },
        {
          "id": "MDRW2100003317.1.2",
          "form": "젤 중요한 거잖아",
          "original_form": "젤 중요한 거잖아",
          "speaker_id": "2",
          "time": "20210820 21:42"
        },
        {
          "id": "MDRW2100003317.1.3",
          "form": "ㅋㅋ 그렇긴 하지",
          "original_form": "ㅋㅋ 그렇긴 하지",
          "speaker_id": "1",
          "time": "20210820 21:42"
        },
        {
          "id": "MDRW2100003317.1.4",
          "form": "삼시3끼 챙겨먹기 너무 힘들어",
          "original_form": "삼시3끼 챙겨먹기 너무 힘들어",
          "speaker_id": "2",
          "time": "20210820 21:43"
        },
        {
          "id": "MDRW2100003317.1.5",
          "form": "그러게 ㅋㅋㅋ 식사 때는 왜이렇게 빨리 돌아오는지",
          "original_form": "그러게 ㅋㅋㅋ 식사 때는 왜이렇게 빨리 돌아오는지",
          "speaker_id": "1",
          "time": "20210820 21:43"
        },
        {
          "id": "MDRW2100003317.1.6",
          "form": "그지 ㅋㅋㅋ",
          "original_form": "그지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 21:43"
        },
        {
          "id": "MDRW2100003317.1.7",
          "form": "아침 잘 챙겨먹어?",
          "original_form": "아침 잘 챙겨먹어?",
          "speaker_id": "1",
          "time": "20210820 21:44"
        },
        {
          "id": "MDRW2100003317.1.8",
          "form": "잘 먹으려고 노력 중이야",
          "original_form": "잘 먹으려고 노력 중이야",
          "speaker_id": "2",
          "time": "20210820 21:44"
        },
        {
          "id": "MDRW2100003317.1.9",
          "form": "건강이 최고지",
          "original_form": "건강이 최고지",
          "speaker_id": "1",
          "time": "20210820 21:44"
        },
        {
          "id": "MDRW2100003317.1.10",
          "form": "잘 먹고 건강해야지 ㅋㅋㅋ",
          "original_form": "잘 먹고 건강해야지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 21:44"
        },
        {
          "id": "MDRW2100003317.1.11",
          "form": "아침은 한식파야 양식파야?",
          "original_form": "아침은 한식파야 양식파야?",
          "speaker_id": "1",
          "time": "20210820 21:44"
        },
        {
          "id": "MDRW2100003317.1.12",
          "form": "안가리고 다 잘먹어",
          "original_form": "안가리고 다 잘먹어",
          "speaker_id": "2",
          "time": "20210820 21:44"
        },
        {
          "id": "MDRW2100003317.1.13",
          "form": "나는 한식",
          "original_form": "나는 한식",
          "speaker_id": "1",
          "time": "20210820 21:45"
        },
        {
          "id": "MDRW2100003317.1.14",
          "form": "나는 아침에 고기도 잘 먹어 ㅋㅋㅋ",
          "original_form": "나는 아침에 고기도 잘 먹어 ㅋㅋㅋ",
