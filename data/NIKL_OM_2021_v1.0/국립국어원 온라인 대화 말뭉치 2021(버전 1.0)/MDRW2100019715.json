      "utterance": [
        {
          "id": "MDRW2100019715.1.1",
          "form": "넷플릭스 자주 보세요?",
          "original_form": "넷플릭스 자주 보세요?",
          "speaker_id": "2",
          "time": "20210827 22:23"
        },
        {
          "id": "MDRW2100019715.1.2",
          "form": "가끔 결제하고 몰아서 봐요ㅋㅋㅋ",
          "original_form": "가끔 결제하고 몰아서 봐요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100019715.1.3",
          "form": "매달 결제하면 잘 안보게 되더라구요",
          "original_form": "매달 결제하면 잘 안보게 되더라구요",
          "speaker_id": "1",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100019715.1.4",
          "form": "넷플 다큐멘터리가 재밌다면서요?",
          "original_form": "넷플 다큐멘터리가 재밌다면서요?",
          "speaker_id": "1",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100019715.1.5",
          "form": "맞아요!!!!!!! 그리고 넷플 오리지널도 볼게 많더라고요",
          "original_form": "맞아요!!!!!!! 그리고 넷플 오리지널도 볼게 많더라고요",
          "speaker_id": "2",
          "time": "20210827 23:00"
        },
        {
          "id": "MDRW2100019715.1.6",
          "form": "킹덤 같은!",
          "original_form": "킹덤 같은!",
          "speaker_id": "2",
          "time": "20210827 23:00"
        },
        {
          "id": "MDRW2100019715.1.7",
          "form": "앗 저 제일 마지막에본 넷플 컨텐츠가 킹덤이랑 스위트홈이었어요ㅋㅋㅋ",
          "original_form": "앗 저 제일 마지막에본 넷플 컨텐츠가 킹덤이랑 스위트홈이었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100019715.1.8",
          "form": "스위트홈보단 킹덤시 더 재밌더라구요",
          "original_form": "스위트홈보단 킹덤시 더 재밌더라구요",
          "speaker_id": "1",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100019715.1.9",
          "form": "헐 둘 다 재밌게 봤는데",
          "original_form": "헐 둘 다 재밌게 봤는데",
          "speaker_id": "2",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019715.1.10",
          "form": "맞아요 킹덤 너무 잘 만든 것 같아요",
          "original_form": "맞아요 킹덤 너무 잘 만든 것 같아요",
          "speaker_id": "2",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019715.1.11",
          "form": "배우들 라인업도 엄청 화려하고!",
          "original_form": "배우들 라인업도 엄청 화려하고!",
          "speaker_id": "2",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019715.1.12",
          "form": "저도 둘다 잼썼어요ㅋㅋㅋㅋ",
          "original_form": "저도 둘다 잼썼어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019715.1.13",
          "form": "킹덤 도포자락이 참 멋있더라구요",
          "original_form": "킹덤 도포자락이 참 멋있더라구요",
          "speaker_id": "1",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019715.1.14",
          "form": "마지막에 전지현 보고 끝났는데 3부를 못보고 있네요 ㅠㅠ",
          "original_form": "마지막에 전지현 보고 끝났는데 3부를 못보고 있네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019715.1.15",
          "form": "저도요 ㅠ 아신전은 외전 느낌이래서 손에 잘 안 잡히져라고요",
          "original_form": "저도요 ㅠ 아신전은 외전 느낌이래서 손에 잘 안 잡히져라고요",
          "speaker_id": "2",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019715.1.16",
          "form": "헉 그런가요?",
          "original_form": "헉 그런가요?",
          "speaker_id": "1",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100019715.1.17",
          "form": "이제 궁 들어간 이후 얘기 보고 싶었는데 ㅠㅠ",
          "original_form": "이제 궁 들어간 이후 얘기 보고 싶었는데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100019715.1.18",
          "form": "네네.... 그 3편은 내년? 넘어서 나오는 걸로 알아요 ㅎㅎ",
          "original_form": "네네.... 그 3편은 내년? 넘어서 나오는 걸로 알아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100019715.1.19",
          "form": "아니 저런....ㅎㅎㅠ 내년에 넷플결제해야겠네요...",
          "original_form": "아니 저런....ㅎㅎㅠ 내년에 넷플결제해야겠네요...",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100019715.1.20",
          "form": "다른 작품은 추천해주실 거 없나요?",
          "original_form": "다른 작품은 추천해주실 거 없나요?",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100019715.1.21",
          "form": "외국 거 중에는 바자르의 불꽃 볼만하고요! 한국 오리지널 중에는 새콤달콤도 재밌어요!",
          "original_form": "외국 거 중에는 바자르의 불꽃 볼만하고요! 한국 오리지널 중에는 새콤달콤도 재밌어요!",
          "speaker_id": "2",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100019715.1.22",
          "form": "태국 거는 그녀의 이름은 난노! 요새ㅜ완전 핫하고요!!",
          "original_form": "태국 거는 그녀의 이름은 난노! 요새ㅜ완전 핫하고요!!",
          "speaker_id": "2",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100019715.1.23",
          "form": "엇 뭔가 바자르의 불꽃 제목이 확 끌리네요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "엇 뭔가 바자르의 불꽃 제목이 확 끌리네요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100019715.1.24",
          "form": "장르가 뭐예요??",
          "original_form": "장르가 뭐예요??",
          "speaker_id": "1",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100019715.1.25",
          "form": "프랑스 드라마인데 약간 격정 로맨스도 있고 반전도 있는? 드라마에요!",
          "original_form": "프랑스 드라마인데 약간 격정 로맨스도 있고 반전도 있는? 드라마에요!",
          "speaker_id": "2",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100019715.1.26",
          "form": "어쩐지 제목부터가 격정 로맨스의 느낌이 났어요",
          "original_form": "어쩐지 제목부터가 격정 로맨스의 느낌이 났어요",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100019715.1.27",
          "form": "한번 봐봐야겠네요 그것도ㅋㅋㅋ",
          "original_form": "한번 봐봐야겠네요 그것도ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100019715.1.28",
          "form": "네!!! 진짜 재밌어요 저 하루만에 다 보고",
          "original_form": "네!!! 진짜 재밌어요 저 하루만에 다 보고",
          "speaker_id": "2",
          "time": "20210828 01:14"
        },
        {
          "id": "MDRW2100019715.1.29",
          "form": "그날 새벽 날럈어요 ㅎㅎ",
          "original_form": "그날 새벽 날럈어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 01:14"
        }
      ]
