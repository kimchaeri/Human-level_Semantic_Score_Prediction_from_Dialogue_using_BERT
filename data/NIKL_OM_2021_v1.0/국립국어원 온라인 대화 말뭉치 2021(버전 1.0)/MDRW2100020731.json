      "utterance": [
        {
          "id": "MDRW2100020731.1.1",
          "form": "곱창이즈뭔들..",
          "original_form": "곱창이즈뭔들..",
          "speaker_id": "2",
          "time": "20210828 11:48"
        },
        {
          "id": "MDRW2100020731.1.2",
          "form": "아 대창 맛있는데 그 부추넣어주는곳있거든요",
          "original_form": "아 대창 맛있는데 그 부추넣어주는곳있거든요",
          "speaker_id": "1",
          "time": "20210828 11:48"
        },
        {
          "id": "MDRW2100020731.1.3",
          "form": "혹시 강경소파신가요",
          "original_form": "혹시 강경소파신가요",
          "speaker_id": "2",
          "time": "20210828 11:49"
        },
        {
          "id": "MDRW2100020731.1.4",
          "form": "거기 진짜 맛있는데",
          "original_form": "거기 진짜 맛있는데",
          "speaker_id": "1",
          "time": "20210828 11:49"
        },
        {
          "id": "MDRW2100020731.1.5",
          "form": "아 말만들어도 맛있겠네요",
          "original_form": "아 말만들어도 맛있겠네요",
          "speaker_id": "2",
          "time": "20210828 11:49"
        },
        {
          "id": "MDRW2100020731.1.6",
          "form": "볶음은 돼지곱창 좋아합니다",
          "original_form": "볶음은 돼지곱창 좋아합니다",
          "speaker_id": "1",
          "time": "20210828 11:49"
        },
        {
          "id": "MDRW2100020731.1.7",
          "form": "파넣어주는 곳도 있더라구요",
          "original_form": "파넣어주는 곳도 있더라구요",
          "speaker_id": "2",
          "time": "20210828 11:49"
        },
        {
          "id": "MDRW2100020731.1.8",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210828 11:49"
        },
        {
          "id": "MDRW2100020731.1.9",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "1",
          "time": "20210828 11:49"
        },
        {
          "id": "MDRW2100020731.1.10",
          "form": "너무맛있겠다",
          "original_form": "너무맛있겠다",
          "speaker_id": "1",
          "time": "20210828 11:49"
        },
        {
          "id": "MDRW2100020731.1.11",
          "form": "기름  쭉 나오면서 익으먼",
          "original_form": "기름  쭉 나오면서 익으먼",
          "speaker_id": "1",
          "time": "20210828 11:49"
        },
        {
          "id": "MDRW2100020731.1.12",
          "form": "다들 아이디어가 대단하세요",
          "original_form": "다들 아이디어가 대단하세요",
          "speaker_id": "2",
          "time": "20210828 11:49"
        },
        {
          "id": "MDRW2100020731.1.13",
          "form": "진짜 맛있잖아요",
          "original_form": "진짜 맛있잖아요",
          "speaker_id": "1",
          "time": "20210828 11:49"
        },
        {
          "id": "MDRW2100020731.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 11:49"
        },
        {
          "id": "MDRW2100020731.1.15",
          "form": "기름도 고소하고",
          "original_form": "기름도 고소하고",
          "speaker_id": "2",
          "time": "20210828 11:50"
        },
        {
          "id": "MDRW2100020731.1.16",
          "form": "부추대창은 만두맛 나거든요",
          "original_form": "부추대창은 만두맛 나거든요",
          "speaker_id": "1",
          "time": "20210828 11:50"
        },
        {
          "id": "MDRW2100020731.1.17",
          "form": "헐 그렇겠네요",
          "original_form": "헐 그렇겠네요",
          "speaker_id": "2",
          "time": "20210828 11:50"
        },
        {
          "id": "MDRW2100020731.1.18",
          "form": "대창이 만두피 역할을 하는건가",
          "original_form": "대창이 만두피 역할을 하는건가",
          "speaker_id": "2",
          "time": "20210828 11:50"
        },
        {
          "id": "MDRW2100020731.1.19",
          "form": "파대창도 비슷하려나",
          "original_form": "파대창도 비슷하려나",
          "speaker_id": "1",
          "time": "20210828 11:50"
        },
        {
          "id": "MDRW2100020731.1.20",
          "form": "네 좀 그런셈",
          "original_form": "네 좀 그런셈",
          "speaker_id": "1",
          "time": "20210828 11:50"
        }
