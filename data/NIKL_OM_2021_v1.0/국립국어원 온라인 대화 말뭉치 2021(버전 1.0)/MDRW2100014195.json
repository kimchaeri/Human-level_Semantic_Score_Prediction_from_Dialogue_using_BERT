      "utterance": [
        {
          "id": "MDRW2100014195.1.1",
          "form": "무슨 웹툰 보시나요",
          "original_form": "무슨 웹툰 보시나요",
          "speaker_id": "1",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100014195.1.2",
          "form": "추천해주실거 있으신가요",
          "original_form": "추천해주실거 있으신가요",
          "speaker_id": "1",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100014195.1.3",
          "form": "무슨장르 좋아하세요??",
          "original_form": "무슨장르 좋아하세요??",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014195.1.4",
          "form": "저는 요즘 화산귀환 잼나게 읽고있어요!",
          "original_form": "저는 요즘 화산귀환 잼나게 읽고있어요!",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014195.1.5",
          "form": "화산귀환은 어느 플렛폼에 있어요??",
          "original_form": "화산귀환은 어느 플렛폼에 있어요??",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014195.1.6",
          "form": "저는 판타지같은건 잘 안보고",
          "original_form": "저는 판타지같은건 잘 안보고",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014195.1.7",
          "form": "약간 맨날 싸우는 내용도 잘 안봐요 ㅋㅋ",
          "original_form": "약간 맨날 싸우는 내용도 잘 안봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014195.1.8",
          "form": "앗 그럼 취향이 아니실것같은뎈ㅋㅋㅋ 우선 시리즈에 있어요!!",
          "original_form": "앗 그럼 취향이 아니실것같은뎈ㅋㅋㅋ 우선 시리즈에 있어요!!",
          "speaker_id": "2",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014195.1.9",
          "form": "그러면 약간 일상툰이나 몽글몽글한 스토리 좋아하시나요",
          "original_form": "그러면 약간 일상툰이나 몽글몽글한 스토리 좋아하시나요",
          "speaker_id": "2",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014195.1.10",
          "form": "모퉁이 뜨개방이 좀 몽글몽글해요",
          "original_form": "모퉁이 뜨개방이 좀 몽글몽글해요",
          "speaker_id": "2",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014195.1.11",
          "form": "그것도 시리즈에있나요??",
          "original_form": "그것도 시리즈에있나요??",
          "speaker_id": "1",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014195.1.12",
          "form": "처음들어봐요 ㅋㅋ",
          "original_form": "처음들어봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014195.1.13",
          "form": "아뇨 그건 네이버 웹툰!!!",
          "original_form": "아뇨 그건 네이버 웹툰!!!",
          "speaker_id": "2",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014195.1.14",
          "form": "젤 좋아했던 웹툰 있으세요?!",
          "original_form": "젤 좋아했던 웹툰 있으세요?!",
          "speaker_id": "2",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100014195.1.15",
          "form": "요즘에 재밌게 본 웹툰은",
          "original_form": "요즘에 재밌게 본 웹툰은",
          "speaker_id": "1",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100014195.1.16",
          "form": "별이삼샵이요!!ㅋㅋㅋㅋ",
          "original_form": "별이삼샵이요!!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100014195.1.17",
          "form": "어그 죽음에 관하여 하신 분의 웹툰?!!",
          "original_form": "어그 죽음에 관하여 하신 분의 웹툰?!!",
          "speaker_id": "2",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014195.1.18",
          "form": "오오 맞아요",
          "original_form": "오오 맞아요",
          "speaker_id": "1",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014195.1.19",
          "form": "그뷴들꺼 재밌어요 ㅎㅎ",
          "original_form": "그뷴들꺼 재밌어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014195.1.20",
          "form": "헉 명작만 보신분이구만요",
          "original_form": "헉 명작만 보신분이구만요",
          "speaker_id": "2",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014195.1.21",
          "form": "명작만 골라봅니다",
          "original_form": "명작만 골라봅니다",
          "speaker_id": "1",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100014195.1.22",
          "form": "요즘엔 썸내일이라는 웹툰도 재밌도라고요!!",
          "original_form": "요즘엔 썸내일이라는 웹툰도 재밌도라고요!!",
          "speaker_id": "1",
          "time": "20210825 17:57"
        },
        {
          "id": "MDRW2100014195.1.23",
          "form": "헉 그건 무슨내용인가요?!",
          "original_form": "헉 그건 무슨내용인가요?!",
          "speaker_id": "2",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100014195.1.24",
          "form": "하트시그널 프로그램아세요??",
          "original_form": "하트시그널 프로그램아세요??",
          "speaker_id": "1",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014195.1.25",
          "form": "약간 그런 프로그램에 출연한 남자 주인공이 갑자기 죽게되는데",
          "original_form": "약간 그런 프로그램에 출연한 남자 주인공이 갑자기 죽게되는데",
          "speaker_id": "1",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014195.1.26",
          "form": "출연한 여자주인공때문이라는 소문도 돌고 아무튼  재밌는내용이요..",
          "original_form": "출연한 여자주인공때문이라는 소문도 돌고 아무튼  재밌는내용이요..",
          "speaker_id": "1",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014195.1.27",
          "form": "제가 영업을 잘 못해요 ㅋㅋ",
          "original_form": "제가 영업을 잘 못해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014195.1.28",
          "form": "헐 아닌데요 영업 잘하시는데여?",
          "original_form": "헐 아닌데요 영업 잘하시는데여?",
          "speaker_id": "2",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100014195.1.29",
          "form": "급 궁금래졌다",
          "original_form": "급 궁금래졌다",
          "speaker_id": "2",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100014195.1.30",
          "form": "보고올게요",
          "original_form": "보고올게요",
          "speaker_id": "2",
          "time": "20210825 18:01"
        }
      ]
