        },
        {
          "id": "MDRW2100038730.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100038730.1.3",
          "form": "저녁 뭐드셨나요",
          "original_form": "저녁 뭐드셨나요",
          "speaker_id": "1",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100038730.1.4",
          "form": "생선조림이요~",
          "original_form": "생선조림이요~",
          "speaker_id": "2",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100038730.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100038730.1.6",
          "form": "떡볶이 사먹었어요ㅎㅎ",
          "original_form": "떡볶이 사먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100038730.1.7",
          "form": "맛저하셨네요~",
          "original_form": "맛저하셨네요~",
          "speaker_id": "2",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100038730.1.8",
          "form": "무슨음식 제일 좋아해요?",
          "original_form": "무슨음식 제일 좋아해요?",
          "speaker_id": "2",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100038730.1.9",
          "form": "라면 좋아해요ㅎㅎ 국민음식",
          "original_form": "라면 좋아해요ㅎㅎ 국민음식",
          "speaker_id": "1",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100038730.1.10",
          "form": "ㅋㅋ다른건요",
          "original_form": "ㅋㅋ다른건요",
          "speaker_id": "2",
          "time": "20210902 21:22"
        },
        {
          "id": "MDRW2100038730.1.11",
          "form": "마카롱이랑 센베이 과자 좋아해요",
          "original_form": "마카롱이랑 센베이 과자 좋아해요",
          "speaker_id": "1",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100038730.1.12",
          "form": "센베이 과자는 뭔가요?",
          "original_form": "센베이 과자는 뭔가요?",
          "speaker_id": "2",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100038730.1.13",
          "form": "얇은 과자 있어요 질소포장된거 말구",
          "original_form": "얇은 과자 있어요 질소포장된거 말구",
          "speaker_id": "1",
          "time": "20210902 22:08"
        },
        {
          "id": "MDRW2100038730.1.14",
          "form": "김이나 땅콩 들어간거요",
          "original_form": "김이나 땅콩 들어간거요",
          "speaker_id": "1",
          "time": "20210902 22:08"
        },
        {
          "id": "MDRW2100038730.1.15",
          "form": "아 먹어본거같아요",
          "original_form": "아 먹어본거같아요",
          "speaker_id": "2",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100038730.1.16",
          "form": "디저트류는 다 맛있죠",
          "original_form": "디저트류는 다 맛있죠",
          "speaker_id": "2",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100038730.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100038730.1.18",
          "form": "달고 고소한건 언제나 진리",
