      "utterance": [
        {
          "id": "MDRW2100034651.1.1",
          "form": "대탈출 보시나요?",
          "original_form": "대탈출 보시나요?",
          "speaker_id": "1",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100034651.1.2",
          "form": "아뇨 ㅜㅜ 재밌나요?",
          "original_form": "아뇨 ㅜㅜ 재밌나요?",
          "speaker_id": "2",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100034651.1.3",
          "form": "한번봤는데 재밌더라구요",
          "original_form": "한번봤는데 재밌더라구요",
          "speaker_id": "1",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100034651.1.4",
          "form": "체험해보고싶어져요",
          "original_form": "체험해보고싶어져요",
          "speaker_id": "1",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100034651.1.5",
          "form": "아 마쟈여 방탈출이랑 비슷한거져!",
          "original_form": "아 마쟈여 방탈출이랑 비슷한거져!",
          "speaker_id": "2",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100034651.1.6",
          "form": "넹넹!",
          "original_form": "넹넹!",
          "speaker_id": "1",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100034651.1.7",
          "form": "방탈출 가봤는데 그런느낌 아니더라구요",
          "original_form": "방탈출 가봤는데 그런느낌 아니더라구요",
          "speaker_id": "1",
          "time": "20210828 17:06"
        },
        {
          "id": "MDRW2100034651.1.8",
          "form": "아진짜요? 전 아직 무경험이라 되게 궁금하고 그랫는데 기대하면안되나봐여ㅋㅋ",
          "original_form": "아진짜요? 전 아직 무경험이라 되게 궁금하고 그랫는데 기대하면안되나봐여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 06:51"
        },
        {
          "id": "MDRW2100034651.1.9",
          "form": "재밌긴한데 티비는 역시 따라갈 수 없더라구요ㅋㅋ",
          "original_form": "재밌긴한데 티비는 역시 따라갈 수 없더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 08:13"
        },
        {
          "id": "MDRW2100034651.1.10",
          "form": "앜ㅋㅋㅋㅋㅋㅋ 반대일줄알았는데 오히려 티비가 재밌다닠ㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ 반대일줄알았는데 오히려 티비가 재밌다닠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 10:29"
        },
        {
          "id": "MDRW2100034651.1.11",
          "form": "저 예전에 그거 보고 방탈출 처음갔을때 티비 생각하고 막 불키면 안돼! 이래서 탈출 실패했어요ㄲㅋㅋㅋ",
          "original_form": "저 예전에 그거 보고 방탈출 처음갔을때 티비 생각하고 막 불키면 안돼! 이래서 탈출 실패했어요ㄲㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:53"
        },
        {
          "id": "MDRW2100034651.1.12",
          "form": "앜ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:23"
        },
        {
          "id": "MDRW2100034651.1.13",
          "form": "방탈출안해봤지만 제 버킷리스트중하나에요",
          "original_form": "방탈출안해봤지만 제 버킷리스트중하나에요",
          "speaker_id": "2",
          "time": "20210830 13:23"
        },
        {
          "id": "MDRW2100034651.1.14",
          "form": "오오ㅋㅋ 얼른 코로나 끝나야 한번 가보실텐데!",
          "original_form": "오오ㅋㅋ 얼른 코로나 끝나야 한번 가보실텐데!",
          "speaker_id": "1",
          "time": "20210830 14:56"
        },
        {
          "id": "MDRW2100034651.1.15",
          "form": "탈출 성공을 기원합니닷ㅋㅋ",
          "original_form": "탈출 성공을 기원합니닷ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100034651.1.16",
          "form": "넹ㅋㅋㅋㅋㅋㅋㅋ 꼭이요",
          "original_form": "넹ㅋㅋㅋㅋㅋㅋㅋ 꼭이요",
          "speaker_id": "2",
          "time": "20210831 13:52"
        },
        {
          "id": "MDRW2100034651.1.17",
          "form": "제가 다 기대되네요ㅋㅋㅋㅋ",
          "original_form": "제가 다 기대되네요ㅋㅋㅋㅋ",
