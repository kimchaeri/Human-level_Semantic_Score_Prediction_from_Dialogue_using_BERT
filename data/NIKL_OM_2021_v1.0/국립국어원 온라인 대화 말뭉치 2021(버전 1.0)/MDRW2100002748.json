      "utterance": [
        {
          "id": "MDRW2100002748.1.1",
          "form": "방청소 같은 건 자주 해?",
          "original_form": "방청소 같은 건 자주 해?",
          "speaker_id": "1",
          "time": "20210820 15:17"
        },
        {
          "id": "MDRW2100002748.1.2",
          "form": "아니 잘 안해",
          "original_form": "아니 잘 안해",
          "speaker_id": "2",
          "time": "20210820 15:18"
        },
        {
          "id": "MDRW2100002748.1.3",
          "form": "맨날 미루지",
          "original_form": "맨날 미루지",
          "speaker_id": "2",
          "time": "20210820 15:18"
        },
        {
          "id": "MDRW2100002748.1.4",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210820 15:18"
        },
        {
          "id": "MDRW2100002748.1.5",
          "form": "나도 맨날 귀찮아서 안하다가",
          "original_form": "나도 맨날 귀찮아서 안하다가",
          "speaker_id": "1",
          "time": "20210820 15:19"
        },
        {
          "id": "MDRW2100002748.1.6",
          "form": "맨날 엄마한테 잔소리 들어",
          "original_form": "맨날 엄마한테 잔소리 들어",
          "speaker_id": "1",
          "time": "20210820 15:19"
        },
        {
          "id": "MDRW2100002748.1.7",
          "form": "아ㅋㅋ 너도",
          "original_form": "아ㅋㅋ 너도",
          "speaker_id": "2",
          "time": "20210820 15:20"
        },
        {
          "id": "MDRW2100002748.1.8",
          "form": "아니 나도",
          "original_form": "아니 나도",
          "speaker_id": "2",
          "time": "20210820 15:20"
        },
        {
          "id": "MDRW2100002748.1.9",
          "form": "짐 쌓아둔거 치워야 하는데",
          "original_form": "짐 쌓아둔거 치워야 하는데",
          "speaker_id": "2",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002748.1.10",
          "form": "너무 귀찮아",
          "original_form": "너무 귀찮아",
          "speaker_id": "2",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002748.1.11",
          "form": "맞아 나도 막 먼지 청소하고 그래야되는데",
          "original_form": "맞아 나도 막 먼지 청소하고 그래야되는데",
          "speaker_id": "1",
          "time": "20210820 15:22"
        },
        {
          "id": "MDRW2100002748.1.12",
          "form": "하기 진짜 싫더라",
          "original_form": "하기 진짜 싫더라",
          "speaker_id": "1",
          "time": "20210820 15:22"
        },
        {
          "id": "MDRW2100002748.1.13",
          "form": "청소는 왜이렇게 하기 싫을까",
          "original_form": "청소는 왜이렇게 하기 싫을까",
          "speaker_id": "2",
          "time": "20210820 15:23"
        },
        {
          "id": "MDRW2100002748.1.14",
          "form": "어지르는건 이렇게 쉬운데",
          "original_form": "어지르는건 이렇게 쉬운데",
          "speaker_id": "2",
          "time": "20210820 15:24"
        },
        {
          "id": "MDRW2100002748.1.15",
          "form": "어지르는건 세계 1위 할수도 있을것 같아",
          "original_form": "어지르는건 세계 1위 할수도 있을것 같아",
          "speaker_id": "1",
          "time": "20210820 15:24"
        },
        {
          "id": "MDRW2100002748.1.16",
          "form": "난 집에 컴퓨터 관련 부품이 많아서 더 정신없는것 같아",
          "original_form": "난 집에 컴퓨터 관련 부품이 많아서 더 정신없는것 같아",
          "speaker_id": "1",
          "time": "20210820 15:24"
        },
        {
          "id": "MDRW2100002748.1.17",
          "form": "아 그렇겠네",
          "original_form": "아 그렇겠네",
          "speaker_id": "2",
          "time": "20210820 15:25"
        },
        {
          "id": "MDRW2100002748.1.18",
          "form": "난 굿즈가 좀 많아..",
          "original_form": "난 굿즈가 좀 많아..",
          "speaker_id": "2",
          "time": "20210820 15:26"
        },
        {
          "id": "MDRW2100002748.1.19",
          "form": "굿즈는 보통 책장? 같은데 해놓고 보관하지 않아?",
          "original_form": "굿즈는 보통 책장? 같은데 해놓고 보관하지 않아?",
          "speaker_id": "1",
          "time": "20210820 15:28"
        },
        {
          "id": "MDRW2100002748.1.20",
          "form": "그래야 하는데",
          "original_form": "그래야 하는데",
          "speaker_id": "2",
          "time": "20210820 15:30"
        },
        {
          "id": "MDRW2100002748.1.21",
          "form": "책상에 쌓여있어",
          "original_form": "책상에 쌓여있어",
          "speaker_id": "2",
          "time": "20210820 15:30"
        },
        {
          "id": "MDRW2100002748.1.22",
          "form": "쌓아두면 그거 먼지 엄청 쌓인다?",
          "original_form": "쌓아두면 그거 먼지 엄청 쌓인다?",
          "speaker_id": "1",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002748.1.23",
          "form": "안돼..",
          "original_form": "안돼..",
          "speaker_id": "2",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002748.1.24",
          "form": "투명 문있는 가구 종류로 하나 사서 정리 해두는거 나쁘지 않을것 같아",
          "original_form": "투명 문있는 가구 종류로 하나 사서 정리 해두는거 나쁘지 않을것 같아",
          "speaker_id": "1",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002748.1.25",
          "form": "빨리 정리 해야지ㅠㅠ",
          "original_form": "빨리 정리 해야지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002748.1.26",
          "form": "그런거 사진으로 봤는데 좋더라",
          "original_form": "그런거 사진으로 봤는데 좋더라",
          "speaker_id": "2",
          "time": "20210820 15:39"
        }
      ]
    }
  ]
