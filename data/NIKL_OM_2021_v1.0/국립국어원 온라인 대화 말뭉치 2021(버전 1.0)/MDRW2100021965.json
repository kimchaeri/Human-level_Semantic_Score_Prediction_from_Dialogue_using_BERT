        },
        {
          "id": "MDRW2100021965.1.2",
          "form": "제일 먼저 기억나는 건?",
          "original_form": "제일 먼저 기억나는 건?",
          "speaker_id": "1",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100021965.1.3",
          "form": "힙합이랑 팝송이랑 발라드도 들어",
          "original_form": "힙합이랑 팝송이랑 발라드도 들어",
          "speaker_id": "2",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100021965.1.4",
          "form": "요즘 제일 많이 듣는 곡이 뭔데?",
          "original_form": "요즘 제일 많이 듣는 곡이 뭔데?",
          "speaker_id": "1",
          "time": "20210829 00:15"
        },
        {
          "id": "MDRW2100021965.1.5",
          "form": "임한별 오늘의 그대에게",
          "original_form": "임한별 오늘의 그대에게",
          "speaker_id": "2",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021965.1.6",
          "form": "이따가 들어봐야겠다",
          "original_form": "이따가 들어봐야겠다",
          "speaker_id": "1",
          "time": "20210829 00:16"
        },
        {
          "id": "MDRW2100021965.1.7",
          "form": "노래 좋아. 요즘 제일 많이 듣는 노래 있어?",
          "original_form": "노래 좋아. 요즘 제일 많이 듣는 노래 있어?",
          "speaker_id": "2",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021965.1.8",
          "form": "사랑과 우정 사이",
          "original_form": "사랑과 우정 사이",
          "speaker_id": "1",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021965.1.9",
          "form": "아 노래 좋더라",
          "original_form": "아 노래 좋더라",
          "speaker_id": "2",
          "time": "20210829 00:17"
        },
        {
          "id": "MDRW2100021965.1.10",
          "form": "ㅇㅇkcm이 부른 게 제일 좋아",
          "original_form": "ㅇㅇkcm이 부른 게 제일 좋아",
          "speaker_id": "1",
          "time": "20210829 00:18"
        },
        {
          "id": "MDRW2100021965.1.11",
          "form": "그거 sg워너비가 원곡 아닌가?",
          "original_form": "그거 sg워너비가 원곡 아닌가?",
          "speaker_id": "2",
          "time": "20210829 00:18"
        },
        {
          "id": "MDRW2100021965.1.12",
          "form": "아니야 더 옛날 노래야",
          "original_form": "아니야 더 옛날 노래야",
          "speaker_id": "1",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100021965.1.13",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "2",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100021965.1.14",
          "form": "응 다 리메이크한 거야",
          "original_form": "응 다 리메이크한 거야",
          "speaker_id": "1",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100021965.1.15",
          "form": "원곡자가 누군데?",
          "original_form": "원곡자가 누군데?",
