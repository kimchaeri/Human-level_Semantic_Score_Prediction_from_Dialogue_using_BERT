      "utterance": [
        {
          "id": "MDRW2100034566.1.1",
          "form": "ㅋㅋㅋ공부 잘하셨나여",
          "original_form": "ㅋㅋㅋ공부 잘하셨나여",
          "speaker_id": "2",
          "time": "20210828 15:19"
        },
        {
          "id": "MDRW2100034566.1.2",
          "form": "ㅋㅋ 못하진 않았어요",
          "original_form": "ㅋㅋ 못하진 않았어요",
          "speaker_id": "1",
          "time": "20210828 15:20"
        },
        {
          "id": "MDRW2100034566.1.3",
          "form": "근데 대학 가니까 공부가 잘 안 되더라구요ㅋㅋㅠ",
          "original_form": "근데 대학 가니까 공부가 잘 안 되더라구요ㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210828 15:20"
        },
        {
          "id": "MDRW2100034566.1.4",
          "form": "그러게여 뭔가 각잡고 시키는게 아니니까 ㅋㅋㅋ",
          "original_form": "그러게여 뭔가 각잡고 시키는게 아니니까 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:20"
        },
        {
          "id": "MDRW2100034566.1.5",
          "form": "주입식 교육형 인간이었나봐요ㅋㅋ",
          "original_form": "주입식 교육형 인간이었나봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:23"
        },
        {
          "id": "MDRW2100034566.1.6",
          "form": "아니 근데 새벽에 계속 깨어계시지 않았어요? 공부할 시간 있었어요?",
          "original_form": "아니 근데 새벽에 계속 깨어계시지 않았어요? 공부할 시간 있었어요?",
          "speaker_id": "2",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100034566.1.7",
          "form": "졸업한지 꽤 되서 요새는 공부할 일 잘 없죠ㅋㅋ",
          "original_form": "졸업한지 꽤 되서 요새는 공부할 일 잘 없죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:26"
        },
        {
          "id": "MDRW2100034566.1.8",
          "form": "아ㅋㅋㅋ학생때는 규칙적으로 사셨군요 ㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋ학생때는 규칙적으로 사셨군요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:28"
        },
        {
          "id": "MDRW2100034566.1.9",
          "form": "시험 안 보는 거 하난 참 좋은 것 같아요",
          "original_form": "시험 안 보는 거 하난 참 좋은 것 같아요",
          "speaker_id": "1",
          "time": "20210828 15:30"
        },
        {
          "id": "MDRW2100034566.1.10",
          "form": "맞아요 시험 없이 그냥 하고싶은 공부 하는 거 좋아요 ㅋㅋ",
          "original_form": "맞아요 시험 없이 그냥 하고싶은 공부 하는 거 좋아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:47"
        },
        {
          "id": "MDRW2100034566.1.11",
          "form": "대학 때 조별과제 자주 하셨어요?",
          "original_form": "대학 때 조별과제 자주 하셨어요?",
          "speaker_id": "1",
          "time": "20210828 17:30"
        },
        {
          "id": "MDRW2100034566.1.12",
          "form": "네 대학때는 조별과제가 거의 다이지 않나여? ㅠㅠㅎㅎ",
          "original_form": "네 대학때는 조별과제가 거의 다이지 않나여? ㅠㅠㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 17:45"
        },
        {
          "id": "MDRW2100034566.1.13",
          "form": "ㅋㅋ 맞아요. 지긋지긋 했죠ㅋㅋ 이상한 조원 걸리면 스트레스",
          "original_form": "ㅋㅋ 맞아요. 지긋지긋 했죠ㅋㅋ 이상한 조원 걸리면 스트레스",
          "speaker_id": "1",
          "time": "20210828 17:46"
        },
        {
          "id": "MDRW2100034566.1.14",
          "form": "진짜 제가 매번 조장했덩 것 같아요 근데 ㅋㅋㅋㅋ",
          "original_form": "진짜 제가 매번 조장했덩 것 같아요 근데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:47"
        },
        {
          "id": "MDRW2100034566.1.15",
          "form": "당했어요..",
          "original_form": "당했어요..",
          "speaker_id": "2",
          "time": "20210828 17:47"
        },
        {
          "id": "MDRW2100034566.1.16",
          "form": "ㅋㅋㅋ 리더십 있는 타입이시군요",
          "original_form": "ㅋㅋㅋ 리더십 있는 타입이시군요",
          "speaker_id": "1",
          "time": "20210828 17:48"
        },
        {
          "id": "MDRW2100034566.1.17",
          "form": "어디가서 사기당하기 좋은 타입인거죠.....후",
          "original_form": "어디가서 사기당하기 좋은 타입인거죠.....후",
          "speaker_id": "2",
          "time": "20210828 17:48"
        },
        {
