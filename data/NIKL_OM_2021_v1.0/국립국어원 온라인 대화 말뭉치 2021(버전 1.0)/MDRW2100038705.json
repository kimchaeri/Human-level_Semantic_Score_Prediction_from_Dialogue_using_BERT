      "utterance": [
        {
          "id": "MDRW2100038705.1.1",
          "form": "저번에 출장쇼 나피디프로 있었잖아",
          "original_form": "저번에 출장쇼 나피디프로 있었잖아",
          "speaker_id": "2",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100038705.1.2",
          "form": "어 알지ㅋ 재밌었자나",
          "original_form": "어 알지ㅋ 재밌었자나",
          "speaker_id": "1",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100038705.1.3",
          "form": "그때 한효주가 자기 드라마 이야기한거 방영할 예정인가봐",
          "original_form": "그때 한효주가 자기 드라마 이야기한거 방영할 예정인가봐",
          "speaker_id": "2",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100038705.1.4",
          "form": "아 한효주 나왔었다",
          "original_form": "아 한효주 나왔었다",
          "speaker_id": "1",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100038705.1.5",
          "form": "해피니스",
          "original_form": "해피니스",
          "speaker_id": "2",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100038705.1.6",
          "form": "한효주랑 누가 나와",
          "original_form": "한효주랑 누가 나와",
          "speaker_id": "1",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100038705.1.7",
          "form": "이것도 계급간 차별이네",
          "original_form": "이것도 계급간 차별이네",
          "speaker_id": "2",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100038705.1.8",
          "form": "옛날 이야기야?",
          "original_form": "옛날 이야기야?",
          "speaker_id": "1",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100038705.1.9",
          "form": "박형식 출연이라는데",
          "original_form": "박형식 출연이라는데",
          "speaker_id": "2",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100038705.1.10",
          "form": "박형식 어제도 고려중인 드라마 있었자나",
          "original_form": "박형식 어제도 고려중인 드라마 있었자나",
          "speaker_id": "1",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100038705.1.11",
          "form": "감염병이 일상화된 시대에 고층은 일반 분양 저층은 임대주택으로 나눈 아파트에서 벌어지는 애기",
          "original_form": "감염병이 일상화된 시대에 고층은 일반 분양 저층은 임대주택으로 나눈 아파트에서 벌어지는 애기",
          "speaker_id": "2",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100038705.1.12",
          "form": "아 재밌을까요?",
          "original_form": "아 재밌을까요?",
          "speaker_id": "1",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100038705.1.13",
          "form": "연이어서 드라마를 찍을려나 보다",
          "original_form": "연이어서 드라마를 찍을려나 보다",
          "speaker_id": "2",
          "time": "20210902 19:53"
        }
