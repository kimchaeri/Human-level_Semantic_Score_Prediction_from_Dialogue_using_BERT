      "utterance": [
        {
          "id": "MDRW2100043345.1.1",
          "form": "주가 급여는 뭔데??",
          "original_form": "주가 급여는 뭔데??",
          "speaker_id": "2",
          "time": "20210907 18:00"
        },
        {
          "id": "MDRW2100043345.1.2",
          "form": "그 소득이 없으면",
          "original_form": "그 소득이 없으면",
          "speaker_id": "1",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100043345.1.3",
          "form": "어",
          "original_form": "어",
          "speaker_id": "2",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100043345.1.4",
          "form": "월세 같은 거를",
          "original_form": "월세 같은 거를",
          "speaker_id": "1",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100043345.1.5",
          "form": "나라에서 지원해 줘요",
          "original_form": "나라에서 지원해 줘요",
          "speaker_id": "1",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100043345.1.6",
          "form": "오 좋네",
          "original_form": "오 좋네",
          "speaker_id": "2",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100043345.1.7",
          "form": "울 사촌언니가 동사무소에 있어서",
          "original_form": "울 사촌언니가 동사무소에 있어서",
          "speaker_id": "1",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100043345.1.8",
          "form": "얘기해줬는데",
          "original_form": "얘기해줬는데",
          "speaker_id": "1",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100043345.1.9",
          "form": "어디서 신청하노",
          "original_form": "어디서 신청하노",
          "speaker_id": "2",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100043345.1.10",
          "form": "그거는 자식들 뭐 재산도 안보고",
          "original_form": "그거는 자식들 뭐 재산도 안보고",
          "speaker_id": "1",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100043345.1.11",
          "form": "우리집도 소득이 없는데",
          "original_form": "우리집도 소득이 없는데",
          "speaker_id": "2",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100043345.1.12",
          "form": "현풍읍사무소",
          "original_form": "현풍읍사무소",
          "speaker_id": "1",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100043345.1.13",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100043345.1.14",
          "form": "주거급여 신청하러 오ㅓㅆ다하면",
          "original_form": "주거급여 신청하러 오ㅓㅆ다하면",
          "speaker_id": "1",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100043345.1.15",
          "form": "서류 주고 자기들이 심사하거든요",
          "original_form": "서류 주고 자기들이 심사하거든요",
          "speaker_id": "1",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043345.1.16",
          "form": "웬만하면 통과",
          "original_form": "웬만하면 통과",
          "speaker_id": "1",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043345.1.17",
          "form": "그러면 달마다 엘에이치에 나가는 돈을",
          "original_form": "그러면 달마다 엘에이치에 나가는 돈을",
          "speaker_id": "1",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043345.1.18",
          "form": "엘에이치로 바로 송금해줘요",
          "original_form": "엘에이치로 바로 송금해줘요",
          "speaker_id": "1",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043345.1.19",
          "form": "다달이 돈 안 내도 됨",
          "original_form": "다달이 돈 안 내도 됨",
          "speaker_id": "1",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043345.1.20",
          "form": "어",
          "original_form": "어",
          "speaker_id": "2",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043345.1.21",
          "form": "오 대박",
          "original_form": "오 대박",
          "speaker_id": "2",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043345.1.22",
          "form": "일단 신청해봐요",
          "original_form": "일단 신청해봐요",
          "speaker_id": "1",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043345.1.23",
          "form": "내일 바로  가라고 해야겠다",
          "original_form": "내일 바로  가라고 해야겠다",
          "speaker_id": "2",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043345.1.24",
          "form": "전화해서 서류 물어봐봐요",
          "original_form": "전화해서 서류 물어봐봐요",
          "speaker_id": "1",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100043345.1.25",
          "form": "그리고 준비해서 바로 가면 됨",
          "original_form": "그리고 준비해서 바로 가면 됨",
          "speaker_id": "1",
          "time": "20210907 18:03"
        }
      ]
