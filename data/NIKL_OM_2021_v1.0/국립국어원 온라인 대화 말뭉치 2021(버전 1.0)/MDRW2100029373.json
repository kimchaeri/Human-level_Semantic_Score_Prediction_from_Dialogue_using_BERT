      "utterance": [
        {
          "id": "MDRW2100029373.1.1",
          "form": "노는언니면 그 박세리 선수님!!",
          "original_form": "노는언니면 그 박세리 선수님!!",
          "speaker_id": "2",
          "time": "20210903 03:27"
        },
        {
          "id": "MDRW2100029373.1.2",
          "form": "맞아요! 세리선수님도 나오세요",
          "original_form": "맞아요! 세리선수님도 나오세요",
          "speaker_id": "1",
          "time": "20210903 03:28"
        },
        {
          "id": "MDRW2100029373.1.3",
          "form": "너무 멋지고귀여운 리치언니",
          "original_form": "너무 멋지고귀여운 리치언니",
          "speaker_id": "1",
          "time": "20210903 03:28"
        },
        {
          "id": "MDRW2100029373.1.4",
          "form": "ㅠㅜㅜㅜㅜㅠ아 진짜 예능으로 몇번 뵈었는데 너무 매력적이고 다하시던데",
          "original_form": "ㅠㅜㅜㅜㅜㅠ아 진짜 예능으로 몇번 뵈었는데 너무 매력적이고 다하시던데",
          "speaker_id": "2",
          "time": "20210903 03:28"
        },
        {
          "id": "MDRW2100029373.1.5",
          "form": "골프 원래 관심없었는데 선수님 보고 이번 올림픽때 찾아봤었어요",
          "original_form": "골프 원래 관심없었는데 선수님 보고 이번 올림픽때 찾아봤었어요",
          "speaker_id": "2",
          "time": "20210903 03:29"
        },
        {
          "id": "MDRW2100029373.1.6",
          "form": "이번올림픽진짜",
          "original_form": "이번올림픽진짜",
          "speaker_id": "1",
          "time": "20210903 03:29"
        },
        {
          "id": "MDRW2100029373.1.7",
          "form": "너무재밌었어요",
          "original_form": "너무재밌었어요",
          "speaker_id": "1",
          "time": "20210903 03:30"
        },
        {
          "id": "MDRW2100029373.1.8",
          "form": "방사능올림픽안볼라고했는데,,",
          "original_form": "방사능올림픽안볼라고했는데,,",
          "speaker_id": "1",
          "time": "20210903 03:30"
        },
        {
          "id": "MDRW2100029373.1.9",
          "form": "아 진짜요ㅜㅜㅠ방사능에 코로나라고 해서 기대안했는데",
          "original_form": "아 진짜요ㅜㅜㅠ방사능에 코로나라고 해서 기대안했는데",
          "speaker_id": "2",
          "time": "20210903 03:30"
        },
        {
          "id": "MDRW2100029373.1.10",
          "form": "아 혹시 개막식 보셨나요",
          "original_form": "아 혹시 개막식 보셨나요",
          "speaker_id": "2",
          "time": "20210903 03:30"
        },
        {
          "id": "MDRW2100029373.1.11",
          "form": "아 개막식에서 일단 모든기대를 접었었어요",
          "original_form": "아 개막식에서 일단 모든기대를 접었었어요",
          "speaker_id": "1",
          "time": "20210903 03:31"
        },
        {
          "id": "MDRW2100029373.1.12",
          "form": "ㅋㅋㄱㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋ저도요ㅋㅋㄱㅋㄱㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋ저도요ㅋㅋㄱㅋㄱㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 03:31"
        },
        {
          "id": "MDRW2100029373.1.13",
          "form": "그나마 제일 기억나는게 픽토그램이었어요ㅋㄱㅋ",
          "original_form": "그나마 제일 기억나는게 픽토그램이었어요ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210903 03:31"
        },
        {
          "id": "MDRW2100029373.1.14",
          "form": "ㅋㄱㅋㅋㅋㄱㅋㄱ픽토그램ㅋㄱㅋㄱㅋㄱㄱ진짜 와,,,",
          "original_form": "ㅋㄱㅋㅋㅋㄱㅋㄱ픽토그램ㅋㄱㅋㄱㅋㄱㄱ진짜 와,,,",
          "speaker_id": "1",
          "time": "20210903 03:32"
        },
        {
          "id": "MDRW2100029373.1.15",
          "form": "우리나라가 일주일만에 개막해도 그거보다 잘했을것같아요",
          "original_form": "우리나라가 일주일만에 개막해도 그거보다 잘했을것같아요",
          "speaker_id": "1",
          "time": "20210903 03:32"
        },
        {
          "id": "MDRW2100029373.1.16",
          "form": "3D라고 해서 디지털기술 생각했더니....갑작스레 등장한.....",
          "original_form": "3D라고 해서 디지털기술 생각했더니....갑작스레 등장한.....",
          "speaker_id": "2",
          "time": "20210903 03:32"
        },
        {
          "id": "MDRW2100029373.1.17",
          "form": "아 그니까요",
          "original_form": "아 그니까요",
          "speaker_id": "2",
          "time": "20210903 03:32"
        },
        {
          "id": "MDRW2100029373.1.18",
          "form": "솔직히 선수들이 하드캐리ㅠ",
          "original_form": "솔직히 선수들이 하드캐리ㅠ",
          "speaker_id": "1",
          "time": "20210903 03:33"
        },
        {
          "id": "MDRW2100029373.1.19",
          "form": "진짜 그전까지는",
          "original_form": "진짜 그전까지는",
          "speaker_id": "2",
          "time": "20210903 03:34"
        },
        {
          "id": "MDRW2100029373.1.20",
          "form": "장미란 박태환 이때에 멈춰있었는데",
          "original_form": "장미란 박태환 이때에 멈춰있었는데",
          "speaker_id": "2",
          "time": "20210903 03:34"
        },
        {
          "id": "MDRW2100029373.1.21",
          "form": "이번에 새로운 선수들 많이 볼수있어서 좋았어요",
          "original_form": "이번에 새로운 선수들 많이 볼수있어서 좋았어요",
          "speaker_id": "2",
          "time": "20210903 03:35"
        },
        {
          "id": "MDRW2100029373.1.22",
          "form": "ㅋㄱㅋㅋㄱㅋㄱㅋㄱㅋ아 올림픽은 장미란박태환이죠 ~~~!!",
          "original_form": "ㅋㄱㅋㅋㄱㅋㄱㅋㄱㅋ아 올림픽은 장미란박태환이죠 ~~~!!",
          "speaker_id": "1",
