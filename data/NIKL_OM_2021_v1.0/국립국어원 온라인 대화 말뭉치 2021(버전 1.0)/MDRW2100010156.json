        {
          "id": "MDRW2100010156.1.2",
          "form": "전 무교입니다",
          "original_form": "전 무교입니다",
          "speaker_id": "2",
          "time": "20210823 15:01"
        },
        {
          "id": "MDRW2100010156.1.3",
          "form": "저도 무교입니다",
