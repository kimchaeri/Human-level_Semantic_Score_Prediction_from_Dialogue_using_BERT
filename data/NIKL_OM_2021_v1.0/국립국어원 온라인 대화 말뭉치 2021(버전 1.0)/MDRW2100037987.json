      "utterance": [
        {
          "id": "MDRW2100037987.1.1",
          "form": "이 언니 오늘 작정했네요",
          "original_form": "이 언니 오늘 작정했네요",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100037987.1.2",
          "form": "어디서 자꾸 꼬신 내가 나더라ㅋㅋㅋ",
          "original_form": "어디서 자꾸 꼬신 내가 나더라ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100037987.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㄱ 난 그냥 자고 싶은데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱ 난 그냥 자고 싶은데",
          "speaker_id": "1",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100037987.1.4",
          "form": "지금 이거 한다고 씻지도 못하고",
          "original_form": "지금 이거 한다고 씻지도 못하고",
          "speaker_id": "1",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100037987.1.5",
          "form": "자야 되는데",
          "original_form": "자야 되는데",
          "speaker_id": "1",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100037987.1.6",
          "form": "우리 원투는 사이좋게 잠들었다",
          "original_form": "우리 원투는 사이좋게 잠들었다",
          "speaker_id": "1",
          "time": "20210901 22:04"
        },
        {
          "id": "MDRW2100037987.1.7",
          "form": "언니도 맛있게 먹고 사이좋게 잠들어요",
          "original_form": "언니도 맛있게 먹고 사이좋게 잠들어요",
          "speaker_id": "2",
          "time": "20210901 22:05"
        },
        {
          "id": "MDRW2100037987.1.8",
          "form": "아인 아인",
          "original_form": "아인 아인",
          "speaker_id": "2",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100037987.1.9",
          "form": "아잉 아잉",
          "original_form": "아잉 아잉",
          "speaker_id": "2",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100037987.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋ  주제 좀 생각해 줄래??",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋ  주제 좀 생각해 줄래??",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100037987.1.11",
          "form": "의식에 흐름대로 말하지 말라고",
          "original_form": "의식에 흐름대로 말하지 말라고",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100037987.1.12",
          "form": "나도 좀 배달해줘요",
          "original_form": "나도 좀 배달해줘요",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100037987.1.13",
          "form": "너무 많이 먹어서 안됨",
          "original_form": "너무 많이 먹어서 안됨",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100037987.1.14",
          "form": "똥집 잘 살 씹어요",
          "original_form": "똥집 잘 살 씹어요",
          "speaker_id": "2",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100037987.1.15",
          "form": "나 얼마 전에 똥집먹고 턱아파 죽는 줄",
          "original_form": "나 얼마 전에 똥집먹고 턱아파 죽는 줄",
          "speaker_id": "2",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100037987.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ 그래 나도 턱이\r\n약해서",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그래 나도 턱이\r\n약해서",
          "speaker_id": "1",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100037987.1.17",
          "form": "조금만 질기면",
          "original_form": "조금만 질기면",
          "speaker_id": "1",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100037987.1.18",
          "form": "턱이\r\n빠질거 같음",
          "original_form": "턱이\r\n빠질거 같음",
          "speaker_id": "1",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100037987.1.19",
          "form": "그걸 이겨내야 돼요ㅋㅋㅋㅋ",
          "original_form": "그걸 이겨내야 돼요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100037987.1.20",
          "form": "똥집한테 지 지 맙시다",
          "original_form": "똥집한테 지 지 맙시다",
          "speaker_id": "2",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100037987.1.21",
          "form": "그래!!!!",
          "original_form": "그래!!!!",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037987.1.22",
          "form": "내가 다 씹.어.",
          "original_form": "내가 다 씹.어.",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037987.1.23",
          "form": "삼켜주겠어!!!!",
          "original_form": "삼켜주겠어!!!!",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037987.1.24",
          "form": "금니빨 빼고 무 조리",
          "original_form": "금니빨 빼고 무 조리",
          "speaker_id": "2",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100037987.1.25",
          "form": "하...무조리래...",
          "original_form": "하...무조리래...",
          "speaker_id": "2",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100037987.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100037987.1.27",
          "form": "군침삼키지 말고",
          "original_form": "군침삼키지 말고",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100037987.1.28",
          "form": "빨리 주무시게 이사람아",
          "original_form": "빨리 주무시게 이사람아",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100037987.1.29",
          "form": "내 재워놓고 편하게 먹을라 그러죠",
          "original_form": "내 재워놓고 편하게 먹을라 그러죠",
          "speaker_id": "2",
          "time": "20210901 22:30"
        }
      ]
    }
