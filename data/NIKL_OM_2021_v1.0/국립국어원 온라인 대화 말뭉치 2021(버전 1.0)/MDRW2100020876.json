      "utterance": [
        {
          "id": "MDRW2100020876.1.1",
          "form": "가고싶은 곳 있어?",
          "original_form": "가고싶은 곳 있어?",
          "speaker_id": "1",
          "time": "20210828 13:46"
        },
        {
          "id": "MDRW2100020876.1.2",
          "form": "가고 싶은 곳 있어?",
          "original_form": "가고 싶은 곳 있어?",
          "speaker_id": "1",
          "time": "20210828 13:46"
        },
        {
          "id": "MDRW2100020876.1.3",
          "form": "해외는 일본",
          "original_form": "해외는 일본",
          "speaker_id": "2",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100020876.1.4",
          "form": "국내는 경상도",
          "original_form": "국내는 경상도",
          "speaker_id": "2",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100020876.1.5",
          "form": "부산?",
          "original_form": "부산?",
          "speaker_id": "2",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100020876.1.6",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100020876.1.7",
          "form": "나도 부산",
          "original_form": "나도 부산",
          "speaker_id": "1",
          "time": "20210828 13:51"
        },
        {
          "id": "MDRW2100020876.1.8",
          "form": "그리고 전주 한옥마을",
          "original_form": "그리고 전주 한옥마을",
          "speaker_id": "1",
          "time": "20210828 13:51"
        },
        {
          "id": "MDRW2100020876.1.9",
          "form": "그리고 동모",
          "original_form": "그리고 동모",
          "speaker_id": "1",
          "time": "20210828 13:51"
        },
        {
          "id": "MDRW2100020876.1.10",
          "form": "동묘",
          "original_form": "동묘",
          "speaker_id": "1",
          "time": "20210828 13:51"
        },
        {
          "id": "MDRW2100020876.1.11",
          "form": "부산에서 뭐 할까",
          "original_form": "부산에서 뭐 할까",
          "speaker_id": "1",
          "time": "20210828 13:51"
        },
        {
          "id": "MDRW2100020876.1.12",
          "form": "아 동묘 진짜 가고싶어",
          "original_form": "아 동묘 진짜 가고싶어",
          "speaker_id": "2",
          "time": "20210828 13:53"
        },
        {
          "id": "MDRW2100020876.1.13",
          "form": "전주가서 한복입고 돌아다녀보고싶음",
          "original_form": "전주가서 한복입고 돌아다녀보고싶음",
          "speaker_id": "2",
          "time": "20210828 13:53"
        },
        {
          "id": "MDRW2100020876.1.14",
          "form": "부산은 할게 뭐 있지?",
          "original_form": "부산은 할게 뭐 있지?",
          "speaker_id": "2",
          "time": "20210828 13:54"
        },
        {
          "id": "MDRW2100020876.1.15",
          "form": "그니까 한복입고 돌아다나자 나중에",
          "original_form": "그니까 한복입고 돌아다나자 나중에",
          "speaker_id": "1",
          "time": "20210828 13:54"
        },
        {
          "id": "MDRW2100020876.1.16",
          "form": "동묘가면 옷 10만원 이상 살거야 진짜",
          "original_form": "동묘가면 옷 10만원 이상 살거야 진짜",
          "speaker_id": "1",
          "time": "20210828 13:55"
        },
        {
          "id": "MDRW2100020876.1.17",
          "form": "부산은 바다..?밖에 몰라 ㅋㅋㅋ",
          "original_form": "부산은 바다..?밖에 몰라 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:55"
        },
        {
          "id": "MDRW2100020876.1.18",
          "form": "오 동묘에서 10만원이면",
          "original_form": "오 동묘에서 10만원이면",
          "speaker_id": "2",
          "time": "20210828 13:56"
        },
        {
          "id": "MDRW2100020876.1.19",
          "form": "거의 박스에 담아와야되는거 아녀??ㅋㅋㅋㅋ",
          "original_form": "거의 박스에 담아와야되는거 아녀??ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:57"
        },
        {
          "id": "MDRW2100020876.1.20",
          "form": "그니까 그게 문제야 ㅋㅋ 캐리어 가져가야 될 듯 진짜",
          "original_form": "그니까 그게 문제야 ㅋㅋ 캐리어 가져가야 될 듯 진짜",
          "speaker_id": "1",
          "time": "20210828 13:58"
        },
        {
          "id": "MDRW2100020876.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020876.1.22",
          "form": "전주에 육전이 그렇게 맛있다는데",
          "original_form": "전주에 육전이 그렇게 맛있다는데",
          "speaker_id": "1",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020876.1.23",
          "form": "차 꼭 있어야될듯zzzz",
          "original_form": "차 꼭 있어야될듯zzzz",
          "speaker_id": "2",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020876.1.24",
          "form": "한옥마을 가서 먹어보자",
          "original_form": "한옥마을 가서 먹어보자",
          "speaker_id": "1",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020876.1.25",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020876.1.26",
          "form": "오 좋아",
          "original_form": "오 좋아",
          "speaker_id": "2",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020876.1.27",
          "form": "난 약간 여행로망있는데",
          "original_form": "난 약간 여행로망있는데",
          "speaker_id": "2",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020876.1.28",
          "form": "여행지에서 옷을 사서 입고다니는거",
          "original_form": "여행지에서 옷을 사서 입고다니는거",
          "speaker_id": "2",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020876.1.29",
          "form": "하루정도만",
          "original_form": "하루정도만",
          "speaker_id": "2",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020876.1.30",
          "form": "오 그거 좋지",
          "original_form": "오 그거 좋지",
          "speaker_id": "1",
          "time": "20210828 14:01"
        },
        {
          "id": "MDRW2100020876.1.31",
          "form": "좋다 진짜 나중에 그렇게 해 보자",
          "original_form": "좋다 진짜 나중에 그렇게 해 보자",
          "speaker_id": "1",
          "time": "20210828 14:02"
        },
        {
          "id": "MDRW2100020876.1.32",
          "form": "재밌을 듯",
          "original_form": "재밌을 듯",
          "speaker_id": "1",
          "time": "20210828 14:02"
        },
        {
          "id": "MDRW2100020876.1.33",
          "form": "뭔가 추억거리도 생기고 좋울거같애",
          "original_form": "뭔가 추억거리도 생기고 좋울거같애",
          "speaker_id": "2",
          "time": "20210828 14:03"
        },
        {
          "id": "MDRW2100020876.1.34",
          "form": "기념품처럼 생각하면 좋을듯",
          "original_form": "기념품처럼 생각하면 좋을듯",
          "speaker_id": "2",
          "time": "20210828 14:04"
        },
        {
          "id": "MDRW2100020876.1.35",
          "form": "그니까 기억에 오래 남아서 헤헤~",
          "original_form": "그니까 기억에 오래 남아서 헤헤~",
          "speaker_id": "1",
          "time": "20210828 14:06"
        },
        {
          "id": "MDRW2100020876.1.36",
          "form": "나 캠핑 가보고 싶어",
          "original_form": "나 캠핑 가보고 싶어",
          "speaker_id": "1",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100020876.1.37",
          "form": "나중에 차 생기면 ㅋㅋ",
          "original_form": "나중에 차 생기면 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100020876.1.38",
          "form": "오 캠핑 좋아",
          "original_form": "오 캠핑 좋아",
          "speaker_id": "2",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100020876.1.39",
          "form": "차박도 요새 인기던데",
          "original_form": "차박도 요새 인기던데",
          "speaker_id": "2",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100020876.1.40",
          "form": "그니까 차박 완전 인기 많더하",
          "original_form": "그니까 차박 완전 인기 많더하",
          "speaker_id": "1",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100020876.1.41",
