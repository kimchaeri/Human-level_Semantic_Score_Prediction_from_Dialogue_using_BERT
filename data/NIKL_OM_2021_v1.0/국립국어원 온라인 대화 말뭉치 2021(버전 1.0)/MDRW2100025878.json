      "utterance": [
        {
          "id": "MDRW2100025878.1.1",
          "form": "저녁 뭐드시나여ㅋㅋ",
          "original_form": "저녁 뭐드시나여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:09"
        },
        {
          "id": "MDRW2100025878.1.2",
          "form": "김치찌개요! ㅋㅋ 저녁드셨나요",
          "original_form": "김치찌개요! ㅋㅋ 저녁드셨나요",
          "speaker_id": "1",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100025878.1.3",
          "form": "아뇨 고민중이에요!ㅋㅋ",
          "original_form": "아뇨 고민중이에요!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100025878.1.4",
          "form": "피자 먹을지",
          "original_form": "피자 먹을지",
          "speaker_id": "2",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100025878.1.5",
          "form": "피자는 페퍼로니",
          "original_form": "피자는 페퍼로니",
          "speaker_id": "1",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100025878.1.6",
          "form": "와이프가 먹고 남긴거....ㅎㅎ",
          "original_form": "와이프가 먹고 남긴거....ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100025878.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 아빠들의 운명인가요... 그래도 먹고싶은거 드실때 있으시죠..?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 아빠들의 운명인가요... 그래도 먹고싶은거 드실때 있으시죠..?",
          "speaker_id": "1",
          "time": "20210901 20:55"
        },
        {
          "id": "MDRW2100025878.1.8",
          "form": "그쵸 이미 있는건 어쩔수 없고",
          "original_form": "그쵸 이미 있는건 어쩔수 없고",
          "speaker_id": "2",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100025878.1.9",
          "form": "그 외에는 ㅎㅎ",
          "original_form": "그 외에는 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100025878.1.10",
          "form": "짜장 vs 짬뽕!",
          "original_form": "짜장 vs 짬뽕!",
          "speaker_id": "1",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100025878.1.11",
          "form": "돌아온 vs 군요",
          "original_form": "돌아온 vs 군요",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100025878.1.12",
          "form": "짜장!!",
          "original_form": "짜장!!",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100025878.1.13",
          "form": "오 대부분 짬뽕이던데 이유가 있으신가요!!",
          "original_form": "오 대부분 짬뽕이던데 이유가 있으신가요!!",
          "speaker_id": "1",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100025878.1.14",
          "form": "ㅋㅋ짜장이 더 맛있어서....",
          "original_form": "ㅋㅋ짜장이 더 맛있어서....",
          "speaker_id": "2",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100025878.1.15",
          "form": "ㅋㅋㅋㅋ 전 원래 짬뽕파였는데 요즘 짜장으로 돌아섰습니다",
          "original_form": "ㅋㅋㅋㅋ 전 원래 짬뽕파였는데 요즘 짜장으로 돌아섰습니다",
          "speaker_id": "1",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100025878.1.16",
          "form": "ㅋㅋㅋ왜 돌아오셨죠",
          "original_form": "ㅋㅋㅋ왜 돌아오셨죠",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100025878.1.17",
          "form": "맛있는 짜장이 없기 때문이죠",
          "original_form": "맛있는 짜장이 없기 때문이죠",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100025878.1.18",
          "form": "짬뽕의 맛은 상향 평준화 되었다는게 저의 생각인데",
          "original_form": "짬뽕의 맛은 상향 평준화 되었다는게 저의 생각인데",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100025878.1.19",
          "form": "맛있는 짜장집는 정말 없는거 같습니다",
          "original_form": "맛있는 짜장집는 정말 없는거 같습니다",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100025878.1.20",
          "form": "그러다보니 짜장의 길로...",
          "original_form": "그러다보니 짜장의 길로...",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100025878.1.21",
          "form": "ㅋㅋ음 짜장은 좀 애매하네요",
          "original_form": "ㅋㅋ음 짜장은 좀 애매하네요",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100025878.1.22",
          "form": "확 맛있는거는 좀 드는거 같기도",
          "original_form": "확 맛있는거는 좀 드는거 같기도",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100025878.1.23",
          "form": "맛있는 짜장집 전 정말 몇군데 못봤어요 ㅋㅋㅋㅋㅋ",
          "original_form": "맛있는 짜장집 전 정말 몇군데 못봤어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100025878.1.24",
          "form": "ㅋㅋㅋ그래도 한곳 정하면 정착하긴 하는데요",
          "original_form": "ㅋㅋㅋ그래도 한곳 정하면 정착하긴 하는데요",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100025878.1.25",
          "form": "저희 동네 중식당들은 요리는맛있지만 짜장은 영...",
          "original_form": "저희 동네 중식당들은 요리는맛있지만 짜장은 영...",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100025878.1.26",
          "form": "ㅋㅋ거기 짜장 맛집 차리면",
          "original_form": "ㅋㅋ거기 짜장 맛집 차리면",
          "speaker_id": "2",
          "time": "20210902 00:45"
        },
        {
          "id": "MDRW2100025878.1.27",
          "form": "대박 나겠는데요",
          "original_form": "대박 나겠는데요",
