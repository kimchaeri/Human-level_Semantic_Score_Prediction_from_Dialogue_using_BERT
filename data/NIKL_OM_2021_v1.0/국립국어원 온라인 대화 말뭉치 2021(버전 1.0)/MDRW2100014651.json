      "utterance": [
        {
          "id": "MDRW2100014651.1.1",
          "form": "최근에 여행 가신적 있나요?",
          "original_form": "최근에 여행 가신적 있나요?",
          "speaker_id": "1",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014651.1.2",
          "form": "최근 올 초에 친구랑 제주도 갔다왔어요",
          "original_form": "최근 올 초에 친구랑 제주도 갔다왔어요",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014651.1.3",
          "form": "대학친구 중 한명이 제주도 살아서",
          "original_form": "대학친구 중 한명이 제주도 살아서",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014651.1.4",
          "form": "편입 같이 준비하는 대학 친구랑 같이 놀러갔다 왔어요",
          "original_form": "편입 같이 준비하는 대학 친구랑 같이 놀러갔다 왔어요",
          "speaker_id": "2",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014651.1.5",
          "form": "오오 저도 작년 말쯤에 제주도 갔는데 ㅎㅎ",
          "original_form": "오오 저도 작년 말쯤에 제주도 갔는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014651.1.6",
          "form": "엄청 힐링하고 오셨겠네요 !",
          "original_form": "엄청 힐링하고 오셨겠네요 !",
          "speaker_id": "1",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014651.1.7",
          "form": "네ㅎㅎ 어디 쪽으로 다녀오셨어요?",
          "original_form": "네ㅎㅎ 어디 쪽으로 다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014651.1.8",
          "form": "저는 4박 5일로 한바퀴 다 돌았어요 ! 어디쪽으로 가셨어요 ?",
          "original_form": "저는 4박 5일로 한바퀴 다 돌았어요 ! 어디쪽으로 가셨어요 ?",
          "speaker_id": "1",
          "time": "20210825 20:13"
        },
        {
          "id": "MDRW2100014651.1.9",
          "form": "헐 대박.. 저희는 3박 4일로 가서 첫날 남부 갔다가",
          "original_form": "헐 대박.. 저희는 3박 4일로 가서 첫날 남부 갔다가",
          "speaker_id": "2",
          "time": "20210825 20:13"
        },
        {
          "id": "MDRW2100014651.1.10",
          "form": "서부쪽으로 쭉 둘러서 올라왔어요",
          "original_form": "서부쪽으로 쭉 둘러서 올라왔어요",
          "speaker_id": "2",
          "time": "20210825 20:13"
        },
        {
          "id": "MDRW2100014651.1.11",
          "form": "오오 어디가 젤 좋으셨어요 ? ㅎㅎ",
          "original_form": "오오 어디가 젤 좋으셨어요 ? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014651.1.12",
          "form": "저는 협재해수욕장이요",
          "original_form": "저는 협재해수욕장이요",
          "speaker_id": "2",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014651.1.13",
          "form": "정말 거기는 일정에 없던 곳이고 버스 잘못타서 가게 된 곳인데",
          "original_form": "정말 거기는 일정에 없던 곳이고 버스 잘못타서 가게 된 곳인데",
          "speaker_id": "2",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014651.1.14",
          "form": "너무 예쁘더라구요",
          "original_form": "너무 예쁘더라구요",
          "speaker_id": "2",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014651.1.15",
          "form": "아 헐 맞아요 협재 ㅠㅠ 저도 너무 이뻤어요 !",
          "original_form": "아 헐 맞아요 협재 ㅠㅠ 저도 너무 이뻤어요 !",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014651.1.16",
          "form": "뭔가 더 유명한건 애월인데 이쁜건 협재인거같아요 ㅎ",
          "original_form": "뭔가 더 유명한건 애월인데 이쁜건 협재인거같아요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014651.1.17",
          "form": "맞아요! 애월도 이뻤는데",
          "original_form": "맞아요! 애월도 이뻤는데",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100014651.1.18",
          "form": "협재는 뜻밖의 여행이라 그런지 더 기억에 남더라구요",
          "original_form": "협재는 뜻밖의 여행이라 그런지 더 기억에 남더라구요",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100014651.1.19",
          "form": "ㅎㅎ 맞아요",
          "original_form": "ㅎㅎ 맞아요",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100014651.1.20",
          "form": "흑돼지도 드셨어요??",
          "original_form": "흑돼지도 드셨어요??",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100014651.1.21",
          "form": "네 당근이죠 ㅎㅎ!",
          "original_form": "네 당근이죠 ㅎㅎ!",
          "speaker_id": "2",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100014651.1.22",
          "form": "저도 ㅠㅠ 너무 맛있었어요 ㅎㅎ",
          "original_form": "저도 ㅠㅠ 너무 맛있었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100014651.1.23",
          "form": "어떤게 젤 맛있으셨어요?",
          "original_form": "어떤게 젤 맛있으셨어요?",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100014651.1.24",
          "form": "전 흑돼지도 맛있었고, 고등어회랑 딱새우요!",
          "original_form": "전 흑돼지도 맛있었고, 고등어회랑 딱새우요!",
          "speaker_id": "2",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100014651.1.25",
          "form": "아 고등어회 진짜 맛있지않나요 ㅠㅠ",
          "original_form": "아 고등어회 진짜 맛있지않나요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100014651.1.26",
          "form": "저도 젤 맛있게 먹었어요 ㅎㅎ",
          "original_form": "저도 젤 맛있게 먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:23"
        }
      ]
