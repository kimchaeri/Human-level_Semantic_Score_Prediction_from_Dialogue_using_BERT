      "utterance": [
        {
          "id": "MDRW2100041588.1.1",
          "form": "날씨가 좋아요!",
          "original_form": "날씨가 좋아요!",
          "speaker_id": "1",
          "time": "20210906 11:53"
        },
        {
          "id": "MDRW2100041588.1.2",
          "form": "여긴 쫌 습해용",
          "original_form": "여긴 쫌 습해용",
          "speaker_id": "2",
          "time": "20210906 11:54"
        },
        {
          "id": "MDRW2100041588.1.3",
          "form": "그래도 여름끝나서 조으네여",
          "original_form": "그래도 여름끝나서 조으네여",
          "speaker_id": "2",
          "time": "20210906 11:54"
        },
        {
          "id": "MDRW2100041588.1.4",
          "form": "오오 아직 에어컨 트세요.?",
          "original_form": "오오 아직 에어컨 트세요.?",
          "speaker_id": "1",
          "time": "20210906 11:55"
        },
        {
          "id": "MDRW2100041588.1.5",
          "form": "저는 땀쟁이라서 살짝틀어요ㅋㅋ",
          "original_form": "저는 땀쟁이라서 살짝틀어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 11:55"
        },
        {
          "id": "MDRW2100041588.1.6",
          "form": "출근길에 시원하더라구요",
          "original_form": "출근길에 시원하더라구요",
          "speaker_id": "2",
          "time": "20210906 11:56"
        },
        {
          "id": "MDRW2100041588.1.7",
          "form": "아직 겉옷 챙길 날씨는 아니죠??",
          "original_form": "아직 겉옷 챙길 날씨는 아니죠??",
          "speaker_id": "1",
          "time": "20210906 11:56"
        },
        {
          "id": "MDRW2100041588.1.8",
          "form": "ㅋㅋ네ㅋㅋ",
          "original_form": "ㅋㅋ네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 11:56"
        },
        {
          "id": "MDRW2100041588.1.9",
          "form": "그래도 여름끝나서 조으네요",
          "original_form": "그래도 여름끝나서 조으네요",
          "speaker_id": "2",
          "time": "20210906 11:57"
        },
        {
          "id": "MDRW2100041588.1.10",
          "form": "그래도 비가 요즘 안와서",
          "original_form": "그래도 비가 요즘 안와서",
          "speaker_id": "1",
          "time": "20210906 11:57"
        },
        {
          "id": "MDRW2100041588.1.11",
          "form": "너무 좋아요 ㅎㅎㅎㅎ",
          "original_form": "너무 좋아요 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 11:57"
        },
        {
          "id": "MDRW2100041588.1.12",
          "form": "그래두 우산은 계속 챙겨다니세용",
          "original_form": "그래두 우산은 계속 챙겨다니세용",
          "speaker_id": "2",
          "time": "20210906 11:57"
        },
        {
          "id": "MDRW2100041588.1.13",
          "form": "오오 그래야 겠어요",
          "original_form": "오오 그래야 겠어요",
          "speaker_id": "1",
          "time": "20210906 11:57"
        },
        {
          "id": "MDRW2100041588.1.14",
          "form": "삼단우산 들고 다녀야겟어요",
          "original_form": "삼단우산 들고 다녀야겟어요",
          "speaker_id": "1",
          "time": "20210906 11:58"
        },
        {
          "id": "MDRW2100041588.1.15",
          "form": "어느계절조아하시나요",
          "original_form": "어느계절조아하시나요",
          "speaker_id": "2",
          "time": "20210906 11:58"
        },
        {
          "id": "MDRW2100041588.1.16",
          "form": "저는 가을이요..!",
          "original_form": "저는 가을이요..!",
          "speaker_id": "1",
          "time": "20210906 12:00"
        },
        {
          "id": "MDRW2100041588.1.17",
          "form": "가을 코디가 제일 좋아요 ㅜㅠ",
          "original_form": "가을 코디가 제일 좋아요 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210906 12:00"
        },
        {
          "id": "MDRW2100041588.1.18",
          "form": "저두요ㅋㅋ오늘 일하다가 급 인터넷으로 가을옷 질럿어욤",
          "original_form": "저두요ㅋㅋ오늘 일하다가 급 인터넷으로 가을옷 질럿어욤",
          "speaker_id": "2",
          "time": "20210906 12:02"
        },
        {
          "id": "MDRW2100041588.1.19",
