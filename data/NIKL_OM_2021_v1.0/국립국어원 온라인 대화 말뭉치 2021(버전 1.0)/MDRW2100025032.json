      "utterance": [
        {
          "id": "MDRW2100025032.1.1",
          "form": "저녁식사 하셨나요?",
          "original_form": "저녁식사 하셨나요?",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100025032.1.2",
          "form": "네 저녁 뭐드셨어요?",
          "original_form": "네 저녁 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100025032.1.3",
          "form": "저는 닭고기에 야채 곁들여서 구워먹었답니다",
          "original_form": "저는 닭고기에 야채 곁들여서 구워먹었답니다",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100025032.1.4",
          "form": "아카시아님은요?",
          "original_form": "아카시아님은요?",
          "speaker_id": "2",
          "time": "20210831 22:30"
        },
        {
          "id": "MDRW2100025032.1.5",
          "form": "저는 미역국 먹었어요 아빠 생일이라ㅎㅎ",
          "original_form": "저는 미역국 먹었어요 아빠 생일이라ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100025032.1.6",
          "form": "헉 아버님 생신이셨군요ㅋㅋㅋ 축하드려요",
          "original_form": "헉 아버님 생신이셨군요ㅋㅋㅋ 축하드려요",
          "speaker_id": "2",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100025032.1.7",
          "form": "미역국 좋아하세요? 전 좋아하는디",
          "original_form": "미역국 좋아하세요? 전 좋아하는디",
          "speaker_id": "2",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100025032.1.8",
          "form": "저도 좋아해요ㅎㅎ입맛이 완전 한국인이에요",
          "original_form": "저도 좋아해요ㅎㅎ입맛이 완전 한국인이에요",
          "speaker_id": "1",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100025032.1.9",
          "form": "통했네요 히히",
          "original_form": "통했네요 히히",
          "speaker_id": "2",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100025032.1.10",
          "form": "제가 저녁으로 먹은 닭고기는 안심살이었는데",
          "original_form": "제가 저녁으로 먹은 닭고기는 안심살이었는데",
          "speaker_id": "2",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100025032.1.11",
          "form": "요즘 닭안심살만 따로 냉동으로 나오는 게 엄청 깔끔하고 괜찮더라구요",
          "original_form": "요즘 닭안심살만 따로 냉동으로 나오는 게 엄청 깔끔하고 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100025032.1.12",
          "form": "닭고기도 좋아하세요?",
          "original_form": "닭고기도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100025032.1.13",
          "form": "네 닭 엄청 좋아하죠ㅎㅎ닭안심 냉동도 괜찮나요?",
          "original_form": "네 닭 엄청 좋아하죠ㅎㅎ닭안심 냉동도 괜찮나요?",
          "speaker_id": "1",
          "time": "20210831 22:35"
        },
        {
          "id": "MDRW2100025032.1.14",
          "form": "네! 뻑뻑할까봐 걱정했는데 생각보다 부드럽고 그냥 구우면 끝이라 엄청 편해요ㅋㅋㅋ",
          "original_form": "네! 뻑뻑할까봐 걱정했는데 생각보다 부드럽고 그냥 구우면 끝이라 엄청 편해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100025032.1.15",
          "form": "닭가슴살만 냉동있는줄 알았는데 신기해요ㅎㅎ닭안심이 훨 맛있잖아요",
          "original_form": "닭가슴살만 냉동있는줄 알았는데 신기해요ㅎㅎ닭안심이 훨 맛있잖아요",
          "speaker_id": "1",
          "time": "20210831 22:36"
        },
        {
          "id": "MDRW2100025032.1.16",
          "form": "양념장없이 그냥 구우시나요?",
          "original_form": "양념장없이 그냥 구우시나요?",
          "speaker_id": "1",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100025032.1.17",
          "form": "그냥 소금이랑 후추 간 해서 먹어도 맛있고",
          "original_form": "그냥 소금이랑 후추 간 해서 먹어도 맛있고",
          "speaker_id": "2",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100025032.1.18",
          "form": "소금 후추 간 해서 굽다가 마지막에 간장양념 하면 살짝 찜닭맛 나고 맛있어요",
          "original_form": "소금 후추 간 해서 굽다가 마지막에 간장양념 하면 살짝 찜닭맛 나고 맛있어요",
          "speaker_id": "2",
          "time": "20210831 22:38"
        },
        {
          "id": "MDRW2100025032.1.19",
          "form": "강추입니다!",
          "original_form": "강추입니다!",
          "speaker_id": "2",
          "time": "20210831 22:38"
        },
        {
          "id": "MDRW2100025032.1.20",
          "form": "오 참고해서 먹어볼게요!",
          "original_form": "오 참고해서 먹어볼게요!",
          "speaker_id": "1",
          "time": "20210831 22:38"
        },
        {
          "id": "MDRW2100025032.1.21",
          "form": "닭안심은 어디서 사셨어요?",
          "original_form": "닭안심은 어디서 사셨어요?",
          "speaker_id": "1",
          "time": "20210831 22:38"
        },
        {
          "id": "MDRW2100025032.1.22",
          "form": "헉 저도 기억이 안 나는데",
          "original_form": "헉 저도 기억이 안 나는데",
          "speaker_id": "2",
          "time": "20210831 22:39"
        },
        {
          "id": "MDRW2100025032.1.23",
          "form": "지금 냉동실 보고 올래요ㅋㅋㅋㅋ",
          "original_form": "지금 냉동실 보고 올래요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:39"
        },
        {
          "id": "MDRW2100025032.1.24",
          "form": "잠시만요",
          "original_form": "잠시만요",
          "speaker_id": "2",
          "time": "20210831 22:39"
        },
        {
          "id": "MDRW2100025032.1.25",
          "form": "하림이려나요ㅎㅎ",
          "original_form": "하림이려나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 22:39"
        },
        {
          "id": "MDRW2100025032.1.26",
          "form": "악 가족이 포장지를 분리수거 해버렸나봐요 찾을 수가 없어요",
          "original_form": "악 가족이 포장지를 분리수거 해버렸나봐요 찾을 수가 없어요",
          "speaker_id": "2",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100025032.1.27",
          "form": "아마 노란 봉투에 든 게 하림거일거예요ㅠㅠ",
          "original_form": "아마 노란 봉투에 든 게 하림거일거예요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100025032.1.28",
          "form": "아쉽네요ㅜㅜ",
          "original_form": "아쉽네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 22:41"
        },
        {
          "id": "MDRW2100025032.1.29",
          "form": "요즘 식품들이 잘 나오니까 고만고만하게 다 맛있을거예요ㅎㅎㅎ",
          "original_form": "요즘 식품들이 잘 나오니까 고만고만하게 다 맛있을거예요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 22:41"
        },
        {
          "id": "MDRW2100025032.1.30",
          "form": "잘 찾아보겠습니다ㅎㅎ",
          "original_form": "잘 찾아보겠습니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 22:41"
        }
      ]
    }
