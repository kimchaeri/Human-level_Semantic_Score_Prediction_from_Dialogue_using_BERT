      "utterance": [
        {
          "id": "MDRW2100035573.1.1",
          "form": "아 저 스트리트우먼파이터 봤어요",
          "original_form": "아 저 스트리트우먼파이터 봤어요",
          "speaker_id": "2",
          "time": "20210829 20:38"
        },
        {
          "id": "MDRW2100035573.1.2",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210829 20:38"
        },
        {
          "id": "MDRW2100035573.1.3",
          "form": "짱짱이었습니다",
          "original_form": "짱짱이었습니다",
          "speaker_id": "2",
          "time": "20210829 20:38"
        },
        {
          "id": "MDRW2100035573.1.4",
          "form": "거기 출연자분중에",
          "original_form": "거기 출연자분중에",
          "speaker_id": "1",
          "time": "20210829 20:38"
        },
        {
          "id": "MDRW2100035573.1.5",
          "form": "예리라는 분 계시던데",
          "original_form": "예리라는 분 계시던데",
          "speaker_id": "1",
          "time": "20210829 20:38"
        },
        {
          "id": "MDRW2100035573.1.6",
          "form": "청각 장애 있으신데도",
          "original_form": "청각 장애 있으신데도",
          "speaker_id": "1",
          "time": "20210829 20:38"
        },
        {
          "id": "MDRW2100035573.1.7",
          "form": "열정이랑 노력하시는 모습이 너무 멋져요",
          "original_form": "열정이랑 노력하시는 모습이 너무 멋져요",
          "speaker_id": "1",
          "time": "20210829 20:38"
        },
        {
          "id": "MDRW2100035573.1.8",
          "form": "아 그분이 그래요?",
          "original_form": "아 그분이 그래요?",
          "speaker_id": "2",
          "time": "20210829 20:38"
        },
        {
          "id": "MDRW2100035573.1.9",
          "form": "네네ㅠ",
          "original_form": "네네ㅠ",
          "speaker_id": "1",
          "time": "20210829 20:38"
        },
        {
          "id": "MDRW2100035573.1.10",
          "form": "춤추는거 진짜 멋지던데",
          "original_form": "춤추는거 진짜 멋지던데",
          "speaker_id": "2",
          "time": "20210829 20:38"
        },
        {
          "id": "MDRW2100035573.1.11",
          "form": "귀에 보청기 착용하고",
          "original_form": "귀에 보청기 착용하고",
          "speaker_id": "1",
          "time": "20210829 20:38"
        },
        {
          "id": "MDRW2100035573.1.12",
          "form": "무대 서신다고 하더라구요",
          "original_form": "무대 서신다고 하더라구요",
          "speaker_id": "1",
          "time": "20210829 20:38"
        },
        {
          "id": "MDRW2100035573.1.13",
          "form": "전혀몰랐어요!",
          "original_form": "전혀몰랐어요!",
          "speaker_id": "2",
          "time": "20210829 20:38"
        },
        {
          "id": "MDRW2100035573.1.14",
          "form": "비걸 이시죠?",
          "original_form": "비걸 이시죠?",
          "speaker_id": "2",
          "time": "20210829 20:38"
        },
        {
          "id": "MDRW2100035573.1.15",
          "form": "저도 전혀 모를정도로 너무 잘하셔서",
          "original_form": "저도 전혀 모를정도로 너무 잘하셔서",
          "speaker_id": "1",
          "time": "20210829 20:39"
        },
        {
          "id": "MDRW2100035573.1.16",
          "form": "그런것같아용!",
          "original_form": "그런것같아용!",
          "speaker_id": "1",
          "time": "20210829 20:39"
        },
        {
          "id": "MDRW2100035573.1.17",
          "form": "대단하시다",
          "original_form": "대단하시다",
          "speaker_id": "2",
          "time": "20210829 20:39"
        },
        {
          "id": "MDRW2100035573.1.18",
          "form": "나이도 되게 어리신데",
          "original_form": "나이도 되게 어리신데",
          "speaker_id": "1",
          "time": "20210829 20:39"
        },
        {
          "id": "MDRW2100035573.1.19",
          "form": "2000년 생이셨나?",
          "original_form": "2000년 생이셨나?",
          "speaker_id": "1",
          "time": "20210829 20:39"
        },
        {
          "id": "MDRW2100035573.1.20",
          "form": "진짜 무대를 휘저어놓으시던데",
          "original_form": "진짜 무대를 휘저어놓으시던데",
          "speaker_id": "2",
          "time": "20210829 20:39"
        },
        {
          "id": "MDRW2100035573.1.21",
          "form": "너무 멋있어요",
          "original_form": "너무 멋있어요",
          "speaker_id": "1",
          "time": "20210829 20:39"
        },
        {
          "id": "MDRW2100035573.1.22",
          "form": "이 프로 요즘 인기 많더라구요",
          "original_form": "이 프로 요즘 인기 많더라구요",
          "speaker_id": "1",
          "time": "20210829 20:39"
        },
        {
          "id": "MDRW2100035573.1.23",
          "form": "히익 새천년대생이세요?",
          "original_form": "히익 새천년대생이세요?",
          "speaker_id": "2",
          "time": "20210829 20:39"
        },
        {
          "id": "MDRW2100035573.1.24",
          "form": "네넼ㅋㅋㅋ",
          "original_form": "네넼ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:39"
        },
        {
          "id": "MDRW2100035573.1.25",
          "form": "대박이에요",
          "original_form": "대박이에요",
          "speaker_id": "2",
          "time": "20210829 20:39"
        }
      ]
    }
