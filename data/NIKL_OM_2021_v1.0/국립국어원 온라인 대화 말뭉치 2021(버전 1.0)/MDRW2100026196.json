      "utterance": [
        {
          "id": "MDRW2100026196.1.1",
          "form": "저녁 메뉴 공개해줘요",
          "original_form": "저녁 메뉴 공개해줘요",
          "speaker_id": "2",
          "time": "20210902 18:47"
        },
        {
          "id": "MDRW2100026196.1.2",
          "form": "회사와서 먹고있지",
          "original_form": "회사와서 먹고있지",
          "speaker_id": "1",
          "time": "20210902 19:04"
        },
        {
          "id": "MDRW2100026196.1.3",
          "form": "괜찬은곳 발견했어",
          "original_form": "괜찬은곳 발견했어",
          "speaker_id": "1",
          "time": "20210902 19:05"
        },
        {
          "id": "MDRW2100026196.1.4",
          "form": "낼이면 제주도 회 드실분이 전야제야?",
          "original_form": "낼이면 제주도 회 드실분이 전야제야?",
          "speaker_id": "2",
          "time": "20210902 19:05"
        },
        {
          "id": "MDRW2100026196.1.5",
          "form": "어디꺼 먹었는데?",
          "original_form": "어디꺼 먹었는데?",
          "speaker_id": "2",
          "time": "20210902 19:05"
        },
        {
          "id": "MDRW2100026196.1.6",
          "form": "난 어제 했던 카레 먹었으",
          "original_form": "난 어제 했던 카레 먹었으",
          "speaker_id": "2",
          "time": "20210902 19:06"
        },
        {
          "id": "MDRW2100026196.1.7",
          "form": "명품회? ㅎㅎ",
          "original_form": "명품회? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 19:24"
        },
        {
          "id": "MDRW2100026196.1.8",
          "form": "포장배달전문인데 푸짐하고 맛있네",
          "original_form": "포장배달전문인데 푸짐하고 맛있네",
          "speaker_id": "1",
          "time": "20210902 19:24"
        },
        {
          "id": "MDRW2100026196.1.9",
          "form": "뭐뭐 배달와?",
          "original_form": "뭐뭐 배달와?",
          "speaker_id": "2",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100026196.1.10",
          "form": "탕이랑 스끼다시도 포함되나?",
          "original_form": "탕이랑 스끼다시도 포함되나?",
          "speaker_id": "2",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100026196.1.11",
          "form": "5만원이상이면 물회도 주는데 양도 많고",
          "original_form": "5만원이상이면 물회도 주는데 양도 많고",
          "speaker_id": "1",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100026196.1.12",
          "form": "낙지, 타코와사비도 주고",
          "original_form": "낙지, 타코와사비도 주고",
          "speaker_id": "1",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100026196.1.13",
          "form": "탕은 따로",
          "original_form": "탕은 따로",
          "speaker_id": "1",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100026196.1.14",
          "form": "오늘은 뭐드셨나?",
          "original_form": "오늘은 뭐드셨나?",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100026196.1.15",
          "form": "보리굴비 먹었지요",
          "original_form": "보리굴비 먹었지요",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100026196.1.16",
          "form": "비싼거 먹었군",
          "original_form": "비싼거 먹었군",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100026196.1.17",
          "form": "보리굴비 맛있는지 잘 모르겠던데",
          "original_form": "보리굴비 맛있는지 잘 모르겠던데",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100026196.1.18",
          "form": "냉동실 구석에서 하나 찾았어",
          "original_form": "냉동실 구석에서 하나 찾았어",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100026196.1.19",
          "form": "나도 냉동실에 있는데 먹어야겠네",
          "original_form": "나도 냉동실에 있는데 먹어야겠네",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100026196.1.20",
          "form": "쌀뜨물에 담궜다 쪄봐",
          "original_form": "쌀뜨물에 담궜다 쪄봐",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100026196.1.21",
          "form": "구이보나 쩌먹는게 낫나?",
          "original_form": "구이보나 쩌먹는게 낫나?",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100026196.1.22",
          "form": "땅콩조림이랑 깻잎장아찌",
          "original_form": "땅콩조림이랑 깻잎장아찌",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100026196.1.23",
          "form": "보리굴비는 쪄야지",
          "original_form": "보리굴비는 쪄야지",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100026196.1.24",
          "form": "내일이라도 먹어야겠어 냉동실을 비워야지",
          "original_form": "내일이라도 먹어야겠어 냉동실을 비워야지",
          "speaker_id": "1",
          "time": "20210907 21:28"
        }
      ]
