      "utterance": [
        {
          "id": "MDRW2100017041.1.1",
          "form": "아보카도가 몸에 좋대~",
          "original_form": "아보카도가 몸에 좋대~",
          "speaker_id": "2",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100017041.1.2",
          "form": "그렇다 하대",
          "original_form": "그렇다 하대",
          "speaker_id": "1",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100017041.1.3",
          "form": "옹옹 하루에 반개씩~",
          "original_form": "옹옹 하루에 반개씩~",
          "speaker_id": "2",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100017041.1.4",
          "form": "아까 사놨는데",
          "original_form": "아까 사놨는데",
          "speaker_id": "1",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100017041.1.5",
          "form": "오오! 잘했옹 ㅋㅋ 먹자",
          "original_form": "오오! 잘했옹 ㅋㅋ 먹자",
          "speaker_id": "2",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100017041.1.6",
          "form": "그래 한번 맛있게 요리해 봐라",
          "original_form": "그래 한번 맛있게 요리해 봐라",
          "speaker_id": "1",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100017041.1.7",
          "form": "옹 ㅋㅋ 그냥 먹어도 맛있지만",
          "original_form": "옹 ㅋㅋ 그냥 먹어도 맛있지만",
          "speaker_id": "2",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100017041.1.8",
          "form": "그래 계란이랑 요리하는 거 있더라",
          "original_form": "그래 계란이랑 요리하는 거 있더라",
          "speaker_id": "1",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100017041.1.9",
          "form": "옹 ㅋㅋ 맞당 간도 하고",
          "original_form": "옹 ㅋㅋ 맞당 간도 하고",
          "speaker_id": "2",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100017041.1.10",
          "form": "간을 한다고",
          "original_form": "간을 한다고",
          "speaker_id": "1",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100017041.1.11",
          "form": "웅 소금 후추 뿌리면 더 맛있댕",
          "original_form": "웅 소금 후추 뿌리면 더 맛있댕",
          "speaker_id": "2",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100017041.1.12",
          "form": "그렇군 고기 같네",
          "original_form": "그렇군 고기 같네",
          "speaker_id": "1",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100017041.1.13",
          "form": "ㅋㅋ 내가 만들어드리겠어용",
          "original_form": "ㅋㅋ 내가 만들어드리겠어용",
          "speaker_id": "2",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100017041.1.14",
