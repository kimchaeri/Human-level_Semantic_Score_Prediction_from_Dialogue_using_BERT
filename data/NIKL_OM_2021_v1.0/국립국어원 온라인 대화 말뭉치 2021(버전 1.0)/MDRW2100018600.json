      "utterance": [
        {
          "id": "MDRW2100018600.1.1",
          "form": "점심식사는 잘하셨나요?ㅎㅎ",
          "original_form": "점심식사는 잘하셨나요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100018600.1.2",
          "form": "저희는 오늘 구내식당에서 콩나물김치국이 메뉴에 있었는데",
          "original_form": "저희는 오늘 구내식당에서 콩나물김치국이 메뉴에 있었는데",
          "speaker_id": "2",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100018600.1.3",
          "form": "흐린 날씨에 시원한 국물이 좋더라고요",
          "original_form": "흐린 날씨에 시원한 국물이 좋더라고요",
          "speaker_id": "2",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100018600.1.4",
          "form": "크으....칼칼하니 맛있었겠어요ㅜ",
          "original_form": "크으....칼칼하니 맛있었겠어요ㅜ",
          "speaker_id": "1",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100018600.1.5",
          "form": "저는 진라면 매운맛에 계란풀어서",
          "original_form": "저는 진라면 매운맛에 계란풀어서",
          "speaker_id": "1",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100018600.1.6",
          "form": "오랜만에 끓여먹는 라면",
          "original_form": "오랜만에 끓여먹는 라면",
          "speaker_id": "1",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100018600.1.7",
          "form": "오 라면 맛있었겠네요",
          "original_form": "오 라면 맛있었겠네요",
          "speaker_id": "2",
          "time": "20210827 20:35"
        },
        {
          "id": "MDRW2100018600.1.8",
          "form": "저녁으로는 컵라면 먹었어요!",
          "original_form": "저녁으로는 컵라면 먹었어요!",
          "speaker_id": "1",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100018600.1.9",
          "form": "참깨라면 큰컵",
          "original_form": "참깨라면 큰컵",
          "speaker_id": "1",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100018600.1.10",
          "form": "저녁 뭐드셨어요?",
          "original_form": "저녁 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100018600.1.11",
          "form": "점심에 라면, 저녁에 컵라면이요?!!",
          "original_form": "점심에 라면, 저녁에 컵라면이요?!!",
          "speaker_id": "2",
          "time": "20210827 22:40"
        },
        {
          "id": "MDRW2100018600.1.12",
          "form": "저는 저녁엔 퇴근하고 조금 이따 운동 가야해서",
          "original_form": "저는 저녁엔 퇴근하고 조금 이따 운동 가야해서",
          "speaker_id": "2",
          "time": "20210827 22:40"
        },
        {
          "id": "MDRW2100018600.1.13",
          "form": "간단히 샐러드 먹었어요",
          "original_form": "간단히 샐러드 먹었어요",
          "speaker_id": "2",
          "time": "20210827 22:41"
        },
        {
          "id": "MDRW2100018600.1.14",
          "form": "오늘 라면데이요ㅎㅎㅎ",
          "original_form": "오늘 라면데이요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100018600.1.15",
          "form": "저도 요즘 샐러드 자주먹어요 어제도 먹었는데",
          "original_form": "저도 요즘 샐러드 자주먹어요 어제도 먹었는데",
          "speaker_id": "1",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100018600.1.16",
          "form": "사먹으려니 은근 비싸더라고요ㅠ",
          "original_form": "사먹으려니 은근 비싸더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100018600.1.17",
          "form": "네 저도 오늘은 밖에서 사 먹었는데",
          "original_form": "네 저도 오늘은 밖에서 사 먹었는데",
          "speaker_id": "2",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100018600.1.18",
          "form": "평소엔 집에 샐러드 밀키트(?) 만들어놓고 하나씩 꺼내먹어요",
          "original_form": "평소엔 집에 샐러드 밀키트(?) 만들어놓고 하나씩 꺼내먹어요",
          "speaker_id": "2",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100018600.1.19",
          "form": "그거 뭐라고 하더라....",
          "original_form": "그거 뭐라고 하더라....",
          "speaker_id": "1",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100018600.1.20",
          "form": "밀프렙?! 맞죠",
          "original_form": "밀프렙?! 맞죠",
          "speaker_id": "1",
          "time": "20210827 22:44"
        },
        {
          "id": "MDRW2100018600.1.21",
          "form": "오오 맞아요 밀프렙!!",
          "original_form": "오오 맞아요 밀프렙!!",
          "speaker_id": "2",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100018600.1.22",
          "form": "단어가 생각이 안났어욯ㅎ",
          "original_form": "단어가 생각이 안났어욯ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100018600.1.23",
          "form": "그거 좋아보이더라고요~",
          "original_form": "그거 좋아보이더라고요~",
          "speaker_id": "1",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100018600.1.24",
          "form": "이사온집 냉장고가 성능이 너무 안좋라서 저는 야채를 사다둘수가 없어요ㅠㅠ",
          "original_form": "이사온집 냉장고가 성능이 너무 안좋라서 저는 야채를 사다둘수가 없어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100018600.1.25",
          "form": "그래서 샐러드는 항상 사먹어요..",
          "original_form": "그래서 샐러드는 항상 사먹어요..",
          "speaker_id": "1",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100018600.1.26",
          "form": "샐러드 사먹으면 은근히 비싼데",
          "original_form": "샐러드 사먹으면 은근히 비싼데",
          "speaker_id": "2",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100018600.1.27",
          "form": "근데 사먹는 게 확실히 맛있긴 하더라고요",
          "original_form": "근데 사먹는 게 확실히 맛있긴 하더라고요",
          "speaker_id": "2",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100018600.1.28",
          "form": "구성도 다양하고",
          "original_form": "구성도 다양하고",
          "speaker_id": "2",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100018600.1.29",
          "form": "그럼 집에서 만들땐",
          "original_form": "그럼 집에서 만들땐",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100018600.1.30",
          "form": "토핑?! 어떤거 올리세요?",
          "original_form": "토핑?! 어떤거 올리세요?",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100018600.1.31",
          "form": "전 거의 연어샐러드 먹어요ㅎㅎ",
          "original_form": "전 거의 연어샐러드 먹어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100018600.1.32",
          "form": "그때그때 조금씩 다른데",
          "original_form": "그때그때 조금씩 다른데",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100018600.1.33",
          "form": "저도 훈제연어 곁들이는 것 좋아해요",
          "original_form": "저도 훈제연어 곁들이는 것 좋아해요",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100018600.1.34",
          "form": "삶은달걀이나",
          "original_form": "삶은달걀이나",
          "speaker_id": "2",
          "time": "20210827 22:52"
        },
        {
          "id": "MDRW2100018600.1.35",
          "form": "치킨큐브 곁들이기도 하고요",
          "original_form": "치킨큐브 곁들이기도 하고요",
          "speaker_id": "2",
          "time": "20210827 22:52"
        }
      ]
    }
