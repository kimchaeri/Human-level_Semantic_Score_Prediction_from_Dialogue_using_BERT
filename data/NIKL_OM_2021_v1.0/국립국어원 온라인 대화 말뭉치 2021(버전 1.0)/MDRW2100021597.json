      "utterance": [
        {
          "id": "MDRW2100021597.1.1",
          "form": "먹거리 얘기 하니까 수월하다",
          "original_form": "먹거리 얘기 하니까 수월하다",
          "speaker_id": "2",
          "time": "20210828 20:49"
        },
        {
          "id": "MDRW2100021597.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:49"
        },
        {
          "id": "MDRW2100021597.1.3",
          "form": "배달음식",
          "original_form": "배달음식",
          "speaker_id": "1",
          "time": "20210828 20:49"
        },
        {
          "id": "MDRW2100021597.1.4",
          "form": "먹니??",
          "original_form": "먹니??",
          "speaker_id": "1",
          "time": "20210828 20:49"
        },
        {
          "id": "MDRW2100021597.1.5",
          "form": "아니 잘 안 먹어 먹으면 치킨이나 피자 정도?",
          "original_form": "아니 잘 안 먹어 먹으면 치킨이나 피자 정도?",
          "speaker_id": "2",
          "time": "20210828 20:50"
        },
        {
          "id": "MDRW2100021597.1.6",
          "form": "대부분 집밥을 먹어",
          "original_form": "대부분 집밥을 먹어",
          "speaker_id": "2",
          "time": "20210828 20:50"
        },
        {
          "id": "MDRW2100021597.1.7",
          "form": "싫어한다기보다는",
          "original_form": "싫어한다기보다는",
          "speaker_id": "2",
          "time": "20210828 20:50"
        },
        {
          "id": "MDRW2100021597.1.8",
          "form": "비용때문에 주로 집밥 먹어",
          "original_form": "비용때문에 주로 집밥 먹어",
          "speaker_id": "2",
          "time": "20210828 20:50"
        },
        {
          "id": "MDRW2100021597.1.9",
          "form": "같은 값에 먹고 싶은 걸 더 많이 만들어먹을 수 있기 때문",
          "original_form": "같은 값에 먹고 싶은 걸 더 많이 만들어먹을 수 있기 때문",
          "speaker_id": "2",
          "time": "20210828 20:50"
        },
        {
          "id": "MDRW2100021597.1.10",
          "form": "난 가끔 날 위한 보상으로 먹어",
          "original_form": "난 가끔 날 위한 보상으로 먹어",
          "speaker_id": "1",
          "time": "20210828 20:51"
        },
        {
          "id": "MDRW2100021597.1.11",
          "form": "마라탕처럼 만들기 힘들거나",
          "original_form": "마라탕처럼 만들기 힘들거나",
          "speaker_id": "1",
          "time": "20210828 20:51"
        },
        {
          "id": "MDRW2100021597.1.12",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210828 20:52"
        },
        {
          "id": "MDRW2100021597.1.13",
          "form": "듣고있어",
          "original_form": "듣고있어",
          "speaker_id": "2",
          "time": "20210828 20:52"
        },
        {
          "id": "MDRW2100021597.1.14",
          "form": "아 말끝난건ㄷ니",
          "original_form": "아 말끝난건ㄷ니",
          "speaker_id": "1",
          "time": "20210828 20:53"
        },
        {
          "id": "MDRW2100021597.1.15",
          "form": "껄껄",
          "original_form": "껄껄",
          "speaker_id": "1",
          "time": "20210828 20:53"
        },
        {
          "id": "MDRW2100021597.1.16",
          "form": "아 그렇구나",
          "original_form": "아 그렇구나",
          "speaker_id": "2",
          "time": "20210828 20:53"
        },
        {
          "id": "MDRW2100021597.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:53"
        },
        {
          "id": "MDRW2100021597.1.18",
          "form": "맞아 내가 만들기 힘든 건",
          "original_form": "맞아 내가 만들기 힘든 건",
          "speaker_id": "2",
          "time": "20210828 20:53"
        },
        {
          "id": "MDRW2100021597.1.19",
          "form": "나도 밖에서 사먹어",
          "original_form": "나도 밖에서 사먹어",
          "speaker_id": "2",
          "time": "20210828 20:54"
        },
        {
          "id": "MDRW2100021597.1.20",
          "form": "넌 만들어먹을때",
          "original_form": "넌 만들어먹을때",
          "speaker_id": "1",
          "time": "20210828 20:54"
        },
        {
          "id": "MDRW2100021597.1.21",
          "form": "제일 자신있는게 뭐야?",
          "original_form": "제일 자신있는게 뭐야?",
          "speaker_id": "1",
          "time": "20210828 20:54"
        },
        {
          "id": "MDRW2100021597.1.22",
          "form": "굴소스볶음밥!",
          "original_form": "굴소스볶음밥!",
          "speaker_id": "2",
          "time": "20210828 20:54"
        },
        {
          "id": "MDRW2100021597.1.23",
          "form": "내가 만들지만 진짜 너무 맛있어서",
          "original_form": "내가 만들지만 진짜 너무 맛있어서",
          "speaker_id": "2",
          "time": "20210828 20:54"
        },
        {
          "id": "MDRW2100021597.1.24",
          "form": "먹고싶다",
          "original_form": "먹고싶다",
          "speaker_id": "1",
          "time": "20210828 20:55"
        },
        {
          "id": "MDRW2100021597.1.25",
          "form": "뭐넣어??",
          "original_form": "뭐넣어??",
          "speaker_id": "1",
          "time": "20210828 20:55"
        },
        {
          "id": "MDRW2100021597.1.26",
          "form": "ㅋㅋㅋ그거 해 먹을 때마다 밥 두 공기씩 먹어",
          "original_form": "ㅋㅋㅋ그거 해 먹을 때마다 밥 두 공기씩 먹어",
          "speaker_id": "2",
          "time": "20210828 20:55"
        },
        {
          "id": "MDRW2100021597.1.27",
          "form": "양파 파 양배추 당근을 주로 넣고",
          "original_form": "양파 파 양배추 당근을 주로 넣고",
          "speaker_id": "2",
          "time": "20210828 20:55"
        },
        {
          "id": "MDRW2100021597.1.28",
          "form": "밥두공기라니 더맛있어보여",
          "original_form": "밥두공기라니 더맛있어보여",
          "speaker_id": "1",
          "time": "20210828 20:56"
        },
        {
          "id": "MDRW2100021597.1.29",
          "form": "나도 볶음밥에 파넣는데",
          "original_form": "나도 볶음밥에 파넣는데",
          "speaker_id": "1",
          "time": "20210828 20:56"
        },
        {
          "id": "MDRW2100021597.1.30",
          "form": "육류는 저번에 넣어봤는데 고기향에 나머지 재료의 풍미가 가려지는 것 같아서",
          "original_form": "육류는 저번에 넣어봤는데 고기향에 나머지 재료의 풍미가 가려지는 것 같아서",
          "speaker_id": "2",
          "time": "20210828 20:56"
        },
        {
          "id": "MDRW2100021597.1.31",
          "form": "파 뒷부분 넣으면 달다",
          "original_form": "파 뒷부분 넣으면 달다",
          "speaker_id": "1",
          "time": "20210828 20:56"
        },
        {
          "id": "MDRW2100021597.1.32",
          "form": "야채만 넣는 편",
          "original_form": "야채만 넣는 편",
          "speaker_id": "2",
          "time": "20210828 20:56"
        },
        {
          "id": "MDRW2100021597.1.33",
          "form": "맞아 파는 흰부분이 맛있더라",
          "original_form": "맞아 파는 흰부분이 맛있더라",
          "speaker_id": "2",
          "time": "20210828 20:56"
        },
        {
          "id": "MDRW2100021597.1.34",
          "form": "파 녹색부분은 영양분 빼앗긴",
          "original_form": "파 녹색부분은 영양분 빼앗긴",
          "speaker_id": "1",
          "time": "20210828 20:56"
        },
        {
          "id": "MDRW2100021597.1.35",
          "form": "처음에 파기름 낸 다음 만들면",
          "original_form": "처음에 파기름 낸 다음 만들면",
          "speaker_id": "2",
          "time": "20210828 20:56"
        },
        {
          "id": "MDRW2100021597.1.36",
          "form": "머리카락같아",
          "original_form": "머리카락같아",
          "speaker_id": "1",
          "time": "20210828 20:56"
        },
        {
          "id": "MDRW2100021597.1.37",
          "form": "맛이없어",
          "original_form": "맛이없어",
          "speaker_id": "1",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100021597.1.38",
          "form": "풍미가 더 좋아",
          "original_form": "풍미가 더 좋아",
          "speaker_id": "2",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100021597.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ식감이 말라 비틀어진 감이 있어",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ식감이 말라 비틀어진 감이 있어",
          "speaker_id": "2",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100021597.1.40",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210828 20:57"
        }
      ]
    }
  ]
