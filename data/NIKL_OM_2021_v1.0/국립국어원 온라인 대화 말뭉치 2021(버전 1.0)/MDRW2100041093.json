      "utterance": [
        {
          "id": "MDRW2100041093.1.1",
          "form": "아 봤어요",
          "original_form": "아 봤어요",
          "speaker_id": "2",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100041093.1.2",
          "form": "티비에서 한번에 쭉 하더라구요",
          "original_form": "티비에서 한번에 쭉 하더라구요",
          "speaker_id": "2",
          "time": "20210905 16:52"
        },
        {
          "id": "MDRW2100041093.1.3",
          "form": "금방 끝날줄 알았는데",
          "original_form": "금방 끝날줄 알았는데",
          "speaker_id": "2",
          "time": "20210905 16:52"
        },
        {
          "id": "MDRW2100041093.1.4",
          "form": "생각보다 길어서 당황했어요",
          "original_form": "생각보다 길어서 당황했어요",
          "speaker_id": "2",
          "time": "20210905 16:52"
        },
        {
          "id": "MDRW2100041093.1.5",
          "form": "맞아요 대탈출이랑 다르게 그건 시즌1이 아예",
          "original_form": "맞아요 대탈출이랑 다르게 그건 시즌1이 아예",
          "speaker_id": "1",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100041093.1.6",
          "form": "다 같은 내용이라...!",
          "original_form": "다 같은 내용이라...!",
          "speaker_id": "1",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100041093.1.7",
          "form": "전 대탈출보다는 여추 좋아하는 편이에요 ㅎㅎ",
          "original_form": "전 대탈출보다는 여추 좋아하는 편이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100041093.1.8",
          "form": "여추는 한번하고 안한거죠?",
          "original_form": "여추는 한번하고 안한거죠?",
          "speaker_id": "2",
          "time": "20210905 16:53"
        },
        {
          "id": "MDRW2100041093.1.9",
          "form": "네 지금 시즌2 준비중인 걸로 알아요",
          "original_form": "네 지금 시즌2 준비중인 걸로 알아요",
          "speaker_id": "1",
          "time": "20210905 16:54"
        },
        {
          "id": "MDRW2100041093.1.10",
          "form": "나오면 봐야겠네요",
          "original_form": "나오면 봐야겠네요",
          "speaker_id": "2",
          "time": "20210905 16:54"
        },
        {
          "id": "MDRW2100041093.1.11",
          "form": "지니어스는 보셨어요?",
          "original_form": "지니어스는 보셨어요?",
          "speaker_id": "2",
          "time": "20210905 16:54"
        },
        {
          "id": "MDRW2100041093.1.12",
          "form": "네 아마 시즌1 까지 봤던 것 같아요",
          "original_form": "네 아마 시즌1 까지 봤던 것 같아요",
          "speaker_id": "1",
          "time": "20210905 16:55"
        },
        {
          "id": "MDRW2100041093.1.13",
          "form": "그 성규 나오는 시즌이요",
          "original_form": "그 성규 나오는 시즌이요",
          "speaker_id": "1",
          "time": "20210905 16:55"
        },
        {
          "id": "MDRW2100041093.1.14",
          "form": "저도요 시즌1만 열심히봤어요",
          "original_form": "저도요 시즌1만 열심히봤어요",
          "speaker_id": "2",
          "time": "20210905 16:55"
        },
        {
          "id": "MDRW2100041093.1.15",
          "form": "시즌2는 출연진바뀌고 안본거같아요",
          "original_form": "시즌2는 출연진바뀌고 안본거같아요",
          "speaker_id": "2",
          "time": "20210905 16:55"
        },
        {
          "id": "MDRW2100041093.1.16",
          "form": "추천해주실 예능 있나요?ㅎ",
          "original_form": "추천해주실 예능 있나요?ㅎ",
          "speaker_id": "2",
          "time": "20210905 16:56"
        },
        {
          "id": "MDRW2100041093.1.17",
          "form": "꼬꼬무랑 심야괴담회 혹시 아시나요?",
          "original_form": "꼬꼬무랑 심야괴담회 혹시 아시나요?",
          "speaker_id": "1",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100041093.1.18",
          "form": "요새 제일 재밌게 보는 예능이에요",
          "original_form": "요새 제일 재밌게 보는 예능이에요",
          "speaker_id": "1",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100041093.1.19",
          "form": "아뇨 무서운건가요?",
          "original_form": "아뇨 무서운건가요?",
          "speaker_id": "2",
          "time": "20210905 17:00"
        },
        {
          "id": "MDRW2100041093.1.20",
          "form": "심야괴담회는 괴담 이야기하는 거라 좀 무서운데",
          "original_form": "심야괴담회는 괴담 이야기하는 거라 좀 무서운데",
          "speaker_id": "1",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100041093.1.21",
          "form": "꼬꼬무는 그냥 이야기? 하는 프로에요",
          "original_form": "꼬꼬무는 그냥 이야기? 하는 프로에요",
          "speaker_id": "1",
          "time": "20210905 17:03"
        },
        {
          "id": "MDRW2100041093.1.22",
          "form": "무서운건 볼엄두가 안나서",
          "original_form": "무서운건 볼엄두가 안나서",
          "speaker_id": "2",
          "time": "20210905 17:03"
        },
        {
          "id": "MDRW2100041093.1.23",
          "form": "꼬꼬무는 한번 찾아볼게요",
          "original_form": "꼬꼬무는 한번 찾아볼게요",
          "speaker_id": "2",
          "time": "20210905 17:03"
        },
        {
          "id": "MDRW2100041093.1.24",
          "form": "앗 무서운 거 안 좋아하시는구나",
          "original_form": "앗 무서운 거 안 좋아하시는구나",
          "speaker_id": "1",
          "time": "20210905 17:03"
        },
        {
          "id": "MDRW2100041093.1.25",
          "form": "꼬꼬무도 살짝 무서운 편 잇어서 골라보시면 될 거예요",
          "original_form": "꼬꼬무도 살짝 무서운 편 잇어서 골라보시면 될 거예요",
          "speaker_id": "1",
          "time": "20210905 17:04"
        },
        {
          "id": "MDRW2100041093.1.26",
          "form": "잠 못잡니다 ㅋㅋㅋ ㅋ",
          "original_form": "잠 못잡니다 ㅋㅋㅋ ㅋ",
          "speaker_id": "2",
          "time": "20210905 17:04"
        },
