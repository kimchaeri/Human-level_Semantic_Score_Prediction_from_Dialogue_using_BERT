        },
        {
          "id": "MDRW2100027254.1.2",
          "form": "김치수제비도",
          "original_form": "김치수제비도",
          "speaker_id": "2",
          "time": "20210902 10:49"
        },
        {
          "id": "MDRW2100027254.1.3",
          "form": "과자는요",
          "original_form": "과자는요",
          "speaker_id": "1",
          "time": "20210902 10:49"
        },
        {
          "id": "MDRW2100027254.1.4",
          "form": "땡기는군요",
          "original_form": "땡기는군요",
          "speaker_id": "2",
          "time": "20210902 10:49"
        },
        {
          "id": "MDRW2100027254.1.5",
          "form": "어제팝콘먹었어요",
          "original_form": "어제팝콘먹었어요",
          "speaker_id": "2",
          "time": "20210902 10:49"
        },
        {
          "id": "MDRW2100027254.1.6",
          "form": "수제비좋죠",
          "original_form": "수제비좋죠",
          "speaker_id": "1",
          "time": "20210902 10:49"
        },
        {
          "id": "MDRW2100027254.1.7",
          "form": "꼬북칩좋아해요",
          "original_form": "꼬북칩좋아해요",
          "speaker_id": "1",
          "time": "20210902 10:49"
        },
        {
          "id": "MDRW2100027254.1.8",
          "form": "사또밥도요",
          "original_form": "사또밥도요",
          "speaker_id": "1",
          "time": "20210902 10:49"
        },
        {
          "id": "MDRW2100027254.1.9",
          "form": "쌀로별, 인절미도 좋고",
          "original_form": "쌀로별, 인절미도 좋고",
          "speaker_id": "1",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027254.1.10",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027254.1.11",
          "form": "전짭짤한거",
          "original_form": "전짭짤한거",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027254.1.12",
          "form": "집에 크라운산도 있어요",
          "original_form": "집에 크라운산도 있어요",
          "speaker_id": "1",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027254.1.13",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027254.1.14",
          "form": "감자과자같은거",
          "original_form": "감자과자같은거",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027254.1.15",
          "form": "두부과자아시나요",
          "original_form": "두부과자아시나요",
          "speaker_id": "1",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027254.1.16",
          "form": "달달한건",
          "original_form": "달달한건",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027254.1.17",
          "form": "고소합니다",
          "original_form": "고소합니다",
          "speaker_id": "1",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027254.1.18",
          "form": "커피랑드세요",
          "original_form": "커피랑드세요",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027254.1.19",
          "form": "안들어봤어요",
          "original_form": "안들어봤어요",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027254.1.20",
          "form": "먹고십네요",
          "original_form": "먹고십네요",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027254.1.21",
          "form": "과자배는 따로있어요",
          "original_form": "과자배는 따로있어요",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027254.1.22",
          "form": "두부찌꺼기로 과자만든거라",
          "original_form": "두부찌꺼기로 과자만든거라",
          "speaker_id": "1",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027254.1.23",
          "form": "건강합니다",
          "original_form": "건강합니다",
          "speaker_id": "1",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027254.1.24",
          "form": "포카칩은 맥주안주이지요",
          "original_form": "포카칩은 맥주안주이지요",
          "speaker_id": "1",
          "time": "20210902 10:51"
        },
        {
          "id": "MDRW2100027254.1.25",
          "form": "어니언맛이",
          "original_form": "어니언맛이",
          "speaker_id": "2",
          "time": "20210902 10:51"
        },
        {
          "id": "MDRW2100027254.1.26",
          "form": "초코칩쿠키도 좋아요",
          "original_form": "초코칩쿠키도 좋아요",
          "speaker_id": "1",
          "time": "20210902 10:51"
        },
        {
          "id": "MDRW2100027254.1.27",
          "form": "진리입니다",
          "original_form": "진리입니다",
          "speaker_id": "2",
          "time": "20210902 10:51"
        }
