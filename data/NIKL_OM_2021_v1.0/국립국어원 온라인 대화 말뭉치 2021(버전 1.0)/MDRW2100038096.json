      "utterance": [
        {
          "id": "MDRW2100038096.1.1",
          "form": "요즘 털갈이하나봐",
          "original_form": "요즘 털갈이하나봐",
          "speaker_id": "2",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100038096.1.2",
          "form": "머리가 너무 많이 빠져.",
          "original_form": "머리가 너무 많이 빠져.",
          "speaker_id": "2",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100038096.1.3",
          "form": "그치? 나도그래",
          "original_form": "그치? 나도그래",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100038096.1.4",
          "form": "샤워만하면 한뭉텅이가 빠져있어",
          "original_form": "샤워만하면 한뭉텅이가 빠져있어",
          "speaker_id": "2",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100038096.1.5",
          "form": "사람도 털갈이를 하나?",
          "original_form": "사람도 털갈이를 하나?",
          "speaker_id": "2",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100038096.1.6",
          "form": "검은콩두유를 먹으면 효과가 있을까??",
          "original_form": "검은콩두유를 먹으면 효과가 있을까??",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100038096.1.7",
          "form": "합리적인 의심이야",
          "original_form": "합리적인 의심이야",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100038096.1.8",
          "form": "흠 그게 두피에 좋대?",
          "original_form": "흠 그게 두피에 좋대?",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100038096.1.9",
          "form": "근데 일단 두유는 건강한 느낌이야",
          "original_form": "근데 일단 두유는 건강한 느낌이야",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100038096.1.10",
          "form": "응 탈모에 좋대 검은콩이",
          "original_form": "응 탈모에 좋대 검은콩이",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100038096.1.11",
          "form": "넌 영양이 부족하니 일단 먹어봐",
          "original_form": "넌 영양이 부족하니 일단 먹어봐",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100038096.1.12",
          "form": "근데 검은콩보다는 두유로 먹는게 맛있으니",
          "original_form": "근데 검은콩보다는 두유로 먹는게 맛있으니",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100038096.1.13",
          "form": "집에 두유라도 사둔거야?",
          "original_form": "집에 두유라도 사둔거야?",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100038096.1.14",
          "form": "말만하는거지?",
          "original_form": "말만하는거지?",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100038096.1.15",
          "form": "오케이 접수!?",
          "original_form": "오케이 접수!?",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100038096.1.16",
          "form": "맞아.그리고 든든해",
          "original_form": "맞아.그리고 든든해",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100038096.1.17",
          "form": "일단 말을 해야 실천하지",
          "original_form": "일단 말을 해야 실천하지",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100038096.1.18",
          "form": "건강은 실천하는게 힘들어",
          "original_form": "건강은 실천하는게 힘들어",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100038096.1.19",
          "form": "맞아. 그리고 썩지않게 보관도 잘 해",
          "original_form": "맞아. 그리고 썩지않게 보관도 잘 해",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100038096.1.20",
          "form": "배탈나면 손해야",
          "original_form": "배탈나면 손해야",
          "speaker_id": "2",
          "time": "20210901 23:10"
        }
      ]
