      "utterance": [
        {
          "id": "MDRW2100025097.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "2",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100025097.1.2",
          "form": "그럼요주체적으로하죠",
          "original_form": "그럼요주체적으로하죠",
          "speaker_id": "1",
          "time": "20210831 23:07"
        },
        {
          "id": "MDRW2100025097.1.3",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "2",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100025097.1.4",
          "form": "ㅋㅋ저도 주로청소해요 요리는 잘하는편이아니라",
          "original_form": "ㅋㅋ저도 주로청소해요 요리는 잘하는편이아니라",
          "speaker_id": "1",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100025097.1.5",
          "form": "화장실청소가 가장 힘들죠",
          "original_form": "화장실청소가 가장 힘들죠",
          "speaker_id": "2",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100025097.1.6",
          "form": "전 매번 다른가족이하길기다려요 ㅋㅋ다른청소하면서",
          "original_form": "전 매번 다른가족이하길기다려요 ㅋㅋ다른청소하면서",
          "speaker_id": "1",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100025097.1.7",
          "form": "아 ㅋㅋ",
          "original_form": "아 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100025097.1.8",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100025097.1.9",
          "form": "꾀 많내오",
          "original_form": "꾀 많내오",
          "speaker_id": "2",
          "time": "20210831 23:22"
        },
        {
          "id": "MDRW2100025097.1.10",
          "form": "가족들이 다들 청소광이라 다행이에요 ㅋ",
          "original_form": "가족들이 다들 청소광이라 다행이에요 ㅋ",
          "speaker_id": "1",
          "time": "20210901 00:30"
        },
        {
          "id": "MDRW2100025097.1.11",
          "form": "깔끔하신가봐요",
          "original_form": "깔끔하신가봐요",
          "speaker_id": "2",
          "time": "20210901 07:13"
        },
        {
          "id": "MDRW2100025097.1.12",
          "form": "네 ㅋㅋ 집이 깔끔하면 서로 기분좋죠 가족도화목하고",
          "original_form": "네 ㅋㅋ 집이 깔끔하면 서로 기분좋죠 가족도화목하고",
          "speaker_id": "1",
          "time": "20210901 11:18"
