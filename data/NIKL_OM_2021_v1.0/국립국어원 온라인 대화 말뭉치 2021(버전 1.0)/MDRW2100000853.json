      "utterance": [
        {
          "id": "MDRW2100000853.1.1",
          "form": "가족들이랑 친하신가여?!",
          "original_form": "가족들이랑 친하신가여?!",
          "speaker_id": "1",
          "time": "20210819 20:05"
        },
        {
          "id": "MDRW2100000853.1.2",
          "form": "그럼요~ 매일보고 매일 같이생활하니까요^^",
          "original_form": "그럼요~ 매일보고 매일 같이생활하니까요^^",
          "speaker_id": "2",
          "time": "20210819 20:06"
        },
        {
          "id": "MDRW2100000853.1.3",
          "form": "저도요 근데 가족하고 많이도 싸워요 ㅎㅎ..",
          "original_form": "저도요 근데 가족하고 많이도 싸워요 ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210819 20:08"
        },
        {
          "id": "MDRW2100000853.1.4",
          "form": "왜요 ㅠㅠ제일 자주 보니까 그럴 수 밖에 없긴하지만",
          "original_form": "왜요 ㅠㅠ제일 자주 보니까 그럴 수 밖에 없긴하지만",
          "speaker_id": "2",
          "time": "20210819 20:09"
        },
        {
          "id": "MDRW2100000853.1.5",
          "form": "그냥 사소한걸로 많이 다투게되더라구요!",
          "original_form": "그냥 사소한걸로 많이 다투게되더라구요!",
          "speaker_id": "1",
          "time": "20210819 20:12"
        },
        {
          "id": "MDRW2100000853.1.6",
          "form": "언니나 동생 있으신가요?",
          "original_form": "언니나 동생 있으신가요?",
          "speaker_id": "1",
          "time": "20210819 20:12"
        },
        {
          "id": "MDRW2100000853.1.7",
          "form": "아니요 없어요",
          "original_form": "아니요 없어요",
          "speaker_id": "2",
          "time": "20210819 20:12"
        },
        {
          "id": "MDRW2100000853.1.8",
          "form": "그게 제일 아쉬워요ㅜㅜ",
          "original_form": "그게 제일 아쉬워요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210819 20:12"
        },
        {
          "id": "MDRW2100000853.1.9",
          "form": "아 외동이세요? 저는 동생들만 있는데 ㅎ ㅎㅎ..",
          "original_form": "아 외동이세요? 저는 동생들만 있는데 ㅎ ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210819 20:15"
        },
        {
          "id": "MDRW2100000853.1.10",
          "form": "전 외동이 부럽더라구요",
          "original_form": "전 외동이 부럽더라구요",
          "speaker_id": "1",
          "time": "20210819 20:16"
        },
        {
          "id": "MDRW2100000853.1.11",
          "form": "전 오빠가있어요 ㅋㅋ",
          "original_form": "전 오빠가있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:16"
        },
        {
          "id": "MDRW2100000853.1.12",
          "form": "동생이 없어서",
          "original_form": "동생이 없어서",
          "speaker_id": "2",
          "time": "20210819 20:16"
        },
        {
          "id": "MDRW2100000853.1.13",
          "form": "동생대하는게 어려워요",
          "original_form": "동생대하는게 어려워요",
          "speaker_id": "2",
          "time": "20210819 20:16"
        },
        {
          "id": "MDRW2100000853.1.14",
          "form": "아하 뭔지 알아요",
          "original_form": "아하 뭔지 알아요",
          "speaker_id": "1",
          "time": "20210819 20:17"
        },
        {
          "id": "MDRW2100000853.1.15",
          "form": "전 동생이 있는데도 어렵더라구요",
          "original_form": "전 동생이 있는데도 어렵더라구요",
          "speaker_id": "1",
          "time": "20210819 20:18"
        },
        {
          "id": "MDRW2100000853.1.16",
          "form": "차라리 위가 편한 거 같아요",
          "original_form": "차라리 위가 편한 거 같아요",
          "speaker_id": "1",
          "time": "20210819 20:18"
        },
        {
          "id": "MDRW2100000853.1.17",
          "form": "맞아요 사회에 나와도",
          "original_form": "맞아요 사회에 나와도",
          "speaker_id": "2",
          "time": "20210819 20:18"
        },
        {
          "id": "MDRW2100000853.1.18",
          "form": "언니오빠들이 잘 챙겨주니 고맙고 편하죠 ㅎㅎ",
          "original_form": "언니오빠들이 잘 챙겨주니 고맙고 편하죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 20:19"
        },
        {
          "id": "MDRW2100000853.1.19",
          "form": "맞아요ㅋㅋㅋㅋㅋㅋㅋㅠ 동생들은 왜인지 모르게 불편해요",
          "original_form": "맞아요ㅋㅋㅋㅋㅋㅋㅋㅠ 동생들은 왜인지 모르게 불편해요",
          "speaker_id": "1",
          "time": "20210819 20:21"
        },
        {
          "id": "MDRW2100000853.1.20",
          "form": "저도 언니나 오빠 갖고싶네요",
          "original_form": "저도 언니나 오빠 갖고싶네요",
          "speaker_id": "1",
          "time": "20210819 20:21"
        },
        {
          "id": "MDRW2100000853.1.21",
          "form": "동성이 좋은거같아요~  그래야 어른이되어서도 마음나누기가 편하더라구요 특히 여자들은 ㅎㅎ",
          "original_form": "동성이 좋은거같아요~  그래야 어른이되어서도 마음나누기가 편하더라구요 특히 여자들은 ㅎㅎ",
