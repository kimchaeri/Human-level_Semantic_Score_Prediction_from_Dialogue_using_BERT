      "utterance": [
        {
          "id": "MDRW2100014385.1.1",
          "form": "안녕하세용ㅎㅎ",
          "original_form": "안녕하세용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014385.1.2",
          "form": "동물 좋아하세요?",
          "original_form": "동물 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014385.1.3",
          "form": "좋아하는데 키우진 않고 보는 걸 더 좋아해요!",
          "original_form": "좋아하는데 키우진 않고 보는 걸 더 좋아해요!",
          "speaker_id": "2",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100014385.1.4",
          "form": "고양이 털에 알레르기가 있거든요",
          "original_form": "고양이 털에 알레르기가 있거든요",
          "speaker_id": "2",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100014385.1.5",
          "form": "name1님은 반려동물 키우시나요?",
          "original_form": "&name1&님은 반려동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100014385.1.6",
          "form": "아하 그러시구나!",
          "original_form": "아하 그러시구나!",
          "speaker_id": "1",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100014385.1.7",
          "form": "저도 보는거 좋아해요!",
          "original_form": "저도 보는거 좋아해요!",
          "speaker_id": "1",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100014385.1.8",
          "form": "물고기 종류는 좀 키워요!",
          "original_form": "물고기 종류는 좀 키워요!",
          "speaker_id": "1",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100014385.1.9",
          "form": "오 물고기! 귀여울 거 같아요",
          "original_form": "오 물고기! 귀여울 거 같아요",
          "speaker_id": "2",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100014385.1.10",
          "form": "물고기 키우기로 결정하신 계기가 있나요?",
          "original_form": "물고기 키우기로 결정하신 계기가 있나요?",
          "speaker_id": "2",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100014385.1.11",
          "form": "아주 작은 구피라는 종을 키우고있는데",
          "original_form": "아주 작은 구피라는 종을 키우고있는데",
          "speaker_id": "1",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100014385.1.12",
          "form": "어릴때부터 제가 동물을 좋아했었는데 큰동물들 보다는 기르기 부담이 안될거같아서 키워보게 됐던거같아요!",
          "original_form": "어릴때부터 제가 동물을 좋아했었는데 큰동물들 보다는 기르기 부담이 안될거같아서 키워보게 됐던거같아요!",
          "speaker_id": "1",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100014385.1.13",
          "form": "물고기들은 아플 때 어떻게 하나요",
          "original_form": "물고기들은 아플 때 어떻게 하나요",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100014385.1.14",
          "form": "이 아이들은 엄청 작아서 아픈게 잘 보이지는 않는데",
          "original_form": "이 아이들은 엄청 작아서 아픈게 잘 보이지는 않는데",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100014385.1.15",
          "form": "오래살더라구요!",
          "original_form": "오래살더라구요!",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100014385.1.16",
          "form": "알 낳을때쯤 배부른게 보이면 그때는 조마조마하긴해요ㅠㅠ",
          "original_form": "알 낳을때쯤 배부른게 보이면 그때는 조마조마하긴해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100014385.1.17",
          "form": "알 낳고 나면 좀 약해지는 편인가요?",
          "original_form": "알 낳고 나면 좀 약해지는 편인가요?",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100014385.1.18",
          "form": "낳고나면 좀 그런거같기도 해요!",
          "original_form": "낳고나면 좀 그런거같기도 해요!",
          "speaker_id": "1",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100014385.1.19",
          "form": "낳기전에도 좀 스트레스 많이 받는거같아요!ㅠㅠ",
          "original_form": "낳기전에도 좀 스트레스 많이 받는거같아요!ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100014385.1.20",
          "form": "물고기들 스트레스 받을 땐 어떻게 해 줘야 하나요..?",
          "original_form": "물고기들 스트레스 받을 땐 어떻게 해 줘야 하나요..?",
          "speaker_id": "2",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100014385.1.21",
          "form": "최대한 물을 갈아준다거나 그런거 하지않고 스트레스를 덜어주려고 하고 밥도 잘 주고 그래요!!",
          "original_form": "최대한 물을 갈아준다거나 그런거 하지않고 스트레스를 덜어주려고 하고 밥도 잘 주고 그래요!!",
          "speaker_id": "1",
          "time": "20210826 14:04"
        },
        {
          "id": "MDRW2100014385.1.22",
          "form": "물 자주 갈아주면 스트레스 받는 거네요",
          "original_form": "물 자주 갈아주면 스트레스 받는 거네요",
          "speaker_id": "2",
          "time": "20210827 11:32"
        },
        {
          "id": "MDRW2100014385.1.23",
          "form": "네 아무래도 환경도 바뀌고 그러니까 스트레스 받는거같더라구요!",
          "original_form": "네 아무래도 환경도 바뀌고 그러니까 스트레스 받는거같더라구요!",
          "speaker_id": "1",
