      "utterance": [
        {
          "id": "MDRW2100039658.1.1",
          "form": "히히",
          "original_form": "히히",
          "speaker_id": "2",
          "time": "20210903 20:14"
        },
        {
          "id": "MDRW2100039658.1.2",
          "form": "영화보신거 있으세용?",
          "original_form": "영화보신거 있으세용?",
          "speaker_id": "1",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039658.1.3",
          "form": "저 모가디슈 봤어요",
          "original_form": "저 모가디슈 봤어요",
          "speaker_id": "2",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100039658.1.4",
          "form": "어어 저도 최근에 모가디슈 봤어요!",
          "original_form": "어어 저도 최근에 모가디슈 봤어요!",
          "speaker_id": "1",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100039658.1.5",
          "form": "문화의 날에 ㅎㅎ",
          "original_form": "문화의 날에 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100039658.1.6",
          "form": "아하!!!!!",
          "original_form": "아하!!!!!",
          "speaker_id": "2",
          "time": "20210903 20:18"
        },
        {
          "id": "MDRW2100039658.1.7",
          "form": "문화의날 아시는 분 별로없던데",
          "original_form": "문화의날 아시는 분 별로없던데",
          "speaker_id": "2",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100039658.1.8",
          "form": "잉?",
          "original_form": "잉?",
          "speaker_id": "1",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100039658.1.9",
          "form": "그래요????",
          "original_form": "그래요????",
          "speaker_id": "1",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100039658.1.10",
          "form": "다들 알던데...제주변은 ㅠ",
          "original_form": "다들 알던데...제주변은 ㅠ",
          "speaker_id": "1",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100039658.1.11",
          "form": "아 진짜여?",
          "original_form": "아 진짜여?",
          "speaker_id": "2",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039658.1.12",
          "form": "제주변은 관심없어해서",
          "original_form": "제주변은 관심없어해서",
          "speaker_id": "2",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039658.1.13",
          "form": "그럼 저렴하게 보셨겠다 부러워요",
          "original_form": "그럼 저렴하게 보셨겠다 부러워요",
          "speaker_id": "2",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039658.1.14",
          "form": "음? 혼자도 볼 수 있잖아요???",
          "original_form": "음? 혼자도 볼 수 있잖아요???",
          "speaker_id": "1",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100039658.1.15",
          "form": "저는 공연보다 집이좋아서",
          "original_form": "저는 공연보다 집이좋아서",
          "speaker_id": "2",
          "time": "20210903 20:25"
        },
        {
          "id": "MDRW2100039658.1.16",
          "form": "히히",
          "original_form": "히히",
          "speaker_id": "2",
          "time": "20210903 20:25"
        },
        {
          "id": "MDRW2100039658.1.17",
          "form": "ㅋㅋㅋㅋ 그건 동의합니다",
          "original_form": "ㅋㅋㅋㅋ 그건 동의합니다",
          "speaker_id": "1",
          "time": "20210903 20:26"
        },
        {
          "id": "MDRW2100039658.1.18",
          "form": "집이 최고죠!",
          "original_form": "집이 최고죠!",
          "speaker_id": "1",
          "time": "20210903 20:26"
        },
        {
          "id": "MDRW2100039658.1.19",
          "form": "모가디슈 어떠셨어요?",
          "original_form": "모가디슈 어떠셨어요?",
          "speaker_id": "2",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100039658.1.20",
          "form": "전 재밌다길래 봤는데 생각보다 잔인해서 놀랐어요",
          "original_form": "전 재밌다길래 봤는데 생각보다 잔인해서 놀랐어요",
          "speaker_id": "2",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100039658.1.21",
          "form": "전 아무것도 모르는 상태에서 친구가 보자길래 끌려가서 봤어요 ㅋㅋㅋㅋ",
          "original_form": "전 아무것도 모르는 상태에서 친구가 보자길래 끌려가서 봤어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100039658.1.22",
          "form": "재미있게봤는데",
          "original_form": "재미있게봤는데",
          "speaker_id": "1",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100039658.1.23",
          "form": "뭔가 실제랑 다르다는 뉴스보고",
          "original_form": "뭔가 실제랑 다르다는 뉴스보고",
          "speaker_id": "1",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100039658.1.24",
          "form": "아니 기사",
          "original_form": "아니 기사",
          "speaker_id": "1",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100039658.1.25",
          "form": "보고",
          "original_form": "보고",
          "speaker_id": "1",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100039658.1.26",
          "form": "작가님 각색쩌네 생각했죠",
          "original_form": "작가님 각색쩌네 생각했죠",
          "speaker_id": "1",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100039658.1.27",
          "form": "잔인했던가요?",
          "original_form": "잔인했던가요?",
          "speaker_id": "1",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100039658.1.28",
          "form": "15세였던거같은데",
          "original_form": "15세였던거같은데",
          "speaker_id": "1",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100039658.1.29",
          "form": "실화가 아니었군요",
          "original_form": "실화가 아니었군요",
          "speaker_id": "2",
          "time": "20210903 20:30"
        },
        {
          "id": "MDRW2100039658.1.30",
          "form": "그 저는 피만 나오면 온몸을 떨어서",
          "original_form": "그 저는 피만 나오면 온몸을 떨어서",
          "speaker_id": "2",
          "time": "20210903 20:30"
        },
        {
          "id": "MDRW2100039658.1.31",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 20:30"
        },
        {
          "id": "MDRW2100039658.1.32",
          "form": "아이고 ㅠㅠㅠㅠ",
          "original_form": "아이고 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 20:31"
        },
        {
          "id": "MDRW2100039658.1.33",
          "form": "실화긴한데",
          "original_form": "실화긴한데",
          "speaker_id": "1",
          "time": "20210903 20:31"
        },
        {
          "id": "MDRW2100039658.1.34",
          "form": "내용큰틀만 같고",
          "original_form": "내용큰틀만 같고",
          "speaker_id": "1",
          "time": "20210903 20:31"
        },
        {
          "id": "MDRW2100039658.1.35",
          "form": "세부적인건 다른?",
          "original_form": "세부적인건 다른?",
          "speaker_id": "1",
          "time": "20210903 20:31"
        },
        {
          "id": "MDRW2100039658.1.36",
          "form": "헐진짜요?",
          "original_form": "헐진짜요?",
          "speaker_id": "2",
          "time": "20210903 20:32"
        },
        {
          "id": "MDRW2100039658.1.37",
          "form": "배신감드네요ㅜㅜ",
          "original_form": "배신감드네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 20:32"
        },
        {
          "id": "MDRW2100039658.1.38",
          "form": "전 다같이 떠났으면했는데",
          "original_form": "전 다같이 떠났으면했는데",
          "speaker_id": "2",
          "time": "20210903 20:32"
        },
        {
          "id": "MDRW2100039658.1.39",
          "form": "한분 돌아가셔서",
          "original_form": "한분 돌아가셔서",
          "speaker_id": "2",
          "time": "20210903 20:32"
        }
      ]
    }
