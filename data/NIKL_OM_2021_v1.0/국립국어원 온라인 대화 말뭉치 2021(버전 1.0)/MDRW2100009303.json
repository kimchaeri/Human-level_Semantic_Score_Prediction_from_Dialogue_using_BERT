      "utterance": [
        {
          "id": "MDRW2100009303.1.1",
          "form": "온라인 피트니스 아세요?",
          "original_form": "온라인 피트니스 아세요?",
          "speaker_id": "1",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100009303.1.2",
          "form": "모르는데 앱같은거 따라하는 건가요?",
          "original_form": "모르는데 앱같은거 따라하는 건가요?",
          "speaker_id": "2",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009303.1.3",
          "form": "그런가봐요",
          "original_form": "그런가봐요",
          "speaker_id": "1",
          "time": "20210822 22:57"
        },
        {
          "id": "MDRW2100009303.1.4",
          "form": "결국 본인의 의지ㅠ",
          "original_form": "결국 본인의 의지ㅠ",
          "speaker_id": "1",
          "time": "20210822 22:57"
        },
        {
          "id": "MDRW2100009303.1.5",
          "form": "그럼 닌텐도로 운동하는것도 온라인 피트니스일까요?",
          "original_form": "그럼 닌텐도로 운동하는것도 온라인 피트니스일까요?",
          "speaker_id": "2",
          "time": "20210822 22:57"
        },
        {
          "id": "MDRW2100009303.1.6",
          "form": "아 그렇겠네요",
          "original_form": "아 그렇겠네요",
          "speaker_id": "1",
          "time": "20210822 22:58"
        },
        {
          "id": "MDRW2100009303.1.7",
          "form": "줌처럼 하는 피트니스도 있을까요?",
          "original_form": "줌처럼 하는 피트니스도 있을까요?",
          "speaker_id": "2",
          "time": "20210822 22:58"
        },
        {
          "id": "MDRW2100009303.1.8",
          "form": "있을것같아요ㅎ",
          "original_form": "있을것같아요ㅎ",
          "speaker_id": "1",
          "time": "20210822 23:32"
        },
        {
          "id": "MDRW2100009303.1.9",
          "form": "해보신거 있으세요?",
          "original_form": "해보신거 있으세요?",
          "speaker_id": "2",
          "time": "20210822 23:36"
        },
        {
          "id": "MDRW2100009303.1.10",
          "form": "줌은 안해봤고",
          "original_form": "줌은 안해봤고",
          "speaker_id": "1",
          "time": "20210822 23:36"
        },
        {
          "id": "MDRW2100009303.1.11",
          "form": "유투부는 해봤어요",
          "original_form": "유투부는 해봤어요",
          "speaker_id": "1",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009303.1.12",
          "form": "저도 유튜브랑 닌텐도랑 앱은 해봤어요",
          "original_form": "저도 유튜브랑 닌텐도랑 앱은 해봤어요",
          "speaker_id": "2",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009303.1.13",
          "form": "모든게 의지가 필요한",
          "original_form": "모든게 의지가 필요한",
          "speaker_id": "1",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009303.1.14",
          "form": "맞아요. 저도 혼자서는 의지를 쉽게 잃어서 운동 소모임 들어갔어요",
          "original_form": "맞아요. 저도 혼자서는 의지를 쉽게 잃어서 운동 소모임 들어갔어요",
          "speaker_id": "2",
