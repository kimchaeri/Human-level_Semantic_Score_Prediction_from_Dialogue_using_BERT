      "utterance": [
        {
          "id": "MDRW2100039352.1.1",
          "form": "배 겨자 식초 좋아하고",
          "original_form": "배 겨자 식초 좋아하고",
          "speaker_id": "2",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100039352.1.2",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100039352.1.3",
          "form": "오인 싫어요",
          "original_form": "오인 싫어요",
          "speaker_id": "2",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100039352.1.4",
          "form": "저랑 잘 통하시네요",
          "original_form": "저랑 잘 통하시네요",
          "speaker_id": "1",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100039352.1.5",
          "form": "오이는 쫄면에는 어울리는데",
          "original_form": "오이는 쫄면에는 어울리는데",
          "speaker_id": "2",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100039352.1.6",
          "form": "물냉면는 정말 아니에요",
          "original_form": "물냉면는 정말 아니에요",
          "speaker_id": "2",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100039352.1.7",
          "form": "그쵸 육수 맛이 이상해져요",
          "original_form": "그쵸 육수 맛이 이상해져요",
          "speaker_id": "1",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100039352.1.8",
          "form": "오이를 좋아하는 편이거든요",
          "original_form": "오이를 좋아하는 편이거든요",
          "speaker_id": "2",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100039352.1.9",
          "form": "아 전 오이 자체를 별로 안 좋아해요",
          "original_form": "아 전 오이 자체를 별로 안 좋아해요",
          "speaker_id": "1",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100039352.1.10",
          "form": "오이소박이 같은 것도 항상 잘 먹고",
          "original_form": "오이소박이 같은 것도 항상 잘 먹고",
          "speaker_id": "2",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100039352.1.11",
          "form": "오이 반찬 다 좋아하는데",
          "original_form": "오이 반찬 다 좋아하는데",
          "speaker_id": "2",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100039352.1.12",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100039352.1.13",
          "form": "물냉면은 정말 아니에요",
          "original_form": "물냉면은 정말 아니에요",
          "speaker_id": "2",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100039352.1.14",
          "form": "혹시 평양냉면 좋아하세요?",
          "original_form": "혹시 평양냉면 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100039352.1.15",
          "form": "슴슴한 매력이 있긴 한데",
          "original_form": "슴슴한 매력이 있긴 한데",
          "speaker_id": "2",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100039352.1.16",
          "form": "방금 백석 같으셨어요",
          "original_form": "방금 백석 같으셨어요",
          "speaker_id": "1",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100039352.1.17",
          "form": "전 함흥냉면 스타일이 좋아요",
          "original_form": "전 함흥냉면 스타일이 좋아요",
          "speaker_id": "2",
          "time": "20210903 16:37"
        },
        {
          "id": "MDRW2100039352.1.18",
          "form": "해주냉면도",
          "original_form": "해주냉면도",
          "speaker_id": "2",
          "time": "20210903 16:37"
        },
        {
          "id": "MDRW2100039352.1.19",
          "form": "해주냉면은 뭐예요?",
          "original_form": "해주냉면은 뭐예요?",
          "speaker_id": "1",
          "time": "20210903 16:37"
        },
        {
          "id": "MDRW2100039352.1.20",
          "form": "근처에 유명한 해주냉면 맛집이 있는데 따끈한 육수를 물 대신 줘요",
