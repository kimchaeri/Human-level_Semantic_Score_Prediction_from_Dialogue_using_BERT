      "utterance": [
        {
          "id": "MDRW2100025537.1.1",
          "form": "사람들이",
          "original_form": "사람들이",
          "speaker_id": "1",
          "time": "20210901 13:00"
        },
        {
          "id": "MDRW2100025537.1.2",
          "form": "보고 노래잘하네 하고",
          "original_form": "보고 노래잘하네 하고",
          "speaker_id": "1",
          "time": "20210901 13:00"
        },
        {
          "id": "MDRW2100025537.1.3",
          "form": "찾아봤더니 서보여서 놀랐다",
          "original_form": "찾아봤더니 서보여서 놀랐다",
          "speaker_id": "1",
          "time": "20210901 13:00"
        },
        {
          "id": "MDRW2100025537.1.4",
          "form": "이래서 마음의안정...",
          "original_form": "이래서 마음의안정...",
          "speaker_id": "1",
          "time": "20210901 13:00"
        },
        {
          "id": "MDRW2100025537.1.5",
          "form": "민기야 곧보러가께...",
          "original_form": "민기야 곧보러가께...",
          "speaker_id": "1",
          "time": "20210901 13:00"
        },
        {
          "id": "MDRW2100025537.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ\r\n자랑스러운 내 새꾸...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ\r\n자랑스러운 내 새꾸...",
          "speaker_id": "2",
          "time": "20210901 13:01"
        },
        {
          "id": "MDRW2100025537.1.7",
          "form": "결국 혼자 보러 가??",
          "original_form": "결국 혼자 보러 가??",
          "speaker_id": "2",
          "time": "20210901 13:01"
        },
        {
          "id": "MDRW2100025537.1.8",
          "form": "네 ㅋㅋㄱㅋㅋ",
          "original_form": "네 ㅋㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:01"
        },
        {
          "id": "MDRW2100025537.1.9",
          "form": "나중에 함물어볼까돠",
          "original_form": "나중에 함물어볼까돠",
          "speaker_id": "1",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100025537.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ내 취향 극 아니면 안보는게 맞긴하지...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ내 취향 극 아니면 안보는게 맞긴하지...",
          "speaker_id": "2",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100025537.1.11",
          "form": "같이갈건지",
          "original_form": "같이갈건지",
          "speaker_id": "1",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100025537.1.12",
          "form": "근데 헤드윅 같은건 안보고 흘려보내기 아쉬운데",
          "original_form": "근데 헤드윅 같은건 안보고 흘려보내기 아쉬운데",
          "speaker_id": "2",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100025537.1.13",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "1",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100025537.1.14",
          "form": "이런기회흔치않아",
          "original_form": "이런기회흔치않아",
          "speaker_id": "1",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100025537.1.15",
          "form": "근데 오만석헤드윅이",
          "original_form": "근데 오만석헤드윅이",
          "speaker_id": "1",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100025537.1.16",
          "form": "완전정석이래서",
          "original_form": "완전정석이래서",
          "speaker_id": "1",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100025537.1.17",
          "form": "빠르게보러가복라공",
          "original_form": "빠르게보러가복라공",
          "speaker_id": "1",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100025537.1.18",
          "form": "가볼라고",
          "original_form": "가볼라고",
          "speaker_id": "1",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100025537.1.19",
          "form": "ㅇㅇ맞어.... 김뎅이 헤드윅으로 뮤 데뷔햇는데",
          "original_form": "ㅇㅇ맞어.... 김뎅이 헤드윅으로 뮤 데뷔햇는데",
          "speaker_id": "2",
          "time": "20210901 13:04"
        },
        {
          "id": "MDRW2100025537.1.20",
          "form": "못보러간거 아직도 슬퍼",
          "original_form": "못보러간거 아직도 슬퍼",
          "speaker_id": "2",
          "time": "20210901 13:04"
        },
        {
          "id": "MDRW2100025537.1.21",
          "form": "ㅇㅇㅇ 아 뎅은 헤드윅잘어울릴듯",
          "original_form": "ㅇㅇㅇ 아 뎅은 헤드윅잘어울릴듯",
          "speaker_id": "1",
          "time": "20210901 13:04"
        },
        {
          "id": "MDRW2100025537.1.22",
          "form": "그때는진짜취향아니었어",
          "original_form": "그때는진짜취향아니었어",
          "speaker_id": "2",
          "time": "20210901 13:04"
        },
        {
          "id": "MDRW2100025537.1.23",
          "form": "내가어릴때라",
          "original_form": "내가어릴때라",
          "speaker_id": "2",
          "time": "20210901 13:04"
        },
        {
          "id": "MDRW2100025537.1.24",
          "form": "ㅇㅇㅇ 저도 정문성 못본거아쉬움",
          "original_form": "ㅇㅇㅇ 저도 정문성 못본거아쉬움",
          "speaker_id": "1",
          "time": "20210901 13:05"
        },
        {
          "id": "MDRW2100025537.1.25",
          "form": "김재욱이랑....",
          "original_form": "김재욱이랑....",
          "speaker_id": "1",
          "time": "20210901 13:05"
        },
        {
          "id": "MDRW2100025537.1.26",
          "form": "뽀랑..",
          "original_form": "뽀랑..",
          "speaker_id": "1",
          "time": "20210901 13:05"
        },
        {
          "id": "MDRW2100025537.1.27",
          "form": "김재욱......",
          "original_form": "김재욱......",
          "speaker_id": "2",
          "time": "20210901 13:05"
        },
        {
          "id": "MDRW2100025537.1.28",
          "form": "상상만해도잘어울려",
          "original_form": "상상만해도잘어울려",
          "speaker_id": "2",
          "time": "20210901 13:05"
        },
        {
          "id": "MDRW2100025537.1.29",
          "form": "ㅇㅇㅇㅇ 충격적인 비주얼이라더라",
          "original_form": "ㅇㅇㅇㅇ 충격적인 비주얼이라더라",
          "speaker_id": "1",
          "time": "20210901 13:05"
        },
        {
          "id": "MDRW2100025537.1.30",
          "form": "연기도잘하고",
          "original_form": "연기도잘하고",
          "speaker_id": "1",
          "time": "20210901 13:05"
        },
        {
          "id": "MDRW2100025537.1.31",
          "form": "뀨도 좋던데 헤롱이같아서",
          "original_form": "뀨도 좋던데 헤롱이같아서",
          "speaker_id": "1",
          "time": "20210901 13:06"
        }
      ]
