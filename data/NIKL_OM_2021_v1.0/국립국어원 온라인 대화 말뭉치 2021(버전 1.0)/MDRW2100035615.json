      "utterance": [
        {
          "id": "MDRW2100035615.1.1",
          "form": "즐기는 편이지만 주량이 좋진않아요",
          "original_form": "즐기는 편이지만 주량이 좋진않아요",
          "speaker_id": "2",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100035615.1.2",
          "form": "그러시군요ㅎㅎ\r\n보통 어떤 종류를 드시나요?",
          "original_form": "그러시군요ㅎㅎ\r\n보통 어떤 종류를 드시나요?",
          "speaker_id": "1",
          "time": "20210829 21:19"
        },
        {
          "id": "MDRW2100035615.1.3",
          "form": "막걸리나 맥주 마셔요 ㅎㅎ",
          "original_form": "막걸리나 맥주 마셔요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100035615.1.4",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "1",
          "time": "20210829 21:24"
        },
        {
          "id": "MDRW2100035615.1.5",
          "form": "집에 같이 술 가볍게 할 수 있는 사람이 있어 좋으시겠어요",
          "original_form": "집에 같이 술 가볍게 할 수 있는 사람이 있어 좋으시겠어요",
          "speaker_id": "1",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100035615.1.6",
          "form": "맞아요.. 저도 그렇게생각해요.ㅋㅋ",
          "original_form": "맞아요.. 저도 그렇게생각해요.ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100035615.1.7",
          "form": "술마시면서 이야기 많이 하시는편이세요?",
          "original_form": "술마시면서 이야기 많이 하시는편이세요?",
          "speaker_id": "1",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100035615.1.8",
          "form": "막 엄청 특별한얘기는 안하고 평소와 같아요 ㅎㅎ",
          "original_form": "막 엄청 특별한얘기는 안하고 평소와 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100035615.1.9",
          "form": "그렇군요ㅎㅎ\r\n동생이랑 자주 대화 나누면 좋죠!",
          "original_form": "그렇군요ㅎㅎ\r\n동생이랑 자주 대화 나누면 좋죠!",
          "speaker_id": "1",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100035615.1.10",
          "form": "맞아여 좋게끝나면 좋죠..\r\n가끔 싸우면 큰일나고요 ㅎㅎ",
          "original_form": "맞아여 좋게끝나면 좋죠..\r\n가끔 싸우면 큰일나고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100035615.1.11",
          "form": "싸우게되면 괜히 대화 시작하게 된걸 후회하게될꺼같아요",
          "original_form": "싸우게되면 괜히 대화 시작하게 된걸 후회하게될꺼같아요",
          "speaker_id": "1",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100035615.1.12",
          "form": "맞아요.. ㅋㅋㅋㅋ",
          "original_form": "맞아요.. ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100035615.1.13",
          "form": "언젠가는 화해하면되긴하죠",
          "original_form": "언젠가는 화해하면되긴하죠",
          "speaker_id": "2",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100035615.1.14",
          "form": "맞아욪 가족들과는 자연스레 금방 화해하게되죠",
          "original_form": "맞아욪 가족들과는 자연스레 금방 화해하게되죠",
          "speaker_id": "1",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100035615.1.15",
          "form": "술 다 드셨나요?",
          "original_form": "술 다 드셨나요?",
          "speaker_id": "1",
          "time": "20210830 00:00"
        },
        {
          "id": "MDRW2100035615.1.16",
          "form": "다 먹고 양치즁이에여~~",
          "original_form": "다 먹고 양치즁이에여~~",
          "speaker_id": "2",
          "time": "20210830 00:21"
        },
        {
          "id": "MDRW2100035615.1.17",
          "form": "술자리가 끝나셨군요~",
          "original_form": "술자리가 끝나셨군요~",
          "speaker_id": "1",
          "time": "20210830 00:22"
        },
        {
          "id": "MDRW2100035615.1.18",
          "form": "좋은시간 보내셨나요?",
          "original_form": "좋은시간 보내셨나요?",
