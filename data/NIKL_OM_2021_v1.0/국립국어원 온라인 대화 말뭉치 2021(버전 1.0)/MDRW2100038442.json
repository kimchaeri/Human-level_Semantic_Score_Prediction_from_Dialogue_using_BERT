      "utterance": [
        {
          "id": "MDRW2100038442.1.1",
          "form": "맞아여 팥들어가면 인절미빙수안부러울맛이에요윸ㅋ",
          "original_form": "맞아여 팥들어가면 인절미빙수안부러울맛이에요윸ㅋ",
          "speaker_id": "1",
          "time": "20210902 13:20"
        },
        {
          "id": "MDRW2100038442.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 어우 맛이 상상이 가네욯ㅎㅎㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 어우 맛이 상상이 가네욯ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:20"
        },
        {
          "id": "MDRW2100038442.1.3",
          "form": "헤헤ㅋㅋㅋㅋㅋㅋㅋ 나름추천이요!",
          "original_form": "헤헤ㅋㅋㅋㅋㅋㅋㅋ 나름추천이요!",
          "speaker_id": "1",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100038442.1.4",
          "form": "큐큐 저도 보이면 바로 먹어볼래요",
          "original_form": "큐큐 저도 보이면 바로 먹어볼래요",
          "speaker_id": "2",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100038442.1.5",
          "form": "점심은 드시나요!",
          "original_form": "점심은 드시나요!",
          "speaker_id": "2",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100038442.1.6",
          "form": "넹! 저점심으로 주먹밥해먹었어요 ㅎㅎㅎ헿",
          "original_form": "넹! 저점심으로 주먹밥해먹었어요 ㅎㅎㅎ헿",
          "speaker_id": "1",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100038442.1.7",
          "form": "헐 맛있겠다",
          "original_form": "헐 맛있겠다",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100038442.1.8",
          "form": "무슨 주먹밥이요!!",
          "original_form": "무슨 주먹밥이요!!",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100038442.1.9",
          "form": "저 땡초참치마요주먹밥이요~~단무지도다져서넣구요ㅎㅎ",
          "original_form": "저 땡초참치마요주먹밥이요~~단무지도다져서넣구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100038442.1.10",
          "form": "헉!!!!!",
          "original_form": "헉!!!!!",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100038442.1.11",
          "form": "저 매운거 진짜 좋아하는디",
          "original_form": "저 매운거 진짜 좋아하는디",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100038442.1.12",
          "form": "땡초라굽쇼",
          "original_form": "땡초라굽쇼",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100038442.1.13",
          "form": "완전 맛나겠어요",
          "original_form": "완전 맛나겠어요",
          "speaker_id": "2",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100038442.1.14",
          "form": "맞아여~ 전 인공적인 매운향보단 걍고추의매운맛이좋아여!!헤헤",
          "original_form": "맞아여~ 전 인공적인 매운향보단 걍고추의매운맛이좋아여!!헤헤",
          "speaker_id": "1",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100038442.1.15",
          "form": "큐큐 저두요",
          "original_form": "큐큐 저두요",
          "speaker_id": "2",
          "time": "20210902 13:49"
        },
        {
          "id": "MDRW2100038442.1.16",
          "form": "그래서 약간 저는 한식 좋아하나봐요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그래서 약간 저는 한식 좋아하나봐요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100038442.1.17",
          "form": "아하 ㅋㅋㅋㅋㅋㅋㅋㅋ 한식좋아하시는군여!!",
          "original_form": "아하 ㅋㅋㅋㅋㅋㅋㅋㅋ 한식좋아하시는군여!!",
          "speaker_id": "1",
          "time": "20210902 13:53"
        },
        {
          "id": "MDRW2100038442.1.18",
          "form": "맞아요 ㅋㅋㅋㅋㅋ 매콤얼큰한거 최고입니다",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ 매콤얼큰한거 최고입니다",
          "speaker_id": "2",
          "time": "20210902 14:04"
        },
        {
          "id": "MDRW2100038442.1.19",
          "form": "오 말하다보니 배고프네요 운동 끝나고 먹어야하는디",
          "original_form": "오 말하다보니 배고프네요 운동 끝나고 먹어야하는디",
