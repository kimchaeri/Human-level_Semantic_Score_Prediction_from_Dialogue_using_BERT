        },
        {
          "id": "MDRW2100017845.1.2",
          "form": "기분이 태도가 되지 않게",
          "original_form": "기분이 태도가 되지 않게",
          "speaker_id": "2",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017845.1.3",
          "form": "무난무난",
          "original_form": "무난무난",
          "speaker_id": "2",
          "time": "20210826 21:52"
        },
        {
          "id": "MDRW2100017845.1.4",
          "form": "원래 있는 책이야?",
          "original_form": "원래 있는 책이야?",
          "speaker_id": "1",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100017845.1.5",
          "form": "집에?",
          "original_form": "집에?",
          "speaker_id": "1",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100017845.1.6",
          "form": "아니 빌렷어!",
          "original_form": "아니 빌렷어!",
          "speaker_id": "2",
          "time": "20210826 22:09"
        },
        {
          "id": "MDRW2100017845.1.7",
          "form": "부도 짱~",
          "original_form": "부도 짱~",
          "speaker_id": "2",
          "time": "20210826 22:09"
        },
        {
          "id": "MDRW2100017845.1.8",
          "form": "아하~",
          "original_form": "아하~",
          "speaker_id": "1",
          "time": "20210826 22:23"
        },
        {
          "id": "MDRW2100017845.1.9",
          "form": "name2가 좋아하는 자기계발서 느김인가?",
          "original_form": "&name2&가 좋아하는 자기계발서 느김인가?",
          "speaker_id": "1",
          "time": "20210826 22:23"
        },
        {
          "id": "MDRW2100017845.1.10",
          "form": "음 심리학도서?",
          "original_form": "음 심리학도서?",
          "speaker_id": "2",
          "time": "20210827 11:55"
        },
        {
          "id": "MDRW2100017845.1.11",
          "form": "근데ㅔ 엄청 가벼눠",
          "original_form": "근데ㅔ 엄청 가벼눠",
          "speaker_id": "2",
          "time": "20210827 11:55"
        },
        {
          "id": "MDRW2100017845.1.12",
          "form": "재밌겠네 잘배워 ㅎㅎ",
          "original_form": "재밌겠네 잘배워 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 15:17"
        },
        {
          "id": "MDRW2100017845.1.13",
          "form": "나는 책 뭐 읽고 있게?",
          "original_form": "나는 책 뭐 읽고 있게?",
          "speaker_id": "1",
          "time": "20210827 15:17"
        },
        {
          "id": "MDRW2100017845.1.14",
          "form": "하말넘많이 추천해준거?",
          "original_form": "하말넘많이 추천해준거?",
          "speaker_id": "2",
          "time": "20210827 16:57"
        },
        {
