      "utterance": [
        {
          "id": "MDRW2100018129.1.1",
          "form": "여행 많이댕기시나요?",
          "original_form": "여행 많이댕기시나요?",
          "speaker_id": "1",
          "time": "20210826 22:52"
        },
        {
          "id": "MDRW2100018129.1.2",
          "form": "별로 못 가봤어요 여행 좋아하세요?",
          "original_form": "별로 못 가봤어요 여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 22:53"
        },
        {
          "id": "MDRW2100018129.1.3",
          "form": "저도 나이먹고는 안다니네요.. \n애기때는 배낭여행 다니고 그랬는데..",
          "original_form": "저도 나이먹고는 안다니네요.. \n애기때는 배낭여행 다니고 그랬는데..",
          "speaker_id": "1",
          "time": "20210826 22:54"
        },
        {
          "id": "MDRW2100018129.1.4",
          "form": "최근 가보신곳 잇어요?",
          "original_form": "최근 가보신곳 잇어요?",
          "speaker_id": "1",
          "time": "20210826 22:54"
        },
        {
          "id": "MDRW2100018129.1.5",
          "form": "최근엔 코로나때문에 못가봤어요",
          "original_form": "최근엔 코로나때문에 못가봤어요",
          "speaker_id": "2",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018129.1.6",
          "form": "어디 다니셨어요?",
          "original_form": "어디 다니셨어요?",
          "speaker_id": "2",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018129.1.7",
          "form": "첨 간곳은 일본 후쿠오카요..",
          "original_form": "첨 간곳은 일본 후쿠오카요..",
          "speaker_id": "1",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018129.1.8",
          "form": "완전 거지모드로.. ㅋㅋㅋ 20살때인가..",
          "original_form": "완전 거지모드로.. ㅋㅋㅋ 20살때인가..",
          "speaker_id": "1",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018129.1.9",
          "form": "와 재밌었겠어요 일본이 다들 제일 좋아하더라구요",
          "original_form": "와 재밌었겠어요 일본이 다들 제일 좋아하더라구요",
          "speaker_id": "2",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100018129.1.10",
          "form": "근데 최근 몇년 반일감정도 심해지고 그래서..",
          "original_form": "근데 최근 몇년 반일감정도 심해지고 그래서..",
          "speaker_id": "1",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018129.1.11",
          "form": "맞아요 그거만 아니면 참 좋을텐데요 ㅠ",
          "original_form": "맞아요 그거만 아니면 참 좋을텐데요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018129.1.12",
          "form": "전 예전 욘사마? 동방신기 한류줌 초창기에 가서 ㅇ득 많이봣어요..",
          "original_form": "전 예전 욘사마? 동방신기 한류줌 초창기에 가서 ㅇ득 많이봣어요..",
          "speaker_id": "1",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018129.1.13",
          "form": "막 먼저 말걸어주고..",
          "original_form": "막 먼저 말걸어주고..",
          "speaker_id": "1",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018129.1.14",
          "form": "와 신기하네요",
          "original_form": "와 신기하네요",
          "speaker_id": "2",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100018129.1.15",
          "form": "우리나라 아이돌들이 거기서 인기많더라구요",
          "original_form": "우리나라 아이돌들이 거기서 인기많더라구요",
          "speaker_id": "2",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100018129.1.16",
          "form": "방탄도 일본에서 뜨고 동남아 진출한거니까요..",
          "original_form": "방탄도 일본에서 뜨고 동남아 진출한거니까요..",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018129.1.17",
          "form": "그래요? 전 한국에서 사재기로 띄웠다는 기사 많이 봤어요 ㅋㅋㅋㅋ",
          "original_form": "그래요? 전 한국에서 사재기로 띄웠다는 기사 많이 봤어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018129.1.18",
          "form": "한국남자 일단 호감 가져주죠..ㅋㅋㅋ \n특히.. 아줌마들이..",
          "original_form": "한국남자 일단 호감 가져주죠..ㅋㅋㅋ \n특히.. 아줌마들이..",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018129.1.19",
          "form": "아줌마들이 ㅋㅋㅋ 욘사마 생각나네요",
          "original_form": "아줌마들이 ㅋㅋㅋ 욘사마 생각나네요",
          "speaker_id": "2",
          "time": "20210826 23:04"
        }
      ]
