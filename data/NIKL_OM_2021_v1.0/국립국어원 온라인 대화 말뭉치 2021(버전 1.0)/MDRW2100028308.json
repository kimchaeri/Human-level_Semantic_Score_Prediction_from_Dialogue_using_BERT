      "utterance": [
        {
          "id": "MDRW2100028308.1.1",
          "form": "사고싶은 것도 말해보자고 제안.",
          "original_form": "사고싶은 것도 말해보자고 제안.",
          "speaker_id": "1",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100028308.1.2",
          "form": "대략 공짜폰이 아닌 갤럭시 스마트폰",
          "original_form": "대략 공짜폰이 아닌 갤럭시 스마트폰",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100028308.1.3",
          "form": "플립이 아니란 점에서 매우 겸손",
          "original_form": "플립이 아니란 점에서 매우 겸손",
          "speaker_id": "2",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100028308.1.4",
          "form": "전 플립 갖고 싶은데",
          "original_form": "전 플립 갖고 싶은데",
          "speaker_id": "1",
          "time": "20210902 19:00"
        },
        {
          "id": "MDRW2100028308.1.5",
          "form": "제법 오만",
          "original_form": "제법 오만",
          "speaker_id": "1",
          "time": "20210902 19:00"
        },
        {
          "id": "MDRW2100028308.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:00"
        },
        {
          "id": "MDRW2100028308.1.7",
          "form": "갤워치는용?",
          "original_form": "갤워치는용?",
          "speaker_id": "1",
          "time": "20210902 19:01"
        },
        {
          "id": "MDRW2100028308.1.8",
          "form": "보고 나니까...",
          "original_form": "보고 나니까...",
          "speaker_id": "2",
          "time": "20210902 19:26"
        },
        {
          "id": "MDRW2100028308.1.9",
          "form": "사실은 그 정도의 고성능이 필요없단 걸 알게 됐어",
          "original_form": "사실은 그 정도의 고성능이 필요없단 걸 알게 됐어",
          "speaker_id": "2",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100028308.1.10",
          "form": "필요하다면 폰이 우선이란 것도...",
          "original_form": "필요하다면 폰이 우선이란 것도...",
          "speaker_id": "2",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100028308.1.11",
          "form": "돈 모아서 폰부터 사겠어",
          "original_form": "돈 모아서 폰부터 사겠어",
          "speaker_id": "2",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100028308.1.12",
          "form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ앗... 그렇군요",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ앗... 그렇군요",
          "speaker_id": "1",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100028308.1.13",
          "form": "마자요 폰이 우선이지",
          "original_form": "마자요 폰이 우선이지",
          "speaker_id": "1",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100028308.1.14",
          "form": "저도 3년째 갤s9써서",
          "original_form": "저도 3년째 갤s9써서",
          "speaker_id": "1",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100028308.1.15",
          "form": "슬슬 바꾸고 싶다...",
          "original_form": "슬슬 바꾸고 싶다...",
          "speaker_id": "1",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100028308.1.16",
          "form": "바꿀때도 됐다",
          "original_form": "바꿀때도 됐다",
          "speaker_id": "2",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100028308.1.17",
          "form": "전 늘 2년 약정으로 공짜폰 대충 골라써서...",
          "original_form": "전 늘 2년 약정으로 공짜폰 대충 골라써서...",
          "speaker_id": "2",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100028308.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ시험끝나고 바꾸려구요ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ시험끝나고 바꾸려구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100028308.1.19",
          "form": "바꾸자 바꾸자~~ 뭘루 바꿀거야",
          "original_form": "바꾸자 바꾸자~~ 뭘루 바꿀거야",
          "speaker_id": "2",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100028308.1.20",
          "form": "아 공짜폰.. 사실 저도 다음 건 갤럭시a쪽으로 알아보려구 생각중",
          "original_form": "아 공짜폰.. 사실 저도 다음 건 갤럭시a쪽으로 알아보려구 생각중",
          "speaker_id": "1",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100028308.1.21",
          "form": "플립 못 사면... 이제",
          "original_form": "플립 못 사면... 이제",
          "speaker_id": "1",
          "time": "20210902 19:30"
        },
        {
          "id": "MDRW2100028308.1.22",
          "form": "갤럭시 S시리즈는 맘에 드는 것두 없구 비싸기만 한듯요...ㅠ",
          "original_form": "갤럭시 S시리즈는 맘에 드는 것두 없구 비싸기만 한듯요...ㅠ",
          "speaker_id": "1",
          "time": "20210902 19:30"
        },
        {
          "id": "MDRW2100028308.1.23",
          "form": "맞아 비쌈 ㅠ 난 노트 갖고 싶은데.....",
          "original_form": "맞아 비쌈 ㅠ 난 노트 갖고 싶은데.....",
          "speaker_id": "2",
          "time": "20210902 19:43"
        },
        {
          "id": "MDRW2100028308.1.24",
          "form": "사실 작년에 폰 바꾸려니 너무 비싸서 대신 갤럭시탭 삼 고가라인은 아니었지만",
          "original_form": "사실 작년에 폰 바꾸려니 너무 비싸서 대신 갤럭시탭 삼 고가라인은 아니었지만",
          "speaker_id": "2",
          "time": "20210902 19:43"
        },
        {
          "id": "MDRW2100028308.1.25",
          "form": "헉...갤탭 좋나요",
          "original_form": "헉...갤탭 좋나요",
          "speaker_id": "1",
          "time": "20210902 19:45"
        },
        {
          "id": "MDRW2100028308.1.26",
          "form": "저도 패드 갖구싶당",
          "original_form": "저도 패드 갖구싶당",
          "speaker_id": "1",
          "time": "20210902 19:45"
        },
        {
          "id": "MDRW2100028308.1.27",
          "form": "노트 좋대요...근데 노트가 특히 비싸긴 하죠...ㅠ",
          "original_form": "노트 좋대요...근데 노트가 특히 비싸긴 하죠...ㅠ",
          "speaker_id": "1",
          "time": "20210902 19:45"
        },
        {
          "id": "MDRW2100028308.1.28",
          "form": "하나 있으니 좋더라 내 폰 구려서 게임 안 돌아가서 다 탭으로 함ㅋㅋㅋ",
          "original_form": "하나 있으니 좋더라 내 폰 구려서 게임 안 돌아가서 다 탭으로 함ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:46"
        },
        {
          "id": "MDRW2100028308.1.29",
          "form": "오오 역시 요즘 시대에 필수인가요",
          "original_form": "오오 역시 요즘 시대에 필수인가요",
          "speaker_id": "1",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100028308.1.30",
          "form": "플립 사고 싶은데",
          "original_form": "플립 사고 싶은데",
          "speaker_id": "1",
          "time": "20210902 20:08"
        },
        {
          "id": "MDRW2100028308.1.31",
          "form": "플립도 리듬게임 못한다니까",
          "original_form": "플립도 리듬게임 못한다니까",
          "speaker_id": "1",
          "time": "20210902 20:08"
        },
        {
          "id": "MDRW2100028308.1.32",
          "form": "탭 수요는 점점 늘어갈 예감...",
          "original_form": "탭 수요는 점점 늘어갈 예감...",
          "speaker_id": "1",
          "time": "20210902 20:08"
        },
        {
          "id": "MDRW2100028308.1.33",
          "form": "일단 화면이 크니 게임하기 속시원혀",
          "original_form": "일단 화면이 크니 게임하기 속시원혀",
          "speaker_id": "2",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100028308.1.34",
          "form": "s6+는 50만원 안으로 사고...",
          "original_form": "s6+는 50만원 안으로 사고...",
          "speaker_id": "2",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100028308.1.35",
          "form": "오.... 완전 짱이네요",
          "original_form": "오.... 완전 짱이네요",
          "speaker_id": "1",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028308.1.36",
          "form": "사실 전 인강용으로 지를까 고민이었는데",
          "original_form": "사실 전 인강용으로 지를까 고민이었는데",
          "speaker_id": "1",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028308.1.37",
          "form": "국민지원금 보태서 사야하나...",
          "original_form": "국민지원금 보태서 사야하나...",
