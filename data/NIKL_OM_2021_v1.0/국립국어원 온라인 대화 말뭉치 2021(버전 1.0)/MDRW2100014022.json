      "utterance": [
        {
          "id": "MDRW2100014022.1.1",
          "form": "여자친구랑 헤어지고",
          "original_form": "여자친구랑 헤어지고",
          "speaker_id": "1",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100014022.1.2",
          "form": "딱 보증금 반환 된다는 6개월 살고 나왔습니다",
          "original_form": "딱 보증금 반환 된다는 6개월 살고 나왔습니다",
          "speaker_id": "1",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100014022.1.3",
          "form": "그래도 일년 안채워도 돼서 다행이네요",
          "original_form": "그래도 일년 안채워도 돼서 다행이네요",
          "speaker_id": "2",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100014022.1.4",
          "form": "사귀던 분이랑 오래연애하셨었나봐요",
          "original_form": "사귀던 분이랑 오래연애하셨었나봐요",
          "speaker_id": "2",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100014022.1.5",
          "form": "아뇨ㅋㅋ 한 3개월 사귀었는데",
          "original_form": "아뇨ㅋㅋ 한 3개월 사귀었는데",
          "speaker_id": "1",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100014022.1.6",
          "form": "알바하다 만나서 알바하는데랑 가까운데 방 잡아서 살자 해서 했는데",
          "original_form": "알바하다 만나서 알바하는데랑 가까운데 방 잡아서 살자 해서 했는데",
          "speaker_id": "1",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100014022.1.7",
          "form": "헤어지고 그분도 그만두고 나가시고",
          "original_form": "헤어지고 그분도 그만두고 나가시고",
          "speaker_id": "1",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100014022.1.8",
          "form": "...",
          "original_form": "...",
          "speaker_id": "1",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100014022.1.9",
          "form": "아하..일하던곳에서 만나다가 헤어지면 그만두게되죠ㅠ",
          "original_form": "아하..일하던곳에서 만나다가 헤어지면 그만두게되죠ㅠ",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014022.1.10",
          "form": "그렇게 되더라구요",
          "original_form": "그렇게 되더라구요",
          "speaker_id": "1",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100014022.1.11",
          "form": "지금은 연애 쉬고있습니다 그때 이후로",
          "original_form": "지금은 연애 쉬고있습니다 그때 이후로",
          "speaker_id": "1",
          "time": "20210825 17:09"
        },
        {
          "id": "MDRW2100014022.1.12",
          "form": "헤어진지는 얼마나 되셨나요?",
          "original_form": "헤어진지는 얼마나 되셨나요?",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014022.1.13",
          "form": "결혼 생각있으신가요?",
          "original_form": "결혼 생각있으신가요?",
          "speaker_id": "2",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014022.1.14",
          "form": "결혼은 언젠가 할것같은데",
          "original_form": "결혼은 언젠가 할것같은데",
          "speaker_id": "1",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100014022.1.15",
          "form": "너무 막연하네요",
          "original_form": "너무 막연하네요",
          "speaker_id": "1",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100014022.1.16",
          "form": "그쵸 아직 막연하죠",
          "original_form": "그쵸 아직 막연하죠",
          "speaker_id": "2",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014022.1.17",
          "form": "결혼 어떻게 생각하세요?",
          "original_form": "결혼 어떻게 생각하세요?",
          "speaker_id": "1",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014022.1.18",
          "form": "저는 결혼생각은 아직 없는거같아요",
          "original_form": "저는 결혼생각은 아직 없는거같아요",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014022.1.19",
          "form": "결혼해서 별로 좋은게 있어보이진않아서..",
          "original_form": "결혼해서 별로 좋은게 있어보이진않아서..",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014022.1.20",
          "form": "그렇긴 해요",
          "original_form": "그렇긴 해요",
          "speaker_id": "1",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014022.1.21",
          "form": "육아도 있구",
          "original_form": "육아도 있구",
          "speaker_id": "1",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014022.1.22",
          "form": "마자요 육아가 진짜 엄청 큰 산인거같더라구요",
          "original_form": "마자요 육아가 진짜 엄청 큰 산인거같더라구요",
          "speaker_id": "2",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100014022.1.23",
          "form": "부모님들이 도와주시거나 해야 직장생활을 할수있을텐데 도와주시는것도 한계가 있구요",
          "original_form": "부모님들이 도와주시거나 해야 직장생활을 할수있을텐데 도와주시는것도 한계가 있구요",
          "speaker_id": "2",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014022.1.24",
          "form": "맞아요. 독립 자금도 문제고",
          "original_form": "맞아요. 독립 자금도 문제고",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014022.1.25",
          "form": "같이 살면서 맞벌이하면서 애 없이 돈만 모으면 어때요?",
          "original_form": "같이 살면서 맞벌이하면서 애 없이 돈만 모으면 어때요?",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014022.1.26",
          "form": "그럼 나쁘진않을거같아요",
          "original_form": "그럼 나쁘진않을거같아요",
          "speaker_id": "2",
          "time": "20210825 17:38"
        }
      ]
