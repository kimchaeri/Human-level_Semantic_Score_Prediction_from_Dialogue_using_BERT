        {
          "id": "MDRW2100046115.1.2",
          "form": "나 오늘배달시켜먹을거야",
          "original_form": "나 오늘배달시켜먹을거야",
          "speaker_id": "1",
          "time": "20210908 10:28"
        },
        {
          "id": "MDRW2100046115.1.3",
          "form": "뭐 먹을거야",
          "original_form": "뭐 먹을거야",
          "speaker_id": "2",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046115.1.4",
          "form": "치킨시켜먹을거야",
          "original_form": "치킨시켜먹을거야",
          "speaker_id": "1",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046115.1.5",
          "form": "헐 맛있겠다",
          "original_form": "헐 맛있겠다",
          "speaker_id": "2",
          "time": "20210908 10:29"
        },
        {
          "id": "MDRW2100046115.1.6",
          "form": "굽네 좋아해?",
          "original_form": "굽네 좋아해?",
          "speaker_id": "1",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046115.1.7",
          "form": "어 굽네치킨 맛있던데",
          "original_form": "어 굽네치킨 맛있던데",
          "speaker_id": "2",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046115.1.8",
          "form": "맞아 오븐구이가맛있더라",
          "original_form": "맞아 오븐구이가맛있더라",
          "speaker_id": "1",
          "time": "20210908 10:30"
        },
        {
          "id": "MDRW2100046115.1.9",
          "form": "매콤 바사삭이 짱이더라",
          "original_form": "매콤 바사삭이 짱이더라",
          "speaker_id": "2",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046115.1.10",
          "form": "매운거잘먹어?",
          "original_form": "매운거잘먹어?",
          "speaker_id": "1",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046115.1.11",
          "form": "매운거 너후좋아해 너는",
          "original_form": "매운거 너후좋아해 너는",
          "speaker_id": "2",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100046115.1.12",
          "form": "나는 매운떡볶이좋아해",
          "original_form": "나는 매운떡볶이좋아해",
          "speaker_id": "1",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100046115.1.13",
          "form": "얼마나  ㅁㅐ운거 먹어?",
          "original_form": "얼마나  ㅁㅐ운거 먹어?",
          "speaker_id": "2",
          "time": "20210908 10:32"
        },
        {
          "id": "MDRW2100046115.1.14",
          "form": "중간맛정도? 넌?",
          "original_form": "중간맛정도? 넌?",
