      "utterance": [
        {
          "id": "MDRW2100002854.1.1",
          "form": "넌 겨울이 좋아 여름이 좋아?",
          "original_form": "넌 겨울이 좋아 여름이 좋아?",
          "speaker_id": "1",
          "time": "20210820 16:01"
        },
        {
          "id": "MDRW2100002854.1.2",
          "form": "난 둘다 싫어",
          "original_form": "난 둘다 싫어",
          "speaker_id": "2",
          "time": "20210820 16:04"
        },
        {
          "id": "MDRW2100002854.1.3",
          "form": "ㅋㅋ 덥거나 추운거 다 싫구나",
          "original_form": "ㅋㅋ 덥거나 추운거 다 싫구나",
          "speaker_id": "1",
          "time": "20210820 16:05"
        },
        {
          "id": "MDRW2100002854.1.4",
          "form": "나는 에어컨이랑 히터만 빵빵하면 둘 다 괜찮은데",
          "original_form": "나는 에어컨이랑 히터만 빵빵하면 둘 다 괜찮은데",
          "speaker_id": "1",
          "time": "20210820 16:05"
        },
        {
          "id": "MDRW2100002854.1.5",
          "form": "보통 그렇지가 않으니..",
          "original_form": "보통 그렇지가 않으니..",
          "speaker_id": "1",
          "time": "20210820 16:05"
        },
        {
          "id": "MDRW2100002854.1.6",
          "form": "우리집은 히터도 없고 에어컨도 내 건강상때문에 못써서 그런것 같아",
          "original_form": "우리집은 히터도 없고 에어컨도 내 건강상때문에 못써서 그런것 같아",
          "speaker_id": "2",
          "time": "20210820 16:06"
        },
        {
          "id": "MDRW2100002854.1.7",
          "form": "에고 이번여름 힘들었겠다",
          "original_form": "에고 이번여름 힘들었겠다",
          "speaker_id": "1",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002854.1.8",
          "form": "덥지 않았어?",
          "original_form": "덥지 않았어?",
          "speaker_id": "1",
          "time": "20210820 16:25"
        },
        {
          "id": "MDRW2100002854.1.9",
          "form": "이번은 그래도 버틸만했어",
          "original_form": "이번은 그래도 버틸만했어",
          "speaker_id": "2",
          "time": "20210820 16:27"
        },
        {
          "id": "MDRW2100002854.1.10",
          "form": "저번이 진짜 더웠지",
          "original_form": "저번이 진짜 더웠지",
          "speaker_id": "2",
          "time": "20210820 16:27"
        },
        {
          "id": "MDRW2100002854.1.11",
          "form": "버틸만했다니 다행이다",
          "original_form": "버틸만했다니 다행이다",
          "speaker_id": "1",
          "time": "20210820 16:28"
        },
        {
          "id": "MDRW2100002854.1.12",
          "form": "나는 이번 여름에 더워서",
          "original_form": "나는 이번 여름에 더워서",
          "speaker_id": "1",
          "time": "20210820 16:31"
        },
        {
          "id": "MDRW2100002854.1.13",
          "form": "죽어가고 있었어",
          "original_form": "죽어가고 있었어",
          "speaker_id": "1",
          "time": "20210820 16:31"
        },
        {
          "id": "MDRW2100002854.1.14",
          "form": "에어컨 안틀었어??",
          "original_form": "에어컨 안틀었어??",
          "speaker_id": "2",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002854.1.15",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002854.1.16",
          "form": "지금 하숙중인데",
          "original_form": "지금 하숙중인데",
          "speaker_id": "1",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002854.1.17",
          "form": "여기 주인이 에어컨을 안틀어줬어",
          "original_form": "여기 주인이 에어컨을 안틀어줬어",
          "speaker_id": "1",
          "time": "20210820 16:34"
        },
        {
          "id": "MDRW2100002854.1.18",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "2",
          "time": "20210820 16:36"
        },
        {
          "id": "MDRW2100002854.1.19",
          "form": "선풍기는 틀었고?",
          "original_form": "선풍기는 틀었고?",
          "speaker_id": "2",
          "time": "20210820 16:36"
        },
        {
          "id": "MDRW2100002854.1.20",
          "form": "응 다행히 선풍기는 있었어",
          "original_form": "응 다행히 선풍기는 있었어",
          "speaker_id": "1",
          "time": "20210820 16:36"
        },
        {
          "id": "MDRW2100002854.1.21",
          "form": "너는 선풍기 있었어?",
          "original_form": "너는 선풍기 있었어?",
          "speaker_id": "1",
          "time": "20210820 16:38"
        },
        {
          "id": "MDRW2100002854.1.22",
          "form": "응 탁상용이랑 가정용 하나 있었어",
          "original_form": "응 탁상용이랑 가정용 하나 있었어",
          "speaker_id": "2",
          "time": "20210820 16:40"
        },
        {
          "id": "MDRW2100002854.1.23",
          "form": "두개다 돌렸거든",
          "original_form": "두개다 돌렸거든",
          "speaker_id": "2",
          "time": "20210820 16:40"
        },
        {
          "id": "MDRW2100002854.1.24",
          "form": "ㅋㅋㅋㅋ 그거 좋네",
          "original_form": "ㅋㅋㅋㅋ 그거 좋네",
          "speaker_id": "1",
          "time": "20210820 16:40"
        }
      ]
