      "utterance": [
        {
          "id": "MDRW2100024695.1.1",
          "form": "오 식음료",
          "original_form": "오 식음료",
          "speaker_id": "2",
          "time": "20210831 17:20"
        },
        {
          "id": "MDRW2100024695.1.2",
          "form": "음 저녁은 뭐 드실건가요",
          "original_form": "음 저녁은 뭐 드실건가요",
          "speaker_id": "2",
          "time": "20210831 17:21"
        },
        {
          "id": "MDRW2100024695.1.3",
          "form": "그냥 집에 있는 반찬이랑 해서 먹을 것 같아요 ㅎㅎ",
          "original_form": "그냥 집에 있는 반찬이랑 해서 먹을 것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 17:23"
        },
        {
          "id": "MDRW2100024695.1.4",
          "form": "그쵸 저도 뭔가 먹고싶은데 체해서 못먹고있어요ㅜ",
          "original_form": "그쵸 저도 뭔가 먹고싶은데 체해서 못먹고있어요ㅜ",
          "speaker_id": "2",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100024695.1.5",
          "form": "오늘 왕교자 택배 왔다는데 못먹어서 슬프네요",
          "original_form": "오늘 왕교자 택배 왔다는데 못먹어서 슬프네요",
          "speaker_id": "2",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024695.1.6",
          "form": "ㅠㅠ 죽을 사가셔야 할까요?",
          "original_form": "ㅠㅠ 죽을 사가셔야 할까요?",
          "speaker_id": "1",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024695.1.7",
          "form": "어제 사놓은게 있어서 그거로 연명하고 있어요ㅠ",
          "original_form": "어제 사놓은게 있어서 그거로 연명하고 있어요ㅠ",
          "speaker_id": "2",
          "time": "20210831 17:26"
        },
        {
          "id": "MDRW2100024695.1.8",
          "form": "내일은 꼭...먹을수 있길 바라는",
          "original_form": "내일은 꼭...먹을수 있길 바라는",
          "speaker_id": "2",
          "time": "20210831 17:27"
        },
        {
          "id": "MDRW2100024695.1.9",
          "form": "점심엔 뭐드셨어요?",
          "original_form": "점심엔 뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210831 17:27"
        },
        {
          "id": "MDRW2100024695.1.10",
          "form": "비비고 사셨어요?",
          "original_form": "비비고 사셨어요?",
          "speaker_id": "1",
          "time": "20210831 17:27"
        },
        {
          "id": "MDRW2100024695.1.11",
          "form": "점심에는 된장찌개랑 먹었어요",
          "original_form": "점심에는 된장찌개랑 먹었어요",
          "speaker_id": "1",
          "time": "20210831 17:27"
        },
        {
          "id": "MDRW2100024695.1.12",
          "form": "네네 비비고! 그게 제일맛있는거 같아요",
          "original_form": "네네 비비고! 그게 제일맛있는거 같아요",
          "speaker_id": "2",
          "time": "20210831 17:28"
        },
        {
          "id": "MDRW2100024695.1.13",
          "form": "된장찌개 맛있었겠다ㅜ",
          "original_form": "된장찌개 맛있었겠다ㅜ",
          "speaker_id": "2",
          "time": "20210831 17:28"
        },
        {
          "id": "MDRW2100024695.1.14",
          "form": "저는 콩물국수 나왔는데 구경만했네요",
          "original_form": "저는 콩물국수 나왔는데 구경만했네요",
          "speaker_id": "2",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024695.1.15",
          "form": "점심 아예 못 드셨어요?",
          "original_form": "점심 아예 못 드셨어요?",
          "speaker_id": "1",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024695.1.16",
          "form": "죽먹었어여 흑..흑",
          "original_form": "죽먹었어여 흑..흑",
          "speaker_id": "2",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024695.1.17",
          "form": "죽 너무 물려요",
          "original_form": "죽 너무 물려요",
          "speaker_id": "2",
          "time": "20210831 17:29"
        },
        {
          "id": "MDRW2100024695.1.18",
          "form": "집에 깻잎김치 사둔거 있는데 그게 너무 먹고싶네요",
          "original_form": "집에 깻잎김치 사둔거 있는데 그게 너무 먹고싶네요",
          "speaker_id": "2",
          "time": "20210831 17:30"
        },
        {
          "id": "MDRW2100024695.1.19",
          "form": "본의아니게 다이어트 효과가ㅠ",
          "original_form": "본의아니게 다이어트 효과가ㅠ",
          "speaker_id": "1",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024695.1.20",
          "form": "전에 한동안 죽만 먹다 보니까 다시 일반식 먹을 때 입맛이 살아나더라구요 ㅎㅎ 냄새도 더 잘 맡게 되고",
          "original_form": "전에 한동안 죽만 먹다 보니까 다시 일반식 먹을 때 입맛이 살아나더라구요 ㅎㅎ 냄새도 더 잘 맡게 되고",
          "speaker_id": "1",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024695.1.21",
          "form": "그니깐요",
          "original_form": "그니깐요",
          "speaker_id": "2",
          "time": "20210831 17:52"
        },
        {
          "id": "MDRW2100024695.1.22",
          "form": "ㅋ큰일이에요 입맛 더 땡겨요",
          "original_form": "ㅋ큰일이에요 입맛 더 땡겨요",
          "speaker_id": "2",
          "time": "20210831 17:52"
        },
        {
          "id": "MDRW2100024695.1.23",
          "form": "미친듯이 먹다가 또 체할까봐",
          "original_form": "미친듯이 먹다가 또 체할까봐",
          "speaker_id": "2",
          "time": "20210831 17:52"
        },
        {
          "id": "MDRW2100024695.1.24",
          "form": "ㅋㅋ 다 낫고 나면 하나하나 천천히 드시는 걸로",
          "original_form": "ㅋㅋ 다 낫고 나면 하나하나 천천히 드시는 걸로",
          "speaker_id": "1",
          "time": "20210831 17:53"
        },
        {
          "id": "MDRW2100024695.1.25",
          "form": "곧 퇴근이네요!!!!!!!!!",
          "original_form": "곧 퇴근이네요!!!!!!!!!",
          "speaker_id": "2",
          "time": "20210831 17:59"
        },
        {
          "id": "MDRW2100024695.1.26",
          "form": "오늘은 죽과 함께지만.. 그래도 즐거운 저녁식사 되시기를",
          "original_form": "오늘은 죽과 함께지만.. 그래도 즐거운 저녁식사 되시기를",
          "speaker_id": "1",
          "time": "20210831 18:04"
        },
        {
          "id": "MDRW2100024695.1.27",
          "form": "곧 점심시간이네요",
          "original_form": "곧 점심시간이네요",
