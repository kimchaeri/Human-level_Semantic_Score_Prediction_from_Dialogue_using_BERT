        {
          "id": "MDRW2100005538.1.1",
          "form": "안녕하세요^^",
          "original_form": "안녕하세요^^",
          "speaker_id": "1",
          "time": "20210821 22:54"
        },
        {
          "id": "MDRW2100005538.1.2",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "2",
          "time": "20210821 22:56"
        },
        {
          "id": "MDRW2100005538.1.3",
          "form": "요",
          "original_form": "요",
          "speaker_id": "2",
          "time": "20210821 22:56"
        },
        {
          "id": "MDRW2100005538.1.4",
          "form": "요즘에 재밌게 읽으신 책이 있으세요?ㅎㅎ",
          "original_form": "요즘에 재밌게 읽으신 책이 있으세요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 22:56"
        },
        {
          "id": "MDRW2100005538.1.5",
          "form": "나는 어쩌다 명왕성을 죽였나",
          "original_form": "나는 어쩌다 명왕성을 죽였나",
          "speaker_id": "2",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005538.1.6",
          "form": "재밌게 읽었어요",
          "original_form": "재밌게 읽었어요",
          "speaker_id": "2",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005538.1.7",
          "form": "책 제목을 처음 들어봐서 그런데",
          "original_form": "책 제목을 처음 들어봐서 그런데",
          "speaker_id": "1",
          "time": "20210821 23:01"
        },
        {
          "id": "MDRW2100005538.1.8",
          "form": "책 내용이 무슨 내용인가요?",
          "original_form": "책 내용이 무슨 내용인가요?",
          "speaker_id": "1",
          "time": "20210821 23:01"
        },
        {
          "id": "MDRW2100005538.1.9",
          "form": "자연과학 책인데 명왕성이 태양계에서 빠지게 되는데 큰 역할을 하게 된 과학자의 얘기에요",
          "original_form": "자연과학 책인데 명왕성이 태양계에서 빠지게 되는데 큰 역할을 하게 된 과학자의 얘기에요",
          "speaker_id": "2",
          "time": "20210821 23:03"
        },
        {
          "id": "MDRW2100005538.1.10",
          "form": "어렵지 않고 다큐멘터리 보는 거 같아서 재밌었어요",
          "original_form": "어렵지 않고 다큐멘터리 보는 거 같아서 재밌었어요",
          "speaker_id": "2",
          "time": "20210821 23:03"
        },
        {
          "id": "MDRW2100005538.1.11",
          "form": "저는 제목만보고 소설인줄알았는데",
          "original_form": "저는 제목만보고 소설인줄알았는데",
          "speaker_id": "1",
          "time": "20210821 23:04"
        },
        {
          "id": "MDRW2100005538.1.12",
          "form": "과학 내용의 책이었네요",
          "original_form": "과학 내용의 책이었네요",
          "speaker_id": "1",
          "time": "20210821 23:04"
        },
        {
          "id": "MDRW2100005538.1.13",
          "form": "다큐멘터리 같으면 재밌을것같아요!",
          "original_form": "다큐멘터리 같으면 재밌을것같아요!",
          "speaker_id": "1",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005538.1.14",
          "form": "진짜 과학 하나도 몰라도 어렵지 않고 작가가 좀 유쾌해서 술술 읽히는 책이었어요",
          "original_form": "진짜 과학 하나도 몰라도 어렵지 않고 작가가 좀 유쾌해서 술술 읽히는 책이었어요",
          "speaker_id": "2",
          "time": "20210821 23:06"
        },
        {
          "id": "MDRW2100005538.1.15",
          "form": "도서관에서 보이면 한 번 읽어보세요 꽤 재밌어요",
          "original_form": "도서관에서 보이면 한 번 읽어보세요 꽤 재밌어요",
          "speaker_id": "2",
          "time": "20210821 23:07"
        },
        {
          "id": "MDRW2100005538.1.16",
          "form": "도서관에 한 번 가봐야겠어요",
          "original_form": "도서관에 한 번 가봐야겠어요",
          "speaker_id": "1",
          "time": "20210821 23:07"
        },
        {
          "id": "MDRW2100005538.1.17",
          "form": "저번에 갔다가 휴관일이라서 그냥 왔거든요",
          "original_form": "저번에 갔다가 휴관일이라서 그냥 왔거든요",
          "speaker_id": "1",
          "time": "20210821 23:08"
        },
        {
          "id": "MDRW2100005538.1.18",
          "form": "name1 님은 어떤 책 주로 읽으세요?",
          "original_form": "&name1& 님은 어떤 책 주로 읽으세요?",
          "speaker_id": "2",
          "time": "20210821 23:08"
        },
        {
          "id": "MDRW2100005538.1.19",
          "form": "저는 소설 위주로 읽는데",
          "original_form": "저는 소설 위주로 읽는데",
          "speaker_id": "1",
          "time": "20210821 23:08"
        },
        {
          "id": "MDRW2100005538.1.20",
          "form": "요즘엔 고전 소설을 많이 보는것같아요",
          "original_form": "요즘엔 고전 소설을 많이 보는것같아요",
          "speaker_id": "1",
          "time": "20210821 23:08"
        },
        {
          "id": "MDRW2100005538.1.21",
          "form": "고전 소설 추천해주실 수 있나요?",
          "original_form": "고전 소설 추천해주실 수 있나요?",
          "speaker_id": "2",
          "time": "20210821 23:09"
        },
        {
          "id": "MDRW2100005538.1.22",
          "form": "요새 영화 오필리아를 보고",
          "original_form": "요새 영화 오필리아를 보고",
          "speaker_id": "1",
          "time": "20210821 23:10"
        },
        {
          "id": "MDRW2100005538.1.23",
          "form": "원작도 궁금해서 햄릿도 읽었는데",
          "original_form": "원작도 궁금해서 햄릿도 읽었는데",
          "speaker_id": "1",
          "time": "20210821 23:10"
        },
        {
          "id": "MDRW2100005538.1.24",
          "form": "영화랑 다른 점을 보면서 읽으니까 재밌었어요",
          "original_form": "영화랑 다른 점을 보면서 읽으니까 재밌었어요",
          "speaker_id": "1",
          "time": "20210821 23:10"
        },
        {
          "id": "MDRW2100005538.1.25",
          "form": "햄릿 흥미롭죠 영화는 안 봤는데 한 번 찾아볼게요",
          "original_form": "햄릿 흥미롭죠 영화는 안 봤는데 한 번 찾아볼게요",
          "speaker_id": "2",
          "time": "20210821 23:13"
        },
        {
          "id": "MDRW2100005538.1.26",
          "form": "감사해요",
          "original_form": "감사해요",
          "speaker_id": "2",
          "time": "20210821 23:13"
        }
      ]
    }
