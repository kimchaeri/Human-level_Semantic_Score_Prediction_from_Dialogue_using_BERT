      "utterance": [
        {
          "id": "MDRW2100036452.1.1",
          "form": "한번드셔보세요 나중에 기회되면 ㅎㅎ",
          "original_form": "한번드셔보세요 나중에 기회되면 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100036452.1.2",
          "form": "네네 벌써부터 위가 두큰거려요!!",
          "original_form": "네네 벌써부터 위가 두큰거려요!!",
          "speaker_id": "1",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100036452.1.3",
          "form": "ㅋㅋㅋㅋ 이런 ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋ 이런 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100036452.1.4",
          "form": "아침드셨나요??",
          "original_form": "아침드셨나요??",
          "speaker_id": "2",
          "time": "20210831 08:29"
        },
        {
          "id": "MDRW2100036452.1.5",
          "form": "아침은 평소처럼 스킵했죠ㅎ",
          "original_form": "아침은 평소처럼 스킵했죠ㅎ",
          "speaker_id": "1",
          "time": "20210831 08:32"
        },
        {
          "id": "MDRW2100036452.1.6",
          "form": "야식을 늦게 먹어 배는 안고프네요",
          "original_form": "야식을 늦게 먹어 배는 안고프네요",
          "speaker_id": "1",
          "time": "20210831 08:33"
        },
        {
          "id": "MDRW2100036452.1.7",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210831 08:33"
        },
        {
          "id": "MDRW2100036452.1.8",
          "form": "저도 아직인데 배고플것 같아서 식빵에 잼발라 먹을까 하고 있어요",
          "original_form": "저도 아직인데 배고플것 같아서 식빵에 잼발라 먹을까 하고 있어요",
          "speaker_id": "2",
          "time": "20210831 08:38"
        },
        {
          "id": "MDRW2100036452.1.9",
          "form": "무슨 잼인가요?",
          "original_form": "무슨 잼인가요?",
          "speaker_id": "1",
          "time": "20210831 08:41"
        },
        {
          "id": "MDRW2100036452.1.10",
          "form": "딸기요 ㅎ",
          "original_form": "딸기요 ㅎ",
          "speaker_id": "2",
          "time": "20210831 08:42"
        },
        {
          "id": "MDRW2100036452.1.11",
          "form": "ㅎㅎ 딸기잼 땅콩잼 듬뿍바른 샌드위치 급 당기네요",
          "original_form": "ㅎㅎ 딸기잼 땅콩잼 듬뿍바른 샌드위치 급 당기네요",
          "speaker_id": "1",
          "time": "20210831 08:45"
        },
        {
          "id": "MDRW2100036452.1.12",
          "form": "배도 안 고팠는데",
          "original_form": "배도 안 고팠는데",
          "speaker_id": "1",
          "time": "20210831 08:45"
        },
        {
          "id": "MDRW2100036452.1.13",
          "form": "아아아아아 맛있겠다 땅콩잼 좋아하는데 ㅎ없네요",
          "original_form": "아아아아아 맛있겠다 땅콩잼 좋아하는데 ㅎ없네요",
          "speaker_id": "2",
          "time": "20210831 08:46"
        },
        {
          "id": "MDRW2100036452.1.14",
          "form": "우리 집에는 셋다 없어요ㅜㅇㅜ",
          "original_form": "우리 집에는 셋다 없어요ㅜㅇㅜ",
          "speaker_id": "1",
          "time": "20210831 08:46"
        },
        {
          "id": "MDRW2100036452.1.15",
          "form": "식빵도 땅콩잼도 딸기잼도",
          "original_form": "식빵도 땅콩잼도 딸기잼도",
          "speaker_id": "1",
          "time": "20210831 08:46"
        },
        {
          "id": "MDRW2100036452.1.16",
          "form": "이런 아예불가능한 메뉴네요ㅠㅠ",
          "original_form": "이런 아예불가능한 메뉴네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 08:46"
        },
        {
          "id": "MDRW2100036452.1.17",
          "form": "왜 제 위장에 불을 지르셨나요ㅜㅇㅜ",
          "original_form": "왜 제 위장에 불을 지르셨나요ㅜㅇㅜ",
          "speaker_id": "1",
          "time": "20210831 08:47"
        },
        {
          "id": "MDRW2100036452.1.18",
          "form": "화르륵",
          "original_form": "화르륵",
          "speaker_id": "2",
          "time": "20210831 08:47"
        },
        {
          "id": "MDRW2100036452.1.19",
          "form": "자 움직이세요 뭐라고 먹게",
          "original_form": "자 움직이세요 뭐라고 먹게",
