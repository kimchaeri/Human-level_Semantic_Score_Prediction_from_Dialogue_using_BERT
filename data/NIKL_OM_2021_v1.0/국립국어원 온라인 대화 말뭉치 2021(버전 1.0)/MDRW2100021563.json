      "utterance": [
        {
          "id": "MDRW2100021563.1.1",
          "form": "아이돌 공연...?",
          "original_form": "아이돌 공연...?",
          "speaker_id": "2",
          "time": "20210828 20:41"
        },
        {
          "id": "MDRW2100021563.1.2",
          "form": "뮤지컬 위주로 보시는거죠??",
          "original_form": "뮤지컬 위주로 보시는거죠??",
          "speaker_id": "1",
          "time": "20210828 20:51"
        },
        {
          "id": "MDRW2100021563.1.3",
          "form": "뮤지컬도 보고 콘서트도 보고",
          "original_form": "뮤지컬도 보고 콘서트도 보고",
          "speaker_id": "2",
          "time": "20210828 20:51"
        },
        {
          "id": "MDRW2100021563.1.4",
          "form": "오케스트라 공연도 좋아해요!",
          "original_form": "오케스트라 공연도 좋아해요!",
          "speaker_id": "2",
          "time": "20210828 20:51"
        },
        {
          "id": "MDRW2100021563.1.5",
          "form": "완전 문화인이시네요ㅜㅜ",
          "original_form": "완전 문화인이시네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 22:08"
        },
        {
          "id": "MDRW2100021563.1.6",
          "form": "추천해주실만한 공연 있나요?",
          "original_form": "추천해주실만한 공연 있나요?",
          "speaker_id": "1",
          "time": "20210828 22:08"
        },
        {
          "id": "MDRW2100021563.1.7",
          "form": "음 클래식 공연 괜찮으세요?? ㅋㅋ",
          "original_form": "음 클래식 공연 괜찮으세요?? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021563.1.8",
          "form": "네네 클래식 좋아요!",
          "original_form": "네네 클래식 좋아요!",
          "speaker_id": "1",
          "time": "20210828 22:12"
        },
        {
          "id": "MDRW2100021563.1.9",
          "form": "저는 조성진이랑 손열음 좋아해요 ㅋㅋ",
          "original_form": "저는 조성진이랑 손열음 좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:12"
        },
        {
          "id": "MDRW2100021563.1.10",
          "form": "조성진 마흐라니노프 2018년 12월 실황 추천이용",
          "original_form": "조성진 마흐라니노프 2018년 12월 실황 추천이용",
          "speaker_id": "2",
          "time": "20210828 22:12"
        },
        {
          "id": "MDRW2100021563.1.11",
          "form": "오 조성진은 아는데 손열음은 처음 들어봐요",
          "original_form": "오 조성진은 아는데 손열음은 처음 들어봐요",
          "speaker_id": "1",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100021563.1.12",
          "form": "유튜브에 한번 찾아봐야겠어요",
          "original_form": "유튜브에 한번 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210828 22:14"
        },
        {
          "id": "MDRW2100021563.1.13",
          "form": "ㅋㅋ 손열음은 유튭도 하구 있어요",
          "original_form": "ㅋㅋ 손열음은 유튭도 하구 있어요",
          "speaker_id": "2",
          "time": "20210828 22:14"
        },
        {
          "id": "MDRW2100021563.1.14",
          "form": "우아한 유령 좋아요 곡 짧구",
          "original_form": "우아한 유령 좋아요 곡 짧구",
          "speaker_id": "2",
          "time": "20210828 22:14"
        },
        {
          "id": "MDRW2100021563.1.15",
          "form": "좀 가볍게 듣기 좋아요",
          "original_form": "좀 가볍게 듣기 좋아요",
          "speaker_id": "2",
          "time": "20210828 22:14"
        },
        {
          "id": "MDRW2100021563.1.16",
          "form": "오 조성진 추천해주신거 검색했는데 예전에 본거같아요ㅎㅎㅎ",
          "original_form": "오 조성진 추천해주신거 검색했는데 예전에 본거같아요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100021563.1.17",
          "form": "조회수가 높네요",
          "original_form": "조회수가 높네요",
          "speaker_id": "1",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100021563.1.18",
          "form": "그 중 몇백은 제가 높인 것 같아요 ㅋㅋㅋㅋ",
          "original_form": "그 중 몇백은 제가 높인 것 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:17"
        },
        {
          "id": "MDRW2100021563.1.19",
          "form": "조성진이 해외에서도 유명해가지구...",
          "original_form": "조성진이 해외에서도 유명해가지구...",
          "speaker_id": "2",
          "time": "20210828 22:17"
        },
        {
          "id": "MDRW2100021563.1.20",
          "form": "모차르트 미공개 악보 초연도 이번에 조성진이 쳤어요 ㅋㅋㅋ",
          "original_form": "모차르트 미공개 악보 초연도 이번에 조성진이 쳤어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021563.1.21",
          "form": "가끔 잠안올때 틀어놓고 들으면 너무 좋더라구여",
          "original_form": "가끔 잠안올때 틀어놓고 들으면 너무 좋더라구여",
          "speaker_id": "1",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021563.1.22",
          "form": "직접 가서 보면 너무 좋을거같아요",
          "original_form": "직접 가서 보면 너무 좋을거같아요",
          "speaker_id": "1",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021563.1.23",
          "form": "네.. 저 공연 실제로 몇 번 보러갔는데 진짜 넘 잘하고 좋더라구요ㅠㅠㅠㅠ",
          "original_form": "네.. 저 공연 실제로 몇 번 보러갔는데 진짜 넘 잘하고 좋더라구요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 22:20"
        },
        {
          "id": "MDRW2100021563.1.24",
          "form": "부러워요ㅠㅠ!!",
          "original_form": "부러워요ㅠㅠ!!",
          "speaker_id": "1",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021563.1.25",
          "form": "근데 넘 오래돼서 기억은 희미한ㅋㅋㅋㅋ",
          "original_form": "근데 넘 오래돼서 기억은 희미한ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:23"
        },
        {
          "id": "MDRW2100021563.1.26",
          "form": "공연이 진짜 실제 보는거랑 영상이랑 너무 다르더라구요",
          "original_form": "공연이 진짜 실제 보는거랑 영상이랑 너무 다르더라구요",
          "speaker_id": "2",
          "time": "20210828 22:23"
        }
      ]
    }
