      "utterance": [
        {
          "id": "MDRW2100032836.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 매운닭튀김볶음이 정답이라구요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 매운닭튀김볶음이 정답이라구요?",
          "speaker_id": "1",
          "time": "20210906 00:52"
        },
        {
          "id": "MDRW2100032836.1.2",
          "form": "양념통닭을 풀어서 설명하면",
          "original_form": "양념통닭을 풀어서 설명하면",
          "speaker_id": "2",
          "time": "20210906 00:52"
        },
        {
          "id": "MDRW2100032836.1.3",
          "form": "저것도 답이지 싶어서",
          "original_form": "저것도 답이지 싶어서",
          "speaker_id": "2",
          "time": "20210906 00:52"
        },
        {
          "id": "MDRW2100032836.1.4",
          "form": "북한이냐고",
          "original_form": "북한이냐고",
          "speaker_id": "1",
          "time": "20210906 00:53"
        },
        {
          "id": "MDRW2100032836.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 00:53"
        },
        {
          "id": "MDRW2100032836.1.6",
          "form": "동무는 매운닭튀김볶음를 드셨구랴",
          "original_form": "동무는 매운닭튀김볶음를 드셨구랴",
          "speaker_id": "1",
          "time": "20210906 00:53"
        },
        {
          "id": "MDRW2100032836.1.7",
          "form": "내는 얼음보숭이를 먹고싶구랴",
          "original_form": "내는 얼음보숭이를 먹고싶구랴",
          "speaker_id": "1",
          "time": "20210906 00:53"
        },
        {
          "id": "MDRW2100032836.1.8",
          "form": "참나 북한 애니메이션 다음으로",
          "original_form": "참나 북한 애니메이션 다음으로",
          "speaker_id": "2",
          "time": "20210906 00:53"
        },
        {
          "id": "MDRW2100032836.1.9",
          "form": "웃긴다",
          "original_form": "웃긴다",
          "speaker_id": "2",
          "time": "20210906 00:53"
        },
        {
          "id": "MDRW2100032836.1.10",
          "form": "그러면 이번에는 니가 내 점심을 맞춰봐",
          "original_form": "그러면 이번에는 니가 내 점심을 맞춰봐",
          "speaker_id": "1",
          "time": "20210906 00:54"
        },
        {
          "id": "MDRW2100032836.1.11",
          "form": "그래좋아 매운거 먹었니?",
          "original_form": "그래좋아 매운거 먹었니?",
          "speaker_id": "2",
          "time": "20210906 00:55"
        },
        {
          "id": "MDRW2100032836.1.12",
          "form": "아니 전혀 맵지 않은 음식이야",
          "original_form": "아니 전혀 맵지 않은 음식이야",
          "speaker_id": "1",
          "time": "20210906 00:55"
        },
        {
          "id": "MDRW2100032836.1.13",
          "form": "볶음밥이니??",
          "original_form": "볶음밥이니??",
          "speaker_id": "2",
          "time": "20210906 00:56"
        },
        {
          "id": "MDRW2100032836.1.14",
          "form": "아니 쌀을 볶는 음식이 아지야",
          "original_form": "아니 쌀을 볶는 음식이 아지야",
          "speaker_id": "1",
          "time": "20210906 00:56"
        },
        {
          "id": "MDRW2100032836.1.15",
          "form": "아니야",
          "original_form": "아니야",
          "speaker_id": "1",
          "time": "20210906 00:56"
        },
        {
          "id": "MDRW2100032836.1.16",
          "form": "다른걸 볶니?",
          "original_form": "다른걸 볶니?",
          "speaker_id": "2",
          "time": "20210906 00:57"
        },
        {
          "id": "MDRW2100032836.1.17",
          "form": "헉 맞아 예리하구나 다른 식재료를 볶아",
          "original_form": "헉 맞아 예리하구나 다른 식재료를 볶아",
          "speaker_id": "1",
          "time": "20210906 00:57"
        },
        {
          "id": "MDRW2100032836.1.18",
          "form": "서얼마설마",
          "original_form": "서얼마설마",
          "speaker_id": "2",
          "time": "20210906 00:58"
        },
        {
          "id": "MDRW2100032836.1.19",
          "form": "토달볶...?",
          "original_form": "토달볶...?",
          "speaker_id": "2",
          "time": "20210906 00:58"
        },
        {
          "id": "MDRW2100032836.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ네 머리에 내가 토달볶에 진심인 사람으로 각인이 됐구나",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ네 머리에 내가 토달볶에 진심인 사람으로 각인이 됐구나",
          "speaker_id": "1",
          "time": "20210906 00:58"
        },
        {
          "id": "MDRW2100032836.1.21",
          "form": "맞지만 점심으론 안 먹었어!!!!",
          "original_form": "맞지만 점심으론 안 먹었어!!!!",
          "speaker_id": "1",
          "time": "20210906 00:59"
        },
        {
          "id": "MDRW2100032836.1.22",
          "form": "쒸익",
          "original_form": "쒸익",
          "speaker_id": "1",
          "time": "20210906 00:59"
        },
        {
          "id": "MDRW2100032836.1.23",
          "form": "점심메뉴는 토달볶 아니었어",
          "original_form": "점심메뉴는 토달볶 아니었어",
          "speaker_id": "1",
          "time": "20210906 00:59"
        },
        {
          "id": "MDRW2100032836.1.24",
          "form": "진심인데 진심이 아니네",
          "original_form": "진심인데 진심이 아니네",
          "speaker_id": "2",
          "time": "20210906 00:59"
        },
        {
          "id": "MDRW2100032836.1.25",
          "form": "그제 그런것이여",
          "original_form": "그제 그런것이여",
          "speaker_id": "1",
          "time": "20210906 00:59"
        },
        {
          "id": "MDRW2100032836.1.26",
          "form": "니가 말한 김에 내일 또 먹어야겠다",
          "original_form": "니가 말한 김에 내일 또 먹어야겠다",
          "speaker_id": "1",
          "time": "20210906 01:00"
        },
        {
          "id": "MDRW2100032836.1.27",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 01:00"
        },
        {
          "id": "MDRW2100032836.1.28",
          "form": "아니다 요즘 토마토 값 비싸니까 과일로 아껴먹을래",
          "original_form": "아니다 요즘 토마토 값 비싸니까 과일로 아껴먹을래",
