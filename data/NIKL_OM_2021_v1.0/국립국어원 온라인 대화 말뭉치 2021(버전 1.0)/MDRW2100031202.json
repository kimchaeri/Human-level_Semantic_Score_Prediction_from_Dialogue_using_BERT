      "utterance": [
        {
          "id": "MDRW2100031202.1.1",
          "form": "네넹ㅜㅜ 피부과는 처음이라서 놀랐어요",
          "original_form": "네넹ㅜㅜ 피부과는 처음이라서 놀랐어요",
          "speaker_id": "2",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100031202.1.2",
          "form": "저도 피부과 잠깐 다녔는데 코로나땜에 안간지 오래됐어요",
          "original_form": "저도 피부과 잠깐 다녔는데 코로나땜에 안간지 오래됐어요",
          "speaker_id": "1",
          "time": "20210904 18:09"
        },
        {
          "id": "MDRW2100031202.1.3",
          "form": "name1님은 어떤 일로 다니셨어요?",
          "original_form": "&name1&님은 어떤 일로 다니셨어요?",
          "speaker_id": "2",
          "time": "20210904 18:12"
        },
        {
          "id": "MDRW2100031202.1.4",
          "form": "저는 여드름 때문에요",
          "original_form": "저는 여드름 때문에요",
          "speaker_id": "1",
          "time": "20210904 18:12"
        },
        {
          "id": "MDRW2100031202.1.5",
          "form": "여드름 관리받았어요",
          "original_form": "여드름 관리받았어요",
          "speaker_id": "1",
          "time": "20210904 18:12"
        },
        {
          "id": "MDRW2100031202.1.6",
          "form": "성인되서도 계속 나더라구요ㅜ",
          "original_form": "성인되서도 계속 나더라구요ㅜ",
          "speaker_id": "1",
          "time": "20210904 18:13"
        },
        {
          "id": "MDRW2100031202.1.7",
          "form": "아, 여드름 짜증나죠ㅜㅜ",
          "original_form": "아, 여드름 짜증나죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 18:14"
        },
        {
          "id": "MDRW2100031202.1.8",
          "form": "그쵸 여드름자국도 넘 심해서",
          "original_form": "그쵸 여드름자국도 넘 심해서",
          "speaker_id": "1",
          "time": "20210904 18:14"
        },
        {
          "id": "MDRW2100031202.1.9",
          "form": "프락셀 한번 받았는데",
          "original_form": "프락셀 한번 받았는데",
          "speaker_id": "1",
          "time": "20210904 18:14"
        },
        {
          "id": "MDRW2100031202.1.10",
          "form": "한번 가지고는 티가 안나네요 ᆢ",
          "original_form": "한번 가지고는 티가 안나네요 ᆢ",
          "speaker_id": "1",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100031202.1.11",
          "form": "프락셀은 어떤 거예요??",
          "original_form": "프락셀은 어떤 거예요??",
          "speaker_id": "2",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100031202.1.12",
          "form": "여드름 자국 없애는거예요",
          "original_form": "여드름 자국 없애는거예요",
          "speaker_id": "1",
          "time": "20210904 18:16"
        },
        {
          "id": "MDRW2100031202.1.13",
          "form": "상처에 자극을줘서",
          "original_form": "상처에 자극을줘서",
          "speaker_id": "1",
          "time": "20210904 18:16"
        },
        {
          "id": "MDRW2100031202.1.14",
          "form": "다시 새살이 차오르게 만드는거요",
          "original_form": "다시 새살이 차오르게 만드는거요",
          "speaker_id": "1",
          "time": "20210904 18:16"
        },
        {
          "id": "MDRW2100031202.1.15",
          "form": "아하, 그런 시술이 있군요",
          "original_form": "아하, 그런 시술이 있군요",
          "speaker_id": "2",
          "time": "20210904 18:17"
        },
        {
          "id": "MDRW2100031202.1.16",
          "form": "신기하다",
          "original_form": "신기하다",
          "speaker_id": "2",
          "time": "20210904 18:17"
        },
        {
          "id": "MDRW2100031202.1.17",
          "form": "한 세번은해야 효과가 보인데요",
          "original_form": "한 세번은해야 효과가 보인데요",
          "speaker_id": "1",
          "time": "20210904 18:17"
        },
        {
          "id": "MDRW2100031202.1.18",
          "form": "근데 한번하고 코로나땜에 못갔어요",
          "original_form": "근데 한번하고 코로나땜에 못갔어요",
          "speaker_id": "1",
          "time": "20210904 18:17"
        },
        {
          "id": "MDRW2100031202.1.19",
          "form": "헉...ㅜㅜ 몹쓸 코로나 같으니라고",
          "original_form": "헉...ㅜㅜ 몹쓸 코로나 같으니라고",
          "speaker_id": "2",
          "time": "20210904 18:22"
        },
        {
          "id": "MDRW2100031202.1.20",
          "form": "진짜 코로나만 없었으면 세번 했을텐데ㅜㅜㅜ",
          "original_form": "진짜 코로나만 없었으면 세번 했을텐데ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 18:23"
        },
        {
          "id": "MDRW2100031202.1.21",
          "form": "아쉬워요",
          "original_form": "아쉬워요",
          "speaker_id": "1",
          "time": "20210904 18:23"
        },
        {
          "id": "MDRW2100031202.1.22",
          "form": "코로나 땜에 여행도 못가고 답답해요",
          "original_form": "코로나 땜에 여행도 못가고 답답해요",
          "speaker_id": "2",
          "time": "20210904 18:24"
        },
        {
          "id": "MDRW2100031202.1.23",
          "form": "이제 코로나가 없던 시절로는 못 돌아간다잖아요",
          "original_form": "이제 코로나가 없던 시절로는 못 돌아간다잖아요",
          "speaker_id": "2",
          "time": "20210904 18:25"
        }
      ]
    }
