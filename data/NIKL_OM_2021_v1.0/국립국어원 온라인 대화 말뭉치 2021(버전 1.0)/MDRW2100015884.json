        {
          "id": "MDRW2100015884.1.3",
          "form": "올림픽 봤어?",
          "original_form": "올림픽 봤어?",
          "speaker_id": "2",
          "time": "20210826 04:28"
        },
        {
          "id": "MDRW2100015884.1.4",
          "form": "이번 도쿄 올림픽 너무 재밌었어",
          "original_form": "이번 도쿄 올림픽 너무 재밌었어",
          "speaker_id": "1",
          "time": "20210826 04:28"
        },
        {
          "id": "MDRW2100015884.1.5",
          "form": "무슨종목 기억나?",
          "original_form": "무슨종목 기억나?",
          "speaker_id": "2",
          "time": "20210826 04:29"
        },
        {
          "id": "MDRW2100015884.1.6",
          "form": "양궁이 바로 결과가 나오니까 재밌게 봤어",
          "original_form": "양궁이 바로 결과가 나오니까 재밌게 봤어",
          "speaker_id": "1",
          "time": "20210826 04:29"
        },
        {
          "id": "MDRW2100015884.1.7",
          "form": "양궁 이번에 금메달만 4갠가?",
          "original_form": "양궁 이번에 금메달만 4갠가?",
          "speaker_id": "2",
          "time": "20210826 04:30"
        },
        {
          "id": "MDRW2100015884.1.8",
          "form": "맞아 진짜 대단하다",
          "original_form": "맞아 진짜 대단하다",
          "speaker_id": "1",
          "time": "20210826 04:30"
        },
        {
          "id": "MDRW2100015884.1.9",
          "form": "혹시 체조도 봤어?",
          "original_form": "혹시 체조도 봤어?",
          "speaker_id": "2",
          "time": "20210826 04:30"
        },
        {
          "id": "MDRW2100015884.1.10",
          "form": "아 도마 경기 봤지 신재환 선수랑",
          "original_form": "아 도마 경기 봤지 신재환 선수랑",
          "speaker_id": "1",
          "time": "20210826 04:31"
        },
        {
          "id": "MDRW2100015884.1.11",
          "form": "여서정선수랑",
          "original_form": "여서정선수랑",
          "speaker_id": "1",
          "time": "20210826 04:31"
        },
        {
          "id": "MDRW2100015884.1.12",
          "form": "맞아! 신재환 선수 진짜 안정적이더라",
          "original_form": "맞아! 신재환 선수 진짜 안정적이더라",
          "speaker_id": "2",
          "time": "20210826 04:31"
        },
        {
          "id": "MDRW2100015884.1.13",
          "form": "펜싱도 금메달 이였지",
          "original_form": "펜싱도 금메달 이였지",
          "speaker_id": "1",
          "time": "20210826 04:32"
        },
        {
          "id": "MDRW2100015884.1.14",
          "form": "펜싱 선수들 잘생기기까지했던데",
          "original_form": "펜싱 선수들 잘생기기까지했던데",
          "speaker_id": "2",
          "time": "20210826 04:32"
        },
        {
          "id": "MDRW2100015884.1.15",
          "form": "정말 실력도 좋고 외모도 멋있더라",
          "original_form": "정말 실력도 좋고 외모도 멋있더라",
          "speaker_id": "1",
          "time": "20210826 04:32"
        },
        {
          "id": "MDRW2100015884.1.16",
          "form": "오상욱선수 예능나와서도 너무귀엽더라",
          "original_form": "오상욱선수 예능나와서도 너무귀엽더라",
          "speaker_id": "2",
          "time": "20210826 04:33"
        },
        {
          "id": "MDRW2100015884.1.17",
          "form": "오상욱 선수가 랭킹 1위 선수라며",
          "original_form": "오상욱 선수가 랭킹 1위 선수라며",
