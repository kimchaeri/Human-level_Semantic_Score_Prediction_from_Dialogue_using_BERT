      "utterance": [
        {
          "id": "MDRW2100012380.1.1",
          "form": "해외는 가고 싶은 곳 있으신가요?",
          "original_form": "해외는 가고 싶은 곳 있으신가요?",
          "speaker_id": "2",
          "time": "20210824 21:42"
        },
        {
          "id": "MDRW2100012380.1.2",
          "form": "저는 요즘엔 동남아쪽 가고싶어요",
          "original_form": "저는 요즘엔 동남아쪽 가고싶어요",
          "speaker_id": "1",
          "time": "20210824 21:43"
        },
        {
          "id": "MDRW2100012380.1.3",
          "form": "단과일도 먹고 수상레져도 하고 푹쉬고싶어요",
          "original_form": "단과일도 먹고 수상레져도 하고 푹쉬고싶어요",
          "speaker_id": "1",
          "time": "20210824 21:43"
        },
        {
          "id": "MDRW2100012380.1.4",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210824 21:43"
        },
        {
          "id": "MDRW2100012380.1.5",
          "form": "수상레저 재밌죠",
          "original_form": "수상레저 재밌죠",
          "speaker_id": "2",
          "time": "20210824 21:43"
        },
        {
          "id": "MDRW2100012380.1.6",
          "form": "전 알레스카 같은 곳이요",
          "original_form": "전 알레스카 같은 곳이요",
          "speaker_id": "2",
          "time": "20210824 21:44"
        },
        {
          "id": "MDRW2100012380.1.7",
          "form": "오로라보러 가고 싶어요",
          "original_form": "오로라보러 가고 싶어요",
          "speaker_id": "2",
          "time": "20210824 21:44"
        },
        {
          "id": "MDRW2100012380.1.8",
          "form": "스쿠버다이빙도 하고 싶네요!",
          "original_form": "스쿠버다이빙도 하고 싶네요!",
          "speaker_id": "2",
          "time": "20210824 21:44"
        },
        {
          "id": "MDRW2100012380.1.9",
          "form": "우와 알레스카 오로라 라니 낭만적이네요",
          "original_form": "우와 알레스카 오로라 라니 낭만적이네요",
          "speaker_id": "1",
          "time": "20210824 21:45"
        },
        {
          "id": "MDRW2100012380.1.10",
          "form": "저도 살면서 한번쯤 보고싶어요",
          "original_form": "저도 살면서 한번쯤 보고싶어요",
          "speaker_id": "1",
          "time": "20210824 21:45"
        },
        {
          "id": "MDRW2100012380.1.11",
          "form": "맞아요 비록 보기 힘들지만",
          "original_form": "맞아요 비록 보기 힘들지만",
          "speaker_id": "2",
          "time": "20210824 21:46"
        },
        {
          "id": "MDRW2100012380.1.12",
          "form": "언젠가 보고싶어요",
          "original_form": "언젠가 보고싶어요",
          "speaker_id": "2",
          "time": "20210824 21:46"
        },
        {
          "id": "MDRW2100012380.1.13",
          "form": "별도 많을것 같고",
          "original_form": "별도 많을것 같고",
          "speaker_id": "2",
          "time": "20210824 21:46"
        },
        {
          "id": "MDRW2100012380.1.14",
          "form": "유우니 사막? 맞나요? 거기도 별이 쏟아지게 보인다고해서 가보고싶어요",
          "original_form": "유우니 사막? 맞나요? 거기도 별이 쏟아지게 보인다고해서 가보고싶어요",
          "speaker_id": "1",
          "time": "20210824 21:48"
        },
        {
          "id": "MDRW2100012380.1.15",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210824 21:48"
        },
        {
          "id": "MDRW2100012380.1.16",
          "form": "사진상으로도 예쁜데",
          "original_form": "사진상으로도 예쁜데",
          "speaker_id": "2",
          "time": "20210824 21:49"
        },
        {
          "id": "MDRW2100012380.1.17",
          "form": "직접 보면 엄청 광활할 것 같아요",
          "original_form": "직접 보면 엄청 광활할 것 같아요",
          "speaker_id": "2",
          "time": "20210824 21:49"
        },
        {
          "id": "MDRW2100012380.1.18",
          "form": "그죠 사진만봐도 신비한데 직접가면 황홀할거 같아요",
          "original_form": "그죠 사진만봐도 신비한데 직접가면 황홀할거 같아요",
          "speaker_id": "1",
          "time": "20210824 21:50"
        },
        {
          "id": "MDRW2100012380.1.19",
          "form": "해외 가보신곳 있나요?",
          "original_form": "해외 가보신곳 있나요?",
          "speaker_id": "1",
          "time": "20210824 21:50"
        },
        {
          "id": "MDRW2100012380.1.20",
          "form": "예전에 말레이시아 한번 가봤어요",
          "original_form": "예전에 말레이시아 한번 가봤어요",
          "speaker_id": "2",
          "time": "20210824 21:51"
        },
        {
          "id": "MDRW2100012380.1.21",
          "form": "근데 그땐 어렸어서 뭣도 모르고",
          "original_form": "근데 그땐 어렸어서 뭣도 모르고",
          "speaker_id": "2",
          "time": "20210824 21:51"
        },
        {
          "id": "MDRW2100012380.1.22",
          "form": "끌려다니기만 했어요",
          "original_form": "끌려다니기만 했어요",
          "speaker_id": "2",
          "time": "20210824 21:51"
        },
        {
          "id": "MDRW2100012380.1.23",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:51"
        },
        {
          "id": "MDRW2100012380.1.24",
          "form": "기억에 남는 일 있으신가요?",
          "original_form": "기억에 남는 일 있으신가요?",
          "speaker_id": "1",
          "time": "20210824 21:52"
        },
        {
          "id": "MDRW2100012380.1.25",
          "form": "자세한 이름은 모르겠는데",
          "original_form": "자세한 이름은 모르겠는데",
          "speaker_id": "2",
          "time": "20210824 21:53"
        },
        {
          "id": "MDRW2100012380.1.26",
          "form": "어떤 사원?에 간게 기억나요",
          "original_form": "어떤 사원?에 간게 기억나요",
          "speaker_id": "2",
          "time": "20210824 21:53"
        },
        {
          "id": "MDRW2100012380.1.27",
          "form": "원숭이들이 엄청 많았는데",
          "original_form": "원숭이들이 엄청 많았는데",
          "speaker_id": "2",
          "time": "20210824 21:53"
        },
        {
          "id": "MDRW2100012380.1.28",
          "form": "굉장히 크고 광활하더라구요",
          "original_form": "굉장히 크고 광활하더라구요",
          "speaker_id": "2",
          "time": "20210824 21:53"
        },
        {
          "id": "MDRW2100012380.1.29",
          "form": "그리고 거기 바다가 에메랄드빛 바다라",
          "original_form": "그리고 거기 바다가 에메랄드빛 바다라",
          "speaker_id": "2",
          "time": "20210824 21:55"
        },
        {
          "id": "MDRW2100012380.1.30",
          "form": "와... 신비한 분위기네요",
          "original_form": "와... 신비한 분위기네요",
          "speaker_id": "1",
          "time": "20210824 21:55"
        },
        {
          "id": "MDRW2100012380.1.31",
          "form": "바다가서 엄청 놀았어요",
          "original_form": "바다가서 엄청 놀았어요",
          "speaker_id": "2",
          "time": "20210824 21:55"
        },
        {
          "id": "MDRW2100012380.1.32",
          "form": "어렸는데도 뭔가 무서워서",
          "original_form": "어렸는데도 뭔가 무서워서",
          "speaker_id": "2",
          "time": "20210824 21:55"
        },
        {
          "id": "MDRW2100012380.1.33",
          "form": "괜히 움츠러들었어요",
          "original_form": "괜히 움츠러들었어요",
          "speaker_id": "2",
          "time": "20210824 21:55"
        },
        {
          "id": "MDRW2100012380.1.34",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:55"
        },
        {
          "id": "MDRW2100012380.1.35",
          "form": "에메랄드빛 바다라니 안그래도 바다좋아하시는거 같은데",
          "original_form": "에메랄드빛 바다라니 안그래도 바다좋아하시는거 같은데",
          "speaker_id": "1",
          "time": "20210824 21:55"
        },
        {
          "id": "MDRW2100012380.1.36",
          "form": "기억에 남으셨겠다",
          "original_form": "기억에 남으셨겠다",
          "speaker_id": "1",
          "time": "20210824 21:55"
        },
        {
          "id": "MDRW2100012380.1.37",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012380.1.38",
          "form": "그러네요 무서울수도 있겠네요",
          "original_form": "그러네요 무서울수도 있겠네요",
          "speaker_id": "1",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012380.1.39",
          "form": "근데 그거 아시나요",
          "original_form": "근데 그거 아시나요",
          "speaker_id": "2",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012380.1.40",
          "form": "뭔가요?",
          "original_form": "뭔가요?",
          "speaker_id": "1",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012380.1.41",
          "form": "바다는 좋아하는데 직접 들어가면 무서운거",
          "original_form": "바다는 좋아하는데 직접 들어가면 무서운거",
          "speaker_id": "2",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012380.1.42",
          "form": "에...?",
          "original_form": "에...?",
          "speaker_id": "1",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012380.1.43",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012380.1.44",
          "form": "깊어서그런가요?",
          "original_form": "깊어서그런가요?",
          "speaker_id": "1",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012380.1.45",
          "form": "바다 보는건 좋아하는데",
          "original_form": "바다 보는건 좋아하는데",
          "speaker_id": "2",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012380.1.46",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012380.1.47",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012380.1.48",
          "form": "깊어서 파도에 한번 쓸리면 빠질것 같아서",
          "original_form": "깊어서 파도에 한번 쓸리면 빠질것 같아서",
          "speaker_id": "2",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100012380.1.49",
          "form": "직접 들어가는 건 좀 무서워해요",
          "original_form": "직접 들어가는 건 좀 무서워해요",
          "speaker_id": "2",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100012380.1.50",
          "form": "아.... 그럼 바다보이는 곳의 수영장이 잘맞으시겠다",
          "original_form": "아.... 그럼 바다보이는 곳의 수영장이 잘맞으시겠다",
          "speaker_id": "1",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100012380.1.51",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100012380.1.52",
          "form": "꽤나 모순적인 편이죠",
          "original_form": "꽤나 모순적인 편이죠",
          "speaker_id": "2",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100012380.1.53",
          "form": "친구들도 들으면 좀 어이없어해요",
          "original_form": "친구들도 들으면 좀 어이없어해요",
          "speaker_id": "2",
          "time": "20210824 21:58"
        },
        {
          "id": "MDRW2100012380.1.54",
          "form": "앜ㅋㅋ모순적이긴하죠ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋ모순적이긴하죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100012380.1.55",
          "form": "근데 뭐 좋아하는거 무서울수도 있죠",
          "original_form": "근데 뭐 좋아하는거 무서울수도 있죠",
          "speaker_id": "1",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100012380.1.56",
          "form": "심리학적으로 흔하지않나요 이런거",
          "original_form": "심리학적으로 흔하지않나요 이런거",
          "speaker_id": "1",
          "time": "20210824 21:59"
        }
      ]
