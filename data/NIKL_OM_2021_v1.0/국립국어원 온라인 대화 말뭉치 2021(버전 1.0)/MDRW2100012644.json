      "utterance": [
        {
          "id": "MDRW2100012644.1.1",
          "form": "스포츠 뭐좋아하세요?",
          "original_form": "스포츠 뭐좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100012644.1.2",
          "form": "축구?",
          "original_form": "축구?",
          "speaker_id": "2",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100012644.1.3",
          "form": "전 축구만 봅니다. 다른스포츠는 잘안봐요",
          "original_form": "전 축구만 봅니다. 다른스포츠는 잘안봐요",
          "speaker_id": "1",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100012644.1.4",
          "form": "축구는 어짜피 해외만 보시겟네요?ㅎ",
          "original_form": "축구는 어짜피 해외만 보시겟네요?ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100012644.1.5",
          "form": "영국입니까? 스페인입니까ㅡ?",
          "original_form": "영국입니까? 스페인입니까ㅡ?",
          "speaker_id": "2",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100012644.1.6",
          "form": "지역가리지 않는데  경기영상 하이라이트로만 봐서.. 유튜브에 올라온거 보는거같아요",
          "original_form": "지역가리지 않는데  경기영상 하이라이트로만 봐서.. 유튜브에 올라온거 보는거같아요",
          "speaker_id": "1",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100012644.1.7",
          "form": "요즘에는 파리 생제르맹 팀 아니겠습니까 ㅋㅋ",
          "original_form": "요즘에는 파리 생제르맹 팀 아니겠습니까 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100012644.1.8",
          "form": "맞습니다ㅎㅎ",
          "original_form": "맞습니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100012644.1.9",
          "form": "메시ㅎ",
          "original_form": "메시ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100012644.1.10",
          "form": "근데 음바페 간대요? 안간대요?",
          "original_form": "근데 음바페 간대요? 안간대요?",
          "speaker_id": "2",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100012644.1.11",
          "form": "안가야지ㅡㅡㅋ 축신이 왔는데",
          "original_form": "안가야지ㅡㅡㅋ 축신이 왔는데",
          "speaker_id": "2",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100012644.1.12",
          "form": "음바페 메시 네이마르 등  전설같은 선수들이 있는데 ㅋㅋ",
          "original_form": "음바페 메시 네이마르 등  전설같은 선수들이 있는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:58"
        },
        {
          "id": "MDRW2100012644.1.13",
          "form": "팀스쿼드 보니까 다 알만한 선수들이구만요",
          "original_form": "팀스쿼드 보니까 다 알만한 선수들이구만요",
          "speaker_id": "1",
          "time": "20210824 23:58"
        },
        {
          "id": "MDRW2100012644.1.14",
          "form": "그쪽은 축구 좋아하실테고 무슨 팀 좋아해요?",
          "original_form": "그쪽은 축구 좋아하실테고 무슨 팀 좋아해요?",
          "speaker_id": "1",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100012644.1.15",
          "form": "전 아스날 찐팬이었는데",
          "original_form": "전 아스날 찐팬이었는데",
          "speaker_id": "2",
          "time": "20210825 00:01"
        },
        {
          "id": "MDRW2100012644.1.16",
          "form": "갈아탔습니다...ㅠ",
          "original_form": "갈아탔습니다...ㅠ",
          "speaker_id": "2",
          "time": "20210825 00:01"
        },
        {
          "id": "MDRW2100012644.1.17",
          "form": "하도 죽을쓰고 있으셔서ㅎ 제대로 팬이된건 앙리있을때고 뱅거감독 까지 팬이었어요",
          "original_form": "하도 죽을쓰고 있으셔서ㅎ 제대로 팬이된건 앙리있을때고 뱅거감독 까지 팬이었어요",
          "speaker_id": "2",
          "time": "20210825 00:01"
        },
        {
          "id": "MDRW2100012644.1.18",
          "form": "근대 해도 너무 하더라구요ㅎ 실망을...ㅠㅠ",
          "original_form": "근대 해도 너무 하더라구요ㅎ 실망을...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 00:02"
        },
        {
          "id": "MDRW2100012644.1.19",
          "form": "최고의 플레이 시기는.. 정해져 있나보네요",
          "original_form": "최고의 플레이 시기는.. 정해져 있나보네요",
          "speaker_id": "1",
          "time": "20210825 00:06"
        },
        {
          "id": "MDRW2100012644.1.20",
          "form": "맞아요ㅠ 아무래도 구단이 재정도 탄탄해야하고ㅎ",
          "original_form": "맞아요ㅠ 아무래도 구단이 재정도 탄탄해야하고ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012644.1.21",
          "form": "요즘엔 어떤 팀이 잘할려나. 파리 셍제르맹이 1위할려나",
          "original_form": "요즘엔 어떤 팀이 잘할려나. 파리 셍제르맹이 1위할려나",
          "speaker_id": "1",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012644.1.22",
          "form": "근데 좀 그렇죠ㅠ",
          "original_form": "근데 좀 그렇죠ㅠ",
          "speaker_id": "2",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012644.1.23",
          "form": "메시오기전에도 맨날 미끄러져서",
          "original_form": "메시오기전에도 맨날 미끄러져서",
          "speaker_id": "2",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012644.1.24",
          "form": "왜그런걸까요? 그 엄청난 선수들이있는데",
          "original_form": "왜그런걸까요? 그 엄청난 선수들이있는데",
          "speaker_id": "2",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012644.1.25",
          "form": "선수들이 좋아서 경기도 기대되고 결과도 좋을거고",
          "original_form": "선수들이 좋아서 경기도 기대되고 결과도 좋을거고",
          "speaker_id": "1",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012644.1.26",
          "form": "그전에는 파리 셍제르맹에  관심이 없었어요 ㅋㅋ",
          "original_form": "그전에는 파리 셍제르맹에  관심이 없었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:09"
        },
        {
          "id": "MDRW2100012644.1.27",
          "form": "그쪽은 축구 자주 보는거같아요?",
          "original_form": "그쪽은 축구 자주 보는거같아요?",
          "speaker_id": "1",
          "time": "20210825 00:09"
        },
        {
          "id": "MDRW2100012644.1.28",
          "form": "글죠ㅎ 파리가 정말 많이 영입하긴했어요ㅎ",
          "original_form": "글죠ㅎ 파리가 정말 많이 영입하긴했어요ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:09"
        },
        {
          "id": "MDRW2100012644.1.29",
          "form": "음바페가있어서ㅎ",
          "original_form": "음바페가있어서ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:10"
        }
      ]
    }
