        },
        {
          "id": "MDRW2100033940.1.2",
          "form": "운동 요즘 해?",
          "original_form": "운동 요즘 해?",
          "speaker_id": "2",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100033940.1.3",
          "form": "유산소만 조금",
          "original_form": "유산소만 조금",
          "speaker_id": "1",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100033940.1.4",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100033940.1.5",
          "form": "나도 걷기 가끔해",
          "original_form": "나도 걷기 가끔해",
          "speaker_id": "2",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100033940.1.6",
          "form": "뚝방 걷는 거 좋아해",
          "original_form": "뚝방 걷는 거 좋아해",
          "speaker_id": "2",
          "time": "20210825 16:22"
        },
        {
          "id": "MDRW2100033940.1.7",
          "form": "나는 공원 자주 가",
          "original_form": "나는 공원 자주 가",
          "speaker_id": "1",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100033940.1.8",
          "form": "근데 비와서 못 갔어",
          "original_form": "근데 비와서 못 갔어",
          "speaker_id": "1",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100033940.1.9",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210825 16:23"
        },
        {
          "id": "MDRW2100033940.1.10",
          "form": "집에서 홈트했어",
          "original_form": "집에서 홈트했어",
          "speaker_id": "1",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100033940.1.11",
          "form": "홈트 뭐해?",
          "original_form": "홈트 뭐해?",
          "speaker_id": "2",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100033940.1.12",
          "form": "유튜브 보고 운동해",
          "original_form": "유튜브 보고 운동해",
          "speaker_id": "1",
          "time": "20210825 16:24"
        },
        {
          "id": "MDRW2100033940.1.13",
          "form": "나도 가끔하는데 뭐 봐?",
          "original_form": "나도 가끔하는데 뭐 봐?",
          "speaker_id": "2",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100033940.1.14",
          "form": "땅끄부부 영상 자주 봐",
          "original_form": "땅끄부부 영상 자주 봐",
          "speaker_id": "1",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100033940.1.15",
          "form": "나도 그거 많이 봤어",
          "original_form": "나도 그거 많이 봤어",
          "speaker_id": "2",
          "time": "20210825 16:25"
        },
        {
          "id": "MDRW2100033940.1.16",
          "form": "친절하고 재밌어",
          "original_form": "친절하고 재밌어",
          "speaker_id": "1",
          "time": "20210825 16:25"
