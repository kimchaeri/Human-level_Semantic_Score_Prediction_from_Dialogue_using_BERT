        },
        {
          "id": "MDRW2100030255.1.2",
          "form": "영진국밥 지금도 줄서요 낮에요",
          "original_form": "영진국밥 지금도 줄서요 낮에요",
          "speaker_id": "1",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030255.1.3",
          "form": "맘스터치에서",
          "original_form": "맘스터치에서",
          "speaker_id": "2",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030255.1.4",
          "form": "맘스터치 옹ㆍ",
          "original_form": "맘스터치 옹ㆍ",
          "speaker_id": "1",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030255.1.5",
          "form": "양념치킨 간장치킨 싸이버거",
          "original_form": "양념치킨 간장치킨 싸이버거",
          "speaker_id": "2",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030255.1.6",
          "form": "싸이버거 ㅜㅜ",
          "original_form": "싸이버거 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030255.1.7",
          "form": "출시했는데요",
          "original_form": "출시했는데요",
          "speaker_id": "2",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030255.1.8",
          "form": "먕념이 제 입맛에 딱맞아서",
          "original_form": "먕념이 제 입맛에 딱맞아서",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030255.1.9",
          "form": "맘스터치  치킨도 맛있나요",
          "original_form": "맘스터치  치킨도 맛있나요",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030255.1.10",
          "form": "자주 시켰거더요",
          "original_form": "자주 시켰거더요",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030255.1.11",
          "form": "치킨안먹어봐시어요",
          "original_form": "치킨안먹어봐시어요",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030255.1.12",
          "form": "햄버거만 먹었어요",
          "original_form": "햄버거만 먹었어요",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030255.1.13",
          "form": "치킨맛 싸이버거였머묘",
          "original_form": "치킨맛 싸이버거였머묘",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030255.1.14",
          "form": "버거먹었어요",
          "original_form": "버거먹었어요",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030255.1.15",
          "form": "먕념치킨 소스 들머가있는데",
          "original_form": "먕념치킨 소스 들머가있는데",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030255.1.16",
          "form": "치킨도  종류많아요",
          "original_form": "치킨도  종류많아요",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030255.1.17",
          "form": "매콤하니 존맛이더라구요?",
          "original_form": "매콤하니 존맛이더라구요?",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030255.1.18",
          "form": "매콤하니 존맛이것어요",
          "original_form": "매콤하니 존맛이것어요",
