      "utterance": [
        {
          "id": "MDRW2100024551.1.1",
          "form": "요즘 챙겨드시는 게 있나요?",
          "original_form": "요즘 챙겨드시는 게 있나요?",
          "speaker_id": "1",
          "time": "20210831 13:59"
        },
        {
          "id": "MDRW2100024551.1.2",
          "form": "다이어트약도 먹고",
          "original_form": "다이어트약도 먹고",
          "speaker_id": "2",
          "time": "20210831 14:29"
        },
        {
          "id": "MDRW2100024551.1.3",
          "form": "건강챙기는 약도먹어요",
          "original_form": "건강챙기는 약도먹어요",
          "speaker_id": "2",
          "time": "20210831 14:30"
        },
        {
          "id": "MDRW2100024551.1.4",
          "form": "건강 챙기는 거면 영양제요?",
          "original_form": "건강 챙기는 거면 영양제요?",
          "speaker_id": "1",
          "time": "20210831 14:33"
        },
        {
          "id": "MDRW2100024551.1.5",
          "form": "비타민과 콜라겐 먹어요",
          "original_form": "비타민과 콜라겐 먹어요",
          "speaker_id": "2",
          "time": "20210831 15:58"
        },
        {
          "id": "MDRW2100024551.1.6",
          "form": "비타민 많이들 챙겨먹죠 저는 이번에 루테인 싸게 나와서 그거 샀어요",
          "original_form": "비타민 많이들 챙겨먹죠 저는 이번에 루테인 싸게 나와서 그거 샀어요",
          "speaker_id": "1",
          "time": "20210831 16:04"
        },
        {
          "id": "MDRW2100024551.1.7",
          "form": "루테인도 먹고있어요",
          "original_form": "루테인도 먹고있어요",
          "speaker_id": "2",
          "time": "20210831 16:04"
        },
        {
          "id": "MDRW2100024551.1.8",
          "form": "점점 챙겨먹을께 많아지네요",
          "original_form": "점점 챙겨먹을께 많아지네요",
          "speaker_id": "2",
          "time": "20210831 16:05"
        },
        {
          "id": "MDRW2100024551.1.9",
          "form": "건강하게 오래 살기 위해 어쩔 수 없죠",
          "original_form": "건강하게 오래 살기 위해 어쩔 수 없죠",
          "speaker_id": "1",
          "time": "20210831 16:07"
        },
        {
          "id": "MDRW2100024551.1.10",
          "form": "저는 요즘 운동도 하고있어요",
          "original_form": "저는 요즘 운동도 하고있어요",
          "speaker_id": "1",
          "time": "20210831 16:07"
        },
        {
          "id": "MDRW2100024551.1.11",
          "form": "저도 걷는 운동은 해요",
          "original_form": "저도 걷는 운동은 해요",
          "speaker_id": "2",
          "time": "20210831 16:07"
        },
        {
          "id": "MDRW2100024551.1.12",
          "form": "어떤운동하세요?",
          "original_form": "어떤운동하세요?",
          "speaker_id": "2",
          "time": "20210831 16:08"
        },
        {
          "id": "MDRW2100024551.1.13",
          "form": "링피트라고 운동하는 게임이요",
          "original_form": "링피트라고 운동하는 게임이요",
          "speaker_id": "1",
          "time": "20210831 16:08"
        },
        {
          "id": "MDRW2100024551.1.14",
          "form": "들어보지 못한 운동이네요",
          "original_form": "들어보지 못한 운동이네요",
          "speaker_id": "2",
          "time": "20210831 16:08"
        },
        {
          "id": "MDRW2100024551.1.15",
          "form": "게임기로 할 수 있는 거라 관심 없는 분들은 모르시더라구요",
          "original_form": "게임기로 할 수 있는 거라 관심 없는 분들은 모르시더라구요",
          "speaker_id": "1",
          "time": "20210831 16:09"
        },
        {
          "id": "MDRW2100024551.1.16",
          "form": "화면보며 하는거라 재밌긴하겠네요",
          "original_form": "화면보며 하는거라 재밌긴하겠네요",
          "speaker_id": "2",
          "time": "20210831 16:41"
        },
        {
          "id": "MDRW2100024551.1.17",
          "form": "다이어트도 되나요?",
          "original_form": "다이어트도 되나요?",
          "speaker_id": "2",
          "time": "20210831 16:41"
        },
        {
          "id": "MDRW2100024551.1.18",
          "form": "운동하는 거니까 되죠!",
          "original_form": "운동하는 거니까 되죠!",
          "speaker_id": "1",
          "time": "20210831 16:43"
        },
        {
          "id": "MDRW2100024551.1.19",
          "form": "걷는건 재미는없어요",
          "original_form": "걷는건 재미는없어요",
          "speaker_id": "2",
          "time": "20210831 16:46"
        },
        {
          "id": "MDRW2100024551.1.20",
          "form": "아프지 않으려고 유지만하는거죠",
          "original_form": "아프지 않으려고 유지만하는거죠",
          "speaker_id": "2",
          "time": "20210831 16:46"
        },
        {
          "id": "MDRW2100024551.1.21",
          "form": "많이 걸으시나요?",
          "original_form": "많이 걸으시나요?",
          "speaker_id": "1",
          "time": "20210831 16:47"
        },
        {
          "id": "MDRW2100024551.1.22",
          "form": "저녁에 1시간30분정도 걸어요",
          "original_form": "저녁에 1시간30분정도 걸어요",
          "speaker_id": "2",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100024551.1.23",
          "form": "비가와서 그나마도 잘 못하고있네요",
          "original_form": "비가와서 그나마도 잘 못하고있네요",
          "speaker_id": "2",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100024551.1.24",
          "form": "아 그러네요 요즘 계속 비가 오죠",
          "original_form": "아 그러네요 요즘 계속 비가 오죠",
          "speaker_id": "1",
          "time": "20210831 16:51"
        }
      ]
    }
