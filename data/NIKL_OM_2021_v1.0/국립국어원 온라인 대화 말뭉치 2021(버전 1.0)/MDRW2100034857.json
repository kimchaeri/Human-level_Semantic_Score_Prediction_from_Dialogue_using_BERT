      "utterance": [
        {
          "id": "MDRW2100034857.1.1",
          "form": "맛집 탐방 좋아하세요?",
          "original_form": "맛집 탐방 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 21:14"
        },
        {
          "id": "MDRW2100034857.1.2",
          "form": "네 좋아해요 맛집 많이 아시나요?",
          "original_form": "네 좋아해요 맛집 많이 아시나요?",
          "speaker_id": "2",
          "time": "20210828 21:18"
        },
        {
          "id": "MDRW2100034857.1.3",
          "form": "지역은 꽤 찾아보고 있어요!",
          "original_form": "지역은 꽤 찾아보고 있어요!",
          "speaker_id": "1",
          "time": "20210828 21:21"
        },
        {
          "id": "MDRW2100034857.1.4",
          "form": "멀어도 가시는편이신가요?",
          "original_form": "멀어도 가시는편이신가요?",
          "speaker_id": "1",
          "time": "20210828 21:21"
        },
        {
          "id": "MDRW2100034857.1.5",
          "form": "너무 멀면 안가고 메모해뒀다가 그근처 갈일있을때 가는편이예요",
          "original_form": "너무 멀면 안가고 메모해뒀다가 그근처 갈일있을때 가는편이예요",
          "speaker_id": "2",
          "time": "20210828 21:34"
        },
        {
          "id": "MDRW2100034857.1.6",
          "form": "멀어도 가시는편이세요?",
          "original_form": "멀어도 가시는편이세요?",
          "speaker_id": "2",
          "time": "20210828 21:35"
        },
        {
          "id": "MDRW2100034857.1.7",
          "form": "아항~ 저도 갈 일 있으면 가요",
          "original_form": "아항~ 저도 갈 일 있으면 가요",
          "speaker_id": "1",
          "time": "20210828 21:38"
        },
        {
          "id": "MDRW2100034857.1.8",
          "form": "그걸 위해 가지는 않네오ㅡ",
          "original_form": "그걸 위해 가지는 않네오ㅡ",
          "speaker_id": "1",
          "time": "20210828 21:38"
        },
        {
          "id": "MDRW2100034857.1.9",
          "form": "저 아는 맛집 알려드릴까요?",
          "original_form": "저 아는 맛집 알려드릴까요?",
          "speaker_id": "2",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100034857.1.10",
          "form": "오!! 저야 감사드리죠!!!!!",
          "original_form": "오!! 저야 감사드리죠!!!!!",
          "speaker_id": "1",
          "time": "20210828 21:45"
        },
        {
          "id": "MDRW2100034857.1.11",
          "form": "부산인데요 부산역 근처에 곱도리탕유명한 그집 이란곳이 있어요",
          "original_form": "부산인데요 부산역 근처에 곱도리탕유명한 그집 이란곳이 있어요",
          "speaker_id": "2",
          "time": "20210828 21:46"
        },
        {
          "id": "MDRW2100034857.1.12",
          "form": "오오 감사합니당",
          "original_form": "오오 감사합니당",
          "speaker_id": "1",
          "time": "20210828 21:47"
        },
        {
          "id": "MDRW2100034857.1.13",
          "form": "부산가면 꼭 먹어볼게요!",
          "original_form": "부산가면 꼭 먹어볼게요!",
          "speaker_id": "1",
          "time": "20210828 21:48"
        },
        {
          "id": "MDRW2100034857.1.14",
          "form": "곱창은 좋아하시나요?",
          "original_form": "곱창은 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100034857.1.15",
          "form": "곱창안좋아하시면 곱도리탕 별로일수도 있어서요",
          "original_form": "곱창안좋아하시면 곱도리탕 별로일수도 있어서요",
          "speaker_id": "2",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100034857.1.16",
          "form": "네! 좋아해요",
          "original_form": "네! 좋아해요",
          "speaker_id": "1",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100034857.1.17",
          "form": "곱도리탕은 곱창+닭도리탕 ㅇㅣ런건가요?",
          "original_form": "곱도리탕은 곱창+닭도리탕 ㅇㅣ런건가요?",
          "speaker_id": "1",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100034857.1.18",
          "form": "네 대창이긴한데 이름은 곱창+닭도리탕이예요",
          "original_form": "네 대창이긴한데 이름은 곱창+닭도리탕이예요",
          "speaker_id": "2",
          "time": "20210828 22:01"
        },
        {
