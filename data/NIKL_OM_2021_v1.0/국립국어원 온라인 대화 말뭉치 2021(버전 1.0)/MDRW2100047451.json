      "utterance": [
        {
          "id": "MDRW2100047451.1.1",
          "form": "저는 새우찜 시켜먹을 생각이예요",
          "original_form": "저는 새우찜 시켜먹을 생각이예요",
          "speaker_id": "2",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100047451.1.2",
          "form": "뭐드시고 싶으신가요?",
          "original_form": "뭐드시고 싶으신가요?",
          "speaker_id": "2",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100047451.1.3",
          "form": "전 피자가 먹고싶네요 새우 좋아하세요?",
          "original_form": "전 피자가 먹고싶네요 새우 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100047451.1.4",
          "form": "새우 좋아해요!",
          "original_form": "새우 좋아해요!",
          "speaker_id": "2",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100047451.1.5",
          "form": "없어서 못 먹죠",
          "original_form": "없어서 못 먹죠",
          "speaker_id": "2",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100047451.1.6",
          "form": "가장 좋아하는 치킨브랜드는 어디세요?",
          "original_form": "가장 좋아하는 치킨브랜드는 어디세요?",
          "speaker_id": "1",
          "time": "20210826 16:33"
        },
        {
          "id": "MDRW2100047451.1.7",
          "form": "저는 교촌이랑 bbp요",
          "original_form": "저는 교촌이랑 bbp요",
          "speaker_id": "2",
          "time": "20210826 16:41"
        },
        {
          "id": "MDRW2100047451.1.8",
          "form": "어디좋아하세요?",
          "original_form": "어디좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 16:41"
        },
        {
          "id": "MDRW2100047451.1.9",
          "form": "전 굽네여! 뿌링클 좋아하세여?",
          "original_form": "전 굽네여! 뿌링클 좋아하세여?",
          "speaker_id": "1",
          "time": "20210826 16:43"
        },
        {
          "id": "MDRW2100047451.1.10",
          "form": "저는 안좋아하는데 룸메가 좋아해서 가끔먹어요",
          "original_form": "저는 안좋아하는데 룸메가 좋아해서 가끔먹어요",
          "speaker_id": "2",
          "time": "20210826 16:43"
        },
        {
          "id": "MDRW2100047451.1.11",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 16:43"
        },
        {
          "id": "MDRW2100047451.1.12",
          "form": "그냥 그래요 ㅎㅎ 로제떡볶이 드셔보셨어요?",
          "original_form": "그냥 그래요 ㅎㅎ 로제떡볶이 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210826 16:45"
        },
        {
          "id": "MDRW2100047451.1.13",
          "form": "떡볶이를 안좋아해서요 안먹어봤어요",
          "original_form": "떡볶이를 안좋아해서요 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210826 16:46"
        },
        {
          "id": "MDRW2100047451.1.14",
          "form": "좋아하시는구나!",
          "original_form": "좋아하시는구나!",
          "speaker_id": "2",
          "time": "20210826 16:46"
        },
        {
          "id": "MDRW2100047451.1.15",
          "form": "넵ㅎㅎ 마라탕은 드셔보셨어요?",
          "original_form": "넵ㅎㅎ 마라탕은 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210826 16:48"
        },
        {
          "id": "MDRW2100047451.1.16",
          "form": "네 먹어봤어요 취향은 아니였지만요",
          "original_form": "네 먹어봤어요 취향은 아니였지만요",
          "speaker_id": "2",
          "time": "20210826 16:49"
        },
        {
          "id": "MDRW2100047451.1.17",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 16:49"
        },
        {
          "id": "MDRW2100047451.1.18",
          "form": "아뇨 저도 별롷ㅎㅎ...",
          "original_form": "아뇨 저도 별롷ㅎㅎ...",
          "speaker_id": "1",
          "time": "20210826 16:51"
        },
        {
          "id": "MDRW2100047451.1.19",
          "form": "무슨 컵라면 좋아하세요?",
          "original_form": "무슨 컵라면 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 16:51"
        },
        {
          "id": "MDRW2100047451.1.20",
          "form": "저는 요즘은 컵누들 매운찜닭맛이요",
