      "utterance": [
        {
          "id": "MDRW2100022338.1.1",
          "form": "나 지금 뻥이요 먹는중이야",
          "original_form": "나 지금 뻥이요 먹는중이야",
          "speaker_id": "2",
          "time": "20210829 12:47"
        },
        {
          "id": "MDRW2100022338.1.2",
          "form": "오 만두?",
          "original_form": "오 만두?",
          "speaker_id": "2",
          "time": "20210829 12:47"
        },
        {
          "id": "MDRW2100022338.1.3",
          "form": "나 초딩때 이후로 만두 빚어본 적 없는듯",
          "original_form": "나 초딩때 이후로 만두 빚어본 적 없는듯",
          "speaker_id": "2",
          "time": "20210829 12:48"
        },
        {
          "id": "MDRW2100022338.1.4",
          "form": "응응 뭔가 무념무상으로 손움직이기",
          "original_form": "응응 뭔가 무념무상으로 손움직이기",
          "speaker_id": "1",
          "time": "20210829 12:48"
        },
        {
          "id": "MDRW2100022338.1.5",
          "form": "아 정말?",
          "original_form": "아 정말?",
          "speaker_id": "1",
          "time": "20210829 12:48"
        },
        {
          "id": "MDRW2100022338.1.6",
          "form": "웅웅ㅋㅋㅋ 명절때 할머니랑 김치만두 빚었었는데 그 이후로 할머니도 나이드셨고 이래서 만두를 안하거든 우리집에서",
          "original_form": "웅웅ㅋㅋㅋ 명절때 할머니랑 김치만두 빚었었는데 그 이후로 할머니도 나이드셨고 이래서 만두를 안하거든 우리집에서",
          "speaker_id": "2",
          "time": "20210829 12:48"
        },
        {
          "id": "MDRW2100022338.1.7",
          "form": "나도 엄청 가끔만 하는데 집에서 빚은 손만두는 또 그거대로 맛있더라구",
          "original_form": "나도 엄청 가끔만 하는데 집에서 빚은 손만두는 또 그거대로 맛있더라구",
          "speaker_id": "1",
          "time": "20210829 12:48"
        },
        {
          "id": "MDRW2100022338.1.8",
          "form": "언니는 김치만두 좋아해 아님 고기만두 좋아해?",
          "original_form": "언니는 김치만두 좋아해 아님 고기만두 좋아해?",
          "speaker_id": "2",
          "time": "20210829 12:49"
        },
        {
          "id": "MDRW2100022338.1.9",
          "form": "맞아 손이 너무 가니까 명절이나? 한번 빚자 할때나 하게되는거같아 ㅋㅋ나도 혼자서보다 엄마한테 같이하자고 해",
          "original_form": "맞아 손이 너무 가니까 명절이나? 한번 빚자 할때나 하게되는거같아 ㅋㅋ나도 혼자서보다 엄마한테 같이하자고 해",
          "speaker_id": "1",
          "time": "20210829 12:49"
        },
        {
          "id": "MDRW2100022338.1.10",
          "form": "나는 김치만두",
          "original_form": "나는 김치만두",
          "speaker_id": "1",
          "time": "20210829 12:49"
        },
        {
          "id": "MDRW2100022338.1.11",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210829 12:49"
        },
        {
          "id": "MDRW2100022338.1.12",
          "form": "나는 고기만두ㅎ",
          "original_form": "나는 고기만두ㅎ",
          "speaker_id": "2",
          "time": "20210829 12:50"
        },
        {
          "id": "MDRW2100022338.1.13",
          "form": "왜? 육즙때문인가?",
          "original_form": "왜? 육즙때문인가?",
          "speaker_id": "1",
          "time": "20210829 12:50"
        },
        {
          "id": "MDRW2100022338.1.14",
          "form": "언니 마포만두에 갈비만두 먹어봤어??",
          "original_form": "언니 마포만두에 갈비만두 먹어봤어??",
          "speaker_id": "2",
          "time": "20210829 12:50"
        },
        {
          "id": "MDRW2100022338.1.15",
          "form": "아니? 맛있어?",
          "original_form": "아니? 맛있어?",
          "speaker_id": "1",
          "time": "20210829 12:50"
        },
        {
          "id": "MDRW2100022338.1.16",
          "form": "나 처음 먹고 눈물 흘릴뻔했잖아 너무 맛있어서",
          "original_form": "나 처음 먹고 눈물 흘릴뻔했잖아 너무 맛있어서",
          "speaker_id": "2",
          "time": "20210829 12:50"
        },
        {
          "id": "MDRW2100022338.1.17",
          "form": "고기가 갈비살이 들어간거야? 아니면 갈비양념맛인거야?",
          "original_form": "고기가 갈비살이 들어간거야? 아니면 갈비양념맛인거야?",
          "speaker_id": "1",
          "time": "20210829 12:50"
        },
        {
          "id": "MDRW2100022338.1.18",
          "form": "갈비양념맛인거같애ㅋㅋㅋㅋ",
          "original_form": "갈비양념맛인거같애ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:51"
        },
        {
          "id": "MDRW2100022338.1.19",
          "form": "둘중에 뭐든 맛있겠네... 그정도로 맛있다고?",
          "original_form": "둘중에 뭐든 맛있겠네... 그정도로 맛있다고?",
          "speaker_id": "1",
          "time": "20210829 12:51"
        },
        {
          "id": "MDRW2100022338.1.20",
          "form": "나 미트볼 파스타하려고 미트볼샀어",
          "original_form": "나 미트볼 파스타하려고 미트볼샀어",
          "speaker_id": "1",
          "time": "20210829 12:51"
        }
      ]
    }
