      "utterance": [
        {
          "id": "MDRW2100006723.1.1",
          "form": "음식 뭐좋아하세요?",
          "original_form": "음식 뭐좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 07:37"
        },
        {
          "id": "MDRW2100006723.1.2",
          "form": "햄버거.피자 요 ㄹ",
          "original_form": "햄버거.피자 요 ㄹ",
          "speaker_id": "2",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100006723.1.3",
          "form": "햄버거 피자는 자주는 아니어도 한번씩 엄청 땡겨요",
          "original_form": "햄버거 피자는 자주는 아니어도 한번씩 엄청 땡겨요",
          "speaker_id": "1",
          "time": "20210908 08:07"
        },
        {
          "id": "MDRW2100006723.1.4",
          "form": "맞아요 한달에 두세 번정도 먹어요",
          "original_form": "맞아요 한달에 두세 번정도 먹어요",
          "speaker_id": "2",
          "time": "20210908 08:09"
        },
        {
          "id": "MDRW2100006723.1.5",
          "form": "어디꺼 좋아하세요?",
          "original_form": "어디꺼 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 08:10"
        },
        {
          "id": "MDRW2100006723.1.6",
          "form": "버거킹.도미노 최애입니다 ㅎㅎ",
          "original_form": "버거킹.도미노 최애입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 08:11"
        },
        {
          "id": "MDRW2100006723.1.7",
          "form": "ㅎㅈㅎ 딱 훌륭한 곳들이네요",
          "original_form": "ㅎㅈㅎ 딱 훌륭한 곳들이네요",
          "speaker_id": "1",
          "time": "20210908 08:13"
        },
        {
          "id": "MDRW2100006723.1.8",
          "form": "좋아하시나요?ㅎㅎ",
          "original_form": "좋아하시나요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 08:16"
        },
        {
          "id": "MDRW2100006723.1.9",
          "form": "네 저는 근데 피자는 피자알볼로라고",
          "original_form": "네 저는 근데 피자는 피자알볼로라고",
          "speaker_id": "1",
          "time": "20210908 08:27"
        },
        {
          "id": "MDRW2100006723.1.10",
          "form": "거기 맛있더라고요",
          "original_form": "거기 맛있더라고요",
          "speaker_id": "1",
          "time": "20210908 08:27"
        },
        {
          "id": "MDRW2100006723.1.11",
          "form": "아 한번정도 먹어봤어요 웃음피자인가 있지않나요??",
          "original_form": "아 한번정도 먹어봤어요 웃음피자인가 있지않나요??",
          "speaker_id": "2",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100006723.1.12",
          "form": "어깨피자",
          "original_form": "어깨피자",
          "speaker_id": "1",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100006723.1.13",
          "form": "꿈을피자?",
          "original_form": "꿈을피자?",
          "speaker_id": "1",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100006723.1.14",
          "form": "이름이 웃겨요 ㅋㅋ",
          "original_form": "이름이 웃겨요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100006723.1.15",
          "form": "아 어깨인가봐요 ㅎㅎ",
          "original_form": "아 어깨인가봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100006723.1.16",
          "form": "맛 섞여있는거 먹어봤는데...",
          "original_form": "맛 섞여있는거 먹어봤는데...",
          "speaker_id": "2",
          "time": "20210908 08:29"
        },
        {
          "id": "MDRW2100006723.1.17",
          "form": "ㅎㅎ 저도 헷갈려요ㅋㅋ",
          "original_form": "ㅎㅎ 저도 헷갈려요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100006723.1.18",
          "form": "갑자기 피자 땅기네요",
          "original_form": "갑자기 피자 땅기네요",
          "speaker_id": "1",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100006723.1.19",
