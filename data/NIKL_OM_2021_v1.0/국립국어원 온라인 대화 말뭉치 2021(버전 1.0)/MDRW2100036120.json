      "utterance": [
        {
          "id": "MDRW2100036120.1.1",
          "form": "안녕하세 뇨즘",
          "original_form": "안녕하세 뇨즘",
          "speaker_id": "2",
          "time": "20210830 16:39"
        },
        {
          "id": "MDRW2100036120.1.2",
          "form": "오늘 뭐 사셧나여",
          "original_form": "오늘 뭐 사셧나여",
          "speaker_id": "2",
          "time": "20210830 16:39"
        },
        {
          "id": "MDRW2100036120.1.3",
          "form": "아무것도 안삼요",
          "original_form": "아무것도 안삼요",
          "speaker_id": "1",
          "time": "20210830 16:40"
        },
        {
          "id": "MDRW2100036120.1.4",
          "form": "저는 체험 샴푸 0원에 삿음여",
          "original_form": "저는 체험 샴푸 0원에 삿음여",
          "speaker_id": "2",
          "time": "20210830 16:49"
        },
        {
          "id": "MDRW2100036120.1.5",
          "form": "양은 많나요?",
          "original_form": "양은 많나요?",
          "speaker_id": "1",
          "time": "20210830 16:55"
        },
        {
          "id": "MDRW2100036120.1.6",
          "form": "밥 5숟갈이네요..",
          "original_form": "밥 5숟갈이네요..",
          "speaker_id": "2",
          "time": "20210830 17:00"
        },
        {
          "id": "MDRW2100036120.1.7",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "2",
          "time": "20210830 17:00"
        },
        {
          "id": "MDRW2100036120.1.8",
          "form": "하하 그랴도 0원이니깐여",
          "original_form": "하하 그랴도 0원이니깐여",
          "speaker_id": "2",
          "time": "20210830 17:02"
        },
        {
          "id": "MDRW2100036120.1.9",
          "form": "1회용이네요ㅋ",
          "original_form": "1회용이네요ㅋ",
          "speaker_id": "1",
          "time": "20210830 17:02"
        },
        {
          "id": "MDRW2100036120.1.10",
          "form": "잘써보새요",
          "original_form": "잘써보새요",
          "speaker_id": "1",
          "time": "20210830 17:02"
        },
        {
          "id": "MDRW2100036120.1.11",
          "form": "왜 저 놀리세여",
          "original_form": "왜 저 놀리세여",
          "speaker_id": "2",
          "time": "20210830 17:02"
        },
        {
          "id": "MDRW2100036120.1.12",
          "form": "어디꺼애요",
          "original_form": "어디꺼애요",
          "speaker_id": "1",
          "time": "20210830 17:02"
        },
        {
          "id": "MDRW2100036120.1.13",
          "form": "부스티모라는 곳이여",
          "original_form": "부스티모라는 곳이여",
          "speaker_id": "2",
          "time": "20210830 17:02"
        },
        {
          "id": "MDRW2100036120.1.14",
          "form": "모르는곳이네",
          "original_form": "모르는곳이네",
          "speaker_id": "1",
          "time": "20210830 17:02"
        },
        {
          "id": "MDRW2100036120.1.15",
          "form": "모공까지 딥클렌징된대서..",
          "original_form": "모공까지 딥클렌징된대서..",
          "speaker_id": "2",
          "time": "20210830 17:03"
        },
        {
          "id": "MDRW2100036120.1.16",
          "form": "하긴 님은 모공 씻어야해요",
          "original_form": "하긴 님은 모공 씻어야해요",
          "speaker_id": "1",
          "time": "20210830 17:03"
