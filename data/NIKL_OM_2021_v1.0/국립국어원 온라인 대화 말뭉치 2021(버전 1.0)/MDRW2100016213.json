      "utterance": [
        {
          "id": "MDRW2100016213.1.1",
          "form": "유튜브 자주 보세요?",
          "original_form": "유튜브 자주 보세요?",
          "speaker_id": "1",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100016213.1.2",
          "form": "네 제 일상이에요",
          "original_form": "네 제 일상이에요",
          "speaker_id": "2",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100016213.1.3",
          "form": "오 뭐 자주 보세요?",
          "original_form": "오 뭐 자주 보세요?",
          "speaker_id": "1",
          "time": "20210826 11:00"
        },
        {
          "id": "MDRW2100016213.1.4",
          "form": "강유미와 세포들이요",
          "original_form": "강유미와 세포들이요",
          "speaker_id": "2",
          "time": "20210826 11:00"
        },
        {
          "id": "MDRW2100016213.1.5",
          "form": "재미있어요",
          "original_form": "재미있어요",
          "speaker_id": "2",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100016213.1.6",
          "form": "그런 채널이 있엉ㅅ?",
          "original_form": "그런 채널이 있엉ㅅ?",
          "speaker_id": "1",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100016213.1.7",
          "form": "처음 들어요",
          "original_form": "처음 들어요",
          "speaker_id": "1",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100016213.1.8",
          "form": "전 자빱티비 봐요",
          "original_form": "전 자빱티비 봐요",
          "speaker_id": "1",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100016213.1.9",
          "form": "요즘 유행을 선도하는 유튜버죠",
          "original_form": "요즘 유행을 선도하는 유튜버죠",
          "speaker_id": "1",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100016213.1.10",
          "form": "콘텐츠 이름일걸요 사실 정확하지 않아요",
          "original_form": "콘텐츠 이름일걸요 사실 정확하지 않아요",
          "speaker_id": "2",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100016213.1.11",
          "form": "오 저도 처음 듣네요",
          "original_form": "오 저도 처음 듣네요",
          "speaker_id": "2",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100016213.1.12",
          "form": "정말요?",
          "original_form": "정말요?",
          "speaker_id": "1",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100016213.1.13",
          "form": "자빱을요?",
          "original_form": "자빱을요?",
          "speaker_id": "1",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100016213.1.14",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100016213.1.15",
          "form": "룸메분은 자빱 안 보시나봐요",
          "original_form": "룸메분은 자빱 안 보시나봐요",
          "speaker_id": "1",
          "time": "20210826 11:02"
        },
        {
          "id": "MDRW2100016213.1.16",
          "form": "어떤 컨텐츠 하시는데요?",
          "original_form": "어떤 컨텐츠 하시는데요?",
          "speaker_id": "2",
          "time": "20210826 11:02"
        },
        {
          "id": "MDRW2100016213.1.17",
          "form": "관심없어요",
          "original_form": "관심없어요",
          "speaker_id": "2",
          "time": "20210826 11:02"
        },
        {
          "id": "MDRW2100016213.1.18",
          "form": "마크 콘텐츠요",
          "original_form": "마크 콘텐츠요",
          "speaker_id": "1",
          "time": "20210826 11:02"
        },
        {
          "id": "MDRW2100016213.1.19",
          "form": "와 말씀이 좀...",
          "original_form": "와 말씀이 좀...",
          "speaker_id": "1",
          "time": "20210826 11:02"
        },
        {
          "id": "MDRW2100016213.1.20",
          "form": "마크로 영화 만드는 유튜법니다",
          "original_form": "마크로 영화 만드는 유튜법니다",
          "speaker_id": "1",
          "time": "20210826 11:02"
        },
        {
          "id": "MDRW2100016213.1.21",
          "form": "마크 재미있네요",
          "original_form": "마크 재미있네요",
          "speaker_id": "2",
          "time": "20210826 11:02"
        },
        {
          "id": "MDRW2100016213.1.22",
          "form": "영화라니 너무 멋지네요",
          "original_form": "영화라니 너무 멋지네요",
          "speaker_id": "2",
          "time": "20210826 11:02"
        },
        {
          "id": "MDRW2100016213.1.23",
          "form": "무슨 영화 만드셨어요?",
          "original_form": "무슨 영화 만드셨어요?",
          "speaker_id": "2",
          "time": "20210826 11:02"
        },
        {
          "id": "MDRW2100016213.1.24",
          "form": "한번 보세요",
          "original_form": "한번 보세요",
          "speaker_id": "1",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100016213.1.25",
          "form": "전 서리사막의 도둑들 추천해요",
          "original_form": "전 서리사막의 도둑들 추천해요",
          "speaker_id": "1",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100016213.1.26",
          "form": "아 네",
          "original_form": "아 네",
          "speaker_id": "2",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100016213.1.27",
          "form": "진짜 영화퀄리티예요",
          "original_form": "진짜 영화퀄리티예요",
          "speaker_id": "1",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100016213.1.28",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100016213.1.29",
          "form": "엔딩만 생각하먼 눈물이 줄줄 납니다",
          "original_form": "엔딩만 생각하먼 눈물이 줄줄 납니다",
          "speaker_id": "1",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100016213.1.30",
          "form": "이름부터 퀄리티가 다른 것 같아요",
          "original_form": "이름부터 퀄리티가 다른 것 같아요",
          "speaker_id": "2",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100016213.1.31",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100016213.1.32",
          "form": "궁금하죠?",
          "original_form": "궁금하죠?",
          "speaker_id": "1",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100016213.1.33",
          "form": "제 알고리즘에 떠줬으면 좋겠어요",
          "original_form": "제 알고리즘에 떠줬으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100016213.1.34",
          "form": "근데 진짜 영화같아요",
          "original_form": "근데 진짜 영화같아요",
          "speaker_id": "1",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100016213.1.35",
          "form": "검색하시면 됩니다",
          "original_form": "검색하시면 됩니다",
          "speaker_id": "1",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100016213.1.36",
          "form": "직접 검색하는건 귀찮아서요..",
          "original_form": "직접 검색하는건 귀찮아서요..",
          "speaker_id": "2",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100016213.1.37",
          "form": "지금 타이핑 잘 하시는데요",
          "original_form": "지금 타이핑 잘 하시는데요",
          "speaker_id": "1",
          "time": "20210826 11:03"
        }
      ]
    }
