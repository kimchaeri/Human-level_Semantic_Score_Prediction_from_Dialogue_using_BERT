      "utterance": [
        {
          "id": "MDRW2100030778.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210904 12:26"
        },
        {
          "id": "MDRW2100030778.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210904 12:35"
        },
        {
          "id": "MDRW2100030778.1.3",
          "form": "날씨가 너무 좋아요!",
          "original_form": "날씨가 너무 좋아요!",
          "speaker_id": "1",
          "time": "20210904 12:35"
        },
        {
          "id": "MDRW2100030778.1.4",
          "form": "맞아요~ 학기중이신가요?",
          "original_form": "맞아요~ 학기중이신가요?",
          "speaker_id": "2",
          "time": "20210904 12:38"
        },
        {
          "id": "MDRW2100030778.1.5",
          "form": "앗 저는 직장인이에요",
          "original_form": "앗 저는 직장인이에요",
          "speaker_id": "1",
          "time": "20210904 13:09"
        },
        {
          "id": "MDRW2100030778.1.6",
          "form": "대학생이신가요?",
          "original_form": "대학생이신가요?",
          "speaker_id": "1",
          "time": "20210904 13:09"
        },
        {
          "id": "MDRW2100030778.1.7",
          "form": "앗 저도 학생 아니에요 ㅋㅋㅋ 그냥..백수요",
          "original_form": "앗 저도 학생 아니에요 ㅋㅋㅋ 그냥..백수요",
          "speaker_id": "2",
          "time": "20210904 13:22"
        },
        {
          "id": "MDRW2100030778.1.8",
          "form": "ㅎㅎㅎ쉬어야 에너지가 생기죠",
          "original_form": "ㅎㅎㅎ쉬어야 에너지가 생기죠",
          "speaker_id": "1",
          "time": "20210904 13:26"
        },
        {
          "id": "MDRW2100030778.1.9",
          "form": "식사는하셨나요?",
          "original_form": "식사는하셨나요?",
          "speaker_id": "1",
          "time": "20210904 13:26"
        },
        {
          "id": "MDRW2100030778.1.10",
          "form": "네 북엇국이랑ㅎㅎ 중고등학생 땐 급식으로 북엇국 나오면 되게 싫어했는데 이젠 없어서 못 먹네요",
          "original_form": "네 북엇국이랑ㅎㅎ 중고등학생 땐 급식으로 북엇국 나오면 되게 싫어했는데 이젠 없어서 못 먹네요",
          "speaker_id": "2",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100030778.1.11",
          "form": "좋아했던 급식이나 학식 있으세요?",
          "original_form": "좋아했던 급식이나 학식 있으세요?",
          "speaker_id": "2",
          "time": "20210904 13:33"
        },
        {
          "id": "MDRW2100030778.1.12",
          "form": "ㅎㅎ저는 그거 좋아했어요 야채튀김!",
          "original_form": "ㅎㅎ저는 그거 좋아했어요 야채튀김!",
          "speaker_id": "1",
          "time": "20210904 21:59"
        },
        {
          "id": "MDRW2100030778.1.13",
          "form": "북엇국좋죠 시원하고 감자랑같이",
          "original_form": "북엇국좋죠 시원하고 감자랑같이",
          "speaker_id": "1",
          "time": "20210904 21:59"
        },
        {
          "id": "MDRW2100030778.1.14",
          "form": "학생 때 좋아하는 과목은 뭐였어요??",
          "original_form": "학생 때 좋아하는 과목은 뭐였어요??",
          "speaker_id": "2",
          "time": "20210904 22:00"
        },
        {
          "id": "MDRW2100030778.1.15",
          "form": "저는 생물이요ㅎㅎㅎ 어떤과목좋아하셨나요?",
          "original_form": "저는 생물이요ㅎㅎㅎ 어떤과목좋아하셨나요?",
          "speaker_id": "1",
          "time": "20210904 22:01"
        },
        {
          "id": "MDRW2100030778.1.16",
          "form": "오 저두 생물좋아했어요 대박대박",
          "original_form": "오 저두 생물좋아했어요 대박대박",
          "speaker_id": "2",
          "time": "20210904 22:02"
        },
        {
          "id": "MDRW2100030778.1.17",
          "form": "이공계로 진학하셨어요?",
          "original_form": "이공계로 진학하셨어요?",
          "speaker_id": "2",
          "time": "20210904 22:02"
        },
        {
          "id": "MDRW2100030778.1.18",
          "form": "ㅎㅎ저는보건계열이요",
          "original_form": "ㅎㅎ저는보건계열이요",
          "speaker_id": "1",
          "time": "20210904 22:24"
        },
        {
          "id": "MDRW2100030778.1.19",
          "form": "어디분야세요?",
          "original_form": "어디분야세요?",
          "speaker_id": "1",
          "time": "20210904 22:24"
        },
        {
          "id": "MDRW2100030778.1.20",
          "form": "음 화확과로 진학은했는데 일은 완전 다른 분야 ㅋㅋㅋ",
          "original_form": "음 화확과로 진학은했는데 일은 완전 다른 분야 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:27"
        }
      ]
