      "utterance": [
        {
          "id": "MDRW2100010087.1.1",
          "form": "전 오늘부터 진짜 건강식단 들어갔어요",
          "original_form": "전 오늘부터 진짜 건강식단 들어갔어요",
          "speaker_id": "2",
          "time": "20210823 16:28"
        },
        {
          "id": "MDRW2100010087.1.2",
          "form": "어떻게 식단 구성하셨는데요?",
          "original_form": "어떻게 식단 구성하셨는데요?",
          "speaker_id": "1",
          "time": "20210823 16:29"
        },
        {
          "id": "MDRW2100010087.1.3",
          "form": "점심은 장어로 든든하게 먹었어요",
          "original_form": "점심은 장어로 든든하게 먹었어요",
          "speaker_id": "2",
          "time": "20210823 16:33"
        },
        {
          "id": "MDRW2100010087.1.4",
          "form": "아하 저는 채식이나 다이어트 식을 생각했었는데",
          "original_form": "아하 저는 채식이나 다이어트 식을 생각했었는데",
          "speaker_id": "1",
          "time": "20210823 16:34"
        },
        {
          "id": "MDRW2100010087.1.5",
          "form": "말그대로 건강식인가보네요",
          "original_form": "말그대로 건강식인가보네요",
          "speaker_id": "1",
          "time": "20210823 16:34"
        },
        {
          "id": "MDRW2100010087.1.6",
          "form": "직접 요리하셨어요",
          "original_form": "직접 요리하셨어요",
          "speaker_id": "1",
          "time": "20210823 16:35"
        },
        {
          "id": "MDRW2100010087.1.7",
          "form": "저녁엔 쉐이크 먹으려고요",
          "original_form": "저녁엔 쉐이크 먹으려고요",
          "speaker_id": "2",
          "time": "20210823 16:35"
        },
        {
          "id": "MDRW2100010087.1.8",
          "form": "아 그렇구나",
          "original_form": "아 그렇구나",
          "speaker_id": "1",
          "time": "20210823 16:36"
        },
        {
          "id": "MDRW2100010087.1.9",
          "form": "단백질 쉐이크?",
          "original_form": "단백질 쉐이크?",
          "speaker_id": "1",
          "time": "20210823 16:36"
        },
        {
          "id": "MDRW2100010087.1.10",
          "form": "네 근데 노니 곡물맛이라서 기대가 안되네요ㅜ",
          "original_form": "네 근데 노니 곡물맛이라서 기대가 안되네요ㅜ",
          "speaker_id": "2",
          "time": "20210823 17:24"
        },
        {
          "id": "MDRW2100010087.1.11",
          "form": "노니요?",
          "original_form": "노니요?",
          "speaker_id": "1",
          "time": "20210823 20:54"
        },
        {
          "id": "MDRW2100010087.1.12",
          "form": "저도 한번도 안먹어봤어요",
          "original_form": "저도 한번도 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210823 20:56"
        },
        {
          "id": "MDRW2100010087.1.13",
          "form": "동생이 주고가써요 ㅋㅋ",
          "original_form": "동생이 주고가써요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:56"
        },
        {
          "id": "MDRW2100010087.1.14",
          "form": "건강에 좋은건가요",
          "original_form": "건강에 좋은건가요",
          "speaker_id": "1",
          "time": "20210823 20:57"
        },
        {
          "id": "MDRW2100010087.1.15",
          "form": "??",
          "original_form": "??",
          "speaker_id": "1",
          "time": "20210823 20:57"
        },
        {
          "id": "MDRW2100010087.1.16",
          "form": "그냥 단백질 쉐이크 같은데 그렇게 적혀있더라고요",
          "original_form": "그냥 단백질 쉐이크 같은데 그렇게 적혀있더라고요",
          "speaker_id": "2",
          "time": "20210823 20:57"
        },
        {
          "id": "MDRW2100010087.1.17",
          "form": "다이어트 때문에 식단관리하시는 건가요?",
          "original_form": "다이어트 때문에 식단관리하시는 건가요?",
          "speaker_id": "1",
          "time": "20210823 21:33"
        },
        {
          "id": "MDRW2100010087.1.18",
          "form": "네 정말 확찐자가 되어서요ㅜ",
          "original_form": "네 정말 확찐자가 되어서요ㅜ",
          "speaker_id": "2",
          "time": "20210823 21:38"
        },
        {
          "id": "MDRW2100010087.1.19",
          "form": "아 저도 결혼하고 체중이 급격하게 불어서",
          "original_form": "아 저도 결혼하고 체중이 급격하게 불어서",
          "speaker_id": "1",
          "time": "20210823 21:45"
        },
        {
          "id": "MDRW2100010087.1.20",
          "form": "운동하면서 샐러드 먹고있는데",
          "original_form": "운동하면서 샐러드 먹고있는데",
          "speaker_id": "1",
          "time": "20210823 21:45"
        },
        {
          "id": "MDRW2100010087.1.21",
          "form": "식욕참는게 너무어려워요",
          "original_form": "식욕참는게 너무어려워요",
          "speaker_id": "1",
          "time": "20210823 21:45"
        },
        {
          "id": "MDRW2100010087.1.22",
          "form": "저도요 딱 저녁만 쉐이크로 바꾸려고요",
          "original_form": "저도요 딱 저녁만 쉐이크로 바꾸려고요",
          "speaker_id": "2",
          "time": "20210823 21:50"
        },
        {
          "id": "MDRW2100010087.1.23",
          "form": "점심도 탄수화물 줄이려고 밥안먹고 장어만 먹음 ㅋㅋ",
          "original_form": "점심도 탄수화물 줄이려고 밥안먹고 장어만 먹음 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:50"
        },
        {
          "id": "MDRW2100010087.1.24",
          "form": "그럼 아침은 평소처럼 일반식으로 드시나요?",
          "original_form": "그럼 아침은 평소처럼 일반식으로 드시나요?",
          "speaker_id": "1",
          "time": "20210823 21:58"
        },
        {
          "id": "MDRW2100010087.1.25",
          "form": "아침은어영부영하다가 안먹었어요",
          "original_form": "아침은어영부영하다가 안먹었어요",
          "speaker_id": "2",
          "time": "20210823 22:01"
        },
        {
          "id": "MDRW2100010087.1.26",
          "form": "근데 낼부턴 비타민? 이거마시려고욥ㅋㅋ",
          "original_form": "근데 낼부턴 비타민? 이거마시려고욥ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 22:01"
        }
      ]
    }
