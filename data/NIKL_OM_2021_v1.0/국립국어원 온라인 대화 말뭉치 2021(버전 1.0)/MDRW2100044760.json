        {
          "id": "MDRW2100044760.1.1",
          "form": "어떤 게 하죠",
          "original_form": "어떤 게 하죠",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044760.1.2",
          "form": "탄산음료 종류예요",
          "original_form": "탄산음료 종류예요",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044760.1.3",
          "form": "소화 안 될 때 탄산음료 먹으면 좋은데",
          "original_form": "소화 안 될 때 탄산음료 먹으면 좋은데",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044760.1.4",
          "form": "실제로 소화가 되진 않아요",
          "original_form": "실제로 소화가 되진 않아요",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044760.1.5",
          "form": "하지만 기분상",
          "original_form": "하지만 기분상",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044760.1.6",
          "form": "맞아요 트림 한번 하면",
          "original_form": "맞아요 트림 한번 하면",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044760.1.7",
          "form": "어떤 거 세일해요",
          "original_form": "어떤 거 세일해요",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044760.1.8",
          "form": "코카콜라 제로",
          "original_form": "코카콜라 제로",
          "speaker_id": "1",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044760.1.9",
          "form": "그리고요",
          "original_form": "그리고요",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044760.1.10",
          "form": "밀키스 세일해요",
          "original_form": "밀키스 세일해요",
          "speaker_id": "1",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044760.1.11",
          "form": "밀키 쓰는 안 먹어 봐서",
          "original_form": "밀키 쓰는 안 먹어 봐서",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044760.1.12",
          "form": "맛이 괜찮아요 달달하고",
          "original_form": "맛이 괜찮아요 달달하고",
          "speaker_id": "1",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044760.1.13",
          "form": "그럼 한번 먹어 봐야겠네요",
          "original_form": "그럼 한번 먹어 봐야겠네요",
