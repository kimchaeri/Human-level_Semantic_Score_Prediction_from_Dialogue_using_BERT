      "utterance": [
        {
          "id": "MDRW2100047521.1.1",
          "form": "식성이 비슷하신거같네요ㅋㅋㅋ",
          "original_form": "식성이 비슷하신거같네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100047521.1.2",
          "form": "ㅋㅋㅋㅋㅋ그러게요",
          "original_form": "ㅋㅋㅋㅋㅋ그러게요",
          "speaker_id": "2",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100047521.1.3",
          "form": "특히 좋아하는 음식 있으세요?",
          "original_form": "특히 좋아하는 음식 있으세요?",
          "speaker_id": "2",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100047521.1.4",
          "form": "초밥이요",
          "original_form": "초밥이요",
          "speaker_id": "1",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100047521.1.5",
          "form": "저도 초밥좋아해요",
          "original_form": "저도 초밥좋아해요",
          "speaker_id": "2",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100047521.1.6",
          "form": "무슨 초밥 좋아하세요?",
          "original_form": "무슨 초밥 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100047521.1.7",
          "form": "전진짜",
          "original_form": "전진짜",
          "speaker_id": "1",
          "time": "20210827 00:18"
        },
        {
          "id": "MDRW2100047521.1.8",
          "form": "다!",
          "original_form": "다!",
          "speaker_id": "1",
          "time": "20210827 00:18"
        },
        {
          "id": "MDRW2100047521.1.9",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "1",
          "time": "20210827 00:18"
        },
        {
          "id": "MDRW2100047521.1.10",
          "form": "저도 초밥 진짜 좋아해요 ㅠㅠ",
          "original_form": "저도 초밥 진짜 좋아해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 00:21"
        },
        {
          "id": "MDRW2100047521.1.11",
          "form": "다 좋아하지만 전 생새우!!",
          "original_form": "다 좋아하지만 전 생새우!!",
          "speaker_id": "2",
          "time": "20210827 00:21"
        },
        {
          "id": "MDRW2100047521.1.12",
          "form": "연어vs광ㅈ어?",
          "original_form": "연어vs광ㅈ어?",
          "speaker_id": "1",
          "time": "20210827 00:21"
        },
        {
          "id": "MDRW2100047521.1.13",
          "form": "하 어렵네요",
          "original_form": "하 어렵네요",
          "speaker_id": "2",
          "time": "20210827 00:21"
        },
        {
          "id": "MDRW2100047521.1.14",
          "form": "name1님은?",
