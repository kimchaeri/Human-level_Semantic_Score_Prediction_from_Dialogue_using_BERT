          "time": "20210827 00:27"
        },
        {
          "id": "MDRW2100047521.1.17",
          "form": "닥!!!!!",
          "original_form": "닥!!!!!",
          "speaker_id": "1",
          "time": "20210827 00:27"
        },
        {
          "id": "MDRW2100047521.1.18",
          "form": "광 어 입니다",
          "original_form": "광 어 입니다",
          "speaker_id": "1",
          "time": "20210827 00:27"
        },
        {
          "id": "MDRW2100047521.1.19",
          "form": "광어 너무 맛잇죠 ㅠㅠ",
          "original_form": "광어 너무 맛잇죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 00:28"
        },
        {
          "id": "MDRW2100047521.1.20",
          "form": "전 근데 광어만큼 연어도 좋아해요 ㅋㅋㅋㅋㅋ",
          "original_form": "전 근데 광어만큼 연어도 좋아해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:28"
        },
        {
          "id": "MDRW2100047521.1.21",
          "form": "ㅋㅋㅋㅋ저는",
          "original_form": "ㅋㅋㅋㅋ저는",
          "speaker_id": "1",
          "time": "20210827 00:28"
        },
        {
          "id": "MDRW2100047521.1.22",
          "form": "어느순간연어는 느끼해졌더라고요",
          "original_form": "어느순간연어는 느끼해졌더라고요",
          "speaker_id": "1",
          "time": "20210827 00:28"
        },
        {
          "id": "MDRW2100047521.1.23",
          "form": "그렇게 찐광어파가",
          "original_form": "그렇게 찐광어파가",
          "speaker_id": "1",
          "time": "20210827 00:28"
        },
        {
          "id": "MDRW2100047521.1.24",
          "form": "되었습니다",
          "original_form": "되었습니다",
          "speaker_id": "1",
          "time": "20210827 00:28"
        },
        {
          "id": "MDRW2100047521.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그럼 참치도 느끼하세요?",
