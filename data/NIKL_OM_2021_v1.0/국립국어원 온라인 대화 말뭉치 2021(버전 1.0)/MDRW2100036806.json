      "utterance": [
        {
          "id": "MDRW2100036806.1.1",
          "form": "좋아하는 굽네치킨 메뉴가잇으세여?",
          "original_form": "좋아하는 굽네치킨 메뉴가잇으세여?",
          "speaker_id": "2",
          "time": "20210831 12:34"
        },
        {
          "id": "MDRW2100036806.1.2",
          "form": "고.추.바.사.삭이요",
          "original_form": "고.추.바.사.삭이요",
          "speaker_id": "1",
          "time": "20210831 12:34"
        },
        {
          "id": "MDRW2100036806.1.3",
          "form": "오 저는 좋아하는게 진짜 많아요.",
          "original_form": "오 저는 좋아하는게 진짜 많아요.",
          "speaker_id": "2",
          "time": "20210831 12:35"
        },
        {
          "id": "MDRW2100036806.1.4",
          "form": "",
          "original_form": "{emoji:♥}{emoji:♥}",
          "speaker_id": "2",
          "time": "20210831 12:36"
        },
        {
          "id": "MDRW2100036806.1.5",
          "form": "뭐요?",
          "original_form": "뭐요?",
          "speaker_id": "1",
          "time": "20210831 12:36"
        },
        {
          "id": "MDRW2100036806.1.6",
          "form": "오리지널, 볼케이노여",
          "original_form": "오리지널, 볼케이노여",
          "speaker_id": "2",
          "time": "20210831 12:36"
        },
        {
          "id": "MDRW2100036806.1.7",
          "form": "오리지널 별로에요..",
          "original_form": "오리지널 별로에요..",
          "speaker_id": "1",
          "time": "20210831 12:39"
        },
        {
          "id": "MDRW2100036806.1.8",
          "form": "그럏군요. 저는 가끔은 담백한것도 좋아하거든요",
          "original_form": "그럏군요. 저는 가끔은 담백한것도 좋아하거든요",
          "speaker_id": "2",
          "time": "20210831 12:40"
        },
        {
          "id": "MDRW2100036806.1.9",
          "form": "담백함을 넘어섰죠 심심해요",
          "original_form": "담백함을 넘어섰죠 심심해요",
          "speaker_id": "1",
          "time": "20210831 12:41"
        },
        {
          "id": "MDRW2100036806.1.10",
          "form": "저는좋더라고요  처음먹엇을때 좋앗어요.",
          "original_form": "저는좋더라고요  처음먹엇을때 좋앗어요.",
          "speaker_id": "2",
          "time": "20210831 12:45"
        },
        {
          "id": "MDRW2100036806.1.11",
          "form": "고추바사삭의 매력이뭔가요",
          "original_form": "고추바사삭의 매력이뭔가요",
          "speaker_id": "2",
          "time": "20210831 12:45"
        },
        {
          "id": "MDRW2100036806.1.12",
          "form": "그냥먹어도 맛있고 마요소스도 맛있어뇨",
          "original_form": "그냥먹어도 맛있고 마요소스도 맛있어뇨",
          "speaker_id": "1",
          "time": "20210831 12:46"
        },
        {
          "id": "MDRW2100036806.1.13",
          "form": "오 그렇군요. 마요소스 두고두고 드세엽",
          "original_form": "오 그렇군요. 마요소스 두고두고 드세엽",
          "speaker_id": "2",
          "time": "20210831 12:48"
        },
        {
          "id": "MDRW2100036806.1.14",
          "form": "마요 맛잇ㄹ어요",
          "original_form": "마요 맛잇ㄹ어요",
          "speaker_id": "1",
          "time": "20210831 12:50"
        },
        {
          "id": "MDRW2100036806.1.15",
          "form": "마요네즈 메니아인가봐요",
          "original_form": "마요네즈 메니아인가봐요",
          "speaker_id": "2",
          "time": "20210831 12:50"
        }
      ]
