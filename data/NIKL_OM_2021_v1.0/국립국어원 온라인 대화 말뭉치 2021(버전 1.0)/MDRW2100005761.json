        {
          "id": "MDRW2100005761.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 11:13"
        },
        {
          "id": "MDRW2100005761.1.2",
          "form": "네~ 안녕하세요",
          "original_form": "네~ 안녕하세요",
          "speaker_id": "2",
          "time": "20210907 11:14"
        },
        {
          "id": "MDRW2100005761.1.3",
          "form": "투자하시는거있으세요?",
          "original_form": "투자하시는거있으세요?",
          "speaker_id": "1",
          "time": "20210907 11:15"
        },
        {
          "id": "MDRW2100005761.1.4",
          "form": "아뇨 딱히 없는데.ㅜㅜ",
          "original_form": "아뇨 딱히 없는데.ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 11:20"
        },
        {
          "id": "MDRW2100005761.1.5",
          "form": "투자하시는거 있으세요??",
          "original_form": "투자하시는거 있으세요??",
          "speaker_id": "2",
          "time": "20210907 11:20"
        },
        {
          "id": "MDRW2100005761.1.6",
          "form": "저는..주식?ㅎㅎ",
          "original_form": "저는..주식?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 11:21"
        },
        {
          "id": "MDRW2100005761.1.7",
          "form": "헉 주식하세용??ㅋㅋ",
          "original_form": "헉 주식하세용??ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 11:51"
        },
        {
          "id": "MDRW2100005761.1.8",
          "form": "전 그런거는 새가슴이라.ㅡㅜㅠㅠㅋㅋㅋㅋ",
          "original_form": "전 그런거는 새가슴이라.ㅡㅜㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 11:51"
        },
        {
          "id": "MDRW2100005761.1.9",
          "form": "ㅎㅎ앗 아주소소하게해요",
          "original_form": "ㅎㅎ앗 아주소소하게해요",
          "speaker_id": "1",
          "time": "20210907 11:53"
        },
        {
          "id": "MDRW2100005761.1.10",
          "form": "ㅎ.ㅎ 어때용 잘 오르고있나요??",
          "original_form": "ㅎ.ㅎ 어때용 잘 오르고있나요??",
          "speaker_id": "2",
          "time": "20210907 11:54"
        },
        {
          "id": "MDRW2100005761.1.11",
          "form": "ㅎㅎㅎ치킨값정도?헤헤",
          "original_form": "ㅎㅎㅎ치킨값정도?헤헤",
          "speaker_id": "1",
          "time": "20210907 11:56"
        },
        {
          "id": "MDRW2100005761.1.12",
          "form": "조금이라도 이득보면 나이스죠 ㅋㅋㅋㅋㅋ",
          "original_form": "조금이라도 이득보면 나이스죠 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:18"
        },
        {
          "id": "MDRW2100005761.1.13",
          "form": "제 주변에도 주식하는애들 많은데",
          "original_form": "제 주변에도 주식하는애들 많은데",
          "speaker_id": "2",
          "time": "20210907 12:18"
        },
        {
          "id": "MDRW2100005761.1.14",
          "form": "조그맣게하는건 괜찮은거같더라구요 ㅋㅋ",
          "original_form": "조그맣게하는건 괜찮은거같더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:18"
        },
        {
          "id": "MDRW2100005761.1.15",
          "form": "ㅎㅎㅎㄱ크게하면스트레스받는다더라고요",
          "original_form": "ㅎㅎㅎㄱ크게하면스트레스받는다더라고요",
          "speaker_id": "1",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100005761.1.16",
          "form": "네....어떤애가 하루종일 핸드폰만보고있는거보면...",
          "original_form": "네....어떤애가 하루종일 핸드폰만보고있는거보면...",
          "speaker_id": "2",
          "time": "20210907 12:23"
        },
        {
          "id": "MDRW2100005761.1.17",
          "form": "그럴거같아요 ㅠㅠ ㅋㅋㅋㅋㅋ",
          "original_form": "그럴거같아요 ㅠㅠ ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:23"
        },
        {
          "id": "MDRW2100005761.1.18",
          "form": "그리고 멘탈도 엄청나갈거같네여..큰거다보니",
          "original_form": "그리고 멘탈도 엄청나갈거같네여..큰거다보니",
          "speaker_id": "2",
          "time": "20210907 12:23"
        },
        {
          "id": "MDRW2100005761.1.19",
          "form": "코인이그렇게장난아니래요ㅎㄷㄷ",
          "original_form": "코인이그렇게장난아니래요ㅎㄷㄷ",
