      "utterance": [
        {
          "id": "MDRW2100042547.1.1",
          "form": "넌 햄버거집 가면 세트로 시켜?",
          "original_form": "넌 햄버거집 가면 세트로 시켜?",
          "speaker_id": "2",
          "time": "20210906 22:45"
        },
        {
          "id": "MDRW2100042547.1.2",
          "form": "응 콜라안먹지만",
          "original_form": "응 콜라안먹지만",
          "speaker_id": "1",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042547.1.3",
          "form": "감튀땜에 시켜",
          "original_form": "감튀땜에 시켜",
          "speaker_id": "1",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042547.1.4",
          "form": "오 나도 콜라 안 먹는데",
          "original_form": "오 나도 콜라 안 먹는데",
          "speaker_id": "2",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042547.1.5",
          "form": "난 감튀를 참아",
          "original_form": "난 감튀를 참아",
          "speaker_id": "2",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042547.1.6",
          "form": "ㅋㅋㅋㅋ단품 시켜",
          "original_form": "ㅋㅋㅋㅋ단품 시켜",
          "speaker_id": "2",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042547.1.7",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042547.1.8",
          "form": "감튀참냐",
          "original_form": "감튀참냐",
          "speaker_id": "1",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042547.1.9",
          "form": "왜",
          "original_form": "왜",
          "speaker_id": "1",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042547.1.10",
          "form": "안 먹는 콜라까지 포함해서",
          "original_form": "안 먹는 콜라까지 포함해서",
          "speaker_id": "2",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042547.1.11",
          "form": "가격을 올려 낼 의사가 없어",
          "original_form": "가격을 올려 낼 의사가 없어",
          "speaker_id": "2",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100042547.1.12",
          "form": "감튀를 참는다니",
          "original_form": "감튀를 참는다니",
          "speaker_id": "1",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100042547.1.13",
          "form": "정말 먹고싶을 땐 그래도 세트 시켜",
          "original_form": "정말 먹고싶을 땐 그래도 세트 시켜",
          "speaker_id": "2",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100042547.1.14",
          "form": "어째서ㅠㅠㅠ",
          "original_form": "어째서ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100042547.1.15",
          "form": "ㅋㅋㅌㅋㅌㅋㅋㅋㅋㅋ사실 나도 감튀 좋아해......",
          "original_form": "ㅋㅋㅌㅋㅌㅋㅋㅋㅋㅋ사실 나도 감튀 좋아해......",
          "speaker_id": "2",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100042547.1.16",
          "form": "근데 또 안 먹는다고 죽는 건 아니길래",
          "original_form": "근데 또 안 먹는다고 죽는 건 아니길래",
          "speaker_id": "2",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100042547.1.17",
          "form": "먹어",
          "original_form": "먹어",
          "speaker_id": "1",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100042547.1.18",
          "form": "돈을 아끼는 편",
          "original_form": "돈을 아끼는 편",
          "speaker_id": "2",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100042547.1.19",
          "form": "먹고 죽자",
          "original_form": "먹고 죽자",
          "speaker_id": "1",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100042547.1.20",
          "form": "알았어ㅋㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "알았어ㅋㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100042547.1.21",
          "form": "다음엔 감튀 먹을게",
          "original_form": "다음엔 감튀 먹을게",
          "speaker_id": "2",
          "time": "20210906 22:47"
        },
        {
          "id": "MDRW2100042547.1.22",
          "form": "좋아하는데도 참는건",
          "original_form": "좋아하는데도 참는건",
          "speaker_id": "1",
          "time": "20210906 22:48"
        },
        {
          "id": "MDRW2100042547.1.23",
          "form": "너무슬퍼",
          "original_form": "너무슬퍼",
          "speaker_id": "1",
          "time": "20210906 22:48"
        },
        {
          "id": "MDRW2100042547.1.24",
          "form": "그건 그래..ㅎ 먹고싶은데 안 먹는 건 괴로워",
          "original_form": "그건 그래..ㅎ 먹고싶은데 안 먹는 건 괴로워",
