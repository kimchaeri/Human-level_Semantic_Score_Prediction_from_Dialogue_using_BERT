      "utterance": [
        {
          "id": "MDRW2100017242.1.1",
          "form": "아아 그럼 중성화 시키셨어요?",
          "original_form": "아아 그럼 중성화 시키셨어요?",
          "speaker_id": "2",
          "time": "20210826 18:14"
        },
        {
          "id": "MDRW2100017242.1.2",
          "form": "네 지금 2살이라",
          "original_form": "네 지금 2살이라",
          "speaker_id": "1",
          "time": "20210826 18:14"
        },
        {
          "id": "MDRW2100017242.1.3",
          "form": "ㅋㅋㅋㅋ 엄청 컸겠네요",
          "original_form": "ㅋㅋㅋㅋ 엄청 컸겠네요",
          "speaker_id": "2",
          "time": "20210826 18:14"
        },
        {
          "id": "MDRW2100017242.1.4",
          "form": "진작 시켰어요",
          "original_form": "진작 시켰어요",
          "speaker_id": "1",
          "time": "20210826 18:14"
        },
        {
          "id": "MDRW2100017242.1.5",
          "form": "아 그랬구나",
          "original_form": "아 그랬구나",
          "speaker_id": "2",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017242.1.6",
          "form": "클 거라 생각은했는데 엄청크더라구요",
          "original_form": "클 거라 생각은했는데 엄청크더라구요",
          "speaker_id": "1",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017242.1.7",
          "form": "2세는 없겠지만",
          "original_form": "2세는 없겠지만",
          "speaker_id": "2",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017242.1.8",
          "form": "그래도 이해해요",
          "original_form": "그래도 이해해요",
          "speaker_id": "2",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017242.1.9",
          "form": "건강하게  사는 게 중요하니까요",
          "original_form": "건강하게  사는 게 중요하니까요",
          "speaker_id": "1",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017242.1.10",
          "form": "집에 와서 안기면",
          "original_form": "집에 와서 안기면",
          "speaker_id": "2",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017242.1.11",
          "form": "뒤로 쓰러지시겠어요",
          "original_form": "뒤로 쓰러지시겠어요",
          "speaker_id": "2",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017242.1.12",
          "form": "안길거라고 생각하시나요",
          "original_form": "안길거라고 생각하시나요",
          "speaker_id": "1",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017242.1.13",
          "form": "ㅎㅎ...",
          "original_form": "ㅎㅎ...",
          "speaker_id": "1",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017242.1.14",
          "form": "몇키로에요?",
          "original_form": "몇키로에요?",
          "speaker_id": "2",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017242.1.15",
          "form": "아니에요?",
          "original_form": "아니에요?",
          "speaker_id": "2",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017242.1.16",
          "form": "안기지않아요 미는거지",
          "original_form": "안기지않아요 미는거지",
          "speaker_id": "1",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017242.1.17",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017242.1.18",
          "form": "미는 거 ㅋㅋㅋㅋ",
          "original_form": "미는 거 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:16"
        },
        {
          "id": "MDRW2100017242.1.19",
          "form": "지금 25키로에요",
          "original_form": "지금 25키로에요",
          "speaker_id": "1",
          "time": "20210826 18:16"
        },
        {
          "id": "MDRW2100017242.1.20",
          "form": "표현이 웃기네요 ㅋㅋㅋ",
          "original_form": "표현이 웃기네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:16"
        },
        {
          "id": "MDRW2100017242.1.21",
          "form": "우와......",
          "original_form": "우와......",
          "speaker_id": "2",
          "time": "20210826 18:16"
        },
        {
          "id": "MDRW2100017242.1.22",
          "form": "한번 업으면 웨이트네요 ㅋㅋㅋ",
          "original_form": "한번 업으면 웨이트네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:16"
        },
        {
          "id": "MDRW2100017242.1.23",
          "form": "못들어요 이제 ㅋㅋ",
          "original_form": "못들어요 이제 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:16"
        },
        {
          "id": "MDRW2100017242.1.24",
          "form": "무거워서",
          "original_form": "무거워서",
          "speaker_id": "1",
          "time": "20210826 18:16"
        },
        {
          "id": "MDRW2100017242.1.25",
          "form": "2년이면 성장 다 끝난 건가요?",
          "original_form": "2년이면 성장 다 끝난 건가요?",
          "speaker_id": "2",
          "time": "20210826 18:16"
        },
        {
          "id": "MDRW2100017242.1.26",
          "form": "아무래도 더 안클거같아요",
          "original_form": "아무래도 더 안클거같아요",
          "speaker_id": "1",
          "time": "20210826 18:17"
        }
