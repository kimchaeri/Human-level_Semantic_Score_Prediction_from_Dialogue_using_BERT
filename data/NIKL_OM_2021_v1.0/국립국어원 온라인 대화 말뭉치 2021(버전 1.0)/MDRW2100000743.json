        },
        {
          "id": "MDRW2100000743.1.2",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100000743.1.3",
          "form": "스타벅스",
          "original_form": "스타벅스",
          "speaker_id": "1",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100000743.1.4",
          "form": "커피먹게?",
          "original_form": "커피먹게?",
          "speaker_id": "2",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100000743.1.5",
          "form": "비아가 맛있음",
          "original_form": "비아가 맛있음",
          "speaker_id": "1",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100000743.1.6",
          "form": "비아가 뭐야?",
          "original_form": "비아가 뭐야?",
          "speaker_id": "2",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100000743.1.7",
          "form": "스틱커피같은거임",
          "original_form": "스틱커피같은거임",
          "speaker_id": "1",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100000743.1.8",
          "form": "집에서 먹는거야?",
          "original_form": "집에서 먹는거야?",
          "speaker_id": "2",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100000743.1.9",
          "form": "응 열두개들어있음",
          "original_form": "응 열두개들어있음",
          "speaker_id": "1",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100000743.1.10",
          "form": "열두개 너무 적다",
          "original_form": "열두개 너무 적다",
          "speaker_id": "2",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100000743.1.11",
          "form": "근데 커피한잔에 비하면 나름 적당함",
          "original_form": "근데 커피한잔에 비하면 나름 적당함",
          "speaker_id": "1",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100000743.1.12",
          "form": "스벅은 커피빼고 별루인듯",
          "original_form": "스벅은 커피빼고 별루인듯",
          "speaker_id": "2",
          "time": "20210819 18:54"
        },
        {
          "id": "MDRW2100000743.1.13",
          "form": "아메리카노가 진리",
          "original_form": "아메리카노가 진리",
          "speaker_id": "1",
          "time": "20210819 18:54"
        },
        {
          "id": "MDRW2100000743.1.14",
          "form": "스벅 바로 앞에 있다",
          "original_form": "스벅 바로 앞에 있다",
          "speaker_id": "2",
          "time": "20210819 18:54"
        },
        {
          "id": "MDRW2100000743.1.15",
          "form": "일단가서 비아구매해야겠다",
          "original_form": "일단가서 비아구매해야겠다",
