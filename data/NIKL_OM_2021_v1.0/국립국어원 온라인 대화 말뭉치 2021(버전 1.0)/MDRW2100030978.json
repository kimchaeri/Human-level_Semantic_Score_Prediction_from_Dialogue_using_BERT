      "utterance": [
        {
          "id": "MDRW2100030978.1.1",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210904 15:02"
        },
        {
          "id": "MDRW2100030978.1.2",
          "form": "일본 좋아하쥬",
          "original_form": "일본 좋아하쥬",
          "speaker_id": "2",
          "time": "20210904 15:03"
        },
        {
          "id": "MDRW2100030978.1.3",
          "form": "아 어디 가보셨어요?",
          "original_form": "아 어디 가보셨어요?",
          "speaker_id": "1",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100030978.1.4",
          "form": "오사카만 가봤어요",
          "original_form": "오사카만 가봤어요",
          "speaker_id": "2",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100030978.1.5",
          "form": "식도락여행을 좋아해서..ㅋㅋ",
          "original_form": "식도락여행을 좋아해서..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100030978.1.6",
          "form": "아ㅋㅋ타코야끼 드셨겠네요",
          "original_form": "아ㅋㅋ타코야끼 드셨겠네요",
          "speaker_id": "1",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100030978.1.7",
          "form": "우동이랑",
          "original_form": "우동이랑",
          "speaker_id": "1",
          "time": "20210904 15:08"
        },
        {
          "id": "MDRW2100030978.1.8",
          "form": "타코야끼 오코노미야끼 라멘",
          "original_form": "타코야끼 오코노미야끼 라멘",
          "speaker_id": "2",
          "time": "20210904 15:14"
        },
        {
          "id": "MDRW2100030978.1.9",
          "form": "배터지게 먹었죠ㅋㅋ",
          "original_form": "배터지게 먹었죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:14"
        },
        {
          "id": "MDRW2100030978.1.10",
          "form": "어디좋아하세요",
          "original_form": "어디좋아하세요",
          "speaker_id": "2",
          "time": "20210904 15:14"
        },
        {
          "id": "MDRW2100030978.1.11",
          "form": "저는 유럽쪽이 좋았던거같아요",
          "original_form": "저는 유럽쪽이 좋았던거같아요",
          "speaker_id": "1",
          "time": "20210904 15:15"
        },
        {
          "id": "MDRW2100030978.1.12",
          "form": "어디 어디 가봤어요?",
          "original_form": "어디 어디 가봤어요?",
          "speaker_id": "2",
          "time": "20210904 15:15"
        },
        {
          "id": "MDRW2100030978.1.13",
          "form": "저는 일본 홍콩 호주 유럽이용",
          "original_form": "저는 일본 홍콩 호주 유럽이용",
          "speaker_id": "1",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100030978.1.14",
          "form": "와 많이 가보셨구나",
          "original_form": "와 많이 가보셨구나",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100030978.1.15",
          "form": "유럽가서 스테이크 먹어보고싶네요",
          "original_form": "유럽가서 스테이크 먹어보고싶네요",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100030978.1.16",
          "form": "호주가 스테이크 정말싸요",
          "original_form": "호주가 스테이크 정말싸요",
          "speaker_id": "1",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100030978.1.17",
          "form": "일주일에 하루 할인하는날 가면",
          "original_form": "일주일에 하루 할인하는날 가면",
          "speaker_id": "1",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100030978.1.18",
          "form": "3달러던가?ㅋㅋ",
          "original_form": "3달러던가?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100030978.1.19",
          "form": "3달러가 얼만큼일까요",
          "original_form": "3달러가 얼만큼일까요",
          "speaker_id": "2",
          "time": "20210904 16:32"
        },
        {
          "id": "MDRW2100030978.1.20",
          "form": "1kg??",
          "original_form": "1kg??",
          "speaker_id": "2",
          "time": "20210904 16:32"
        },
        {
          "id": "MDRW2100030978.1.21",
          "form": "레어로 소금찍어먹으면맛있는데",
          "original_form": "레어로 소금찍어먹으면맛있는데",
          "speaker_id": "2",
          "time": "20210904 16:32"
        },
        {
          "id": "MDRW2100030978.1.22",
          "form": "그냥 1인분이요ㅋㅋ",
          "original_form": "그냥 1인분이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:35"
        },
        {
          "id": "MDRW2100030978.1.23",
          "form": "호주가 소고기가 싸어",
          "original_form": "호주가 소고기가 싸어",
          "speaker_id": "1",
          "time": "20210904 16:35"
        },
        {
          "id": "MDRW2100030978.1.24",
          "form": "와 근데 진짜 좋긴하다 1인분 3달라라니",
          "original_form": "와 근데 진짜 좋긴하다 1인분 3달라라니",
          "speaker_id": "2",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100030978.1.25",
          "form": "질리도록 먹을수 있겠네요",
          "original_form": "질리도록 먹을수 있겠네요",
          "speaker_id": "2",
          "time": "20210904 16:45"
        },
        {
          "id": "MDRW2100030978.1.26",
          "form": "휴 강강술래 고기 맛있네요",
          "original_form": "휴 강강술래 고기 맛있네요",
          "speaker_id": "2",
