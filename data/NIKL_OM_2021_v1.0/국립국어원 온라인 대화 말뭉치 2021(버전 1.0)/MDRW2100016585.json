      "utterance": [
        {
          "id": "MDRW2100016585.1.1",
          "form": "종교있으세요?",
          "original_form": "종교있으세요?",
          "speaker_id": "1",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100016585.1.2",
          "form": "불교를 약하게 믿고있어요 name1님은요?",
          "original_form": "불교를 약하게 믿고있어요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016585.1.3",
          "form": "무교예요~",
          "original_form": "무교예요~",
          "speaker_id": "1",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100016585.1.4",
          "form": "근데 절은 가봤어요",
          "original_form": "근데 절은 가봤어요",
          "speaker_id": "1",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100016585.1.5",
          "form": "그럼 다른 종교믿는 사람들이 자기종교 위주로 스케쥴 짜는거 어찌생각하세요?",
          "original_form": "그럼 다른 종교믿는 사람들이 자기종교 위주로 스케쥴 짜는거 어찌생각하세요?",
          "speaker_id": "2",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100016585.1.6",
          "form": "기독교인이 일요일 모임 거부한다거나 하는거요",
          "original_form": "기독교인이 일요일 모임 거부한다거나 하는거요",
          "speaker_id": "2",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100016585.1.7",
          "form": "솔직히 그렇게까지해야하나 싶어요",
          "original_form": "솔직히 그렇게까지해야하나 싶어요",
          "speaker_id": "1",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100016585.1.8",
          "form": "제가 신앙심이 없어서그런거겠지만! 피해만 안 준다면..상관없어요ㅎ",
          "original_form": "제가 신앙심이 없어서그런거겠지만! 피해만 안 준다면..상관없어요ㅎ",
          "speaker_id": "1",
          "time": "20210826 14:02"
        },
        {
          "id": "MDRW2100016585.1.9",
          "form": "저는 종교자체를 별로 안좋아해요",
          "original_form": "저는 종교자체를 별로 안좋아해요",
          "speaker_id": "2",
          "time": "20210826 14:14"
        },
        {
          "id": "MDRW2100016585.1.10",
          "form": "필요악이란 생각이들어요",
          "original_form": "필요악이란 생각이들어요",
          "speaker_id": "2",
          "time": "20210826 14:15"
        },
        {
          "id": "MDRW2100016585.1.11",
          "form": "어.. 그런데 불교는 조금 믿으시는건가요?",
          "original_form": "어.. 그런데 불교는 조금 믿으시는건가요?",
          "speaker_id": "1",
          "time": "20210826 14:16"
        },
        {
          "id": "MDRW2100016585.1.12",
          "form": "네 아주조금요 ㅎㅎㅎ",
          "original_form": "네 아주조금요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 14:20"
        },
        {
          "id": "MDRW2100016585.1.13",
          "form": "믿게 된 동기가 있으세요?",
          "original_form": "믿게 된 동기가 있으세요?",
          "speaker_id": "1",
          "time": "20210826 14:21"
        },
        {
          "id": "MDRW2100016585.1.14",
          "form": "주변보면 나중에 종교를 믿게된 경우는 계기가 있더라고요 대부분",
          "original_form": "주변보면 나중에 종교를 믿게된 경우는 계기가 있더라고요 대부분",
          "speaker_id": "1",
          "time": "20210826 14:21"
        },
        {
          "id": "MDRW2100016585.1.15",
          "form": "엄마가 믿어서요",
          "original_form": "엄마가 믿어서요",
          "speaker_id": "2",
          "time": "20210826 14:34"
        },
        {
          "id": "MDRW2100016585.1.16",
          "form": "어릴때 어린이불교반 같은데 다녔어요",
          "original_form": "어릴때 어린이불교반 같은데 다녔어요",
          "speaker_id": "2",
          "time": "20210826 14:34"
        },
        {
          "id": "MDRW2100016585.1.17",
          "form": "불교가 기독교보다는 거부감이 덜한거같아요",
          "original_form": "불교가 기독교보다는 거부감이 덜한거같아요",
          "speaker_id": "1",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016585.1.18",
          "form": "우와 불교유치원 지나가다 보기만했는데",
          "original_form": "우와 불교유치원 지나가다 보기만했는데",
          "speaker_id": "1",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016585.1.19",
          "form": "네 기독교는 안티가많죠",
          "original_form": "네 기독교는 안티가많죠",
          "speaker_id": "2",
          "time": "20210826 14:42"
        },
        {
          "id": "MDRW2100016585.1.20",
          "form": "저도 거부감있어요 ㅜ",
          "original_form": "저도 거부감있어요 ㅜ",
          "speaker_id": "2",
          "time": "20210826 14:42"
        },
        {
          "id": "MDRW2100016585.1.21",
          "form": "전도하는게 너무 심하고 이래저래 부정적이게된...",
          "original_form": "전도하는게 너무 심하고 이래저래 부정적이게된...",
          "speaker_id": "1",
          "time": "20210826 14:44"
        },
        {
          "id": "MDRW2100016585.1.22",
          "form": "그러다보니 무교가 된 것 같아요ㅎㅎ",
          "original_form": "그러다보니 무교가 된 것 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 14:45"
        }
      ]
    }
