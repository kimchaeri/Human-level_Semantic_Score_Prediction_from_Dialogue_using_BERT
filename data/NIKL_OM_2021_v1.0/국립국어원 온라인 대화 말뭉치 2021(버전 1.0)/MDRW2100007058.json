        },
        {
          "id": "MDRW2100007058.1.3",
          "form": "이따가ㅋㅋㅋ",
          "original_form": "이따가ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007058.1.4",
          "form": "이따가는 자야되는뎅",
          "original_form": "이따가는 자야되는뎅",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007058.1.5",
          "form": "그럼 내일 운동해야지",
          "original_form": "그럼 내일 운동해야지",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007058.1.6",
          "form": "그랭 내일 걷기 좀 하자",
          "original_form": "그랭 내일 걷기 좀 하자",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007058.1.7",
          "form": "아 내일 머리자를랬는데 미룰깜",
          "original_form": "아 내일 머리자를랬는데 미룰깜",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007058.1.8",
          "form": "일하면서 걸으셈",
          "original_form": "일하면서 걸으셈",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
