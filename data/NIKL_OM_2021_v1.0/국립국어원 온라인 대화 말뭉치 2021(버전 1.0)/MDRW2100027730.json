      "utterance": [
        {
          "id": "MDRW2100027730.1.1",
          "form": "현재 갖고 싶은 물건이 있나유",
          "original_form": "현재 갖고 싶은 물건이 있나유",
          "speaker_id": "1",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100027730.1.2",
          "form": "갖고싶은 물건...",
          "original_form": "갖고싶은 물건...",
          "speaker_id": "2",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100027730.1.3",
          "form": "베스타 게임칩...",
          "original_form": "베스타 게임칩...",
          "speaker_id": "2",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100027730.1.4",
          "form": "돈이있는데 물량이 업서",
          "original_form": "돈이있는데 물량이 업서",
          "speaker_id": "2",
          "time": "20210902 14:36"
        },
        {
          "id": "MDRW2100027730.1.5",
          "form": "아앗...직원분들 얼른 일해주십쇼ㅠㅠ",
          "original_form": "아앗...직원분들 얼른 일해주십쇼ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 14:38"
        },
        {
          "id": "MDRW2100027730.1.6",
          "form": "돈이 있는데 왜 가져가질 못하니",
          "original_form": "돈이 있는데 왜 가져가질 못하니",
          "speaker_id": "1",
          "time": "20210902 14:38"
        },
        {
          "id": "MDRW2100027730.1.7",
          "form": "그니까....",
          "original_form": "그니까....",
          "speaker_id": "2",
          "time": "20210902 16:24"
        },
        {
          "id": "MDRW2100027730.1.8",
          "form": "아님 벌써 더 안만드는건가 설마그건아니겟지",
          "original_form": "아님 벌써 더 안만드는건가 설마그건아니겟지",
          "speaker_id": "2",
          "time": "20210902 16:24"
        },
        {
          "id": "MDRW2100027730.1.9",
          "form": "name1 누님은 머사는거잇나여",
          "original_form": "&name1& 누님은 머사는거잇나여",
          "speaker_id": "2",
          "time": "20210902 16:24"
        },
        {
          "id": "MDRW2100027730.1.10",
          "form": "최근에 산건 옷이랑 신발 밖에 없네유",
          "original_form": "최근에 산건 옷이랑 신발 밖에 없네유",
          "speaker_id": "1",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100027730.1.11",
          "form": "여름 옷 샀는데 벌써 가을이야 얼마 못 입겠엌ㅋㅋㅋ",
          "original_form": "여름 옷 샀는데 벌써 가을이야 얼마 못 입겠엌ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:30"
        },
        {
          "id": "MDRW2100027730.1.12",
          "form": "ㅋㅋㅋㅋㅋㅜㅜㅜ계절 넘빨리지나가",
          "original_form": "ㅋㅋㅋㅋㅋㅜㅜㅜ계절 넘빨리지나가",
          "speaker_id": "2",
          "time": "20210902 16:55"
        },
        {
          "id": "MDRW2100027730.1.13",
          "form": "가을하면 또 겨울돼서 겨울옷사야할듯",
          "original_form": "가을하면 또 겨울돼서 겨울옷사야할듯",
          "speaker_id": "2",
          "time": "20210902 16:56"
        },
        {
          "id": "MDRW2100027730.1.14",
          "form": "앜ㅋㅋㅋㅋ맞아 곧 겨울일듯...",
          "original_form": "앜ㅋㅋㅋㅋ맞아 곧 겨울일듯...",
          "speaker_id": "1",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100027730.1.15",
          "form": "동숲 나왔을때부터 닌텐도 갖고싶었는데 내 플스 상태를 보면 얼마 쓰지도 않을 것 같아서 고민ㅋㅋㅋㅋ...",
          "original_form": "동숲 나왔을때부터 닌텐도 갖고싶었는데 내 플스 상태를 보면 얼마 쓰지도 않을 것 같아서 고민ㅋㅋㅋㅋ...",
          "speaker_id": "1",
          "time": "20210902 17:10"
        },
        {
          "id": "MDRW2100027730.1.16",
          "form": "플스삿엇구나 듣고보니 그랫던가 생각하고잇다ㅋㅋㅋㅋㅋㅋ",
          "original_form": "플스삿엇구나 듣고보니 그랫던가 생각하고잇다ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:16"
        },
        {
          "id": "MDRW2100027730.1.17",
          "form": "사서 쓰다가 더안쓰면 다시 중고로 파는건 어때",
          "original_form": "사서 쓰다가 더안쓰면 다시 중고로 파는건 어때",
          "speaker_id": "2",
          "time": "20210902 17:16"
        },
        {
          "id": "MDRW2100027730.1.18",
          "form": "오케 열심히 돈 모아서 내 자신한테 선물해야겠다ㅋㅋㅋ",
          "original_form": "오케 열심히 돈 모아서 내 자신한테 선물해야겠다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:26"
        },
        {
          "id": "MDRW2100027730.1.19",
          "form": "조아조아",
          "original_form": "조아조아",
          "speaker_id": "2",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100027730.1.20",
          "form": "그때같이 스위치겜을 하자꾸낭",
          "original_form": "그때같이 스위치겜을 하자꾸낭",
          "speaker_id": "2",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100027730.1.21",
          "form": "링피트랑 모동숲 다 같이 할 수 있는거쥬?",
          "original_form": "링피트랑 모동숲 다 같이 할 수 있는거쥬?",
          "speaker_id": "1",
          "time": "20210902 17:32"
        },
        {
          "id": "MDRW2100027730.1.22",
          "form": "링피트는 1인용이구",
          "original_form": "링피트는 1인용이구",
          "speaker_id": "2",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100027730.1.23",
          "form": "모동숲은 친구섬 놀러갈수잇는거같긴하던데",
          "original_form": "모동숲은 친구섬 놀러갈수잇는거같긴하던데",
          "speaker_id": "2",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100027730.1.24",
          "form": "기본적으론 혼자할걸??",
          "original_form": "기본적으론 혼자할걸??",
          "speaker_id": "2",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100027730.1.25",
          "form": "아하 놀러만 갈 수 있구나",
          "original_form": "아하 놀러만 갈 수 있구나",
          "speaker_id": "1",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100027730.1.26",
          "form": "솔플 좋아하니까 딱이구먼",
