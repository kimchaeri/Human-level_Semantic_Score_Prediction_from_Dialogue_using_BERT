      "utterance": [
        {
          "id": "MDRW2100026076.1.1",
          "form": "반려동물 키운 적 있어?",
          "original_form": "반려동물 키운 적 있어?",
          "speaker_id": "1",
          "time": "20210901 19:36"
        },
        {
          "id": "MDRW2100026076.1.2",
          "form": "이구아나 키운 적 있어! 근데 내가 아니라 오빠가 키운 거라... 물론 같이 구경은 했지만",
          "original_form": "이구아나 키운 적 있어! 근데 내가 아니라 오빠가 키운 거라... 물론 같이 구경은 했지만",
          "speaker_id": "2",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100026076.1.3",
          "form": "와 나 주변에 이구아나 키운 사람은 너가 첨이야",
          "original_form": "와 나 주변에 이구아나 키운 사람은 너가 첨이야",
          "speaker_id": "1",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100026076.1.4",
          "form": "어릴 적에 철없이 키운 거라 좋지 않게 보냈던 기억이 있네...",
          "original_form": "어릴 적에 철없이 키운 거라 좋지 않게 보냈던 기억이 있네...",
          "speaker_id": "2",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100026076.1.5",
          "form": "완전 궁금하다",
          "original_form": "완전 궁금하다",
          "speaker_id": "1",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100026076.1.6",
          "form": "아ㅠㅠㅠㅠ 안타깝다ㅠㅠㅠ",
          "original_form": "아ㅠㅠㅠㅠ 안타깝다ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100026076.1.7",
          "form": "딱히 관리법이라든가 그런 건 몰랐어..",
          "original_form": "딱히 관리법이라든가 그런 건 몰랐어..",
          "speaker_id": "2",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100026076.1.8",
          "form": "애가 입에 병이 나가지고 밥도 못먹고",
          "original_form": "애가 입에 병이 나가지고 밥도 못먹고",
          "speaker_id": "2",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100026076.1.9",
          "form": "앓다가 죽어버렸던..",
          "original_form": "앓다가 죽어버렸던..",
          "speaker_id": "2",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100026076.1.10",
          "form": "아무래도 파충류니까 힘들지ㅠㅠㅠ",
          "original_form": "아무래도 파충류니까 힘들지ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100026076.1.11",
          "form": "넌 반려동물 키운 적 있니",
          "original_form": "넌 반려동물 키운 적 있니",
          "speaker_id": "2",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100026076.1.12",
          "form": "맞아... 철없던 것도 있고",
          "original_form": "맞아... 철없던 것도 있고",
          "speaker_id": "2",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100026076.1.13",
          "form": "지금 다시 키운다면 더 애정을 담아서 키우고 싶네",
          "original_form": "지금 다시 키운다면 더 애정을 담아서 키우고 싶네",
          "speaker_id": "2",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100026076.1.14",
          "form": "나도 사슴벌레 키워봤는데 그냥 산에서 잡아서 키우다보니 관리방법을 몰라서 자기들끼리 싸우더니 죽었어ㅠㅠㅠ",
          "original_form": "나도 사슴벌레 키워봤는데 그냥 산에서 잡아서 키우다보니 관리방법을 몰라서 자기들끼리 싸우더니 죽었어ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100026076.1.15",
          "form": "진짜 철없을 때지",
          "original_form": "진짜 철없을 때지",
          "speaker_id": "1",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100026076.1.16",
          "form": "ㅋㅋㅋㅋ 나도 사슴벌레 키운 적 있는데...",
          "original_form": "ㅋㅋㅋㅋ 나도 사슴벌레 키운 적 있는데...",
          "speaker_id": "2",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100026076.1.17",
          "form": "유충 때부터 키웠는데",
          "original_form": "유충 때부터 키웠는데",
          "speaker_id": "2",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100026076.1.18",
          "form": "와 대박",
          "original_form": "와 대박",
          "speaker_id": "1",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100026076.1.19",
          "form": "다 성장하더니 집 탈출했어",
          "original_form": "다 성장하더니 집 탈출했어",
          "speaker_id": "2",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100026076.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100026076.1.21",
          "form": "아닠ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아닠ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100026076.1.22",
          "form": "반전인데?ㅋㅋㅋㅋㅋ",
          "original_form": "반전인데?ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100026076.1.23",
          "form": "자유를 찾으러 가버렸어...",
          "original_form": "자유를 찾으러 가버렸어...",
          "speaker_id": "2",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100026076.1.24",
          "form": "문을 열어둔거야?",
          "original_form": "문을 열어둔거야?",
          "speaker_id": "1",
          "time": "20210901 19:40"
        },
        {
          "id": "MDRW2100026076.1.25",
          "form": "개미집도 키워봤다",
          "original_form": "개미집도 키워봤다",
          "speaker_id": "2",
          "time": "20210901 19:40"
        },
        {
          "id": "MDRW2100026076.1.26",
          "form": "날아서 탈출했나..?",
          "original_form": "날아서 탈출했나..?",
          "speaker_id": "1",
          "time": "20210901 19:40"
        },
        {
          "id": "MDRW2100026076.1.27",
          "form": "아마 그랬던 거 같아",
          "original_form": "아마 그랬던 거 같아",
          "speaker_id": "2",
          "time": "20210901 19:40"
        },
        {
          "id": "MDRW2100026076.1.28",
          "form": "오 나도 그 파란색",
          "original_form": "오 나도 그 파란색",
          "speaker_id": "1",
          "time": "20210901 19:40"
        },
        {
          "id": "MDRW2100026076.1.29",
          "form": "어디로 간 건지는... 지금도 의문이다",
          "original_form": "어디로 간 건지는... 지금도 의문이다",
          "speaker_id": "2",
          "time": "20210901 19:40"
        },
        {
          "id": "MDRW2100026076.1.30",
          "form": "개미집 어릴 때 보면서 진짜 신기해했었는데",
          "original_form": "개미집 어릴 때 보면서 진짜 신기해했었는데",
          "speaker_id": "1",
          "time": "20210901 19:41"
        }
      ]
