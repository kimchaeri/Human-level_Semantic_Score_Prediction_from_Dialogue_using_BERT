      "utterance": [
        {
          "id": "MDRW2100018878.1.1",
          "form": "오호 좋구먼 비안오네",
          "original_form": "오호 좋구먼 비안오네",
          "speaker_id": "2",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100018878.1.2",
          "form": "일기예보가 수시로 바뀌니",
          "original_form": "일기예보가 수시로 바뀌니",
          "speaker_id": "2",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100018878.1.3",
          "form": "비가 오락가락 하는구먼",
          "original_form": "비가 오락가락 하는구먼",
          "speaker_id": "2",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100018878.1.4",
          "form": "후덥지근하겠다",
          "original_form": "후덥지근하겠다",
          "speaker_id": "2",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100018878.1.5",
          "form": "지금 딱 습하고 더운 날씨 요",
          "original_form": "지금 딱 습하고 더운 날씨 요",
          "speaker_id": "1",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100018878.1.6",
          "form": "아침에 날 흐려서 선크림도 안 발라 보냈는데",
          "original_form": "아침에 날 흐려서 선크림도 안 발라 보냈는데",
          "speaker_id": "1",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100018878.1.7",
          "form": "더 새까매지겠군요",
          "original_form": "더 새까매지겠군요",
          "speaker_id": "1",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018878.1.8",
          "form": "ㅋㅋㄱㅋㅋㄱㄱ 나도 어제 안발라보냈는데",
          "original_form": "ㅋㅋㄱㅋㅋㄱㄱ 나도 어제 안발라보냈는데",
          "speaker_id": "2",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018878.1.9",
          "form": "뭔가 시커머진 느낌이라",
          "original_form": "뭔가 시커머진 느낌이라",
          "speaker_id": "2",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018878.1.10",
          "form": "오늘 발라 보냈어",
          "original_form": "오늘 발라 보냈어",
          "speaker_id": "2",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018878.1.11",
          "form": "하지만 캠핑 가서 또 까매지겠지",
          "original_form": "하지만 캠핑 가서 또 까매지겠지",
          "speaker_id": "2",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018878.1.12",
          "form": "언니네 애들은 새까매진 것도 아님",
          "original_form": "언니네 애들은 새까매진 것도 아님",
          "speaker_id": "1",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018878.1.13",
          "form": "첫째는 얼마나 하얀 지",
          "original_form": "첫째는 얼마나 하얀 지",
          "speaker_id": "1",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018878.1.14",
          "form": "부럽구만요",
          "original_form": "부럽구만요",
          "speaker_id": "1",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018878.1.15",
          "form": "name3는 못 본 지가 너무 오래 됐네요",
          "original_form": "&name3&는 못 본 지가 너무 오래 됐네요",
          "speaker_id": "1",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018878.1.16",
          "form": "그래 원 투가 바꼈어 야 됐는데",
          "original_form": "그래 원 투가 바꼈어 야 됐는데",
          "speaker_id": "2",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018878.1.17",
          "form": "투 많이 컸지",
          "original_form": "투 많이 컸지",
          "speaker_id": "2",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018878.1.18",
          "form": "이제 제법 말도 잘한다",
          "original_form": "이제 제법 말도 잘한다",
          "speaker_id": "2",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018878.1.19",
          "form": "투 애기 때 함께 오래 했잖아요",
          "original_form": "투 애기 때 함께 오래 했잖아요",
          "speaker_id": "1",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018878.1.20",
          "form": "112 동이 얼마 전에 너희두 치 봤는데 말 엄청 잘한다고",
          "original_form": "112 동이 얼마 전에 너희두 치 봤는데 말 엄청 잘한다고",
          "speaker_id": "2",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018878.1.21",
          "form": "큰 게 안 믿기네요",
          "original_form": "큰 게 안 믿기네요",
          "speaker_id": "1",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018878.1.22",
          "form": "그래 그때 행복했었지",
          "original_form": "그래 그때 행복했었지",
          "speaker_id": "2",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018878.1.23",
          "form": "언니 부스터 맨날 가지고 다니고",
          "original_form": "언니 부스터 맨날 가지고 다니고",
          "speaker_id": "1",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018878.1.24",
          "form": "짬뽕집에서",
          "original_form": "짬뽕집에서",
          "speaker_id": "1",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018878.1.25",
          "form": "이제 날씨 좋아지면 마실까요",
          "original_form": "이제 날씨 좋아지면 마실까요",
          "speaker_id": "1",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018878.1.26",
          "form": "아우 그때 힘들었다",
          "original_form": "아우 그때 힘들었다",
          "speaker_id": "2",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018878.1.27",
          "form": "알씨 좋은 날 놀러가자",
          "original_form": "알씨 좋은 날 놀러가자",
          "speaker_id": "2",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018878.1.28",
          "form": "날씨",
          "original_form": "날씨",
          "speaker_id": "2",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018878.1.29",
          "form": "그래요 언니",
          "original_form": "그래요 언니",
          "speaker_id": "1",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018878.1.30",
          "form": "찰떡같이 알아들어요",
          "original_form": "찰떡같이 알아들어요",
          "speaker_id": "1",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018878.1.31",
          "form": "그래 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱㅋ",
          "original_form": "그래 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱㅋ",
          "speaker_id": "2",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018878.1.32",
          "form": "날씨가 오락가락하니 애들 옷차림이",
          "original_form": "날씨가 오락가락하니 애들 옷차림이",
          "speaker_id": "2",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018878.1.33",
          "form": "고민이네",
          "original_form": "고민이네",
          "speaker_id": "2",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018878.1.34",
          "form": "오락가락 해도 반팔 이에요",
          "original_form": "오락가락 해도 반팔 이에요",
          "speaker_id": "1",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018878.1.35",
          "form": "오전에 선선할까봐 긴 팔도 챙기고",
          "original_form": "오전에 선선할까봐 긴 팔도 챙기고",
          "speaker_id": "2",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018878.1.36",
          "form": "우리 애들은 땀범벅이에요",
          "original_form": "우리 애들은 땀범벅이에요",
          "speaker_id": "1",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018878.1.37",
          "form": "밤에 절대 더울까 봐 반팔도 챙기고",
          "original_form": "밤에 절대 더울까 봐 반팔도 챙기고",
          "speaker_id": "2",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018878.1.38",
          "form": "가을까지는 땀범벅",
          "original_form": "가을까지는 땀범벅",
          "speaker_id": "1",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018878.1.39",
          "form": "그렇지 땀 많이 흘리지",
          "original_form": "그렇지 땀 많이 흘리지",
          "speaker_id": "2",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018878.1.40",
          "form": "아 언니 캠핑 가죠",
          "original_form": "아 언니 캠핑 가죠",
          "speaker_id": "1",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018878.1.41",
          "form": "우리 투도 땀이 그래 많다",
          "original_form": "우리 투도 땀이 그래 많다",
          "speaker_id": "2",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018878.1.42",
          "form": "아 오늘",
          "original_form": "아 오늘",
          "speaker_id": "2",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018878.1.43",
          "form": "원은 땀이 별로 없거든",
          "original_form": "원은 땀이 별로 없거든",
          "speaker_id": "2",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018878.1.44",
          "form": "그래도 비 안 와서 다행이에요",
          "original_form": "그래도 비 안 와서 다행이에요",
          "speaker_id": "1",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018878.1.45",
          "form": "근데 투는 진짜 열이 많은 거 같애",
          "original_form": "근데 투는 진짜 열이 많은 거 같애",
          "speaker_id": "2",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018878.1.46",
          "form": "에어컨 틀어놔도 매일 땀 흘리면서 자고",
          "original_form": "에어컨 틀어놔도 매일 땀 흘리면서 자고",
          "speaker_id": "2",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018878.1.47",
          "form": "원은 이불 덮고 자고",
          "original_form": "원은 이불 덮고 자고",
          "speaker_id": "2",
          "time": "20210827 13:33"
        },
        {
          "id": "MDRW2100018878.1.48",
          "form": "습관성 땀 흘림",
          "original_form": "습관성 땀 흘림",
          "speaker_id": "1",
          "time": "20210827 13:33"
        },
        {
          "id": "MDRW2100018878.1.49",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210827 13:33"
        },
        {
          "id": "MDRW2100018878.1.50",
          "form": "그것도 습관이 가??",
          "original_form": "그것도 습관이 가??",
          "speaker_id": "2",
          "time": "20210827 13:33"
        },
        {
          "id": "MDRW2100018878.1.51",
          "form": "얼마나 흘리는 지",
          "original_form": "얼마나 흘리는 지",
          "speaker_id": "1",
          "time": "20210827 13:33"
        }
      ]
