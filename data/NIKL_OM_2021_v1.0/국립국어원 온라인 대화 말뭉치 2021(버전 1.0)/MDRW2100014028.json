      "utterance": [
        {
          "id": "MDRW2100014028.1.1",
          "form": "최근에 여행 가신 적 있나요?",
          "original_form": "최근에 여행 가신 적 있나요?",
          "speaker_id": "1",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100014028.1.2",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100014028.1.3",
          "form": "한참전인데",
          "original_form": "한참전인데",
          "speaker_id": "2",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100014028.1.4",
          "form": "강릉이요!",
          "original_form": "강릉이요!",
          "speaker_id": "2",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100014028.1.5",
          "form": "강릉에서 뭐하셨어요?ㅋㅋ",
          "original_form": "강릉에서 뭐하셨어요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:08"
        },
        {
          "id": "MDRW2100014028.1.6",
          "form": "맛집여행이요ㅋㅋㅋ",
          "original_form": "맛집여행이요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014028.1.7",
          "form": "바다도보고 카페도요",
          "original_form": "바다도보고 카페도요",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100014028.1.8",
          "form": "갈만한 카페 있어요?ㅎㅎ",
          "original_form": "갈만한 카페 있어요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:25"
        },
        {
          "id": "MDRW2100014028.1.9",
          "form": "안목 카페거리",
          "original_form": "안목 카페거리",
          "speaker_id": "2",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014028.1.10",
          "form": "유명하죠",
          "original_form": "유명하죠",
          "speaker_id": "2",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014028.1.11",
          "form": "거기서 추천하시는 카페 있어요?ㅎㅎ",
          "original_form": "거기서 추천하시는 카페 있어요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100014028.1.12",
          "form": "이름은 기억이안나네요 영어이름이라",
          "original_form": "이름은 기억이안나네요 영어이름이라",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014028.1.13",
          "form": "그쪽에 웬만하면 다 이뻐요ㅋㅋㅋ 오션뷰라",
          "original_form": "그쪽에 웬만하면 다 이뻐요ㅋㅋㅋ 오션뷰라",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014028.1.14",
          "form": "전 강릉 카페하면 테라로사만 생각나네요ㅎㅎ",
          "original_form": "전 강릉 카페하면 테라로사만 생각나네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014028.1.15",
          "form": "그 안목카페거리쪽",
          "original_form": "그 안목카페거리쪽",
          "speaker_id": "2",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100014028.1.16",
          "form": "카페들 나중에 기회되면한번가보세요ㅋㅋㅋ",
          "original_form": "카페들 나중에 기회되면한번가보세요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100014028.1.17",
          "form": "루프탑쭉있어요!",
          "original_form": "루프탑쭉있어요!",
          "speaker_id": "2",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100014028.1.18",
          "form": "오 분위기 좋을 것 같아요ㅎㅎ",
          "original_form": "오 분위기 좋을 것 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014028.1.19",
          "form": "전 얼마전에 바로 옆 속초를 다녀왔었어요ㅋㅋ",
          "original_form": "전 얼마전에 바로 옆 속초를 다녀왔었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100014028.1.20",
          "form": "오우 속초좋죠",
          "original_form": "오우 속초좋죠",
          "speaker_id": "2",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100014028.1.21",
          "form": "가서뭐하셨나요?!",
          "original_form": "가서뭐하셨나요?!",
          "speaker_id": "2",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014028.1.22",
          "form": "바닷가 가서 놀다가 물회도 먹었어요ㅋㅋ",
          "original_form": "바닷가 가서 놀다가 물회도 먹었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:04"
        }
      ]
