      "utterance": [
        {
          "id": "MDRW2100029966.1.1",
          "form": "오늘 학교 너무 힘들엇다...",
          "original_form": "오늘 학교 너무 힘들엇다...",
          "speaker_id": "1",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100029966.1.2",
          "form": "온라인이었지만...",
          "original_form": "온라인이었지만...",
          "speaker_id": "1",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100029966.1.3",
          "form": "온라인도 지치는 듯..",
          "original_form": "온라인도 지치는 듯..",
          "speaker_id": "1",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100029966.1.4",
          "form": "인정",
          "original_form": "인정",
          "speaker_id": "2",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100029966.1.5",
          "form": "나는 오히려 온라인 수업이 더 힘들어",
          "original_form": "나는 오히려 온라인 수업이 더 힘들어",
          "speaker_id": "2",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100029966.1.6",
          "form": "차라리 학교에 가고 싶음",
          "original_form": "차라리 학교에 가고 싶음",
          "speaker_id": "2",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100029966.1.7",
          "form": "친구들이랑 쉬는시간마다 떠들면서 놀고 싶어",
          "original_form": "친구들이랑 쉬는시간마다 떠들면서 놀고 싶어",
          "speaker_id": "2",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100029966.1.8",
          "form": "아 매점가서 매점빵도 먹고싶다",
          "original_form": "아 매점가서 매점빵도 먹고싶다",
          "speaker_id": "2",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100029966.1.9",
          "form": "소세지빵 진짜 맛있는데",
          "original_form": "소세지빵 진짜 맛있는데",
          "speaker_id": "2",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100029966.1.10",
          "form": "와 매점",
          "original_form": "와 매점",
          "speaker_id": "1",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100029966.1.11",
          "form": "우리학교 매점 ㅇ없는데",
          "original_form": "우리학교 매점 ㅇ없는데",
          "speaker_id": "1",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100029966.1.12",
          "form": "매점 어때?",
          "original_form": "매점 어때?",
          "speaker_id": "1",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100029966.1.13",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "2",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100029966.1.14",
          "form": "처음에는 엄청 좋았는데",
          "original_form": "처음에는 엄청 좋았는데",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100029966.1.15",
          "form": "사실 우리 매점이 다 비싸서",
          "original_form": "사실 우리 매점이 다 비싸서",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100029966.1.16",
          "form": "ㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㄴㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100029966.1.17",
          "form": "교회 권사님께서 봉사하시는데",
          "original_form": "교회 권사님께서 봉사하시는데",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100029966.1.18",
          "form": "그치...물가 비싸지...",
          "original_form": "그치...물가 비싸지...",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100029966.1.19",
          "form": "ㅇ웅웅",
          "original_form": "ㅇ웅웅",
          "speaker_id": "1",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100029966.1.20",
          "form": "매번 미안하다고",
          "original_form": "매번 미안하다고",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100029966.1.21",
          "form": "이게 독점이라서",
          "original_form": "이게 독점이라서",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100029966.1.22",
          "form": "가격을 학교에서 정하는 게 아닌가봐",
          "original_form": "가격을 학교에서 정하는 게 아닌가봐",
          "speaker_id": "2",
          "time": "20210903 17:33"
        },
        {
          "id": "MDRW2100029966.1.23",
          "form": "물품을 주는 곳에서 가격을 정하는 거라고 하시더라",
          "original_form": "물품을 주는 곳에서 가격을 정하는 거라고 하시더라",
          "speaker_id": "2",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100029966.1.24",
          "form": "아이스크림 값도 올랐어",
          "original_form": "아이스크림 값도 올랐어",
          "speaker_id": "2",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100029966.1.25",
          "form": "아 생각해보니 과제 다 안 했는데",
          "original_form": "아 생각해보니 과제 다 안 했는데",
          "speaker_id": "2",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100029966.1.26",
          "form": "큰일났다",
          "original_form": "큰일났다",
          "speaker_id": "2",
          "time": "20210903 17:34"
        },
        {
          "id": "MDRW2100029966.1.27",
          "form": "아ㅏ 나도 과제하니까",
          "original_form": "아ㅏ 나도 과제하니까",
          "speaker_id": "1",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100029966.1.28",
          "form": "오늘까지 내는거 있었는ㄷ",
          "original_form": "오늘까지 내는거 있었는ㄷ",
          "speaker_id": "1",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100029966.1.29",
          "form": "안햇어...",
          "original_form": "안햇어...",
          "speaker_id": "1",
          "time": "20210903 17:35"
        },
        {
          "id": "MDRW2100029966.1.30",
          "form": "헐 나 지금 생각남",
          "original_form": "헐 나 지금 생각남",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100029966.1.31",
          "form": "과제 누락될 뻔",
          "original_form": "과제 누락될 뻔",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100029966.1.32",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100029966.1.33",
          "form": "어떻게 ㄷ했어?",
          "original_form": "어떻게 ㄷ했어?",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100029966.1.34",
          "form": "잘 됐어?",
          "original_form": "잘 됐어?",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100029966.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100029966.1.36",
          "form": "지금 하는 중",
          "original_form": "지금 하는 중",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100029966.1.37",
          "form": "아이고....",
          "original_form": "아이고....",
          "speaker_id": "1",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100029966.1.38",
          "form": "와 까먹을 뻔 했어",
          "original_form": "와 까먹을 뻔 했어",
          "speaker_id": "2",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100029966.1.39",
          "form": "사실 나도 해야하는데 귀찮아서 안하고 잇다..ㅎ",
          "original_form": "사실 나도 해야하는데 귀찮아서 안하고 잇다..ㅎ",
          "speaker_id": "1",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100029966.1.40",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100029966.1.41",
          "form": "엄청 깐깐하신 선생님이신데",
          "original_form": "엄청 깐깐하신 선생님이신데",
          "speaker_id": "2",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100029966.1.42",
          "form": "다행이다",
          "original_form": "다행이다",
          "speaker_id": "2",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100029966.1.43",
          "form": "ㅋㅋㅋㅋㅋ아니",
          "original_form": "ㅋㅋㅋㅋㅋ아니",
          "speaker_id": "2",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100029966.1.44",
          "form": "같이 하자",
          "original_form": "같이 하자",
          "speaker_id": "2",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100029966.1.45",
          "form": "너네 경제수학 지금 배워?",
          "original_form": "너네 경제수학 지금 배워?",
          "speaker_id": "2",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100029966.1.46",
          "form": "안돼...닐어나기 싫어.  ..과제 싫어...",
          "original_form": "안돼...닐어나기 싫어.  ..과제 싫어...",
          "speaker_id": "1",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100029966.1.47",
          "form": "아니아니 내년",
          "original_form": "아니아니 내년",
          "speaker_id": "1",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100029966.1.48",
          "form": "3학년 때 배우는 건가?",
          "original_form": "3학년 때 배우는 건가?",
          "speaker_id": "2",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100029966.1.49",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100029966.1.50",
          "form": "그치만 난 수학주제탐구임",
          "original_form": "그치만 난 수학주제탐구임",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100029966.1.51",
          "form": "경제수학 은근히 헷갈림",
          "original_form": "경제수학 은근히 헷갈림",
          "speaker_id": "2",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100029966.1.52",
          "form": "경제수학 안함",
          "original_form": "경제수학 안함",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100029966.1.53",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210903 23:17"
        }
