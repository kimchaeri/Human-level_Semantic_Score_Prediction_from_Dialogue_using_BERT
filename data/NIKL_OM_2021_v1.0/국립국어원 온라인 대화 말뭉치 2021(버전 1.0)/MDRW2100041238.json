          "speaker_id": "1",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100041238.1.6",
          "form": "뭐 드셨나요?",
          "original_form": "뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210905 20:44"
        },
        {
          "id": "MDRW2100041238.1.7",
          "form": "전 치킨 먹었어요",
          "original_form": "전 치킨 먹었어요",
          "speaker_id": "2",
          "time": "20210905 20:48"
        },
        {
