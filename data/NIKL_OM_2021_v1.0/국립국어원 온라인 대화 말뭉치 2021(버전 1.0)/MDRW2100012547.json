      "utterance": [
        {
          "id": "MDRW2100012547.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100012547.1.2",
          "form": "아니요 name2은요?",
          "original_form": "아니요 &name2&은요?",
          "speaker_id": "1",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100012547.1.3",
          "form": "저도 즐기진 않는데 가끔 가는건 좋아해요!",
          "original_form": "저도 즐기진 않는데 가끔 가는건 좋아해요!",
          "speaker_id": "2",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100012547.1.4",
          "form": "사실 집을 가장 좋아해서 여행도 많이 다녀보진 못했어요",
          "original_form": "사실 집을 가장 좋아해서 여행도 많이 다녀보진 못했어요",
          "speaker_id": "2",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100012547.1.5",
          "form": "저도 집순이예요!",
          "original_form": "저도 집순이예요!",
          "speaker_id": "1",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100012547.1.6",
          "form": "코로나 풀리면",
          "original_form": "코로나 풀리면",
          "speaker_id": "1",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100012547.1.7",
          "form": "해외 어디가고 싶으세요?",
          "original_form": "해외 어디가고 싶으세요?",
          "speaker_id": "1",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100012547.1.8",
          "form": "저는 싱가포르요",
          "original_form": "저는 싱가포르요",
          "speaker_id": "2",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100012547.1.9",
          "form": "코로나 전에 여행계획 잡아놨다가 취소되서 아쉬움이 남아서요 ㅠㅠㅠ",
          "original_form": "코로나 전에 여행계획 잡아놨다가 취소되서 아쉬움이 남아서요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100012547.1.10",
          "form": "name1도 가고싶으신 곳 있으신가요?",
          "original_form": "&name1&도 가고싶으신 곳 있으신가요?",
          "speaker_id": "2",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100012547.1.11",
          "form": "저도 싱가포르 가보고싶어요!",
          "original_form": "저도 싱가포르 가보고싶어요!",
          "speaker_id": "1",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100012547.1.12",
          "form": "멕시코 가고싶습니다",
          "original_form": "멕시코 가고싶습니다",
          "speaker_id": "1",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100012547.1.13",
          "form": "타코먹으러",
          "original_form": "타코먹으러",
          "speaker_id": "1",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100012547.1.14",
          "form": "싱가포르는 치안이 잘되어있다고 해서 저같은 쫄보에겐 너무 좋은 곳인 것 같아요 ㅎㅎ",
          "original_form": "싱가포르는 치안이 잘되어있다고 해서 저같은 쫄보에겐 너무 좋은 곳인 것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100012547.1.15",
          "form": "현지에서 먹으면 맛있겠네요!",
          "original_form": "현지에서 먹으면 맛있겠네요!",
          "speaker_id": "2",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100012547.1.16",
          "form": "타코 많이 좋아하세요?",
          "original_form": "타코 많이 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100012547.1.17",
          "form": "싱가포르는 인프라가 좋다고해서",
          "original_form": "싱가포르는 인프라가 좋다고해서",
          "speaker_id": "1",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100012547.1.18",
          "form": "살아보고 싶기도해요",
          "original_form": "살아보고 싶기도해요",
          "speaker_id": "1",
          "time": "20210824 23:12"
        },
        {
          "id": "MDRW2100012547.1.19",
          "form": "영어만 된다면",
          "original_form": "영어만 된다면",
          "speaker_id": "1",
          "time": "20210824 23:12"
        },
        {
          "id": "MDRW2100012547.1.20",
          "form": "타코 완전 좋아해요",
          "original_form": "타코 완전 좋아해요",
          "speaker_id": "1",
          "time": "20210824 23:12"
        },
        {
          "id": "MDRW2100012547.1.21",
          "form": "맞아요 영어.. 여행에서 가장 큰 장벽은 영어인 것 같아요 잘하고 싶은데 어렵네요 ㅠㅠ",
          "original_form": "맞아요 영어.. 여행에서 가장 큰 장벽은 영어인 것 같아요 잘하고 싶은데 어렵네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 23:13"
        },
        {
          "id": "MDRW2100012547.1.22",
          "form": "타코 얘기하시니까 먹고싶네요 ㅠㅠ",
          "original_form": "타코 얘기하시니까 먹고싶네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100012547.1.23",
          "form": "영어 공부해서 해외 다녀봅시다!",
          "original_form": "영어 공부해서 해외 다녀봅시다!",
          "speaker_id": "1",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100012547.1.24",
          "form": "좋아요 어느정도 생활영어만 되도 좋게 열심히 해봐야겠어요~",
          "original_form": "좋아요 어느정도 생활영어만 되도 좋게 열심히 해봐야겠어요~",
          "speaker_id": "2",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100012547.1.25",
          "form": "멕시코 현지 타코가 궁금해서 검색해보니까 새우타코가 유명한가봐요",
          "original_form": "멕시코 현지 타코가 궁금해서 검색해보니까 새우타코가 유명한가봐요",
          "speaker_id": "2",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100012547.1.26",
          "form": "이밤에 찾으니 너무 배고프네요 ㅠㅠ",
          "original_form": "이밤에 찾으니 너무 배고프네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100012547.1.27",
          "form": "싱가포르는 어떤 음식이 유명한가요?",
          "original_form": "싱가포르는 어떤 음식이 유명한가요?",
          "speaker_id": "1",
          "time": "20210824 23:18"
        },
        {
          "id": "MDRW2100012547.1.28",
          "form": "칠리크랩이 유명하고 카야토스트도 많이들 먹어요!",
          "original_form": "칠리크랩이 유명하고 카야토스트도 많이들 먹어요!",
          "speaker_id": "2",
          "time": "20210824 23:21"
        }
      ]
    }
