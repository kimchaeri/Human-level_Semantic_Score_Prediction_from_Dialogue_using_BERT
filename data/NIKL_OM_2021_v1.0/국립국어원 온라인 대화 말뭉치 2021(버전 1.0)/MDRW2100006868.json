      "utterance": [
        {
          "id": "MDRW2100006868.1.1",
          "form": "운동을 합니다",
          "original_form": "운동을 합니다",
          "speaker_id": "1",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100006868.1.2",
          "form": "어떤운동하세요?",
          "original_form": "어떤운동하세요?",
          "speaker_id": "2",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100006868.1.3",
          "form": "헬스요",
          "original_form": "헬스요",
          "speaker_id": "1",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100006868.1.4",
          "form": "저는 필라테스해요 ㅎ",
          "original_form": "저는 필라테스해요 ㅎ",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100006868.1.5",
          "form": "오래되셨나요",
          "original_form": "오래되셨나요",
          "speaker_id": "1",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100006868.1.6",
          "form": "저는 3년되어가는거같아요",
          "original_form": "저는 3년되어가는거같아요",
          "speaker_id": "2",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100006868.1.7",
          "form": "오래하셨네요",
          "original_form": "오래하셨네요",
          "speaker_id": "1",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100006868.1.8",
          "form": "아이낳고 허리아파서",
          "original_form": "아이낳고 허리아파서",
          "speaker_id": "2",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100006868.1.9",
          "form": "시작했는데",
          "original_form": "시작했는데",
          "speaker_id": "2",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100006868.1.10",
          "form": "허리가 안아파서 계속해요",
          "original_form": "허리가 안아파서 계속해요",
          "speaker_id": "2",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100006868.1.11",
          "form": "안하면 다시 허리아파질것같아서요",
          "original_form": "안하면 다시 허리아파질것같아서요",
          "speaker_id": "2",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100006868.1.12",
          "form": "그룹레슨으로 다니시나요",
          "original_form": "그룹레슨으로 다니시나요",
          "speaker_id": "1",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100006868.1.13",
          "form": "네 5:1다녀요",
          "original_form": "네 5:1다녀요",
          "speaker_id": "2",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100006868.1.14",
          "form": "첨에는 3:1다녔는데",
          "original_form": "첨에는 3:1다녔는데",
          "speaker_id": "2",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100006868.1.15",
          "form": "거의 2:1 아님 1대일이였어요 ㅎ",
          "original_form": "거의 2:1 아님 1대일이였어요 ㅎ",
          "speaker_id": "2",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100006868.1.16",
          "form": "아 참석을 안해서요?",
          "original_form": "아 참석을 안해서요?",
          "speaker_id": "1",
          "time": "20210908 11:23"
        },
        {
          "id": "MDRW2100006868.1.17",
          "form": "네 회원분들이 안나오셔서",
          "original_form": "네 회원분들이 안나오셔서",
          "speaker_id": "2",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100006868.1.18",
          "form": "그랬죠",
          "original_form": "그랬죠",
          "speaker_id": "2",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100006868.1.19",
          "form": "개이득이네요...",
          "original_form": "개이득이네요...",
          "speaker_id": "1",
          "time": "20210908 11:26"
        },
        {
          "id": "MDRW2100006868.1.20",
          "form": "필라테스가 잘 맞으시나봐요",
          "original_form": "필라테스가 잘 맞으시나봐요",
          "speaker_id": "1",
          "time": "20210908 11:26"
        }
