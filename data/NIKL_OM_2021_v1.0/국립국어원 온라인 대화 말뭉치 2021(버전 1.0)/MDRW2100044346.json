      "utterance": [
        {
          "id": "MDRW2100044346.1.1",
          "form": "우수무당 가두심 어제 한퍈 봄",
          "original_form": "우수무당 가두심 어제 한퍈 봄",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044346.1.2",
          "form": "김새론 나오는건디",
          "original_form": "김새론 나오는건디",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044346.1.3",
          "form": "오 그게 풀 제목임??",
          "original_form": "오 그게 풀 제목임??",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044346.1.4",
          "form": "어어 아마도",
          "original_form": "어어 아마도",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044346.1.5",
          "form": "카카오티비에하는 거여",
          "original_form": "카카오티비에하는 거여",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044346.1.6",
          "form": "오 뭐야 무당내용?",
          "original_form": "오 뭐야 무당내용?",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044346.1.7",
          "form": "어엉ㅋㅋㅋ좀 오글거리는데",
          "original_form": "어엉ㅋㅋㅋ좀 오글거리는데",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044346.1.8",
          "form": "왴ㅋㅋㅋㅋ",
          "original_form": "왴ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044346.1.9",
          "form": "고등학교 무대에",
          "original_form": "고등학교 무대에",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044346.1.10",
          "form": "남주 하는 말 모든게 오글거림",
          "original_form": "남주 하는 말 모든게 오글거림",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044346.1.11",
          "form": "ㅋㅋㅋㅋㅋ아 학교물임?",
          "original_form": "ㅋㅋㅋㅋㅋ아 학교물임?",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044346.1.12",
          "form": "ㅇㅇㅋㅋㅋㅋ고등학샹ㅇㅇ",
          "original_form": "ㅇㅇㅋㅋㅋㅋ고등학샹ㅇㅇ",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044346.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아그면 신내림받은거가 여주가",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아그면 신내림받은거가 여주가",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044346.1.14",
          "form": "근데 김새론이 무당같은거고",
          "original_form": "근데 김새론이 무당같은거고",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044346.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ무당",
          "original_form": "ㅋㅋㅋㅋㅋㅋ무당",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044346.1.16",
          "form": "남주는 김새론 할머니가 빙의하는 바람에",
          "original_form": "남주는 김새론 할머니가 빙의하는 바람에",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044346.1.17",
          "form": "귀신보는 그런내용",
          "original_form": "귀신보는 그런내용",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044346.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ특이하네",
          "original_form": "ㅋㅋㅋㅋㅋㅋ특이하네",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044346.1.19",
          "form": "재밋겟놐ㅋㅋㅋㅋ",
          "original_form": "재밋겟놐ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044346.1.20",
          "form": "어엉ㅋㅋㅋ특이해서 한번 봄ㅋㅋㅋ",
          "original_form": "어엉ㅋㅋㅋ특이해서 한번 봄ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044346.1.21",
          "form": "나도 봐야겟다 ㅋㅋㅋㅋㅋ",
          "original_form": "나도 봐야겟다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:26"
        }
