        },
        {
          "id": "MDRW2100016040.1.2",
          "form": "고기나 회요",
          "original_form": "고기나 회요",
          "speaker_id": "2",
          "time": "20210826 08:34"
        },
        {
          "id": "MDRW2100016040.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210826 08:34"
        },
        {
          "id": "MDRW2100016040.1.4",
          "form": "회 괜찮아요?",
          "original_form": "회 괜찮아요?",
          "speaker_id": "1",
          "time": "20210826 08:34"
        },
        {
          "id": "MDRW2100016040.1.5",
          "form": "이마트 회 안먹어봐서",
          "original_form": "이마트 회 안먹어봐서",
          "speaker_id": "1",
          "time": "20210826 08:34"
        },
        {
          "id": "MDRW2100016040.1.6",
          "form": "저는 스트링치즈알아요?",
          "original_form": "저는 스트링치즈알아요?",
          "speaker_id": "1",
          "time": "20210826 08:34"
        },
        {
          "id": "MDRW2100016040.1.7",
          "form": "네 맛있어요",
          "original_form": "네 맛있어요",
          "speaker_id": "2",
          "time": "20210826 08:34"
        },
        {
          "id": "MDRW2100016040.1.8",
          "form": "그 이마트가 수입한 스트링치즈",
          "original_form": "그 이마트가 수입한 스트링치즈",
          "speaker_id": "1",
          "time": "20210826 08:34"
        },
        {
          "id": "MDRW2100016040.1.9",
          "form": "그거 사와요",
          "original_form": "그거 사와요",
          "speaker_id": "1",
          "time": "20210826 08:34"
        },
        {
          "id": "MDRW2100016040.1.10",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210826 08:34"
        },
        {
          "id": "MDRW2100016040.1.11",
          "form": "초록색인가",
          "original_form": "초록색인가",
          "speaker_id": "2",
          "time": "20210826 08:34"
        },
        {
          "id": "MDRW2100016040.1.12",
          "form": "겉면이",
          "original_form": "겉면이",
          "speaker_id": "2",
          "time": "20210826 08:34"
        },
        {
          "id": "MDRW2100016040.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 08:34"
        },
        {
          "id": "MDRW2100016040.1.14",
          "form": "엄청짠거 ㅋㅋㅋ",
          "original_form": "엄청짠거 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:35"
        },
        {
          "id": "MDRW2100016040.1.15",
          "form": "그거 전자렌지에 돌려먹으면",
          "original_form": "그거 전자렌지에 돌려먹으면",
          "speaker_id": "2",
          "time": "20210826 08:35"
        },
        {
          "id": "MDRW2100016040.1.16",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "2",
          "time": "20210826 08:35"
        },
        {
          "id": "MDRW2100016040.1.17",
          "form": "좀 타야돼요",
          "original_form": "좀 타야돼요",
          "speaker_id": "2",
          "time": "20210826 08:35"
        },
        {
          "id": "MDRW2100016040.1.18",
          "form": "맞아요 쭈욱 늘어나요",
          "original_form": "맞아요 쭈욱 늘어나요",
          "speaker_id": "1",
          "time": "20210826 08:35"
        },
        {
          "id": "MDRW2100016040.1.19",
          "form": "태우는거 좋아하시는구나",
          "original_form": "태우는거 좋아하시는구나",
          "speaker_id": "1",
          "time": "20210826 08:35"
        },
        {
          "id": "MDRW2100016040.1.20",
          "form": "약간 바삭하게 해드시나",
          "original_form": "약간 바삭하게 해드시나",
          "speaker_id": "1",
          "time": "20210826 08:35"
        },
        {
          "id": "MDRW2100016040.1.21",
          "form": "포크에 돌려먹어야돼요",
          "original_form": "포크에 돌려먹어야돼요",
          "speaker_id": "2",
          "time": "20210826 08:35"
        },
        {
          "id": "MDRW2100016040.1.22",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "2",
          "time": "20210826 08:35"
        },
        {
          "id": "MDRW2100016040.1.23",
          "form": "치즈좋아하시면",
          "original_form": "치즈좋아하시면",
          "speaker_id": "2",
          "time": "20210826 08:36"
        },
        {
          "id": "MDRW2100016040.1.24",
          "form": "포크에? 새로운 사실을 알아가네요",
          "original_form": "포크에? 새로운 사실을 알아가네요",
          "speaker_id": "1",
          "time": "20210826 08:36"
        },
        {
          "id": "MDRW2100016040.1.25",
          "form": "이마트 와인클럽 가입하세요",
          "original_form": "이마트 와인클럽 가입하세요",
          "speaker_id": "2",
          "time": "20210826 08:36"
        },
        {
          "id": "MDRW2100016040.1.26",
          "form": "무료에요",
          "original_form": "무료에요",
          "speaker_id": "2",
          "time": "20210826 08:36"
        },
        {
          "id": "MDRW2100016040.1.27",
          "form": "와인클럽? 꿀팁 고맙습니다",
          "original_form": "와인클럽? 꿀팁 고맙습니다",
          "speaker_id": "1",
          "time": "20210826 08:36"
        },
        {
          "id": "MDRW2100016040.1.28",
          "form": "치즈 종종 50프로 할인되고",
          "original_form": "치즈 종종 50프로 할인되고",
          "speaker_id": "2",
          "time": "20210826 08:36"
        },
        {
          "id": "MDRW2100016040.1.29",
          "form": "오 대박",
          "original_form": "오 대박",
          "speaker_id": "1",
          "time": "20210826 08:36"
        },
        {
          "id": "MDRW2100016040.1.30",
          "form": "발라먹는 크림치즈 50프로했었어요",
          "original_form": "발라먹는 크림치즈 50프로했었어요",
          "speaker_id": "2",
          "time": "20210826 08:36"
        },
        {
          "id": "MDRW2100016040.1.31",
          "form": "소세지 살라미 하몽 포함",
          "original_form": "소세지 살라미 하몽 포함",
