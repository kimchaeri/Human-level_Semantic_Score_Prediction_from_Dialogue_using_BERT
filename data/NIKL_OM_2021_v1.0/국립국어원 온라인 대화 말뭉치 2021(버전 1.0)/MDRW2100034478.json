      "utterance": [
        {
          "id": "MDRW2100034478.1.1",
          "form": "식사하셨나요? ㅋㅋ",
          "original_form": "식사하셨나요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:31"
        },
        {
          "id": "MDRW2100034478.1.2",
          "form": "네 저는 오늘 출근해가지구ㅠ\r\n회사에서 밥먹었어요ㅠ",
          "original_form": "네 저는 오늘 출근해가지구ㅠ\r\n회사에서 밥먹었어요ㅠ",
          "speaker_id": "2",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100034478.1.3",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "2",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100034478.1.4",
          "form": "아 주말인데 힘드시겠다ㅜㅠ",
          "original_form": "아 주말인데 힘드시겠다ㅜㅠ",
          "speaker_id": "1",
          "time": "20210828 13:46"
        },
        {
          "id": "MDRW2100034478.1.5",
          "form": "네 저는 방금 닭갈비 먹었어요",
          "original_form": "네 저는 방금 닭갈비 먹었어요",
          "speaker_id": "1",
          "time": "20210828 13:46"
        },
        {
          "id": "MDRW2100034478.1.6",
          "form": "오 맛있겠네요\r\n밥 볶아드셨나요?",
          "original_form": "오 맛있겠네요\r\n밥 볶아드셨나요?",
          "speaker_id": "2",
          "time": "20210828 13:47"
        },
        {
          "id": "MDRW2100034478.1.7",
          "form": "저어제 배달시킨거여서 ㅋㅋㅋ 넵 그거도 시켜서 먹었죠",
          "original_form": "저어제 배달시킨거여서 ㅋㅋㅋ 넵 그거도 시켜서 먹었죠",
          "speaker_id": "1",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100034478.1.8",
          "form": "양이 엄청 많더라구요",
          "original_form": "양이 엄청 많더라구요",
          "speaker_id": "1",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100034478.1.9",
          "form": "아~배달해서 드신거였군요",
          "original_form": "아~배달해서 드신거였군요",
          "speaker_id": "2",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100034478.1.10",
          "form": "저녁은 뭐드실건가요?",
          "original_form": "저녁은 뭐드실건가요?",
          "speaker_id": "2",
          "time": "20210828 13:49"
        },
        {
          "id": "MDRW2100034478.1.11",
          "form": "글쎄요 흠 ..가볍게 먹어야할거같아요",
          "original_form": "글쎄요 흠 ..가볍게 먹어야할거같아요",
          "speaker_id": "1",
          "time": "20210828 13:49"
        },
        {
          "id": "MDRW2100034478.1.12",
          "form": "n년째 말만 다이어터라서요 ㅋㅋㅋ",
          "original_form": "n년째 말만 다이어터라서요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:49"
        },
        {
          "id": "MDRW2100034478.1.13",
          "form": "ㅋㅋㅋㅋ그쵸 저녁만 조절해도 괜찮아요!",
          "original_form": "ㅋㅋㅋㅋ그쵸 저녁만 조절해도 괜찮아요!",
          "speaker_id": "2",
          "time": "20210828 13:50"
        },
        {
          "id": "MDRW2100034478.1.14",
          "form": "저도 맨날 말만 살빼야지 그래요ㅋㅋㅋ",
          "original_form": "저도 맨날 말만 살빼야지 그래요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:51"
        },
        {
          "id": "MDRW2100034478.1.15",
          "form": "맞아여 ㅋㅋㅋ",
          "original_form": "맞아여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:52"
        },
        {
          "id": "MDRW2100034478.1.16",
          "form": "다이어트 성공하는 사람 넘 신기해요",
          "original_form": "다이어트 성공하는 사람 넘 신기해요",
          "speaker_id": "1",
          "time": "20210828 13:52"
        },
        {
          "id": "MDRW2100034478.1.17",
          "form": "그니까요ㅠ 의지가 강한사람들이에요",
          "original_form": "그니까요ㅠ 의지가 강한사람들이에요",
          "speaker_id": "2",
          "time": "20210828 14:04"
        },
        {
          "id": "MDRW2100034478.1.18",
          "form": "저녁메뉴 추천 하실거있으신가요?",
          "original_form": "저녁메뉴 추천 하실거있으신가요?",
          "speaker_id": "2",
          "time": "20210828 14:05"
        },
        {
          "id": "MDRW2100034478.1.19",
          "form": "주말에 일했으니까 보상을 해줘야겠어요ㅋㅋㅋ",
          "original_form": "주말에 일했으니까 보상을 해줘야겠어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:05"
        },
        {
          "id": "MDRW2100034478.1.20",
          "form": "아 타당한 선택이십니다",
          "original_form": "아 타당한 선택이십니다",
          "speaker_id": "1",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100034478.1.21",
          "form": "흠 추천이라면.. 차돌박이 떡볶이 어떠세요??",
          "original_form": "흠 추천이라면.. 차돌박이 떡볶이 어떠세요??",
          "speaker_id": "1",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100034478.1.22",
          "form": "오! 좋네요 새로운메뉴예요",
          "original_form": "오! 좋네요 새로운메뉴예요",
          "speaker_id": "2",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100034478.1.23",
          "form": "어디꺼가 맛있나요?",
          "original_form": "어디꺼가 맛있나요?",
          "speaker_id": "2",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100034478.1.24",
          "form": "청년다방이나 사이드쇼 추천해요!",
          "original_form": "청년다방이나 사이드쇼 추천해요!",
          "speaker_id": "1",
          "time": "20210828 14:10"
        },
        {
          "id": "MDRW2100034478.1.25",
          "form": "청년다방이 더 유명하긴한데 전 너무 달아서 사이드쇼가 더 맛있더라구요 ㅋㅋㅋ 고기의 깊은맛이 국물에 배서 최고 ㅠㅠ",
          "original_form": "청년다방이 더 유명하긴한데 전 너무 달아서 사이드쇼가 더 맛있더라구요 ㅋㅋㅋ 고기의 깊은맛이 국물에 배서 최고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 14:10"
        },
        {
          "id": "MDRW2100034478.1.26",
          "form": "사이드쇼도 브랜드 이름인가요?",
          "original_form": "사이드쇼도 브랜드 이름인가요?",
          "speaker_id": "2",
          "time": "20210828 14:10"
        },
        {
          "id": "MDRW2100034478.1.27",
          "form": "오~설명들으니까 더 맛있어보이네요",
          "original_form": "오~설명들으니까 더 맛있어보이네요",
          "speaker_id": "2",
          "time": "20210828 14:10"
        },
        {
          "id": "MDRW2100034478.1.28",
          "form": "네 ㅋㅋㅋ 거긴 좀 덜 유명하긴해요 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ 거긴 좀 덜 유명하긴해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:11"
        },
        {
          "id": "MDRW2100034478.1.29",
          "form": "코엑스에있는데 평일에도 줄한시간씩 서서 먹더라구요! 요샌 몰겠네요",
          "original_form": "코엑스에있는데 평일에도 줄한시간씩 서서 먹더라구요! 요샌 몰겠네요",
          "speaker_id": "1",
          "time": "20210828 14:11"
        },
        {
          "id": "MDRW2100034478.1.30",
          "form": "와 줄서서먹으면 진짜 맛있긴한가봐요!",
          "original_form": "와 줄서서먹으면 진짜 맛있긴한가봐요!",
          "speaker_id": "2",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100034478.1.31",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100034478.1.32",
          "form": "거기 버터갈릭 감튀인가 치즈감튀있는데 필수입니다",
          "original_form": "거기 버터갈릭 감튀인가 치즈감튀있는데 필수입니다",
          "speaker_id": "1",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100034478.1.33",
          "form": "청년다방도 있어요 버터갈릭ㅋㅋㅋ",
          "original_form": "청년다방도 있어요 버터갈릭ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100034478.1.34",
          "form": "기회되면 드셔보셔요",
          "original_form": "기회되면 드셔보셔요",
          "speaker_id": "1",
          "time": "20210828 14:13"
        }
      ]
