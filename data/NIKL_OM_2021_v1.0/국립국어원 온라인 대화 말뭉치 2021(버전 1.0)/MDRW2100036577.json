      "utterance": [
        {
          "id": "MDRW2100036577.1.1",
          "form": "다 이제 좋아하니",
          "original_form": "다 이제 좋아하니",
          "speaker_id": "2",
          "time": "20210831 00:58"
        },
        {
          "id": "MDRW2100036577.1.2",
          "form": "초코맛 좋아해",
          "original_form": "초코맛 좋아해",
          "speaker_id": "1",
          "time": "20210831 00:58"
        },
        {
          "id": "MDRW2100036577.1.3",
          "form": "그거 요즘은 얇게 도 나오던데",
          "original_form": "그거 요즘은 얇게 도 나오던데",
          "speaker_id": "2",
          "time": "20210831 00:58"
        },
        {
          "id": "MDRW2100036577.1.4",
          "form": "초코틴틴이 더 맛있었는데",
          "original_form": "초코틴틴이 더 맛있었는데",
          "speaker_id": "1",
          "time": "20210831 00:58"
        },
        {
          "id": "MDRW2100036577.1.5",
          "form": "맞아 그거 없어져서 아쉬워",
          "original_form": "맞아 그거 없어져서 아쉬워",
          "speaker_id": "2",
          "time": "20210831 00:58"
        },
        {
          "id": "MDRW2100036577.1.6",
          "form": "다시 나오긴 했는데 통통하게 나왔어",
          "original_form": "다시 나오긴 했는데 통통하게 나왔어",
          "speaker_id": "1",
          "time": "20210831 00:59"
        },
        {
          "id": "MDRW2100036577.1.7",
          "form": "우리 엄마는 다이제를 좋아해",
          "original_form": "우리 엄마는 다이제를 좋아해",
          "speaker_id": "2",
          "time": "20210831 00:59"
        },
        {
          "id": "MDRW2100036577.1.8",
          "form": "나도 좋아하기는",
          "original_form": "나도 좋아하기는",
          "speaker_id": "1",
          "time": "20210831 00:59"
        },
        {
          "id": "MDRW2100036577.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210831 00:59"
        },
        {
          "id": "MDRW2100036577.1.10",
          "form": "해",
          "original_form": "해",
          "speaker_id": "1",
          "time": "20210831 00:59"
        },
        {
          "id": "MDRW2100036577.1.11",
          "form": "그래서 민트 맛을 사다 드렸어",
          "original_form": "그래서 민트 맛을 사다 드렸어",
          "speaker_id": "2",
          "time": "20210831 00:59"
        },
        {
          "id": "MDRW2100036577.1.12",
          "form": "요즘 민트 맛이 대세 네",
          "original_form": "요즘 민트 맛이 대세 네",
          "speaker_id": "1",
          "time": "20210831 00:59"
        },
        {
          "id": "MDRW2100036577.1.13",
          "form": "근데 별로 안 좋아하시던데",
          "original_form": "근데 별로 안 좋아하시던데",
          "speaker_id": "2",
          "time": "20210831 00:59"
        },
        {
          "id": "MDRW2100036577.1.14",
          "form": "역시 민트는 별로야",
          "original_form": "역시 민트는 별로야",
          "speaker_id": "1",
          "time": "20210831 00:59"
        },
        {
          "id": "MDRW2100036577.1.15",
          "form": "우리 민트를 욕하지마",
          "original_form": "우리 민트를 욕하지마",
          "speaker_id": "2",
          "time": "20210831 00:59"
        },
        {
          "id": "MDRW2100036577.1.16",
          "form": "너는 민초 다니구나",
          "original_form": "너는 민초 다니구나",
          "speaker_id": "1",
          "time": "20210831 00:59"
        }
