      "utterance": [
        {
          "id": "MDRW2100023341.1.1",
          "form": "너무 대단하셔서ㅎ",
          "original_form": "너무 대단하셔서ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:25"
        },
        {
          "id": "MDRW2100023341.1.2",
          "form": "복학하고 어찌하다보니",
          "original_form": "복학하고 어찌하다보니",
          "speaker_id": "1",
          "time": "20210830 00:25"
        },
        {
          "id": "MDRW2100023341.1.3",
          "form": "회사에서 인정해줄만하네요ㅎ",
          "original_form": "회사에서 인정해줄만하네요ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:25"
        },
        {
          "id": "MDRW2100023341.1.4",
          "form": "저도 엄청 짬있는 사람이 되어잇던거죠 ㅎㅎ",
          "original_form": "저도 엄청 짬있는 사람이 되어잇던거죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 00:25"
        },
        {
          "id": "MDRW2100023341.1.5",
          "form": "저는 솔직히 팀장님 말대로만",
          "original_form": "저는 솔직히 팀장님 말대로만",
          "speaker_id": "1",
          "time": "20210830 00:25"
        },
        {
          "id": "MDRW2100023341.1.6",
          "form": "지금 생각해보면 어린데 참 어른스럽게 일 잘햇어요",
          "original_form": "지금 생각해보면 어린데 참 어른스럽게 일 잘햇어요",
          "speaker_id": "1",
          "time": "20210830 00:26"
        },
        {
          "id": "MDRW2100023341.1.7",
          "form": "제 기억으로 연대 경제학과 엿나 그랫늗데 ㅎㅎㅎ",
          "original_form": "제 기억으로 연대 경제학과 엿나 그랫늗데 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 00:27"
        },
        {
          "id": "MDRW2100023341.1.8",
          "form": "이야ㅎ 타이밍도 좋았네요ㅎㅎ",
          "original_form": "이야ㅎ 타이밍도 좋았네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 00:28"
        },
        {
          "id": "MDRW2100023341.1.9",
          "form": "근데 힘들었죠?ㅎ",
          "original_form": "근데 힘들었죠?ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:28"
        },
        {
          "id": "MDRW2100023341.1.10",
          "form": "그쵸 ㅎㅎㅎ",
          "original_form": "그쵸 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 00:29"
        },
        {
          "id": "MDRW2100023341.1.11",
          "form": "그만큼 팔려고하면",
          "original_form": "그만큼 팔려고하면",
          "speaker_id": "2",
          "time": "20210830 00:29"
        },
        {
          "id": "MDRW2100023341.1.12",
          "form": "아 힘든건 음",
          "original_form": "아 힘든건 음",
          "speaker_id": "1",
          "time": "20210830 00:29"
        },
        {
          "id": "MDRW2100023341.1.13",
          "form": "그만한 노력이있어야하고",
          "original_form": "그만한 노력이있어야하고",
          "speaker_id": "2",
          "time": "20210830 00:29"
        },
        {
          "id": "MDRW2100023341.1.14",
          "form": "그게 마음대로 안되잖아요ㅎ",
          "original_form": "그게 마음대로 안되잖아요ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:29"
        },
        {
          "id": "MDRW2100023341.1.15",
          "form": "이게 매장별 경쟁이니",
          "original_form": "이게 매장별 경쟁이니",
          "speaker_id": "1",
          "time": "20210830 00:29"
        },
        {
          "id": "MDRW2100023341.1.16",
          "form": "영업, 서비스가 힘든데",
          "original_form": "영업, 서비스가 힘든데",
          "speaker_id": "2",
          "time": "20210830 00:29"
        },
        {
          "id": "MDRW2100023341.1.17",
          "form": "그런거라",
          "original_form": "그런거라",
          "speaker_id": "2",
          "time": "20210830 00:29"
        },
        {
          "id": "MDRW2100023341.1.18",
          "form": "본사에서보면",
          "original_form": "본사에서보면",
          "speaker_id": "1",
          "time": "20210830 00:29"
        },
        {
          "id": "MDRW2100023341.1.19",
          "form": "매출이 보여요",
          "original_form": "매출이 보여요",
          "speaker_id": "1",
          "time": "20210830 00:29"
        },
        {
          "id": "MDRW2100023341.1.20",
          "form": "네네ㅎ",
          "original_form": "네네ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:29"
        },
        {
          "id": "MDRW2100023341.1.21",
          "form": "비교 대상인거죠 서로서로",
          "original_form": "비교 대상인거죠 서로서로",
          "speaker_id": "1",
          "time": "20210830 00:29"
        },
        {
          "id": "MDRW2100023341.1.22",
          "form": "그렇겟네요 ㅡㅎ",
          "original_form": "그렇겟네요 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210830 00:30"
        },
        {
          "id": "MDRW2100023341.1.23",
          "form": "글구 그때 당시엔  제가 막내기도 했고 나이든 경력이드눟",
          "original_form": "글구 그때 당시엔  제가 막내기도 했고 나이든 경력이드눟",
          "speaker_id": "1",
          "time": "20210830 00:30"
        },
        {
          "id": "MDRW2100023341.1.24",
          "form": "매장 지원을 자주 갓거든요",
          "original_form": "매장 지원을 자주 갓거든요",
          "speaker_id": "1",
          "time": "20210830 00:30"
        },
        {
          "id": "MDRW2100023341.1.25",
          "form": "제가 근무 하는곳만 출근 하는게 아니고",
          "original_form": "제가 근무 하는곳만 출근 하는게 아니고",
          "speaker_id": "1",
          "time": "20210830 00:30"
        },
        {
          "id": "MDRW2100023341.1.26",
          "form": "아아ㅎ",
          "original_form": "아아ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:30"
        },
        {
          "id": "MDRW2100023341.1.27",
          "form": "새로 오픈하거나 리모델링으로 행사하면",
          "original_form": "새로 오픈하거나 리모델링으로 행사하면",
          "speaker_id": "1",
          "time": "20210830 00:30"
        },
        {
          "id": "MDRW2100023341.1.28",
          "form": "그건좀 힘들엇겟네요",
          "original_form": "그건좀 힘들엇겟네요",
          "speaker_id": "2",
          "time": "20210830 00:30"
        },
        {
          "id": "MDRW2100023341.1.29",
          "form": "지원나가는데",
          "original_form": "지원나가는데",
          "speaker_id": "1",
          "time": "20210830 00:30"
        },
        {
          "id": "MDRW2100023341.1.30",
          "form": "가거 지원ㅠ",
          "original_form": "가거 지원ㅠ",
          "speaker_id": "2",
          "time": "20210830 00:31"
        },
        {
          "id": "MDRW2100023341.1.31",
          "form": "수원에서 ㅋㅋ 수유리까지",
          "original_form": "수원에서 ㅋㅋ 수유리까지",
          "speaker_id": "1",
          "time": "20210830 00:31"
        },
        {
          "id": "MDRW2100023341.1.32",
          "form": "본사직원이라 할것도 많았겟네요ㅎ",
          "original_form": "본사직원이라 할것도 많았겟네요ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:31"
        },
        {
          "id": "MDRW2100023341.1.33",
          "form": "지하철 약 2시간",
          "original_form": "지하철 약 2시간",
          "speaker_id": "1",
          "time": "20210830 00:31"
        },
        {
          "id": "MDRW2100023341.1.34",
          "form": "뭔지알아요ㅎ",
          "original_form": "뭔지알아요ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:31"
        },
        {
          "id": "MDRW2100023341.1.35",
          "form": "엇?지하철로?",
          "original_form": "엇?지하철로?",
          "speaker_id": "2",
          "time": "20210830 00:31"
        },
        {
          "id": "MDRW2100023341.1.36",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210830 00:31"
        },
        {
          "id": "MDRW2100023341.1.37",
          "form": "명색이본사직원인데 회사차량 해주지는",
          "original_form": "명색이본사직원인데 회사차량 해주지는",
          "speaker_id": "2",
          "time": "20210830 00:31"
        },
        {
          "id": "MDRW2100023341.1.38",
          "form": "차로가면 답없음 ㄷㄷ",
          "original_form": "차로가면 답없음 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210830 00:31"
        },
        {
          "id": "MDRW2100023341.1.39",
          "form": "에고ㅠ",
          "original_form": "에고ㅠ",
          "speaker_id": "2",
          "time": "20210830 00:31"
        },
        {
          "id": "MDRW2100023341.1.40",
          "form": "고생많이하셨네요ㅠ",
          "original_form": "고생많이하셨네요ㅠ",
          "speaker_id": "2",
          "time": "20210830 00:32"
        },
        {
          "id": "MDRW2100023341.1.41",
          "form": "출근더 매장쪽은 8시까지거든요",
          "original_form": "출근더 매장쪽은 8시까지거든요",
          "speaker_id": "1",
          "time": "20210830 00:32"
        },
        {
          "id": "MDRW2100023341.1.42",
          "form": "지금은 안그러죠?",
          "original_form": "지금은 안그러죠?",
          "speaker_id": "2",
          "time": "20210830 00:32"
        },
        {
          "id": "MDRW2100023341.1.43",
          "form": "에고에고ㅠ",
          "original_form": "에고에고ㅠ",
          "speaker_id": "2",
          "time": "20210830 00:32"
        },
        {
          "id": "MDRW2100023341.1.44",
          "form": "그럼 새벽차타고가야하네요",
          "original_form": "그럼 새벽차타고가야하네요",
          "speaker_id": "2",
          "time": "20210830 00:32"
        },
        {
          "id": "MDRW2100023341.1.45",
          "form": "지금은  사무잖아요 ㅋ",
          "original_form": "지금은  사무잖아요 ㅋ",
          "speaker_id": "1",
          "time": "20210830 00:32"
        },
        {
          "id": "MDRW2100023341.1.46",
          "form": "아 근데 다행히 지원갊댄 10시까지  출근요",
          "original_form": "아 근데 다행히 지원갊댄 10시까지  출근요",
          "speaker_id": "1",
          "time": "20210830 00:32"
        },
        {
          "id": "MDRW2100023341.1.47",
          "form": "ㅎㅎ 지금은 그때에 비하면 힘들진않겟내요ㅎㅎ",
          "original_form": "ㅎㅎ 지금은 그때에 비하면 힘들진않겟내요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 00:32"
        },
        {
          "id": "MDRW2100023341.1.48",
          "form": "지금은 옛날말이져 ㅎㅎ",
          "original_form": "지금은 옛날말이져 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 00:32"
        },
        {
          "id": "MDRW2100023341.1.49",
          "form": "오오ㅎ 그나마 다행이네요ㅎ",
          "original_form": "오오ㅎ 그나마 다행이네요ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:32"
        }
      ]
    }
