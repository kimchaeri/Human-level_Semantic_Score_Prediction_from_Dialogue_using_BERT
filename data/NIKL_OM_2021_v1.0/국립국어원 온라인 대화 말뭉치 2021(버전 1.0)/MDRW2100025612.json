      "utterance": [
        {
          "id": "MDRW2100025612.1.1",
          "form": "왜자꾸바꿔여ㅋㅋㅋ",
          "original_form": "왜자꾸바꿔여ㅋㅋㅋ",
          "speaker_id": "2",
