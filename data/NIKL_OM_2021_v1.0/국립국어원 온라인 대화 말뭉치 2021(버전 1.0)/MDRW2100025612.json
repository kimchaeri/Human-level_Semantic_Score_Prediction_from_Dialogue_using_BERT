        },
        {
          "id": "MDRW2100025612.1.2",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210901 14:20"
        },
        {
          "id": "MDRW2100025612.1.3",
          "form": "30대라 그런지 조작법 잘 안돼서",
          "original_form": "30대라 그런지 조작법 잘 안돼서",
          "speaker_id": "1",
          "time": "20210901 14:20"
        },
        {
          "id": "MDRW2100025612.1.4",
          "form": "슬펐어요",
          "original_form": "슬펐어요",
          "speaker_id": "1",
          "time": "20210901 14:21"
        },
        {
          "id": "MDRW2100025612.1.5",
          "form": "악",
          "original_form": "악",
          "speaker_id": "2",
          "time": "20210901 14:21"
        },
        {
          "id": "MDRW2100025612.1.6",
          "form": "궁금해요어떤거예요",
          "original_form": "궁금해요어떤거예요",
          "speaker_id": "2",
          "time": "20210901 14:21"
        },
        {
          "id": "MDRW2100025612.1.7",
          "form": "달리기 1등도 20대, 집무실 이벤트도 20대",
          "original_form": "달리기 1등도 20대, 집무실 이벤트도 20대",
          "speaker_id": "1",
          "time": "20210901 14:21"
        },
        {
          "id": "MDRW2100025612.1.8",
          "form": "제페토앱인데요 거기 맵이있어요",
          "original_form": "제페토앱인데요 거기 맵이있어요",
          "speaker_id": "1",
          "time": "20210901 14:21"
        },
        {
          "id": "MDRW2100025612.1.9",
          "form": "거기서 달리기하고 집무실 찾아가서 사장님이랑 사진찍고",
          "original_form": "거기서 달리기하고 집무실 찾아가서 사장님이랑 사진찍고",
          "speaker_id": "1",
          "time": "20210901 14:21"
        },
        {
          "id": "MDRW2100025612.1.10",
          "form": "사장님연설하고 그런 행사",
          "original_form": "사장님연설하고 그런 행사",
          "speaker_id": "1",
          "time": "20210901 14:21"
        },
        {
          "id": "MDRW2100025612.1.11",
          "form": "님제페토이뻣어여",
          "original_form": "님제페토이뻣어여",
          "speaker_id": "2",
          "time": "20210901 14:49"
        },
        {
          "id": "MDRW2100025612.1.12",
          "form": "1등해서 상품 타고싶어요",
          "original_form": "1등해서 상품 타고싶어요",
          "speaker_id": "1",
          "time": "20210901 15:42"
        },
        {
          "id": "MDRW2100025612.1.13",
          "form": "할수있어요 힘내봐요",
          "original_form": "할수있어요 힘내봐요",
          "speaker_id": "2",
          "time": "20210901 16:36"
        },
        {
          "id": "MDRW2100025612.1.14",
          "form": "메타버스세계신기해요",
          "original_form": "메타버스세계신기해요",
          "speaker_id": "2",
          "time": "20210901 16:36"
        },
        {
          "id": "MDRW2100025612.1.15",
          "form": "맞아요 에스파도 8명이잖아요",
          "original_form": "맞아요 에스파도 8명이잖아요",
          "speaker_id": "1",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100025612.1.16",
          "form": "에스파8명 좀무리수라생각했었는데,..",
          "original_form": "에스파8명 좀무리수라생각했었는데,..",
          "speaker_id": "2",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100025612.1.17",
          "form": "그래도 열심히 컨셉 잡았다",
          "original_form": "그래도 열심히 컨셉 잡았다",
          "speaker_id": "1",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100025612.1.18",
          "form": "응 나름정착도한거같다",
          "original_form": "응 나름정착도한거같다",
          "speaker_id": "2",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100025612.1.19",
          "form": "그렇다",
          "original_form": "그렇다",
          "speaker_id": "1",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100025612.1.20",
