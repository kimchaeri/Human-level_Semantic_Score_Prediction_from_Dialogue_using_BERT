      "utterance": [
        {
          "id": "MDRW2100010865.1.1",
          "form": "극장 자주가시나요?",
          "original_form": "극장 자주가시나요?",
          "speaker_id": "1",
          "time": "20210823 22:31"
        },
        {
          "id": "MDRW2100010865.1.2",
          "form": "그랬는데 요새 코로나가ㅠ",
          "original_form": "그랬는데 요새 코로나가ㅠ",
          "speaker_id": "2",
          "time": "20210823 22:52"
        },
        {
          "id": "MDRW2100010865.1.3",
          "form": "장르 안가리고 다 보시나여?",
          "original_form": "장르 안가리고 다 보시나여?",
          "speaker_id": "1",
          "time": "20210823 22:53"
        },
        {
          "id": "MDRW2100010865.1.4",
          "form": "액션 좋아하는데 대작 같으면 봐요 사실",
          "original_form": "액션 좋아하는데 대작 같으면 봐요 사실",
          "speaker_id": "2",
          "time": "20210823 22:54"
        },
        {
          "id": "MDRW2100010865.1.5",
          "form": "저도요.. 극장가는데 멜로나 드라마는 뭔가 돈 아까운.. ㅎㅎㅎ",
          "original_form": "저도요.. 극장가는데 멜로나 드라마는 뭔가 돈 아까운.. ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 22:55"
        },
        {
          "id": "MDRW2100010865.1.6",
          "form": "놀란 영화가 진짜 극장 대박인데",
          "original_form": "놀란 영화가 진짜 극장 대박인데",
          "speaker_id": "1",
          "time": "20210823 22:55"
        },
        {
          "id": "MDRW2100010865.1.7",
          "form": "최근에 뭐 보셨어요?",
          "original_form": "최근에 뭐 보셨어요?",
          "speaker_id": "2",
          "time": "20210823 23:05"
        },
        {
          "id": "MDRW2100010865.1.8",
          "form": "마블에..",
          "original_form": "마블에..",
          "speaker_id": "1",
          "time": "20210823 23:06"
        },
        {
          "id": "MDRW2100010865.1.9",
          "form": "그 르마노프 나오는거 제목이..",
          "original_form": "그 르마노프 나오는거 제목이..",
          "speaker_id": "1",
          "time": "20210823 23:06"
        },
        {
          "id": "MDRW2100010865.1.10",
          "form": "음 마블은 잘 모르겠어서ㅋㅋ",
          "original_form": "음 마블은 잘 모르겠어서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:24"
        },
        {
          "id": "MDRW2100010865.1.11",
          "form": "코로나땜시 극장 못가본지 몇년된 느낌이네요..",
          "original_form": "코로나땜시 극장 못가본지 몇년된 느낌이네요..",
          "speaker_id": "1",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100010865.1.12",
          "form": "막상 가면 사람 엄청 없어서",
          "original_form": "막상 가면 사람 엄청 없어서",
          "speaker_id": "2",
          "time": "20210823 23:31"
        },
        {
          "id": "MDRW2100010865.1.13",
          "form": "안전해보임요",
          "original_form": "안전해보임요",
          "speaker_id": "2",
          "time": "20210823 23:31"
        },
        {
          "id": "MDRW2100010865.1.14",
          "form": "하긴 지하철이나 이런데가 더 바글바글하니..",
          "original_form": "하긴 지하철이나 이런데가 더 바글바글하니..",
          "speaker_id": "1",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100010865.1.15",
          "form": "콘서트나 극장 다 죽은듯..",
          "original_form": "콘서트나 극장 다 죽은듯..",
          "speaker_id": "1",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100010865.1.16",
          "form": "그러게요 진짜 지하철이 바글바글",
          "original_form": "그러게요 진짜 지하철이 바글바글",
          "speaker_id": "2",
          "time": "20210824 00:43"
        },
        {
          "id": "MDRW2100010865.1.17",
          "form": "얼렁 코로나 끝나서 극장갓음 좋겟네여",
          "original_form": "얼렁 코로나 끝나서 극장갓음 좋겟네여",
