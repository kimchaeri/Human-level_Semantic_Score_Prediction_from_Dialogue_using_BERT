          "time": "20210831 13:21"
        },
        {
          "id": "MDRW2100036836.1.3",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210831 13:22"
        },
        {
          "id": "MDRW2100036836.1.4",
          "form": "저는 가족들과 살고있어요",
          "original_form": "저는 가족들과 살고있어요",
          "speaker_id": "2",
          "time": "20210831 13:22"
        },
        {
          "id": "MDRW2100036836.1.5",
          "form": "룸메와 자취하시나요",
          "original_form": "룸메와 자취하시나요",
          "speaker_id": "2",
          "time": "20210831 13:22"
        },
        {
          "id": "MDRW2100036836.1.6",
          "form": "아뇨 저도 가족들이랑 살아요",
          "original_form": "아뇨 저도 가족들이랑 살아요",
          "speaker_id": "1",
          "time": "20210831 13:23"
        },
        {
          "id": "MDRW2100036836.1.7",
          "form": "그렇군용 ㅎㅎ",
          "original_form": "그렇군용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 13:25"
        },
        {
          "id": "MDRW2100036836.1.8",
          "form": "집안일 많아 하세요?",
          "original_form": "집안일 많아 하세요?",
          "speaker_id": "2",
          "time": "20210831 13:25"
        },
        {
          "id": "MDRW2100036836.1.9",
          "form": "아뇨 저는 많이안해요 ㅎㅎ",
          "original_form": "아뇨 저는 많이안해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 13:27"
        },
        {
          "id": "MDRW2100036836.1.10",
          "form": "ㅎㅎ 저두용 ....",
          "original_form": "ㅎㅎ 저두용 ....",
          "speaker_id": "2",
          "time": "20210831 13:28"
        },
        {
          "id": "MDRW2100036836.1.11",
          "form": "부모님이 많이 하시나요?",
          "original_form": "부모님이 많이 하시나요?",
          "speaker_id": "1",
          "time": "20210831 13:29"
        },
        {
          "id": "MDRW2100036836.1.12",
          "form": "넵..ㅎㅎㅎ",
          "original_form": "넵..ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 13:32"
        },
        {
          "id": "MDRW2100036836.1.13",
          "form": "저는 조금 돕는 정도에요",
          "original_form": "저는 조금 돕는 정도에요",
          "speaker_id": "2",
          "time": "20210831 13:32"
        },
        {
          "id": "MDRW2100036836.1.14",
          "form": "형제는 없으세요?",
          "original_form": "형제는 없으세요?",
          "speaker_id": "1",
          "time": "20210831 13:35"
        },
        {
          "id": "MDRW2100036836.1.15",
          "form": "있어요!!",
          "original_form": "있어요!!",
          "speaker_id": "2",
          "time": "20210831 13:36"
        },
        {
          "id": "MDRW2100036836.1.16",
          "form": "제가 막내입니다 ㅎㅎ",
          "original_form": "제가 막내입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 13:36"
        },
        {
          "id": "MDRW2100036836.1.17",
          "form": "형제있으세요?",
          "original_form": "형제있으세요?",
          "speaker_id": "2",
          "time": "20210831 13:36"
        },
        {
          "id": "MDRW2100036836.1.18",
          "form": "아뇨 저는 외동이에요",
          "original_form": "아뇨 저는 외동이에요",
          "speaker_id": "1",
          "time": "20210831 13:36"
        },
        {
          "id": "MDRW2100036836.1.19",
          "form": "사랑을 듬뿍 받으셨겠어요",
          "original_form": "사랑을 듬뿍 받으셨겠어요",
          "speaker_id": "2",
