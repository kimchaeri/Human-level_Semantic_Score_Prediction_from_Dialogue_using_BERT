      "utterance": [
        {
          "id": "MDRW2100002745.1.1",
          "form": "경주 여행에 대해 어떻게 생각해?",
          "original_form": "경주 여행에 대해 어떻게 생각해?",
          "speaker_id": "2",
          "time": "20210820 15:15"
        },
        {
          "id": "MDRW2100002745.1.2",
          "form": "경주 좋지.",
          "original_form": "경주 좋지.",
          "speaker_id": "1",
          "time": "20210820 15:15"
        },
        {
          "id": "MDRW2100002745.1.3",
          "form": "아무래도 신라의 수도였으니까.",
          "original_form": "아무래도 신라의 수도였으니까.",
          "speaker_id": "1",
          "time": "20210820 15:15"
        },
        {
          "id": "MDRW2100002745.1.4",
          "form": "유물도 많고 좋아.",
          "original_form": "유물도 많고 좋아.",
          "speaker_id": "1",
          "time": "20210820 15:15"
        },
        {
          "id": "MDRW2100002745.1.5",
          "form": "인구도 북적대지 않아 좋은 거 같아.",
          "original_form": "인구도 북적대지 않아 좋은 거 같아.",
          "speaker_id": "1",
          "time": "20210820 15:15"
        },
        {
          "id": "MDRW2100002745.1.6",
          "form": "맞아 나는 유물 보는걸 좋아해",
          "original_form": "맞아 나는 유물 보는걸 좋아해",
          "speaker_id": "2",
          "time": "20210820 15:15"
        },
        {
          "id": "MDRW2100002745.1.7",
          "form": "특히 첨성대를 좋아해",
          "original_form": "특히 첨성대를 좋아해",
          "speaker_id": "2",
          "time": "20210820 15:15"
        },
        {
          "id": "MDRW2100002745.1.8",
          "form": "천마총이 졸더라.",
          "original_form": "천마총이 졸더라.",
          "speaker_id": "1",
          "time": "20210820 15:15"
        },
        {
          "id": "MDRW2100002745.1.9",
          "form": "아하 첨성대를 좋아하는구나",
          "original_form": "아하 첨성대를 좋아하는구나",
          "speaker_id": "1",
          "time": "20210820 15:15"
        },
        {
          "id": "MDRW2100002745.1.10",
          "form": "밤에 가봤니?",
          "original_form": "밤에 가봤니?",
          "speaker_id": "2",
          "time": "20210820 15:16"
        },
        {
          "id": "MDRW2100002745.1.11",
          "form": "거기 밤하늘과 해 질 녘 노을도 예뻤어",
          "original_form": "거기 밤하늘과 해 질 녘 노을도 예뻤어",
          "speaker_id": "1",
          "time": "20210820 15:16"
        },
        {
          "id": "MDRW2100002745.1.12",
          "form": "그럼.",
          "original_form": "그럼.",
          "speaker_id": "1",
          "time": "20210820 15:16"
        },
        {
          "id": "MDRW2100002745.1.13",
          "form": "요즘 조명이 멋있더라.",
          "original_form": "요즘 조명이 멋있더라.",
          "speaker_id": "1",
          "time": "20210820 15:16"
        },
        {
          "id": "MDRW2100002745.1.14",
          "form": "맞아 경주는 밤에도 예쁘더라",
          "original_form": "맞아 경주는 밤에도 예쁘더라",
          "speaker_id": "2",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002745.1.15",
          "form": "고분들에도 불을 켜뒀더라구",
          "original_form": "고분들에도 불을 켜뒀더라구",
          "speaker_id": "2",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002745.1.16",
          "form": "황리단길은 가봤니?",
          "original_form": "황리단길은 가봤니?",
          "speaker_id": "2",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002745.1.17",
          "form": "당연하지.",
          "original_form": "당연하지.",
          "speaker_id": "1",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002745.1.18",
          "form": "근데 길이 좀 짧더랴",
          "original_form": "근데 길이 좀 짧더랴",
          "speaker_id": "1",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002745.1.19",
          "form": "맞아 생각보다 볼건 없었어",
          "original_form": "맞아 생각보다 볼건 없었어",
          "speaker_id": "2",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002745.1.20",
          "form": "맞아. 사실 여행이 먹고 사진찍으면 다야",
          "original_form": "맞아. 사실 여행이 먹고 사진찍으면 다야",
          "speaker_id": "1",
          "time": "20210820 15:30"
        },
        {
          "id": "MDRW2100002745.1.21",
          "form": "너는 언제 경주에 갔네?",
          "original_form": "너는 언제 경주에 갔네?",
          "speaker_id": "1",
          "time": "20210820 15:30"
        },
        {
          "id": "MDRW2100002745.1.22",
          "form": "나는 저번에 벚꽃을 보러갔너",
          "original_form": "나는 저번에 벚꽃을 보러갔너",
          "speaker_id": "2",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002745.1.23",
          "form": "경주에 벚꽃나무가 참 많더라",
          "original_form": "경주에 벚꽃나무가 참 많더라",
          "speaker_id": "2",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002745.1.24",
          "form": "아름다웠겠다",
          "original_form": "아름다웠겠다",
          "speaker_id": "1",
          "time": "20210820 15:34"
        },
        {
          "id": "MDRW2100002745.1.25",
          "form": "유적지와 벚꽃이 꽤 잘어울리는구나",
          "original_form": "유적지와 벚꽃이 꽤 잘어울리는구나",
          "speaker_id": "1",
          "time": "20210820 15:35"
        },
        {
          "id": "MDRW2100002745.1.26",
          "form": "맞아 신기한 분위기였어",
          "original_form": "맞아 신기한 분위기였어",
          "speaker_id": "2",
          "time": "20210820 15:35"
        }
      ]
    }
