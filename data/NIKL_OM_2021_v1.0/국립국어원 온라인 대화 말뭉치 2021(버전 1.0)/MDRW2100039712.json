      "utterance": [
        {
          "id": "MDRW2100039712.1.1",
          "form": "강강쥐는 잘 있나용~",
          "original_form": "강강쥐는 잘 있나용~",
          "speaker_id": "1",
          "time": "20210903 20:53"
