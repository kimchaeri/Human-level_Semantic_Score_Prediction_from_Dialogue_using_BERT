      "utterance": [
        {
          "id": "MDRW2100042066.1.1",
          "form": "마스크 쓰고나오니까 땀난다",
          "original_form": "마스크 쓰고나오니까 땀난다",
          "speaker_id": "2",
          "time": "20210906 19:35"
        },
        {
          "id": "MDRW2100042066.1.2",
          "form": "잉 밖이신가 이시간에 무ㅡㄴ일로",
          "original_form": "잉 밖이신가 이시간에 무ㅡㄴ일로",
          "speaker_id": "1",
          "time": "20210906 19:35"
        },
        {
          "id": "MDRW2100042066.1.3",
          "form": "쓰레기 버리러 나왔어",
          "original_form": "쓰레기 버리러 나왔어",
          "speaker_id": "2",
          "time": "20210906 19:35"
        },
        {
          "id": "MDRW2100042066.1.4",
          "form": "아하 맞아요 마스크 쓰니까 여름 다 끝나도 덥더라구요",
          "original_form": "아하 맞아요 마스크 쓰니까 여름 다 끝나도 덥더라구요",
          "speaker_id": "1",
          "time": "20210906 19:35"
        },
        {
          "id": "MDRW2100042066.1.5",
          "form": "지금 2년째 마스크 쓰는건데도 익숙해지질 않네",
          "original_form": "지금 2년째 마스크 쓰는건데도 익숙해지질 않네",
          "speaker_id": "2",
          "time": "20210906 19:36"
        },
        {
          "id": "MDRW2100042066.1.6",
          "form": "코로나가 진짜 무섭죠 ㅠㅠ",
          "original_form": "코로나가 진짜 무섭죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 19:36"
        },
        {
          "id": "MDRW2100042066.1.7",
          "form": "백신",
          "original_form": "백신",
          "speaker_id": "2",
          "time": "20210906 19:36"
        },
        {
          "id": "MDRW2100042066.1.8",
          "form": "백신 2차까지 다 맞으면 좀 나아지려나",
          "original_form": "백신 2차까지 다 맞으면 좀 나아지려나",
          "speaker_id": "2",
          "time": "20210906 19:36"
        },
        {
          "id": "MDRW2100042066.1.9",
          "form": "그러면 마스크 벗게되려나요 그럼 좀 나을텐데",
          "original_form": "그러면 마스크 벗게되려나요 그럼 좀 나을텐데",
          "speaker_id": "1",
          "time": "20210906 19:36"
        },
        {
          "id": "MDRW2100042066.1.10",
          "form": "얼른 다들 백신 맞았으면 좋겠다",
          "original_form": "얼른 다들 백신 맞았으면 좋겠다",
          "speaker_id": "2",
          "time": "20210906 19:36"
        },
        {
          "id": "MDRW2100042066.1.11",
          "form": "저두 마스크 대문에 힘들어서 운동 못하겠어요",
          "original_form": "저두 마스크 대문에 힘들어서 운동 못하겠어요",
          "speaker_id": "1",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100042066.1.12",
          "form": "핑계대는거 아니구?",
          "original_form": "핑계대는거 아니구?",
          "speaker_id": "2",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100042066.1.13",
          "form": "쪼끔은 그럴수도 있지만 진짜 힘들다구요 ㅎ ㅎ",
          "original_form": "쪼끔은 그럴수도 있지만 진짜 힘들다구요 ㅎ ㅎ",
          "speaker_id": "1",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100042066.1.14",
          "form": "하긴 마스크 쓰니까 폐활량 떨어지는건지 많이 필요한건지",
          "original_form": "하긴 마스크 쓰니까 폐활량 떨어지는건지 많이 필요한건지",
          "speaker_id": "2",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100042066.1.15",
          "form": "아무튼 너무 불편해요 ㅠ",
          "original_form": "아무튼 너무 불편해요 ㅠ",
          "speaker_id": "1",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100042066.1.16",
          "form": "덴탈마스크 쓰면 좀 나은데",
          "original_form": "덴탈마스크 쓰면 좀 나은데",
          "speaker_id": "2",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100042066.1.17",
          "form": "근데 그거는 또 그렇잔아요",
          "original_form": "근데 그거는 또 그렇잔아요",
          "speaker_id": "1",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100042066.1.18",
          "form": "KF94 안쓰면 눈치보이지",
          "original_form": "KF94 안쓰면 눈치보이지",
          "speaker_id": "2",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100042066.1.19",
          "form": "맞아요 지난번에 결혼식장에서도 저희만 덴탈마스크 섰고",
          "original_form": "맞아요 지난번에 결혼식장에서도 저희만 덴탈마스크 섰고",
          "speaker_id": "1",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100042066.1.20",
          "form": "그건 우리가 잘못했지만",
          "original_form": "그건 우리가 잘못했지만",
          "speaker_id": "2",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100042066.1.21",
          "form": "ㅎㅎㅎ 좀 쪽팔렸어요",
          "original_form": "ㅎㅎㅎ 좀 쪽팔렸어요",
          "speaker_id": "1",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100042066.1.22",
          "form": "새부리형 말고 큰거가 편하더라 엄마는",
          "original_form": "새부리형 말고 큰거가 편하더라 엄마는",
          "speaker_id": "2",
          "time": "20210906 19:41"
        },
        {
          "id": "MDRW2100042066.1.23",
          "form": "저두요 대형 쓰면   숨이 덜 막히는거같아요",
          "original_form": "저두요 대형 쓰면   숨이 덜 막히는거같아요",
          "speaker_id": "1",
          "time": "20210906 19:41"
        },
        {
          "id": "MDRW2100042066.1.24",
          "form": "새부리형은 대형 없나?",
          "original_form": "새부리형은 대형 없나?",
          "speaker_id": "2",
          "time": "20210906 19:41"
        },
        {
