      "utterance": [
        {
          "id": "MDRW2100028992.1.1",
          "form": "사실 여름 너무 더워서 비왔으면 했거든",
          "original_form": "사실 여름 너무 더워서 비왔으면 했거든",
          "speaker_id": "1",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028992.1.2",
          "form": "근데 비오니까 한번에 우르르온다...",
          "original_form": "근데 비오니까 한번에 우르르온다...",
          "speaker_id": "1",
          "time": "20210902 23:14"
        },
        {
          "id": "MDRW2100028992.1.3",
          "form": "그니까 천둥도 치고...",
          "original_form": "그니까 천둥도 치고...",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028992.1.4",
          "form": "여름 너무 더우니까 비왔으면 했는데",
          "original_form": "여름 너무 더우니까 비왔으면 했는데",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028992.1.5",
          "form": "맞아 나 어제 천둥쳐서 우리집 전등깜빡였다ㅠ",
          "original_form": "맞아 나 어제 천둥쳐서 우리집 전등깜빡였다ㅠ",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028992.1.6",
          "form": "과하니까 별로지?",
          "original_form": "과하니까 별로지?",
          "speaker_id": "2",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028992.1.7",
          "form": "헐 그건 큰일인데",
          "original_form": "헐 그건 큰일인데",
          "speaker_id": "2",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100028992.1.8",
          "form": "여름 너무 극과극이야 더울건가 비계속올건가",
          "original_form": "여름 너무 극과극이야 더울건가 비계속올건가",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028992.1.9",
          "form": "다행히 이젠 괜찮은데 :(",
          "original_form": "다행히 이젠 괜찮은데 :(",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028992.1.10",
          "form": "하나만 했음 좋겠어",
          "original_form": "하나만 했음 좋겠어",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028992.1.11",
          "form": "우산들고다니기도 힘들고",
          "original_form": "우산들고다니기도 힘들고",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028992.1.12",
          "form": "역시 난 겨울이좋아",
          "original_form": "역시 난 겨울이좋아",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028992.1.13",
          "form": "일단 벌레가 싫어엇",
          "original_form": "일단 벌레가 싫어엇",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100028992.1.14",
          "form": "그건 그래 나도 벌레는 진짜 싫어",
          "original_form": "그건 그래 나도 벌레는 진짜 싫어",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100028992.1.15",
          "form": "여름에 특히 많지",
          "original_form": "여름에 특히 많지",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100028992.1.16",
          "form": "그거 알아? 도시에 매미가 더 많대",
          "original_form": "그거 알아? 도시에 매미가 더 많대",
          "speaker_id": "1",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100028992.1.17",
          "form": "시골이 더 적막하다는데",
          "original_form": "시골이 더 적막하다는데",
          "speaker_id": "1",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100028992.1.18",
          "form": "헉...진짜?왜지",
          "original_form": "헉...진짜?왜지",
          "speaker_id": "2",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100028992.1.19",
          "form": "나도 여름에 시끄러운 매미소리말고 적막하게 낭만을 즐기고싶다구",
          "original_form": "나도 여름에 시끄러운 매미소리말고 적막하게 낭만을 즐기고싶다구",
          "speaker_id": "1",
          "time": "20210902 23:19"
        },
        {
          "id": "MDRW2100028992.1.20",
          "form": "여름에 조용한 감각도 즐거울 것 같은데 말야",
          "original_form": "여름에 조용한 감각도 즐거울 것 같은데 말야",
          "speaker_id": "2",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100028992.1.21",
          "form": "일단 조명때문에 환하고 매미 더운거 좋아하는데 도시가 시골보다 덥대ㅠ",
          "original_form": "일단 조명때문에 환하고 매미 더운거 좋아하는데 도시가 시골보다 덥대ㅠ",
          "speaker_id": "1",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100028992.1.22",
          "form": "나두... 휴 아직도 매미소리들린다",
          "original_form": "나두... 휴 아직도 매미소리들린다",
          "speaker_id": "1",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100028992.1.23",
          "form": "우린 그래도 조용한 것 같아",
          "original_form": "우린 그래도 조용한 것 같아",
          "speaker_id": "2",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100028992.1.24",
          "form": "여름은 비오는 거 말곤 다 싫어",
          "original_form": "여름은 비오는 거 말곤 다 싫어",
          "speaker_id": "2",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100028992.1.25",
          "form": "폭풍아니라면 나도 비오는건 좋아",
          "original_form": "폭풍아니라면 나도 비오는건 좋아",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100028992.1.26",
          "form": "작년에 너무 폭풍몰아쳐서 힘들었어ㅠ",
          "original_form": "작년에 너무 폭풍몰아쳐서 힘들었어ㅠ",
          "speaker_id": "1",
          "time": "20210902 23:23"
        }
      ]
    }
