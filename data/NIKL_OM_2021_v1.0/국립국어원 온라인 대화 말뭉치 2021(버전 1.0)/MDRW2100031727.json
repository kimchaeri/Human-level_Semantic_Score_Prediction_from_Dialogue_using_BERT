      "utterance": [
        {
          "id": "MDRW2100031727.1.1",
          "form": "미용 아,, 자주가세요(?)",
          "original_form": "미용 아,, 자주가세요(?)",
          "speaker_id": "2",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100031727.1.2",
          "form": "제가 제출을 해버려서ㅎㅎ 주제가 마땅치가 않네여ㅎㅎ",
          "original_form": "제가 제출을 해버려서ㅎㅎ 주제가 마땅치가 않네여ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100031727.1.3",
          "form": "전 자주가야하는데 안가요ㅎㅎ",
          "original_form": "전 자주가야하는데 안가요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100031727.1.4",
          "form": "전 자주안가요",
          "original_form": "전 자주안가요",
          "speaker_id": "2",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100031727.1.5",
          "form": "한 2달에 한번정도?",
          "original_form": "한 2달에 한번정도?",
          "speaker_id": "1",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100031727.1.6",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100031727.1.7",
          "form": "비슷",
          "original_form": "비슷",
          "speaker_id": "2",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100031727.1.8",
          "form": "글죠ㅎㅎ",
          "original_form": "글죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 23:53"
        },
        {
          "id": "MDRW2100031727.1.9",
          "form": "가서 그냥 쨟게요",
          "original_form": "가서 그냥 쨟게요",
          "speaker_id": "2",
          "time": "20210904 23:53"
        },
        {
          "id": "MDRW2100031727.1.10",
          "form": "다똑같을꺼에요ㅎ",
          "original_form": "다똑같을꺼에요ㅎ",
          "speaker_id": "1",
          "time": "20210904 23:53"
        },
        {
          "id": "MDRW2100031727.1.11",
          "form": "스타일있으세요?",
          "original_form": "스타일있으세요?",
          "speaker_id": "2",
          "time": "20210904 23:53"
        },
        {
          "id": "MDRW2100031727.1.12",
          "form": "저두요ㅎㅎ",
          "original_form": "저두요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 23:53"
        },
        {
          "id": "MDRW2100031727.1.13",
          "form": "아뇨 없어요ㅎ",
          "original_form": "아뇨 없어요ㅎ",
          "speaker_id": "1",
          "time": "20210904 23:53"
        },
        {
          "id": "MDRW2100031727.1.14",
          "form": "무조건 짧게ㅎㅎ",
          "original_form": "무조건 짧게ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 23:53"
        },
        {
          "id": "MDRW2100031727.1.15",
          "form": "숫많이요",
          "original_form": "숫많이요",
          "speaker_id": "2",
          "time": "20210904 23:53"
        },
        {
          "id": "MDRW2100031727.1.16",
          "form": "ㅎㅎ 남자들은 다똑같죠ㅎㅎ",
          "original_form": "ㅎㅎ 남자들은 다똑같죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 23:54"
        },
        {
          "id": "MDRW2100031727.1.17",
          "form": "애~그래도 투블럭이나 뭐 어떤컷 해주세요 하는분들도있음ㅋ",
          "original_form": "애~그래도 투블럭이나 뭐 어떤컷 해주세요 하는분들도있음ㅋ",
          "speaker_id": "2",
          "time": "20210904 23:54"
        },
        {
          "id": "MDRW2100031727.1.18",
          "form": "전원래 2달에 한번 짜르면 안되는 머리에요ㅎ 모발이굵고 엄청 빨리자라는 모리라",
          "original_form": "전원래 2달에 한번 짜르면 안되는 머리에요ㅎ 모발이굵고 엄청 빨리자라는 모리라",
          "speaker_id": "1",
          "time": "20210904 23:55"
        },
        {
          "id": "MDRW2100031727.1.19",
          "form": "머리라ㅎ 돈아낀다고 좀 늦게가는것뿐이에요ㅎ",
          "original_form": "머리라ㅎ 돈아낀다고 좀 늦게가는것뿐이에요ㅎ",
          "speaker_id": "1",
          "time": "20210904 23:55"
        },
        {
          "id": "MDRW2100031727.1.20",
          "form": "저는 짧게요 지금생각하신것보다 더 짧게요",
          "original_form": "저는 짧게요 지금생각하신것보다 더 짧게요",
          "speaker_id": "2",
          "time": "20210904 23:55"
        },
        {
          "id": "MDRW2100031727.1.21",
          "form": "라고함ㅋ",
          "original_form": "라고함ㅋ",
          "speaker_id": "2",
          "time": "20210904 23:55"
        },
        {
          "id": "MDRW2100031727.1.22",
          "form": "ㅎㅎ 저두요ㅎ",
          "original_form": "ㅎㅎ 저두요ㅎ",
          "speaker_id": "1",
          "time": "20210904 23:55"
        },
        {
          "id": "MDRW2100031727.1.23",
          "form": "근데 미용실은 다시와야 이득이니깐 항상 내가생각한거보다는 짧게 안짤라줭ㅅ",
          "original_form": "근데 미용실은 다시와야 이득이니깐 항상 내가생각한거보다는 짧게 안짤라줭ㅅ",
          "speaker_id": "1",
          "time": "20210904 23:56"
        },
        {
          "id": "MDRW2100031727.1.24",
          "form": "아껴아죠",
          "original_form": "아껴아죠",
          "speaker_id": "2",
          "time": "20210904 23:56"
        },
        {
          "id": "MDRW2100031727.1.25",
          "form": "다시",
          "original_form": "다시",
          "speaker_id": "2",
          "time": "20210904 23:56"
        },
        {
          "id": "MDRW2100031727.1.26",
          "form": "ㄱㄱ요ㅎㅎ",
