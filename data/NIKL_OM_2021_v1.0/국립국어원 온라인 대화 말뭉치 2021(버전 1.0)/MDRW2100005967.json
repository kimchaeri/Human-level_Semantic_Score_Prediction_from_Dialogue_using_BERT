      "utterance": [
        {
          "id": "MDRW2100005967.1.1",
          "form": "집콕 나만의 비법 있으신가여",
          "original_form": "집콕 나만의 비법 있으신가여",
          "speaker_id": "1",
          "time": "20210907 17:49"
        },
        {
          "id": "MDRW2100005967.1.2",
          "form": "집콕...그냥 집에서 넷플릭스 보구, 홈트하구~",
          "original_form": "집콕...그냥 집에서 넷플릭스 보구, 홈트하구~",
          "speaker_id": "2",
          "time": "20210907 17:49"
        },
        {
          "id": "MDRW2100005967.1.3",
          "form": "제가 저번달에 밀접접촉자였어서",
          "original_form": "제가 저번달에 밀접접촉자였어서",
          "speaker_id": "2",
          "time": "20210907 17:49"
        },
        {
          "id": "MDRW2100005967.1.4",
          "form": "2주동안 집에서 격리됐었는데",
          "original_form": "2주동안 집에서 격리됐었는데",
          "speaker_id": "2",
          "time": "20210907 17:49"
        },
        {
          "id": "MDRW2100005967.1.5",
          "form": "진짜 그땐 미치는줄 알았어요 ㅠ.ㅠ",
          "original_form": "진짜 그땐 미치는줄 알았어요 ㅠ.ㅠ",
          "speaker_id": "2",
          "time": "20210907 17:49"
        },
        {
          "id": "MDRW2100005967.1.6",
          "form": "하하 저도 작년에 인도에서 입국했는데",
          "original_form": "하하 저도 작년에 인도에서 입국했는데",
          "speaker_id": "1",
          "time": "20210907 17:51"
        },
        {
          "id": "MDRW2100005967.1.7",
          "form": "외향적이어서 힘들었어요",
          "original_form": "외향적이어서 힘들었어요",
          "speaker_id": "1",
          "time": "20210907 17:51"
        },
        {
          "id": "MDRW2100005967.1.8",
          "form": "헉 그때도 그 생활센터인가 거기서 격리하죠?",
          "original_form": "헉 그때도 그 생활센터인가 거기서 격리하죠?",
          "speaker_id": "2",
          "time": "20210907 17:51"
        },
        {
          "id": "MDRW2100005967.1.9",
          "form": "저는 단독주택에서 있었어요",
          "original_form": "저는 단독주택에서 있었어요",
          "speaker_id": "1",
          "time": "20210907 17:53"
        },
        {
          "id": "MDRW2100005967.1.10",
          "form": "대신 코로나 검사 격리 시작일 마지막일 2번했구요",
          "original_form": "대신 코로나 검사 격리 시작일 마지막일 2번했구요",
          "speaker_id": "1",
          "time": "20210907 17:53"
        },
        {
          "id": "MDRW2100005967.1.11",
          "form": "그때 흠 뭐했더라",
          "original_form": "그때 흠 뭐했더라",
          "speaker_id": "1",
          "time": "20210907 17:53"
        },
        {
          "id": "MDRW2100005967.1.12",
          "form": "반신욕",
          "original_form": "반신욕",
          "speaker_id": "1",
          "time": "20210907 17:53"
        },
        {
          "id": "MDRW2100005967.1.13",
          "form": "TV",
          "original_form": "TV",
          "speaker_id": "1",
          "time": "20210907 17:53"
        },
        {
          "id": "MDRW2100005967.1.14",
          "form": "배달의민족 메뉴보기",
          "original_form": "배달의민족 메뉴보기",
          "speaker_id": "1",
          "time": "20210907 17:54"
        },
        {
          "id": "MDRW2100005967.1.15",
          "form": "배달의 민족 메뉴보기 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "배달의 민족 메뉴보기 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100005967.1.16",
          "form": "저도 그런거 비슷한거 좋아해요... 광고 보기나",
          "original_form": "저도 그런거 비슷한거 좋아해요... 광고 보기나",
          "speaker_id": "2",
          "time": "20210907 17:56"
        },
        {
          "id": "MDRW2100005967.1.17",
          "form": "유튜브 보구~",
          "original_form": "유튜브 보구~",
          "speaker_id": "2",
          "time": "20210907 17:56"
        },
        {
          "id": "MDRW2100005967.1.18",
          "form": "뉴스는 보기 싫어서요",
          "original_form": "뉴스는 보기 싫어서요",
          "speaker_id": "1",
          "time": "20210907 17:56"
        },
        {
          "id": "MDRW2100005967.1.19",
          "form": "오 유튜브",
          "original_form": "오 유튜브",
          "speaker_id": "1",
          "time": "20210907 17:56"
        },
        {
          "id": "MDRW2100005967.1.20",
          "form": "저 최근에 침착맨 입덕했어요",
          "original_form": "저 최근에 침착맨 입덕했어요",
          "speaker_id": "1",
          "time": "20210907 17:56"
        },
        {
          "id": "MDRW2100005967.1.21",
          "form": "헉 침착맨 좋죠",
          "original_form": "헉 침착맨 좋죠",
          "speaker_id": "2",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100005967.1.22",
          "form": "저는 설거지할때나 집안일할때 항상",
          "original_form": "저는 설거지할때나 집안일할때 항상",
          "speaker_id": "2",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100005967.1.23",
          "form": "침착맨 원본박물관 켜놓고",
          "original_form": "침착맨 원본박물관 켜놓고",
          "speaker_id": "2",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100005967.1.24",
          "form": "침펄풍 셋이서 떠드는거 들으면서 일해요 ㅋㅋㅋ",
          "original_form": "침펄풍 셋이서 떠드는거 들으면서 일해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:02"
        },
        {
          "id": "MDRW2100005967.1.25",
          "form": "아 콘텐츠로 좀따 합시다 저 완전 이야기하고 싶은데 제 친구들 안봐서 답답",
          "original_form": "아 콘텐츠로 좀따 합시다 저 완전 이야기하고 싶은데 제 친구들 안봐서 답답",
          "speaker_id": "1",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100005967.1.26",
          "form": "라디오인가요 ㅎㅎ",
          "original_form": "라디오인가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100005967.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ 원본박물관 구독해놓으셨죠?!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 원본박물관 구독해놓으셨죠?!",
          "speaker_id": "2",
          "time": "20210907 18:11"
        },
        {
          "id": "MDRW2100005967.1.28",
          "form": "그게 진짜 완전 꿀이여요",
          "original_form": "그게 진짜 완전 꿀이여요",
          "speaker_id": "2",
          "time": "20210907 18:11"
        },
        {
          "id": "MDRW2100005967.1.29",
          "form": "요즘 근데 쏘영이가 안나와서 슬퍼요 ㅠ..",
          "original_form": "요즘 근데 쏘영이가 안나와서 슬퍼요 ㅠ..",
          "speaker_id": "2",
          "time": "20210907 18:11"
        },
        {
          "id": "MDRW2100005967.1.30",
          "form": "아 쏘영삼촌이시군요",
          "original_form": "아 쏘영삼촌이시군요",
          "speaker_id": "1",
          "time": "20210907 18:12"
        },
        {
          "id": "MDRW2100005967.1.31",
          "form": "기엽기엽...",
          "original_form": "기엽기엽...",
          "speaker_id": "2",
          "time": "20210907 18:13"
        },
        {
          "id": "MDRW2100005967.1.32",
          "form": "예전 영상들도 보셨어욤?",
          "original_form": "예전 영상들도 보셨어욤?",
          "speaker_id": "2",
          "time": "20210907 18:14"
        },
        {
          "id": "MDRW2100005967.1.33",
          "form": "역시 유튜브가 집콕의 완성",
          "original_form": "역시 유튜브가 집콕의 완성",
          "speaker_id": "1",
          "time": "20210907 18:14"
        },
        {
          "id": "MDRW2100005967.1.34",
          "form": "요즘 왕날편들어요",
          "original_form": "요즘 왕날편들어요",
          "speaker_id": "1",
          "time": "20210907 18:14"
        }
      ]
    }
