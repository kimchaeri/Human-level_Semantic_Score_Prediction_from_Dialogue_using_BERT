      "utterance": [
        {
          "id": "MDRW2100015965.1.1",
          "form": "싸이월드 하셨나용?",
          "original_form": "싸이월드 하셨나용?",
          "speaker_id": "1",
          "time": "20210826 04:06"
        },
        {
          "id": "MDRW2100015965.1.2",
          "form": "아뇨 저는 싸이월드는 안했어요~",
          "original_form": "아뇨 저는 싸이월드는 안했어요~",
          "speaker_id": "2",
          "time": "20210826 04:06"
        },
        {
          "id": "MDRW2100015965.1.3",
          "form": "버디버디까지가 마지노선이었던 것 같아요 ㅋㅋㅋㅋ",
          "original_form": "버디버디까지가 마지노선이었던 것 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 04:07"
        },
        {
          "id": "MDRW2100015965.1.4",
          "form": "아하ㅋㅋㅋ 싸이월드랑 버디버디랑",
          "original_form": "아하ㅋㅋㅋ 싸이월드랑 버디버디랑",
          "speaker_id": "1",
          "time": "20210826 04:07"
        },
        {
          "id": "MDRW2100015965.1.5",
          "form": "싸이월드 하셨었나요?ㅋㅋㅋㅋ",
          "original_form": "싸이월드 하셨었나요?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 04:07"
        },
        {
          "id": "MDRW2100015965.1.6",
          "form": "투탑이었죵",
          "original_form": "투탑이었죵",
          "speaker_id": "1",
          "time": "20210826 04:07"
        },
        {
          "id": "MDRW2100015965.1.7",
          "form": "저는 했어요!",
          "original_form": "저는 했어요!",
          "speaker_id": "1",
          "time": "20210826 04:07"
        },
        {
          "id": "MDRW2100015965.1.8",
          "form": "그쵸 ㅋㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 04:07"
        },
        {
          "id": "MDRW2100015965.1.9",
          "form": "최근에 부활했다고해서",
          "original_form": "최근에 부활했다고해서",
          "speaker_id": "1",
          "time": "20210826 04:07"
        },
        {
          "id": "MDRW2100015965.1.10",
          "form": "좀 걱정이 되지만..ㅎ",
          "original_form": "좀 걱정이 되지만..ㅎ",
          "speaker_id": "1",
          "time": "20210826 04:07"
        },
        {
          "id": "MDRW2100015965.1.11",
          "form": "오오 일촌신청 좀 하셨겠네요 ㅋㅋㅋㅋ",
          "original_form": "오오 일촌신청 좀 하셨겠네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 04:07"
        },
        {
          "id": "MDRW2100015965.1.12",
          "form": "네 도토리도 많이 샀어요ㅠ",
          "original_form": "네 도토리도 많이 샀어요ㅠ",
          "speaker_id": "1",
          "time": "20210826 04:07"
        },
        {
          "id": "MDRW2100015965.1.13",
          "form": "아깝네요",
          "original_form": "아깝네요",
          "speaker_id": "1",
          "time": "20210826 04:07"
        },
        {
          "id": "MDRW2100015965.1.14",
          "form": "도토리 환불 안되나여?ㅠㅠ",
          "original_form": "도토리 환불 안되나여?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 04:08"
        },
        {
          "id": "MDRW2100015965.1.15",
          "form": "안쓴건 마지막에 접을때 일부 환불해준다고도 봤는데",
          "original_form": "안쓴건 마지막에 접을때 일부 환불해준다고도 봤는데",
          "speaker_id": "1",
          "time": "20210826 04:08"
        },
        {
          "id": "MDRW2100015965.1.16",
          "form": "저는 충전하는족족 써버려서ㅋㅋ",
          "original_form": "저는 충전하는족족 써버려서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 04:08"
        },
        {
          "id": "MDRW2100015965.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 04:08"
        },
        {
          "id": "MDRW2100015965.1.18",
          "form": "쓰려고 사는거잖아요!",
          "original_form": "쓰려고 사는거잖아요!",
          "speaker_id": "2",
          "time": "20210826 04:09"
        },
        {
          "id": "MDRW2100015965.1.19",
          "form": "배경음악도 사고",
          "original_form": "배경음악도 사고",
          "speaker_id": "2",
          "time": "20210826 04:09"
        },
        {
          "id": "MDRW2100015965.1.20",
          "form": "그쵸ㅋㅋㅋ 문화상품권 참 많이 긁었는데",
          "original_form": "그쵸ㅋㅋㅋ 문화상품권 참 많이 긁었는데",
          "speaker_id": "1",
          "time": "20210826 04:09"
        },
        {
          "id": "MDRW2100015965.1.21",
          "form": "추억이네요",
          "original_form": "추억이네요",
          "speaker_id": "1",
          "time": "20210826 04:09"
        },
        {
          "id": "MDRW2100015965.1.22",
          "form": "스킨같은것도 도토리로 사는거죠?",
          "original_form": "스킨같은것도 도토리로 사는거죠?",
          "speaker_id": "2",
          "time": "20210826 04:09"
        },
        {
          "id": "MDRW2100015965.1.23",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210826 04:09"
        }
      ]
    }
