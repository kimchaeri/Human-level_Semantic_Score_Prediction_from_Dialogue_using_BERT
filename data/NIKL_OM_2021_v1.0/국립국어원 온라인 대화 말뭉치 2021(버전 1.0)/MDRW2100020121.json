      "utterance": [
        {
          "id": "MDRW2100020121.1.1",
          "form": "하여간 아까 본부장님이 bird소리할 때마다 표정관리 안하고 아. 네. 이러고 끝내니까",
          "original_form": "하여간 아까 본부장님이 bird소리할 때마다 표정관리 안하고 아. 네. 이러고 끝내니까",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020121.1.2",
          "form": "확실히 효과있었어요",
          "original_form": "확실히 효과있었어요",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020121.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ줄더라구요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ줄더라구요",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020121.1.4",
          "form": "한번 녹음해서",
          "original_form": "한번 녹음해서",
          "speaker_id": "1",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020121.1.5",
          "form": "직장내괴롭힘으로",
          "original_form": "직장내괴롭힘으로",
          "speaker_id": "1",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020121.1.6",
          "form": "신고해야하는거아닌가요",
          "original_form": "신고해야하는거아닌가요",
          "speaker_id": "1",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020121.1.7",
          "form": "재밌지않아? 이러면 이제 좀 적응됐다고 재미없는데요 이러고ㅋㅋㅋ",
          "original_form": "재밌지않아? 이러면 이제 좀 적응됐다고 재미없는데요 이러고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020121.1.8",
          "form": "말버릇 무슨일",
          "original_form": "말버릇 무슨일",
          "speaker_id": "1",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020121.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020121.1.10",
          "form": "뭔가 괴롭힘같으면서도 괴롭힘같지않다보니",
          "original_form": "뭔가 괴롭힘같으면서도 괴롭힘같지않다보니",
          "speaker_id": "2",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020121.1.11",
          "form": "애매해요...",
          "original_form": "애매해요...",
          "speaker_id": "2",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020121.1.12",
          "form": "누적이면 괴롭힘이에요",
          "original_form": "누적이면 괴롭힘이에요",
          "speaker_id": "1",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020121.1.13",
          "form": "한번씩 말해봐요 어? 이거 직장내괴롭힘아닌가여",
          "original_form": "한번씩 말해봐요 어? 이거 직장내괴롭힘아닌가여",
          "speaker_id": "1",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020121.1.14",
          "form": "저는 대표님 한번씩 말같지도않은농담할때마다",
          "original_form": "저는 대표님 한번씩 말같지도않은농담할때마다",
          "speaker_id": "1",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020121.1.15",
          "form": "그래욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그래욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020121.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ와우",
          "original_form": "ㅋㅋㅋㅋㅋㅋ와우",
          "speaker_id": "2",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100020121.1.17",
          "form": "장난인척 진심을 담는거져",
          "original_form": "장난인척 진심을 담는거져",
          "speaker_id": "1",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100020121.1.18",
          "form": "대표님 아무말못하시나요",
          "original_form": "대표님 아무말못하시나요",
          "speaker_id": "2",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100020121.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100020121.1.20",
          "form": "뭘 또 직장내 괴롭힘이래",
          "original_form": "뭘 또 직장내 괴롭힘이래",
          "speaker_id": "1",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100020121.1.21",
          "form": "하면서 가세요",
          "original_form": "하면서 가세요",
          "speaker_id": "1",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100020121.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ효과직빵",
          "original_form": "ㅋㅋㅋㅋㅋㅋ효과직빵",
          "speaker_id": "1",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100020121.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100020121.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ역시 직설적으로 얘길해줘야",
          "original_form": "ㅋㅋㅋㅋㅋㅋ역시 직설적으로 얘길해줘야",
          "speaker_id": "2",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100020121.1.25",
          "form": "효과가있다니까요",
          "original_form": "효과가있다니까요",
          "speaker_id": "2",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100020121.1.26",
          "form": "맞아욬ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100020121.1.27",
          "form": "물러가라 훠이",
          "original_form": "물러가라 훠이",
          "speaker_id": "1",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100020121.1.28",
          "form": "ㅋㅋ다른회사 썰보면 성희롱도 많더라구요",
          "original_form": "ㅋㅋ다른회사 썰보면 성희롱도 많더라구요",
          "speaker_id": "2",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100020121.1.29",
          "form": "그런게 없는게 그나마 다행...",
          "original_form": "그런게 없는게 그나마 다행...",
