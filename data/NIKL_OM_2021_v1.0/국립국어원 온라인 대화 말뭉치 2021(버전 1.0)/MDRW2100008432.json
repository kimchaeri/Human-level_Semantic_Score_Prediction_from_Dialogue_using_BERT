      "utterance": [
        {
          "id": "MDRW2100008432.1.1",
          "form": "게임 좋아하새요?",
          "original_form": "게임 좋아하새요?",
          "speaker_id": "2",
          "time": "20210822 15:48"
        },
        {
          "id": "MDRW2100008432.1.2",
          "form": "네 저 게임 좋아해요",
          "original_form": "네 저 게임 좋아해요",
          "speaker_id": "1",
          "time": "20210822 15:48"
        },
        {
          "id": "MDRW2100008432.1.3",
          "form": "게임 좋아하시죠?",
          "original_form": "게임 좋아하시죠?",
          "speaker_id": "1",
          "time": "20210822 15:48"
        },
        {
          "id": "MDRW2100008432.1.4",
          "form": "네 좋아하는 편이애요",
          "original_form": "네 좋아하는 편이애요",
          "speaker_id": "2",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008432.1.5",
          "form": "무슨 게임하세요?",
          "original_form": "무슨 게임하세요?",
          "speaker_id": "2",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008432.1.6",
          "form": "최근에는 오버워치랑 스타듀밸리 오버쿡드 친구들하고 했어요",
          "original_form": "최근에는 오버워치랑 스타듀밸리 오버쿡드 친구들하고 했어요",
          "speaker_id": "1",
          "time": "20210822 15:53"
        },
        {
          "id": "MDRW2100008432.1.7",
          "form": "오 저도 스타듀밸리랑 오버쿡드해요!",
          "original_form": "오 저도 스타듀밸리랑 오버쿡드해요!",
          "speaker_id": "2",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008432.1.8",
          "form": "오쿡 특히 좋아해요",
          "original_form": "오쿡 특히 좋아해요",
          "speaker_id": "2",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008432.1.9",
          "form": "오쿡 정신없어서 재밌죠ㅋㅋㅋㅋ",
          "original_form": "오쿡 정신없어서 재밌죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:54"
        },
        {
          "id": "MDRW2100008432.1.10",
          "form": "친구들이랑 디스코드켜두고 하면 너무 웃겨요",
          "original_form": "친구들이랑 디스코드켜두고 하면 너무 웃겨요",
          "speaker_id": "1",
          "time": "20210822 15:55"
        },
        {
          "id": "MDRW2100008432.1.11",
          "form": "맞아요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008432.1.12",
          "form": "유료 컨텐츠 더 서려다가",
          "original_form": "유료 컨텐츠 더 서려다가",
          "speaker_id": "2",
          "time": "20210822 15:57"
        },
        {
          "id": "MDRW2100008432.1.13",
          "form": "지갑보고 참았어요",
          "original_form": "지갑보고 참았어요",
          "speaker_id": "2",
          "time": "20210822 15:57"
        },
        {
          "id": "MDRW2100008432.1.14",
          "form": "스듀는 어떤 캐릭 좋아하새요?",
          "original_form": "스듀는 어떤 캐릭 좋아하새요?",
          "speaker_id": "2",
          "time": "20210822 15:57"
        },
        {
          "id": "MDRW2100008432.1.15",
          "form": "유독좋아하는 캐릭은 없어요",
          "original_form": "유독좋아하는 캐릭은 없어요",
          "speaker_id": "1",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008432.1.16",
          "form": "어떤캐릭 좋아해요?",
          "original_form": "어떤캐릭 좋아해요?",
          "speaker_id": "1",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008432.1.17",
          "form": "저는 헤일리 좋아했었어요 ㅎㅎ",
          "original_form": "저는 헤일리 좋아했었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 16:00"
        },
        {
          "id": "MDRW2100008432.1.18",
          "form": "맨날 선물주러 쫒아가고",
          "original_form": "맨날 선물주러 쫒아가고",
          "speaker_id": "2",
          "time": "20210822 16:00"
        },
        {
          "id": "MDRW2100008432.1.19",
          "form": "애들 공략집 열심히 외웠었내요",
          "original_form": "애들 공략집 열심히 외웠었내요",
          "speaker_id": "2",
          "time": "20210822 16:00"
        },
        {
          "id": "MDRW2100008432.1.20",
          "form": "저는 첫결혼은 페니였어요",
          "original_form": "저는 첫결혼은 페니였어요",
          "speaker_id": "1",
          "time": "20210822 16:03"
        },
        {
          "id": "MDRW2100008432.1.21",
          "form": "페니 좋아하는 선물은 아직도 기억나요ㅋㅋㅋ",
          "original_form": "페니 좋아하는 선물은 아직도 기억나요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:03"
        },
        {
          "id": "MDRW2100008432.1.22",
          "form": "저는 이제 기억 안나는데",
          "original_form": "저는 이제 기억 안나는데",
          "speaker_id": "2",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100008432.1.23",
          "form": "뭐였죠? zzz",
          "original_form": "뭐였죠? zzz",
          "speaker_id": "2",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100008432.1.24",
          "form": "양귀비랑 수선화랑 다이아몬드 등등이요ㅋㅋㅋㅋ",
          "original_form": "양귀비랑 수선화랑 다이아몬드 등등이요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008432.1.25",
          "form": "양귀비 좋아하는게 독특해서 기억나요",
          "original_form": "양귀비 좋아하는게 독특해서 기억나요",
          "speaker_id": "1",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008432.1.26",
          "form": "와 진짜 독특하네요",
          "original_form": "와 진짜 독특하네요",
          "speaker_id": "2",
          "time": "20210822 16:12"
        },
        {
          "id": "MDRW2100008432.1.27",
          "form": "잘 모르는 애들은 항상 수선화랑 다이아몬드 챙겨줬는데 ㅎㅎ",
          "original_form": "잘 모르는 애들은 항상 수선화랑 다이아몬드 챙겨줬는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 16:12"
        }
      ]
