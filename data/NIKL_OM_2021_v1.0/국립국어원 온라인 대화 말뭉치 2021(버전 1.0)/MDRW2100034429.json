      "utterance": [
        {
          "id": "MDRW2100034429.1.1",
          "form": "하 씁 진짜 안보고싶네요 범죄자들 ㅜㅜ",
          "original_form": "하 씁 진짜 안보고싶네요 범죄자들 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 10:38"
        },
        {
          "id": "MDRW2100034429.1.2",
          "form": "범죄 저질렀거나 의혹있는 연예인들 다 들어가야돼요",
          "original_form": "범죄 저질렀거나 의혹있는 연예인들 다 들어가야돼요",
          "speaker_id": "1",
          "time": "20210828 10:38"
        },
        {
          "id": "MDRW2100034429.1.3",
          "form": "이수근은 양심도 없죠",
          "original_form": "이수근은 양심도 없죠",
          "speaker_id": "1",
          "time": "20210828 10:38"
        },
        {
          "id": "MDRW2100034429.1.4",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210828 10:38"
        },
        {
          "id": "MDRW2100034429.1.5",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100034429.1.6",
          "form": "어딜..나오는지?",
          "original_form": "어딜..나오는지?",
          "speaker_id": "2",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100034429.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100034429.1.8",
          "form": "전 사실 아는형님도 그래서 안보거든요...",
          "original_form": "전 사실 아는형님도 그래서 안보거든요...",
          "speaker_id": "2",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100034429.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100034429.1.10",
          "form": "저도 싫어요 그거",
          "original_form": "저도 싫어요 그거",
          "speaker_id": "1",
          "time": "20210828 10:39"
        },
        {
          "id": "MDRW2100034429.1.11",
          "form": "알탕예능은 다 싫어요",
          "original_form": "알탕예능은 다 싫어요",
          "speaker_id": "1",
          "time": "20210828 10:40"
        },
        {
          "id": "MDRW2100034429.1.12",
          "form": "인정합니다...",
          "original_form": "인정합니다...",
          "speaker_id": "2",
          "time": "20210828 10:42"
        },
        {
          "id": "MDRW2100034429.1.13",
          "form": "알탕예능 전부 안보고싶어요ㅋㅋㅋㅋㅋ",
          "original_form": "알탕예능 전부 안보고싶어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:42"
        },
        {
          "id": "MDRW2100034429.1.14",
          "form": "전 사람들이 노홍철 좋아하는 이유도 공감을 못하겠더라고요",
          "original_form": "전 사람들이 노홍철 좋아하는 이유도 공감을 못하겠더라고요",
          "speaker_id": "2",
          "time": "20210828 10:42"
        },
        {
          "id": "MDRW2100034429.1.15",
          "form": "으 노홍철하면 빨간마후라 생각나요",
          "original_form": "으 노홍철하면 빨간마후라 생각나요",
          "speaker_id": "1",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100034429.1.16",
          "form": "웩웩",
          "original_form": "웩웩",
          "speaker_id": "2",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100034429.1.17",
          "form": "너무싫어요진짜",
          "original_form": "너무싫어요진짜",
          "speaker_id": "2",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100034429.1.18",
          "form": "뭐가 자랑이라고 얘기하고 있는지 ㅋㅋ",
          "original_form": "뭐가 자랑이라고 얘기하고 있는지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100034429.1.19",
          "form": "무슨 이슈가 생겨도 커리어에 영향 없는 남연예인들 너무 많아요",
          "original_form": "무슨 이슈가 생겨도 커리어에 영향 없는 남연예인들 너무 많아요",
          "speaker_id": "1",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100034429.1.20",
          "form": "맞아요 여자 연예인들은 짝다리 한번으로 그렇게 욕을 먹는데",
          "original_form": "맞아요 여자 연예인들은 짝다리 한번으로 그렇게 욕을 먹는데",
          "speaker_id": "2",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100034429.1.21",
          "form": "남자연예인은ㅋㅋ...참..",
          "original_form": "남자연예인은ㅋㅋ...참..",
          "speaker_id": "2",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100034429.1.22",
          "form": "그니까요!!",
          "original_form": "그니까요!!",
          "speaker_id": "1",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100034429.1.23",
          "form": "똑같은 학폭이어도 여자연예인들만 다 아웃됐다는게 너무 어이가없죠",
          "original_form": "똑같은 학폭이어도 여자연예인들만 다 아웃됐다는게 너무 어이가없죠",
          "speaker_id": "2",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100034429.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ나참..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ나참..",
          "speaker_id": "2",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100034429.1.25",
          "form": "김유정 배우 논란(?) 생겼을 때 완전 어이없었어요",
          "original_form": "김유정 배우 논란(?) 생겼을 때 완전 어이없었어요",
          "speaker_id": "1",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100034429.1.26",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100034429.1.27",
          "form": "맞아요 ㅋㅋㅋㅋㅋ 아니 컨디션도 안좋았다는게 그게 뭐가 그렇게 문제였는지...",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ 아니 컨디션도 안좋았다는게 그게 뭐가 그렇게 문제였는지...",
          "speaker_id": "2",
          "time": "20210828 10:46"
        },
        {
          "id": "MDRW2100034429.1.28",
          "form": "전 설리님한테도 그 ~씨가지고 언급하는 것도 너무 어이없었어요",
          "original_form": "전 설리님한테도 그 ~씨가지고 언급하는 것도 너무 어이없었어요",
          "speaker_id": "2",
          "time": "20210828 10:47"
        },
        {
          "id": "MDRW2100034429.1.29",
          "form": "아ㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:47"
        },
        {
          "id": "MDRW2100034429.1.30",
          "form": "여자연예인하기 참 힘들어요",
          "original_form": "여자연예인하기 참 힘들어요",
          "speaker_id": "1",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100034429.1.31",
          "form": "맞아요 지금은 그래도 논란이 덜 나는 것 같긴하지만",
          "original_form": "맞아요 지금은 그래도 논란이 덜 나는 것 같긴하지만",
          "speaker_id": "2",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100034429.1.32",
          "form": "아무리생각해도 박나래님 경찰조사사건은..ㅋㅋ",
          "original_form": "아무리생각해도 박나래님 경찰조사사건은..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100034429.1.33",
          "form": "너무어이없었죠",
          "original_form": "너무어이없었죠",
          "speaker_id": "2",
          "time": "20210828 10:49"
        },
        {
          "id": "MDRW2100034429.1.34",
          "form": "경찰조사 받을 남연예인도 찾아보면 한둘이 아닐텐데",
          "original_form": "경찰조사 받을 남연예인도 찾아보면 한둘이 아닐텐데",
          "speaker_id": "1",
          "time": "20210828 10:49"
        },
        {
          "id": "MDRW2100034429.1.35",
          "form": "기사 봤을 때 충격이었어요",
          "original_form": "기사 봤을 때 충격이었어요",
          "speaker_id": "1",
