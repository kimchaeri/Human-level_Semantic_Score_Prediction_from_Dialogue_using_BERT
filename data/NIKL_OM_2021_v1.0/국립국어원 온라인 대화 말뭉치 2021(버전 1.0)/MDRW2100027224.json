      "utterance": [
        {
          "id": "MDRW2100027224.1.1",
          "form": "양궁해보고싶어요",
          "original_form": "양궁해보고싶어요",
          "speaker_id": "1",
          "time": "20210902 10:34"
        },
        {
          "id": "MDRW2100027224.1.2",
          "form": "팔에힘이 없어서 될까요",
          "original_form": "팔에힘이 없어서 될까요",
          "speaker_id": "1",
          "time": "20210902 10:34"
        },
        {
          "id": "MDRW2100027224.1.3",
          "form": "저는 펜싱요",
          "original_form": "저는 펜싱요",
          "speaker_id": "2",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100027224.1.4",
          "form": "양궁은 팔에 피날거에요",
          "original_form": "양궁은 팔에 피날거에요",
          "speaker_id": "2",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100027224.1.5",
          "form": "멋지네요",
          "original_form": "멋지네요",
          "speaker_id": "1",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100027224.1.6",
          "form": "수영도 필수지요",
          "original_form": "수영도 필수지요",
          "speaker_id": "2",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100027224.1.7",
          "form": "전검도도 해보고싶었어요",
          "original_form": "전검도도 해보고싶었어요",
          "speaker_id": "1",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100027224.1.8",
          "form": "어울리네요",
          "original_form": "어울리네요",
          "speaker_id": "2",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100027224.1.9",
          "form": "요즘같이 험한세상엔",
          "original_form": "요즘같이 험한세상엔",
          "speaker_id": "1",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100027224.1.10",
          "form": "역도는 어떠세요",
          "original_form": "역도는 어떠세요",
          "speaker_id": "2",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100027224.1.11",
          "form": "운동하는게 중요할것같앙ㅂ",
          "original_form": "운동하는게 중요할것같앙ㅂ",
          "speaker_id": "1",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100027224.1.12",
          "form": "역도보다는",
          "original_form": "역도보다는",
          "speaker_id": "1",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100027224.1.13",
          "form": "그뭐죠",
          "original_form": "그뭐죠",
          "speaker_id": "1",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100027224.1.14",
          "form": "투포환도 어울리실거에요",
          "original_form": "투포환도 어울리실거에요",
          "speaker_id": "2",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100027224.1.15",
          "form": "격투기 배우고싶네요",
          "original_form": "격투기 배우고싶네요",
          "speaker_id": "1",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100027224.1.16",
          "form": "온몸이무기!",
          "original_form": "온몸이무기!",
          "speaker_id": "1",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100027224.1.17",
          "form": "고양이니 장애물넘기도 잘하실거에요",
          "original_form": "고양이니 장애물넘기도 잘하실거에요",
          "speaker_id": "2",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100027224.1.18",
          "form": "뛰기를 못해요",
          "original_form": "뛰기를 못해요",
          "speaker_id": "1",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100027224.1.19",
          "form": "잘하는운동있나요",
          "original_form": "잘하는운동있나요",
          "speaker_id": "1",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100027224.1.20",
          "form": "격투기 스트레스 해소에 좋겠어요",
          "original_form": "격투기 스트레스 해소에 좋겠어요",
          "speaker_id": "2",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100027224.1.21",
          "form": "전 숨쉬기운동 장인입니다",
          "original_form": "전 숨쉬기운동 장인입니다",
          "speaker_id": "1",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100027224.1.22",
          "form": "저는 걷기를 잘합니다",
          "original_form": "저는 걷기를 잘합니다",
          "speaker_id": "2",
          "time": "20210902 10:36"
        }
      ]
