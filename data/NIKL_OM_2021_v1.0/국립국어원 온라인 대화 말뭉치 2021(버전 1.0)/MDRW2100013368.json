      "utterance": [
        {
          "id": "MDRW2100013368.1.1",
          "form": "현재 연애 중 이신가요?",
          "original_form": "현재 연애 중 이신가요?",
          "speaker_id": "1",
          "time": "20210825 18:09"
        },
        {
          "id": "MDRW2100013368.1.2",
          "form": "네 저는 여자친구가 있어요",
          "original_form": "네 저는 여자친구가 있어요",
          "speaker_id": "2",
          "time": "20210825 18:09"
        },
        {
          "id": "MDRW2100013368.1.3",
          "form": "연애하고 계신가요",
          "original_form": "연애하고 계신가요",
          "speaker_id": "2",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100013368.1.4",
          "form": "네 저도 남자친구가 있어요",
          "original_form": "네 저도 남자친구가 있어요",
          "speaker_id": "1",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100013368.1.5",
          "form": "좋은 연애를 하고있으신가요?",
          "original_form": "좋은 연애를 하고있으신가요?",
          "speaker_id": "2",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100013368.1.6",
          "form": "마음맞는 사람을 만나서 잘 만나고있어요",
          "original_form": "마음맞는 사람을 만나서 잘 만나고있어요",
          "speaker_id": "1",
          "time": "20210825 18:11"
        },
        {
          "id": "MDRW2100013368.1.7",
          "form": "다행이네요 저도 잘 만나고있어요",
          "original_form": "다행이네요 저도 잘 만나고있어요",
          "speaker_id": "2",
          "time": "20210825 18:12"
        },
        {
          "id": "MDRW2100013368.1.8",
          "form": "다행입니다  데이트는 주로 뭘 하시나요",
          "original_form": "다행입니다  데이트는 주로 뭘 하시나요",
          "speaker_id": "1",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100013368.1.9",
          "form": "저흰 주로 산책을해요",
          "original_form": "저흰 주로 산책을해요",
          "speaker_id": "2",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100013368.1.10",
          "form": "저희는 아이쇼핑하고 카페가는 걸 좋아해요",
          "original_form": "저희는 아이쇼핑하고 카페가는 걸 좋아해요",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100013368.1.11",
          "form": "맞아묘 저희도 카페좋아해요",
          "original_form": "맞아묘 저희도 카페좋아해요",
          "speaker_id": "2",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100013368.1.12",
          "form": "맛있는 거 먹는것도 좋아요",
          "original_form": "맛있는 거 먹는것도 좋아요",
          "speaker_id": "2",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100013368.1.13",
          "form": "사람들 데이트가 다 비슷하네요",
          "original_form": "사람들 데이트가 다 비슷하네요",
          "speaker_id": "1",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100013368.1.14",
          "form": "맞아요 그래도 그렇게 소소한 게 잘 맞아서좋아요",
          "original_form": "맞아요 그래도 그렇게 소소한 게 잘 맞아서좋아요",
          "speaker_id": "2",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100013368.1.15",
          "form": "저희 커플이랑 취향이 비슷하네요",
          "original_form": "저희 커플이랑 취향이 비슷하네요",
          "speaker_id": "1",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100013368.1.16",
          "form": "저희 커플도 소소한거 좋아해요",
          "original_form": "저희 커플도 소소한거 좋아해요",
          "speaker_id": "1",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100013368.1.17",
          "form": "데이트로 여행도 가고 싶은데 시국이...",
          "original_form": "데이트로 여행도 가고 싶은데 시국이...",
          "speaker_id": "2",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100013368.1.18",
          "form": "맞아요 여행도 좋죠",
          "original_form": "맞아요 여행도 좋죠",
          "speaker_id": "1",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100013368.1.19",
          "form": "결혼은 생각있으신가요?",
          "original_form": "결혼은 생각있으신가요?",
          "speaker_id": "2",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100013368.1.20",
          "form": "가능하면 빨리 하고싶어요",
          "original_form": "가능하면 빨리 하고싶어요",
          "speaker_id": "1",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100013368.1.21",
          "form": "결혼 생각있으세요?",
          "original_form": "결혼 생각있으세요?",
          "speaker_id": "1",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100013368.1.22",
          "form": "저도 가능한 빨리 결혼하고싶어요",
          "original_form": "저도 가능한 빨리 결혼하고싶어요",
          "speaker_id": "2",
