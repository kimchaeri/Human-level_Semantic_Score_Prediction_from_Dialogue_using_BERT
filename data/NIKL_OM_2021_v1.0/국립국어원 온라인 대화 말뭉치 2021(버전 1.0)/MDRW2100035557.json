      "utterance": [
        {
          "id": "MDRW2100035557.1.1",
          "form": "티비 보고 계시나요?",
          "original_form": "티비 보고 계시나요?",
          "speaker_id": "1",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100035557.1.2",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210829 22:37"
        },
        {
          "id": "MDRW2100035557.1.3",
          "form": "어떤거 보고 계세요?",
          "original_form": "어떤거 보고 계세요?",
          "speaker_id": "1",
          "time": "20210829 22:41"
        },
        {
          "id": "MDRW2100035557.1.4",
          "form": "옥탑방문제아들이요",
          "original_form": "옥탑방문제아들이요",
          "speaker_id": "2",
          "time": "20210829 22:41"
        },
        {
          "id": "MDRW2100035557.1.5",
          "form": "name1님은 보고계세요?",
          "original_form": "&name1&님은 보고계세요?",
          "speaker_id": "2",
          "time": "20210829 22:41"
        },
        {
          "id": "MDRW2100035557.1.6",
          "form": "전 가끔봐요!",
          "original_form": "전 가끔봐요!",
          "speaker_id": "1",
          "time": "20210829 22:44"
        },
        {
          "id": "MDRW2100035557.1.7",
          "form": "챙겨보진 못하고 있어요ㅜ",
          "original_form": "챙겨보진 못하고 있어요ㅜ",
          "speaker_id": "1",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100035557.1.8",
          "form": "저도요 ㅎㅎ",
          "original_form": "저도요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100035557.1.9",
          "form": "가끔 vod틀어서봐요",
          "original_form": "가끔 vod틀어서봐요",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100035557.1.10",
          "form": "볼때마다 지식이 늘어나는 기분이에요!",
          "original_form": "볼때마다 지식이 늘어나는 기분이에요!",
          "speaker_id": "1",
          "time": "20210829 23:20"
        },
        {
          "id": "MDRW2100035557.1.11",
          "form": "맞아요 그리구 재밌어요 ㅎㅎ",
          "original_form": "맞아요 그리구 재밌어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 23:41"
        },
        {
          "id": "MDRW2100035557.1.12",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:20"
        },
        {
          "id": "MDRW2100035557.1.13",
          "form": "맞추는 재미도 있구요!",
          "original_form": "맞추는 재미도 있구요!",
          "speaker_id": "1",
          "time": "20210830 00:20"
        },
        {
          "id": "MDRW2100035557.1.14",
          "form": "그쵸 ㅎㅎ",
          "original_form": "그쵸 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 00:23"
        },
        {
          "id": "MDRW2100035557.1.15",
          "form": "어떤 프로그램 즐겨보세요?",
          "original_form": "어떤 프로그램 즐겨보세요?",
          "speaker_id": "2",
          "time": "20210830 00:23"
        },
        {
          "id": "MDRW2100035557.1.16",
          "form": "대탈출이요!",
          "original_form": "대탈출이요!",
          "speaker_id": "1",
          "time": "20210830 00:31"
        },
        {
          "id": "MDRW2100035557.1.17",
          "form": "방금도 보고왔어요!",
          "original_form": "방금도 보고왔어요!",
          "speaker_id": "1",
          "time": "20210830 00:31"
        },
        {
          "id": "MDRW2100035557.1.18",
          "form": "그",
          "original_form": "그",
          "speaker_id": "2",
          "time": "20210830 00:33"
        },
        {
          "id": "MDRW2100035557.1.19",
          "form": "강호동나오는건가요?!",
          "original_form": "강호동나오는건가요?!",
          "speaker_id": "2",
          "time": "20210830 00:33"
        },
        {
          "id": "MDRW2100035557.1.20",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "1",
          "time": "20210830 00:37"
        },
        {
          "id": "MDRW2100035557.1.21",
          "form": "보다보니 재밌어서 챙겨보고 있어요ㅋㅋ",
          "original_form": "보다보니 재밌어서 챙겨보고 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:38"
        },
        {
          "id": "MDRW2100035557.1.22",
          "form": "저도 한번 봐볼게요 ㅎㅎ",
