      "utterance": [
        {
          "id": "MDRW2100020007.1.1",
          "form": "결혼한 지인들은 많죠?",
          "original_form": "결혼한 지인들은 많죠?",
          "speaker_id": "1",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100020007.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100020007.1.3",
          "form": "많으신가요..?",
          "original_form": "많으신가요..?",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100020007.1.4",
          "form": "ㅋㅋ원체 친구가 없어서..",
          "original_form": "ㅋㅋ원체 친구가 없어서..",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100020007.1.5",
          "form": "회사에 미혼이 많아요",
          "original_form": "회사에 미혼이 많아요",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100020007.1.6",
          "form": "회사에는 기혼이 더 많아요",
          "original_form": "회사에는 기혼이 더 많아요",
          "speaker_id": "2",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100020007.1.7",
          "form": "기혼상사가 미혼들 무시하는 경향이",
          "original_form": "기혼상사가 미혼들 무시하는 경향이",
          "speaker_id": "1",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020007.1.8",
          "form": "기혼상사도 일때문에 그런 이야기 할 틈이 없는데 회사에 그런 상사 있으면 힘들죠ㅠㅠ",
          "original_form": "기혼상사도 일때문에 그런 이야기 할 틈이 없는데 회사에 그런 상사 있으면 힘들죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100020007.1.9",
          "form": "편애가 심해요",
          "original_form": "편애가 심해요",
          "speaker_id": "1",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100020007.1.10",
          "form": "편애요?",
          "original_form": "편애요?",
          "speaker_id": "2",
          "time": "20210828 23:49"
        },
        {
          "id": "MDRW2100020007.1.11",
          "form": "애엄마들만 야근빼주고 챙겨줭ㆍ",
          "original_form": "애엄마들만 야근빼주고 챙겨줭ㆍ",
          "speaker_id": "1",
          "time": "20210828 23:49"
        },
        {
          "id": "MDRW2100020007.1.12",
          "form": "아.. 수당이 없나요?",
          "original_form": "아.. 수당이 없나요?",
          "speaker_id": "2",
          "time": "20210828 23:49"
        },
        {
          "id": "MDRW2100020007.1.13",
          "form": "결혼할 직원 부러워서 어쩌냐고",
          "original_form": "결혼할 직원 부러워서 어쩌냐고",
          "speaker_id": "1",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100020007.1.14",
          "form": "넙ㅜ",
          "original_form": "넙ㅜ",
          "speaker_id": "1",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100020007.1.15",
          "form": "저런..",
          "original_form": "저런..",
          "speaker_id": "2",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100020007.1.16",
          "form": "일이 많은 회사인거 같은데 대책이 필요해 보이네요",
          "original_form": "일이 많은 회사인거 같은데 대책이 필요해 보이네요",
          "speaker_id": "2",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100020007.1.17",
          "form": "결혼은 안부러운데 디폴트값을 결혼으로 두는 사람들때매 힘들어요",
          "original_form": "결혼은 안부러운데 디폴트값을 결혼으로 두는 사람들때매 힘들어요",
          "speaker_id": "1",
          "time": "20210828 23:51"
        }
      ]
