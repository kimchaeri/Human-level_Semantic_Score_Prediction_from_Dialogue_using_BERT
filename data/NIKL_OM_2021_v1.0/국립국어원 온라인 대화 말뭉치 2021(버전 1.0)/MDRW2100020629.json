      "utterance": [
        {
          "id": "MDRW2100020629.1.1",
          "form": "요즘 가을장마가 기승이죠~? 여기는 지금 비 안오네요. 님 지역은요?",
          "original_form": "요즘 가을장마가 기승이죠~? 여기는 지금 비 안오네요. 님 지역은요?",
          "speaker_id": "1",
          "time": "20210828 10:38"
        },
        {
          "id": "MDRW2100020629.1.2",
          "form": "여기도 햇빛났어요",
          "original_form": "여기도 햇빛났어요",
          "speaker_id": "2",
          "time": "20210828 11:37"
        },
        {
          "id": "MDRW2100020629.1.3",
          "form": "예보랑 다르네요~",
          "original_form": "예보랑 다르네요~",
          "speaker_id": "2",
          "time": "20210828 11:37"
        },
        {
          "id": "MDRW2100020629.1.4",
          "form": "그러게요!",
          "original_form": "그러게요!",
          "speaker_id": "1",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100020629.1.5",
          "form": "주중에 비오면 너무 싫더라고요..",
          "original_form": "주중에 비오면 너무 싫더라고요..",
          "speaker_id": "1",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100020629.1.6",
          "form": "돌아다닐 때 너무 습해서 아직은 땀나더라고요",
          "original_form": "돌아다닐 때 너무 습해서 아직은 땀나더라고요",
          "speaker_id": "1",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100020629.1.7",
          "form": "맞아요 습도가 너무 높았어요",
          "original_form": "맞아요 습도가 너무 높았어요",
          "speaker_id": "2",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100020629.1.8",
          "form": "여름 겨울 중에 언제가 낫나요",
          "original_form": "여름 겨울 중에 언제가 낫나요",
          "speaker_id": "2",
          "time": "20210828 13:38"
        },
        {
          "id": "MDRW2100020629.1.9",
          "form": "저는 겨울이요~!",
          "original_form": "저는 겨울이요~!",
          "speaker_id": "1",
          "time": "20210828 13:40"
        },
        {
          "id": "MDRW2100020629.1.10",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210828 13:41"
        },
        {
          "id": "MDRW2100020629.1.11",
          "form": "저도 겨울파였는데 점점 추운게 힘들어요ㅋㅋ",
          "original_form": "저도 겨울파였는데 점점 추운게 힘들어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100020629.1.12",
          "form": "나이때문인지",
          "original_form": "나이때문인지",
          "speaker_id": "2",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100020629.1.13",
          "form": "아 그쵸 롱패딩입어도 춥더라고요",
          "original_form": "아 그쵸 롱패딩입어도 춥더라고요",
          "speaker_id": "1",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100020629.1.14",
          "form": "예전에는 짧은 패딩으로 어떻게 버텼는지..신기해요",
          "original_form": "예전에는 짧은 패딩으로 어떻게 버텼는지..신기해요",
          "speaker_id": "1",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100020629.1.15",
          "form": "맞아요ㅋㅋ롱패딩 산뒤로 그것만 입어요",
          "original_form": "맞아요ㅋㅋ롱패딩 산뒤로 그것만 입어요",
          "speaker_id": "2",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020629.1.16",
          "form": "멋부릴때나 코트입고",
          "original_form": "멋부릴때나 코트입고",
          "speaker_id": "2",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020629.1.17",
          "form": "얼죽코 아시죠 ㅋㅋㅋ 맞아요 저도 롱패딩만 주구장창 입어요",
          "original_form": "얼죽코 아시죠 ㅋㅋㅋ 맞아요 저도 롱패딩만 주구장창 입어요",
          "speaker_id": "1",
          "time": "20210828 14:01"
        },
        {
          "id": "MDRW2100020629.1.18",
          "form": "요새 옷 어떻게 입으시나요?",
          "original_form": "요새 옷 어떻게 입으시나요?",
          "speaker_id": "1",
          "time": "20210828 14:01"
        },
        {
          "id": "MDRW2100020629.1.19",
          "form": "ㅋㅋㅋ얼죽코하다 진짜 얼죽될거같아요",
          "original_form": "ㅋㅋㅋ얼죽코하다 진짜 얼죽될거같아요",
          "speaker_id": "2",
          "time": "20210828 14:02"
        },
        {
          "id": "MDRW2100020629.1.20",
          "form": "요즘은 아직 반팔입어요",
          "original_form": "요즘은 아직 반팔입어요",
          "speaker_id": "2",
          "time": "20210828 14:02"
        },
        {
          "id": "MDRW2100020629.1.21",
          "form": "좀 선선하다싶으면 얇은가디건걸치고",
          "original_form": "좀 선선하다싶으면 얇은가디건걸치고",
          "speaker_id": "2",
          "time": "20210828 14:02"
        },
        {
          "id": "MDRW2100020629.1.22",
          "form": "오 맞아요 저도 아직은 반팔입어요",
          "original_form": "오 맞아요 저도 아직은 반팔입어요",
          "speaker_id": "1",
          "time": "20210828 14:06"
        },
        {
          "id": "MDRW2100020629.1.23",
          "form": "밤되면 추워서 문도 다 닫고 자게 되더라고요",
          "original_form": "밤되면 추워서 문도 다 닫고 자게 되더라고요",
          "speaker_id": "1",
          "time": "20210828 14:06"
        },
        {
          "id": "MDRW2100020629.1.24",
          "form": "환절기라고 일교차 있네요ㅋㅋ",
          "original_form": "환절기라고 일교차 있네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100020629.1.25",
          "form": "그쵸 진짜 조심해야돼요 괜히 코로나 의심받는단 말이에요",
          "original_form": "그쵸 진짜 조심해야돼요 괜히 코로나 의심받는단 말이에요",
          "speaker_id": "1",
          "time": "20210828 14:07"
        }
      ]
    }
