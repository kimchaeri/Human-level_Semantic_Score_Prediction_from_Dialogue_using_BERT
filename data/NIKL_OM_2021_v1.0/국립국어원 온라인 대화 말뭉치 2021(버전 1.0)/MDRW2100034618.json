      "utterance": [
        {
          "id": "MDRW2100034618.1.1",
          "form": "코로나전에 뭐가 제일 그리우세요?",
          "original_form": "코로나전에 뭐가 제일 그리우세요?",
          "speaker_id": "1",
          "time": "20210828 16:26"
        },
        {
          "id": "MDRW2100034618.1.2",
          "form": "여행도 그렇구요ㅠ",
          "original_form": "여행도 그렇구요ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:27"
        },
        {
          "id": "MDRW2100034618.1.3",
          "form": "아 ㅜㅜ",
          "original_form": "아 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 16:27"
        },
        {
          "id": "MDRW2100034618.1.4",
          "form": "그냥 숨을 시원하게 쉬기만해도 좋을거같아요ㅠ",
          "original_form": "그냥 숨을 시원하게 쉬기만해도 좋을거같아요ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:28"
        },
        {
          "id": "MDRW2100034618.1.5",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 16:28"
        },
        {
          "id": "MDRW2100034618.1.6",
          "form": "마스크 너무 갑갑해요",
          "original_form": "마스크 너무 갑갑해요",
          "speaker_id": "1",
          "time": "20210828 16:28"
        },
        {
          "id": "MDRW2100034618.1.7",
          "form": "name1님은 어떤게 그리우세요?",
          "original_form": "&name1&님은 어떤게 그리우세요?",
          "speaker_id": "2",
          "time": "20210828 16:37"
        },
        {
          "id": "MDRW2100034618.1.8",
          "form": "음 저는 영화관에서 팝콘 먹고 싶어요 ㅌㅋㅋ",
          "original_form": "음 저는 영화관에서 팝콘 먹고 싶어요 ㅌㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100034618.1.9",
          "form": "아ㅋㅋ\r\n진짜 이렇게 소소한거 못한다는게 너무 슬프네요ㅠ",
          "original_form": "아ㅋㅋ\r\n진짜 이렇게 소소한거 못한다는게 너무 슬프네요ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:40"
        },
        {
          "id": "MDRW2100034618.1.10",
          "form": "저는 배드민턴이나 피아노 드럼 이런거 배워보고싶은데",
          "original_form": "저는 배드민턴이나 피아노 드럼 이런거 배워보고싶은데",
          "speaker_id": "2",
          "time": "20210828 16:40"
        },
        {
          "id": "MDRW2100034618.1.11",
          "form": "괜히 여러군데 돌아다니다가 코로나 걸리거나 걸리게할까봐",
          "original_form": "괜히 여러군데 돌아다니다가 코로나 걸리거나 걸리게할까봐",
          "speaker_id": "2",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100034618.1.12",
          "form": "그것도 걱정돼서 쉽게 배우러 못가겠더라구요",
          "original_form": "그것도 걱정돼서 쉽게 배우러 못가겠더라구요",
          "speaker_id": "2",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100034618.1.13",
          "form": "맞아요 뭔가",
          "original_form": "맞아요 뭔가",
          "speaker_id": "1",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100034618.1.14",
          "form": "제가 슈퍼전파자가 될거같고 막 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "제가 슈퍼전파자가 될거같고 막 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100034618.1.15",
          "form": "아 맞아요ㅋㅋㄱ\r\n제가 걸리는건 그나마 괜찮은데",
          "original_form": "아 맞아요ㅋㅋㄱ\r\n제가 걸리는건 그나마 괜찮은데",
          "speaker_id": "2",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100034618.1.16",
          "form": "제가 옮기는건 진짜 싫어요ㅠ",
          "original_form": "제가 옮기는건 진짜 싫어요ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:45"
        },
        {
          "id": "MDRW2100034618.1.17",
          "form": "맞아여 ㅜㅜ",
          "original_form": "맞아여 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 16:45"
        },
        {
          "id": "MDRW2100034618.1.18",
          "form": "민폐될까봐 ㅋㅋㅋ 창피하기도 하고욬ㅋㅋㅋ",
          "original_form": "민폐될까봐 ㅋㅋㅋ 창피하기도 하고욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:45"
        },
        {
          "id": "MDRW2100034618.1.19",
          "form": "마자요 민폐되는느낌ㅜ",
          "original_form": "마자요 민폐되는느낌ㅜ",
          "speaker_id": "2",
          "time": "20210828 16:45"
        },
        {
          "id": "MDRW2100034618.1.20",
          "form": "미안하고 창피하고 다하죠ㅠ",
          "original_form": "미안하고 창피하고 다하죠ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:45"
        },
        {
          "id": "MDRW2100034618.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 16:45"
        },
        {
          "id": "MDRW2100034618.1.22",
          "form": "예전엔 경로도 뿌렸잖아요 ㅋㅋㅋㅋㅋ",
          "original_form": "예전엔 경로도 뿌렸잖아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:46"
        },
        {
          "id": "MDRW2100034618.1.23",
          "form": "그쵸 초반에는 경로도 알려줘서ㅠ",
          "original_form": "그쵸 초반에는 경로도 알려줘서ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100034618.1.24",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100034618.1.25",
          "form": "그래도 주변에는 막 소문나고 할거같아요ㅠ",
          "original_form": "그래도 주변에는 막 소문나고 할거같아요ㅠ",
