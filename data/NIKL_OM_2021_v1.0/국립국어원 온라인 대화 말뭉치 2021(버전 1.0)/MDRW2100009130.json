      "utterance": [
        {
          "id": "MDRW2100009130.1.1",
          "form": "굿밤되세요",
          "original_form": "굿밤되세요",
          "speaker_id": "1",
          "time": "20210822 21:34"
        },
        {
          "id": "MDRW2100009130.1.2",
          "form": "곰표맥주는 드셔보셨나요?",
          "original_form": "곰표맥주는 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210822 23:34"
        },
        {
          "id": "MDRW2100009130.1.3",
          "form": "네 팝콘도요",
          "original_form": "네 팝콘도요",
          "speaker_id": "1",
          "time": "20210822 23:35"
        },
        {
          "id": "MDRW2100009130.1.4",
          "form": "오 팝콘도 있어요?",
          "original_form": "오 팝콘도 있어요?",
          "speaker_id": "2",
          "time": "20210822 23:36"
        },
        {
          "id": "MDRW2100009130.1.5",
          "form": "네 진짜 크고 맛나요 ㅋㅋ",
          "original_form": "네 진짜 크고 맛나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:36"
        },
        {
          "id": "MDRW2100009130.1.6",
          "form": "저는 맥주만먹어봤어요",
          "original_form": "저는 맥주만먹어봤어요",
          "speaker_id": "2",
          "time": "20210822 23:36"
        },
        {
          "id": "MDRW2100009130.1.7",
          "form": "오래 먹을수 있어요",
          "original_form": "오래 먹을수 있어요",
          "speaker_id": "1",
          "time": "20210822 23:36"
        },
        {
          "id": "MDRW2100009130.1.8",
          "form": "씨유에서 파나요?",
          "original_form": "씨유에서 파나요?",
          "speaker_id": "2",
          "time": "20210822 23:36"
        },
        {
          "id": "MDRW2100009130.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009130.1.10",
          "form": "곰표맥주 넘 스위트해서 좋더라구요",
          "original_form": "곰표맥주 넘 스위트해서 좋더라구요",
          "speaker_id": "2",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009130.1.11",
          "form": "깔끔하게 스위트한맛",
          "original_form": "깔끔하게 스위트한맛",
          "speaker_id": "2",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009130.1.12",
          "form": "맞아요 곰이 스윗한 걸까요 ㅋㅋ",
          "original_form": "맞아요 곰이 스윗한 걸까요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009130.1.13",
          "form": "2병만사봤는데 아쉽더라구요",
          "original_form": "2병만사봤는데 아쉽더라구요",
          "speaker_id": "2",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009130.1.14",
          "form": "2캔",
          "original_form": "2캔",
          "speaker_id": "2",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009130.1.15",
          "form": "아 그렇죠",
          "original_form": "아 그렇죠",
          "speaker_id": "1",
          "time": "20210822 23:38"
        },
        {
          "id": "MDRW2100009130.1.16",
          "form": "말표는 별로였어요 ㅎ",
          "original_form": "말표는 별로였어요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 23:38"
        },
        {
          "id": "MDRW2100009130.1.17",
          "form": "적세사면 항시 아쉬워요",
          "original_form": "적세사면 항시 아쉬워요",
          "speaker_id": "1",
          "time": "20210822 23:38"
        },
        {
          "id": "MDRW2100009130.1.18",
          "form": "전 곰만 먹어 봤어요",
          "original_form": "전 곰만 먹어 봤어요",
          "speaker_id": "1",
          "time": "20210822 23:38"
        },
        {
          "id": "MDRW2100009130.1.19",
          "form": "말은 못먹었네요",
          "original_form": "말은 못먹었네요",
          "speaker_id": "1",
          "time": "20210822 23:38"
        },
        {
          "id": "MDRW2100009130.1.20",
          "form": "술을 좀 드시나요?ㅎ",
          "original_form": "술을 좀 드시나요?ㅎ",
          "speaker_id": "2",
          "time": "20210822 23:38"
        },
        {
          "id": "MDRW2100009130.1.21",
          "form": "맛없다니 패쓰요",
          "original_form": "맛없다니 패쓰요",
          "speaker_id": "1",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009130.1.22",
          "form": "말은 흑맥인데 기네스생각하고 마셨더니",
          "original_form": "말은 흑맥인데 기네스생각하고 마셨더니",
          "speaker_id": "2",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009130.1.23",
          "form": "요샌 조금요",
          "original_form": "요샌 조금요",
          "speaker_id": "1",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009130.1.24",
          "form": "좀 닝닝해요",
          "original_form": "좀 닝닝해요",
          "speaker_id": "2",
          "time": "20210822 23:39"
        }
      ]
