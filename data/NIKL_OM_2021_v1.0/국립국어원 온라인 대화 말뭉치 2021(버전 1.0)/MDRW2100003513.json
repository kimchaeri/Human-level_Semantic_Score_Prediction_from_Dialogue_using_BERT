      "utterance": [
        {
          "id": "MDRW2100003513.1.1",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210821 00:08"
        },
        {
          "id": "MDRW2100003513.1.2",
          "form": "저는 케이크도 좋아하는데 쿠키종류를 좋아해요",
          "original_form": "저는 케이크도 좋아하는데 쿠키종류를 좋아해요",
          "speaker_id": "2",
          "time": "20210821 00:10"
        },
        {
          "id": "MDRW2100003513.1.3",
          "form": "쿠키요? 어떤쿠키 좋아하시나요",
          "original_form": "쿠키요? 어떤쿠키 좋아하시나요",
          "speaker_id": "1",
          "time": "20210821 00:11"
        },
        {
          "id": "MDRW2100003513.1.4",
          "form": "초코칩쿠키 좋아해요~",
          "original_form": "초코칩쿠키 좋아해요~",
          "speaker_id": "2",
          "time": "20210821 00:15"
        },
        {
          "id": "MDRW2100003513.1.5",
          "form": "커피랑 같이먹으면 궁합이 최고에요",
          "original_form": "커피랑 같이먹으면 궁합이 최고에요",
          "speaker_id": "2",
          "time": "20210821 00:15"
        },
        {
          "id": "MDRW2100003513.1.6",
          "form": "케이크는 어떤거 주로 드세요?",
          "original_form": "케이크는 어떤거 주로 드세요?",
          "speaker_id": "2",
          "time": "20210821 00:15"
        },
        {
          "id": "MDRW2100003513.1.7",
          "form": "초코칩 박힌 거 너무 좋죠 초코칩 씹는 재미도 있구요",
          "original_form": "초코칩 박힌 거 너무 좋죠 초코칩 씹는 재미도 있구요",
          "speaker_id": "1",
          "time": "20210821 00:15"
        },
        {
          "id": "MDRW2100003513.1.8",
          "form": "케이크는  투썸플레이스 아이스박스 좋아해요",
          "original_form": "케이크는  투썸플레이스 아이스박스 좋아해요",
          "speaker_id": "1",
          "time": "20210821 00:16"
        },
        {
          "id": "MDRW2100003513.1.9",
          "form": "되게 맛있더라구요",
          "original_form": "되게 맛있더라구요",
          "speaker_id": "1",
          "time": "20210821 00:16"
        },
        {
          "id": "MDRW2100003513.1.10",
          "form": "어떤맛이 가장 맛있었어요?",
          "original_form": "어떤맛이 가장 맛있었어요?",
          "speaker_id": "2",
          "time": "20210821 00:22"
        },
        {
          "id": "MDRW2100003513.1.11",
          "form": "아이스박스라고",
          "original_form": "아이스박스라고",
          "speaker_id": "1",
          "time": "20210821 00:23"
        },
        {
          "id": "MDRW2100003513.1.12",
          "form": "약간 오레오맛 케이크거든요",
          "original_form": "약간 오레오맛 케이크거든요",
          "speaker_id": "1",
          "time": "20210821 00:23"
        },
        {
          "id": "MDRW2100003513.1.13",
          "form": "쿠키가 씹히는 케이크라",
          "original_form": "쿠키가 씹히는 케이크라",
          "speaker_id": "1",
          "time": "20210821 00:24"
        },
        {
          "id": "MDRW2100003513.1.14",
          "form": "아주 맛있어요",
          "original_form": "아주 맛있어요",
          "speaker_id": "1",
          "time": "20210821 00:24"
        },
        {
          "id": "MDRW2100003513.1.15",
          "form": "아...! 제가 다른 디저트랑 착각했네요ㅎㅎ\r\n오레오맛은 맛이 없을수가 없어요",
          "original_form": "아...! 제가 다른 디저트랑 착각했네요ㅎㅎ\r\n오레오맛은 맛이 없을수가 없어요",
          "speaker_id": "2",
          "time": "20210821 00:27"
        },
        {
          "id": "MDRW2100003513.1.16",
          "form": "맞아요 오레오는 아마 누구나 다 좋아 할 거에요",
          "original_form": "맞아요 오레오는 아마 누구나 다 좋아 할 거에요",
          "speaker_id": "1",
          "time": "20210821 00:27"
        },
        {
          "id": "MDRW2100003513.1.17",
          "form": "음료는 어떤거 주로 마시나요?",
          "original_form": "음료는 어떤거 주로 마시나요?",
          "speaker_id": "1",
          "time": "20210821 00:28"
        },
        {
          "id": "MDRW2100003513.1.18",
          "form": "저는 주로 아이스아메리카노나 아이스라떼를 많이 마셔요",
          "original_form": "저는 주로 아이스아메리카노나 아이스라떼를 많이 마셔요",
          "speaker_id": "2",
          "time": "20210821 00:29"
        },
        {
          "id": "MDRW2100003513.1.19",
          "form": "디저트에 잘 어울리는 음료에요",
          "original_form": "디저트에 잘 어울리는 음료에요",
          "speaker_id": "1",
          "time": "20210821 00:30"
        },
        {
          "id": "MDRW2100003513.1.20",
          "form": "어디 커피가 가장 괜찮던가요?",
          "original_form": "어디 커피가 가장 괜찮던가요?",
          "speaker_id": "1",
          "time": "20210821 00:30"
        },
        {
          "id": "MDRW2100003513.1.21",
          "form": "커피는 그래도 스타벅스가 가장 나은거 같아요",
          "original_form": "커피는 그래도 스타벅스가 가장 나은거 같아요",
          "speaker_id": "2",
          "time": "20210821 12:34"
        },
        {
          "id": "MDRW2100003513.1.22",
          "form": "다른음료도 다양하게 팔잖아요",
          "original_form": "다른음료도 다양하게 팔잖아요",
          "speaker_id": "2",
          "time": "20210821 12:34"
        },
        {
          "id": "MDRW2100003513.1.23",
          "form": "아무래도 제일 무난한곳인거같아요",
          "original_form": "아무래도 제일 무난한곳인거같아요",
          "speaker_id": "1",
          "time": "20210821 12:34"
        },
        {
          "id": "MDRW2100003513.1.24",
          "form": "가볍게 가기도 좋고",
          "original_form": "가볍게 가기도 좋고",
          "speaker_id": "1",
          "time": "20210821 12:35"
        }
      ]
    }
