      "utterance": [
        {
          "id": "MDRW2100027365.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 11:55"
        },
        {
          "id": "MDRW2100027365.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210902 11:56"
        },
        {
          "id": "MDRW2100027365.1.3",
          "form": "재미있게 읽으신 책 있나요",
          "original_form": "재미있게 읽으신 책 있나요",
          "speaker_id": "1",
          "time": "20210902 11:57"
        },
        {
          "id": "MDRW2100027365.1.4",
          "form": "저 최근에 만화책 읽었는데",
          "original_form": "저 최근에 만화책 읽었는데",
          "speaker_id": "2",
          "time": "20210902 11:58"
        },
        {
          "id": "MDRW2100027365.1.5",
          "form": "위국일기라고",
          "original_form": "위국일기라고",
          "speaker_id": "2",
          "time": "20210902 11:58"
        },
        {
          "id": "MDRW2100027365.1.6",
          "form": "비혼여성이 고아가 된 조카를 키우게 된 얘기였어요",
          "original_form": "비혼여성이 고아가 된 조카를 키우게 된 얘기였어요",
          "speaker_id": "2",
          "time": "20210902 11:58"
        },
        {
          "id": "MDRW2100027365.1.7",
          "form": "처음 들어봤어요",
          "original_form": "처음 들어봤어요",
          "speaker_id": "1",
          "time": "20210902 11:58"
        },
        {
          "id": "MDRW2100027365.1.8",
          "form": "내용이 밝지 않을거 같은데...",
          "original_form": "내용이 밝지 않을거 같은데...",
          "speaker_id": "1",
          "time": "20210902 11:58"
        },
        {
          "id": "MDRW2100027365.1.9",
          "form": "네 좀 어두운데 감정을 엄청 섬세하게 다뤘더라구요",
          "original_form": "네 좀 어두운데 감정을 엄청 섬세하게 다뤘더라구요",
          "speaker_id": "2",
          "time": "20210902 12:00"
        },
        {
          "id": "MDRW2100027365.1.10",
          "form": "만화책인데 소설책이나 수필읽는 느낌",
          "original_form": "만화책인데 소설책이나 수필읽는 느낌",
          "speaker_id": "2",
          "time": "20210902 12:00"
        },
        {
          "id": "MDRW2100027365.1.11",
          "form": "궁금하네요",
          "original_form": "궁금하네요",
          "speaker_id": "1",
          "time": "20210902 12:01"
        },
        {
          "id": "MDRW2100027365.1.12",
          "form": "한번 읽어봐야겠어요",
          "original_form": "한번 읽어봐야겠어요",
          "speaker_id": "1",
          "time": "20210902 12:01"
        },
        {
          "id": "MDRW2100027365.1.13",
          "form": "나름 반응이 좋은 책이더라구요ㅎㅎ..",
          "original_form": "나름 반응이 좋은 책이더라구요ㅎㅎ..",
          "speaker_id": "2",
          "time": "20210902 12:05"
        },
        {
          "id": "MDRW2100027365.1.14",
          "form": "책 읽는게 지쳐서 만화책 자주 읽고있어요...ㅎㅎ",
          "original_form": "책 읽는게 지쳐서 만화책 자주 읽고있어요...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 12:05"
        },
        {
          "id": "MDRW2100027365.1.15",
          "form": "요즘엔 만화책 내용도 다양하고 심도 깊어서",
          "original_form": "요즘엔 만화책 내용도 다양하고 심도 깊어서",
          "speaker_id": "1",
          "time": "20210902 12:06"
        },
        {
          "id": "MDRW2100027365.1.16",
          "form": "상관 없는거 같아요",
          "original_form": "상관 없는거 같아요",
          "speaker_id": "1",
          "time": "20210902 12:06"
        },
        {
          "id": "MDRW2100027365.1.17",
          "form": "어떨때는 소설보다 좋기도 하구요",
          "original_form": "어떨때는 소설보다 좋기도 하구요",
          "speaker_id": "1",
          "time": "20210902 12:06"
        },
        {
          "id": "MDRW2100027365.1.18",
          "form": "네 유치하지 않은 작품들이 꽤 나오더라구요",
          "original_form": "네 유치하지 않은 작품들이 꽤 나오더라구요",
          "speaker_id": "2",
          "time": "20210902 12:07"
        },
        {
          "id": "MDRW2100027365.1.19",
          "form": "예전에 죽음에 관하여였나? 울면서 봤었어요",
          "original_form": "예전에 죽음에 관하여였나? 울면서 봤었어요",
          "speaker_id": "1",
          "time": "20210902 12:09"
        },
        {
          "id": "MDRW2100027365.1.20",
          "form": "아 맞아요 그거 진짜 엄청 감동이 크게 왔어요",
          "original_form": "아 맞아요 그거 진짜 엄청 감동이 크게 왔어요",
          "speaker_id": "2",
          "time": "20210902 12:10"
        },
        {
          "id": "MDRW2100027365.1.21",
          "form": "작가가 내용을 참 잘 썼죠",
          "original_form": "작가가 내용을 참 잘 썼죠",
          "speaker_id": "1",
          "time": "20210902 12:15"
        },
        {
          "id": "MDRW2100027365.1.22",
          "form": "천재같아요 진짜",
          "original_form": "천재같아요 진짜",
          "speaker_id": "2",
          "time": "20210902 12:17"
        }
      ]
    }
