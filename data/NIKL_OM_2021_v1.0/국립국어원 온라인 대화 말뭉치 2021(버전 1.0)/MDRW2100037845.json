      "utterance": [
        {
          "id": "MDRW2100037845.1.1",
          "form": "탈모를 걱정하고 계신가요 혹시?!",
          "original_form": "탈모를 걱정하고 계신가요 혹시?!",
          "speaker_id": "2",
          "time": "20210901 19:31"
        },
        {
          "id": "MDRW2100037845.1.2",
          "form": "네ㅜㅜ",
          "original_form": "네ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100037845.1.3",
          "form": "콩이란콩 다찾아보고있어요",
          "original_form": "콩이란콩 다찾아보고있어요",
          "speaker_id": "1",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100037845.1.4",
          "form": "헉 그러시군요",
          "original_form": "헉 그러시군요",
          "speaker_id": "2",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100037845.1.5",
          "form": "탈색이나 염색 자주 하셨나요?",
          "original_form": "탈색이나 염색 자주 하셨나요?",
          "speaker_id": "2",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100037845.1.6",
          "form": "한번도 한적없어요ㅜ",
          "original_form": "한번도 한적없어요ㅜ",
          "speaker_id": "1",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100037845.1.7",
          "form": "대신 지병이..",
          "original_form": "대신 지병이..",
          "speaker_id": "1",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100037845.1.8",
          "form": "아이고 어떤 지병이 있으신가요ㅠ",
          "original_form": "아이고 어떤 지병이 있으신가요ㅠ",
          "speaker_id": "2",
          "time": "20210901 19:33"
        },
        {
          "id": "MDRW2100037845.1.9",
          "form": "피부염이 어릴때 심했어가지고요",
          "original_form": "피부염이 어릴때 심했어가지고요",
          "speaker_id": "1",
          "time": "20210901 19:33"
        },
        {
          "id": "MDRW2100037845.1.10",
          "form": "지루성두피염이랑 얼굴에도 올라왔었는데",
          "original_form": "지루성두피염이랑 얼굴에도 올라왔었는데",
          "speaker_id": "1",
          "time": "20210901 19:33"
        },
        {
          "id": "MDRW2100037845.1.11",
          "form": "헉 네네!!!!",
          "original_form": "헉 네네!!!!",
          "speaker_id": "2",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100037845.1.12",
          "form": "와 지루성두피염",
          "original_form": "와 지루성두피염",
          "speaker_id": "2",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100037845.1.13",
          "form": "네ㅜㅜ",
          "original_form": "네ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100037845.1.14",
          "form": "저도 있어요....",
          "original_form": "저도 있어요....",
          "speaker_id": "2",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100037845.1.15",
          "form": "앗ㅜㅜ",
          "original_form": "앗ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100037845.1.16",
          "form": "어렸을 때 아토피가 되게 심했거든요....",
          "original_form": "어렸을 때 아토피가 되게 심했거든요....",
          "speaker_id": "2",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100037845.1.17",
          "form": "두피는 진짜",
          "original_form": "두피는 진짜",
          "speaker_id": "2",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100037845.1.18",
          "form": "그래서 저 어릴때 심했던 후유증으로 탈모가와서",
          "original_form": "그래서 저 어릴때 심했던 후유증으로 탈모가와서",
          "speaker_id": "1",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100037845.1.19",
          "form": "아직도 너무 휑해요",
          "original_form": "아직도 너무 휑해요",
          "speaker_id": "1",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100037845.1.20",
          "form": "사실 처음에는 그 딱지 떼는게 재밌어가지고ㅠ",
          "original_form": "사실 처음에는 그 딱지 떼는게 재밌어가지고ㅠ",
          "speaker_id": "2",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100037845.1.21",
          "form": "으아",
          "original_form": "으아",
          "speaker_id": "2",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100037845.1.22",
          "form": "그래도 유전적인게 아니니까",
          "original_form": "그래도 유전적인게 아니니까",
          "speaker_id": "2",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100037845.1.23",
          "form": "그쳐ㅜ저 딱지가 점되가지고",
          "original_form": "그쳐ㅜ저 딱지가 점되가지고",
          "speaker_id": "1",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100037845.1.24",
          "form": "치료하면 다시 수북해지지 않을까요?",
          "original_form": "치료하면 다시 수북해지지 않을까요?",
          "speaker_id": "2",
          "time": "20210901 19:40"
        },
        {
          "id": "MDRW2100037845.1.25",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210901 19:40"
        },
        {
          "id": "MDRW2100037845.1.26",
          "form": "두피가 얼룩덜룩합니다ㅜ",
          "original_form": "두피가 얼룩덜룩합니다ㅜ",
          "speaker_id": "1",
          "time": "20210901 19:40"
        },
        {
          "id": "MDRW2100037845.1.27",
