      "utterance": [
        {
          "id": "MDRW2100013564.1.1",
          "form": "지금은 짧은 머리",
          "original_form": "지금은 짧은 머리",
          "speaker_id": "2",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013564.1.2",
          "form": "짧은길이 어느정도???",
          "original_form": "짧은길이 어느정도???",
          "speaker_id": "1",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013564.1.3",
          "form": "어깨 살짝 아래",
          "original_form": "어깨 살짝 아래",
          "speaker_id": "2",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013564.1.4",
          "form": "어 미디길이가 잘어울린다고 하드라",
          "original_form": "어 미디길이가 잘어울린다고 하드라",
          "speaker_id": "1",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013564.1.5",
          "form": "그길이가 어울리기힘든길이라데",
          "original_form": "그길이가 어울리기힘든길이라데",
          "speaker_id": "1",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100013564.1.6",
          "form": "거지존이잖아ㅜ",
          "original_form": "거지존이잖아ㅜ",
          "speaker_id": "2",
          "time": "20210825 15:56"
        },
        {
          "id": "MDRW2100013564.1.7",
          "form": "어 근데 난 어울리노 ㅋㅋ",
          "original_form": "어 근데 난 어울리노 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100013564.1.8",
          "form": "거지인가 ㅋ",
          "original_form": "거지인가 ㅋ",
          "speaker_id": "1",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100013564.1.9",
          "form": "머리관리를 해야지 안함 거지존이라고",
          "original_form": "머리관리를 해야지 안함 거지존이라고",
          "speaker_id": "2",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013564.1.10",
          "form": "ㅇㅇ난 생머리가 어울린다",
          "original_form": "ㅇㅇ난 생머리가 어울린다",
          "speaker_id": "1",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100013564.1.11",
          "form": "미디에 드라이하면 될듯",
          "original_form": "미디에 드라이하면 될듯",
          "speaker_id": "1",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100013564.1.12",
          "form": "그치 드라이 꼭 해야지",
          "original_form": "그치 드라이 꼭 해야지",
          "speaker_id": "2",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100013564.1.13",
          "form": "ㅇㅇ저번에 누가놀라드라",
          "original_form": "ㅇㅇ저번에 누가놀라드라",
          "speaker_id": "1",
          "time": "20210825 16:56"
        },
        {
          "id": "MDRW2100013564.1.14",
          "form": "미디길이머리가 이래잘어울리냐고",
          "original_form": "미디길이머리가 이래잘어울리냐고",
          "speaker_id": "1",
          "time": "20210825 16:56"
        },
        {
          "id": "MDRW2100013564.1.15",
          "form": "맞나~ㅋㅋㅋ자랑하나 근데??",
          "original_form": "맞나~ㅋㅋㅋ자랑하나 근데??",
          "speaker_id": "2",
          "time": "20210825 16:56"
        },
        {
          "id": "MDRW2100013564.1.16",
          "form": "엉 ㅎ 근데 딱 미디만 어울리는데므ㅏ",
          "original_form": "엉 ㅎ 근데 딱 미디만 어울리는데므ㅏ",
          "speaker_id": "1",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100013564.1.17",
          "form": "어울리는데뭘",
          "original_form": "어울리는데뭘",
          "speaker_id": "1",
          "time": "20210825 17:04"
        },
        {
          "id": "MDRW2100013564.1.18",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100013564.1.19",
          "form": "알았어 인정",
