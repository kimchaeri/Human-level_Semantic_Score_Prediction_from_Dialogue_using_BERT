      "utterance": [
        {
          "id": "MDRW2100012544.1.1",
          "form": "정모 어땠는데요 ㅎㅎ",
          "original_form": "정모 어땠는데요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 22:57"
        },
        {
          "id": "MDRW2100012544.1.2",
          "form": "거상 정모 했었는데ㅎㅎ 너무 이상했었어요ㅎㅎ",
          "original_form": "거상 정모 했었는데ㅎㅎ 너무 이상했었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:57"
        },
        {
          "id": "MDRW2100012544.1.3",
          "form": "헉 왜용",
          "original_form": "헉 왜용",
          "speaker_id": "1",
          "time": "20210824 22:57"
        },
        {
          "id": "MDRW2100012544.1.4",
          "form": "정상적인 사람이 별로없어서ㅎㅎ",
          "original_form": "정상적인 사람이 별로없어서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:57"
        },
        {
          "id": "MDRW2100012544.1.5",
          "form": "혼잣말하는사람ㅎ",
          "original_form": "혼잣말하는사람ㅎ",
          "speaker_id": "2",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012544.1.6",
          "form": "뭐 버럭화내는사람ㅎ",
          "original_form": "뭐 버럭화내는사람ㅎ",
          "speaker_id": "2",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012544.1.7",
          "form": "싸우는사람ㅎㅎ",
          "original_form": "싸우는사람ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012544.1.8",
          "form": "무슨게임 공성전하듯이ㅎㅎ",
          "original_form": "무슨게임 공성전하듯이ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012544.1.9",
          "form": "어멋 적응 안되겠아요",
          "original_form": "어멋 적응 안되겠아요",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012544.1.10",
          "form": "전 20대 초반 정모가  좀 엄청난???",
          "original_form": "전 20대 초반 정모가  좀 엄청난???",
          "speaker_id": "1",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100012544.1.11",
          "form": "학생들 제외하고 한분빼고 다 외제차 ㅎㅎㅎ",
          "original_form": "학생들 제외하고 한분빼고 다 외제차 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100012544.1.12",
          "form": "제일 큰오빠(아빠뻘 ㅋ) 님이  계산 다했는데\n음 제기억으로 돈백 넘게 나왓던거 같아요",
          "original_form": "제일 큰오빠(아빠뻘 ㅋ) 님이  계산 다했는데\n음 제기억으로 돈백 넘게 나왓던거 같아요",
          "speaker_id": "1",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100012544.1.13",
          "form": "집에갈때 택시비로 10만원 주셨던 기억이 있네요",
          "original_form": "집에갈때 택시비로 10만원 주셨던 기억이 있네요",
          "speaker_id": "1",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100012544.1.14",
          "form": "우와ㅎㅎㅎ",
          "original_form": "우와ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100012544.1.15",
          "form": "엄청났네요ㅎㅎ",
          "original_form": "엄청났네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100012544.1.16",
          "form": "이런것도 운이있어야하나ㅎㅎㅎ",
          "original_form": "이런것도 운이있어야하나ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100012544.1.17",
          "form": "자영업 하셔서 음 다들  여유가 많으셨던거 같아요",
          "original_form": "자영업 하셔서 음 다들  여유가 많으셨던거 같아요",
          "speaker_id": "1",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100012544.1.18",
          "form": "여유가 있었으면 좋겟네요ㅎㅎㅎ",
          "original_form": "여유가 있었으면 좋겟네요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100012544.1.19",
          "form": "게임에 돈 엄청 많이 쓰시는분들 엄청많잖아요ㅎ",
          "original_form": "게임에 돈 엄청 많이 쓰시는분들 엄청많잖아요ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100012544.1.20",
          "form": "음 ㅋ 열심히 하시니 곧 여유 잇어 질거에요 ㅎㅎ",
          "original_form": "음 ㅋ 열심히 하시니 곧 여유 잇어 질거에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100012544.1.21",
          "form": "네네 많죠",
          "original_form": "네네 많죠",
          "speaker_id": "1",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100012544.1.22",
          "form": "저 모바일로 뮤했는데ㅎ 5억이상 쓰시더라구요ㅎ",
          "original_form": "저 모바일로 뮤했는데ㅎ 5억이상 쓰시더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100012544.1.23",
          "form": "꼭 그렇게되길 바래봐야죠ㅎㅎ",
          "original_form": "꼭 그렇게되길 바래봐야죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100012544.1.24",
          "form": "근데 안될것같애요ㅎㅎㅎㅎㅎㅎㅎㄹㄹㄹ",
          "original_form": "근데 안될것같애요ㅎㅎㅎㅎㅎㅎㅎㄹㄹㄹ",
          "speaker_id": "2",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100012544.1.25",
          "form": "돈많아서 쓰시는분도 있구\n대출쓰시는분도 있고 ㄷㄷㄷ",
          "original_form": "돈많아서 쓰시는분도 있구\n대출쓰시는분도 있고 ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100012544.1.26",
          "form": "ㄷㄷ 근데 돈을 쓰게 게임이 만들긴해요ㅎ",
          "original_form": "ㄷㄷ 근데 돈을 쓰게 게임이 만들긴해요ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100012544.1.27",
          "form": "저도 한창할때 100만원정도 쓴듯ㅎ",
          "original_form": "저도 한창할때 100만원정도 쓴듯ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100012544.1.28",
          "form": "커뮤니티 활동도 안했는데ㄹㄹㄹ",
          "original_form": "커뮤니티 활동도 안했는데ㄹㄹㄹ",
          "speaker_id": "2",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100012544.1.29",
          "form": "음 남자들은 특히 음 머랄까 ㅎㅎㅎ 과시하고 싶은 것도",
          "original_form": "음 남자들은 특히 음 머랄까 ㅎㅎㅎ 과시하고 싶은 것도",
          "speaker_id": "1",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100012544.1.30",
          "form": "있고  게임이 템이 안되면 안끼워줘요 ㅎㅎㅎ",
          "original_form": "있고  게임이 템이 안되면 안끼워줘요 ㅎㅎㅎ",
