      "utterance": [
        {
          "id": "MDRW2100034856.1.1",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210828 20:58"
        },
        {
          "id": "MDRW2100034856.1.2",
          "form": "일어나서 아직 암것도 안 먹었어?",
          "original_form": "일어나서 아직 암것도 안 먹었어?",
          "speaker_id": "1",
          "time": "20210828 20:59"
        },
        {
          "id": "MDRW2100034856.1.3",
          "form": "아까 5시쯤 김치찌개 먹긴 했는데",
          "original_form": "아까 5시쯤 김치찌개 먹긴 했는데",
          "speaker_id": "2",
          "time": "20210828 21:00"
        },
        {
          "id": "MDRW2100034856.1.4",
          "form": "거의 안 먹은 거나 마찬가지라",
          "original_form": "거의 안 먹은 거나 마찬가지라",
          "speaker_id": "2",
          "time": "20210828 21:00"
        },
        {
          "id": "MDRW2100034856.1.5",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210828 21:00"
        },
        {
          "id": "MDRW2100034856.1.6",
          "form": "지금 식당 다 닫았겠지",
          "original_form": "지금 식당 다 닫았겠지",
          "speaker_id": "2",
          "time": "20210828 21:01"
        },
        {
          "id": "MDRW2100034856.1.7",
          "form": "10시도 아니고 9시 너무함",
          "original_form": "10시도 아니고 9시 너무함",
          "speaker_id": "2",
          "time": "20210828 21:01"
        },
        {
          "id": "MDRW2100034856.1.8",
          "form": "배달은 되지 않아?",
          "original_form": "배달은 되지 않아?",
          "speaker_id": "1",
          "time": "20210828 21:01"
        },
        {
          "id": "MDRW2100034856.1.9",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210828 21:02"
        },
        {
          "id": "MDRW2100034856.1.10",
          "form": "피로랑 같이 온 식욕감퇴라 걱정되는 구만 잠자리 안 좋은 거 말고는 크게 아픈데 없어서 몸이 안 좋은지도 잘 모르겠구...",
          "original_form": "피로랑 같이 온 식욕감퇴라 걱정되는 구만 잠자리 안 좋은 거 말고는 크게 아픈데 없어서 몸이 안 좋은지도 잘 모르겠구...",
          "speaker_id": "1",
          "time": "20210828 21:06"
        },
        {
          "id": "MDRW2100034856.1.11",
          "form": "그래도 오늘 잠은 나쁘지 않았어",
          "original_form": "그래도 오늘 잠은 나쁘지 않았어",
          "speaker_id": "2",
          "time": "20210828 21:07"
        },
        {
          "id": "MDRW2100034856.1.12",
          "form": "꿈은 안 꾼 거 같고",
          "original_form": "꿈은 안 꾼 거 같고",
          "speaker_id": "2",
          "time": "20210828 21:08"
        },
        {
          "id": "MDRW2100034856.1.13",
          "form": "한시간마다 깨긴 핶는데ㅋㅋㅋ",
          "original_form": "한시간마다 깨긴 핶는데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:08"
        },
        {
          "id": "MDRW2100034856.1.14",
          "form": "아이고ㅋㅋㅋ",
          "original_form": "아이고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:08"
        },
        {
          "id": "MDRW2100034856.1.15",
          "form": "근데 지금",
          "original_form": "근데 지금",
          "speaker_id": "2",
          "time": "20210828 21:08"
        },
        {
          "id": "MDRW2100034856.1.16",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210828 21:08"
        },
        {
          "id": "MDRW2100034856.1.17",
          "form": "다른 건 몰라도 아이스크림은 좋다",
          "original_form": "다른 건 몰라도 아이스크림은 좋다",
          "speaker_id": "2",
          "time": "20210828 21:08"
        },
        {
          "id": "MDRW2100034856.1.18",
          "form": "하루에 세개씩 먹고 있어",
          "original_form": "하루에 세개씩 먹고 있어",
          "speaker_id": "2",
          "time": "20210828 21:08"
        },
        {
          "id": "MDRW2100034856.1.19",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210828 21:09"
        },
        {
          "id": "MDRW2100034856.1.20",
          "form": "먹고 싶은 거 먹어",
          "original_form": "먹고 싶은 거 먹어",
          "speaker_id": "1",
          "time": "20210828 21:09"
        },
        {
          "id": "MDRW2100034856.1.21",
          "form": "식욕 없을 때는 그게 제일 좋은 거 같음",
          "original_form": "식욕 없을 때는 그게 제일 좋은 거 같음",
          "speaker_id": "1",
          "time": "20210828 21:09"
        },
        {
          "id": "MDRW2100034856.1.22",
          "form": "응 배탈만 안 나면 되지 머",
          "original_form": "응 배탈만 안 나면 되지 머",
          "speaker_id": "2",
          "time": "20210828 21:09"
        }
      ]
