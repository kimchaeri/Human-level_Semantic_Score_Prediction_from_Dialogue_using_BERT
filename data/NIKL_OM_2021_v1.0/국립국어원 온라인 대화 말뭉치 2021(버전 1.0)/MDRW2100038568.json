      "utterance": [
        {
          "id": "MDRW2100038568.1.1",
          "form": "직장 다니시나요",
          "original_form": "직장 다니시나요",
          "speaker_id": "2",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100038568.1.2",
          "form": "알바하고있어요ㅋ",
          "original_form": "알바하고있어요ㅋ",
          "speaker_id": "1",
          "time": "20210902 16:10"
        },
        {
          "id": "MDRW2100038568.1.3",
          "form": "직장다니세요?",
          "original_form": "직장다니세요?",
          "speaker_id": "1",
          "time": "20210902 16:10"
        },
        {
          "id": "MDRW2100038568.1.4",
          "form": "저는 아직 대학생이라서요",
          "original_form": "저는 아직 대학생이라서요",
          "speaker_id": "2",
          "time": "20210902 16:13"
        },
        {
          "id": "MDRW2100038568.1.5",
          "form": "어떤 알바 하세요?",
          "original_form": "어떤 알바 하세요?",
          "speaker_id": "2",
          "time": "20210902 16:13"
        },
        {
          "id": "MDRW2100038568.1.6",
          "form": "아 대학생이시구나",
          "original_form": "아 대학생이시구나",
          "speaker_id": "1",
          "time": "20210902 17:02"
        },
        {
          "id": "MDRW2100038568.1.7",
          "form": "저는 복지관에",
          "original_form": "저는 복지관에",
          "speaker_id": "1",
          "time": "20210902 17:02"
        },
        {
          "id": "MDRW2100038568.1.8",
          "form": "미술보조강사로 일하고있어요ㅋ",
          "original_form": "미술보조강사로 일하고있어요ㅋ",
          "speaker_id": "1",
          "time": "20210902 17:02"
        },
        {
          "id": "MDRW2100038568.1.9",
          "form": "와 대단하시네요",
          "original_form": "와 대단하시네요",
          "speaker_id": "2",
          "time": "20210902 17:03"
        },
        {
          "id": "MDRW2100038568.1.10",
          "form": "저도 예전에 미술 조금 하다가",
          "original_form": "저도 예전에 미술 조금 하다가",
          "speaker_id": "2",
          "time": "20210902 17:03"
        },
        {
          "id": "MDRW2100038568.1.11",
          "form": "지금은 디자인 전공하고 있어요",
          "original_form": "지금은 디자인 전공하고 있어요",
          "speaker_id": "2",
          "time": "20210902 17:03"
        },
        {
          "id": "MDRW2100038568.1.12",
          "form": "아 디자인전공 멋지시네요",
          "original_form": "아 디자인전공 멋지시네요",
          "speaker_id": "1",
          "time": "20210902 17:04"
        },
        {
          "id": "MDRW2100038568.1.13",
          "form": "저는 근데 대단한건아니고ㅋㅋ전업주부하면서",
          "original_form": "저는 근데 대단한건아니고ㅋㅋ전업주부하면서",
          "speaker_id": "1",
          "time": "20210902 17:04"
        },
        {
          "id": "MDRW2100038568.1.14",
          "form": "알바하는데",
          "original_form": "알바하는데",
          "speaker_id": "1",
          "time": "20210902 17:05"
        },
        {
          "id": "MDRW2100038568.1.15",
          "form": "좋은것같아요.ㅋㅋ",
          "original_form": "좋은것같아요.ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:05"
        },
        {
          "id": "MDRW2100038568.1.16",
          "form": "부담이없어서요ㅋ",
          "original_form": "부담이없어서요ㅋ",
          "speaker_id": "1",
          "time": "20210902 17:05"
        },
        {
          "id": "MDRW2100038568.1.17",
          "form": "와 주부님이 알바하는건 처음 봤어요",
          "original_form": "와 주부님이 알바하는건 처음 봤어요",
          "speaker_id": "2",
          "time": "20210902 17:09"
        },
        {
          "id": "MDRW2100038568.1.18",
          "form": "아 그래요?ㅋㅋ",
          "original_form": "아 그래요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 08:29"
        },
        {
          "id": "MDRW2100038568.1.19",
          "form": "애키우면서 다른일 하다가 애가 아프고 맡길곳 없어서ㅋㅋ",
          "original_form": "애키우면서 다른일 하다가 애가 아프고 맡길곳 없어서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 08:29"
        },
        {
          "id": "MDRW2100038568.1.20",
          "form": "그만뒀다가ㅋㅋ어느정도 키우고 다시 일갔어요ㅋㅋ",
          "original_form": "그만뒀다가ㅋㅋ어느정도 키우고 다시 일갔어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 08:29"
        },
        {
          "id": "MDRW2100038568.1.21",
          "form": "제 주변 어른들은 애 키우는데에만 전념하시더라구요",
          "original_form": "제 주변 어른들은 애 키우는데에만 전념하시더라구요",
          "speaker_id": "2",
          "time": "20210903 08:30"
        },
        {
          "id": "MDRW2100038568.1.22",
          "form": "물론 저희 부모님도 그렇구요",
          "original_form": "물론 저희 부모님도 그렇구요",
          "speaker_id": "2",
          "time": "20210903 08:30"
        },
        {
          "id": "MDRW2100038568.1.23",
          "form": "네네 저도 그랬어요. 알바도 일주일에 4시간밖에안해요ㅋㅋㅋ용돈벌이겸 다니죠ㅋㅋ",
          "original_form": "네네 저도 그랬어요. 알바도 일주일에 4시간밖에안해요ㅋㅋㅋ용돈벌이겸 다니죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 08:31"
        },
        {
          "id": "MDRW2100038568.1.24",
          "form": "4시간이라도 어디에요 그거 모으시면",
          "original_form": "4시간이라도 어디에요 그거 모으시면",
          "speaker_id": "2",
          "time": "20210903 08:33"
        },
        {
          "id": "MDRW2100038568.1.25",
          "form": "원하는 물건은 쭉 장만하실 수 있을거 같은데요ㅎㅎ",
          "original_form": "원하는 물건은 쭉 장만하실 수 있을거 같은데요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 08:33"
        },
        {
          "id": "MDRW2100038568.1.26",
          "form": "네 맞아요ㅋㅋㅋㅋㅋ좋아요ㅋㅋ",
          "original_form": "네 맞아요ㅋㅋㅋㅋㅋ좋아요ㅋㅋ",
