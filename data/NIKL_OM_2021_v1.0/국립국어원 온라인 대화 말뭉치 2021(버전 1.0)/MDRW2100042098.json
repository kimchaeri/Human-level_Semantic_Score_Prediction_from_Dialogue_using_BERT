      "utterance": [
        {
          "id": "MDRW2100042098.1.1",
          "form": "요즘 시청중인 프로그램 있으신가요?",
          "original_form": "요즘 시청중인 프로그램 있으신가요?",
          "speaker_id": "1",
          "time": "20210906 19:39"
        },
        {
          "id": "MDRW2100042098.1.2",
          "form": "놀면뭐하니! 일박이일ㅋㅋ",
          "original_form": "놀면뭐하니! 일박이일ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:46"
        },
        {
          "id": "MDRW2100042098.1.3",
          "form": "봅니다",
          "original_form": "봅니다",
          "speaker_id": "2",
          "time": "20210906 19:46"
        },
        {
          "id": "MDRW2100042098.1.4",
          "form": "오 예능 주로 보시나보네요!",
          "original_form": "오 예능 주로 보시나보네요!",
          "speaker_id": "1",
          "time": "20210906 20:05"
        },
        {
          "id": "MDRW2100042098.1.5",
          "form": "드라마는 안보시나요??",
          "original_form": "드라마는 안보시나요??",
          "speaker_id": "1",
          "time": "20210906 20:06"
        },
        {
          "id": "MDRW2100042098.1.6",
          "form": "ㅋㅋ드라마는 주로 일일드라마ㅋㅋ",
          "original_form": "ㅋㅋ드라마는 주로 일일드라마ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:06"
        },
        {
          "id": "MDRW2100042098.1.7",
          "form": "부모님 보셔서ㅋㅋ",
          "original_form": "부모님 보셔서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:06"
        },
        {
          "id": "MDRW2100042098.1.8",
          "form": "뭐 보세요?",
          "original_form": "뭐 보세요?",
          "speaker_id": "2",
          "time": "20210906 20:07"
        },
        {
          "id": "MDRW2100042098.1.9",
          "form": "저는 종영한 드라마 보고있어요!",
          "original_form": "저는 종영한 드라마 보고있어요!",
          "speaker_id": "1",
          "time": "20210906 20:07"
        },
        {
          "id": "MDRW2100042098.1.10",
          "form": "보이스4랑 나의나라요!!",
          "original_form": "보이스4랑 나의나라요!!",
          "speaker_id": "1",
          "time": "20210906 20:07"
        },
        {
          "id": "MDRW2100042098.1.11",
          "form": "너의나라인가.....ㅎㅎ",
          "original_form": "너의나라인가.....ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 20:07"
        },
        {
          "id": "MDRW2100042098.1.12",
          "form": "혹시 보셨나요?",
          "original_form": "혹시 보셨나요?",
          "speaker_id": "1",
          "time": "20210906 20:07"
        },
        {
          "id": "MDRW2100042098.1.13",
          "form": "아뇨ㅋㅋ 누구 나와요?",
          "original_form": "아뇨ㅋㅋ 누구 나와요?",
          "speaker_id": "2",
          "time": "20210906 20:09"
        },
        {
          "id": "MDRW2100042098.1.14",
          "form": "보이스는 송승헌 나온거 맞죠?",
          "original_form": "보이스는 송승헌 나온거 맞죠?",
          "speaker_id": "2",
          "time": "20210906 20:18"
        },
        {
          "id": "MDRW2100042098.1.15",
          "form": "네 맞아요!!",
          "original_form": "네 맞아요!!",
          "speaker_id": "1",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042098.1.16",
          "form": "유튜브 자주 보시나요?",
          "original_form": "유튜브 자주 보시나요?",
          "speaker_id": "1",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042098.1.17",
          "form": "네ㅋㅋ 여러컨텐츠 다봐요",
          "original_form": "네ㅋㅋ 여러컨텐츠 다봐요",
          "speaker_id": "2",
          "time": "20210906 22:39"
        },
        {
          "id": "MDRW2100042098.1.18",
          "form": "오 주로 어떤거 보시나요?!",
          "original_form": "오 주로 어떤거 보시나요?!",
          "speaker_id": "1",
          "time": "20210906 22:41"
        },
        {
          "id": "MDRW2100042098.1.19",
          "form": "홈트랑 음악 공연 찾아봐요!",
          "original_form": "홈트랑 음악 공연 찾아봐요!",
          "speaker_id": "2",
          "time": "20210906 22:46"
        },
        {
          "id": "MDRW2100042098.1.20",
          "form": "홈트 어떤거 하시나요?!",
          "original_form": "홈트 어떤거 하시나요?!",
          "speaker_id": "1",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042098.1.21",
          "form": "저도 요즘 홈트에 관심이 생겨서",
          "original_form": "저도 요즘 홈트에 관심이 생겨서",
          "speaker_id": "1",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042098.1.22",
          "form": "빅씨스라고",
          "original_form": "빅씨스라고",
          "speaker_id": "2",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042098.1.23",
          "form": "되게 좋아요!",
          "original_form": "되게 좋아요!",
          "speaker_id": "2",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042098.1.24",
          "form": "동작반복없고 단계별이고",
          "original_form": "동작반복없고 단계별이고",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042098.1.25",
          "form": "오 방금 찾아봤는데 좋아보여요!!!",
          "original_form": "오 방금 찾아봤는데 좋아보여요!!!",
          "speaker_id": "1",
          "time": "20210906 23:43"
        },
        {
          "id": "MDRW2100042098.1.26",
          "form": "꼭 해볼게요!!",
          "original_form": "꼭 해볼게요!!",
          "speaker_id": "1",
          "time": "20210906 23:43"
        }
      ]
    }
