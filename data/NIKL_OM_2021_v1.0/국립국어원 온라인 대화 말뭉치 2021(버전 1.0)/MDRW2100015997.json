      "utterance": [
        {
          "id": "MDRW2100015997.1.1",
          "form": "티비봐요?",
          "original_form": "티비봐요?",
          "speaker_id": "1",
          "time": "20210826 06:49"
        },
        {
          "id": "MDRW2100015997.1.2",
          "form": "티비는 뉴스볼때만 보고",
          "original_form": "티비는 뉴스볼때만 보고",
          "speaker_id": "2",
          "time": "20210826 10:31"
        },
        {
          "id": "MDRW2100015997.1.3",
          "form": "주로 넷플이랑 유투브 봐요~",
          "original_form": "주로 넷플이랑 유투브 봐요~",
          "speaker_id": "2",
          "time": "20210826 10:31"
        },
        {
          "id": "MDRW2100015997.1.4",
          "form": "전 런닝맨요",
          "original_form": "전 런닝맨요",
          "speaker_id": "1",
          "time": "20210826 12:44"
        },
        {
          "id": "MDRW2100015997.1.5",
          "form": "런닝맨 요즘에 재밌나요??",
          "original_form": "런닝맨 요즘에 재밌나요??",
          "speaker_id": "2",
          "time": "20210826 13:19"
        },
        {
          "id": "MDRW2100015997.1.6",
          "form": "광수없어도 잼나요",
          "original_form": "광수없어도 잼나요",
          "speaker_id": "1",
          "time": "20210826 17:52"
        },
        {
          "id": "MDRW2100015997.1.7",
          "form": "아 정말요??ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 정말요??ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100015997.1.8",
          "form": "멤버들 케미가 좋아서~ 재밌나보네요~",
          "original_form": "멤버들 케미가 좋아서~ 재밌나보네요~",
          "speaker_id": "2",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100015997.1.9",
          "form": "그쵸?",
          "original_form": "그쵸?",
          "speaker_id": "1",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100015997.1.10",
          "form": "본방은 못보고",
          "original_form": "본방은 못보고",
          "speaker_id": "2",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100015997.1.11",
          "form": "클입으로 봤었는데",
          "original_form": "클입으로 봤었는데",
          "speaker_id": "2",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100015997.1.12",
          "form": "너무 웃기더라고요~",
          "original_form": "너무 웃기더라고요~",
          "speaker_id": "2",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100015997.1.13",
          "form": "전소민요 웃겨요",
          "original_form": "전소민요 웃겨요",
          "speaker_id": "1",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100015997.1.14",
          "form": "ㅎㅎ 그 식스센스인가??",
          "original_form": "ㅎㅎ 그 식스센스인가??",
          "speaker_id": "2",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100015997.1.15",
          "form": "거기서도 엄청 웃기더라고요~",
          "original_form": "거기서도 엄청 웃기더라고요~",
          "speaker_id": "2",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100015997.1.16",
          "form": "제시도요",
          "original_form": "제시도요",
