      "utterance": [
        {
          "id": "MDRW2100020509.1.1",
          "form": "그럼 테두리 드십니까",
          "original_form": "그럼 테두리 드십니까",
          "speaker_id": "1",
          "time": "20210828 08:40"
        },
        {
          "id": "MDRW2100020509.1.2",
          "form": "물론이죠",
          "original_form": "물론이죠",
          "speaker_id": "2",
          "time": "20210828 08:40"
        },
        {
          "id": "MDRW2100020509.1.3",
          "form": "테두리가 꽃인걸요",
          "original_form": "테두리가 꽃인걸요",
          "speaker_id": "2",
          "time": "20210828 08:40"
        },
        {
          "id": "MDRW2100020509.1.4",
          "form": "크러스트 추가하십니까",
          "original_form": "크러스트 추가하십니까",
          "speaker_id": "1",
          "time": "20210828 08:40"
        },
        {
          "id": "MDRW2100020509.1.5",
          "form": "와 아시는군요",
          "original_form": "와 아시는군요",
          "speaker_id": "1",
          "time": "20210828 08:40"
        },
        {
          "id": "MDRW2100020509.1.6",
          "form": "그럼요 비싸서 크러스트로 만족하긴 합니다...",
          "original_form": "그럼요 비싸서 크러스트로 만족하긴 합니다...",
          "speaker_id": "2",
          "time": "20210828 08:40"
        },
        {
          "id": "MDRW2100020509.1.7",
          "form": "테두리 약간 그 디핑소스 찍어먹어야하는거알죠",
          "original_form": "테두리 약간 그 디핑소스 찍어먹어야하는거알죠",
          "speaker_id": "1",
          "time": "20210828 08:41"
        },
        {
          "id": "MDRW2100020509.1.8",
          "form": "바이트는 남의 돈으로 먹을 때 해요..",
          "original_form": "바이트는 남의 돈으로 먹을 때 해요..",
          "speaker_id": "2",
          "time": "20210828 08:41"
        },
        {
          "id": "MDRW2100020509.1.9",
          "form": "그쵸 이때 또 갈릭이 나와줘야죠",
          "original_form": "그쵸 이때 또 갈릭이 나와줘야죠",
          "speaker_id": "2",
          "time": "20210828 08:41"
        },
        {
          "id": "MDRW2100020509.1.10",
          "form": "진짜 남들 그거 왜먹냐 그럴 때 모르는소리",
          "original_form": "진짜 남들 그거 왜먹냐 그럴 때 모르는소리",
          "speaker_id": "1",
          "time": "20210828 08:41"
        },
        {
          "id": "MDRW2100020509.1.11",
          "form": "갈릭찍으면 얼마나 맛있게요",
          "original_form": "갈릭찍으면 얼마나 맛있게요",
          "speaker_id": "1",
          "time": "20210828 08:41"
        },
        {
          "id": "MDRW2100020509.1.12",
          "form": "누가 그런소릴 한답니까",
          "original_form": "누가 그런소릴 한답니까",
          "speaker_id": "2",
          "time": "20210828 08:41"
        },
        {
          "id": "MDRW2100020509.1.13",
          "form": "이런 무지한 자들",
          "original_form": "이런 무지한 자들",
          "speaker_id": "2",
          "time": "20210828 08:41"
        },
        {
          "id": "MDRW2100020509.1.14",
          "form": "있어요 저희집에 ㅋㅋㅋㅋ",
          "original_form": "있어요 저희집에 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 08:41"
        },
        {
          "id": "MDRW2100020509.1.15",
          "form": "앗 가족분이시군뇨ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앗 가족분이시군뇨ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 08:42"
        },
        {
          "id": "MDRW2100020509.1.16",
          "form": "맛못알이라",
          "original_form": "맛못알이라",
          "speaker_id": "1",
          "time": "20210828 08:42"
        },
        {
          "id": "MDRW2100020509.1.17",
          "form": "가족끼리도 입맛이 다 달러요",
          "original_form": "가족끼리도 입맛이 다 달러요",
          "speaker_id": "2",
          "time": "20210828 08:42"
        },
        {
          "id": "MDRW2100020509.1.18",
          "form": "테두리를 안먹더라구요",
          "original_form": "테두리를 안먹더라구요",
          "speaker_id": "1",
          "time": "20210828 08:42"
        },
        {
          "id": "MDRW2100020509.1.19",
          "form": "저런 가족이면 인정입니다",
          "original_form": "저런 가족이면 인정입니다",
          "speaker_id": "2",
          "time": "20210828 08:42"
        },
        {
          "id": "MDRW2100020509.1.20",
          "form": "다 내께 되니까요",
          "original_form": "다 내께 되니까요",
