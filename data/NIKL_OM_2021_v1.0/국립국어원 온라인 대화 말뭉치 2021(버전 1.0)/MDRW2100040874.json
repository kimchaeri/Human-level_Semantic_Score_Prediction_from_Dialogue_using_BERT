      "utterance": [
        {
          "id": "MDRW2100040874.1.1",
          "form": "어제 구연산 다 써서 사야하겠네했는데",
          "original_form": "어제 구연산 다 써서 사야하겠네했는데",
          "speaker_id": "1",
          "time": "20210905 12:25"
        },
        {
          "id": "MDRW2100040874.1.2",
          "form": "포인트로 살수있는곳 부터 찾고있었네요 ㅎ",
          "original_form": "포인트로 살수있는곳 부터 찾고있었네요 ㅎ",
          "speaker_id": "1",
          "time": "20210905 12:25"
        },
        {
          "id": "MDRW2100040874.1.3",
          "form": "ㅎ 저도요. 제값주고사면 이제 비싸게 산듯해요",
          "original_form": "ㅎ 저도요. 제값주고사면 이제 비싸게 산듯해요",
          "speaker_id": "2",
          "time": "20210905 13:53"
        },
        {
          "id": "MDRW2100040874.1.4",
          "form": "먼가 손해보는 느낌이죠 ㅎ",
          "original_form": "먼가 손해보는 느낌이죠 ㅎ",
          "speaker_id": "1",
          "time": "20210905 14:41"
        },
        {
          "id": "MDRW2100040874.1.5",
          "form": "근데 할인만하면 자꾸 사서 문제네요.",
          "original_form": "근데 할인만하면 자꾸 사서 문제네요.",
          "speaker_id": "2",
          "time": "20210905 15:15"
        },
        {
          "id": "MDRW2100040874.1.6",
          "form": "쟁인다고 사게 되요 ㅎㅎ",
          "original_form": "쟁인다고 사게 되요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 15:23"
        },
        {
          "id": "MDRW2100040874.1.7",
          "form": "맞아요.  더이상 들어갈데도없어요.",
          "original_form": "맞아요.  더이상 들어갈데도없어요.",
          "speaker_id": "2",
          "time": "20210905 15:24"
        },
        {
          "id": "MDRW2100040874.1.8",
          "form": "욕심만 많아져요",
          "original_form": "욕심만 많아져요",
          "speaker_id": "2",
          "time": "20210905 15:24"
        },
        {
          "id": "MDRW2100040874.1.9",
          "form": "ㅎㅎ 근데 또 안사면 안될것같은 맘이 들어서 ㅎ",
          "original_form": "ㅎㅎ 근데 또 안사면 안될것같은 맘이 들어서 ㅎ",
          "speaker_id": "1",
          "time": "20210905 15:28"
        },
        {
          "id": "MDRW2100040874.1.10",
          "form": "맞아요.  안사면 손해보는듯해요",
          "original_form": "맞아요.  안사면 손해보는듯해요",
          "speaker_id": "2",
          "time": "20210905 15:32"
        },
        {
          "id": "MDRW2100040874.1.11",
          "form": "화장품도 샘플을 하도 많이 받아놔서",
          "original_form": "화장품도 샘플을 하도 많이 받아놔서",
          "speaker_id": "1",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100040874.1.12",
          "form": "화장품 산지가 언제인지 ㅎㅎ",
          "original_form": "화장품 산지가 언제인지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100040874.1.13",
          "form": "저도요.  작년부턴 기초도 잘안바르고사네요",
          "original_form": "저도요.  작년부턴 기초도 잘안바르고사네요",
          "speaker_id": "2",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100040874.1.14",
          "form": "아고 ㅎㅎ 그래도 기초 안바르면 땡기지않으세요?",
          "original_form": "아고 ㅎㅎ 그래도 기초 안바르면 땡기지않으세요?",
          "speaker_id": "1",
          "time": "20210905 17:00"
        },
        {
          "id": "MDRW2100040874.1.15",
          "form": "요즘엔 땡기더라구요",
          "original_form": "요즘엔 땡기더라구요",
          "speaker_id": "2",
          "time": "20210905 17:01"
        },
        {
          "id": "MDRW2100040874.1.16",
          "form": "그래서 챌린져스 팩하기 신청해서 하고있어요",
          "original_form": "그래서 챌린져스 팩하기 신청해서 하고있어요",
          "speaker_id": "2",
          "time": "20210905 17:01"
        },
        {
          "id": "MDRW2100040874.1.17",
          "form": "의무감에 해야지 잘안해지더라구요",
          "original_form": "의무감에 해야지 잘안해지더라구요",
          "speaker_id": "2",
          "time": "20210905 17:01"
        },
        {
          "id": "MDRW2100040874.1.18",
          "form": "그래서 챌린저스가 좋은거같아요",
          "original_form": "그래서 챌린저스가 좋은거같아요",
          "speaker_id": "1",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100040874.1.19",
          "form": "아 챌린저스 언더아머 마스크였나?",
          "original_form": "아 챌린저스 언더아머 마스크였나?",
          "speaker_id": "1",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100040874.1.20",
          "form": "그거 왔나요?",
          "original_form": "그거 왔나요?",
          "speaker_id": "1",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100040874.1.21",
          "form": "아직이요",
          "original_form": "아직이요",
          "speaker_id": "2",
          "time": "20210905 17:04"
        },
        {
          "id": "MDRW2100040874.1.22",
          "form": "설마 먹튀하진 않겠죠?ㅎ",
          "original_form": "설마 먹튀하진 않겠죠?ㅎ",
          "speaker_id": "1",
          "time": "20210905 17:51"
        },
        {
          "id": "MDRW2100040874.1.23",
          "form": "네. 설마요~~ 아마 담주엔 오지않을까해요",
          "original_form": "네. 설마요~~ 아마 담주엔 오지않을까해요",
          "speaker_id": "2",
          "time": "20210905 17:55"
        },
        {
          "id": "MDRW2100040874.1.24",
          "form": "잊지말고 챙겨야할까봐요 ㅎ",
          "original_form": "잊지말고 챙겨야할까봐요 ㅎ",
          "speaker_id": "1",
          "time": "20210905 18:13"
        },
        {
          "id": "MDRW2100040874.1.25",
          "form": "밥먹고 올께요~",
          "original_form": "밥먹고 올께요~",
          "speaker_id": "1",
          "time": "20210905 18:13"
        },
        {
          "id": "MDRW2100040874.1.26",
