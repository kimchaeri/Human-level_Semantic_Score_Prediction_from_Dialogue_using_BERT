      "utterance": [
        {
          "id": "MDRW2100045853.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ토마토랑 달걀에",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ토마토랑 달걀에",
          "speaker_id": "2",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100045853.1.2",
          "form": "회사 언니한테 처음 토달볶 얘기를 접했을땐 너무 충격이었어요 왜 달걀을 토마토랑 볶지...?ㅋㅋㅋㅋ",
          "original_form": "회사 언니한테 처음 토달볶 얘기를 접했을땐 너무 충격이었어요 왜 달걀을 토마토랑 볶지...?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100045853.1.3",
          "form": "추가로 채 친 감자 넣으면 맛나요",
          "original_form": "추가로 채 친 감자 넣으면 맛나요",
          "speaker_id": "2",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100045853.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ아 저두요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 저두요",
          "speaker_id": "2",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100045853.1.5",
          "form": "왜 하필 토마토를 달걀이랑..?",
          "original_form": "왜 하필 토마토를 달걀이랑..?",
          "speaker_id": "2",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100045853.1.6",
          "form": "오 감자",
          "original_form": "오 감자",
          "speaker_id": "1",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100045853.1.7",
          "form": "저 감자도 좋아해요!!",
          "original_form": "저 감자도 좋아해요!!",
          "speaker_id": "1",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100045853.1.8",
          "form": "토마토 달걀 감자 넣고",
          "original_form": "토마토 달걀 감자 넣고",
          "speaker_id": "2",
          "time": "20210908 08:07"
        },
        {
          "id": "MDRW2100045853.1.9",
          "form": "소금간만 해도 맛나더라구요",
          "original_form": "소금간만 해도 맛나더라구요",
          "speaker_id": "2",
          "time": "20210908 08:07"
        },
        {
          "id": "MDRW2100045853.1.10",
          "form": "전 그 간이 참 힘들더라구여..",
          "original_form": "전 그 간이 참 힘들더라구여..",
          "speaker_id": "1",
          "time": "20210908 08:07"
        },
        {
          "id": "MDRW2100045853.1.11",
          "form": "ㅋㅌㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅌㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:07"
        },
        {
          "id": "MDRW2100045853.1.12",
          "form": "알리오올리오 할 때마다 간이 안맞아요",
          "original_form": "알리오올리오 할 때마다 간이 안맞아요",
          "speaker_id": "1",
          "time": "20210908 08:07"
        },
        {
          "id": "MDRW2100045853.1.13",
          "form": "제일 어려운 적당한 간",
          "original_form": "제일 어려운 적당한 간",
          "speaker_id": "2",
          "time": "20210908 08:07"
        },
        {
          "id": "MDRW2100045853.1.14",
          "form": "로제나 토마토파스타는 시중에 나온 소스 와랄라 넣고 볶으면 그만인데",
          "original_form": "로제나 토마토파스타는 시중에 나온 소스 와랄라 넣고 볶으면 그만인데",
          "speaker_id": "1",
          "time": "20210908 08:09"
        },
        {
          "id": "MDRW2100045853.1.15",
          "form": "알리오올리오는 제가 직접 간을 해야하니까 어렵도라구요 ㅋㅋ",
          "original_form": "알리오올리오는 제가 직접 간을 해야하니까 어렵도라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:09"
        },
        {
          "id": "MDRW2100045853.1.16",
          "form": "맞아요 맞아요 맞아요",
          "original_form": "맞아요 맞아요 맞아요",
          "speaker_id": "2",
          "time": "20210908 08:10"
        },
        {
          "id": "MDRW2100045853.1.17",
          "form": "시판소스가 뙇 있으면 괜찮은데",
          "original_form": "시판소스가 뙇 있으면 괜찮은데",
          "speaker_id": "2",
          "time": "20210908 08:10"
        },
        {
          "id": "MDRW2100045853.1.18",
          "form": "알리오는 시판소스가 없나요..?",
          "original_form": "알리오는 시판소스가 없나요..?",
          "speaker_id": "2",
          "time": "20210908 08:11"
        },
        {
          "id": "MDRW2100045853.1.19",
          "form": "있을텐데....",
          "original_form": "있을텐데....",
          "speaker_id": "1",
          "time": "20210908 08:11"
        },
        {
          "id": "MDRW2100045853.1.20",
          "form": "제가 해먹고싶어서 올리브유로 사왔어요(?)",
          "original_form": "제가 해먹고싶어서 올리브유로 사왔어요(?)",
          "speaker_id": "1",
          "time": "20210908 08:11"
        },
        {
          "id": "MDRW2100045853.1.21",
          "form": "성공하셨나이까",
          "original_form": "성공하셨나이까",
          "speaker_id": "2",
          "time": "20210908 08:12"
        },
        {
          "id": "MDRW2100045853.1.22",
          "form": "아유 항상 어딘가 이상하더라고요",
          "original_form": "아유 항상 어딘가 이상하더라고요",
          "speaker_id": "1",
          "time": "20210908 08:13"
