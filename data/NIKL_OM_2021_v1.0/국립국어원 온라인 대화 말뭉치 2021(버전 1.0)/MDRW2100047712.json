      "utterance": [
        {
          "id": "MDRW2100047712.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210827 09:14"
        },
        {
          "id": "MDRW2100047712.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210827 19:24"
        },
        {
          "id": "MDRW2100047712.1.3",
          "form": "혹시 연애중이신가요?",
          "original_form": "혹시 연애중이신가요?",
          "speaker_id": "1",
          "time": "20210827 19:29"
        },
        {
          "id": "MDRW2100047712.1.4",
          "form": "아니오 ㅎㅎ name1님은요??",
          "original_form": "아니오 ㅎㅎ &name1&님은요??",
          "speaker_id": "2",
          "time": "20210827 19:33"
        },
        {
          "id": "MDRW2100047712.1.5",
          "form": "저는 장거리 중이라 그냥 혼자 지내는 것 같아요 ㅋㅋㅋㅋ",
          "original_form": "저는 장거리 중이라 그냥 혼자 지내는 것 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100047712.1.6",
          "form": "확실히 혼자가 더 편하네요 ㅎㅎ",
          "original_form": "확실히 혼자가 더 편하네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100047712.1.7",
          "form": "아 ㅜ 혼자가 저도 더 편하긴 해요",
          "original_form": "아 ㅜ 혼자가 저도 더 편하긴 해요",
          "speaker_id": "2",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100047712.1.8",
          "form": "시간 감정 돈 다 요새 아까워서..",
          "original_form": "시간 감정 돈 다 요새 아까워서..",
          "speaker_id": "2",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100047712.1.9",
          "form": "그쵸 저는 시간이 특히 더 아쉬워요",
          "original_form": "그쵸 저는 시간이 특히 더 아쉬워요",
          "speaker_id": "1",
          "time": "20210827 19:42"
        },
        {
          "id": "MDRW2100047712.1.10",
          "form": "마음이 식었나싶기도 해요 ㅎㅎ",
          "original_form": "마음이 식었나싶기도 해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 19:42"
        },
        {
          "id": "MDRW2100047712.1.11",
          "form": "결혼에 대해서도 별로 생각이 없으신가요?",
          "original_form": "결혼에 대해서도 별로 생각이 없으신가요?",
          "speaker_id": "1",
          "time": "20210827 19:43"
        },
        {
          "id": "MDRW2100047712.1.12",
          "form": "네 저는 아마 비혼일거같아요!",
          "original_form": "네 저는 아마 비혼일거같아요!",
          "speaker_id": "2",
          "time": "20210827 19:43"
        },
        {
          "id": "MDRW2100047712.1.13",
          "form": "혼자 사는게 젤 효율적이더라구요",
          "original_form": "혼자 사는게 젤 효율적이더라구요",
          "speaker_id": "2",
          "time": "20210827 19:43"
        },
        {
          "id": "MDRW2100047712.1.14",
          "form": "저도 그렇게 될 것 같아요 ㅋㅋㅋㅋ\r\n장거리하면 연락에 집착하게 되고 휴일 생겼다싶으면 서로 달려간다던데",
          "original_form": "저도 그렇게 될 것 같아요 ㅋㅋㅋㅋ\r\n장거리하면 연락에 집착하게 되고 휴일 생겼다싶으면 서로 달려간다던데",
          "speaker_id": "1",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100047712.1.15",
          "form": "저는 다 귀찮고 그러네요... ㅎㅎ",
          "original_form": "저는 다 귀찮고 그러네요... ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100047712.1.16",
          "form": "휴일에 달려가다니..",
          "original_form": "휴일에 달려가다니..",
          "speaker_id": "2",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100047712.1.17",
          "form": "기력이 없는뎈ㅋㅋㅋㅋ",
          "original_form": "기력이 없는뎈ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100047712.1.18",
          "form": "평일에 일하면 휴일에 앓아눕지않나요",
          "original_form": "평일에 일하면 휴일에 앓아눕지않나요",
          "speaker_id": "2",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100047712.1.19",
          "form": "그쵸 그게 맞지않나요?",
          "original_form": "그쵸 그게 맞지않나요?",
          "speaker_id": "1",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100047712.1.20",
          "form": "어떻게 움직이지?",
          "original_form": "어떻게 움직이지?",
          "speaker_id": "1",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100047712.1.21",
          "form": "근데 기차를 타든, 운전을 4시간해서 가든 만나야한대요",
          "original_form": "근데 기차를 타든, 운전을 4시간해서 가든 만나야한대요",
          "speaker_id": "1",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100047712.1.22",
          "form": "그걸로 진짜 많이 싸웠어요 피곤해 죽겠는데 뭘 그렇게까지 만나야하냐고..",
          "original_form": "그걸로 진짜 많이 싸웠어요 피곤해 죽겠는데 뭘 그렇게까지 만나야하냐고..",
          "speaker_id": "1",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100047712.1.23",
          "form": "운전 4시간이면 왕복 8시간인데ㅠ",
          "original_form": "운전 4시간이면 왕복 8시간인데ㅠ",
          "speaker_id": "1",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100047712.1.24",
          "form": "에바인데요",
          "original_form": "에바인데요",
          "speaker_id": "2",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100047712.1.25",
          "form": "제주도인가요? 너무 머네요",
          "original_form": "제주도인가요? 너무 머네요",
          "speaker_id": "2",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100047712.1.26",
          "form": "보고싶은사람이 오던가..",
          "original_form": "보고싶은사람이 오던가..",
          "speaker_id": "2",
          "time": "20210827 19:47"
        },
        {
          "id": "MDRW2100047712.1.27",
          "form": "제가 한번도 안 보러 가서 섭섭하대요 ㅋㅋㅋㅋㅋ",
          "original_form": "제가 한번도 안 보러 가서 섭섭하대요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:01"
        },
        {
          "id": "MDRW2100047712.1.28",
          "form": "근데 섭섭한건 니 사정이다 싶은게...\r\n마음 다 식은 것 같아요 ㅋㅋㅋㅋ",
          "original_form": "근데 섭섭한건 니 사정이다 싶은게...\r\n마음 다 식은 것 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:02"
        },
        {
          "id": "MDRW2100047712.1.29",
          "form": "왕복 8시간이면 하루 다 날리는데..",
          "original_form": "왕복 8시간이면 하루 다 날리는데..",
          "speaker_id": "2",
          "time": "20210827 20:02"
        }
      ]
    }
