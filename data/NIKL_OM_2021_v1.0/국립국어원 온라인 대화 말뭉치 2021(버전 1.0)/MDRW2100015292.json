      "utterance": [
        {
          "id": "MDRW2100015292.1.1",
          "form": "재택근무 일자리가 주제야",
          "original_form": "재택근무 일자리가 주제야",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015292.1.2",
          "form": "요즘 코로나 때문에 재택근무가 많아졌지",
          "original_form": "요즘 코로나 때문에 재택근무가 많아졌지",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015292.1.3",
          "form": "맞아 그래서 재택근무로 아예 돌린곳도 있고",
          "original_form": "맞아 그래서 재택근무로 아예 돌린곳도 있고",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015292.1.4",
          "form": "그치 그런곳도 많지",
          "original_form": "그치 그런곳도 많지",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015292.1.5",
          "form": "아니면 번갈아가면서 재택근무를 하는 곳도 있더라고",
          "original_form": "아니면 번갈아가면서 재택근무를 하는 곳도 있더라고",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015292.1.6",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015292.1.7",
          "form": "친구도 재택근무 하고 있는데",
          "original_form": "친구도 재택근무 하고 있는데",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015292.1.8",
          "form": "그게 훨씬 편하다고 하더라고",
          "original_form": "그게 훨씬 편하다고 하더라고",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015292.1.9",
          "form": "출퇴근이 없어지는게 큰 장점이지",
          "original_form": "출퇴근이 없어지는게 큰 장점이지",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015292.1.10",
          "form": "맞아 너도 그렇게 생가해",
          "original_form": "맞아 너도 그렇게 생가해",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015292.1.11",
          "form": "아니 나도 그렇게 생각해",
          "original_form": "아니 나도 그렇게 생각해",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015292.1.12",
          "form": "근데 회의는 어떻게 하려나",
          "original_form": "근데 회의는 어떻게 하려나",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015292.1.13",
          "form": "아마 실시간 화상 회의로 하겠지",
          "original_form": "아마 실시간 화상 회의로 하겠지",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015292.1.14",
          "form": "그럼 집중력은 좀 떨어지겠다",
          "original_form": "그럼 집중력은 좀 떨어지겠다",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015292.1.15",
          "form": "그게 좀 단점이긴 하지",
          "original_form": "그게 좀 단점이긴 하지",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015292.1.16",
          "form": "소통도 힘들고",
          "original_form": "소통도 힘들고",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015292.1.17",
          "form": "소통의 부재가 제일 큰 문제겠다",
          "original_form": "소통의 부재가 제일 큰 문제겠다",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015292.1.18",
          "form": "알바도 재택근무가 있었으면 좋겠다",
          "original_form": "알바도 재택근무가 있었으면 좋겠다",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015292.1.19",
          "form": "그런 알바가 있으면 다들 하려고 난리겠지",
          "original_form": "그런 알바가 있으면 다들 하려고 난리겠지",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015292.1.20",
          "form": "나도 제일 먼저 전화할 것 같아 ㅋㅋ",
          "original_form": "나도 제일 먼저 전화할 것 같아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015292.1.21",
          "form": "코로나 때문에 참 변하는게 많네",
          "original_form": "코로나 때문에 참 변하는게 많네",
          "speaker_id": "1",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015292.1.22",
          "form": "맞아 코로나가 환경도 바꾸고",
          "original_form": "맞아 코로나가 환경도 바꾸고",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015292.1.23",
          "form": "경제도 바꾸고",
          "original_form": "경제도 바꾸고",
          "speaker_id": "1",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015292.1.24",
          "form": "참 많이 달라진 것 같아",
          "original_form": "참 많이 달라진 것 같아",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100015292.1.25",
          "form": "흠",
          "original_form": "흠",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015292.1.26",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015292.1.27",
          "form": "그래도 재택근무는 장점도 있고 단점도 있는 것 같아",
          "original_form": "그래도 재택근무는 장점도 있고 단점도 있는 것 같아",
          "speaker_id": "1",
          "time": "20210825 22:39"
        }
      ]
    }
