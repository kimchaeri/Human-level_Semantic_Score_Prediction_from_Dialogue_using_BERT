      "utterance": [
        {
          "id": "MDRW2100019439.1.1",
          "form": "왜 갑자기???",
          "original_form": "왜 갑자기???",
          "speaker_id": "2",
          "time": "20210827 19:20"
        },
        {
          "id": "MDRW2100019439.1.2",
          "form": "아까 실내화 작다고 했잖아 ㅎㅎ",
          "original_form": "아까 실내화 작다고 했잖아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 19:21"
        },
        {
          "id": "MDRW2100019439.1.3",
          "form": "아 그래서 재는구나",
          "original_form": "아 그래서 재는구나",
          "speaker_id": "2",
          "time": "20210827 19:21"
        },
        {
          "id": "MDRW2100019439.1.4",
          "form": "ㅇㅇ 또 작은 거 사면 안되니까",
          "original_form": "ㅇㅇ 또 작은 거 사면 안되니까",
          "speaker_id": "1",
          "time": "20210827 19:21"
        },
        {
          "id": "MDRW2100019439.1.5",
          "form": "당장 다음주에 신어야 할지도 모르니까 빨리 재 ㅎㅎ",
          "original_form": "당장 다음주에 신어야 할지도 모르니까 빨리 재 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:22"
        },
        {
          "id": "MDRW2100019439.1.6",
          "form": "글쎄 다음 주엔 아직 나오라는 얘긴 없던데 ㅋ",
          "original_form": "글쎄 다음 주엔 아직 나오라는 얘긴 없던데 ㅋ",
          "speaker_id": "1",
          "time": "20210827 19:23"
        },
        {
          "id": "MDRW2100019439.1.7",
          "form": "곧 4단계 풀리면 나갈테니까",
          "original_form": "곧 4단계 풀리면 나갈테니까",
          "speaker_id": "2",
          "time": "20210827 19:23"
        },
        {
          "id": "MDRW2100019439.1.8",
          "form": "지마켓에서 사야겠네~ 250 사이즈 사면 될 것 같아 ㅎㅎ",
          "original_form": "지마켓에서 사야겠네~ 250 사이즈 사면 될 것 같아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 19:23"
        },
        {
          "id": "MDRW2100019439.1.9",
          "form": "응 잘 골라서 사",
          "original_form": "응 잘 골라서 사",
          "speaker_id": "2",
          "time": "20210827 19:24"
        },
        {
          "id": "MDRW2100019439.1.10",
          "form": "이젠 내 발보다 더 크네 ㅋ",
          "original_form": "이젠 내 발보다 더 크네 ㅋ",
          "speaker_id": "1",
          "time": "20210827 19:24"
        },
        {
          "id": "MDRW2100019439.1.11",
          "form": "엄마보다 작으면 안되지",
          "original_form": "엄마보다 작으면 안되지",
          "speaker_id": "2",
          "time": "20210827 19:24"
        },
        {
          "id": "MDRW2100019439.1.12",
          "form": "ㅋㅋ 그건 그래",
          "original_form": "ㅋㅋ 그건 그래",
          "speaker_id": "1",
          "time": "20210827 19:25"
        },
        {
          "id": "MDRW2100019439.1.13",
          "form": "실내화 모양 없는거 사야지",
          "original_form": "실내화 모양 없는거 사야지",
          "speaker_id": "2",
          "time": "20210827 19:25"
        },
        {
          "id": "MDRW2100019439.1.14",
