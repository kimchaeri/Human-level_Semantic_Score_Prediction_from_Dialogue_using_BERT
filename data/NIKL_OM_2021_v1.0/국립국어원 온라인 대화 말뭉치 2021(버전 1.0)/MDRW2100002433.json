      "utterance": [
        {
          "id": "MDRW2100002433.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210820 12:13"
        },
        {
          "id": "MDRW2100002433.1.2",
          "form": "안녕하세요~~",
          "original_form": "안녕하세요~~",
          "speaker_id": "2",
          "time": "20210820 12:14"
        },
        {
          "id": "MDRW2100002433.1.3",
          "form": "네~! 아르바이트해보신적있으신가요?",
          "original_form": "네~! 아르바이트해보신적있으신가요?",
          "speaker_id": "1",
          "time": "20210820 12:15"
        },
        {
          "id": "MDRW2100002433.1.4",
          "form": "넵~~ 햄버거가게에서 아르바이트해봤어요 좋은경험이었죠",
          "original_form": "넵~~ 햄버거가게에서 아르바이트해봤어요 좋은경험이었죠",
          "speaker_id": "2",
          "time": "20210820 12:18"
        },
        {
          "id": "MDRW2100002433.1.5",
          "form": "오 정말좋네요ㅎㅎㅎ",
          "original_form": "오 정말좋네요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 12:20"
        },
        {
          "id": "MDRW2100002433.1.6",
          "form": "저는 빵집에서 해봤답니다",
          "original_form": "저는 빵집에서 해봤답니다",
          "speaker_id": "1",
          "time": "20210820 12:20"
        },
        {
          "id": "MDRW2100002433.1.7",
          "form": "카페 아르바이트 해보고 싶었는데 혹시 커피도 같이 하셨나요?",
          "original_form": "카페 아르바이트 해보고 싶었는데 혹시 커피도 같이 하셨나요?",
          "speaker_id": "2",
          "time": "20210820 12:34"
        },
        {
          "id": "MDRW2100002433.1.8",
          "form": "저는 작은곳이어서 커피는안했었어요ㅋㅋㅋ\r\n그리고어릴때라",
          "original_form": "저는 작은곳이어서 커피는안했었어요ㅋㅋㅋ\r\n그리고어릴때라",
          "speaker_id": "1",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002433.1.9",
          "form": "빵집에서 커피안하는곳이 많았거든요!",
          "original_form": "빵집에서 커피안하는곳이 많았거든요!",
          "speaker_id": "1",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002433.1.10",
          "form": "아하 빵집은 가격을 외어야되서 초반엔 많이 헤깔리셨겟어요",
          "original_form": "아하 빵집은 가격을 외어야되서 초반엔 많이 헤깔리셨겟어요",
          "speaker_id": "2",
          "time": "20210820 13:20"
        },
        {
          "id": "MDRW2100002433.1.11",
          "form": "가격이 가격표가",
          "original_form": "가격이 가격표가",
          "speaker_id": "1",
          "time": "20210820 13:22"
        },
        {
          "id": "MDRW2100002433.1.12",
          "form": "붙어있답니다ㅎㅎㅎ!",
          "original_form": "붙어있답니다ㅎㅎㅎ!",
          "speaker_id": "1",
          "time": "20210820 13:22"
        },
        {
          "id": "MDRW2100002433.1.13",
          "form": "아 파리빠게트 나 프렌차이즈 가게가면 항상 외우기 어렵겠다 생각했는데 그건 좋았겠네용",
          "original_form": "아 파리빠게트 나 프렌차이즈 가게가면 항상 외우기 어렵겠다 생각했는데 그건 좋았겠네용",
          "speaker_id": "2",
          "time": "20210820 17:18"
        },
        {
          "id": "MDRW2100002433.1.14",
          "form": "대신 이름을",
          "original_form": "대신 이름을",
          "speaker_id": "1",
          "time": "20210822 18:11"
        },
        {
          "id": "MDRW2100002433.1.15",
          "form": "외워야해요ㅠㅠ",
          "original_form": "외워야해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 18:11"
        },
        {
          "id": "MDRW2100002433.1.16",
          "form": "이거 있어요~ 하면 찾아야되내요",
          "original_form": "이거 있어요~ 하면 찾아야되내요",
          "speaker_id": "2",
          "time": "20210822 18:31"
        },
        {
          "id": "MDRW2100002433.1.17",
          "form": "손님이",
          "original_form": "손님이",
          "speaker_id": "1",
          "time": "20210823 15:54"
        },
        {
          "id": "MDRW2100002433.1.18",
          "form": "빵을 가져오면 그게뭔지 이름을 눌러서",
          "original_form": "빵을 가져오면 그게뭔지 이름을 눌러서",
          "speaker_id": "1",
          "time": "20210823 15:55"
        },
        {
          "id": "MDRW2100002433.1.19",
          "form": "계산해야한답니다ㅋㅋㅋ",
          "original_form": "계산해야한답니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:55"
        },
        {
          "id": "MDRW2100002433.1.20",
          "form": "저는 엄청 실수할꺼같아요",
          "original_form": "저는 엄청 실수할꺼같아요",
          "speaker_id": "2",
