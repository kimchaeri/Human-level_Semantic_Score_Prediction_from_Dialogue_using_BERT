        {
          "id": "MDRW2100000612.1.1",
          "form": "가사노동ㅠ",
          "original_form": "가사노동ㅠ",
          "speaker_id": "2",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000612.1.2",
          "form": "ㅠㅠㅠ 해도해도 끝이 없는 집안일..",
          "original_form": "ㅠㅠㅠ 해도해도 끝이 없는 집안일..",
          "speaker_id": "1",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000612.1.3",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "2",
          "time": "20210819 17:09"
        },
        {
          "id": "MDRW2100000612.1.4",
          "form": "돈도안주는데",
          "original_form": "돈도안주는데",
          "speaker_id": "2",
          "time": "20210819 17:10"
        },
        {
          "id": "MDRW2100000612.1.5",
          "form": "힘들기만하고",
          "original_form": "힘들기만하고",
          "speaker_id": "2",
          "time": "20210819 17:10"
        },
        {
          "id": "MDRW2100000612.1.6",
          "form": "마자요 ㅠ 돈 좀 많이 받으면서 해야하는 노동 ,ㅠㅠ",
          "original_form": "마자요 ㅠ 돈 좀 많이 받으면서 해야하는 노동 ,ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 17:10"
        },
        {
          "id": "MDRW2100000612.1.7",
          "form": "설거지 특히 넘 싫어용..",
          "original_form": "설거지 특히 넘 싫어용..",
          "speaker_id": "1",
          "time": "20210819 17:11"
        },
        {
          "id": "MDRW2100000612.1.8",
          "form": "식기세척기사고싶어요",
          "original_form": "식기세척기사고싶어요",
          "speaker_id": "2",
          "time": "20210819 17:11"
        },
        {
          "id": "MDRW2100000612.1.9",
          "form": "저두요~ 넘 편하고좋을것같아요",
          "original_form": "저두요~ 넘 편하고좋을것같아요",
          "speaker_id": "1",
          "time": "20210819 17:16"
        },
        {
          "id": "MDRW2100000612.1.10",
          "form": "비싼게단점",
          "original_form": "비싼게단점",
          "speaker_id": "2",
          "time": "20210819 17:20"
        },
        {
          "id": "MDRW2100000612.1.11",
          "form": "그쵸 ,, 집에 설치할 자리도 없는..",
          "original_form": "그쵸 ,, 집에 설치할 자리도 없는..",
          "speaker_id": "1",
          "time": "20210819 17:20"
        },
        {
          "id": "MDRW2100000612.1.12",
          "form": "누가 대신 해주면좋겠어용 ㅠㅋㅋ",
          "original_form": "누가 대신 해주면좋겠어용 ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000612.1.13",
          "form": "누가대신ㅎㅎㅎㅎ",
          "original_form": "누가대신ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000612.1.14",
          "form": "저희집도좁아서",
          "original_form": "저희집도좁아서",
          "speaker_id": "2",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000612.1.15",
          "form": "흡 ㅠㅋㅋ 근데 또 넓은데 살면 그만큼 가사노동도 많아지겠져 ㅠ?",
          "original_form": "흡 ㅠㅋㅋ 근데 또 넓은데 살면 그만큼 가사노동도 많아지겠져 ㅠ?",
          "speaker_id": "1",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000612.1.16",
