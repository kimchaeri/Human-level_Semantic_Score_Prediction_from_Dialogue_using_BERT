      "utterance": [
        {
          "id": "MDRW2100023740.1.1",
          "form": "앗 보면 재밌게 보는데",
          "original_form": "앗 보면 재밌게 보는데",
          "speaker_id": "2",
          "time": "20210830 16:15"
        },
        {
          "id": "MDRW2100023740.1.2",
          "form": "시리즈를 따로 챙겨보진않았어요!",
          "original_form": "시리즈를 따로 챙겨보진않았어요!",
          "speaker_id": "2",
          "time": "20210830 16:16"
        },
        {
          "id": "MDRW2100023740.1.3",
          "form": "저는 이번에 새로운 스파이더맨 예고편이 공개되서 기대하고 있어요",
          "original_form": "저는 이번에 새로운 스파이더맨 예고편이 공개되서 기대하고 있어요",
          "speaker_id": "1",
          "time": "20210830 16:16"
        },
        {
          "id": "MDRW2100023740.1.4",
          "form": "시리즈 다 보셨어요?",
          "original_form": "시리즈 다 보셨어요?",
          "speaker_id": "2",
          "time": "20210830 16:17"
        },
        {
          "id": "MDRW2100023740.1.5",
          "form": "네 스파이더맨은 좋아해서 예전 소니가 만든 것도 다 봤어요",
          "original_form": "네 스파이더맨은 좋아해서 예전 소니가 만든 것도 다 봤어요",
          "speaker_id": "1",
          "time": "20210830 16:19"
        },
        {
          "id": "MDRW2100023740.1.6",
          "form": "오 스파이더맨 좋아하시는구나",
          "original_form": "오 스파이더맨 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210830 16:20"
        },
        {
          "id": "MDRW2100023740.1.7",
          "form": "네 스파이디 중에서 앤드류 스파이디 좋아했는데 주인공 바껴서 아쉬웠어요ㅠㅠ",
          "original_form": "네 스파이디 중에서 앤드류 스파이디 좋아했는데 주인공 바껴서 아쉬웠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 16:22"
        },
        {
          "id": "MDRW2100023740.1.8",
          "form": "그 유명한 스파이더맨 키스신이 앤드류인건가요?",
          "original_form": "그 유명한 스파이더맨 키스신이 앤드류인건가요?",
          "speaker_id": "2",
          "time": "20210830 16:23"
        },
        {
          "id": "MDRW2100023740.1.9",
          "form": "저는 톰홀랜드로 바뀌고나서 봤네요ㅠㅠ",
          "original_form": "저는 톰홀랜드로 바뀌고나서 봤네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 16:24"
        },
        {
          "id": "MDRW2100023740.1.10",
          "form": "그거는 첫번째 꺼일거에요 앤드류는 두 번째 어메이징 스파이더맨에 나와요",
          "original_form": "그거는 첫번째 꺼일거에요 앤드류는 두 번째 어메이징 스파이더맨에 나와요",
          "speaker_id": "1",
          "time": "20210830 16:24"
        },
        {
          "id": "MDRW2100023740.1.11",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210830 16:24"
        },
        {
          "id": "MDRW2100023740.1.12",
          "form": "외국은 보통 시리즈물이 많은데",
          "original_form": "외국은 보통 시리즈물이 많은데",
          "speaker_id": "2",
          "time": "20210830 16:25"
        },
        {
          "id": "MDRW2100023740.1.13",
          "form": "저는 시리즈물을 잘 못챙겨봐서ㅠㅠ",
          "original_form": "저는 시리즈물을 잘 못챙겨봐서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 16:25"
        },
        {
          "id": "MDRW2100023740.1.14",
          "form": "해리포터도 아직 못봤어요",
          "original_form": "해리포터도 아직 못봤어요",
          "speaker_id": "2",
          "time": "20210830 16:25"
        },
        {
          "id": "MDRW2100023740.1.15",
          "form": "구런건 좀 있죠 앞에거 봐여될 것 같고 근데 챙기긴 귀찮고 ㅋㅋ",
          "original_form": "구런건 좀 있죠 앞에거 봐여될 것 같고 근데 챙기긴 귀찮고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:37"
        },
        {
          "id": "MDRW2100023740.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210830 16:40"
        },
        {
          "id": "MDRW2100023740.1.17",
          "form": "뭔가 숙제처럼 느껴져서..",
          "original_form": "뭔가 숙제처럼 느껴져서..",
          "speaker_id": "2",
          "time": "20210830 16:40"
        },
        {
          "id": "MDRW2100023740.1.18",
          "form": "메이즈러너인가 그건 1 너무 재밌게봤는데 2부터 못봤어요ㅠㅠ",
          "original_form": "메이즈러너인가 그건 1 너무 재밌게봤는데 2부터 못봤어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 16:41"
        },
        {
          "id": "MDRW2100023740.1.19",
          "form": "그건 1편이 젤 재밌어요 ㅋㅋㅋ",
          "original_form": "그건 1편이 젤 재밌어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:42"
        },
        {
          "id": "MDRW2100023740.1.20",
          "form": "저도 재밌게 보고 원작도 봤는데 원작이 별로...였어요...",
          "original_form": "저도 재밌게 보고 원작도 봤는데 원작이 별로...였어요...",
          "speaker_id": "1",
          "time": "20210830 16:42"
        },
        {
          "id": "MDRW2100023740.1.21",
          "form": "아정말요?",
          "original_form": "아정말요?",
          "speaker_id": "2",
          "time": "20210830 16:46"
        },
        {
          "id": "MDRW2100023740.1.22",
          "form": "그럼 그냥 1편만 품고 사는걸로...",
          "original_form": "그럼 그냥 1편만 품고 사는걸로...",
          "speaker_id": "2",
          "time": "20210830 16:46"
        },
        {
          "id": "MDRW2100023740.1.23",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 16:46"
        },
        {
          "id": "MDRW2100023740.1.24",
          "form": "그렇게 하세요 ㅎㅎ",
          "original_form": "그렇게 하세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 16:47"
        }
      ]
    }
