      "utterance": [
        {
          "id": "MDRW2100004297.1.1",
          "form": "집에서 재택으로 일이 가능하나",
          "original_form": "집에서 재택으로 일이 가능하나",
          "speaker_id": "1",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004297.1.2",
          "form": "말로만 재택..",
          "original_form": "말로만 재택..",
          "speaker_id": "1",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004297.1.3",
          "form": "절대 불가능이요",
          "original_form": "절대 불가능이요",
          "speaker_id": "2",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004297.1.4",
          "form": "디자인쪽은 가능할지도..",
          "original_form": "디자인쪽은 가능할지도..",
          "speaker_id": "2",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004297.1.5",
          "form": "그런가 근데 요즘 마이하더라",
          "original_form": "그런가 근데 요즘 마이하더라",
          "speaker_id": "1",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004297.1.6",
          "form": "저희 센터도 이제 전자결재  도입했으니 가능할지도 몰라요",
          "original_form": "저희 센터도 이제 전자결재  도입했으니 가능할지도 몰라요",
          "speaker_id": "2",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004297.1.7",
          "form": "재택근무 하고싶네요",
          "original_form": "재택근무 하고싶네요",
          "speaker_id": "2",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004297.1.8",
          "form": "재택근무가 가능한 날이 올까",
          "original_form": "재택근무가 가능한 날이 올까",
          "speaker_id": "1",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004297.1.9",
          "form": "앞으로 많이 하겠죠? 코로나ㅜㅜ",
          "original_form": "앞으로 많이 하겠죠? 코로나ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100004297.1.10",
          "form": "아니 코로나 끌나면 안할듯",
          "original_form": "아니 코로나 끌나면 안할듯",
          "speaker_id": "1",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004297.1.11",
          "form": "코로나 안끝나니 계속 재택근무 할듯여",
          "original_form": "코로나 안끝나니 계속 재택근무 할듯여",
          "speaker_id": "2",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004297.1.12",
          "form": "아하 재택이 트랜드?",
          "original_form": "아하 재택이 트랜드?",
          "speaker_id": "1",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004297.1.13",
          "form": "주변에 재택근무 하는곳 못봤어요",
          "original_form": "주변에 재택근무 하는곳 못봤어요",
          "speaker_id": "2",
          "time": "20210821 17:24"
        },
        {
          "id": "MDRW2100004297.1.14",
          "form": "아무래도 지방이라..",
          "original_form": "아무래도 지방이라..",
          "speaker_id": "1",
          "time": "20210821 17:24"
        },
        {
          "id": "MDRW2100004297.1.15",
          "form": "언젠가 집에서 일하는 날이 오겠죠",
          "original_form": "언젠가 집에서 일하는 날이 오겠죠",
          "speaker_id": "2",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004297.1.16",
