        {
          "id": "MDRW2100035819.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "1",
          "time": "20210831 00:30"
        },
        {
          "id": "MDRW2100035819.1.2",
          "form": "에버랜드 갈까",
          "original_form": "에버랜드 갈까",
          "speaker_id": "2",
          "time": "20210831 00:30"
        },
        {
          "id": "MDRW2100035819.1.3",
          "form": "놀이기구 타러?",
          "original_form": "놀이기구 타러?",
          "speaker_id": "1",
          "time": "20210831 00:31"
        },
        {
          "id": "MDRW2100035819.1.4",
          "form": "티익스프레스 좋아해?",
          "original_form": "티익스프레스 좋아해?",
          "speaker_id": "2",
          "time": "20210831 00:31"
        },
        {
          "id": "MDRW2100035819.1.5",
          "form": "좋아해 여러번 타고오자",
          "original_form": "좋아해 여러번 타고오자",
          "speaker_id": "1",
          "time": "20210831 00:31"
        },
        {
          "id": "MDRW2100035819.1.6",
          "form": "동물도 보고자",
          "original_form": "동물도 보고자",
          "speaker_id": "2",
          "time": "20210831 00:32"
        },
        {
          "id": "MDRW2100035819.1.7",
          "form": "사파리 월드 꼭 가야지",
          "original_form": "사파리 월드 꼭 가야지",
          "speaker_id": "1",
          "time": "20210831 00:32"
        },
        {
          "id": "MDRW2100035819.1.8",
          "form": "거기에는  호랑이가 있지",
          "original_form": "거기에는  호랑이가 있지",
          "speaker_id": "2",
          "time": "20210831 00:32"
        },
        {
          "id": "MDRW2100035819.1.9",
          "form": "백호 백호 보는 것도 좋아",
          "original_form": "백호 백호 보는 것도 좋아",
          "speaker_id": "1",
          "time": "20210831 00:32"
        },
        {
          "id": "MDRW2100035819.1.10",
          "form": "한국 호랑이도 있어",
          "original_form": "한국 호랑이도 있어",
          "speaker_id": "2",
          "time": "20210831 00:32"
        },
        {
          "id": "MDRW2100035819.1.11",
          "form": "기린 도 보러 가자",
          "original_form": "기린 도 보러 가자",
          "speaker_id": "1",
          "time": "20210831 00:33"
        },
        {
          "id": "MDRW2100035819.1.12",
          "form": "기린한테 먹이 주는 거 재밌어",
          "original_form": "기린한테 먹이 주는 거 재밌어",
          "speaker_id": "2",
          "time": "20210831 00:33"
        },
        {
          "id": "MDRW2100035819.1.13",
          "form": "판다 도 보러 가자",
          "original_form": "판다 도 보러 가자",
          "speaker_id": "1",
          "time": "20210831 00:33"
        },
        {
          "id": "MDRW2100035819.1.14",
          "form": "판단은 인기가 너무 많아",
          "original_form": "판단은 인기가 너무 많아",
          "speaker_id": "2",
          "time": "20210831 00:33"
        },
        {
          "id": "MDRW2100035819.1.15",
          "form": "아기 판다가 귀여우니까",
          "original_form": "아기 판다가 귀여우니까",
          "speaker_id": "1",
          "time": "20210831 00:34"
        },
        {
          "id": "MDRW2100035819.1.16",
          "form": "음 팬더 뭐야 팬더 모양 머리띠도 하고 다니자",
          "original_form": "음 팬더 뭐야 팬더 모양 머리띠도 하고 다니자",
          "speaker_id": "2",
          "time": "20210831 00:35"
        },
        {
          "id": "MDRW2100035819.1.17",
          "form": "팝콘 통도 살래 판다 모양",
          "original_form": "팝콘 통도 살래 판다 모양",
          "speaker_id": "1",
          "time": "20210831 00:35"
        }
