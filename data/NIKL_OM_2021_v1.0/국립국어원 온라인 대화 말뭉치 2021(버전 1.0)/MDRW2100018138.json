        },
        {
          "id": "MDRW2100018138.1.2",
          "form": "스위트홈",
          "original_form": "스위트홈",
          "speaker_id": "2",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018138.1.3",
          "form": "스위트홈 그렇게 재미있었어?",
          "original_form": "스위트홈 그렇게 재미있었어?",
          "speaker_id": "1",
          "time": "20210826 22:50"
        },
        {
          "id": "MDRW2100018138.1.4",
          "form": "재밌었어",
          "original_form": "재밌었어",
          "speaker_id": "2",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100018138.1.5",
          "form": "그거 잔인한 거야 무서운거야?",
          "original_form": "그거 잔인한 거야 무서운거야?",
          "speaker_id": "1",
          "time": "20210826 22:52"
        },
        {
          "id": "MDRW2100018138.1.6",
          "form": "둘다 있는것 같아",
          "original_form": "둘다 있는것 같아",
          "speaker_id": "2",
          "time": "20210826 22:52"
        },
        {
          "id": "MDRW2100018138.1.7",
          "form": "아 잔인하고 무서워?",
          "original_form": "아 잔인하고 무서워?",
          "speaker_id": "1",
          "time": "20210826 22:52"
        },
        {
          "id": "MDRW2100018138.1.8",
          "form": "응 약간",
          "original_form": "응 약간",
          "speaker_id": "2",
          "time": "20210826 22:53"
        },
        {
          "id": "MDRW2100018138.1.9",
          "form": "원래 무서운 거 별로 안좋아하잖아",
          "original_form": "원래 무서운 거 별로 안좋아하잖아",
          "speaker_id": "1",
          "time": "20210826 22:54"
        },
        {
          "id": "MDRW2100018138.1.10",
          "form": "근데 무서워도 재밌어서",
          "original_form": "근데 무서워도 재밌어서",
          "speaker_id": "2",
          "time": "20210826 22:54"
        },
        {
          "id": "MDRW2100018138.1.11",
          "form": "무슨 내용이라 그랬지?",
          "original_form": "무슨 내용이라 그랬지?",
          "speaker_id": "1",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018138.1.12",
          "form": "부산행처럼 좀비물인데  건물에 갇혀서 구조를 기다리는거야",
          "original_form": "부산행처럼 좀비물인데  건물에 갇혀서 구조를 기다리는거야",
          "speaker_id": "2",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018138.1.13",
          "form": "난 좀비가 아니라 괴물인 줄 알았어",
          "original_form": "난 좀비가 아니라 괴물인 줄 알았어",
          "speaker_id": "1",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100018138.1.14",
          "form": "괴물도 말 되",
          "original_form": "괴물도 말 되",
          "speaker_id": "2",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100018138.1.15",
          "form": "괴물이기도 해?",
          "original_form": "괴물이기도 해?",
          "speaker_id": "1",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100018138.1.16",
          "form": "좀비인데 괴물인 거야?",
          "original_form": "좀비인데 괴물인 거야?",
          "speaker_id": "1",
          "time": "20210826 22:58"
        }
