      "utterance": [
        {
          "id": "MDRW2100031391.1.1",
          "form": "먹어야지",
          "original_form": "먹어야지",
          "speaker_id": "2",
          "time": "20210904 21:09"
        },
        {
          "id": "MDRW2100031391.1.2",
          "form": "근데 아저씨 나이트라 저녁에 밥먹여보내야 되니까 그렇지",
          "original_form": "근데 아저씨 나이트라 저녁에 밥먹여보내야 되니까 그렇지",
          "speaker_id": "2",
          "time": "20210904 21:09"
        },
        {
          "id": "MDRW2100031391.1.3",
          "form": "그럼 아저씨 나이트 끝나고 먹어?",
          "original_form": "그럼 아저씨 나이트 끝나고 먹어?",
          "speaker_id": "1",
          "time": "20210904 21:09"
        },
        {
          "id": "MDRW2100031391.1.4",
          "form": "우리끼리 먹자",
          "original_form": "우리끼리 먹자",
          "speaker_id": "2",
          "time": "20210904 21:09"
        },
        {
          "id": "MDRW2100031391.1.5",
          "form": "엄마 먹튀는 안돼ㅡㅡ",
          "original_form": "엄마 먹튀는 안돼ㅡㅡ",
          "speaker_id": "1",
          "time": "20210904 21:10"
        },
        {
          "id": "MDRW2100031391.1.6",
          "form": "지금 삼일째 안마만받고",
          "original_form": "지금 삼일째 안마만받고",
          "speaker_id": "1",
          "time": "20210904 21:10"
        },
        {
          "id": "MDRW2100031391.1.7",
          "form": "치킨은 안 사주고있잖아",
          "original_form": "치킨은 안 사주고있잖아",
          "speaker_id": "1",
          "time": "20210904 21:10"
        },
        {
          "id": "MDRW2100031391.1.8",
          "form": "먹튀 안한다",
          "original_form": "먹튀 안한다",
          "speaker_id": "2",
          "time": "20210904 21:10"
        },
        {
          "id": "MDRW2100031391.1.9",
          "form": "담주에 사줄께",
          "original_form": "담주에 사줄께",
          "speaker_id": "2",
          "time": "20210904 21:10"
        },
        {
          "id": "MDRW2100031391.1.10",
          "form": "기다리다 목 빠지겠다",
          "original_form": "기다리다 목 빠지겠다",
          "speaker_id": "1",
          "time": "20210904 21:10"
        },
        {
          "id": "MDRW2100031391.1.11",
          "form": "엄마 안마하기도 엄청 힘들다고",
          "original_form": "엄마 안마하기도 엄청 힘들다고",
          "speaker_id": "1",
          "time": "20210904 21:11"
        },
        {
          "id": "MDRW2100031391.1.12",
          "form": "알았어 담주에 꼭 먹자",
          "original_form": "알았어 담주에 꼭 먹자",
          "speaker_id": "2",
          "time": "20210904 21:11"
        },
        {
          "id": "MDRW2100031391.1.13",
          "form": "나 바베큐먹을거야 양념",
          "original_form": "나 바베큐먹을거야 양념",
          "speaker_id": "1",
          "time": "20210904 21:11"
        },
        {
          "id": "MDRW2100031391.1.14",
          "form": "나 바베큐 안먹는데",
          "original_form": "나 바베큐 안먹는데",
          "speaker_id": "2",
          "time": "20210904 21:11"
        },
        {
          "id": "MDRW2100031391.1.15",
          "form": "아 그러니까 후라이드도 하나 시킴되지!",
          "original_form": "아 그러니까 후라이드도 하나 시킴되지!",
          "speaker_id": "1",
          "time": "20210904 21:12"
        },
        {
          "id": "MDRW2100031391.1.16",
          "form": "니가 웬수다",
          "original_form": "니가 웬수다",
          "speaker_id": "2",
          "time": "20210904 21:12"
        },
        {
          "id": "MDRW2100031391.1.17",
          "form": "내가 왜 웬수야 엄마가 지금 먹튀직전인데",
          "original_form": "내가 왜 웬수야 엄마가 지금 먹튀직전인데",
          "speaker_id": "1",
          "time": "20210904 21:12"
        },
        {
          "id": "MDRW2100031391.1.18",
          "form": "엄마가 웬수지",
          "original_form": "엄마가 웬수지",
          "speaker_id": "1",
          "time": "20210904 21:12"
        },
        {
          "id": "MDRW2100031391.1.19",
          "form": "먹튀 안한다고",
          "original_form": "먹튀 안한다고",
          "speaker_id": "2",
          "time": "20210904 21:13"
        },
        {
          "id": "MDRW2100031391.1.20",
          "form": "치킨 어디서",
          "original_form": "치킨 어디서",
          "speaker_id": "2",
          "time": "20210904 21:14"
        },
        {
          "id": "MDRW2100031391.1.21",
          "form": "주문할지",
          "original_form": "주문할지",
          "speaker_id": "2",
          "time": "20210904 21:14"
        },
        {
          "id": "MDRW2100031391.1.22",
          "form": "골라나",
          "original_form": "골라나",
          "speaker_id": "2",
          "time": "20210904 21:14"
        },
        {
          "id": "MDRW2100031391.1.23",
          "form": "놔라",
          "original_form": "놔라",
          "speaker_id": "2",
          "time": "20210904 21:14"
        },
        {
          "id": "MDRW2100031391.1.24",
          "form": "진작에 다 골라 놨다...",
          "original_form": "진작에 다 골라 놨다...",
          "speaker_id": "1",
          "time": "20210904 21:14"
        }
