        },
        {
          "id": "MDRW2100037695.1.2",
          "form": "안녕하세요.",
          "original_form": "안녕하세요.",
          "speaker_id": "1",
          "time": "20210901 16:44"
        },
        {
          "id": "MDRW2100037695.1.3",
          "form": "감자로 또 뭐 만드실 예정인가요?",
          "original_form": "감자로 또 뭐 만드실 예정인가요?",
          "speaker_id": "2",
          "time": "20210901 16:54"
        },
        {
          "id": "MDRW2100037695.1.4",
          "form": "저는 감자튀김할거에요.",
          "original_form": "저는 감자튀김할거에요.",
          "speaker_id": "1",
          "time": "20210901 17:51"
        },
        {
          "id": "MDRW2100037695.1.5",
          "form": "감자튀김 맛있겠어요!",
          "original_form": "감자튀김 맛있겠어요!",
          "speaker_id": "2",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100037695.1.6",
          "form": "너무 배고파요\r\n흑흑",
          "original_form": "너무 배고파요\r\n흑흑",
          "speaker_id": "1",
          "time": "20210901 18:12"
        },
        {
          "id": "MDRW2100037695.1.7",
          "form": "점심 드신게 다 소화되었나봐요",
          "original_form": "점심 드신게 다 소화되었나봐요",
          "speaker_id": "2",
          "time": "20210901 18:13"
        },
        {
          "id": "MDRW2100037695.1.8",
          "form": "두시에먹엇는데 벌써 다내려갓나봐요",
          "original_form": "두시에먹엇는데 벌써 다내려갓나봐요",
          "speaker_id": "1",
          "time": "20210901 18:14"
        },
        {
          "id": "MDRW2100037695.1.9",
          "form": "가볍게 먹은 날은 4시간 정도 지나면 소화가 어느정도 되더라구요",
          "original_form": "가볍게 먹은 날은 4시간 정도 지나면 소화가 어느정도 되더라구요",
          "speaker_id": "2",
          "time": "20210901 18:14"
        },
        {
          "id": "MDRW2100037695.1.10",
          "form": "라면을 반.개만 먹.는 느낌",
          "original_form": "라면을 반.개만 먹.는 느낌",
          "speaker_id": "1",
          "time": "20210901 19:28"
        },
        {
          "id": "MDRW2100037695.1.11",
          "form": "ㅎ",
          "original_form": "ㅎ",
          "speaker_id": "1",
          "time": "20210901 19:28"
        },
        {
          "id": "MDRW2100037695.1.12",
          "form": "뭔가 맛만 본 느낌? 중간에 딱 끊긴 느낌이군요",
          "original_form": "뭔가 맛만 본 느낌? 중간에 딱 끊긴 느낌이군요",
          "speaker_id": "2",
          "time": "20210901 20:10"
        },
        {
          "id": "MDRW2100037695.1.13",
          "form": "그래도 저녁은 맛잇게 돈까스로 끝냇어요 ㅋㅋ",
          "original_form": "그래도 저녁은 맛잇게 돈까스로 끝냇어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100037695.1.14",
          "form": "ㅇ오 돈까스 맛있는거 드셧네요",
          "original_form": "ㅇ오 돈까스 맛있는거 드셧네요",
          "speaker_id": "2",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100037695.1.15",
          "form": "네.. 제가만든 튀김옷이라 최고네요",
          "original_form": "네.. 제가만든 튀김옷이라 최고네요",
