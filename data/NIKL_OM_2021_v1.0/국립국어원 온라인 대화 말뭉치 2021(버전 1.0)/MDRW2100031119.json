      "utterance": [
        {
          "id": "MDRW2100031119.1.1",
          "form": "근데 그런애들만 그러고",
          "original_form": "근데 그런애들만 그러고",
          "speaker_id": "1",
          "time": "20210904 17:17"
        },
        {
          "id": "MDRW2100031119.1.2",
          "form": "또 아주 얄미운애들도 있어요",
          "original_form": "또 아주 얄미운애들도 있어요",
          "speaker_id": "1",
          "time": "20210904 17:17"
        },
        {
          "id": "MDRW2100031119.1.3",
          "form": "신입이라고 다 귀여운게 아니더라구요",
          "original_form": "신입이라고 다 귀여운게 아니더라구요",
          "speaker_id": "1",
          "time": "20210904 17:18"
        },
        {
          "id": "MDRW2100031119.1.4",
          "form": "그럼요!",
          "original_form": "그럼요!",
          "speaker_id": "2",
          "time": "20210904 17:18"
        },
        {
          "id": "MDRW2100031119.1.5",
          "form": "뺀질거리고 머리굴리는게 뻔히 보이는 친구들...ㅜ",
          "original_form": "뺀질거리고 머리굴리는게 뻔히 보이는 친구들...ㅜ",
          "speaker_id": "2",
          "time": "20210904 17:19"
        },
        {
          "id": "MDRW2100031119.1.6",
          "form": "그리고",
          "original_form": "그리고",
          "speaker_id": "1",
          "time": "20210904 17:19"
        },
        {
          "id": "MDRW2100031119.1.7",
          "form": "요즘은 신입 나이가 좀 있는사람들도 많이 오자나요",
          "original_form": "요즘은 신입 나이가 좀 있는사람들도 많이 오자나요",
          "speaker_id": "1",
          "time": "20210904 17:19"
        },
        {
          "id": "MDRW2100031119.1.8",
          "form": "뺀질거리는사람 많아요 ㅋㅋㅋ",
          "original_form": "뺀질거리는사람 많아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:19"
        },
        {
          "id": "MDRW2100031119.1.9",
          "form": "아! 맞아요 ㅋㅋ 커뮤로 사회생활 배운 사람들도..ㅜ",
          "original_form": "아! 맞아요 ㅋㅋ 커뮤로 사회생활 배운 사람들도..ㅜ",
          "speaker_id": "2",
          "time": "20210904 17:20"
        },
        {
          "id": "MDRW2100031119.1.10",
          "form": "내가 너무 꼰대가 싶은데",
          "original_form": "내가 너무 꼰대가 싶은데",
          "speaker_id": "2",
          "time": "20210904 17:20"
        },
        {
          "id": "MDRW2100031119.1.11",
          "form": "봉사하시나요ㅋㅋ",
          "original_form": "봉사하시나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:20"
        },
        {
          "id": "MDRW2100031119.1.12",
          "form": "봉사할 시간이 없어요",
          "original_form": "봉사할 시간이 없어요",
          "speaker_id": "1",
          "time": "20210904 17:21"
        },
        {
          "id": "MDRW2100031119.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:21"
        },
        {
          "id": "MDRW2100031119.1.14",
          "form": "ㅋㅋ 야근만 안해도 할텐데",
          "original_form": "ㅋㅋ 야근만 안해도 할텐데",
          "speaker_id": "2",
          "time": "20210904 17:21"
        },
        {
          "id": "MDRW2100031119.1.15",
          "form": "기부는 좀 해봤습니다 ㅋㅋㅋㅋㅋ",
          "original_form": "기부는 좀 해봤습니다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:21"
        },
        {
          "id": "MDRW2100031119.1.16",
          "form": "(연말정산에 유리한 기부)",
          "original_form": "(연말정산에 유리한 기부)",
          "speaker_id": "1",
          "time": "20210904 17:21"
        },
        {
          "id": "MDRW2100031119.1.17",
          "form": "저도 하다가 요즘은 그냥 후원을할까...",
          "original_form": "저도 하다가 요즘은 그냥 후원을할까...",
          "speaker_id": "2",
          "time": "20210904 17:21"
        },
        {
          "id": "MDRW2100031119.1.18",
          "form": "일대일이 있더라고요",
          "original_form": "일대일이 있더라고요",
          "speaker_id": "2",
          "time": "20210904 17:21"
        },
        {
          "id": "MDRW2100031119.1.19",
          "form": "그게 차라리 낫죠",
          "original_form": "그게 차라리 낫죠",
          "speaker_id": "1",
          "time": "20210904 17:22"
        },
        {
          "id": "MDRW2100031119.1.20",
          "form": "연말정산...ㅜㅋㅋ 미혼 넘 토해내니까 봉사하는거나 마찬가지",
          "original_form": "연말정산...ㅜㅋㅋ 미혼 넘 토해내니까 봉사하는거나 마찬가지",
          "speaker_id": "2",
          "time": "20210904 17:22"
        },
        {
          "id": "MDRW2100031119.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ 안쓰면 겁나 뜯어가자나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 안쓰면 겁나 뜯어가자나요",
          "speaker_id": "1",
          "time": "20210904 17:22"
        },
        {
          "id": "MDRW2100031119.1.22",
          "form": "기부라도 해서 나도 좋고 그사람들도 좋고",
          "original_form": "기부라도 해서 나도 좋고 그사람들도 좋고",
          "speaker_id": "1",
          "time": "20210904 17:23"
        },
        {
          "id": "MDRW2100031119.1.23",
          "form": "작년에 많은 봉사했습니다",
          "original_form": "작년에 많은 봉사했습니다",
          "speaker_id": "2",
          "time": "20210904 17:23"
        },
        {
          "id": "MDRW2100031119.1.24",
          "form": "오",
          "original_form": "오",
