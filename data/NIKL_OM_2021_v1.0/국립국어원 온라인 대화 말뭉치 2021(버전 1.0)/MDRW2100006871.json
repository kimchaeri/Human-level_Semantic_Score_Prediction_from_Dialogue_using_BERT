      "utterance": [
        {
          "id": "MDRW2100006871.1.1",
          "form": "추석엔 어디 가시나요",
          "original_form": "추석엔 어디 가시나요",
          "speaker_id": "1",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100006871.1.2",
          "form": "가까이 양가부모님이 계셔서 식사할것같아요",
          "original_form": "가까이 양가부모님이 계셔서 식사할것같아요",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100006871.1.3",
          "form": "오 좋으시네요",
          "original_form": "오 좋으시네요",
          "speaker_id": "1",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100006871.1.4",
          "form": "저는 집콕 예정이에요 넘  멀어서 명절은 엄두도 못내요",
          "original_form": "저는 집콕 예정이에요 넘  멀어서 명절은 엄두도 못내요",
          "speaker_id": "1",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100006871.1.5",
          "form": "아 멀리가셔야하나요",
          "original_form": "아 멀리가셔야하나요",
          "speaker_id": "2",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100006871.1.6",
          "form": "네 전라도 끝이라",
          "original_form": "네 전라도 끝이라",
          "speaker_id": "1",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100006871.1.7",
          "form": "막히면 답 없을것 같더라고요",
          "original_form": "막히면 답 없을것 같더라고요",
          "speaker_id": "1",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100006871.1.8",
          "form": "아 해남? 이런곳인가요",
          "original_form": "아 해남? 이런곳인가요",
          "speaker_id": "2",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100006871.1.9",
          "form": "완도에요",
          "original_form": "완도에요",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100006871.1.10",
          "form": "와 머네요 배 타야\n하죠?",
          "original_form": "와 머네요 배 타야\n하죠?",
          "speaker_id": "2",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100006871.1.11",
          "form": "아니요 완도까진 그냥가요",
          "original_form": "아니요 완도까진 그냥가요",
          "speaker_id": "1",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100006871.1.12",
          "form": "시가의 친척뵈러 청산도 갈때만 배타고요",
          "original_form": "시가의 친척뵈러 청산도 갈때만 배타고요",
          "speaker_id": "1",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100006871.1.13",
          "form": "아 다리가 놓아져있나보네요",
          "original_form": "아 다리가 놓아져있나보네요",
          "speaker_id": "2",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100006871.1.14",
          "form": "네 결혼전에 다행이 여러 대교가 공사했더라고요",
          "original_form": "네 결혼전에 다행이 여러 대교가 공사했더라고요",
          "speaker_id": "1",
          "time": "20210908 11:23"
        },
        {
          "id": "MDRW2100006871.1.15",
          "form": "그렇군요 그럼 몇시간 걸리나요",
          "original_form": "그렇군요 그럼 몇시간 걸리나요",
          "speaker_id": "2",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100006871.1.16",
          "form": "휴게소 한번도 안쉬면",
          "original_form": "휴게소 한번도 안쉬면",
          "speaker_id": "1",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100006871.1.17",
          "form": "5시간이요",
          "original_form": "5시간이요",
          "speaker_id": "1",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100006871.1.18",
          "form": "헉...정말 머네요",
          "original_form": "헉...정말 머네요",
          "speaker_id": "2",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100006871.1.19",
          "form": "네 ㅎㅎ 멀어요 진짜ㅜㅜ",
          "original_form": "네 ㅎㅎ 멀어요 진짜ㅜㅜ",
