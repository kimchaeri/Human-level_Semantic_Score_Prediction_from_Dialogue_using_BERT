      "utterance": [
        {
          "id": "MDRW2100010026.1.1",
          "form": "첫째 학교생활은 어떻대?",
          "original_form": "첫째 학교생활은 어떻대?",
          "speaker_id": "1",
          "time": "20210823 13:19"
        },
        {
          "id": "MDRW2100010026.1.2",
          "form": "잘 다니나? 학교 곧 개학인데 좋아하더나?ㅋ",
          "original_form": "잘 다니나? 학교 곧 개학인데 좋아하더나?ㅋ",
          "speaker_id": "1",
          "time": "20210823 13:19"
        },
        {
          "id": "MDRW2100010026.1.3",
          "form": "엄청 싫어한다;;",
          "original_form": "엄청 싫어한다;;",
          "speaker_id": "2",
          "time": "20210823 13:22"
        },
        {
          "id": "MDRW2100010026.1.4",
          "form": "젤 좋은시간이 급식시간이래..밥먹는거 그래 싫어하는대..",
          "original_form": "젤 좋은시간이 급식시간이래..밥먹는거 그래 싫어하는대..",
          "speaker_id": "2",
          "time": "20210823 13:22"
        },
        {
          "id": "MDRW2100010026.1.5",
          "form": "밥먹고 나면 집에 온다고 ㅋㅋㅋ",
          "original_form": "밥먹고 나면 집에 온다고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:22"
        },
        {
          "id": "MDRW2100010026.1.6",
          "form": "지금도 학습지하는데..",
          "original_form": "지금도 학습지하는데..",
          "speaker_id": "2",
          "time": "20210823 13:23"
        },
        {
          "id": "MDRW2100010026.1.7",
          "form": "속이 터짐 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "속이 터짐 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:23"
        },
        {
          "id": "MDRW2100010026.1.8",
          "form": "가르치는 엄마도 늠나 스트레스..",
          "original_form": "가르치는 엄마도 늠나 스트레스..",
          "speaker_id": "2",
          "time": "20210823 13:23"
        },
        {
          "id": "MDRW2100010026.1.9",
          "form": "ㅋㅋㅋㄱㅋㅋ엄청싫어한다니ㅋㅋ",
          "original_form": "ㅋㅋㅋㄱㅋㅋ엄청싫어한다니ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:27"
        },
        {
          "id": "MDRW2100010026.1.10",
          "form": "반전느낌이다ㅋ",
          "original_form": "반전느낌이다ㅋ",
          "speaker_id": "1",
          "time": "20210823 13:27"
        },
        {
          "id": "MDRW2100010026.1.11",
          "form": "당연하지ㅜㅜ",
          "original_form": "당연하지ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 13:27"
        },
        {
          "id": "MDRW2100010026.1.12",
          "form": "정말 학습지 속터진다ㅜ",
          "original_form": "정말 학습지 속터진다ㅜ",
          "speaker_id": "1",
          "time": "20210823 13:27"
        },
        {
          "id": "MDRW2100010026.1.13",
          "form": "요즘 진짜 공부에 질린듯 ㅋㅋㅋㅋ",
          "original_form": "요즘 진짜 공부에 질린듯 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:31"
        },
        {
          "id": "MDRW2100010026.1.14",
          "form": "수학 진짜 안할라칸다ㅠ",
          "original_form": "수학 진짜 안할라칸다ㅠ",
          "speaker_id": "2",
          "time": "20210823 13:31"
        },
        {
          "id": "MDRW2100010026.1.15",
          "form": "오늘도 두자리수 더하기 하는데..이건 뭐 하루쉬고나면",
          "original_form": "오늘도 두자리수 더하기 하는데..이건 뭐 하루쉬고나면",
          "speaker_id": "2",
          "time": "20210823 13:32"
        },
        {
          "id": "MDRW2100010026.1.16",
          "form": "다까먹어뿐다.",
          "original_form": "다까먹어뿐다.",
          "speaker_id": "2",
          "time": "20210823 13:32"
        },
        {
          "id": "MDRW2100010026.1.17",
          "form": "응응 ㅜㅜ",
          "original_form": "응응 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 13:32"
        },
        {
          "id": "MDRW2100010026.1.18",
          "form": "두자리수는 우리 첫째도 어려워하더라",
          "original_form": "두자리수는 우리 첫째도 어려워하더라",
          "speaker_id": "1",
          "time": "20210823 13:32"
        },
        {
          "id": "MDRW2100010026.1.19",
          "form": "방학할때부터 했눈데 아직까지 못한다;;",
          "original_form": "방학할때부터 했눈데 아직까지 못한다;;",
          "speaker_id": "2",
          "time": "20210823 13:32"
        },
        {
          "id": "MDRW2100010026.1.20",
          "form": "반복밖에는 답이없는것같은데",
          "original_form": "반복밖에는 답이없는것같은데",
          "speaker_id": "1",
          "time": "20210823 13:32"
        },
        {
          "id": "MDRW2100010026.1.21",
          "form": "좀 자신있는문제는",
          "original_form": "좀 자신있는문제는",
          "speaker_id": "2",
          "time": "20210823 13:32"
        },
        {
          "id": "MDRW2100010026.1.22",
          "form": "자기도 쉽다생각하니 문제도 제대로 안읽고 ㅋㅋㅋㅋ",
          "original_form": "자기도 쉽다생각하니 문제도 제대로 안읽고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:33"
        },
        {
          "id": "MDRW2100010026.1.23",
          "form": "대충풀어가 틀리고",
          "original_form": "대충풀어가 틀리고",
          "speaker_id": "2",
          "time": "20210823 13:33"
        },
        {
          "id": "MDRW2100010026.1.24",
          "form": "자신없는 문제는 자신없어가 틀리고 ㅋㅋㅋㅋㅋ",
          "original_form": "자신없는 문제는 자신없어가 틀리고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:33"
        },
        {
          "id": "MDRW2100010026.1.25",
          "form": "그캐놓고는 이젠 캐도 답이랑 비슷하네 이칸다ㅡㅡ",
          "original_form": "그캐놓고는 이젠 캐도 답이랑 비슷하네 이칸다ㅡㅡ",
          "speaker_id": "2",
          "time": "20210823 13:34"
        },
        {
          "id": "MDRW2100010026.1.26",
          "form": "수학에 비슷한게 어딧너 ㅋㅋㅋㅋㅋ",
          "original_form": "수학에 비슷한게 어딧너 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:34"
        },
        {
          "id": "MDRW2100010026.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱ아 웃겨ㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱ아 웃겨ㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210823 13:35"
        },
        {
          "id": "MDRW2100010026.1.28",
          "form": "터득한 방법인가보다",
          "original_form": "터득한 방법인가보다",
          "speaker_id": "1",
          "time": "20210823 13:35"
        },
        {
          "id": "MDRW2100010026.1.29",
          "form": "우리애는 .. 더  힘들어한다ㅋㅋ",
          "original_form": "우리애는 .. 더  힘들어한다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:35"
        },
        {
          "id": "MDRW2100010026.1.30",
          "form": "어떻게 해야될지 진짜ㅋㅋ",
          "original_form": "어떻게 해야될지 진짜ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:36"
        },
        {
          "id": "MDRW2100010026.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋ진짜 학원가야..둘다 행복할듯..",
          "original_form": "ㅋㅋㅋㅋㅋㅋ진짜 학원가야..둘다 행복할듯..",
          "speaker_id": "2",
          "time": "20210823 13:36"
        },
        {
          "id": "MDRW2100010026.1.32",
          "form": "근데..또 숙제 내주면..",
          "original_form": "근데..또 숙제 내주면..",
          "speaker_id": "2",
          "time": "20210823 13:36"
        },
        {
          "id": "MDRW2100010026.1.33",
          "form": "그건 또 우리의 몫이잖아;;",
          "original_form": "그건 또 우리의 몫이잖아;;",
          "speaker_id": "2",
          "time": "20210823 13:36"
        },
        {
          "id": "MDRW2100010026.1.34",
          "form": "ㅋㅋㅋㅋ그러니깐ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ그러니깐ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:36"
        },
        {
          "id": "MDRW2100010026.1.35",
          "form": "학원은 또 가기싫다하고ㅋㅋ",
          "original_form": "학원은 또 가기싫다하고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:37"
        },
        {
          "id": "MDRW2100010026.1.36",
          "form": "아이고..포기해야지 카는데도..늘 화남 ㅋㅋㅋ",
          "original_form": "아이고..포기해야지 카는데도..늘 화남 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:37"
        },
        {
          "id": "MDRW2100010026.1.37",
          "form": "줄넘기학원은 그나마 좋아해서 다행ㅋㅋ",
          "original_form": "줄넘기학원은 그나마 좋아해서 다행ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:37"
        },
        {
          "id": "MDRW2100010026.1.38",
          "form": "그니깐~영어 얘기했는데 제발 안가고싶데 ㅋㅋㅋㅋ",
          "original_form": "그니깐~영어 얘기했는데 제발 안가고싶데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:37"
        },
        {
          "id": "MDRW2100010026.1.39",
          "form": "아 진짜? 대박ㅋㅋㅋㅋ귀엽네ㅋㅋ",
          "original_form": "아 진짜? 대박ㅋㅋㅋㅋ귀엽네ㅋㅋ",
