      "utterance": [
        {
          "id": "MDRW2100042482.1.1",
          "form": "주차 못해서",
          "original_form": "주차 못해서",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042482.1.2",
          "form": "뒤에 차 밀리고 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "뒤에 차 밀리고 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042482.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ내얘기일수도..ㅎ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ내얘기일수도..ㅎ",
          "speaker_id": "1",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042482.1.4",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042482.1.5",
          "form": "약간 힐링느낌으로 다닐꺼면",
          "original_form": "약간 힐링느낌으로 다닐꺼면",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042482.1.6",
          "form": "차 없어도",
          "original_form": "차 없어도",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042482.1.7",
          "form": "괜찮을 같기도 하고",
          "original_form": "괜찮을 같기도 하고",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042482.1.8",
          "form": "뚜벅이도",
          "original_form": "뚜벅이도",
          "speaker_id": "1",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042482.1.9",
          "form": "나쁘지않지",
          "original_form": "나쁘지않지",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042482.1.10",
          "form": "우리 체력이 되기만하면...ㅎ",
          "original_form": "우리 체력이 되기만하면...ㅎ",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042482.1.11",
          "form": "제주시에 반 있고 서귀포에 반 있고 이러면 되니까",
          "original_form": "제주시에 반 있고 서귀포에 반 있고 이러면 되니까",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042482.1.12",
          "form": "아 맞다 울릉도도 진짜 이쁘던데",
          "original_form": "아 맞다 울릉도도 진짜 이쁘던데",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042482.1.13",
          "form": "ㅎ",
          "original_form": "ㅎ",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042482.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042482.1.15",
          "form": "내년에 가보자",
          "original_form": "내년에 가보자",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042482.1.16",
          "form": "ㅇㅋㅇㅋ",
          "original_form": "ㅇㅋㅇㅋ",
          "speaker_id": "1",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042482.1.17",
          "form": "재주도에서 하고 싶었던거 없어?",
          "original_form": "재주도에서 하고 싶었던거 없어?",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042482.1.18",
          "form": "제주도",
          "original_form": "제주도",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042482.1.19",
          "form": "엄",
          "original_form": "엄",
          "speaker_id": "1",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042482.1.20",
          "form": "오타 왜이렇게 많이 나냐",
          "original_form": "오타 왜이렇게 많이 나냐",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042482.1.21",
          "form": "뭐가있는지 잘 모르니까",
          "original_form": "뭐가있는지 잘 모르니까",
          "speaker_id": "1",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042482.1.22",
          "form": "뭐 하고싶은지도 잘모루겟음",
          "original_form": "뭐 하고싶은지도 잘모루겟음",
          "speaker_id": "1",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042482.1.23",
          "form": "근데 제주도는 진짜 자연이 이뻐서",
          "original_form": "근데 제주도는 진짜 자연이 이뻐서",
          "speaker_id": "2",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042482.1.24",
          "form": "걍 바다만 봐도",
          "original_form": "걍 바다만 봐도",
          "speaker_id": "2",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042482.1.25",
          "form": "이쁘더라",
          "original_form": "이쁘더라",
          "speaker_id": "2",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042482.1.26",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "1",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042482.1.27",
          "form": "좋네",
          "original_form": "좋네",
          "speaker_id": "1",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042482.1.28",
          "form": "그리고 겨울엔 눈 오니까",
          "original_form": "그리고 겨울엔 눈 오니까",
          "speaker_id": "2",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042482.1.29",
          "form": "눈 구경 하지 뭐",
          "original_form": "눈 구경 하지 뭐",
          "speaker_id": "2",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042482.1.30",
          "form": "좋다",
          "original_form": "좋다",
          "speaker_id": "1",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042482.1.31",
          "form": "테디베어 박물관 이런거 좋아하나?",
          "original_form": "테디베어 박물관 이런거 좋아하나?",
          "speaker_id": "2",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042482.1.32",
          "form": "나 눈구경 완전좋아하잖아",
          "original_form": "나 눈구경 완전좋아하잖아",
          "speaker_id": "1",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042482.1.33",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042482.1.34",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "2",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042482.1.35",
          "form": "미.친",
          "original_form": "미.친",
          "speaker_id": "1",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042482.1.36",
          "form": "ㄱ ㅐ좋음",
          "original_form": "ㄱ ㅐ좋음",
          "speaker_id": "1",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042482.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042482.1.38",
          "form": "그런 박물관 좋아함??",
          "original_form": "그런 박물관 좋아함??",
          "speaker_id": "2",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042482.1.39",
          "form": "아니 테디베어라서",
          "original_form": "아니 테디베어라서",
          "speaker_id": "1",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042482.1.40",
          "form": "재밌어보여",
          "original_form": "재밌어보여",
          "speaker_id": "1",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042482.1.41",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ가보자",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ가보자",
          "speaker_id": "2",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042482.1.42",
          "form": "나도 어릴때 갔어서",
          "original_form": "나도 어릴때 갔어서",
          "speaker_id": "2",
          "time": "20210906 22:29"
        },
        {
          "id": "MDRW2100042482.1.43",
          "form": "그리고 우린 버스타서 이야기 하면 되니까",
          "original_form": "그리고 우린 버스타서 이야기 하면 되니까",
          "speaker_id": "2",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042482.1.44",
