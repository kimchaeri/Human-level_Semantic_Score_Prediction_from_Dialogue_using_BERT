      "utterance": [
        {
          "id": "MDRW2100035257.1.1",
          "form": "좀비놀이 ㅋㅋ 박하선 장난이 아니네",
          "original_form": "좀비놀이 ㅋㅋ 박하선 장난이 아니네",
          "speaker_id": "2",
          "time": "20210829 12:54"
        },
        {
          "id": "MDRW2100035257.1.2",
          "form": "박하선 잘 받아주네",
          "original_form": "박하선 잘 받아주네",
          "speaker_id": "1",
          "time": "20210829 12:54"
        },
        {
          "id": "MDRW2100035257.1.3",
          "form": "엉 완전 잘 받아주네 성격이 좋은듯",
          "original_form": "엉 완전 잘 받아주네 성격이 좋은듯",
          "speaker_id": "2",
          "time": "20210829 12:54"
        },
        {
          "id": "MDRW2100035257.1.4",
          "form": "아니 일반인도 문신 많이 하네",
          "original_form": "아니 일반인도 문신 많이 하네",
          "speaker_id": "1",
          "time": "20210829 12:55"
        },
        {
          "id": "MDRW2100035257.1.5",
          "form": "친구도 테이핑 있네",
          "original_form": "친구도 테이핑 있네",
          "speaker_id": "1",
          "time": "20210829 12:55"
        },
        {
          "id": "MDRW2100035257.1.6",
          "form": "헤어가 세단다 ㅋㅋ 근데 잘 안어울린다",
          "original_form": "헤어가 세단다 ㅋㅋ 근데 잘 안어울린다",
          "speaker_id": "2",
          "time": "20210829 12:55"
        },
        {
          "id": "MDRW2100035257.1.7",
          "form": "얼굴이 커 보여ㅋ",
          "original_form": "얼굴이 커 보여ㅋ",
          "speaker_id": "1",
          "time": "20210829 12:56"
        },
        {
          "id": "MDRW2100035257.1.8",
          "form": "친구따라서 같이 문신 했나보다",
          "original_form": "친구따라서 같이 문신 했나보다",
          "speaker_id": "2",
          "time": "20210829 12:56"
        },
        {
          "id": "MDRW2100035257.1.9",
          "form": "매니저 얼굴 더 크네ㅋ",
          "original_form": "매니저 얼굴 더 크네ㅋ",
          "speaker_id": "1",
          "time": "20210829 12:56"
        },
        {
          "id": "MDRW2100035257.1.10",
          "form": "못생긴편은 아닌데 일단 헤어스타일을 바꾸어야할듯하다",
          "original_form": "못생긴편은 아닌데 일단 헤어스타일을 바꾸어야할듯하다",
          "speaker_id": "2",
          "time": "20210829 12:56"
        },
        {
          "id": "MDRW2100035257.1.11",
          "form": "ㅋㅋ 근데 계속 저 머리",
          "original_form": "ㅋㅋ 근데 계속 저 머리",
          "speaker_id": "1",
          "time": "20210829 12:57"
        },
        {
          "id": "MDRW2100035257.1.12",
          "form": "체격이 메니저가 틀리네 성인인랑 어린이 같다야",
          "original_form": "체격이 메니저가 틀리네 성인인랑 어린이 같다야",
          "speaker_id": "2",
          "time": "20210829 12:57"
        },
        {
          "id": "MDRW2100035257.1.13",
          "form": "키가 작드라 아니가?  목소리도 애같다",
          "original_form": "키가 작드라 아니가?  목소리도 애같다",
          "speaker_id": "1",
          "time": "20210829 12:57"
        },
        {
          "id": "MDRW2100035257.1.14",
          "form": "저 헤어스타일이 관리하기가 편하긴 할듯",
          "original_form": "저 헤어스타일이 관리하기가 편하긴 할듯",
          "speaker_id": "2",
          "time": "20210829 12:57"
