      "utterance": [
        {
          "id": "MDRW2100025039.1.1",
          "form": "동호회 이름이 알라는 아니겠지?",
          "original_form": "동호회 이름이 알라는 아니겠지?",
          "speaker_id": "2",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100025039.1.2",
          "form": "우리 지역에 그런 게 있다는 건 봤는데...",
          "original_form": "우리 지역에 그런 게 있다는 건 봤는데...",
          "speaker_id": "2",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100025039.1.3",
          "form": "너 혹시 가입했어?",
          "original_form": "너 혹시 가입했어?",
          "speaker_id": "2",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100025039.1.4",
          "form": "알아?? 의 오타야.",
          "original_form": "알아?? 의 오타야.",
          "speaker_id": "1",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100025039.1.5",
          "form": "아니 근데 궁금은하다",
          "original_form": "아니 근데 궁금은하다",
          "speaker_id": "1",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100025039.1.6",
          "form": "아아 미안해ㅠ",
          "original_form": "아아 미안해ㅠ",
          "speaker_id": "2",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100025039.1.7",
          "form": "어디서 본 정보야??",
          "original_form": "어디서 본 정보야??",
          "speaker_id": "1",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100025039.1.8",
          "form": "그런건 어디서 가입하는걸꺄?",
          "original_form": "그런건 어디서 가입하는걸꺄?",
          "speaker_id": "2",
          "time": "20210831 22:37"
        },
        {
          "id": "MDRW2100025039.1.9",
          "form": "아아 나는 커뮤니티 하고든",
          "original_form": "아아 나는 커뮤니티 하고든",
          "speaker_id": "2",
          "time": "20210831 22:38"
        },
        {
          "id": "MDRW2100025039.1.10",
          "form": "나중에 기회가 된다면 커뮤니티 정보를 줄게!",
          "original_form": "나중에 기회가 된다면 커뮤니티 정보를 줄게!",
          "speaker_id": "2",
          "time": "20210831 22:38"
        },
        {
          "id": "MDRW2100025039.1.11",
          "form": "세상에 너무 고마워",
          "original_form": "세상에 너무 고마워",
          "speaker_id": "1",
          "time": "20210831 22:38"
        },
        {
          "id": "MDRW2100025039.1.12",
          "form": "비혼동호회에서 독서모임을 하면 좋겠어",
          "original_form": "비혼동호회에서 독서모임을 하면 좋겠어",
          "speaker_id": "2",
          "time": "20210831 22:38"
        },
        {
          "id": "MDRW2100025039.1.13",
          "form": "당근마켓에서도 동호회를 찾던데",
          "original_form": "당근마켓에서도 동호회를 찾던데",
          "speaker_id": "1",
          "time": "20210831 22:38"
        },
        {
          "id": "MDRW2100025039.1.14",
          "form": "그리고 친구도 사귀고 싶어",
          "original_form": "그리고 친구도 사귀고 싶어",
          "speaker_id": "2",
          "time": "20210831 22:38"
        },
        {
          "id": "MDRW2100025039.1.15",
          "form": "그러게 독서모임을 하면",
          "original_form": "그러게 독서모임을 하면",
          "speaker_id": "1",
          "time": "20210831 22:38"
        },
        {
          "id": "MDRW2100025039.1.16",
          "form": "그런데서는 가입하고싶지않아",
          "original_form": "그런데서는 가입하고싶지않아",
          "speaker_id": "2",
          "time": "20210831 22:38"
        },
        {
          "id": "MDRW2100025039.1.17",
          "form": "억지로 책을 읽을 수 있어서 좋아",
          "original_form": "억지로 책을 읽을 수 있어서 좋아",
          "speaker_id": "1",
          "time": "20210831 22:38"
        },
        {
          "id": "MDRW2100025039.1.18",
          "form": "맞아. 얻는 게 더 많아질거야",
          "original_form": "맞아. 얻는 게 더 많아질거야",
          "speaker_id": "2",
          "time": "20210831 22:38"
        },
        {
          "id": "MDRW2100025039.1.19",
          "form": "너는 독서모임을 해본적있니?",
          "original_form": "너는 독서모임을 해본적있니?",
          "speaker_id": "1",
          "time": "20210831 22:38"
        }
      ]
