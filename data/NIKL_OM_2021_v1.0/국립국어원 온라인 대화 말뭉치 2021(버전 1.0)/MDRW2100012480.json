      "utterance": [
        {
          "id": "MDRW2100012480.1.1",
          "form": "어느 계절 좋아하세요?",
          "original_form": "어느 계절 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 22:22"
        },
        {
          "id": "MDRW2100012480.1.2",
          "form": "저는 가을 이요 그다음은 겨울이 좋아요",
          "original_form": "저는 가을 이요 그다음은 겨울이 좋아요",
          "speaker_id": "1",
          "time": "20210824 22:34"
        },
        {
          "id": "MDRW2100012480.1.3",
          "form": "저도 가을 좋아해요",
          "original_form": "저도 가을 좋아해요",
          "speaker_id": "2",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100012480.1.4",
          "form": "여름보다는 겨울",
          "original_form": "여름보다는 겨울",
          "speaker_id": "2",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100012480.1.5",
          "form": "가을은 하늘이 예뻐서 좋아요",
          "original_form": "가을은 하늘이 예뻐서 좋아요",
          "speaker_id": "2",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100012480.1.6",
          "form": "날씨도 선선하고 과일들도 맛있는거 많고",
          "original_form": "날씨도 선선하고 과일들도 맛있는거 많고",
          "speaker_id": "2",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100012480.1.7",
          "form": "하늘도 이쁘고 산책하기도 너무 좋고 낙엽도 이쁘고 낭만적인 계절이예요",
          "original_form": "하늘도 이쁘고 산책하기도 너무 좋고 낙엽도 이쁘고 낭만적인 계절이예요",
          "speaker_id": "1",
          "time": "20210824 22:57"
        },
        {
          "id": "MDRW2100012480.1.8",
          "form": "이럴때 궁궐이나 공원 걸어주면 너무 좋죠",
          "original_form": "이럴때 궁궐이나 공원 걸어주면 너무 좋죠",
          "speaker_id": "1",
          "time": "20210824 22:57"
        },
        {
          "id": "MDRW2100012480.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100012480.1.10",
          "form": "낙엽이랑 궁궐이랑 잘어울려요",
          "original_form": "낙엽이랑 궁궐이랑 잘어울려요",
          "speaker_id": "2",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100012480.1.11",
          "form": "낙엽 색감이 궁궐이랑 잘어울려서",
          "original_form": "낙엽 색감이 궁궐이랑 잘어울려서",
          "speaker_id": "2",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100012480.1.12",
          "form": "진짜 그림이죠",
          "original_form": "진짜 그림이죠",
          "speaker_id": "2",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100012480.1.13",
          "form": "한국에 은행나무 많은 이유를 알겠어요",
          "original_form": "한국에 은행나무 많은 이유를 알겠어요",
          "speaker_id": "2",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100012480.1.14",
          "form": "겨울에 눈쌓인 궁궐도 너무 좋아요",
          "original_form": "겨울에 눈쌓인 궁궐도 너무 좋아요",
          "speaker_id": "1",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100012480.1.15",
          "form": "진짜 직접 가서",
          "original_form": "진짜 직접 가서",
          "speaker_id": "2",
          "time": "20210824 23:15"
        },
        {
          "id": "MDRW2100012480.1.16",
          "form": "한번 차한잔 하면 너무 좋을 것 같아요",
          "original_form": "한번 차한잔 하면 너무 좋을 것 같아요",
          "speaker_id": "2",
          "time": "20210824 23:16"
        },
        {
          "id": "MDRW2100012480.1.17",
          "form": "궁궐에서 파는 차랑 다과가 있다고들었는데",
          "original_form": "궁궐에서 파는 차랑 다과가 있다고들었는데",
          "speaker_id": "1",
          "time": "20210824 23:19"
        },
        {
          "id": "MDRW2100012480.1.18",
          "form": "겨울에 눈오는거 보면서 마시면 더할나위 없을거 같아요",
          "original_form": "겨울에 눈오는거 보면서 마시면 더할나위 없을거 같아요",
          "speaker_id": "1",
          "time": "20210824 23:19"
        },
        {
          "id": "MDRW2100012480.1.19",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100012480.1.20",
          "form": "차분하고 세상 평화롭네요",
          "original_form": "차분하고 세상 평화롭네요",
          "speaker_id": "2",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100012480.1.21",
          "form": "이번 여름은 너무 더워서 힘들었어요",
          "original_form": "이번 여름은 너무 더워서 힘들었어요",
          "speaker_id": "1",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100012480.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100012480.1.23",
          "form": "2018만큼은 아니지맘",
          "original_form": "2018만큼은 아니지맘",
          "speaker_id": "2",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100012480.1.24",
          "form": "이제 선선해진게 느껴져서 진짜 좋아요",
          "original_form": "이제 선선해진게 느껴져서 진짜 좋아요",
          "speaker_id": "2",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100012480.1.25",
          "form": "작년엔 비가 많이 왔는데 올해는 안와서",
          "original_form": "작년엔 비가 많이 왔는데 올해는 안와서",
          "speaker_id": "2",
          "time": "20210824 23:37"
        },
        {
          "id": "MDRW2100012480.1.26",
          "form": "습하진않았네요",
          "original_form": "습하진않았네요",
          "speaker_id": "2",
          "time": "20210824 23:37"
        },
        {
          "id": "MDRW2100012480.1.27",
          "form": "안왔던 비가 요즘몰아서 오나봐요",
          "original_form": "안왔던 비가 요즘몰아서 오나봐요",
          "speaker_id": "1",
          "time": "20210824 23:38"
        },
        {
          "id": "MDRW2100012480.1.28",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100012480.1.29",
          "form": "태풍이라니 날씨가 너무 한번에 바뀌어서",
          "original_form": "태풍이라니 날씨가 너무 한번에 바뀌어서",
          "speaker_id": "2",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100012480.1.30",
          "form": "정신없어요ㅋㅋㅋ",
          "original_form": "정신없어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100012480.1.31",
          "form": "빨래해야하는데 매일비와요ㅠㅠ",
          "original_form": "빨래해야하는데 매일비와요ㅠㅠ",
