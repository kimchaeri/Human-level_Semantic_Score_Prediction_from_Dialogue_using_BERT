      "utterance": [
        {
          "id": "MDRW2100021467.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 19:19"
        },
        {
          "id": "MDRW2100021467.1.2",
          "form": "안녕하세요. 저녁은 드셨어요?",
          "original_form": "안녕하세요. 저녁은 드셨어요?",
          "speaker_id": "2",
          "time": "20210828 19:21"
        },
        {
          "id": "MDRW2100021467.1.3",
          "form": "아직요 드셨나요??",
          "original_form": "아직요 드셨나요??",
          "speaker_id": "1",
          "time": "20210828 19:22"
        },
        {
          "id": "MDRW2100021467.1.4",
          "form": "네 밥 다먹고 커피 마시고 있어요",
          "original_form": "네 밥 다먹고 커피 마시고 있어요",
          "speaker_id": "2",
          "time": "20210828 19:22"
        },
        {
          "id": "MDRW2100021467.1.5",
          "form": "후식까지 완벽하시네요 ㅋㅋㅋ",
          "original_form": "후식까지 완벽하시네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:23"
        },
        {
          "id": "MDRW2100021467.1.6",
          "form": "밥 뭐드셨나요?",
          "original_form": "밥 뭐드셨나요?",
          "speaker_id": "1",
          "time": "20210828 19:23"
        },
        {
          "id": "MDRW2100021467.1.7",
          "form": "아스파라거스랑 스팸넣은 볶음밥 해먹었어요",
          "original_form": "아스파라거스랑 스팸넣은 볶음밥 해먹었어요",
          "speaker_id": "2",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100021467.1.8",
          "form": "아스파라거스 전 안 먹어봤는데 어떤가요?!",
          "original_form": "아스파라거스 전 안 먹어봤는데 어떤가요?!",
          "speaker_id": "1",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100021467.1.9",
          "form": "작년인가 재작년에 처음 먹어봤는데",
          "original_form": "작년인가 재작년에 처음 먹어봤는데",
          "speaker_id": "2",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100021467.1.10",
          "form": "고기나 베이컨이랑 같이 볶아먹거나 볶음밥 해먹으면 맛있어요!",
          "original_form": "고기나 베이컨이랑 같이 볶아먹거나 볶음밥 해먹으면 맛있어요!",
          "speaker_id": "2",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100021467.1.11",
          "form": "기름기 있는거랑 정말 잘어울려요",
          "original_form": "기름기 있는거랑 정말 잘어울려요",
          "speaker_id": "2",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100021467.1.12",
          "form": "오 참고할게요",
          "original_form": "오 참고할게요",
          "speaker_id": "1",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100021467.1.13",
          "form": "신기하게 생겼더라구요 뭔가 ㅋㅋㅋ",
          "original_form": "신기하게 생겼더라구요 뭔가 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100021467.1.14",
          "form": "네ㅋㅋ 뭔가 꽃봉오리 같기도 하고요",
          "original_form": "네ㅋㅋ 뭔가 꽃봉오리 같기도 하고요",
          "speaker_id": "2",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100021467.1.15",
          "form": "어떻게 손질해야하나 처음에 난감하더라구요",
          "original_form": "어떻게 손질해야하나 처음에 난감하더라구요",
          "speaker_id": "2",
          "time": "20210828 19:31"
        },
        {
          "id": "MDRW2100021467.1.16",
          "form": "아 그것도 난해하겠네요 ㅠㅠ",
          "original_form": "아 그것도 난해하겠네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 19:31"
        },
        {
          "id": "MDRW2100021467.1.17",
          "form": "근데 찾아보니 진짜 간단했어요 그냥 아랫부분 좀 잘라내고 줄기에 싹? 같은거 깎으면 되는?",
          "original_form": "근데 찾아보니 진짜 간단했어요 그냥 아랫부분 좀 잘라내고 줄기에 싹? 같은거 깎으면 되는?",
          "speaker_id": "2",
          "time": "20210828 19:42"
        },
        {
          "id": "MDRW2100021467.1.18",
          "form": "저녁 얼른 드셔야죠",
          "original_form": "저녁 얼른 드셔야죠",
          "speaker_id": "2",
          "time": "20210828 19:42"
        },
        {
          "id": "MDRW2100021467.1.19",
          "form": "네 곧 먹으려구요 ㅋㅋㅋ",
          "original_form": "네 곧 먹으려구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:42"
        }
      ]
