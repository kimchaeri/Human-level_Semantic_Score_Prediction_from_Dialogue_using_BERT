      "utterance": [
        {
          "id": "MDRW2100005842.1.1",
          "form": "다회용컵 갖고 다니시나요",
          "original_form": "다회용컵 갖고 다니시나요",
          "speaker_id": "1",
          "time": "20210907 14:01"
        },
        {
          "id": "MDRW2100005842.1.2",
          "form": "회사에 갖다놓긴 했는데 카페같은데 들고 다니진 않아요",
          "original_form": "회사에 갖다놓긴 했는데 카페같은데 들고 다니진 않아요",
          "speaker_id": "2",
          "time": "20210907 14:09"
        },
        {
          "id": "MDRW2100005842.1.3",
          "form": "아 스벅 텀블러 마케팅 같은거 어떻게 생각하세요",
          "original_form": "아 스벅 텀블러 마케팅 같은거 어떻게 생각하세요",
          "speaker_id": "1",
          "time": "20210907 14:31"
        },
        {
          "id": "MDRW2100005842.1.4",
          "form": "좋은거 같아요ㅋㅋ 할인하니까 쓰는 사람들도 많구요",
          "original_form": "좋은거 같아요ㅋㅋ 할인하니까 쓰는 사람들도 많구요",
          "speaker_id": "2",
          "time": "20210907 14:41"
        },
        {
          "id": "MDRW2100005842.1.5",
          "form": "이것도 한 수백번인가 수천번정도 써야 환경에 도움이 된대요",
          "original_form": "이것도 한 수백번인가 수천번정도 써야 환경에 도움이 된대요",
          "speaker_id": "1",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100005842.1.6",
          "form": "그런데 한 사람이 여러개 갖고있으면",
          "original_form": "그런데 한 사람이 여러개 갖고있으면",
          "speaker_id": "1",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100005842.1.7",
          "form": "오히려 환경파괴라고 그러더라구요",
          "original_form": "오히려 환경파괴라고 그러더라구요",
          "speaker_id": "1",
          "time": "20210907 15:12"
        },
        {
          "id": "MDRW2100005842.1.8",
          "form": "복잡하네요ㅋㅋㅋ",
          "original_form": "복잡하네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100005842.1.9",
          "form": "스벅은 매 시즌마다 새로 팔더라구요",
          "original_form": "스벅은 매 시즌마다 새로 팔더라구요",
          "speaker_id": "2",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100005842.1.10",
          "form": "그쵸 주변에 그거 수집하는 사람있는데",
          "original_form": "그쵸 주변에 그거 수집하는 사람있는데",
          "speaker_id": "1",
          "time": "20210907 15:43"
        },
        {
          "id": "MDRW2100005842.1.11",
          "form": "못 가지면 중고로라도 꼭 사더라구요",
          "original_form": "못 가지면 중고로라도 꼭 사더라구요",
          "speaker_id": "1",
          "time": "20210907 15:43"
        },
        {
          "id": "MDRW2100005842.1.12",
          "form": "맞아요. 예쁘긴하지만 매번 살 정도는 아니던데",
          "original_form": "맞아요. 예쁘긴하지만 매번 살 정도는 아니던데",
          "speaker_id": "2",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100005842.1.13",
          "form": "컬렉션 사는 사람들 꽤 있어요",
          "original_form": "컬렉션 사는 사람들 꽤 있어요",
          "speaker_id": "2",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100005842.1.14",
          "form": "제친구도 그거구한다고",
          "original_form": "제친구도 그거구한다고",
          "speaker_id": "1",
          "time": "20210907 16:23"
        },
        {
          "id": "MDRW2100005842.1.15",
          "form": "스벅매장마다 전화돌리더라구요",
          "original_form": "스벅매장마다 전화돌리더라구요",
          "speaker_id": "1",
          "time": "20210907 16:24"
        },
        {
          "id": "MDRW2100005842.1.16",
          "form": "수집의 개념인가봐요",
          "original_form": "수집의 개념인가봐요",
          "speaker_id": "1",
          "time": "20210907 16:24"
        },
        {
          "id": "MDRW2100005842.1.17",
          "form": "전 대기하는거도 싫어해서ㅋㅋ 그렇게 사는거 보면 신기하더라구요",
          "original_form": "전 대기하는거도 싫어해서ㅋㅋ 그렇게 사는거 보면 신기하더라구요",
          "speaker_id": "2",
          "time": "20210907 16:25"
        },
        {
          "id": "MDRW2100005842.1.18",
          "form": "그렇죠 저도 좀이해가 안되지만",
          "original_form": "그렇죠 저도 좀이해가 안되지만",
          "speaker_id": "1",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100005842.1.19",
          "form": "그게 또 비싼가격에 팔리는거보면",
          "original_form": "그게 또 비싼가격에 팔리는거보면",
          "speaker_id": "1",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100005842.1.20",
          "form": "내가 시대를 못따라가나 싶기도하고요",
          "original_form": "내가 시대를 못따라가나 싶기도하고요",
          "speaker_id": "1",
          "time": "20210907 16:48"
        },
        {
          "id": "MDRW2100005842.1.21",
          "form": "관심없어서 안 사는 사람들도 있어서 균형이 맞는거 아닐까요?",
          "original_form": "관심없어서 안 사는 사람들도 있어서 균형이 맞는거 아닐까요?",
          "speaker_id": "2",
          "time": "20210907 16:49"
        },
        {
          "id": "MDRW2100005842.1.22",
          "form": "환경에도 좋구요ㅋㅋㅋㅋ",
          "original_form": "환경에도 좋구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:49"
        },
        {
          "id": "MDRW2100005842.1.23",
          "form": "그렇네요 뭐 수집품이라는 목적은 좋지만 친환경은 아니네요 결국",
          "original_form": "그렇네요 뭐 수집품이라는 목적은 좋지만 친환경은 아니네요 결국",
          "speaker_id": "1",
          "time": "20210907 18:25"
        }
      ]
