          "time": "20210904 12:13"
        },
        {
          "id": "MDRW2100030743.1.2",
          "form": "이래도 되는걸까",
          "original_form": "이래도 되는걸까",
          "speaker_id": "2",
          "time": "20210904 12:13"
        },
        {
          "id": "MDRW2100030743.1.3",
          "form": "웅 돼",
          "original_form": "웅 돼",
          "speaker_id": "1",
          "time": "20210904 12:13"
        },
        {
          "id": "MDRW2100030743.1.4",
          "form": "난 언성듀엣이 너무 사고싶어",
          "original_form": "난 언성듀엣이 너무 사고싶어",
          "speaker_id": "2",
          "time": "20210904 12:13"
        },
        {
          "id": "MDRW2100030743.1.5",
          "form": "ctyep 친구도 단답대화하는데 다 인정해줌",
          "original_form": "ctyep 친구도 단답대화하는데 다 인정해줌",
          "speaker_id": "1",
          "time": "20210904 12:13"
        },
        {
          "id": "MDRW2100030743.1.6",
          "form": "룰북 곧 가질 수 있어",
          "original_form": "룰북 곧 가질 수 있어",
          "speaker_id": "1",
          "time": "20210904 12:13"
        },
        {
          "id": "MDRW2100030743.1.7",
          "form": "그리고 시날집도 사고싶어",
          "original_form": "그리고 시날집도 사고싶어",
          "speaker_id": "2",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100030743.1.8",
          "form": "흡끅",
          "original_form": "흡끅",
          "speaker_id": "2",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100030743.1.9",
          "form": "나도 더 많은 룰북 사고 싶어",
          "original_form": "나도 더 많은 룰북 사고 싶어",
          "speaker_id": "1",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100030743.1.10",
          "form": "최클데 하나 샀더니",
          "original_form": "최클데 하나 샀더니",
          "speaker_id": "2",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100030743.1.11",
