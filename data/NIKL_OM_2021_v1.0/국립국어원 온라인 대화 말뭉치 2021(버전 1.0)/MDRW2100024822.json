      "utterance": [
        {
          "id": "MDRW2100024822.1.1",
          "form": "너는 반려동물 안키워?",
          "original_form": "너는 반려동물 안키워?",
          "speaker_id": "1",
          "time": "20210831 19:09"
        },
        {
          "id": "MDRW2100024822.1.2",
          "form": "나는 안 키워 너는?",
          "original_form": "나는 안 키워 너는?",
          "speaker_id": "2",
          "time": "20210831 19:10"
        },
        {
          "id": "MDRW2100024822.1.3",
          "form": "나는 고양이 키워",
          "original_form": "나는 고양이 키워",
          "speaker_id": "1",
          "time": "20210831 19:10"
        },
        {
          "id": "MDRW2100024822.1.4",
          "form": "고양이?",
          "original_form": "고양이?",
          "speaker_id": "2",
          "time": "20210831 19:11"
        },
        {
          "id": "MDRW2100024822.1.5",
          "form": "이름이 뭐야",
          "original_form": "이름이 뭐야",
          "speaker_id": "2",
          "time": "20210831 19:11"
        },
        {
          "id": "MDRW2100024822.1.6",
          "form": "name3야",
          "original_form": "&name3&야",
          "speaker_id": "1",
          "time": "20210831 19:12"
        },
        {
          "id": "MDRW2100024822.1.7",
          "form": "name3?",
          "original_form": "&name3&?",
          "speaker_id": "2",
          "time": "20210831 19:13"
        },
        {
          "id": "MDRW2100024822.1.8",
          "form": "이름을 특별히 name3라고 지은 이유가 있어?",
          "original_form": "이름을 특별히 &name3&라고 지은 이유가 있어?",
          "speaker_id": "2",
          "time": "20210831 19:13"
        },
        {
          "id": "MDRW2100024822.1.9",
          "form": "귀여운 이름을 하고싶엇어",
          "original_form": "귀여운 이름을 하고싶엇어",
          "speaker_id": "1",
          "time": "20210831 19:14"
        },
        {
          "id": "MDRW2100024822.1.10",
          "form": "아하 그랬구나",
          "original_form": "아하 그랬구나",
          "speaker_id": "2",
          "time": "20210831 19:15"
        },
        {
          "id": "MDRW2100024822.1.11",
          "form": "name3는 무슨 간식 제일 좋아해?",
          "original_form": "&name3&는 무슨 간식 제일 좋아해?",
          "speaker_id": "2",
          "time": "20210831 19:15"
        },
        {
          "id": "MDRW2100024822.1.12",
          "form": "name3 다 좋아해 가리는거 없어",
          "original_form": "&name3& 다 좋아해 가리는거 없어",
          "speaker_id": "1",
          "time": "20210831 19:16"
        },
        {
          "id": "MDRW2100024822.1.13",
          "form": "오 그렇구낭",
          "original_form": "오 그렇구낭",
          "speaker_id": "2",
          "time": "20210831 19:18"
        },
        {
          "id": "MDRW2100024822.1.14",
          "form": "나는 예전에",
          "original_form": "나는 예전에",
          "speaker_id": "2",
          "time": "20210831 19:18"
        },
        {
          "id": "MDRW2100024822.1.15",
          "form": "너는 키울생각 없어?",
          "original_form": "너는 키울생각 없어?",
          "speaker_id": "1",
          "time": "20210831 19:18"
        },
        {
          "id": "MDRW2100024822.1.16",
          "form": "나는",
          "original_form": "나는",
          "speaker_id": "2",
          "time": "20210831 19:18"
        },
        {
          "id": "MDRW2100024822.1.17",
          "form": "강아지 키우고 싶어",
          "original_form": "강아지 키우고 싶어",
          "speaker_id": "2",
          "time": "20210831 19:18"
        },
        {
          "id": "MDRW2100024822.1.18",
          "form": "시베리안 허스키",
          "original_form": "시베리안 허스키",
          "speaker_id": "2",
          "time": "20210831 19:18"
        },
        {
          "id": "MDRW2100024822.1.19",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210831 19:18"
        },
        {
          "id": "MDRW2100024822.1.20",
          "form": "먼가 귀여우면서 카리스마 있자나",
          "original_form": "먼가 귀여우면서 카리스마 있자나",
          "speaker_id": "2",
          "time": "20210831 19:28"
        },
        {
          "id": "MDRW2100024822.1.21",
          "form": "너는 다른 반려동물 더 안 키우고 싶어?",
          "original_form": "너는 다른 반려동물 더 안 키우고 싶어?",
          "speaker_id": "2",
          "time": "20210831 19:28"
        },
        {
          "id": "MDRW2100024822.1.22",
          "form": "응 너는 강아지 키우면 이름 뭐로 지을거야?",
          "original_form": "응 너는 강아지 키우면 이름 뭐로 지을거야?",
          "speaker_id": "1",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100024822.1.23",
          "form": "나는 name4",
          "original_form": "나는 &name4&",
          "speaker_id": "2",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100024822.1.24",
          "form": "귀여운 이름이 좋아",
          "original_form": "귀여운 이름이 좋아",
          "speaker_id": "2",
          "time": "20210902 19:21"
        },
        {
          "id": "MDRW2100024822.1.25",
          "form": "하나도 안귀여운데?",
          "original_form": "하나도 안귀여운데?",
          "speaker_id": "1",
          "time": "20210902 19:21"
        },
        {
          "id": "MDRW2100024822.1.26",
          "form": "귀엽거든?!",
          "original_form": "귀엽거든?!",
          "speaker_id": "2",
          "time": "20210902 19:21"
        }
      ]
