      },
      "utterance": [
        {
          "id": "MDRW2100009006.1.1",
          "form": "벌써 다음주 메뉴를 고민해?",
          "original_form": "벌써 다음주 메뉴를 고민해?",
          "speaker_id": "2",
          "time": "20210822 20:25"
        },
        {
          "id": "MDRW2100009006.1.2",
          "form": "엄마는 뭐 먹고 싶은데?",
          "original_form": "엄마는 뭐 먹고 싶은데?",
          "speaker_id": "2",
          "time": "20210822 20:25"
        },
        {
          "id": "MDRW2100009006.1.3",
          "form": "딱히. 가지가 있는데 그걸 해먹을까?",
          "original_form": "딱히. 가지가 있는데 그걸 해먹을까?",
          "speaker_id": "1",
          "time": "20210822 20:26"
        },
        {
          "id": "MDRW2100009006.1.4",
          "form": "뭐 해먹지?",
          "original_form": "뭐 해먹지?",
          "speaker_id": "1",
          "time": "20210822 20:26"
        },
        {
          "id": "MDRW2100009006.1.5",
          "form": "쪄먹는 거 어때?",
          "original_form": "쪄먹는 거 어때?",
          "speaker_id": "2",
          "time": "20210822 20:26"
        },
        {
          "id": "MDRW2100009006.1.6",
          "form": "식감이 흐물흐물해서 싫어.",
          "original_form": "식감이 흐물흐물해서 싫어.",
          "speaker_id": "1",
          "time": "20210822 20:27"
        },
        {
          "id": "MDRW2100009006.1.7",
          "form": "인터넷에 검색해봤더니 가지솥밥 레시피 나온다",
          "original_form": "인터넷에 검색해봤더니 가지솥밥 레시피 나온다",
          "speaker_id": "2",
          "time": "20210822 20:27"
        },
        {
          "id": "MDRW2100009006.1.8",
          "form": "이거 괜찮을 것 같아",
          "original_form": "이거 괜찮을 것 같아",
          "speaker_id": "2",
          "time": "20210822 20:27"
        },
        {
          "id": "MDRW2100009006.1.9",
          "form": "그거 괜찮을 것 같다",
          "original_form": "그거 괜찮을 것 같다",
          "speaker_id": "1",
          "time": "20210822 20:28"
        },
        {
          "id": "MDRW2100009006.1.10",
          "form": "버터구이도 해볼까",
          "original_form": "버터구이도 해볼까",
          "speaker_id": "1",
          "time": "20210822 20:28"
        },
        {
          "id": "MDRW2100009006.1.11",
          "form": "반찬도 있으면 좋지",
          "original_form": "반찬도 있으면 좋지",
          "speaker_id": "2",
          "time": "20210822 20:29"
        },
        {
          "id": "MDRW2100009006.1.12",
          "form": "그래 준비해볼게",
          "original_form": "그래 준비해볼게",
          "speaker_id": "1",
          "time": "20210822 20:29"
        },
        {
          "id": "MDRW2100009006.1.13",
          "form": "더 먹고 싶은 거 있음 말하고.",
          "original_form": "더 먹고 싶은 거 있음 말하고.",
          "speaker_id": "1",
          "time": "20210822 20:30"
        },
        {
          "id": "MDRW2100009006.1.14",
          "form": "난 사실 비빔밥 먹고 싶긴 해",
          "original_form": "난 사실 비빔밥 먹고 싶긴 해",
          "speaker_id": "2",
          "time": "20210822 20:30"
        },
        {
          "id": "MDRW2100009006.1.15",
          "form": "그럼 점심은 가지솥밥, 저녁은 비빔밥. 오케이?",
          "original_form": "그럼 점심은 가지솥밥, 저녁은 비빔밥. 오케이?",
          "speaker_id": "1",
          "time": "20210822 20:31"
        },
        {
          "id": "MDRW2100009006.1.16",
          "form": "비빔밥 참가름 많이 넣고 비벼줄게.",
          "original_form": "비빔밥 참가름 많이 넣고 비벼줄게.",
          "speaker_id": "1",
          "time": "20210822 20:32"
        },
        {
          "id": "MDRW2100009006.1.17",
          "form": "오 그거 좋다",
          "original_form": "오 그거 좋다",
          "speaker_id": "1",
          "time": "20210822 20:34"
        },
        {
          "id": "MDRW2100009006.1.18",
          "form": "버터구이까지.",
          "original_form": "버터구이까지.",
          "speaker_id": "1",
          "time": "20210822 20:35"
        },
        {
          "id": "MDRW2100009006.1.19",
          "form": "그래~그리고 디저트는 내가 사갈게",
          "original_form": "그래~그리고 디저트는 내가 사갈게",
          "speaker_id": "2",
          "time": "20210822 20:35"
        },
        {
          "id": "MDRW2100009006.1.20",
          "form": "그럼 아이스크림 사와.",
          "original_form": "그럼 아이스크림 사와.",
          "speaker_id": "1",
          "time": "20210822 20:36"
        },
        {
          "id": "MDRW2100009006.1.21",
          "form": "알겠어ㅋㅋㅋ 비빔밥 맵게 먹고 아이스크림 먹으면 딱이겠다",
          "original_form": "알겠어ㅋㅋㅋ 비빔밥 맵게 먹고 아이스크림 먹으면 딱이겠다",
          "speaker_id": "2",
          "time": "20210822 20:37"
        }
      ]
