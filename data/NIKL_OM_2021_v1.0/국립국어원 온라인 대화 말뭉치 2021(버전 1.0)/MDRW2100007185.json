      "utterance": [
        {
          "id": "MDRW2100007185.1.1",
          "form": "학교생활 어땠니 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "학교생활 어땠니 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:46"
        },
        {
          "id": "MDRW2100007185.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋ너는 알거라고 생각하는걸?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ너는 알거라고 생각하는걸?",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007185.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007185.1.4",
          "form": "뭐가 가장 기억나?",
          "original_form": "뭐가 가장 기억나?",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007185.1.5",
          "form": "음~~others?",
          "original_form": "음~~&others&?",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007185.1.6",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007185.1.7",
          "form": "후....",
          "original_form": "후....",
          "speaker_id": "1",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007185.1.8",
          "form": "그게 뭐니?",
          "original_form": "그게 뭐니?",
          "speaker_id": "1",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007185.1.9",
          "form": "ㅋㅋㅋㅋ어 학생들 기강잡는 거지같은 캠프야",
          "original_form": "ㅋㅋㅋㅋ어 학생들 기강잡는 거지같은 캠프야",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007185.1.10",
          "form": "세상에ㅋㅋㅋ 힘들었겠다",
          "original_form": "세상에ㅋㅋㅋ 힘들었겠다",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007185.1.11",
          "form": "시험은 어땠니",
          "original_form": "시험은 어땠니",
          "speaker_id": "1",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007185.1.12",
          "form": "나는 실기시험이 많암ㅅ어",
          "original_form": "나는 실기시험이 많암ㅅ어",
          "speaker_id": "1",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007185.1.13",
          "form": "시험은 뭐ㅋ 열심히는 했지",
          "original_form": "시험은 뭐ㅋ 열심히는 했지",
          "speaker_id": "2",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007185.1.14",
          "form": "나는 요령껏 했던거 같아",
          "original_form": "나는 요령껏 했던거 같아",
          "speaker_id": "1",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007185.1.15",
          "form": "그치 요령껏ㅋㅋㅋ교수들 성격 파악하고",
          "original_form": "그치 요령껏ㅋㅋㅋ교수들 성격 파악하고",
          "speaker_id": "2",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007185.1.16",
          "form": "어쩌다가 시험 성적 잘나오면 기분 좋고",
          "original_form": "어쩌다가 시험 성적 잘나오면 기분 좋고",
          "speaker_id": "1",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007185.1.17",
          "form": "나쁘면 납득  못하고 ㅋㅋㅋ",
          "original_form": "나쁘면 납득  못하고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007185.1.18",
          "form": "아니야...나는 늘 납득했어...",
          "original_form": "아니야...나는 늘 납득했어...",
          "speaker_id": "2",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007185.1.19",
          "form": "그래? ㅋㅋㅋㅋ 정직하구나",
          "original_form": "그래? ㅋㅋㅋㅋ 정직하구나",
          "speaker_id": "1",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007185.1.20",
          "form": "나는 플러스에 집착",
          "original_form": "나는 플러스에 집착",
          "speaker_id": "1",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007185.1.21",
          "form": "왜 플러스가 없는가",
          "original_form": "왜 플러스가 없는가",
