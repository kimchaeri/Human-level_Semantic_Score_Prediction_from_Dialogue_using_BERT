      "utterance": [
        {
          "id": "MDRW2100032808.1.1",
          "form": "오스트리아도 꼭 한번 가보고 싶었는데",
          "original_form": "오스트리아도 꼭 한번 가보고 싶었는데",
          "speaker_id": "2",
          "time": "20210906 00:04"
        },
        {
          "id": "MDRW2100032808.1.2",
          "form": "그렇게 가는 곳 마다 다 사진이 잘나온다면서요? ㅠㅠ",
          "original_form": "그렇게 가는 곳 마다 다 사진이 잘나온다면서요? ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 00:05"
        },
        {
          "id": "MDRW2100032808.1.3",
          "form": "분위기가 완전 중세 느낌? 인 곳이 많기도 하고 자연도 예쁘더라구요!",
          "original_form": "분위기가 완전 중세 느낌? 인 곳이 많기도 하고 자연도 예쁘더라구요!",
          "speaker_id": "1",
          "time": "20210906 00:06"
        },
        {
          "id": "MDRW2100032808.1.4",
          "form": "허어엉 ㅠ 그 사운드오브뮤직에 나왔던 그곳 가보셨나요?",
          "original_form": "허어엉 ㅠ 그 사운드오브뮤직에 나왔던 그곳 가보셨나요?",
          "speaker_id": "2",
          "time": "20210906 00:08"
        },
        {
          "id": "MDRW2100032808.1.5",
          "form": "네 갔었는데 한겨울이어서 휑하고",
          "original_form": "네 갔었는데 한겨울이어서 휑하고",
          "speaker_id": "1",
          "time": "20210906 00:19"
        },
        {
          "id": "MDRW2100032808.1.6",
          "form": "비도왔어서...ㅠㅠ",
          "original_form": "비도왔어서...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 00:20"
        },
        {
          "id": "MDRW2100032808.1.7",
          "form": "아니 ㅠㅠ 아아니 ㅠㅠㅠ",
          "original_form": "아니 ㅠㅠ 아아니 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 13:03"
        },
        {
          "id": "MDRW2100032808.1.8",
          "form": "꼭 가야할 곳 중 하나로 골라둔 곳이었거든요 ㅠ",
          "original_form": "꼭 가야할 곳 중 하나로 골라둔 곳이었거든요 ㅠ",
          "speaker_id": "2",
          "time": "20210906 13:03"
        },
        {
          "id": "MDRW2100032808.1.9",
          "form": "너무 아쉬웠을 것 같아요 ㅠ",
          "original_form": "너무 아쉬웠을 것 같아요 ㅠ",
          "speaker_id": "2",
          "time": "20210906 13:03"
        },
        {
          "id": "MDRW2100032808.1.10",
          "form": "네네 진짜...유럽여행 나중에 가시면 꼭 5~7월 정도",
          "original_form": "네네 진짜...유럽여행 나중에 가시면 꼭 5~7월 정도",
          "speaker_id": "1",
          "time": "20210906 13:51"
        },
        {
          "id": "MDRW2100032808.1.11",
          "form": "푸릇푸릇할때 가세요...",
          "original_form": "푸릇푸릇할때 가세요...",
          "speaker_id": "1",
          "time": "20210906 13:51"
        },
        {
          "id": "MDRW2100032808.1.12",
          "form": "정원이나 공원 같은게 많아서요",
          "original_form": "정원이나 공원 같은게 많아서요",
          "speaker_id": "1",
          "time": "20210906 13:51"
        },
        {
          "id": "MDRW2100032808.1.13",
          "form": "겨울의 유럽도 아름다웠을 것 같아요",
          "original_form": "겨울의 유럽도 아름다웠을 것 같아요",
          "speaker_id": "2",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100032808.1.14",
          "form": "눈이 쌓인 장면을 상상하기만 해도 예쁜걸요",
          "original_form": "눈이 쌓인 장면을 상상하기만 해도 예쁜걸요",
          "speaker_id": "2",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100032808.1.15",
          "form": "5~7월 꼭 기억하고 있을게요",
          "original_form": "5~7월 꼭 기억하고 있을게요",
          "speaker_id": "2",
          "time": "20210906 14:49"
        },
        {
          "id": "MDRW2100032808.1.16",
          "form": "겨울에도 매력이 있긴 한 것 같아요 ㅎㅎ",
          "original_form": "겨울에도 매력이 있긴 한 것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 14:51"
        },
        {
          "id": "MDRW2100032808.1.17",
          "form": "전 코로나 끝나면...사람 엄청 많은 큰 관광도시 가보고싶어욬ㅋㅋㅋ",
          "original_form": "전 코로나 끝나면...사람 엄청 많은 큰 관광도시 가보고싶어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:51"
        },
        {
          "id": "MDRW2100032808.1.18",
          "form": "어디 가보고 싶으세요??",
          "original_form": "어디 가보고 싶으세요??",
          "speaker_id": "2",
          "time": "20210906 15:35"
        },
        {
          "id": "MDRW2100032808.1.19",
          "form": "동남아 같은 곳도 가보고 싶고",
          "original_form": "동남아 같은 곳도 가보고 싶고",
          "speaker_id": "1",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100032808.1.20",
          "form": "뉴욕 이런데도 가보고 싶어요 ㅎㅎ",
          "original_form": "뉴욕 이런데도 가보고 싶어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 15:39"
        },
        {
          "id": "MDRW2100032808.1.21",
          "form": "뉴욕! 가서 어떤거 해보고 싶으세요??",
          "original_form": "뉴욕! 가서 어떤거 해보고 싶으세요??",
          "speaker_id": "2",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100032808.1.22",
          "form": "뉴욕 야경 구경 하고싶어요!",
          "original_form": "뉴욕 야경 구경 하고싶어요!",
          "speaker_id": "1",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100032808.1.23",
          "form": "멋질 것 같아요 ㅎ",
          "original_form": "멋질 것 같아요 ㅎ",
          "speaker_id": "1",
          "time": "20210906 15:43"
        },
        {
          "id": "MDRW2100032808.1.24",
          "form": "야경! 정말 사진으로만 보던걸 직접보면 즐거울 것 같아요!",
          "original_form": "야경! 정말 사진으로만 보던걸 직접보면 즐거울 것 같아요!",
          "speaker_id": "2",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100032808.1.25",
          "form": "저는 미국을 가게 된다면 뮤지컬을 꼭 보고 싶어요",
          "original_form": "저는 미국을 가게 된다면 뮤지컬을 꼭 보고 싶어요",
          "speaker_id": "2",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100032808.1.26",
          "form": "동남아는 어디어디요??",
          "original_form": "동남아는 어디어디요??",
          "speaker_id": "2",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100032808.1.27",
          "form": "동남아는 가본적이 없어서 어딜가도 다 좋을 것 같아요 ㅎㅎ",
          "original_form": "동남아는 가본적이 없어서 어딜가도 다 좋을 것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 15:48"
        }
      ]
