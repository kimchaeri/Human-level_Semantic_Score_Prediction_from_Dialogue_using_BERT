      "utterance": [
        {
          "id": "MDRW2100014840.1.1",
          "form": "너 판교 뚜벅초라고 알아???",
          "original_form": "너 판교 뚜벅초라고 알아???",
          "speaker_id": "2",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014840.1.2",
          "form": "유튜버인데",
          "original_form": "유튜버인데",
          "speaker_id": "2",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014840.1.3",
          "form": "그사람이 카카오 다니거든",
          "original_form": "그사람이 카카오 다니거든",
          "speaker_id": "2",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014840.1.4",
          "form": "누구야??",
          "original_form": "누구야??",
          "speaker_id": "1",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014840.1.5",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014840.1.6",
          "form": "근데 거기서 메타버스로 일하더라",
          "original_form": "근데 거기서 메타버스로 일하더라",
          "speaker_id": "2",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014840.1.7",
          "form": "개발자 유튜버인데 재밌어",
          "original_form": "개발자 유튜버인데 재밌어",
          "speaker_id": "2",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014840.1.8",
          "form": "메타버스로 일한다는게",
          "original_form": "메타버스로 일한다는게",
          "speaker_id": "1",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014840.1.9",
          "form": "뭔데??",
          "original_form": "뭔데??",
          "speaker_id": "1",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014840.1.10",
          "form": "면접얘기도 해 도움많이될듯",
          "original_form": "면접얘기도 해 도움많이될듯",
          "speaker_id": "2",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014840.1.11",
          "form": "요즘은 재택 많이하잖아",
          "original_form": "요즘은 재택 많이하잖아",
          "speaker_id": "2",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100014840.1.12",
          "form": "근데 재택근무만 하면",
          "original_form": "근데 재택근무만 하면",
          "speaker_id": "2",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014840.1.13",
          "form": "집중도 안되고 능률이 떨어지니까",
          "original_form": "집중도 안되고 능률이 떨어지니까",
          "speaker_id": "2",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014840.1.14",
          "form": "메타버스로 출근해서",
          "original_form": "메타버스로 출근해서",
          "speaker_id": "2",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014840.1.15",
          "form": "거기서 회의도 하고 밥도먹고 커피도 마시고 ㅋㅋㅋㅋㅋ",
          "original_form": "거기서 회의도 하고 밥도먹고 커피도 마시고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014840.1.16",
          "form": "아 그건가",
          "original_form": "아 그건가",
          "speaker_id": "1",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014840.1.17",
          "form": "막 회사도 꾸미고 그래",
          "original_form": "막 회사도 꾸미고 그래",
          "speaker_id": "2",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014840.1.18",
          "form": "저번에 테크 블로그에서 본",
          "original_form": "저번에 테크 블로그에서 본",
          "speaker_id": "1",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014840.1.19",
          "form": "게임같은곳에서",
          "original_form": "게임같은곳에서",
          "speaker_id": "1",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100014840.1.20",
          "form": "어어어 그거야",
          "original_form": "어어어 그거야",
          "speaker_id": "2",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014840.1.21",
          "form": "채팅하고 움직이면서 일하는",
          "original_form": "채팅하고 움직이면서 일하는",
          "speaker_id": "1",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014840.1.22",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014840.1.23",
          "form": "재밌겠지",
          "original_form": "재밌겠지",
          "speaker_id": "2",
          "time": "20210825 20:55"
        },
        {
          "id": "MDRW2100014840.1.24",
          "form": "근데 요즘 웬만한곳은",
          "original_form": "근데 요즘 웬만한곳은",
          "speaker_id": "2",
          "time": "20210825 20:55"
        },
        {
