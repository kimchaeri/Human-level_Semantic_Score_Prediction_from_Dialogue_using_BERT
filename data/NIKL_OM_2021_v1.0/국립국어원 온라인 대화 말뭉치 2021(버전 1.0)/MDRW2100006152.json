        {
          "id": "MDRW2100006152.1.2",
          "form": "라이언",
          "original_form": "라이언",
          "speaker_id": "1",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100006152.1.3",
          "form": "안사셨죠?",
          "original_form": "안사셨죠?",
          "speaker_id": "1",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100006152.1.4",
          "form": "일 좀 한가해질 때 돌 거임!",
          "original_form": "일 좀 한가해질 때 돌 거임!",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100006152.1.5",
          "form": "보니 일찍 입고됐을 때 때맞춰 가야겠더라구요",
          "original_form": "보니 일찍 입고됐을 때 때맞춰 가야겠더라구요",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100006152.1.6",
          "form": "바쁜 노비일 땐 경쟁력이 떨어지니",
          "original_form": "바쁜 노비일 땐 경쟁력이 떨어지니",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100006152.1.7",
          "form": "한가한 노비일 때 쟁취할거예유ㅎ",
          "original_form": "한가한 노비일 때 쟁취할거예유ㅎ",
          "speaker_id": "2",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100006152.1.8",
          "form": "이제 한정판 끝날겁니다 ㅎㅎ",
          "original_form": "이제 한정판 끝날겁니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100006152.1.9",
          "form": "저주하지 마세욧!",
          "original_form": "저주하지 마세욧!",
          "speaker_id": "2",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100006152.1.10",
          "form": "님은 태어날 때부터 역장이었남욧!",
          "original_form": "님은 태어날 때부터 역장이었남욧!",
          "speaker_id": "2",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100006152.1.11",
          "form": "저는 일개 역무원입니다",
          "original_form": "저는 일개 역무원입니다",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100006152.1.12",
          "form": "ㅂㄷㅂㄷ 내가 역장이 돼야 님을 부리는데",
          "original_form": "ㅂㄷㅂㄷ 내가 역장이 돼야 님을 부리는데",
          "speaker_id": "2",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100006152.1.13",
          "form": "갑질이 몸에 배어 계신...",
          "original_form": "갑질이 몸에 배어 계신...",
          "speaker_id": "1",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100006152.1.14",
          "form": "카스트 밑바닥 인생에게 그게 무슨 망발이쥬",
          "original_form": "카스트 밑바닥 인생에게 그게 무슨 망발이쥬",
          "speaker_id": "2",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100006152.1.15",
          "form": "부르죠아 같으신데",
          "original_form": "부르죠아 같으신데",
          "speaker_id": "1",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100006152.1.16",
          "form": "엣헴 제가 좀 귀티나긴 하쥬",
          "original_form": "엣헴 제가 좀 귀티나긴 하쥬",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100006152.1.17",
          "form": "그럴거 같아요 무거운거 다 후배시키고 ㅎㅎ",
          "original_form": "그럴거 같아요 무거운거 다 후배시키고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100006152.1.18",
          "form": "ㅎㅎ 후배니?",
          "original_form": "ㅎㅎ 후배니?",
          "speaker_id": "2",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100006152.1.19",
          "form": "오늘도 고마웠어ㅎㅎ",
          "original_form": "오늘도 고마웠어ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100006152.1.20",
          "form": "사내갑질..",
          "original_form": "사내갑질..",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100006152.1.21",
          "form": "저런 누가 괴롭혔니",
          "original_form": "저런 누가 괴롭혔니",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100006152.1.22",
          "form": "선배가 상담해줄게ㅎㅎ",
          "original_form": "선배가 상담해줄게ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100006152.1.23",
          "form": "서울시랑 본사요",
          "original_form": "서울시랑 본사요",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100006152.1.24",
          "form": "혼 좀 내주세요",
          "original_form": "혼 좀 내주세요",
