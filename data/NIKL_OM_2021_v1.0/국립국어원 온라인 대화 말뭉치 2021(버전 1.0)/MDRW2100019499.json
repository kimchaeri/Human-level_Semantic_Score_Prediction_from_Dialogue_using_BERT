      "utterance": [
        {
          "id": "MDRW2100019499.1.1",
          "form": "내일 또 신입연수에요",
          "original_form": "내일 또 신입연수에요",
          "speaker_id": "2",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019499.1.2",
          "form": "우와 선배님이신가보다",
          "original_form": "우와 선배님이신가보다",
          "speaker_id": "1",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019499.1.3",
          "form": "네ㅋㅋ",
          "original_form": "네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019499.1.4",
          "form": "정규직이세요?",
          "original_form": "정규직이세요?",
          "speaker_id": "1",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019499.1.5",
          "form": "다음달이면 정규직이에요",
          "original_form": "다음달이면 정규직이에요",
          "speaker_id": "2",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019499.1.6",
          "form": "우와 축하드려요",
          "original_form": "우와 축하드려요",
          "speaker_id": "1",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019499.1.7",
          "form": "출근하기싫네요",
          "original_form": "출근하기싫네요",
          "speaker_id": "2",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019499.1.8",
          "form": "주말이라 더 싫으시겠어요",
          "original_form": "주말이라 더 싫으시겠어요",
          "speaker_id": "1",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019499.1.9",
          "form": "네... 또 어떤일잊생길지",
          "original_form": "네... 또 어떤일잊생길지",
          "speaker_id": "2",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100019499.1.10",
          "form": "주말이 이상한가요?",
          "original_form": "주말이 이상한가요?",
          "speaker_id": "1",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100019499.1.11",
          "form": "음 좀 바빠요",
          "original_form": "음 좀 바빠요",
          "speaker_id": "2",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100019499.1.12",
          "form": "내일은 안바쁠 거에요",
          "original_form": "내일은 안바쁠 거에요",
          "speaker_id": "1",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100019499.1.13",
          "form": "그럴까요?",
          "original_form": "그럴까요?",
          "speaker_id": "2",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100019499.1.14",
          "form": "넹! 해야할일 다 끝내시길 바랄게요",
          "original_form": "넹! 해야할일 다 끝내시길 바랄게요",
          "speaker_id": "1",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100019499.1.15",
          "form": "감사합니다",
          "original_form": "감사합니다",
          "speaker_id": "2",
          "time": "20210827 19:59"
        }
