      "utterance": [
        {
          "id": "MDRW2100044677.1.1",
          "form": "헐 전원투표 뭐야",
          "original_form": "헐 전원투표 뭐야",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044677.1.2",
          "form": "ㅜ",
          "original_form": "ㅜ",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044677.1.3",
          "form": "떨려떨려",
          "original_form": "떨려떨려",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044677.1.4",
          "form": "와 워스트댄서가 지목하다니",
          "original_form": "와 워스트댄서가 지목하다니",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044677.1.5",
          "form": "와 진짜 너무 무섭다",
          "original_form": "와 진짜 너무 무섭다",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044677.1.6",
          "form": "메인댄서 지목할수도 있잖아",
          "original_form": "메인댄서 지목할수도 있잖아",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044677.1.7",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044677.1.8",
          "form": "만만해서 지목당해도 진짜 열받을듯",
          "original_form": "만만해서 지목당해도 진짜 열받을듯",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044677.1.9",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044677.1.10",
          "form": "응 그리고 우리 크루원 건드리면",
          "original_form": "응 그리고 우리 크루원 건드리면",
          "speaker_id": "1",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044677.1.11",
          "form": "왤케 침들을 바를까",
          "original_form": "왤케 침들을 바를까",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044677.1.12",
          "form": "코로나 옮으면 어케",
          "original_form": "코로나 옮으면 어케",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100044677.1.13",
          "form": "ㅋㅋㅋ비말감염 위험",
          "original_form": "ㅋㅋㅋ비말감염 위험",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100044677.1.14",
          "form": "다들 이 갈고 추네",
          "original_form": "다들 이 갈고 추네",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044677.1.15",
          "form": "ㅋㅋㅋㅋㅋ그니까",
          "original_form": "ㅋㅋㅋㅋㅋ그니까",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044677.1.16",
          "form": "멋있다",
          "original_form": "멋있다",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044677.1.17",
          "form": "저 훅이 지난번에 엄청 지목당했어서 이겼음 좋겠어",
          "original_form": "저 훅이 지난번에 엄청 지목당했어서 이겼음 좋겠어",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044677.1.18",
          "form": "응 훅이 만만한 느낌ㅜ",
          "original_form": "응 훅이 만만한 느낌ㅜ",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044677.1.19",
          "form": "혀 를 다들",
          "original_form": "혀 를 다들",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044677.1.20",
          "form": "저 동네 도발스타일인가?",
          "original_form": "저 동네 도발스타일인가?",
          "speaker_id": "1",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044677.1.21",
          "form": "ㅋㅋㅋㅋㅋ그런가봐",
          "original_form": "ㅋㅋㅋㅋㅋ그런가봐",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044677.1.22",
          "form": "다들 멋있네",
          "original_form": "다들 멋있네",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044677.1.23",
          "form": "엉덩이나 혓바닥을 내미는게",
          "original_form": "엉덩이나 혓바닥을 내미는게",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044677.1.24",
          "form": "공격자세인가봐",
          "original_form": "공격자세인가봐",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044677.1.25",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044677.1.26",
          "form": "그런가봐",
          "original_form": "그런가봐",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044677.1.27",
          "form": "와 몰표...",
          "original_form": "와 몰표...",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044677.1.28",
          "form": "자막과 말이 달라",
          "original_form": "자막과 말이 달라",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044677.1.29",
          "form": "헐 냉정하네",
          "original_form": "헐 냉정하네",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044677.1.30",
          "form": "아이돌이라 차별도 당하는 부분 있는거 같고?",
          "original_form": "아이돌이라 차별도 당하는 부분 있는거 같고?",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044677.1.31",
          "form": "다들 냉정해ㅜ",
          "original_form": "다들 냉정해ㅜ",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044677.1.32",
          "form": "채연이도 뭔가 매번 무시당해서ㅜ",
          "original_form": "채연이도 뭔가 매번 무시당해서ㅜ",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044677.1.33",
          "form": "와 그래도 0대 몰표라고 하면",
          "original_form": "와 그래도 0대 몰표라고 하면",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044677.1.34",
          "form": "넘 슬프다",
          "original_form": "넘 슬프다",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044677.1.35",
          "form": "응 저기 앉아있기 힘들듯",
          "original_form": "응 저기 앉아있기 힘들듯",
          "speaker_id": "1",
          "time": "20210907 23:39"
        }
      ]
    }
