      "utterance": [
        {
          "id": "MDRW2100005366.1.1",
          "form": "요리랑 먹방 ㅋㅋㅋ",
          "original_form": "요리랑 먹방 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005366.1.2",
          "form": "유튜버 추천 해줄래",
          "original_form": "유튜버 추천 해줄래",
          "speaker_id": "1",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005366.1.3",
          "form": "나는 햇님 좋아해",
          "original_form": "나는 햇님 좋아해",
          "speaker_id": "2",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005366.1.4",
          "form": "햇님?",
          "original_form": "햇님?",
          "speaker_id": "1",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005366.1.5",
          "form": "입짧은 햇님 넘 좋아",
          "original_form": "입짧은 햇님 넘 좋아",
          "speaker_id": "2",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005366.1.6",
          "form": "아 먹방 ㅋㅋ 알아",
          "original_form": "아 먹방 ㅋㅋ 알아",
          "speaker_id": "1",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005366.1.7",
          "form": "본 적 있어?",
          "original_form": "본 적 있어?",
          "speaker_id": "2",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005366.1.8",
          "form": "캡쳐만 봤고 영상은 본 적 없어",
          "original_form": "캡쳐만 봤고 영상은 본 적 없어",
          "speaker_id": "1",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005366.1.9",
          "form": "엄청 잘 드셔",
          "original_form": "엄청 잘 드셔",
          "speaker_id": "2",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005366.1.10",
          "form": "요리도 잘하고",
          "original_form": "요리도 잘하고",
          "speaker_id": "2",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005366.1.11",
          "form": "나는 유튜브는 잘 안봐서",
          "original_form": "나는 유튜브는 잘 안봐서",
          "speaker_id": "1",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005366.1.12",
          "form": "나는 유튜브를 많이 보고",
          "original_form": "나는 유튜브를 많이 보고",
          "speaker_id": "2",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005366.1.13",
          "form": "드라마는 요즘 잘 안봐",
          "original_form": "드라마는 요즘 잘 안봐",
          "speaker_id": "2",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005366.1.14",
          "form": "유튜브 재밌는 거 많다고는 하더라",
          "original_form": "유튜브 재밌는 거 많다고는 하더라",
          "speaker_id": "1",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005366.1.15",
          "form": "검색하면 다 나오니까 편해 ㅋㅋ",
          "original_form": "검색하면 다 나오니까 편해 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:04"
        }
