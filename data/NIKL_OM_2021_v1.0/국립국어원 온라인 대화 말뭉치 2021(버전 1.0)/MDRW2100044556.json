      "utterance": [
        {
          "id": "MDRW2100044556.1.1",
          "form": "피부 관리 하시나요?",
          "original_form": "피부 관리 하시나요?",
          "speaker_id": "1",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044556.1.2",
          "form": "해야할거같아요ㅜ",
          "original_form": "해야할거같아요ㅜ",
          "speaker_id": "2",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100044556.1.3",
          "form": "전 1일 1팩을 목표로 하고 있고든요",
          "original_form": "전 1일 1팩을 목표로 하고 있고든요",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044556.1.4",
          "form": "아 저 보고 따라하다 3개의\r\n트러블이 나서ㅋㅋ",
          "original_form": "아 저 보고 따라하다 3개의\r\n트러블이 나서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044556.1.5",
          "form": "근데 생리때문이었어요!",
          "original_form": "근데 생리때문이었어요!",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044556.1.6",
          "form": "아!! 근데 이게 안하다 하묜 뭐 나는 분들도 있어요",
          "original_form": "아!! 근데 이게 안하다 하묜 뭐 나는 분들도 있어요",
          "speaker_id": "1",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044556.1.7",
          "form": "그니까요! 절묘한 타이밍ㅋㄱ",
          "original_form": "그니까요! 절묘한 타이밍ㅋㄱ",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044556.1.8",
          "form": "전 건성이라 꼭 해줘여 하는데요.",
          "original_form": "전 건성이라 꼭 해줘여 하는데요.",
          "speaker_id": "1",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044556.1.9",
          "form": "팩이 너무 고영양이었나봐요ㅜ",
          "original_form": "팩이 너무 고영양이었나봐요ㅜ",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044556.1.10",
          "form": "너무 좋은거 하신거 아니예요?",
          "original_form": "너무 좋은거 하신거 아니예요?",
          "speaker_id": "1",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044556.1.11",
          "form": "ㅋㅋ굴러다니는거 해서...ㅜ 그래서 트러블이...ㅜ",
          "original_form": "ㅋㅋ굴러다니는거 해서...ㅜ 그래서 트러블이...ㅜ",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044556.1.12",
          "form": "그럴땐 이틀에 한번으로 줄여주세요",
          "original_form": "그럴땐 이틀에 한번으로 줄여주세요",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044556.1.13",
          "form": "내일 다시 도전하겠습니다!",
          "original_form": "내일 다시 도전하겠습니다!",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044556.1.14",
          "form": "나중에 피부에서 막 광이 나요",
          "original_form": "나중에 피부에서 막 광이 나요",
          "speaker_id": "1",
