      "utterance": [
        {
          "id": "MDRW2100019872.1.1",
          "form": "트레이더스는 뭐가 좋아요?",
          "original_form": "트레이더스는 뭐가 좋아요?",
          "speaker_id": "2",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019872.1.2",
          "form": "음 술?",
          "original_form": "음 술?",
          "speaker_id": "1",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019872.1.3",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019872.1.4",
          "form": "맥주요",
          "original_form": "맥주요",
          "speaker_id": "1",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019872.1.5",
          "form": "오 좋네요",
          "original_form": "오 좋네요",
          "speaker_id": "2",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019872.1.6",
          "form": "네 맥주랑 양주싸요",
          "original_form": "네 맥주랑 양주싸요",
          "speaker_id": "1",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019872.1.7",
          "form": "식품은 어때요",
          "original_form": "식품은 어때요",
          "speaker_id": "2",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019872.1.8",
          "form": "식품은 괜찮아요",
          "original_form": "식품은 괜찮아요",
          "speaker_id": "1",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019872.1.9",
          "form": "고기질이 좋아요",
          "original_form": "고기질이 좋아요",
          "speaker_id": "1",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019872.1.10",
          "form": "많이 팔죠?",
          "original_form": "많이 팔죠?",
          "speaker_id": "2",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019872.1.11",
          "form": "네 그래서 한번 사면",
          "original_form": "네 그래서 한번 사면",
          "speaker_id": "1",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019872.1.12",
          "form": "절반 이상 남아요",
          "original_form": "절반 이상 남아요",
          "speaker_id": "1",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019872.1.13",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210827 22:36"
        },
        {
          "id": "MDRW2100019872.1.14",
          "form": "네 근데 딴데보다 훨씬 싸고",
          "original_form": "네 근데 딴데보다 훨씬 싸고",
          "speaker_id": "1",
          "time": "20210827 22:36"
        },
        {
          "id": "MDRW2100019872.1.15",
          "form": "질이 좋아요",
          "original_form": "질이 좋아요",
          "speaker_id": "1",
          "time": "20210827 22:36"
        },
        {
          "id": "MDRW2100019872.1.16",
          "form": "사 볼만 하겠어요",
          "original_form": "사 볼만 하겠어요",
          "speaker_id": "2",
          "time": "20210827 22:36"
        },
        {
          "id": "MDRW2100019872.1.17",
          "form": "네 ㅋㅋ 오늘은 갈비 사왔어요",
          "original_form": "네 ㅋㅋ 오늘은 갈비 사왔어요",
          "speaker_id": "1",
          "time": "20210827 22:36"
        },
        {
          "id": "MDRW2100019872.1.18",
          "form": "얼마에요?",
          "original_form": "얼마에요?",
          "speaker_id": "2",
          "time": "20210827 22:36"
        },
        {
          "id": "MDRW2100019872.1.19",
          "form": "절단갈비 3키로에 3만원이요",
          "original_form": "절단갈비 3키로에 3만원이요",
          "speaker_id": "1",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019872.1.20",
          "form": "국내산이요???",
          "original_form": "국내산이요???",
          "speaker_id": "2",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019872.1.21",
          "form": "네 정말 싸죠",
          "original_form": "네 정말 싸죠",
          "speaker_id": "1",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019872.1.22",
          "form": "이거 좋아요",
          "original_form": "이거 좋아요",
          "speaker_id": "1",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019872.1.23",
          "form": "이번에 청소기 샀어요",
          "original_form": "이번에 청소기 샀어요",
          "speaker_id": "1",
          "time": "20210903 19:15"
        }
