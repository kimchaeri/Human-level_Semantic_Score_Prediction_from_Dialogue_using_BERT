      "utterance": [
        {
          "id": "MDRW2100021203.1.1",
          "form": "네 좋아해요 주로 어떤집 가시나요?",
          "original_form": "네 좋아해요 주로 어떤집 가시나요?",
          "speaker_id": "2",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021203.1.2",
          "form": "돈까스요!",
          "original_form": "돈까스요!",
          "speaker_id": "1",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100021203.1.3",
          "form": "전에 저랑 삼대 돈가스 얘길 하셨던것 같은데",
          "original_form": "전에 저랑 삼대 돈가스 얘길 하셨던것 같은데",
          "speaker_id": "2",
          "time": "20210828 16:54"
        },
        {
          "id": "MDRW2100021203.1.4",
          "form": "다른분이신가 가물가물하네요 ㅋㅋ",
          "original_form": "다른분이신가 가물가물하네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:54"
        },
        {
          "id": "MDRW2100021203.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ  가물가물하네요 치킨도좋아해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ  가물가물하네요 치킨도좋아해요",
          "speaker_id": "1",
          "time": "20210828 16:55"
        },
        {
          "id": "MDRW2100021203.1.6",
          "form": "전 교촌좋아해요 님은요?",
          "original_form": "전 교촌좋아해요 님은요?",
          "speaker_id": "2",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100021203.1.7",
          "form": "저도 허니콤보요!",
          "original_form": "저도 허니콤보요!",
          "speaker_id": "1",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021203.1.8",
          "form": "전 레드콤보요 ㅋㅋㅋ",
          "original_form": "전 레드콤보요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100021203.1.9",
          "form": "피자는 드시나요?",
          "original_form": "피자는 드시나요?",
          "speaker_id": "2",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100021203.1.10",
          "form": "피자 너무좋죠ㅎㅎㅎ",
          "original_form": "피자 너무좋죠ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100021203.1.11",
          "form": "전 피자헛을 주로 먹어요 님은요?",
          "original_form": "전 피자헛을 주로 먹어요 님은요?",
          "speaker_id": "2",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100021203.1.12",
          "form": "파파존스요!",
          "original_form": "파파존스요!",
          "speaker_id": "1",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100021203.1.13",
          "form": "파파존스 맛있죠  어떤 메뉴를 주로 드시나요?",
          "original_form": "파파존스 맛있죠  어떤 메뉴를 주로 드시나요?",
          "speaker_id": "2",
          "time": "20210828 17:08"
        },
        {
          "id": "MDRW2100021203.1.14",
          "form": "존스페이버릿이요!",
          "original_form": "존스페이버릿이요!",
          "speaker_id": "1",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100021203.1.15",
          "form": "전 피자럿 포테이토요 ㅋㅋ 담백해서 좋아요",
          "original_form": "전 피자럿 포테이토요 ㅋㅋ 담백해서 좋아요",
          "speaker_id": "2",
