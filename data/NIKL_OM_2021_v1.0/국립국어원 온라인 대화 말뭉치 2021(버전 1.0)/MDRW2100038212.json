        },
        {
          "id": "MDRW2100038212.1.5",
          "form": "상자깡을 진짜",
          "original_form": "상자깡을 진짜",
          "speaker_id": "2",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100038212.1.6",
          "form": "겁나 열심히",
          "original_form": "겁나 열심히",
          "speaker_id": "2",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100038212.1.7",
          "form": "해야겠는걸",
          "original_form": "해야겠는걸",
          "speaker_id": "2",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100038212.1.8",
          "form": "상자깡 난",
          "original_form": "상자깡 난",
          "speaker_id": "1",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100038212.1.9",
          "form": "이보다 더 열심히 할 수 업어",
          "original_form": "이보다 더 열심히 할 수 업어",
          "speaker_id": "1",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100038212.1.10",
          "form": "리월 900개 넘겻다구....",
          "original_form": "리월 900개 넘겻다구....",
          "speaker_id": "1",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100038212.1.11",
          "form": "이제 이나즈마도 해야지",
          "original_form": "이제 이나즈마도 해야지",
          "speaker_id": "2",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100038212.1.12",
          "form": "이나즈마도 이미",
          "original_form": "이나즈마도 이미",
          "speaker_id": "1",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100038212.1.13",
          "form": "200개정도는 깠는데",
          "original_form": "200개정도는 깠는데",
          "speaker_id": "1",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100038212.1.14",
          "form": "난 몬드 계란빵",
          "original_form": "난 몬드 계란빵",
          "speaker_id": "2",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100038212.1.15",
          "form": "이제 얻었어ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이제 얻었어ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100038212.1.16",
          "form": "오늘 몬드 월퀘",
          "original_form": "오늘 몬드 월퀘",
          "speaker_id": "2",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100038212.1.17",
          "form": "다끝냄",
          "original_form": "다끝냄",
          "speaker_id": "2",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100038212.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ계란빵이 평판보상인가ㅡ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ계란빵이 평판보상인가ㅡ",
          "speaker_id": "1",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100038212.1.19",
          "form": "등가교환 하나때문에",
          "original_form": "등가교환 하나때문에",
          "speaker_id": "2",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100038212.1.20",
          "form": "ㅇㅇ평판 4렙보상일걸",
          "original_form": "ㅇㅇ평판 4렙보상일걸",
          "speaker_id": "2",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100038212.1.21",
          "form": "ㅇㅎㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅎㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100038212.1.22",
          "form": "일퀘 선행해야 뜨는 월퀘",
          "original_form": "일퀘 선행해야 뜨는 월퀘",
          "speaker_id": "2",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100038212.1.23",
          "form": "빡세",
          "original_form": "빡세",
          "speaker_id": "2",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100038212.1.24",
          "form": "ㅇㅇ 그건 진짜 빡세",
          "original_form": "ㅇㅇ 그건 진짜 빡세",
          "speaker_id": "1",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100038212.1.25",
          "form": "ㄹㅇ죽어라고 안떠",
          "original_form": "ㄹㅇ죽어라고 안떠",
          "speaker_id": "2",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100038212.1.26",
          "form": "ㅇ ㅏ 나 몬드 현상토벌도 해야하는데",
          "original_form": "ㅇ ㅏ 나 몬드 현상토벌도 해야하는데",
          "speaker_id": "1",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100038212.1.27",
          "form": "귀찮아서 방치중",
          "original_form": "귀찮아서 방치중",
          "speaker_id": "1",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100038212.1.28",
          "form": "날개 얼른 얻어서",
          "original_form": "날개 얼른 얻어서",
          "speaker_id": "1",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100038212.1.29",
          "form": "우인단 조무래기 기념품 사주는건",
          "original_form": "우인단 조무래기 기념품 사주는건",
          "speaker_id": "2",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100038212.1.30",
          "form": "좀 웃기긴 했다",
          "original_form": "좀 웃기긴 했다",
          "speaker_id": "2",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100038212.1.31",
          "form": "맛있게 먹어라 짜식",
          "original_form": "맛있게 먹어라 짜식",
          "speaker_id": "2",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100038212.1.32",
          "form": "주인공이나 푸른계열 캐 달아주고싶은데",
          "original_form": "주인공이나 푸른계열 캐 달아주고싶은데",
          "speaker_id": "1",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100038212.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100038212.1.34",
          "form": "그거",
          "original_form": "그거",
          "speaker_id": "1",
          "time": "20210902 00:53"
        },
        {
          "id": "MDRW2100038212.1.35",
          "form": "주인공이 어떤 선택지를 고르든",
          "original_form": "주인공이 어떤 선택지를 고르든",
          "speaker_id": "1",
          "time": "20210902 00:53"
        },
        {
          "id": "MDRW2100038212.1.36",
          "form": "생각보다 우인단에 글케 적대적이지 않다고",
          "original_form": "생각보다 우인단에 글케 적대적이지 않다고",
          "speaker_id": "1",
          "time": "20210902 00:53"
        },
        {
          "id": "MDRW2100038212.1.37",
          "form": "나중에 우인단도 들어간다는 떡밥 아니냐 그러던데...",
          "original_form": "나중에 우인단도 들어간다는 떡밥 아니냐 그러던데...",
          "speaker_id": "1",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100038212.1.38",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100038212.1.39",
          "form": "설마 그렇게까지?",
          "original_form": "설마 그렇게까지?",
          "speaker_id": "2",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100038212.1.40",
          "form": "그럼 앞으로 맵에 있는 우인단 조무래기들 얼굴을",
          "original_form": "그럼 앞으로 맵에 있는 우인단 조무래기들 얼굴을",
          "speaker_id": "2",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100038212.1.41",
          "form": "억케 봐",
          "original_form": "억케 봐",
          "speaker_id": "2",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100038212.1.42",
          "form": "걔네가 날 칠거야 내가 걔넬 칠거야",
          "original_form": "걔네가 날 칠거야 내가 걔넬 칠거야",
          "speaker_id": "2",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100038212.1.43",
          "form": "신병의 휘장만 내놓으면 봐주겠다고 협박을 할거야",
          "original_form": "신병의 휘장만 내놓으면 봐주겠다고 협박을 할거야",
          "speaker_id": "2",
          "time": "20210902 00:55"
        },
        {
          "id": "MDRW2100038212.1.44",
          "form": "그냥 덜 적대적인 관계가",
          "original_form": "그냥 덜 적대적인 관계가",
          "speaker_id": "2",
          "time": "20210902 00:56"
        },
        {
          "id": "MDRW2100038212.1.45",
          "form": "마지노선 아닐까",
          "original_form": "마지노선 아닐까",
          "speaker_id": "2",
          "time": "20210902 00:56"
        },
        {
          "id": "MDRW2100038212.1.46",
          "form": "난 계속 걔넬 뚜까패서",
          "original_form": "난 계속 걔넬 뚜까패서",
          "speaker_id": "2",
          "time": "20210902 00:56"
        },
        {
          "id": "MDRW2100038212.1.47",
          "form": "휘장 수급을 해야하는데",
          "original_form": "휘장 수급을 해야하는데",
          "speaker_id": "2",
          "time": "20210902 00:56"
        },
        {
          "id": "MDRW2100038212.1.48",
          "form": "아닠ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아닠ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:58"
        },
        {
          "id": "MDRW2100038212.1.49",
          "form": "우인단 조무래기 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "우인단 조무래기 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:58"
        },
        {
          "id": "MDRW2100038212.1.50",
          "form": "아니 그럿지 안아....?",
          "original_form": "아니 그럿지 안아....?",
