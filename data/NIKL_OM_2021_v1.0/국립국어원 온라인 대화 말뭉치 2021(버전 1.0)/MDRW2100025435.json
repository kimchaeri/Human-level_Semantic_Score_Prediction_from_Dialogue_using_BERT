          "time": "20210901 11:18"
        },
        {
          "id": "MDRW2100025435.1.2",
          "form": "근데 가브리먹다가",
          "original_form": "근데 가브리먹다가",
          "speaker_id": "1",
          "time": "20210901 11:19"
        },
        {
          "id": "MDRW2100025435.1.3",
          "form": "목살먹음안돼",
          "original_form": "목살먹음안돼",
          "speaker_id": "1",
          "time": "20210901 11:19"
        },
        {
          "id": "MDRW2100025435.1.4",
          "form": "목살먼저먹고가브리먹어야지",
          "original_form": "목살먼저먹고가브리먹어야지",
          "speaker_id": "1",
          "time": "20210901 11:19"
        },
        {
          "id": "MDRW2100025435.1.5",
          "form": "아 글고 여기 가족들이랑 가봐",
          "original_form": "아 글고 여기 가족들이랑 가봐",
          "speaker_id": "1",
          "time": "20210901 11:20"
        },
        {
          "id": "MDRW2100025435.1.6",
          "form": "문대감의정육식당인가",
          "original_form": "문대감의정육식당인가",
          "speaker_id": "1",
          "time": "20210901 11:20"
        },
        {
          "id": "MDRW2100025435.1.7",
          "form": "문대감의 정육식당 저장해놔야지",
          "original_form": "문대감의 정육식당 저장해놔야지",
          "speaker_id": "2",
          "time": "20210901 11:20"
        },
        {
          "id": "MDRW2100025435.1.8",
          "form": "600그람 시키면 600그람더줌",
          "original_form": "600그람 시키면 600그람더줌",
          "speaker_id": "1",
          "time": "20210901 11:20"
        },
        {
          "id": "MDRW2100025435.1.9",
          "form": "450그람 시키면 450그람더주고",
          "original_form": "450그람 시키면 450그람더주고",
          "speaker_id": "1",
          "time": "20210901 11:21"
        },
        {
          "id": "MDRW2100025435.1.10",
          "form": "한우는아닌데 고기질이 좋아",
          "original_form": "한우는아닌데 고기질이 좋아",
          "speaker_id": "1",
          "time": "20210901 11:21"
        },
        {
          "id": "MDRW2100025435.1.11",
          "form": "원쁠원인가여",
          "original_form": "원쁠원인가여",
          "speaker_id": "2",
          "time": "20210901 11:21"
        },
        {
          "id": "MDRW2100025435.1.12",
          "form": "배고프다",
          "original_form": "배고프다",
          "speaker_id": "2",
          "time": "20210901 11:21"
        },
        {
          "id": "MDRW2100025435.1.13",
          "form": "빨리점심시간와라",
          "original_form": "빨리점심시간와라",
          "speaker_id": "2",
          "time": "20210901 11:21"
        },
        {
          "id": "MDRW2100025435.1.14",
          "form": "ㅇㅇㅇㅇㅇ",
