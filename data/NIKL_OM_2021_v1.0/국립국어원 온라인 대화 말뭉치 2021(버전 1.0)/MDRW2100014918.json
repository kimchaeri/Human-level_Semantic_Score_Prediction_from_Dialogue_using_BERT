          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014918.1.12",
          "form": "맞어 가을이자나",
          "original_form": "맞어 가을이자나",
          "speaker_id": "2",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014918.1.13",
          "form": "당근마켓은 다녀왔어?",
          "original_form": "당근마켓은 다녀왔어?",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014918.1.14",
