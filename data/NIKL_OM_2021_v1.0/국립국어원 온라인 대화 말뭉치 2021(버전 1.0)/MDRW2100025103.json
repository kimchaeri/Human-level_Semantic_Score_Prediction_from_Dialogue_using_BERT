      "utterance": [
        {
          "id": "MDRW2100025103.1.1",
          "form": "회사식당 음식 잘 나오나여?",
          "original_form": "회사식당 음식 잘 나오나여?",
          "speaker_id": "1",
          "time": "20210831 23:10"
        },
        {
          "id": "MDRW2100025103.1.2",
          "form": "네네 회사구내는아닌데",
          "original_form": "네네 회사구내는아닌데",
          "speaker_id": "2",
          "time": "20210831 23:10"
        },
        {
          "id": "MDRW2100025103.1.3",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210831 23:10"
        },
        {
          "id": "MDRW2100025103.1.4",
          "form": "그런구내식당같은 식당이라",
          "original_form": "그런구내식당같은 식당이라",
          "speaker_id": "2",
          "time": "20210831 23:10"
        },
        {
          "id": "MDRW2100025103.1.5",
          "form": "매일 반찬다르게 국다르게해서 나와요",
          "original_form": "매일 반찬다르게 국다르게해서 나와요",
          "speaker_id": "2",
          "time": "20210831 23:10"
        },
        {
          "id": "MDRW2100025103.1.6",
          "form": "ㅠㅠㅠㅠ맛있겠어요",
          "original_form": "ㅠㅠㅠㅠ맛있겠어요",
          "speaker_id": "1",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100025103.1.7",
          "form": "맛도괜찮고... 메뉴가 한정적이어서 자주가면",
          "original_form": "맛도괜찮고... 메뉴가 한정적이어서 자주가면",
          "speaker_id": "2",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100025103.1.8",
          "form": "저는 지금 회사 안 다니고 있어서",
          "original_form": "저는 지금 회사 안 다니고 있어서",
          "speaker_id": "1",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100025103.1.9",
          "form": "겹치긴하지만 없는것보단...",
          "original_form": "겹치긴하지만 없는것보단...",
          "speaker_id": "2",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100025103.1.10",
          "form": "누가 끼니때마다 밥 좀 줬으면 좋겠어요..",
          "original_form": "누가 끼니때마다 밥 좀 줬으면 좋겠어요..",
          "speaker_id": "1",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100025103.1.11",
          "form": "아 그거진짜공감해요",
          "original_form": "아 그거진짜공감해요",
          "speaker_id": "2",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100025103.1.12",
          "form": "회사이사오기전에 있던곳은",
          "original_form": "회사이사오기전에 있던곳은",
          "speaker_id": "2",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100025103.1.13",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100025103.1.14",
          "form": "그런곳마저없어서",
          "original_form": "그런곳마저없어서",
          "speaker_id": "2",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100025103.1.15",
          "form": "점심이고역이었어요",
          "original_form": "점심이고역이었어요",
          "speaker_id": "2",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100025103.1.16",
          "form": "ㅠㅠ 어유",
          "original_form": "ㅠㅠ 어유",
          "speaker_id": "1",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100025103.1.17",
          "form": "그쳐 매번 사먹기도 질리져",
          "original_form": "그쳐 매번 사먹기도 질리져",
          "speaker_id": "1",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100025103.1.18",
          "form": "사먹는것도 괜찮은곳이있으면",
          "original_form": "사먹는것도 괜찮은곳이있으면",
          "speaker_id": "2",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100025103.1.19",
          "form": "질려도거길갈텐데",
          "original_form": "질려도거길갈텐데",
          "speaker_id": "2",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100025103.1.20",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100025103.1.21",
          "form": "다비싸기만하고 맛이없고...",
          "original_form": "다비싸기만하고 맛이없고...",
          "speaker_id": "2",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100025103.1.22",
          "form": "오메",
          "original_form": "오메",
          "speaker_id": "1",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100025103.1.23",
          "form": "혹시 강남으로 직장 다니시나여",
          "original_form": "혹시 강남으로 직장 다니시나여",
          "speaker_id": "1",
          "time": "20210831 23:12"
        },
        {
          "id": "MDRW2100025103.1.24",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "2",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100025103.1.25",
          "form": "역시...",
          "original_form": "역시...",
          "speaker_id": "1",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100025103.1.26",
          "form": "저희 언니도 거기가 회사인데",
          "original_form": "저희 언니도 거기가 회사인데",
          "speaker_id": "1",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100025103.1.27",
          "form": "예전에 있던곳이 신사동",
          "original_form": "예전에 있던곳이 신사동",
          "speaker_id": "2",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100025103.1.28",
          "form": "비싸고 맛없다고 화내드라고요 매일",
          "original_form": "비싸고 맛없다고 화내드라고요 매일",
          "speaker_id": "1",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100025103.1.29",
          "form": "가로수길쪽인데",
          "original_form": "가로수길쪽인데",
          "speaker_id": "2",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100025103.1.30",
          "form": "가로수길이랑멀리떨어진곳이기도하고",
          "original_form": "가로수길이랑멀리떨어진곳이기도하고",
          "speaker_id": "2",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100025103.1.31",
          "form": "식당도 다 별로고 비싸더라구요",
          "original_form": "식당도 다 별로고 비싸더라구요",
          "speaker_id": "2",
          "time": "20210831 23:13"
        }
      ]
    }
  ]
