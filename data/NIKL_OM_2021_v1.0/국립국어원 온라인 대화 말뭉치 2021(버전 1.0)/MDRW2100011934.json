      "utterance": [
        {
          "id": "MDRW2100011934.1.1",
          "form": "최근에 극장가서 보신 영화있으신가요?",
          "original_form": "최근에 극장가서 보신 영화있으신가요?",
          "speaker_id": "2",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011934.1.2",
          "form": "블랙위도우 봤어요!",
          "original_form": "블랙위도우 봤어요!",
          "speaker_id": "1",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100011934.1.3",
          "form": "마블 좋아하시나봐요 저는 마블을 안봐서..",
          "original_form": "마블 좋아하시나봐요 저는 마블을 안봐서..",
          "speaker_id": "2",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011934.1.4",
          "form": "곧 개봉하는 샹치도 보러가시나요?",
          "original_form": "곧 개봉하는 샹치도 보러가시나요?",
          "speaker_id": "2",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011934.1.5",
          "form": "샹치는 조금 고민되네요ㅎㅎ",
          "original_form": "샹치는 조금 고민되네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100011934.1.6",
          "form": "다른 영화는 보신거 없으세요?",
          "original_form": "다른 영화는 보신거 없으세요?",
          "speaker_id": "2",
          "time": "20210824 18:11"
        },
        {
          "id": "MDRW2100011934.1.7",
          "form": "저는 최근에 프리가이 재밌게 봤어요",
          "original_form": "저는 최근에 프리가이 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210824 18:11"
        },
        {
          "id": "MDRW2100011934.1.8",
          "form": "헐 저 그거 보고싶었는데",
          "original_form": "헐 저 그거 보고싶었는데",
          "speaker_id": "1",
          "time": "20210824 18:13"
        },
        {
          "id": "MDRW2100011934.1.9",
          "form": "소재 자체가 재밌어서 그런지 영화도 지루하지 않았어요",
          "original_form": "소재 자체가 재밌어서 그런지 영화도 지루하지 않았어요",
          "speaker_id": "2",
          "time": "20210824 18:38"
        },
        {
          "id": "MDRW2100011934.1.10",
          "form": "지금 상영중인가요?",
          "original_form": "지금 상영중인가요?",
          "speaker_id": "1",
          "time": "20210824 18:41"
        },
        {
          "id": "MDRW2100011934.1.11",
          "form": "상영관이 적긴하지만 아직도 상영하더라구요!",
          "original_form": "상영관이 적긴하지만 아직도 상영하더라구요!",
          "speaker_id": "2",
          "time": "20210824 18:42"
        },
        {
          "id": "MDRW2100011934.1.12",
          "form": "오 보러가야겠어요!",
          "original_form": "오 보러가야겠어요!",
          "speaker_id": "1",
          "time": "20210824 18:50"
        },
        {
          "id": "MDRW2100011934.1.13",
          "form": "네네 재밌어요ㅎㅎ",
          "original_form": "네네 재밌어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 19:13"
        },
        {
          "id": "MDRW2100011934.1.14",
          "form": "평점도 괜찮네요ㅎㅎ",
          "original_form": "평점도 괜찮네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 19:59"
        }
