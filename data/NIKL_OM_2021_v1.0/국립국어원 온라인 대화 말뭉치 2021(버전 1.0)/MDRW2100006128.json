      "utterance": [
        {
          "id": "MDRW2100006128.1.1",
          "form": "계란을 친환경으로 바꿔봤는데",
          "original_form": "계란을 친환경으로 바꿔봤는데",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100006128.1.2",
          "form": "의외로 진짜 만족스럽더라구요",
          "original_form": "의외로 진짜 만족스럽더라구요",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100006128.1.3",
          "form": "아 유정란 이런거요?",
          "original_form": "아 유정란 이런거요?",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100006128.1.4",
          "form": "유정란은아닌데 친환경인증 계란이래요",
          "original_form": "유정란은아닌데 친환경인증 계란이래요",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100006128.1.5",
          "form": "근데 확실히 신선하고 안 비려요",
          "original_form": "근데 확실히 신선하고 안 비려요",
          "speaker_id": "2",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100006128.1.6",
          "form": "오 달걀맛이 다르군요 저는 깰때나 노른자 상태를 보면 다른게 느껴지기도...",
          "original_form": "오 달걀맛이 다르군요 저는 깰때나 노른자 상태를 보면 다른게 느껴지기도...",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100006128.1.7",
          "form": "그쵸 첨엔 동네계란값이 비싸져서 인쇼로구매한건데",
          "original_form": "그쵸 첨엔 동네계란값이 비싸져서 인쇼로구매한건데",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100006128.1.8",
          "form": "가격도 별로안비싸고 좋아서",
          "original_form": "가격도 별로안비싸고 좋아서",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100006128.1.9",
          "form": "이젠 거기서 계속사요",
          "original_form": "이젠 거기서 계속사요",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100006128.1.10",
          "form": "10구애 얼마정도인가요?",
          "original_form": "10구애 얼마정도인가요?",
          "speaker_id": "1",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100006128.1.11",
          "form": "아 저희는 가족이 많아서",
          "original_form": "아 저희는 가족이 많아서",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100006128.1.12",
          "form": "90구에 25500주고사요",
          "original_form": "90구에 25500주고사요",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100006128.1.13",
          "form": "헉..90구를 한번에 사시나요..",
          "original_form": "헉..90구를 한번에 사시나요..",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100006128.1.14",
          "form": "어 근데 그렇게 사도 한달 못먹어요ㅠ",
          "original_form": "어 근데 그렇게 사도 한달 못먹어요ㅠ",
          "speaker_id": "2",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100006128.1.15",
          "form": "부침개랑 김밥 같은거 해먹으면 금방이네요",
          "original_form": "부침개랑 김밥 같은거 해먹으면 금방이네요",
          "speaker_id": "2",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100006128.1.16",
          "form": "그러시군요 달걀 소비가 많으시네요 인터넷 사이트에서 사시나요?",
          "original_form": "그러시군요 달걀 소비가 많으시네요 인터넷 사이트에서 사시나요?",
          "speaker_id": "1",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100006128.1.17",
          "form": "네 네이버쇼핑으로사요",
          "original_form": "네 네이버쇼핑으로사요",
          "speaker_id": "2",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100006128.1.18",
          "form": "네이버쇼핑 쓰세요?",
          "original_form": "네이버쇼핑 쓰세요?",
          "speaker_id": "2",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100006128.1.19",
          "form": "네 ㅎㅎ 애용합니다",
          "original_form": "네 ㅎㅎ 애용합니다",
