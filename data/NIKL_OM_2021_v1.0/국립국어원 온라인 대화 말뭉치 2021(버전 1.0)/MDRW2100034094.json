      "utterance": [
        {
          "id": "MDRW2100034094.1.1",
          "form": "최근에 사신거 있으세요??",
          "original_form": "최근에 사신거 있으세요??",
          "speaker_id": "1",
          "time": "20210827 15:40"
        },
        {
          "id": "MDRW2100034094.1.2",
          "form": "전 최근에 당근으로 에그팬 샀어요 ㅋㅋㅋ",
          "original_form": "전 최근에 당근으로 에그팬 샀어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100034094.1.3",
          "form": "에그팬은 계란 붙어있는 펜인가요??",
          "original_form": "에그팬은 계란 붙어있는 펜인가요??",
          "speaker_id": "1",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100034094.1.4",
          "form": "아뇨 ㅋㅋㅋㅋ 붙어있는게 아니라 계란전용 프라이팬이에요~",
          "original_form": "아뇨 ㅋㅋㅋㅋ 붙어있는게 아니라 계란전용 프라이팬이에요~",
          "speaker_id": "2",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100034094.1.5",
          "form": "딱 계란하나 해먹기 좋은 사이즈에요",
          "original_form": "딱 계란하나 해먹기 좋은 사이즈에요",
          "speaker_id": "2",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100034094.1.6",
          "form": "헉 펜으류 봤네요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "헉 펜으류 봤네요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100034094.1.7",
          "form": "오 편할거같아요",
          "original_form": "오 편할거같아요",
          "speaker_id": "1",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100034094.1.8",
          "form": "앜ㅋㅋㅋ ㅋㅋㅋㅋ 볼펜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ ㅋㅋㅋㅋ 볼펜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100034094.1.9",
          "form": "네 편하고 좋더라구요ㅋㅋㅋㅋ",
          "original_form": "네 편하고 좋더라구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100034094.1.10",
          "form": "계랑후라이해먹기 딱 좋겠어요 ㅌㅋㅋ",
          "original_form": "계랑후라이해먹기 딱 좋겠어요 ㅌㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100034094.1.11",
          "form": "전 네모난 팬 갖고 싶더라구요 계란말이용으루 ㅋㅋㅋ",
          "original_form": "전 네모난 팬 갖고 싶더라구요 계란말이용으루 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:49"
        },
        {
          "id": "MDRW2100034094.1.12",
          "form": "아하! 저 그것도 살까생각중이에요 ㅋㅋ 정말 편해보이더라궁ㅛ",
          "original_form": "아하! 저 그것도 살까생각중이에요 ㅋㅋ 정말 편해보이더라궁ㅛ",
          "speaker_id": "2",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100034094.1.13",
          "form": "그쳐 개발한 사람 머리 진짜 좋은거같아요 싱크빅ㅋㅋㅋ",
          "original_form": "그쳐 개발한 사람 머리 진짜 좋은거같아요 싱크빅ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100034094.1.14",
          "form": "맞죠맞죠ㅋㅋㅋㅋㅋ",
          "original_form": "맞죠맞죠ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100034094.1.15",
          "form": "넵ㅋㅋㅋㅋ",
          "original_form": "넵ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100034094.1.16",
          "form": "흠 중고거래 해보셨나요?",
          "original_form": "흠 중고거래 해보셨나요?",
          "speaker_id": "2",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100034094.1.17",
          "form": "네 당근이나 중고나라 해봤어요 ㅋㅋㅋ",
          "original_form": "네 당근이나 중고나라 해봤어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100034094.1.18",
          "form": "오 역시 당근 요새 지분율 많이늘어난거같네요",
