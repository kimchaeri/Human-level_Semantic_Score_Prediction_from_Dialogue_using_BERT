      "utterance": [
        {
          "id": "MDRW2100035240.1.1",
          "form": "이제 점심 드시겠네여",
          "original_form": "이제 점심 드시겠네여",
          "speaker_id": "2",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100035240.1.2",
          "form": "네! 국수로 맘을 굳혔어요",
          "original_form": "네! 국수로 맘을 굳혔어요",
          "speaker_id": "1",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100035240.1.3",
          "form": "맛있겠네여ㅋㅋㅋ 잔치국슈요?",
          "original_form": "맛있겠네여ㅋㅋㅋ 잔치국슈요?",
          "speaker_id": "2",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100035240.1.4",
          "form": "골뱅이 비빔 국수요!",
          "original_form": "골뱅이 비빔 국수요!",
          "speaker_id": "1",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100035240.1.5",
          "form": "이따 라멘드신댔죠?!",
          "original_form": "이따 라멘드신댔죠?!",
          "speaker_id": "1",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100035240.1.6",
          "form": "와 맛잇는거 드셨네요... 네 저 오늘을 고대했어요 츠케멘",
          "original_form": "와 맛잇는거 드셨네요... 네 저 오늘을 고대했어요 츠케멘",
          "speaker_id": "2",
          "time": "20210829 13:29"
        },
        {
          "id": "MDRW2100035240.1.7",
          "form": "츠케멘 맛있길 저도 기대중입니닷,",
          "original_form": "츠케멘 맛있길 저도 기대중입니닷,",
          "speaker_id": "1",
          "time": "20210829 13:30"
        },
        {
          "id": "MDRW2100035240.1.8",
          "form": "주말에 면은 필수랄까요?!ㅋㅋ",
          "original_form": "주말에 면은 필수랄까요?!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:30"
        },
        {
          "id": "MDRW2100035240.1.9",
          "form": "맞아요 오 아직 안드셨나요",
          "original_form": "맞아요 오 아직 안드셨나요",
          "speaker_id": "2",
          "time": "20210829 13:31"
        },
        {
          "id": "MDRW2100035240.1.10",
          "form": "저는 예전에 먹었더니 다시 먹고싶네요ㅋㅋ",
          "original_form": "저는 예전에 먹었더니 다시 먹고싶네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:36"
        },
        {
          "id": "MDRW2100035240.1.11",
          "form": "ㅋㅋㅋㅋㅋ넹 기대기대",
          "original_form": "ㅋㅋㅋㅋㅋ넹 기대기대",
          "speaker_id": "2",
          "time": "20210829 13:39"
        },
        {
          "id": "MDRW2100035240.1.12",
          "form": "이따 면 먹을거지만",
          "original_form": "이따 면 먹을거지만",
          "speaker_id": "2",
          "time": "20210829 13:39"
        },
        {
          "id": "MDRW2100035240.1.13",
          "form": "신라면 볶음면이 궁금해서 끓이는 중이에요",
          "original_form": "신라면 볶음면이 궁금해서 끓이는 중이에요",
          "speaker_id": "2",
          "time": "20210829 13:39"
        },
        {
          "id": "MDRW2100035240.1.14",
          "form": "오?ㅋㅋㄱ 신라면이 발전했네요",
          "original_form": "오?ㅋㅋㄱ 신라면이 발전했네요",
          "speaker_id": "1",
          "time": "20210829 13:41"
        },
        {
          "id": "MDRW2100035240.1.15",
          "form": "볶음면이 나왔군욥",
          "original_form": "볶음면이 나왔군욥",
          "speaker_id": "1",
          "time": "20210829 13:41"
        },
        {
          "id": "MDRW2100035240.1.16",
          "form": "네네 특이하게 맛있네요",
          "original_form": "네네 특이하게 맛있네요",
          "speaker_id": "2",
          "time": "20210829 13:48"
        },
        {
          "id": "MDRW2100035240.1.17",
          "form": "오옹 궁금하네요ㅋㅋ",
          "original_form": "오옹 궁금하네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100035240.1.18",
          "form": "모르는새 라면도 여러가지 나와네요",
          "original_form": "모르는새 라면도 여러가지 나와네요",
          "speaker_id": "1",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100035240.1.19",
          "form": "좀 맵긴해요 ㅋㅋㅋㅋ 눈물콧물 줄줄",
          "original_form": "좀 맵긴해요 ㅋㅋㅋㅋ 눈물콧물 줄줄",
          "speaker_id": "2",
          "time": "20210829 13:54"
        },
        {
          "id": "MDRW2100035240.1.20",
          "form": "ㅋㅋㅋㅋ맵찔이지만 도전하고싶네요",
          "original_form": "ㅋㅋㅋㅋ맵찔이지만 도전하고싶네요",
          "speaker_id": "1",
