      "utterance": [
        {
          "id": "MDRW2100000340.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210819 09:40"
        },
        {
          "id": "MDRW2100000340.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210819 09:50"
        },
        {
          "id": "MDRW2100000340.1.3",
          "form": "지금 벤앤제리스에서 30프로 할인하던데",
          "original_form": "지금 벤앤제리스에서 30프로 할인하던데",
          "speaker_id": "1",
          "time": "20210819 09:54"
        },
        {
          "id": "MDRW2100000340.1.4",
          "form": "저는 초콜릿칩 쿠키도우 파인트로 사먹으려고요",
          "original_form": "저는 초콜릿칩 쿠키도우 파인트로 사먹으려고요",
          "speaker_id": "1",
          "time": "20210819 09:55"
        },
        {
          "id": "MDRW2100000340.1.5",
          "form": "벤앤제리너스가 뭐예요?",
          "original_form": "벤앤제리너스가 뭐예요?",
          "speaker_id": "2",
          "time": "20210819 09:56"
        },
        {
          "id": "MDRW2100000340.1.6",
          "form": "외국 아이스크림 브랜드요!",
          "original_form": "외국 아이스크림 브랜드요!",
          "speaker_id": "1",
          "time": "20210819 09:59"
        },
        {
          "id": "MDRW2100000340.1.7",
          "form": "편의점에 있는데",
          "original_form": "편의점에 있는데",
          "speaker_id": "1",
          "time": "20210819 09:59"
        },
        {
          "id": "MDRW2100000340.1.8",
          "form": "한번쯤은 먹어보고 싶었는데",
          "original_form": "한번쯤은 먹어보고 싶었는데",
          "speaker_id": "1",
          "time": "20210819 09:59"
        },
        {
          "id": "MDRW2100000340.1.9",
          "form": "너무 비싸서 할인할때까지 사먹는건 미뤄두고 있었거든요",
          "original_form": "너무 비싸서 할인할때까지 사먹는건 미뤄두고 있었거든요",
          "speaker_id": "1",
          "time": "20210819 10:00"
        },
        {
          "id": "MDRW2100000340.1.10",
          "form": "오 할인전은 얼만데요?",
          "original_form": "오 할인전은 얼만데요?",
          "speaker_id": "2",
          "time": "20210819 10:00"
        },
        {
          "id": "MDRW2100000340.1.11",
          "form": "파인트 개당 정가 11200원이요..",
          "original_form": "파인트 개당 정가 11200원이요..",
          "speaker_id": "1",
          "time": "20210819 10:01"
        },
        {
          "id": "MDRW2100000340.1.12",
          "form": "인터넷으로 사면 싸네요!",
          "original_form": "인터넷으로 사면 싸네요!",
          "speaker_id": "2",
          "time": "20210819 10:02"
        },
        {
          "id": "MDRW2100000340.1.13",
          "form": "정가 11200원에서 30프로 할인해서",
          "original_form": "정가 11200원에서 30프로 할인해서",
          "speaker_id": "1",
          "time": "20210819 10:03"
        },
        {
          "id": "MDRW2100000340.1.14",
          "form": "지금은 개당 8120원이에요!",
          "original_form": "지금은 개당 8120원이에요!",
          "speaker_id": "1",
          "time": "20210819 10:03"
        },
        {
          "id": "MDRW2100000340.1.15",
          "form": "베라가 더 좋은거같아요!",
          "original_form": "베라가 더 좋은거같아요!",
          "speaker_id": "2",
          "time": "20210819 10:11"
        },
        {
          "id": "MDRW2100000340.1.16",
          "form": "ㅋㅋ저도 그 생각에",
          "original_form": "ㅋㅋ저도 그 생각에",
          "speaker_id": "1",
          "time": "20210819 10:11"
        },
        {
          "id": "MDRW2100000340.1.17",
          "form": "그냥 안 사먹고 베라 사먹었는데",
          "original_form": "그냥 안 사먹고 베라 사먹었는데",
          "speaker_id": "1",
          "time": "20210819 10:11"
        },
        {
          "id": "MDRW2100000340.1.18",
          "form": "한국론칭 2주년 기념으로 할인하는거라",
          "original_form": "한국론칭 2주년 기념으로 할인하는거라",
          "speaker_id": "1",
          "time": "20210819 10:13"
        },
        {
          "id": "MDRW2100000340.1.19",
          "form": "베라 네고할때 먹었어요?",
          "original_form": "베라 네고할때 먹었어요?",
          "speaker_id": "2",
          "time": "20210819 10:13"
        },
        {
          "id": "MDRW2100000340.1.20",
          "form": "아뇨 실상 31일 할인같이 정기 할인하는거랑",
          "original_form": "아뇨 실상 31일 할인같이 정기 할인하는거랑",
          "speaker_id": "1",
          "time": "20210819 10:13"
        },
        {
          "id": "MDRW2100000340.1.21",
          "form": "별다른거 없다고 해서",
          "original_form": "별다른거 없다고 해서",
          "speaker_id": "1",
          "time": "20210819 10:14"
        },
        {
          "id": "MDRW2100000340.1.22",
          "form": "저는 그냥 이번달 말에 사먹으려고요",
          "original_form": "저는 그냥 이번달 말에 사먹으려고요",
          "speaker_id": "1",
          "time": "20210819 10:14"
        },
        {
          "id": "MDRW2100000340.1.23",
          "form": "아 저도 그때는 못먹고 기프티콘있어서 먹었어요~",
          "original_form": "아 저도 그때는 못먹고 기프티콘있어서 먹었어요~",
          "speaker_id": "2",
          "time": "20210819 10:15"
        },
        {
          "id": "MDRW2100000340.1.24",
          "form": "지금 일해요? 학생이예요?",
          "original_form": "지금 일해요? 학생이예요?",
          "speaker_id": "2",
          "time": "20210819 10:16"
        },
        {
          "id": "MDRW2100000340.1.25",
          "form": "지금 병원 입사 대기중이에요",
          "original_form": "지금 병원 입사 대기중이에요",
          "speaker_id": "1",
          "time": "20210819 10:17"
        },
        {
          "id": "MDRW2100000340.1.26",
          "form": "name2님께서는 이번달말에 베라 사드실거에요?",
          "original_form": "&name2&님께서는 이번달말에 베라 사드실거에요?",
          "speaker_id": "1",
          "time": "20210819 10:18"
        },
        {
          "id": "MDRW2100000340.1.27",
          "form": "아 이번달이 31이군요",
          "original_form": "아 이번달이 31이군요",
          "speaker_id": "2",
          "time": "20210819 10:45"
        },
        {
          "id": "MDRW2100000340.1.28",
          "form": "간호사세요?",
          "original_form": "간호사세요?",
          "speaker_id": "2",
          "time": "20210819 10:45"
        },
        {
          "id": "MDRW2100000340.1.29",
          "form": "저도간호사랑 일해요 병원은아니고요",
          "original_form": "저도간호사랑 일해요 병원은아니고요",
          "speaker_id": "2",
          "time": "20210819 10:45"
        },
        {
          "id": "MDRW2100000340.1.30",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210819 10:47"
        },
        {
          "id": "MDRW2100000340.1.31",
          "form": "이제 갓졸업생인가요?",
          "original_form": "이제 갓졸업생인가요?",
          "speaker_id": "2",
          "time": "20210819 10:49"
        },
        {
          "id": "MDRW2100000340.1.32",
          "form": "앗 저.. 대화 주제에만 집중하면 안될까요?ㅠㅠ",
          "original_form": "앗 저.. 대화 주제에만 집중하면 안될까요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 10:49"
        },
        {
          "id": "MDRW2100000340.1.33",
          "form": "주제와 관련없는 대화는 보상받을 수 없다고 해서요",
          "original_form": "주제와 관련없는 대화는 보상받을 수 없다고 해서요",
          "speaker_id": "1",
          "time": "20210819 10:50"
        },
        {
          "id": "MDRW2100000340.1.34",
          "form": "네 ㅎ",
          "original_form": "네 ㅎ",
          "speaker_id": "2",
          "time": "20210819 10:53"
        },
        {
          "id": "MDRW2100000340.1.35",
          "form": "뭐 구매하는거좋아해요?",
          "original_form": "뭐 구매하는거좋아해요?",
          "speaker_id": "2",
          "time": "20210819 10:53"
        },
        {
          "id": "MDRW2100000340.1.36",
          "form": "쇼핑은 어디서하세요? 브랜디? 지그재그?",
          "original_form": "쇼핑은 어디서하세요? 브랜디? 지그재그?",
          "speaker_id": "2",
          "time": "20210819 10:53"
        },
        {
          "id": "MDRW2100000340.1.37",
          "form": "좋아하는 쇼핑몰있어요?",
          "original_form": "좋아하는 쇼핑몰있어요?",
          "speaker_id": "2",
          "time": "20210819 10:53"
        },
        {
          "id": "MDRW2100000340.1.38",
          "form": "브랜디요!",
