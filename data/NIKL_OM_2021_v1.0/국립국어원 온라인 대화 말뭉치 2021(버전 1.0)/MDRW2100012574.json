      "utterance": [
        {
          "id": "MDRW2100012574.1.1",
          "form": "게임은 자주하시나요?",
          "original_form": "게임은 자주하시나요?",
          "speaker_id": "2",
          "time": "20210825 12:52"
        },
        {
          "id": "MDRW2100012574.1.2",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210825 12:59"
        },
        {
          "id": "MDRW2100012574.1.3",
          "form": "모바일게임정도요?",
          "original_form": "모바일게임정도요?",
          "speaker_id": "1",
          "time": "20210825 12:59"
        },
        {
          "id": "MDRW2100012574.1.4",
          "form": "아 요즘 재미있는 거 있나요?",
          "original_form": "아 요즘 재미있는 거 있나요?",
          "speaker_id": "2",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100012574.1.5",
          "form": "ㅋㅋㅋ전그냥",
          "original_form": "ㅋㅋㅋ전그냥",
          "speaker_id": "1",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100012574.1.6",
          "form": "카트 잠깐합니다",
          "original_form": "카트 잠깐합니다",
          "speaker_id": "1",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100012574.1.7",
          "form": "아 카트도 모발이 있나보네요",
          "original_form": "아 카트도 모발이 있나보네요",
          "speaker_id": "2",
          "time": "20210825 16:56"
        },
        {
          "id": "MDRW2100012574.1.8",
          "form": "ㅋㅋㅋ 꽤됐답니다",
          "original_form": "ㅋㅋㅋ 꽤됐답니다",
          "speaker_id": "1",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100012574.1.9",
          "form": "게임하시나요?",
          "original_form": "게임하시나요?",
          "speaker_id": "1",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100012574.1.10",
          "form": "저는 심즈랑 트로피카 같은거해용",
          "original_form": "저는 심즈랑 트로피카 같은거해용",
          "speaker_id": "2",
          "time": "20210825 17:10"
        },
        {
          "id": "MDRW2100012574.1.11",
          "form": "컴퓨터게임이요!ㅋㅋ",
          "original_form": "컴퓨터게임이요!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:10"
        },
        {
          "id": "MDRW2100012574.1.12",
          "form": "트로피카는",
          "original_form": "트로피카는",
          "speaker_id": "1",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100012574.1.13",
          "form": "먼가요?",
          "original_form": "먼가요?",
          "speaker_id": "1",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100012574.1.14",
          "form": "독재자가 나라를 지배하면서 점점 발전시키는 거예요",
          "original_form": "독재자가 나라를 지배하면서 점점 발전시키는 거예요",
          "speaker_id": "2",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100012574.1.15",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100012574.1.16",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "1",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100012574.1.17",
          "form": "넵넵 근데 심즈가 더 재밌는거 같아요",
          "original_form": "넵넵 근데 심즈가 더 재밌는거 같아요",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100012574.1.18",
          "form": "심즈는",
          "original_form": "심즈는",
          "speaker_id": "1",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100012574.1.19",
          "form": "과금게임인가요?",
          "original_form": "과금게임인가요?",
          "speaker_id": "1",
          "time": "20210825 21:51"
        },
        {
          "id": "MDRW2100012574.1.20",
          "form": "아니요 컴퓨터게임인데 확장팩이 수십개라 다 갖추면 수십만원 정도하는거 같아요",
          "original_form": "아니요 컴퓨터게임인데 확장팩이 수십개라 다 갖추면 수십만원 정도하는거 같아요",
          "speaker_id": "2",
          "time": "20210825 21:52"
        }
      ]
