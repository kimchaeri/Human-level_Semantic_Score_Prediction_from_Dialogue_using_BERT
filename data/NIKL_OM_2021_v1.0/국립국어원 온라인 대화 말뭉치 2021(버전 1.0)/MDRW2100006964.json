      "utterance": [
        {
          "id": "MDRW2100006964.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210822 00:05"
        },
        {
          "id": "MDRW2100006964.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006964.1.3",
          "form": "하는 게임있으신가요?",
          "original_form": "하는 게임있으신가요?",
          "speaker_id": "2",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006964.1.4",
          "form": "전 닌텐도 스위치랑 스팀은 폴가이즈요!",
          "original_form": "전 닌텐도 스위치랑 스팀은 폴가이즈요!",
          "speaker_id": "1",
          "time": "20210822 00:06"
        },
        {
          "id": "MDRW2100006964.1.5",
          "form": "name2님은 어떤 게임하세요?",
          "original_form": "&name2&님은 어떤 게임하세요?",
          "speaker_id": "1",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006964.1.6",
          "form": "저 요즘은 롤이요!",
          "original_form": "저 요즘은 롤이요!",
          "speaker_id": "2",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006964.1.7",
          "form": "전에는 오버워치랑 동숲했었어요",
          "original_form": "전에는 오버워치랑 동숲했었어요",
          "speaker_id": "2",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006964.1.8",
          "form": "오 동숲 저도 한때 열심히 했었어요ㅋㅋㅋ",
          "original_form": "오 동숲 저도 한때 열심히 했었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:07"
        },
        {
          "id": "MDRW2100006964.1.9",
          "form": "롤은 방송은 여러번봤는데 넘 복잡하더라구요",
          "original_form": "롤은 방송은 여러번봤는데 넘 복잡하더라구요",
          "speaker_id": "1",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006964.1.10",
          "form": "ㅋㅋㅋㅋ 맞아요 롤 너무 어려워요 저도 잘못해요",
          "original_form": "ㅋㅋㅋㅋ 맞아요 롤 너무 어려워요 저도 잘못해요",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006964.1.11",
          "form": "동숲 힐링이긴하지만 한번 잊어먹으면 그대로 안하게되는 것같아요 ㅋㅋㅋㅋ",
          "original_form": "동숲 힐링이긴하지만 한번 잊어먹으면 그대로 안하게되는 것같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:08"
        },
        {
          "id": "MDRW2100006964.1.12",
          "form": "맞아요 저 몇달만에 들어갔더니 동물들이 잊어버릴뻔했다고 그러더라구요..ㅋㅋㅋㅋ",
          "original_form": "맞아요 저 몇달만에 들어갔더니 동물들이 잊어버릴뻔했다고 그러더라구요..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:09"
        },
        {
          "id": "MDRW2100006964.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ저도 그랬어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저도 그랬어요",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006964.1.14",
          "form": "폴 가이즈는 어떤 게임이에요?",
          "original_form": "폴 가이즈는 어떤 게임이에요?",
          "speaker_id": "2",
          "time": "20210822 00:11"
        },
        {
          "id": "MDRW2100006964.1.15",
          "form": "60명이서 출발드림팀같은 게임 같이 하면서 우승 목표로 하는 게임인데 캐릭터들 귀염뽀짝하고 재밌어요!",
          "original_form": "60명이서 출발드림팀같은 게임 같이 하면서 우승 목표로 하는 게임인데 캐릭터들 귀염뽀짝하고 재밌어요!",
          "speaker_id": "1",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006964.1.16",
          "form": "동숲하셨으면 폴가이즈도 좋아하실것같아요!",
          "original_form": "동숲하셨으면 폴가이즈도 좋아하실것같아요!",
          "speaker_id": "1",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006964.1.17",
          "form": "헉 방금 검색해봤는데 너무 귀여워요",
          "original_form": "헉 방금 검색해봤는데 너무 귀여워요",
          "speaker_id": "2",
          "time": "20210822 00:12"
        },
        {
          "id": "MDRW2100006964.1.18",
          "form": "그쵸 경험치쌓이면 의상주는데 의상들도 넘 귀여워요ㅋㅋㅋ",
          "original_form": "그쵸 경험치쌓이면 의상주는데 의상들도 넘 귀여워요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:13"
        },
        {
          "id": "MDRW2100006964.1.19",
          "form": "우와 다음에 꼭 해봐야겠어요!",
          "original_form": "우와 다음에 꼭 해봐야겠어요!",
          "speaker_id": "2",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006964.1.20",
          "form": "게임방송도 자주보세요?",
          "original_form": "게임방송도 자주보세요?",
          "speaker_id": "2",
          "time": "20210822 00:14"
        },
        {
          "id": "MDRW2100006964.1.21",
          "form": "엇 네 지금도 보고있어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "엇 네 지금도 보고있어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:14"
        }
      ]
    }
