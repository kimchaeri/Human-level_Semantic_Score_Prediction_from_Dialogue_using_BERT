      "utterance": [
        {
          "id": "MDRW2100012413.1.1",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 21:53"
        },
        {
          "id": "MDRW2100012413.1.2",
          "form": "폰으로 하는 퍼즐 게임 좋아해요 애니팡이나 꿈의정원이요",
          "original_form": "폰으로 하는 퍼즐 게임 좋아해요 애니팡이나 꿈의정원이요",
          "speaker_id": "2",
          "time": "20210824 21:54"
        },
        {
          "id": "MDRW2100012413.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210824 21:54"
        },
        {
          "id": "MDRW2100012413.1.4",
          "form": "아까 대머리얘기 하신분인가요?",
          "original_form": "아까 대머리얘기 하신분인가요?",
          "speaker_id": "1",
          "time": "20210824 21:55"
        },
        {
          "id": "MDRW2100012413.1.5",
          "form": "네 ㅋㅋ 그 대머리요",
          "original_form": "네 ㅋㅋ 그 대머리요",
          "speaker_id": "2",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012413.1.6",
          "form": "데바데였나 하신댔죠? 어떤 장르인가요?",
          "original_form": "데바데였나 하신댔죠? 어떤 장르인가요?",
          "speaker_id": "2",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012413.1.7",
          "form": "ㅋㅋㅋㅋ 애니팡 최근에 하셨나요?",
          "original_form": "ㅋㅋㅋㅋ 애니팡 최근에 하셨나요?",
          "speaker_id": "1",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012413.1.8",
          "form": "안무서운 공포 게임입니다",
          "original_form": "안무서운 공포 게임입니다",
          "speaker_id": "1",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012413.1.9",
          "form": "애니팡은 예전에요 ㅋㅋ 아니 공포게임인데 안무서우면 안공포 아닌가요?",
          "original_form": "애니팡은 예전에요 ㅋㅋ 아니 공포게임인데 안무서우면 안공포 아닌가요?",
          "speaker_id": "2",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100012413.1.10",
          "form": "술래잡기 게임 비슷한데",
          "original_form": "술래잡기 게임 비슷한데",
          "speaker_id": "1",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100012413.1.11",
          "form": "술래를 놀리는 맛이 있어서",
          "original_form": "술래를 놀리는 맛이 있어서",
          "speaker_id": "1",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100012413.1.12",
          "form": "장르가 공포에서 코믹으로 바뀝니다",
          "original_form": "장르가 공포에서 코믹으로 바뀝니다",
          "speaker_id": "1",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100012413.1.13",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210824 22:00"
        },
        {
          "id": "MDRW2100012413.1.14",
          "form": "능숙해지면 코믹이 되나봐요!",
          "original_form": "능숙해지면 코믹이 되나봐요!",
          "speaker_id": "2",
          "time": "20210824 22:01"
        },
        {
          "id": "MDRW2100012413.1.15",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:01"
        },
        {
          "id": "MDRW2100012413.1.16",
          "form": "자주보는 스트리머 있으신가요?",
          "original_form": "자주보는 스트리머 있으신가요?",
          "speaker_id": "1",
          "time": "20210824 22:02"
        },
        {
          "id": "MDRW2100012413.1.17",
          "form": "아뇨 스트리머는 잘 안챙겨봐요 ㅋㅋ",
          "original_form": "아뇨 스트리머는 잘 안챙겨봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100012413.1.18",
          "form": "데바데 스트리머중에 추천해줄만한 분 계신가요?",
          "original_form": "데바데 스트리머중에 추천해줄만한 분 계신가요?",
          "speaker_id": "2",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100012413.1.19",
          "form": "궁금해졌어요 ㅋㅋㅋ",
          "original_form": "궁금해졌어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100012413.1.20",
          "form": "데바데 전문 스트리머는 아니지만",
          "original_form": "데바데 전문 스트리머는 아니지만",
          "speaker_id": "1",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100012413.1.21",
          "form": "짬타수아랑",
          "original_form": "짬타수아랑",
          "speaker_id": "1",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100012413.1.22",
          "form": "자빱이요",
          "original_form": "자빱이요",
          "speaker_id": "1",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100012413.1.23",
          "form": "자빱티비는 들어봤아요! 두분 플레이 한번 찾아봐야겠네요",
          "original_form": "자빱티비는 들어봤아요! 두분 플레이 한번 찾아봐야겠네요",
          "speaker_id": "2",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100012413.1.24",
          "form": "한 게임만 전문으로 하는 스트리머들도 있나보네요? 신기해라",
          "original_form": "한 게임만 전문으로 하는 스트리머들도 있나보네요? 신기해라",
          "speaker_id": "2",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100012413.1.25",
          "form": "그런분들은 완전 고수더라고요ㅋㅋ",
          "original_form": "그런분들은 완전 고수더라고요ㅋㅋ",
