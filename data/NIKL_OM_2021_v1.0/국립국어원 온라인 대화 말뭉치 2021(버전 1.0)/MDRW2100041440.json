      "utterance": [
        {
          "id": "MDRW2100041440.1.1",
          "form": "맞다 나 에프킬라 사야돼",
          "original_form": "맞다 나 에프킬라 사야돼",
          "speaker_id": "2",
          "time": "20210905 23:43"
        },
        {
          "id": "MDRW2100041440.1.2",
          "form": "에프킬라가 왜 사야 되는데?",
          "original_form": "에프킬라가 왜 사야 되는데?",
          "speaker_id": "1",
          "time": "20210905 23:44"
        },
        {
          "id": "MDRW2100041440.1.3",
          "form": "마트에서 사",
          "original_form": "마트에서 사",
          "speaker_id": "1",
          "time": "20210905 23:44"
        },
        {
          "id": "MDRW2100041440.1.4",
          "form": "얼마 전에 집에서 벌레 나왓어",
          "original_form": "얼마 전에 집에서 벌레 나왓어",
          "speaker_id": "2",
          "time": "20210905 23:44"
        },
        {
          "id": "MDRW2100041440.1.5",
          "form": "마트에서 사면 비싸잖아",
          "original_form": "마트에서 사면 비싸잖아",
          "speaker_id": "2",
          "time": "20210905 23:44"
        },
        {
          "id": "MDRW2100041440.1.6",
          "form": "바퀴벌레면 에프킬라 소용없어",
          "original_form": "바퀴벌레면 에프킬라 소용없어",
          "speaker_id": "1",
          "time": "20210905 23:45"
        },
        {
          "id": "MDRW2100041440.1.7",
          "form": "비오킬 같은 걸로 사야돼",
          "original_form": "비오킬 같은 걸로 사야돼",
          "speaker_id": "1",
          "time": "20210905 23:45"
        },
        {
          "id": "MDRW2100041440.1.8",
          "form": "비오킬.. 그거 약국에 파나?",
          "original_form": "비오킬.. 그거 약국에 파나?",
          "speaker_id": "2",
          "time": "20210905 23:45"
        },
        {
          "id": "MDRW2100041440.1.9",
          "form": "어 인터넷에도 팔긴 하는데 급한거면 마트나 약국에서 사",
          "original_form": "어 인터넷에도 팔긴 하는데 급한거면 마트나 약국에서 사",
          "speaker_id": "1",
          "time": "20210905 23:45"
        },
        {
          "id": "MDRW2100041440.1.10",
          "form": "오키",
          "original_form": "오키",
          "speaker_id": "2",
          "time": "20210905 23:49"
        },
        {
          "id": "MDRW2100041440.1.11",
          "form": "엄마 전기장판은 안 사도 돼?",
          "original_form": "엄마 전기장판은 안 사도 돼?",
          "speaker_id": "2",
          "time": "20210905 23:49"
        },
        {
          "id": "MDRW2100041440.1.12",
          "form": "왜. 너네집 장판 고장나서 사야돼?",
          "original_form": "왜. 너네집 장판 고장나서 사야돼?",
          "speaker_id": "1",
          "time": "20210905 23:49"
        },
        {
          "id": "MDRW2100041440.1.13",
          "form": "아니 내 거는 산지 1년밖에 안돼서 멀쩡해",
          "original_form": "아니 내 거는 산지 1년밖에 안돼서 멀쩡해",
          "speaker_id": "2",
          "time": "20210905 23:50"
        },
        {
          "id": "MDRW2100041440.1.14",
          "form": "작년에 본가에 있는 장판이 너무 딱딱하다고 해서",
          "original_form": "작년에 본가에 있는 장판이 너무 딱딱하다고 해서",
          "speaker_id": "2",
          "time": "20210905 23:50"
        },
        {
          "id": "MDRW2100041440.1.15",
          "form": "올해 새로 살까 싶어서!",
          "original_form": "올해 새로 살까 싶어서!",
          "speaker_id": "2",
          "time": "20210905 23:51"
        },
        {
          "id": "MDRW2100041440.1.16",
          "form": "사려면 겨울 전에 세일할 때 사는 게 좋긴 하지",
          "original_form": "사려면 겨울 전에 세일할 때 사는 게 좋긴 하지",
          "speaker_id": "1",
          "time": "20210905 23:51"
        },
        {
          "id": "MDRW2100041440.1.17",
          "form": "아빠한테 물어볼게",
          "original_form": "아빠한테 물어볼게",
          "speaker_id": "1",
          "time": "20210905 23:51"
        },
        {
          "id": "MDRW2100041440.1.18",
          "form": "응ㅋㅋㅋ 나도 겨울 이불 사야될 거 같아서 같이 몰아서 사려고",
          "original_form": "응ㅋㅋㅋ 나도 겨울 이불 사야될 거 같아서 같이 몰아서 사려고",
          "speaker_id": "2",
          "time": "20210905 23:52"
        },
        {
          "id": "MDRW2100041440.1.19",
          "form": "아 너 여름이불밖에 안 가져 갔지",
          "original_form": "아 너 여름이불밖에 안 가져 갔지",
          "speaker_id": "1",
          "time": "20210905 23:52"
        },
        {
          "id": "MDRW2100041440.1.20",
          "form": "겨울이불 얇은 거 사지 말고 비싼 거로 사",
          "original_form": "겨울이불 얇은 거 사지 말고 비싼 거로 사",
          "speaker_id": "1",
          "time": "20210905 23:52"
        },
        {
          "id": "MDRW2100041440.1.21",
          "form": "구스 이불 같은 거 괜찮나?",
          "original_form": "구스 이불 같은 거 괜찮나?",
          "speaker_id": "2",
          "time": "20210905 23:52"
        },
        {
          "id": "MDRW2100041440.1.22",
          "form": "집 주변에 이불 가게가 없어서 인터넷에서 사야될 거 같은데",
          "original_form": "집 주변에 이불 가게가 없어서 인터넷에서 사야될 거 같은데",
          "speaker_id": "2",
          "time": "20210905 23:53"
        },
        {
          "id": "MDRW2100041440.1.23",
          "form": "이불은 직접 보고 사는 게 좋은데.",
          "original_form": "이불은 직접 보고 사는 게 좋은데.",
          "speaker_id": "1",
          "time": "20210905 23:53"
        },
        {
          "id": "MDRW2100041440.1.24",
          "form": "엄마가 차 가지고 나갈테니까 같이 백화점 가서 보고 사던지",
          "original_form": "엄마가 차 가지고 나갈테니까 같이 백화점 가서 보고 사던지",
          "speaker_id": "1",
          "time": "20210905 23:53"
        },
        {
          "id": "MDRW2100041440.1.25",
          "form": "우왕 우리 신세계 갈까??",
          "original_form": "우왕 우리 신세계 갈까??",
          "speaker_id": "2",
          "time": "20210905 23:54"
        },
        {
          "id": "MDRW2100041440.1.26",
          "form": "거기 구경할 거 많대서 가보고 싶었어",
          "original_form": "거기 구경할 거 많대서 가보고 싶었어",
          "speaker_id": "2",
          "time": "20210905 23:54"
        },
        {
          "id": "MDRW2100041440.1.27",
          "form": "가는 김에 이불도 사는 거지~~",
          "original_form": "가는 김에 이불도 사는 거지~~",
          "speaker_id": "2",
          "time": "20210905 23:54"
        }
      ]
    }
