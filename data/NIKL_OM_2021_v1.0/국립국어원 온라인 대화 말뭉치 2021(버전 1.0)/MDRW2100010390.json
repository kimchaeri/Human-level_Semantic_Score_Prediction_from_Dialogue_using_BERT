      "utterance": [
        {
          "id": "MDRW2100010390.1.1",
          "form": "여름휴가는 잘 다녀왔니?",
          "original_form": "여름휴가는 잘 다녀왔니?",
          "speaker_id": "2",
          "time": "20210823 17:25"
        },
        {
          "id": "MDRW2100010390.1.2",
          "form": "요즘같은 시국에 미국이라니..대단해",
          "original_form": "요즘같은 시국에 미국이라니..대단해",
          "speaker_id": "2",
          "time": "20210823 17:25"
        },
        {
          "id": "MDRW2100010390.1.3",
          "form": "아무래도 일이 있어서 간거니까...",
          "original_form": "아무래도 일이 있어서 간거니까...",
          "speaker_id": "1",
          "time": "20210823 17:41"
        },
        {
          "id": "MDRW2100010390.1.4",
          "form": "학회 참석을 위해서 간거라 간김에 좀 놀다가 왔지",
          "original_form": "학회 참석을 위해서 간거라 간김에 좀 놀다가 왔지",
          "speaker_id": "1",
          "time": "20210823 17:42"
        },
        {
          "id": "MDRW2100010390.1.5",
          "form": "미팅은 잘 했어?",
          "original_form": "미팅은 잘 했어?",
          "speaker_id": "2",
          "time": "20210823 17:42"
        },
        {
          "id": "MDRW2100010390.1.6",
          "form": "멋있는걸.",
          "original_form": "멋있는걸.",
          "speaker_id": "2",
          "time": "20210823 17:42"
        },
        {
          "id": "MDRW2100010390.1.7",
          "form": "너는 오로라 보러 간다더니 잘 다녀왔니?",
          "original_form": "너는 오로라 보러 간다더니 잘 다녀왔니?",
          "speaker_id": "1",
          "time": "20210823 17:42"
        },
        {
          "id": "MDRW2100010390.1.8",
          "form": "어디갔었어?",
          "original_form": "어디갔었어?",
          "speaker_id": "2",
          "time": "20210823 17:42"
        },
        {
          "id": "MDRW2100010390.1.9",
          "form": "오로라.. 아무래도 코로나시국이라.. 격리만 해잇었네",
          "original_form": "오로라.. 아무래도 코로나시국이라.. 격리만 해잇었네",
          "speaker_id": "2",
          "time": "20210823 17:42"
        },
        {
          "id": "MDRW2100010390.1.10",
          "form": "아쉽다.",
          "original_form": "아쉽다.",
          "speaker_id": "2",
          "time": "20210823 17:42"
        },
        {
          "id": "MDRW2100010390.1.11",
          "form": "다음에 같이 가지 않을래?",
          "original_form": "다음에 같이 가지 않을래?",
          "speaker_id": "2",
          "time": "20210823 17:42"
        },
        {
          "id": "MDRW2100010390.1.12",
          "form": "응 좋아 너무 궁금해",
          "original_form": "응 좋아 너무 궁금해",
          "speaker_id": "1",
          "time": "20210823 17:43"
        },
        {
          "id": "MDRW2100010390.1.13",
          "form": "한국에서는 볼 수없눈걸 보는게",
          "original_form": "한국에서는 볼 수없눈걸 보는게",
          "speaker_id": "1",
          "time": "20210823 17:43"
        },
        {
          "id": "MDRW2100010390.1.14",
          "form": "맞아.",
          "original_form": "맞아.",
          "speaker_id": "2",
          "time": "20210823 17:43"
        },
        {
          "id": "MDRW2100010390.1.15",
          "form": "해외여행의 즐거움이지",
          "original_form": "해외여행의 즐거움이지",
          "speaker_id": "1",
          "time": "20210823 17:43"
        },
        {
          "id": "MDRW2100010390.1.16",
          "form": "너무 멋있지.",
          "original_form": "너무 멋있지.",
          "speaker_id": "2",
          "time": "20210823 17:43"
        },
        {
          "id": "MDRW2100010390.1.17",
          "form": "쇼핑도 즐거워.",
          "original_form": "쇼핑도 즐거워.",
          "speaker_id": "2",
          "time": "20210823 17:43"
        },
        {
          "id": "MDRW2100010390.1.18",
          "form": "돈을 많이 모으자 우리.",
          "original_form": "돈을 많이 모으자 우리.",
          "speaker_id": "2",
          "time": "20210823 17:43"
        },
        {
          "id": "MDRW2100010390.1.19",
          "form": "맞아 면세찬스를 잘 써야해",
          "original_form": "맞아 면세찬스를 잘 써야해",
          "speaker_id": "1",
          "time": "20210823 17:43"
        },
        {
          "id": "MDRW2100010390.1.20",
          "form": "좋아 역시 정승같이 벌어야",
          "original_form": "좋아 역시 정승같이 벌어야",
          "speaker_id": "1",
          "time": "20210823 17:43"
        },
        {
          "id": "MDRW2100010390.1.21",
          "form": "개처럼 쓸 수 있어",
          "original_form": "개처럼 쓸 수 있어",
          "speaker_id": "1",
          "time": "20210823 17:43"
        },
        {
          "id": "MDRW2100010390.1.22",
          "form": "펑펑 쓸 수 있지.",
          "original_form": "펑펑 쓸 수 있지.",
          "speaker_id": "2",
          "time": "20210823 17:43"
        },
        {
          "id": "MDRW2100010390.1.23",
          "form": "여행은 가서 놀기만하니까 더 좋은것같아",
          "original_form": "여행은 가서 놀기만하니까 더 좋은것같아",
          "speaker_id": "1",
          "time": "20210823 17:43"
        },
        {
          "id": "MDRW2100010390.1.24",
          "form": "겨울 휴가일정도 정해놓은거야m",
          "original_form": "겨울 휴가일정도 정해놓은거야m",
          "speaker_id": "2",
          "time": "20210823 17:43"
        },
        {
          "id": "MDRW2100010390.1.25",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210823 17:43"
        },
        {
          "id": "MDRW2100010390.1.26",
          "form": "아니 아직.",
          "original_form": "아니 아직.",
          "speaker_id": "1",
          "time": "20210823 17:44"
        },
        {
          "id": "MDRW2100010390.1.27",
          "form": "맞아. 걱정없이 놀 수 있지.",
          "original_form": "맞아. 걱정없이 놀 수 있지.",
          "speaker_id": "2",
          "time": "20210823 17:44"
        },
        {
          "id": "MDRW2100010390.1.28",
          "form": "그때 코로나가 어떨지",
          "original_form": "그때 코로나가 어떨지",
          "speaker_id": "1",
          "time": "20210823 17:44"
        },
        {
          "id": "MDRW2100010390.1.29",
          "form": "우리 전에 부산여행갔잖아",
          "original_form": "우리 전에 부산여행갔잖아",
