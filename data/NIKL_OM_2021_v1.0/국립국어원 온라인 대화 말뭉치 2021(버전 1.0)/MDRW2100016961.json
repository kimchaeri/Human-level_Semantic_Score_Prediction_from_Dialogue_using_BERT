      "utterance": [
        {
          "id": "MDRW2100016961.1.1",
          "form": "혹시 영양제 챙겨드시나용?",
          "original_form": "혹시 영양제 챙겨드시나용?",
          "speaker_id": "2",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100016961.1.2",
          "form": "먹다안먹다해요 ㅎㅎ",
          "original_form": "먹다안먹다해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100016961.1.3",
          "form": "드세요?",
          "original_form": "드세요?",
          "speaker_id": "1",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100016961.1.4",
          "form": "오 저랑 같아요 !  저도 사놓고 먹다 안먹다 하고있어요 ㅎㅎ",
          "original_form": "오 저랑 같아요 !  저도 사놓고 먹다 안먹다 하고있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:08"
        },
        {
          "id": "MDRW2100016961.1.5",
          "form": "귀차나서 뭐든 꾸준히 잘 못해요",
          "original_form": "귀차나서 뭐든 꾸준히 잘 못해요",
          "speaker_id": "1",
          "time": "20210826 17:17"
        },
        {
          "id": "MDRW2100016961.1.6",
          "form": "저두...ㅎㅎ 사놓은건진짜많아요\r\n지르고안하는타입..",
          "original_form": "저두...ㅎㅎ 사놓은건진짜많아요\r\n지르고안하는타입..",
          "speaker_id": "2",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100016961.1.7",
          "form": "퇴근길에 더워서 ㅋㅋ 방전됏어요....\r\n배민을.....ㅎㅎ",
          "original_form": "퇴근길에 더워서 ㅋㅋ 방전됏어요....\r\n배민을.....ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 17:18"
        },
        {
          "id": "MDRW2100016961.1.8",
          "form": "악ㅋㅋㅋㅋ 배민최고죠..다이어트의적이지만...",
          "original_form": "악ㅋㅋㅋㅋ 배민최고죠..다이어트의적이지만...",
          "speaker_id": "2",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100016961.1.9",
          "form": "오늘진짜덥네요",
          "original_form": "오늘진짜덥네요",
          "speaker_id": "2",
          "time": "20210826 17:20"
        },
        {
          "id": "MDRW2100016961.1.10",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "1",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100016961.1.11",
          "form": "먹어야해요!  고민중입니당! ㅎㅎ 저도 배민켜볼까 하고 있었어요..ㅋㅋㅋ..",
          "original_form": "먹어야해요!  고민중입니당! ㅎㅎ 저도 배민켜볼까 하고 있었어요..ㅋㅋㅋ..",
          "speaker_id": "2",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100016961.1.12",
          "form": "저는 탄수화물 면 중독자라서 잔치국수 시켰어요",
          "original_form": "저는 탄수화물 면 중독자라서 잔치국수 시켰어요",
          "speaker_id": "1",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100016961.1.13",
          "form": "미련하고 행복한 돼지 할래욧",
          "original_form": "미련하고 행복한 돼지 할래욧",
          "speaker_id": "1",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100016961.1.14",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100016961.1.15",
          "form": "저 좀 신기한게 공통점이 많으셔가지구..!!!! 저도 탄수화물이랑 면진짜 완전 좋아해요ㅜㅜ!!!!!!!",
          "original_form": "저 좀 신기한게 공통점이 많으셔가지구..!!!! 저도 탄수화물이랑 면진짜 완전 좋아해요ㅜㅜ!!!!!!!",
          "speaker_id": "2",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100016961.1.16",
          "form": "다이어트는포기",
          "original_form": "다이어트는포기",
          "speaker_id": "2",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100016961.1.17",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100016961.1.18",
          "form": "나쁜말안치는데  자꾸 대화안가지네요",
          "original_form": "나쁜말안치는데  자꾸 대화안가지네요",
          "speaker_id": "2",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100016961.1.19",
          "form": "ㅋㅋㄱㄲㅋㅋㅋ",
          "original_form": "ㅋㅋㄱㄲㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:32"
        },
        {
          "id": "MDRW2100016961.1.20",
          "form": "칼답 부담없이 시간 될때마다 편하게 대화 햇음 좋겠어요",
          "original_form": "칼답 부담없이 시간 될때마다 편하게 대화 햇음 좋겠어요",
          "speaker_id": "1",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100016961.1.21",
          "form": "저는 근데 진짜",
          "original_form": "저는 근데 진짜",
          "speaker_id": "1",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100016961.1.22",
          "form": "저질체력,허리 때문에",
          "original_form": "저질체력,허리 때문에",
          "speaker_id": "1",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100016961.1.23",
          "form": "운동하긴해야하는데",
          "original_form": "운동하긴해야하는데",
          "speaker_id": "1",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100016961.1.24",
          "form": "이놈의 귀차니즘 죽겟어요ㅜㅜ",
          "original_form": "이놈의 귀차니즘 죽겟어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100016961.1.25",
          "form": "편하게 대화하자구 해주셔서 감사해요 ㅋㅋ조금 조급했거든요 ㅠㅅㅠㅋㅋㅋ  헤헤\r\n운동 꾸준히하기가 힘이들지요 진짜..",
          "original_form": "편하게 대화하자구 해주셔서 감사해요 ㅋㅋ조금 조급했거든요 ㅠㅅㅠㅋㅋㅋ  헤헤\r\n운동 꾸준히하기가 힘이들지요 진짜..",
          "speaker_id": "2",
          "time": "20210826 17:36"
        },
        {
          "id": "MDRW2100016961.1.26",
          "form": "앗 전혀 조급하지마세욤ㅎㅎ\r\n저는 이제 문자치기 힘드러져서 \r\n쉬엄쉬엄 할려구요 ㅎㅎ",
          "original_form": "앗 전혀 조급하지마세욤ㅎㅎ\r\n저는 이제 문자치기 힘드러져서 \r\n쉬엄쉬엄 할려구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100016961.1.27",
          "form": "편하게 답해주세요",
          "original_form": "편하게 답해주세요",
          "speaker_id": "1",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100016961.1.28",
          "form": "저도그렇게합니당",
          "original_form": "저도그렇게합니당",
          "speaker_id": "1",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100016961.1.29",
          "form": "네네 천천히 편할때해여ㅋㅋㅋ~_~  집에서는 좀 쉬시궁! ㅋㅋㅋ  그래야 건강하지여",
          "original_form": "네네 천천히 편할때해여ㅋㅋㅋ~_~  집에서는 좀 쉬시궁! ㅋㅋㅋ  그래야 건강하지여",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100016961.1.30",
          "form": "ㅎㅎ배민기다리면서 에어컨 바람쐬고 천국인가요...",
          "original_form": "ㅎㅎ배민기다리면서 에어컨 바람쐬고 천국인가요...",
          "speaker_id": "1",
          "time": "20210826 17:43"
        },
        {
          "id": "MDRW2100016961.1.31",
          "form": "집 최고이에여",
          "original_form": "집 최고이에여",
          "speaker_id": "1",
          "time": "20210826 17:43"
        },
        {
          "id": "MDRW2100016961.1.32",
          "form": "와 그거진짜 천국 인정입니다..ㅋㅋㅋㅋ",
          "original_form": "와 그거진짜 천국 인정입니다..ㅋㅋㅋㅋ",
          "speaker_id": "2",
