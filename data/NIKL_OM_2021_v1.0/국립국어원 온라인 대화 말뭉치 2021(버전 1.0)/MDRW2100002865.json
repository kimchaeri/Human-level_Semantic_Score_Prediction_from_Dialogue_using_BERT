      "utterance": [
        {
          "id": "MDRW2100002865.1.1",
          "form": "네! 배달 자주 시켜먹어요^^",
          "original_form": "네! 배달 자주 시켜먹어요^^",
          "speaker_id": "2",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002865.1.2",
          "form": "족발 막국수 요근래에 시켜먹었는데",
          "original_form": "족발 막국수 요근래에 시켜먹었는데",
          "speaker_id": "2",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002865.1.3",
          "form": "맛있더라고요!",
          "original_form": "맛있더라고요!",
          "speaker_id": "2",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002865.1.4",
          "form": "족발 좋죠",
          "original_form": "족발 좋죠",
          "speaker_id": "1",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002865.1.5",
          "form": "보쌈도 좋더라고요!",
          "original_form": "보쌈도 좋더라고요!",
          "speaker_id": "2",
          "time": "20210820 16:34"
        },
        {
          "id": "MDRW2100002865.1.6",
          "form": "근데 갠적으론 쫀득한 족발이 좀 더 낫긴해요 히히",
          "original_form": "근데 갠적으론 쫀득한 족발이 좀 더 낫긴해요 히히",
          "speaker_id": "2",
          "time": "20210820 16:34"
        },
        {
          "id": "MDRW2100002865.1.7",
          "form": "아 소주도 생각나네요",
          "original_form": "아 소주도 생각나네요",
          "speaker_id": "1",
          "time": "20210820 16:34"
        },
        {
          "id": "MDRW2100002865.1.8",
          "form": "크으 소주도 진짜 최고죠",
          "original_form": "크으 소주도 진짜 최고죠",
          "speaker_id": "2",
          "time": "20210820 16:37"
        },
        {
          "id": "MDRW2100002865.1.9",
          "form": "소주에 족발..",
          "original_form": "소주에 족발..",
          "speaker_id": "2",
          "time": "20210820 16:37"
        },
        {
          "id": "MDRW2100002865.1.10",
          "form": "최고죠 정말",
          "original_form": "최고죠 정말",
          "speaker_id": "2",
          "time": "20210820 16:37"
        },
        {
          "id": "MDRW2100002865.1.11",
          "form": "침이 고입니다",
          "original_form": "침이 고입니다",
          "speaker_id": "1",
          "time": "20210820 16:37"
        },
        {
          "id": "MDRW2100002865.1.12",
          "form": "그러게요 ㅎㅎ",
          "original_form": "그러게요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 16:38"
        },
        {
          "id": "MDRW2100002865.1.13",
          "form": "오늘도 배달시켜먹어야하나 고민이되네요",
          "original_form": "오늘도 배달시켜먹어야하나 고민이되네요",
          "speaker_id": "2",
          "time": "20210820 16:38"
        },
        {
          "id": "MDRW2100002865.1.14",
          "form": "불금이니 당연히",
          "original_form": "불금이니 당연히",
          "speaker_id": "1",
          "time": "20210820 16:39"
        },
        {
          "id": "MDRW2100002865.1.15",
          "form": "불금이면 치맥이 좋을려나요?",
          "original_form": "불금이면 치맥이 좋을려나요?",
          "speaker_id": "2",
          "time": "20210820 16:40"
        },
        {
          "id": "MDRW2100002865.1.16",
          "form": "아니면 곱창에 소주?",
          "original_form": "아니면 곱창에 소주?",
          "speaker_id": "2",
          "time": "20210820 16:40"
        },
        {
          "id": "MDRW2100002865.1.17",
          "form": "것도 좋죠",
          "original_form": "것도 좋죠",
          "speaker_id": "1",
          "time": "20210820 16:41"
        },
        {
          "id": "MDRW2100002865.1.18",
          "form": "뭘 먹을지 행복한 상상이 나네요 ㅎㅎ",
          "original_form": "뭘 먹을지 행복한 상상이 나네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 16:44"
        },
        {
          "id": "MDRW2100002865.1.19",
          "form": "그래요",
          "original_form": "그래요",
          "speaker_id": "1",
          "time": "20210820 16:46"
        },
        {
          "id": "MDRW2100002865.1.20",
          "form": "개인적으로 오늘은 양념곱창에 소주가 좀 땡기네요~",
          "original_form": "개인적으로 오늘은 양념곱창에 소주가 좀 땡기네요~",
          "speaker_id": "2",
          "time": "20210820 16:47"
        }
