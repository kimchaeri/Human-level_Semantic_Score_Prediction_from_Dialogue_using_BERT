      "utterance": [
        {
          "id": "MDRW2100020136.1.1",
          "form": "나의 소녀시대 보셨나요?",
          "original_form": "나의 소녀시대 보셨나요?",
          "speaker_id": "1",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100020136.1.2",
          "form": "봣어요",
          "original_form": "봣어요",
          "speaker_id": "2",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020136.1.3",
          "form": "저는 그시절 이게 젤 재밋더라구요",
          "original_form": "저는 그시절 이게 젤 재밋더라구요",
          "speaker_id": "2",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020136.1.4",
          "form": "세번은본듯!!",
          "original_form": "세번은본듯!!",
          "speaker_id": "2",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020136.1.5",
          "form": "크으 진짜 그 특유의 분위기 너무 좋아요",
          "original_form": "크으 진짜 그 특유의 분위기 너무 좋아요",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020136.1.6",
          "form": "대리 설렘ㅋㅋ",
          "original_form": "대리 설렘ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020136.1.7",
          "form": "좋아하는 연예인 잇으신가욤?",
          "original_form": "좋아하는 연예인 잇으신가욤?",
          "speaker_id": "2",
          "time": "20210828 09:30"
        },
        {
          "id": "MDRW2100020136.1.8",
          "form": "송중기요!",
          "original_form": "송중기요!",
          "speaker_id": "1",
          "time": "20210828 09:30"
        },
        {
          "id": "MDRW2100020136.1.9",
          "form": "여전히 멋있더라구요",
          "original_form": "여전히 멋있더라구요",
          "speaker_id": "1",
          "time": "20210828 09:31"
        },
        {
          "id": "MDRW2100020136.1.10",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210828 09:35"
        },
        {
          "id": "MDRW2100020136.1.11",
          "form": "저는 하정우영화 좋아햇엇는데",
          "original_form": "저는 하정우영화 좋아햇엇는데",
          "speaker_id": "2",
          "time": "20210828 09:35"
        },
        {
          "id": "MDRW2100020136.1.12",
          "form": "이제 흑역사되버렷어요",
          "original_form": "이제 흑역사되버렷어요",
          "speaker_id": "2",
          "time": "20210828 09:35"
        },
        {
          "id": "MDRW2100020136.1.13",
          "form": "쳇",
          "original_form": "쳇",
          "speaker_id": "2",
          "time": "20210828 09:35"
        },
        {
          "id": "MDRW2100020136.1.14",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020136.1.15",
          "form": "하정우 연기는 최고죠",
          "original_form": "하정우 연기는 최고죠",
          "speaker_id": "1",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020136.1.16",
          "form": "빈센조보셧나욤",
          "original_form": "빈센조보셧나욤",
          "speaker_id": "2",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020136.1.17",
          "form": "넵!",
          "original_form": "넵!",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020136.1.18",
          "form": "멋드러지더라구요",
          "original_form": "멋드러지더라구요",
          "speaker_id": "1",
          "time": "20210828 10:52"
        },
        {
          "id": "MDRW2100020136.1.19",
          "form": "맞아용ㅋㅋ",
          "original_form": "맞아용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020136.1.20",
          "form": "태양의후예는 넘 오글거려서 안밧어요 ㅋㅋㅋㅋ",
          "original_form": "태양의후예는 넘 오글거려서 안밧어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020136.1.21",
          "form": "ㅋㅋ저도 약간 힘들었지만 얼굴로 봤어요ㅋㅋㅋ",
          "original_form": "ㅋㅋ저도 약간 힘들었지만 얼굴로 봤어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:56"
        },
        {
          "id": "MDRW2100020136.1.22",
          "form": "저는 지금 유퀴즈 재방 보고잇어요",
          "original_form": "저는 지금 유퀴즈 재방 보고잇어요",
          "speaker_id": "2",
          "time": "20210828 10:57"
        },
        {
          "id": "MDRW2100020136.1.23",
          "form": "누구 나오나요?!",
          "original_form": "누구 나오나요?!",
          "speaker_id": "1",
          "time": "20210828 10:57"
        },
        {
          "id": "MDRW2100020136.1.24",
          "form": "올림픽 메달리스트들 나와욧",
