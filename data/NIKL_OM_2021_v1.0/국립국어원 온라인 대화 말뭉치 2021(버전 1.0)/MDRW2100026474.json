      "utterance": [
        {
          "id": "MDRW2100026474.1.1",
          "form": "맞아여..",
          "original_form": "맞아여..",
          "speaker_id": "2",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100026474.1.2",
          "form": "저희도 그래요",
          "original_form": "저희도 그래요",
          "speaker_id": "2",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100026474.1.3",
          "form": "근데 그마음을 아니까",
          "original_form": "근데 그마음을 아니까",
          "speaker_id": "2",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100026474.1.4",
          "form": "잡을 수도 없어요ㅎㅎ",
          "original_form": "잡을 수도 없어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100026474.1.5",
          "form": "전 못잡죠",
          "original_form": "전 못잡죠",
          "speaker_id": "1",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026474.1.6",
          "form": "더 좋은데 가라고",
          "original_form": "더 좋은데 가라고",
          "speaker_id": "1",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026474.1.7",
          "form": "응원해줄쀼",
          "original_form": "응원해줄쀼",
          "speaker_id": "1",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026474.1.8",
          "form": "뿐",
          "original_form": "뿐",
          "speaker_id": "1",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026474.1.9",
          "form": "맞아요... 나가서 잘되면 다 좋을 뿐..",
          "original_form": "맞아요... 나가서 잘되면 다 좋을 뿐..",
          "speaker_id": "2",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100026474.1.10",
          "form": "하다못해 저조차도 퇴사각을 재고 있는뎈ㅋㅋㅋ",
          "original_form": "하다못해 저조차도 퇴사각을 재고 있는뎈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100026474.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100026474.1.12",
          "form": "회사는 집하고 가까우세요?",
          "original_form": "회사는 집하고 가까우세요?",
          "speaker_id": "1",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100026474.1.13",
          "form": "다행히도 그렇네요..",
          "original_form": "다행히도 그렇네요..",
          "speaker_id": "2",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100026474.1.14",
          "form": "집ㅁ가지 멀었으면 더 최악일 뻔 했어요",
          "original_form": "집ㅁ가지 멀었으면 더 최악일 뻔 했어요",
          "speaker_id": "2",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100026474.1.15",
          "form": "전",
          "original_form": "전",
          "speaker_id": "1",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100026474.1.16",
          "form": "집도 멀답니다",
          "original_form": "집도 멀답니다",
          "speaker_id": "1",
          "time": "20210901 22:44"
        },
        {
          "id": "MDRW2100026474.1.17",
          "form": "하아ㅜㅜ",
          "original_form": "하아ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 22:44"
        },
        {
          "id": "MDRW2100026474.1.18",
          "form": "헉.. 얼마정도 걸리시나요",
          "original_form": "헉.. 얼마정도 걸리시나요",
          "speaker_id": "2",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100026474.1.19",
          "form": "차로 한시간",
          "original_form": "차로 한시간",
          "speaker_id": "1",
          "time": "20210901 22:46"
        },
        {
          "id": "MDRW2100026474.1.20",
          "form": "지하철 두시간 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "지하철 두시간 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:46"
        },
        {
          "id": "MDRW2100026474.1.21",
          "form": "차로 출퇴근합니다",
          "original_form": "차로 출퇴근합니다",
          "speaker_id": "1",
          "time": "20210901 22:46"
        },
        {
          "id": "MDRW2100026474.1.22",
          "form": "와...",
          "original_form": "와...",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100026474.1.23",
          "form": "둘 다 너무 빡세네요",
          "original_form": "둘 다 너무 빡세네요",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100026474.1.24",
          "form": "이미 출퇴근 시간만해도 하루에 두시간인데...",
          "original_form": "이미 출퇴근 시간만해도 하루에 두시간인데...",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100026474.1.25",
          "form": "안힘드시나요ㅠ",
          "original_form": "안힘드시나요ㅠ",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100026474.1.26",
          "form": "힘들죠",
          "original_form": "힘들죠",
          "speaker_id": "1",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100026474.1.27",
          "form": "참 뭘 위해서 이렇게 일하는지",
          "original_form": "참 뭘 위해서 이렇게 일하는지",
          "speaker_id": "1",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100026474.1.28",
          "form": "얼마나 잘살겠다고",
          "original_form": "얼마나 잘살겠다고",
          "speaker_id": "1",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026474.1.29",
          "form": "그러니가요..",
          "original_form": "그러니가요..",
          "speaker_id": "2",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026474.1.30",
          "form": "이렇게 돈벌면 통장에 돈이 눈덩이 처럼 불어난다면 하겠는데..",
          "original_form": "이렇게 돈벌면 통장에 돈이 눈덩이 처럼 불어난다면 하겠는데..",
          "speaker_id": "2",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100026474.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026474.1.32",
          "form": "그럴리가....",
          "original_form": "그럴리가....",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026474.1.33",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 22:50"
        },
        {
          "id": "MDRW2100026474.1.34",
          "form": "맞죠ㅠㅠ",
          "original_form": "맞죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026474.1.35",
          "form": "백신맞고도 담날 일하는....",
          "original_form": "백신맞고도 담날 일하는....",
          "speaker_id": "1",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026474.1.36",
          "form": "월급쟁이의 서러움이란...",
          "original_form": "월급쟁이의 서러움이란...",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026474.1.37",
          "form": "와....",
          "original_form": "와....",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026474.1.38",
          "form": "부작용은 없으샸어요??",
          "original_form": "부작용은 없으샸어요??",
          "speaker_id": "2",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026474.1.39",
          "form": "1차만 맞았는데",
          "original_form": "1차만 맞았는데",
          "speaker_id": "1",
          "time": "20210901 22:51"
        },
        {
          "id": "MDRW2100026474.1.40",
          "form": "열한번나고 별거 없긴했어요",
          "original_form": "열한번나고 별거 없긴했어요",
          "speaker_id": "1",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100026474.1.41",
          "form": "열 심하게 안나셨나요..?",
          "original_form": "열 심하게 안나셨나요..?",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100026474.1.42",
          "form": "네 1차라 그런가봐요",
          "original_form": "네 1차라 그런가봐요",
          "speaker_id": "1",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100026474.1.43",
          "form": "다행히 백신 강요는 안하더라구요?",
          "original_form": "다행히 백신 강요는 안하더라구요?",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026474.1.44",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026474.1.45",
          "form": "사장이 물어보길래 강요하는줄....",
          "original_form": "사장이 물어보길래 강요하는줄....",
          "speaker_id": "1",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100026474.1.46",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026474.1.47",
          "form": "백신까지 강요당하면...",
          "original_form": "백신까지 강요당하면...",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026474.1.48",
          "form": "저 그럼 진짜",
          "original_form": "저 그럼 진짜",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026474.1.49",
          "form": "고용노동부에 신고할거에요",
          "original_form": "고용노동부에 신고할거에요",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026474.1.50",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026474.1.51",
          "form": "좋은 생각이에요",
          "original_form": "좋은 생각이에요",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100026474.1.52",
          "form": "사장님들안 근데 신경 많이 쓸까요?",
          "original_form": "사장님들안 근데 신경 많이 쓸까요?",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026474.1.53",
          "form": "저희 회사는 신경 안쓰던데...",
          "original_form": "저희 회사는 신경 안쓰던데...",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026474.1.54",
          "form": "저희회사 누가 신고해서",
          "original_form": "저희회사 누가 신고해서",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026474.1.55",
          "form": "신고당해도 바뀌는게 없어..",
          "original_form": "신고당해도 바뀌는게 없어..",
          "speaker_id": "2",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026474.1.56",
          "form": "실제로 사람들 쳐들어왔었거든요",
          "original_form": "실제로 사람들 쳐들어왔었거든요",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026474.1.57",
          "form": "근데 달라지는게 없더라구요?",
          "original_form": "근데 달라지는게 없더라구요?",
          "speaker_id": "1",
          "time": "20210901 22:59"
        },
        {
          "id": "MDRW2100026474.1.58",
          "form": "한번 더 당해봐야되나",
          "original_form": "한번 더 당해봐야되나",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100026474.1.59",
          "form": "ㅋㅋㅋㅋㅋㅋ그러니까요...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그러니까요...",
          "speaker_id": "2",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100026474.1.60",
          "form": "안달라지더라구요",
          "original_form": "안달라지더라구요",
