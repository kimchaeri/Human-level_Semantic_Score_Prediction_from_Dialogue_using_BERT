      "utterance": [
        {
          "id": "MDRW2100029212.1.1",
          "form": "록시땅 괜찮지",
          "original_form": "록시땅 괜찮지",
          "speaker_id": "2",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029212.1.2",
          "form": "향도 좋을걸",
          "original_form": "향도 좋을걸",
          "speaker_id": "2",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029212.1.3",
          "form": "록시땅 핸드크림은 써봤는데 다른건 안써봐서 쓰는거 기대중",
          "original_form": "록시땅 핸드크림은 써봤는데 다른건 안써봐서 쓰는거 기대중",
          "speaker_id": "1",
          "time": "20210903 00:29"
        },
        {
          "id": "MDRW2100029212.1.4",
          "form": "ㅎㅎ 비누, 바디워시도 바디로션도 괜찮다더라ㅎ",
          "original_form": "ㅎㅎ 비누, 바디워시도 바디로션도 괜찮다더라ㅎ",
          "speaker_id": "2",
          "time": "20210903 08:05"
        },
        {
          "id": "MDRW2100029212.1.5",
          "form": "ㅇㅇ바디워시 오일왔엉 ㅋㅋ 나 바디워시가 오일인건 첨봐 ㅋㅋ",
          "original_form": "ㅇㅇ바디워시 오일왔엉 ㅋㅋ 나 바디워시가 오일인건 첨봐 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 09:43"
        },
        {
          "id": "MDRW2100029212.1.6",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 11:22"
        },
        {
          "id": "MDRW2100029212.1.7",
          "form": "향 좋지?",
          "original_form": "향 좋지?",
          "speaker_id": "2",
          "time": "20210903 11:22"
        },
        {
          "id": "MDRW2100029212.1.8",
          "form": "아직 개봉 안했엉 ㅋㅋㅋ",
          "original_form": "아직 개봉 안했엉 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:26"
        },
        {
          "id": "MDRW2100029212.1.9",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 12:11"
        },
        {
          "id": "MDRW2100029212.1.10",
          "form": "샤인 사고싶었는데",
          "original_form": "샤인 사고싶었는데",
          "speaker_id": "2",
          "time": "20210903 12:11"
        },
        {
          "id": "MDRW2100029212.1.11",
          "form": "회사에서 주는 샤인이랑일정 겹칠듯 아쉽",
          "original_form": "회사에서 주는 샤인이랑일정 겹칠듯 아쉽",
          "speaker_id": "2",
          "time": "20210903 12:12"
        },
        {
          "id": "MDRW2100029212.1.12",
          "form": "결제 직전까지 갔다 못했네",
          "original_form": "결제 직전까지 갔다 못했네",
          "speaker_id": "2",
          "time": "20210903 12:12"
        },
        {
          "id": "MDRW2100029212.1.13",
          "form": "아 회사 추석선물?",
          "original_form": "아 회사 추석선물?",
          "speaker_id": "1",
          "time": "20210903 12:12"
        },
        {
          "id": "MDRW2100029212.1.14",
          "form": "응ㅎ",
          "original_form": "응ㅎ",
          "speaker_id": "2",
          "time": "20210903 12:55"
        },
        {
          "id": "MDRW2100029212.1.15",
          "form": "소고기랑 샤인준다네",
          "original_form": "소고기랑 샤인준다네",
          "speaker_id": "2",
          "time": "20210903 12:55"
        },
        {
          "id": "MDRW2100029212.1.16",
          "form": "와 대박이다 요즘 소고기 비싼데",
          "original_form": "와 대박이다 요즘 소고기 비싼데",
          "speaker_id": "1",
          "time": "20210903 13:51"
        },
        {
          "id": "MDRW2100029212.1.17",
          "form": "그르게 고기를 지금까진 집에 보냈는데\n이번엔 내가 친구들과 먹을까 고민중",
          "original_form": "그르게 고기를 지금까진 집에 보냈는데\n이번엔 내가 친구들과 먹을까 고민중",
          "speaker_id": "2",
          "time": "20210903 15:32"
        },
        {
          "id": "MDRW2100029212.1.18",
          "form": "요즘 축산물 다 가격 올라서 비싸더라",
          "original_form": "요즘 축산물 다 가격 올라서 비싸더라",
