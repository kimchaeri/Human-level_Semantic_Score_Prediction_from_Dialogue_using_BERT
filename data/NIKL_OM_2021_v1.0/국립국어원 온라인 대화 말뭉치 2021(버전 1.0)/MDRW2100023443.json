      "utterance": [
        {
          "id": "MDRW2100023443.1.1",
          "form": "나 책쓰고 싶어",
          "original_form": "나 책쓰고 싶어",
          "speaker_id": "2",
          "time": "20210830 02:48"
        },
        {
          "id": "MDRW2100023443.1.2",
          "form": "어떤 책?",
          "original_form": "어떤 책?",
          "speaker_id": "1",
          "time": "20210830 02:49"
        },
        {
          "id": "MDRW2100023443.1.3",
          "form": "여행일기",
          "original_form": "여행일기",
          "speaker_id": "2",
          "time": "20210830 02:49"
        },
        {
          "id": "MDRW2100023443.1.4",
          "form": "저번에 유럽 다녀온거?",
          "original_form": "저번에 유럽 다녀온거?",
          "speaker_id": "1",
          "time": "20210830 02:49"
        },
        {
          "id": "MDRW2100023443.1.5",
          "form": "블로그에 올린게 반응이 좋아",
          "original_form": "블로그에 올린게 반응이 좋아",
          "speaker_id": "2",
          "time": "20210830 02:49"
        },
        {
          "id": "MDRW2100023443.1.6",
          "form": "너 사진도 잘찍었으니까",
          "original_form": "너 사진도 잘찍었으니까",
          "speaker_id": "1",
          "time": "20210830 02:49"
        },
        {
          "id": "MDRW2100023443.1.7",
          "form": "출판사에서 메일이 왔어",
          "original_form": "출판사에서 메일이 왔어",
          "speaker_id": "2",
          "time": "20210830 02:49"
        },
        {
          "id": "MDRW2100023443.1.8",
          "form": "책 내보자고?",
          "original_form": "책 내보자고?",
          "speaker_id": "1",
          "time": "20210830 02:50"
        },
        {
          "id": "MDRW2100023443.1.9",
          "form": "응 어떨거같아?",
          "original_form": "응 어떨거같아?",
          "speaker_id": "2",
          "time": "20210830 02:50"
        },
        {
          "id": "MDRW2100023443.1.10",
          "form": "난 무조건 찬성",
          "original_form": "난 무조건 찬성",
          "speaker_id": "1",
          "time": "20210830 02:50"
        },
        {
          "id": "MDRW2100023443.1.11",
          "form": "좋지",
          "original_form": "좋지",
          "speaker_id": "2",
          "time": "20210830 02:51"
        },
        {
          "id": "MDRW2100023443.1.12",
          "form": "ㅎㅇ",
          "original_form": "ㅎㅇ",
          "speaker_id": "1",
          "time": "20210830 03:04"
        },
        {
          "id": "MDRW2100023443.1.13",
          "form": "나 요즘 꽂힌 책이 있어",
          "original_form": "나 요즘 꽂힌 책이 있어",
          "speaker_id": "2",
          "time": "20210830 03:04"
        },
        {
          "id": "MDRW2100023443.1.14",
          "form": "뭔지 말해봐",
          "original_form": "뭔지 말해봐",
          "speaker_id": "1",
          "time": "20210830 03:04"
        },
        {
          "id": "MDRW2100023443.1.15",
          "form": "요즘 커피 책을 열심히 읽고 있어",
          "original_form": "요즘 커피 책을 열심히 읽고 있어",
          "speaker_id": "2",
          "time": "20210830 03:04"
        },
        {
          "id": "MDRW2100023443.1.16",
          "form": "커피를 만드는 거에 대한 책이니",
          "original_form": "커피를 만드는 거에 대한 책이니",
          "speaker_id": "1",
          "time": "20210830 03:04"
        },
        {
          "id": "MDRW2100023443.1.17",
          "form": "그거 보단 역사에 대한 책이야",
          "original_form": "그거 보단 역사에 대한 책이야",
          "speaker_id": "2",
          "time": "20210830 03:04"
        },
        {
          "id": "MDRW2100023443.1.18",
          "form": "커피의 역사 말하는 거지",
          "original_form": "커피의 역사 말하는 거지",
          "speaker_id": "1",
          "time": "20210830 03:04"
        },
        {
          "id": "MDRW2100023443.1.19",
          "form": "맞아 원두 얘기도 많고",
          "original_form": "맞아 원두 얘기도 많고",
          "speaker_id": "2",
          "time": "20210830 03:05"
        },
        {
          "id": "MDRW2100023443.1.20",
          "form": "커피 좋아하는 너한테 딱 맞는 책이네",
          "original_form": "커피 좋아하는 너한테 딱 맞는 책이네",
          "speaker_id": "1",
          "time": "20210830 03:05"
        },
        {
          "id": "MDRW2100023443.1.21",
          "form": "그거를 읽고 커피를 마시면 더 맛있게 느껴져",
          "original_form": "그거를 읽고 커피를 마시면 더 맛있게 느껴져",
          "speaker_id": "2",
          "time": "20210830 03:05"
        },
        {
          "id": "MDRW2100023443.1.22",
          "form": "뭐든 더 알게 될수록 더 좋아지더라",
          "original_form": "뭐든 더 알게 될수록 더 좋아지더라",
          "speaker_id": "1",
          "time": "20210830 03:05"
        },
        {
          "id": "MDRW2100023443.1.23",
          "form": "나중에 바리스타 자격증도 따고 싶",
          "original_form": "나중에 바리스타 자격증도 따고 싶",
          "speaker_id": "2",
          "time": "20210830 03:05"
        },
        {
          "id": "MDRW2100023443.1.24",
          "form": "바리스타 자격증에 대한 책도 분명 있을 거야",
          "original_form": "바리스타 자격증에 대한 책도 분명 있을 거야",
          "speaker_id": "1",
          "time": "20210830 03:05"
        },
        {
          "id": "MDRW2100023443.1.25",
          "form": "한번 찾아볼게",
          "original_form": "한번 찾아볼게",
          "speaker_id": "2",
          "time": "20210830 03:05"
        },
        {
          "id": "MDRW2100023443.1.26",
          "form": "ㅎㅇ",
          "original_form": "ㅎㅇ",
          "speaker_id": "1",
