      "utterance": [
        {
          "id": "MDRW2100012773.1.1",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210825 01:01"
        },
        {
          "id": "MDRW2100012773.1.2",
          "form": "근데 주무실거면 주무셔도 돼요",
          "original_form": "근데 주무실거면 주무셔도 돼요",
          "speaker_id": "2",
          "time": "20210825 01:01"
        },
        {
          "id": "MDRW2100012773.1.3",
          "form": "전 안 자요",
          "original_form": "전 안 자요",
          "speaker_id": "1",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100012773.1.4",
          "form": "왜죠?",
          "original_form": "왜죠?",
          "speaker_id": "2",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100012773.1.5",
          "form": "방학의 기쁨이에요",
          "original_form": "방학의 기쁨이에요",
          "speaker_id": "1",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100012773.1.6",
          "form": "어차피 곧 개강이라서요",
          "original_form": "어차피 곧 개강이라서요",
          "speaker_id": "1",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100012773.1.7",
          "form": "그리고 지금은 좀 괜찮아졌는데",
          "original_form": "그리고 지금은 좀 괜찮아졌는데",
          "speaker_id": "1",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100012773.1.8",
          "form": "이럴시간이 적겠군요...",
          "original_form": "이럴시간이 적겠군요...",
          "speaker_id": "2",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100012773.1.9",
          "form": "더울 때 불면증 있었거든쇼",
          "original_form": "더울 때 불면증 있었거든쇼",
          "speaker_id": "1",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100012773.1.10",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100012773.1.11",
          "form": "더위에 취약해서....",
          "original_form": "더위에 취약해서....",
          "speaker_id": "1",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100012773.1.12",
          "form": "저돕니다",
          "original_form": "저돕니다",
          "speaker_id": "2",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100012773.1.13",
          "form": "그래서 더워서 못 자다가",
          "original_form": "그래서 더워서 못 자다가",
          "speaker_id": "1",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100012773.1.14",
          "form": "맨날 기절하듯이 잠 드는 거 아니면 못 잤어요",
          "original_form": "맨날 기절하듯이 잠 드는 거 아니면 못 잤어요",
          "speaker_id": "1",
          "time": "20210825 01:03"
        },
        {
          "id": "MDRW2100012773.1.15",
          "form": "오 더위에 취약하시군요",
          "original_form": "오 더위에 취약하시군요",
          "speaker_id": "1",
          "time": "20210825 01:03"
        },
        {
          "id": "MDRW2100012773.1.16",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210825 01:03"
        },
        {
          "id": "MDRW2100012773.1.17",
          "form": "계속 부산에 살 계획이에요?",
          "original_form": "계속 부산에 살 계획이에요?",
          "speaker_id": "1",
          "time": "20210825 01:03"
        },
        {
          "id": "MDRW2100012773.1.18",
          "form": "부산도 전 덥던데",
          "original_form": "부산도 전 덥던데",
          "speaker_id": "1",
          "time": "20210825 01:03"
        },
        {
          "id": "MDRW2100012773.1.19",
          "form": "님은 어디 사신다 하셨죠?",
          "original_form": "님은 어디 사신다 하셨죠?",
          "speaker_id": "2",
          "time": "20210825 01:03"
        },
        {
          "id": "MDRW2100012773.1.20",
          "form": "저 경기북부요",
          "original_form": "저 경기북부요",
          "speaker_id": "1",
          "time": "20210825 01:03"
        },
        {
          "id": "MDRW2100012773.1.21",
          "form": "김포예요",
          "original_form": "김포예요",
          "speaker_id": "1",
          "time": "20210825 01:03"
        },
        {
          "id": "MDRW2100012773.1.22",
          "form": "공항근처이신가요?",
          "original_form": "공항근처이신가요?",
          "speaker_id": "2",
          "time": "20210825 01:04"
        },
        {
          "id": "MDRW2100012773.1.23",
          "form": "근데 대학을 경남 갔더니 죽을 거 같아요",
          "original_form": "근데 대학을 경남 갔더니 죽을 거 같아요",
          "speaker_id": "1",
          "time": "20210825 01:04"
        },
        {
          "id": "MDRW2100012773.1.24",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210825 01:04"
        },
        {
          "id": "MDRW2100012773.1.25",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210825 01:04"
        },
        {
          "id": "MDRW2100012773.1.26",
          "form": "공항까지 지하철로 15분 걸려요",
          "original_form": "공항까지 지하철로 15분 걸려요",
          "speaker_id": "1",
          "time": "20210825 01:04"
        },
        {
          "id": "MDRW2100012773.1.27",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "2",
          "time": "20210825 01:04"
        },
        {
          "id": "MDRW2100012773.1.28",
          "form": "차라리 인천갈게요",
          "original_form": "차라리 인천갈게요",
          "speaker_id": "2",
          "time": "20210825 01:04"
        },
        {
          "id": "MDRW2100012773.1.29",
          "form": "여기가 그나마 좀 나아요",
          "original_form": "여기가 그나마 좀 나아요",
          "speaker_id": "1",
          "time": "20210825 01:04"
        },
        {
          "id": "MDRW2100012773.1.30",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:04"
        },
        {
          "id": "MDRW2100012773.1.31",
          "form": "인천은 왜요?",
          "original_form": "인천은 왜요?",
          "speaker_id": "1",
          "time": "20210825 01:04"
        },
        {
          "id": "MDRW2100012773.1.32",
          "form": "인천은 친구가 거주중이라",
          "original_form": "인천은 친구가 거주중이라",
          "speaker_id": "2",
          "time": "20210825 01:05"
        },
        {
          "id": "MDRW2100012773.1.33",
          "form": "도시라서?",
          "original_form": "도시라서?",
          "speaker_id": "1",
          "time": "20210825 01:05"
        },
        {
          "id": "MDRW2100012773.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:05"
        },
        {
          "id": "MDRW2100012773.1.35",
          "form": "아하 친구 있는 도시가 최고예요",
          "original_form": "아하 친구 있는 도시가 최고예요",
          "speaker_id": "1",
          "time": "20210825 01:05"
        },
        {
          "id": "MDRW2100012773.1.36",
          "form": "그 친구 아버지가 파일럿이에요",
          "original_form": "그 친구 아버지가 파일럿이에요",
          "speaker_id": "2",
          "time": "20210825 01:05"
        }
      ]
    }
