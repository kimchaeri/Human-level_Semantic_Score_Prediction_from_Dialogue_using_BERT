      "utterance": [
        {
          "id": "MDRW2100024991.1.1",
          "form": "음식은 다 돌고 도나봐요",
          "original_form": "음식은 다 돌고 도나봐요",
          "speaker_id": "2",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100024991.1.2",
          "form": "맥바셋이라는 이름인가봐요",
          "original_form": "맥바셋이라는 이름인가봐요",
          "speaker_id": "1",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100024991.1.3",
          "form": "구글에쳐보니",
          "original_form": "구글에쳐보니",
          "speaker_id": "1",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100024991.1.4",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100024991.1.5",
          "form": "근데 아무리봐도 아포가토같은데...",
          "original_form": "근데 아무리봐도 아포가토같은데...",
          "speaker_id": "1",
          "time": "20210831 21:57"
        },
        {
          "id": "MDRW2100024991.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100024991.1.7",
          "form": "한국인빠진 한국레시피유행",
          "original_form": "한국인빠진 한국레시피유행",
          "speaker_id": "1",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100024991.1.8",
          "form": "뭐지",
          "original_form": "뭐지",
          "speaker_id": "2",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100024991.1.9",
          "form": "홍철없는홍철팀같네요",
          "original_form": "홍철없는홍철팀같네요",
          "speaker_id": "1",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100024991.1.10",
          "form": "그냥 우리나라를 선망하는 분위기인건가 ㅎ",
          "original_form": "그냥 우리나라를 선망하는 분위기인건가 ㅎ",
          "speaker_id": "2",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100024991.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ그러게요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그러게요",
          "speaker_id": "2",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100024991.1.12",
          "form": "아니 케이커피면..",
          "original_form": "아니 케이커피면..",
          "speaker_id": "2",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100024991.1.13",
          "form": "당연히 스틱믹스커피 아닌가요",
          "original_form": "당연히 스틱믹스커피 아닌가요",
          "speaker_id": "2",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100024991.1.14",
          "form": "맞아요 맥심 노란색 국룰인데",
          "original_form": "맞아요 맥심 노란색 국룰인데",
          "speaker_id": "1",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100024991.1.15",
          "form": "외국에서도 그런스타일이 없어서",
          "original_form": "외국에서도 그런스타일이 없어서",
          "speaker_id": "1",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100024991.1.16",
          "form": "그것도 외국인들 되게 좋아한다던데",
          "original_form": "그것도 외국인들 되게 좋아한다던데",
          "speaker_id": "2",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100024991.1.17",
          "form": "인기많다고하던데...",
          "original_form": "인기많다고하던데...",
          "speaker_id": "1",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100024991.1.18",
          "form": ",마자요",
          "original_form": ",마자요",
          "speaker_id": "1",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100024991.1.19",
          "form": "ㅋㅋㅋㅋㅋ웬 아포가토가 케이커피자리를ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ웬 아포가토가 케이커피자리를ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100024991.1.20",
          "form": "아 그러고보니",
          "original_form": "아 그러고보니",
          "speaker_id": "1",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100024991.1.21",
          "form": "베트남에서 라이스페이퍼로 떡볶이를 만들어 먹는다는데",
          "original_form": "베트남에서 라이스페이퍼로 떡볶이를 만들어 먹는다는데",
          "speaker_id": "1",
          "time": "20210831 22:00"
        },
        {
