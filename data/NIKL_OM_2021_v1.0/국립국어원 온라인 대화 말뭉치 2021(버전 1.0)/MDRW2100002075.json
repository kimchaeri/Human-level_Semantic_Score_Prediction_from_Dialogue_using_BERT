      "utterance": [
        {
          "id": "MDRW2100002075.1.1",
          "form": "혹시 여행 좋아하세요?",
          "original_form": "혹시 여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210819 23:21"
        },
        {
          "id": "MDRW2100002075.1.2",
          "form": "네 방학때 많이 다녔는데 요즘은 못가네요ㅎ",
          "original_form": "네 방학때 많이 다녔는데 요즘은 못가네요ㅎ",
          "speaker_id": "2",
          "time": "20210819 23:22"
        },
        {
          "id": "MDRW2100002075.1.3",
          "form": "수능끝나면 어디가보고 싶으세요?",
          "original_form": "수능끝나면 어디가보고 싶으세요?",
          "speaker_id": "2",
          "time": "20210819 23:23"
        },
        {
          "id": "MDRW2100002075.1.4",
          "form": "전 일본쪽 가고싶어요",
          "original_form": "전 일본쪽 가고싶어요",
          "speaker_id": "1",
          "time": "20210819 23:24"
        },
        {
          "id": "MDRW2100002075.1.5",
          "form": "사진보면 감성이 되게 좋더라구요",
          "original_form": "사진보면 감성이 되게 좋더라구요",
          "speaker_id": "1",
          "time": "20210819 23:24"
        },
        {
          "id": "MDRW2100002075.1.6",
          "form": "오~일본 좋죠",
          "original_form": "오~일본 좋죠",
          "speaker_id": "2",
          "time": "20210819 23:24"
        },
        {
          "id": "MDRW2100002075.1.7",
          "form": "우리나라랑 비슷하면서도 다른 감성이 저도 좋더라구요 아기자기한",
          "original_form": "우리나라랑 비슷하면서도 다른 감성이 저도 좋더라구요 아기자기한",
          "speaker_id": "2",
          "time": "20210819 23:25"
        },
        {
          "id": "MDRW2100002075.1.8",
          "form": "혹시 일본 가보셨어요?",
          "original_form": "혹시 일본 가보셨어요?",
          "speaker_id": "1",
          "time": "20210819 23:27"
        },
        {
          "id": "MDRW2100002075.1.9",
          "form": "네 일본은 그래도 많이가봤네요 다른곳보다비용도 부담이 덜하고^^;",
          "original_form": "네 일본은 그래도 많이가봤네요 다른곳보다비용도 부담이 덜하고^^;",
          "speaker_id": "2",
          "time": "20210819 23:29"
        },
        {
          "id": "MDRW2100002075.1.10",
          "form": "오 일본 어디 가보셨어요?",
          "original_form": "오 일본 어디 가보셨어요?",
          "speaker_id": "1",
          "time": "20210819 23:32"
        },
        {
          "id": "MDRW2100002075.1.11",
          "form": "홋카이도쪽 빼고는 대도시 많이갔어요",
          "original_form": "홋카이도쪽 빼고는 대도시 많이갔어요",
          "speaker_id": "2",
          "time": "20210819 23:32"
        },
        {
          "id": "MDRW2100002075.1.12",
          "form": "도쿄 오사카 후쿠오카 등등요ㅎ",
          "original_form": "도쿄 오사카 후쿠오카 등등요ㅎ",
          "speaker_id": "2",
          "time": "20210819 23:33"
        },
        {
          "id": "MDRW2100002075.1.13",
          "form": "어디가 제일 좋았어요?",
          "original_form": "어디가 제일 좋았어요?",
          "speaker_id": "1",
          "time": "20210819 23:33"
        },
        {
          "id": "MDRW2100002075.1.14",
          "form": "다 느낌이 다른데 쉬는 느낌으론 유후인 온천이 좋았구요",
          "original_form": "다 느낌이 다른데 쉬는 느낌으론 유후인 온천이 좋았구요",
          "speaker_id": "2",
          "time": "20210819 23:33"
        },
        {
          "id": "MDRW2100002075.1.15",
          "form": "룸메가 덕후라서 아키하바라 같은데 피규어 구경 간것도 재밌었어요",
          "original_form": "룸메가 덕후라서 아키하바라 같은데 피규어 구경 간것도 재밌었어요",
          "speaker_id": "2",
          "time": "20210819 23:34"
        },
        {
          "id": "MDRW2100002075.1.16",
          "form": "전 중학교때 너의이름은 보고",
          "original_form": "전 중학교때 너의이름은 보고",
          "speaker_id": "1",
          "time": "20210819 23:36"
        },
        {
          "id": "MDRW2100002075.1.17",
          "form": "그런 비스무리한 곳도 가보고 싶더라구요",
          "original_form": "그런 비스무리한 곳도 가보고 싶더라구요",
          "speaker_id": "1",
          "time": "20210819 23:36"
        },
        {
          "id": "MDRW2100002075.1.18",
          "form": "소도시여행이 진짜 재밌고좋아요",
          "original_form": "소도시여행이 진짜 재밌고좋아요",
          "speaker_id": "2",
          "time": "20210819 23:37"
        },
        {
          "id": "MDRW2100002075.1.19",
          "form": "전 고독한미식가에 나온 식당 돌아보고싶어요",
          "original_form": "전 고독한미식가에 나온 식당 돌아보고싶어요",
          "speaker_id": "2",
          "time": "20210819 23:37"
        },
        {
          "id": "MDRW2100002075.1.20",
          "form": "저도 일본라멘 직접 가서 먹어보고 싶은데",
          "original_form": "저도 일본라멘 직접 가서 먹어보고 싶은데",
          "speaker_id": "1",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002075.1.21",
          "form": "코로나랑 수능만 끝나면 가보고 싶네요...",
          "original_form": "코로나랑 수능만 끝나면 가보고 싶네요...",
          "speaker_id": "1",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002075.1.22",
          "form": "라멘은 한국에도 맛집많으니까 일단 아쉬움을 달래고",
          "original_form": "라멘은 한국에도 맛집많으니까 일단 아쉬움을 달래고",
          "speaker_id": "2",
          "time": "20210819 23:38"
        },
        {
          "id": "MDRW2100002075.1.23",
          "form": "자유롭게 여행갈 날을 꿈꿔봐요^^",
          "original_form": "자유롭게 여행갈 날을 꿈꿔봐요^^",
