      "utterance": [
        {
          "id": "MDRW2100018647.1.1",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 09:52"
        },
        {
          "id": "MDRW2100018647.1.2",
          "form": "여행 좋아해요! 코로나 터지고 해외는 못가서 아쉬워요 ㅠㅠ",
          "original_form": "여행 좋아해요! 코로나 터지고 해외는 못가서 아쉬워요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 09:54"
        },
        {
          "id": "MDRW2100018647.1.3",
          "form": "그죠ㅠㅠ 너무 아쉬워요",
          "original_form": "그죠ㅠㅠ 너무 아쉬워요",
          "speaker_id": "1",
          "time": "20210827 09:56"
        },
        {
          "id": "MDRW2100018647.1.4",
          "form": "국내여행은 최근에 가신곳 있나요?",
          "original_form": "국내여행은 최근에 가신곳 있나요?",
          "speaker_id": "1",
          "time": "20210827 09:56"
        },
        {
          "id": "MDRW2100018647.1.5",
          "form": "저 공주랑 대천이여 ㅎㅎ 별님은요?",
          "original_form": "저 공주랑 대천이여 ㅎㅎ 별님은요?",
          "speaker_id": "2",
          "time": "20210827 09:57"
        },
        {
          "id": "MDRW2100018647.1.6",
          "form": "저는 그나마 최근이 전주요",
          "original_form": "저는 그나마 최근이 전주요",
          "speaker_id": "1",
          "time": "20210827 09:58"
        },
        {
          "id": "MDRW2100018647.1.7",
          "form": "공주랑 대천은 어떠셨어요?",
          "original_form": "공주랑 대천은 어떠셨어요?",
          "speaker_id": "1",
          "time": "20210827 09:58"
        },
        {
          "id": "MDRW2100018647.1.8",
          "form": "오오 전주면 한옥마을가셨나요??",
          "original_form": "오오 전주면 한옥마을가셨나요??",
          "speaker_id": "2",
          "time": "20210827 09:58"
        },
        {
          "id": "MDRW2100018647.1.9",
          "form": "공주가 진짜 좋았어요 ㅎㅎ 공주한옥마을 갔었거든요",
          "original_form": "공주가 진짜 좋았어요 ㅎㅎ 공주한옥마을 갔었거든요",
          "speaker_id": "2",
          "time": "20210827 09:58"
        },
        {
          "id": "MDRW2100018647.1.10",
          "form": "네 한옥 마을갔어요",
          "original_form": "네 한옥 마을갔어요",
          "speaker_id": "1",
          "time": "20210827 09:59"
        },
        {
          "id": "MDRW2100018647.1.11",
          "form": "공주를 안가봤거든요",
          "original_form": "공주를 안가봤거든요",
          "speaker_id": "1",
          "time": "20210827 09:59"
        },
        {
          "id": "MDRW2100018647.1.12",
          "form": "한옥마을 넓은편인가요?",
          "original_form": "한옥마을 넓은편인가요?",
          "speaker_id": "1",
          "time": "20210827 09:59"
        },
        {
          "id": "MDRW2100018647.1.13",
          "form": "저는 숙박했거든요 ㅎㅎ 가격도 저렴하고 방이 넓어서 좋아요! 근처 관광하기도 좋아요",
          "original_form": "저는 숙박했거든요 ㅎㅎ 가격도 저렴하고 방이 넓어서 좋아요! 근처 관광하기도 좋아요",
          "speaker_id": "2",
          "time": "20210827 10:01"
        },
        {
          "id": "MDRW2100018647.1.14",
          "form": "저도 처음 가봤습니다 ㅎㅎ",
          "original_form": "저도 처음 가봤습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 10:01"
        },
        {
          "id": "MDRW2100018647.1.15",
          "form": "우와 사람많은편인가요?",
          "original_form": "우와 사람많은편인가요?",
          "speaker_id": "1",
          "time": "20210827 10:02"
        },
        {
          "id": "MDRW2100018647.1.16",
          "form": "아뇨 사람 별로없었어요 저 갔을때는!",
          "original_form": "아뇨 사람 별로없었어요 저 갔을때는!",
          "speaker_id": "2",
          "time": "20210827 10:03"
        },
        {
          "id": "MDRW2100018647.1.17",
          "form": "ㅋㅋㅋ전주는 사람많나요?",
          "original_form": "ㅋㅋㅋ전주는 사람많나요?",
          "speaker_id": "2",
          "time": "20210827 10:04"
        },
        {
          "id": "MDRW2100018647.1.18",
          "form": "전주는 3년전이지만 예전보다 유명해져서 사람이 좀 있었어요",
          "original_form": "전주는 3년전이지만 예전보다 유명해져서 사람이 좀 있었어요",
          "speaker_id": "1",
          "time": "20210827 10:04"
        },
        {
          "id": "MDRW2100018647.1.19",
          "form": "공주 가보고싶네요!",
          "original_form": "공주 가보고싶네요!",
          "speaker_id": "1",
          "time": "20210827 10:04"
        },
        {
          "id": "MDRW2100018647.1.20",
          "form": "오오 저도 전주간지 오래됐네요 ㅎㅎ 가서 친구들이랑 교복빌려입고했는데..",
          "original_form": "오오 저도 전주간지 오래됐네요 ㅎㅎ 가서 친구들이랑 교복빌려입고했는데..",
          "speaker_id": "2",
          "time": "20210827 10:05"
        },
        {
          "id": "MDRW2100018647.1.21",
          "form": "공주는 진짜 추천해요 ㅎㅎ",
          "original_form": "공주는 진짜 추천해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 10:06"
        },
        {
          "id": "MDRW2100018647.1.22",
          "form": "덕분에 좋은 여행지 알아가요 감사합니다!",
          "original_form": "덕분에 좋은 여행지 알아가요 감사합니다!",
          "speaker_id": "1",
          "time": "20210827 10:09"
        }
      ]
    }
