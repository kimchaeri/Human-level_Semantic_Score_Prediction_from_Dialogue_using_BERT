      "utterance": [
        {
          "id": "MDRW2100026717.1.1",
          "form": "여행간지 얼마나 되셨어요?",
          "original_form": "여행간지 얼마나 되셨어요?",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026717.1.2",
          "form": "저 십구년도에 가고 못갔어요",
          "original_form": "저 십구년도에 가고 못갔어요",
          "speaker_id": "2",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026717.1.3",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026717.1.4",
          "form": "저랑 비슷하시네요ㅜㅜ",
          "original_form": "저랑 비슷하시네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026717.1.5",
          "form": "고성 다녀온게 마지막이었어요",
          "original_form": "고성 다녀온게 마지막이었어요",
          "speaker_id": "2",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026717.1.6",
          "form": "저도 제주도 마지막으로 ᆢ",
          "original_form": "저도 제주도 마지막으로 ᆢ",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100026717.1.7",
          "form": "가고싶으신곳 있으세요?",
          "original_form": "가고싶으신곳 있으세요?",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026717.1.8",
          "form": "현실적으로는 제주도 가야될거 같네욬ㅋㅋㅋ",
          "original_form": "현실적으로는 제주도 가야될거 같네욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026717.1.9",
          "form": "사실 유럽여행 한번도 못해봐서 가고 싶었는데",
          "original_form": "사실 유럽여행 한번도 못해봐서 가고 싶었는데",
          "speaker_id": "2",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026717.1.10",
          "form": "그쵸 ㅎㅎ 갈수있는곳이 ᆢ",
          "original_form": "그쵸 ㅎㅎ 갈수있는곳이 ᆢ",
          "speaker_id": "1",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026717.1.11",
          "form": "앞으로 갈수나 있을까요",
          "original_form": "앞으로 갈수나 있을까요",
          "speaker_id": "2",
          "time": "20210901 23:45"
        },
        {
          "id": "MDRW2100026717.1.12",
          "form": "아 유럽 저도 못가봤어요",
          "original_form": "아 유럽 저도 못가봤어요",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026717.1.13",
          "form": "동양인이라고 처맞을까봐...",
          "original_form": "동양인이라고 처맞을까봐...",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026717.1.14",
          "form": "그런데는 대학때 가야한다는데 ㅜㅜ",
          "original_form": "그런데는 대학때 가야한다는데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026717.1.15",
          "form": "ㅋㅋㅋㅋ 걱정되네여...",
          "original_form": "ㅋㅋㅋㅋ 걱정되네여...",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026717.1.16",
          "form": "그러니까요 대학때 못가본게 ㅜㅜ",
          "original_form": "그러니까요 대학때 못가본게 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026717.1.17",
          "form": "코로나 잠잠해지면 괜찮겠죠ㅜㅜ",
          "original_form": "코로나 잠잠해지면 괜찮겠죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026717.1.18",
          "form": "시간도 많았는데",
          "original_form": "시간도 많았는데",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026717.1.19",
          "form": "돈이 없었져 ㅋㅋㅋ",
          "original_form": "돈이 없었져 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026717.1.20",
          "form": "맞아요 돈이 문제예요",
          "original_form": "맞아요 돈이 문제예요",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026717.1.21",
          "form": "저는 싱가폴가고싶어요",
          "original_form": "저는 싱가폴가고싶어요",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026717.1.22",
          "form": "예전에 가보셨어요??",
          "original_form": "예전에 가보셨어요??",
          "speaker_id": "2",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026717.1.23",
          "form": "아뇨 예전에 티비에서",
          "original_form": "아뇨 예전에 티비에서",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026717.1.24",
          "form": "나오는데 너무 멋져서",
          "original_form": "나오는데 너무 멋져서",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026717.1.25",
          "form": "가고싶어졌어요",
          "original_form": "가고싶어졌어요",
          "speaker_id": "1",
          "time": "20210901 23:47"
        }
      ]
    }
  ]
