      "utterance": [
        {
          "id": "MDRW2100019498.1.1",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 23:05"
        },
        {
          "id": "MDRW2100019498.1.2",
          "form": "네 좋아해요!",
          "original_form": "네 좋아해요!",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019498.1.3",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019498.1.4",
          "form": "네 좋아해요~^^ 어디가보셨어요?",
          "original_form": "네 좋아해요~^^ 어디가보셨어요?",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019498.1.5",
          "form": "체코랑 프랑스, 독일, 이탈리아 가봤어요!",
          "original_form": "체코랑 프랑스, 독일, 이탈리아 가봤어요!",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100019498.1.6",
          "form": "가보신 곳 중에 어디가 제일 좋으셨나요?",
          "original_form": "가보신 곳 중에 어디가 제일 좋으셨나요?",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100019498.1.7",
          "form": "우와 저는 유럽쪽은 한번도 못가봤어요\r\n후쿠오카 여러번~, 오사카 홍콩 마카오 대만 태국 세부갔었어요",
          "original_form": "우와 저는 유럽쪽은 한번도 못가봤어요\r\n후쿠오카 여러번~, 오사카 홍콩 마카오 대만 태국 세부갔었어요",
          "speaker_id": "1",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100019498.1.8",
          "form": "그중에서 저는 오사카요!",
          "original_form": "그중에서 저는 오사카요!",
          "speaker_id": "1",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100019498.1.9",
          "form": "어디가좋으셨어요?",
          "original_form": "어디가좋으셨어요?",
          "speaker_id": "1",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100019498.1.10",
          "form": "저는 이탈리아가 제일 좋았어요!",
          "original_form": "저는 이탈리아가 제일 좋았어요!",
          "speaker_id": "2",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100019498.1.11",
          "form": "저는 일본을 한번도 못 가봤네요 ㅎㅎ",
          "original_form": "저는 일본을 한번도 못 가봤네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100019498.1.12",
          "form": "오사카에서 뭐가 제일 좋으셨나요?",
          "original_form": "오사카에서 뭐가 제일 좋으셨나요?",
          "speaker_id": "2",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100019498.1.13",
          "form": "다재밋었는데 유니버셜스튜디오요~ 놀이기구안좋아하는데도 재밋었어요",
          "original_form": "다재밋었는데 유니버셜스튜디오요~ 놀이기구안좋아하는데도 재밋었어요",
          "speaker_id": "1",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100019498.1.14",
          "form": "이탈리아는 음식이 맛없다고 친구가 그러던데 어떠셨나요?",
          "original_form": "이탈리아는 음식이 맛없다고 친구가 그러던데 어떠셨나요?",
          "speaker_id": "1",
          "time": "20210828 12:36"
        },
        {
          "id": "MDRW2100019498.1.15",
          "form": "유니버셜은 싱가포르만 가봤는데, 거기는 해리포터가 없어서 아쉬웠어요!",
          "original_form": "유니버셜은 싱가포르만 가봤는데, 거기는 해리포터가 없어서 아쉬웠어요!",
          "speaker_id": "2",
          "time": "20210828 13:35"
        },
        {
          "id": "MDRW2100019498.1.16",
          "form": "이탈리아는 음식이.. 우리나라 이탈리아 음식점들이 워낙 잘하니까 기대치가 높은 상태로 가게 돼서 더 그런 것 같아요 ㅋㅋㅋㅋ",
          "original_form": "이탈리아는 음식이.. 우리나라 이탈리아 음식점들이 워낙 잘하니까 기대치가 높은 상태로 가게 돼서 더 그런 것 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100019498.1.17",
          "form": "ㅋㅋㅋ네 좀 식재료의 본연의맛?에 충실한 맛이라고 하더라구요",
          "original_form": "ㅋㅋㅋ네 좀 식재료의 본연의맛?에 충실한 맛이라고 하더라구요",
          "speaker_id": "1",
          "time": "20210828 13:56"
        },
        {
          "id": "MDRW2100019498.1.18",
          "form": "해리포터 두번인가 세번탓어요 너무짱재밋",
          "original_form": "해리포터 두번인가 세번탓어요 너무짱재밋",
          "speaker_id": "1",
          "time": "20210828 13:57"
        },
        {
          "id": "MDRW2100019498.1.19",
          "form": "맞아요 너무 살렸어요...\r\n소스도 너무 묽은 맛이고...\r\n커피는 정말 맛있었어요!",
          "original_form": "맞아요 너무 살렸어요...\r\n소스도 너무 묽은 맛이고...\r\n커피는 정말 맛있었어요!",
          "speaker_id": "2",
          "time": "20210828 14:06"
        },
        {
          "id": "MDRW2100019498.1.20",
          "form": "버터비어 마셔보셨나요?",
          "original_form": "버터비어 마셔보셨나요?",
          "speaker_id": "2",
          "time": "20210828 14:06"
        },
        {
          "id": "MDRW2100019498.1.21",
          "form": "아 네 해리포터 버터비어",
          "original_form": "아 네 해리포터 버터비어",
          "speaker_id": "1",
          "time": "20210828 14:28"
        },
        {
          "id": "MDRW2100019498.1.22",
          "form": "말씀하시는거죠?",
          "original_form": "말씀하시는거죠?",
          "speaker_id": "1",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100019498.1.23",
          "form": "네! 저 그거 먹어보고싶었는데 아직 기회가 없네요ㅠ",
          "original_form": "네! 저 그거 먹어보고싶었는데 아직 기회가 없네요ㅠ",
          "speaker_id": "2",
          "time": "20210828 14:33"
        },
        {
          "id": "MDRW2100019498.1.24",
          "form": "호불호 너무 갈려서 궁금해요 ㅎㅎ",
          "original_form": "호불호 너무 갈려서 궁금해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:33"
        },
        {
          "id": "MDRW2100019498.1.25",
          "form": "저는 별로였어요 한잔정도는 괜탆은거같구요",
          "original_form": "저는 별로였어요 한잔정도는 괜탆은거같구요",
          "speaker_id": "1",
          "time": "20210828 14:50"
        },
        {
          "id": "MDRW2100019498.1.26",
          "form": "유니버셜에서 기념품 많이 사셨나요?",
          "original_form": "유니버셜에서 기념품 많이 사셨나요?",
          "speaker_id": "2",
          "time": "20210828 14:58"
        }
      ]
