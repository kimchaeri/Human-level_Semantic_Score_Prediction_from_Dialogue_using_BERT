      "utterance": [
        {
          "id": "MDRW2100041430.1.1",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "2",
          "time": "20210907 13:52"
        },
        {
          "id": "MDRW2100041430.1.2",
          "form": "네 회사 구내식당에서 먹었어요",
          "original_form": "네 회사 구내식당에서 먹었어요",
          "speaker_id": "1",
          "time": "20210907 14:02"
        },
        {
          "id": "MDRW2100041430.1.3",
          "form": "구내식당 잘 나오는 편인가요?",
          "original_form": "구내식당 잘 나오는 편인가요?",
          "speaker_id": "2",
          "time": "20210907 14:11"
        },
        {
          "id": "MDRW2100041430.1.4",
          "form": "네 저희는 구내식당 식사가 잘 나오는 편이예요",
          "original_form": "네 저희는 구내식당 식사가 잘 나오는 편이예요",
          "speaker_id": "1",
          "time": "20210907 16:07"
        },
        {
          "id": "MDRW2100041430.1.5",
          "form": "특히 수요일엔 특식을 제공해요",
          "original_form": "특히 수요일엔 특식을 제공해요",
          "speaker_id": "1",
          "time": "20210907 16:07"
        },
        {
          "id": "MDRW2100041430.1.6",
          "form": "좋은 회사네요",
          "original_form": "좋은 회사네요",
          "speaker_id": "2",
          "time": "20210907 16:12"
        },
        {
          "id": "MDRW2100041430.1.7",
          "form": "매일 먹는 구내식당이 잘 나오면 회사 다닐 맛 나죠",
          "original_form": "매일 먹는 구내식당이 잘 나오면 회사 다닐 맛 나죠",
          "speaker_id": "2",
          "time": "20210907 16:12"
        },
        {
          "id": "MDRW2100041430.1.8",
          "form": "특식은 주로 어떤 게 나오나요?",
          "original_form": "특식은 주로 어떤 게 나오나요?",
          "speaker_id": "2",
          "time": "20210907 16:12"
        },
        {
          "id": "MDRW2100041430.1.9",
          "form": "그때그때 조금씩 다르뉘ㅣ",
          "original_form": "그때그때 조금씩 다르뉘ㅣ",
          "speaker_id": "1",
          "time": "20210907 17:12"
        },
        {
          "id": "MDRW2100041430.1.10",
          "form": "다른데",
          "original_form": "다른데",
          "speaker_id": "1",
          "time": "20210907 17:12"
        },
        {
          "id": "MDRW2100041430.1.11",
          "form": "지난주엔 갈비탕, 이번주는 주꾸미비빔밥...",
          "original_form": "지난주엔 갈비탕, 이번주는 주꾸미비빔밥...",
          "speaker_id": "1",
          "time": "20210907 17:12"
        },
        {
          "id": "MDRW2100041430.1.12",
          "form": "이런식으로 나와요",
          "original_form": "이런식으로 나와요",
          "speaker_id": "1",
          "time": "20210907 17:13"
        },
        {
          "id": "MDRW2100041430.1.13",
          "form": "와 직원복지가 잘 돼있는 회사네요",
          "original_form": "와 직원복지가 잘 돼있는 회사네요",
          "speaker_id": "2",
          "time": "20210907 17:15"
        },
        {
          "id": "MDRW2100041430.1.14",
          "form": "저녁도 회사에서 먹고 퇴근하나요?",
          "original_form": "저녁도 회사에서 먹고 퇴근하나요?",
          "speaker_id": "2",
          "time": "20210907 17:15"
        },
        {
          "id": "MDRW2100041430.1.15",
          "form": "아니요 다섯시쯤이면 퇴근이라",
          "original_form": "아니요 다섯시쯤이면 퇴근이라",
          "speaker_id": "1",
          "time": "20210907 17:19"
        },
        {
          "id": "MDRW2100041430.1.16",
          "form": "저녁은 집에서 먹어요",
          "original_form": "저녁은 집에서 먹어요",
          "speaker_id": "1",
          "time": "20210907 17:19"
        },
        {
          "id": "MDRW2100041430.1.17",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210907 17:20"
        },
        {
          "id": "MDRW2100041430.1.18",
          "form": "어떤 회사는 아침 저녁도 구내식당 이용할 수 있게 돼있더라구요",
          "original_form": "어떤 회사는 아침 저녁도 구내식당 이용할 수 있게 돼있더라구요",
          "speaker_id": "2",
          "time": "20210907 17:20"
        },
        {
          "id": "MDRW2100041430.1.19",
          "form": "오늘 저녁 메뉴는 정했나요?",
          "original_form": "오늘 저녁 메뉴는 정했나요?",
          "speaker_id": "2",
          "time": "20210907 17:20"
        },
        {
          "id": "MDRW2100041430.1.20",
          "form": "네 그래도 밥보단 퇴근이 좋아요ㅎㅎ",
          "original_form": "네 그래도 밥보단 퇴근이 좋아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 17:21"
        },
        {
          "id": "MDRW2100041430.1.21",
          "form": "오늘 저녁은 집에 가봐야 알 것 같은데",
          "original_form": "오늘 저녁은 집에 가봐야 알 것 같은데",
          "speaker_id": "1",
          "time": "20210907 17:21"
        },
        {
          "id": "MDRW2100041430.1.22",
          "form": "비가 와서 그런지 따끈한 국물류가 당기네요",
          "original_form": "비가 와서 그런지 따끈한 국물류가 당기네요",
          "speaker_id": "1",
          "time": "20210907 17:21"
        },
        {
          "id": "MDRW2100041430.1.23",
          "form": "저녁식사 메뉴 정하셨어요?",
          "original_form": "저녁식사 메뉴 정하셨어요?",
          "speaker_id": "1",
          "time": "20210907 17:25"
        },
        {
          "id": "MDRW2100041430.1.24",
          "form": "따끈한 국물 좋네요",
          "original_form": "따끈한 국물 좋네요",
          "speaker_id": "2",
          "time": "20210907 17:26"
        },
        {
          "id": "MDRW2100041430.1.25",
          "form": "저는 이제 해야하는데 인절미를 구워볼까 생각중이에요",
          "original_form": "저는 이제 해야하는데 인절미를 구워볼까 생각중이에요",
          "speaker_id": "2",
          "time": "20210907 17:26"
        },
        {
          "id": "MDRW2100041430.1.26",
          "form": "인절미 구운 것도 좋네요~",
          "original_form": "인절미 구운 것도 좋네요~",
          "speaker_id": "1",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100041430.1.27",
          "form": "네 맛있어요",
          "original_form": "네 맛있어요",
          "speaker_id": "2",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100041430.1.28",
          "form": "떡을 잔뜩 해놔서 틈틈히 먹어야 해요",
          "original_form": "떡을 잔뜩 해놔서 틈틈히 먹어야 해요",
          "speaker_id": "2",
          "time": "20210907 17:27"
        },
        {
          "id": "MDRW2100041430.1.29",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210907 17:28"
        },
        {
          "id": "MDRW2100041430.1.30",
          "form": "떡 해 놓은 것 있음 틈틈히 먹기 좋죠",
          "original_form": "떡 해 놓은 것 있음 틈틈히 먹기 좋죠",
          "speaker_id": "1",
          "time": "20210907 17:28"
        }
      ]
