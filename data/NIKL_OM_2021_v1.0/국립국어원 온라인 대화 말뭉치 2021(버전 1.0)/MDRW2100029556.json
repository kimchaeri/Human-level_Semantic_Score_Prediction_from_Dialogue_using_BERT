      "utterance": [
        {
          "id": "MDRW2100029556.1.1",
          "form": "네 리얼한거 같은데 ㅋㅋ",
          "original_form": "네 리얼한거 같은데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 10:23"
        },
        {
          "id": "MDRW2100029556.1.2",
          "form": "6화가 끝이라니 엉엉",
          "original_form": "6화가 끝이라니 엉엉",
          "speaker_id": "2",
          "time": "20210903 10:23"
        },
        {
          "id": "MDRW2100029556.1.3",
          "form": "설마 6부작인건가요?",
          "original_form": "설마 6부작인건가요?",
          "speaker_id": "1",
          "time": "20210903 10:31"
        },
        {
          "id": "MDRW2100029556.1.4",
          "form": "아님 뒤가 덜나온건가뇨",
          "original_form": "아님 뒤가 덜나온건가뇨",
          "speaker_id": "1",
          "time": "20210903 10:31"
        },
        {
          "id": "MDRW2100029556.1.5",
          "form": "6부작이요",
          "original_form": "6부작이요",
          "speaker_id": "2",
          "time": "20210903 10:35"
        },
        {
          "id": "MDRW2100029556.1.6",
          "form": "시즌1은 ㅎㅎ",
          "original_form": "시즌1은 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 10:35"
        },
        {
          "id": "MDRW2100029556.1.7",
          "form": "헉 그럼 시즌2 나올려면 한참이겠이요",
          "original_form": "헉 그럼 시즌2 나올려면 한참이겠이요",
          "speaker_id": "1",
          "time": "20210903 10:38"
        },
        {
          "id": "MDRW2100029556.1.8",
          "form": "또 금방할 거 같기도?",
          "original_form": "또 금방할 거 같기도?",
          "speaker_id": "2",
          "time": "20210903 10:41"
        },
        {
          "id": "MDRW2100029556.1.9",
          "form": "인기가 많아서 ㅎㅎ 금방 제작각",
          "original_form": "인기가 많아서 ㅎㅎ 금방 제작각",
          "speaker_id": "1",
          "time": "20210903 10:45"
        },
        {
          "id": "MDRW2100029556.1.10",
          "form": "최근 본 영화는 뭐있으세요",
          "original_form": "최근 본 영화는 뭐있으세요",
          "speaker_id": "1",
          "time": "20210903 10:57"
        },
        {
          "id": "MDRW2100029556.1.11",
          "form": "모가디슈 봤어요",
          "original_form": "모가디슈 봤어요",
          "speaker_id": "2",
          "time": "20210903 11:40"
        },
        {
          "id": "MDRW2100029556.1.12",
          "form": "name1님은?",
          "original_form": "&name1&님은?",
          "speaker_id": "2",
          "time": "20210903 11:40"
        },
        {
          "id": "MDRW2100029556.1.13",
          "form": "오오 저도요",
          "original_form": "오오 저도요",
          "speaker_id": "1",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100029556.1.14",
          "form": "그거 실화라면서요",
          "original_form": "그거 실화라면서요",
          "speaker_id": "1",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100029556.1.15",
          "form": "네ㅋㅋ약간 각색한?",
          "original_form": "네ㅋㅋ약간 각색한?",
          "speaker_id": "2",
          "time": "20210903 12:43"
        },
        {
          "id": "MDRW2100029556.1.16",
          "form": "마자요 실제로는 마중안나오고 인사도 했다더라구요 ㅋㅋ",
          "original_form": "마자요 실제로는 마중안나오고 인사도 했다더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 12:52"
        },
        {
          "id": "MDRW2100029556.1.17",
          "form": "네ㅋㅋ 근데 영화가 더 잘 살린듯",
          "original_form": "네ㅋㅋ 근데 영화가 더 잘 살린듯",
          "speaker_id": "2",
          "time": "20210903 12:54"
        },
        {
          "id": "MDRW2100029556.1.18",
          "form": "그러게요 ㅎㅎ 눈물 한방울 훔쳤다는",
          "original_form": "그러게요 ㅎㅎ 눈물 한방울 훔쳤다는",
          "speaker_id": "1",
          "time": "20210903 12:58"
        },
        {
          "id": "MDRW2100029556.1.19",
          "form": "눈물이 많으시군요!",
          "original_form": "눈물이 많으시군요!",
          "speaker_id": "2",
          "time": "20210903 14:14"
        },
        {
          "id": "MDRW2100029556.1.20",
          "form": "마자요ㅜㅋㅋ 별거 아닌거 봐도 슬프더라구요",
          "original_form": "마자요ㅜㅋㅋ 별거 아닌거 봐도 슬프더라구요",
          "speaker_id": "1",
