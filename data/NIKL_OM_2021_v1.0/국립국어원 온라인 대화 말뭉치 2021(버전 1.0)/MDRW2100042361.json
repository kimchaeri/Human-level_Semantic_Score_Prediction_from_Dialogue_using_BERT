      "utterance": [
        {
          "id": "MDRW2100042361.1.1",
          "form": "저는 유퀴즈요",
          "original_form": "저는 유퀴즈요",
          "speaker_id": "2",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042361.1.2",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042361.1.3",
          "form": "저는 놀면뭐하니.. 환승연애",
          "original_form": "저는 놀면뭐하니.. 환승연애",
          "speaker_id": "1",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100042361.1.4",
          "form": "최근엔 뭉쳐야 쏜다 봣었어요 ㅋㅋ",
          "original_form": "최근엔 뭉쳐야 쏜다 봣었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100042361.1.5",
          "form": "놀면뭐하니 요즘 재밌나요?",
          "original_form": "놀면뭐하니 요즘 재밌나요?",
          "speaker_id": "2",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042361.1.6",
          "form": "아 저번주를 놓쳣어요 ㅠㅠ",
          "original_form": "아 저번주를 놓쳣어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042361.1.7",
          "form": "무도 멤버들이 나오나봐요!!",
          "original_form": "무도 멤버들이 나오나봐요!!",
          "speaker_id": "1",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042361.1.8",
          "form": "무도 좋아하셧어요?",
          "original_form": "무도 좋아하셧어요?",
          "speaker_id": "1",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042361.1.9",
          "form": "네 무도 좋아하셨나요?",
          "original_form": "네 무도 좋아하셨나요?",
          "speaker_id": "2",
          "time": "20210906 22:01"
        },
        {
          "id": "MDRW2100042361.1.10",
          "form": "네네 너뮤 재밌었는데 아쉬워요 ㅠㅠ",
          "original_form": "네네 너뮤 재밌었는데 아쉬워요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042361.1.11",
          "form": "멤버들도 다 늙엇드라구요",
          "original_form": "멤버들도 다 늙엇드라구요",
          "speaker_id": "1",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042361.1.12",
          "form": "그러니까요 진짜 생각보다 시간이 후딱 지나갔더라고요",
          "original_form": "그러니까요 진짜 생각보다 시간이 후딱 지나갔더라고요",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042361.1.13",
          "form": "유퀴즈는 올림픽 멤버 나오는거 본거 같아요!!",
          "original_form": "유퀴즈는 올림픽 멤버 나오는거 본거 같아요!!",
          "speaker_id": "1",
          "time": "20210906 23:35"
        },
        {
          "id": "MDRW2100042361.1.14",
          "form": "재밌었나요?",
          "original_form": "재밌었나요?",
          "speaker_id": "2",
          "time": "20210906 23:36"
        },
        {
          "id": "MDRW2100042361.1.15",
          "form": "네!!! 재밌었어요 ㅋㅋㅋㅋ",
          "original_form": "네!!! 재밌었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:38"
        },
        {
          "id": "MDRW2100042361.1.16",
          "form": "근데 옛날에 시민 인터뷰할때가 더 재밋었어요 ㅋㅋ",
          "original_form": "근데 옛날에 시민 인터뷰할때가 더 재밋었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:39"
        },
        {
          "id": "MDRW2100042361.1.17",
          "form": "맞아요 그 때가 뭔가 더 날 것의 느낌이 있어서 좋았는데 말이죠",
          "original_form": "맞아요 그 때가 뭔가 더 날 것의 느낌이 있어서 좋았는데 말이죠",
          "speaker_id": "2",
          "time": "20210906 23:46"
        },
        {
          "id": "MDRW2100042361.1.18",
          "form": "맞아요 ㅠㅠ 지금은 코로나땜에 ㅜㅜ",
          "original_form": "맞아요 ㅠㅠ 지금은 코로나땜에 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 11:11"
        },
        {
          "id": "MDRW2100042361.1.19",
          "form": "다시 햇음 좋겟어요 ㅋㅋㅋ",
          "original_form": "다시 햇음 좋겟어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:11"
        },
