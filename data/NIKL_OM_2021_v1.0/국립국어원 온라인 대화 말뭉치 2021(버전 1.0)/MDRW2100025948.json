      "utterance": [
        {
          "id": "MDRW2100025948.1.1",
          "form": "어느쪽에서 일하고 싶으세요?",
          "original_form": "어느쪽에서 일하고 싶으세요?",
          "speaker_id": "1",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100025948.1.2",
          "form": "아직 구체적으로 생각해보진 않았는데",
          "original_form": "아직 구체적으로 생각해보진 않았는데",
          "speaker_id": "2",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100025948.1.3",
          "form": "아마 제약회사 다니지 않을까 싶어요!",
          "original_form": "아마 제약회사 다니지 않을까 싶어요!",
          "speaker_id": "2",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100025948.1.4",
          "form": "제약회사 돈 많이 버시겠어용",
          "original_form": "제약회사 돈 많이 버시겠어용",
          "speaker_id": "1",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100025948.1.5",
          "form": "네 들어가고 싶네요ㅎㅎㅎ",
          "original_form": "네 들어가고 싶네요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:00"
        },
        {
          "id": "MDRW2100025948.1.6",
          "form": "지금 직장 있으신가요??",
          "original_form": "지금 직장 있으신가요??",
          "speaker_id": "2",
          "time": "20210901 20:00"
        },
        {
          "id": "MDRW2100025948.1.7",
          "form": "네 4년차에요ㅋㅋ",
          "original_form": "네 4년차에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100025948.1.8",
          "form": "오~ 어떤일하세요??",
          "original_form": "오~ 어떤일하세요??",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100025948.1.9",
          "form": "저는 others에서 일해요 ㅎㅎ",
          "original_form": "저는 &others&에서 일해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100025948.1.10",
          "form": "저희 가족중에도 others에서 근무하는 사람 있어요ㅎㅎㅎ",
          "original_form": "저희 가족중에도 &others&에서 근무하는 사람 있어요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100025948.1.11",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "2",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100025948.1.12",
          "form": "오잉 설마",
          "original_form": "오잉 설마",
          "speaker_id": "1",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100025948.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100025948.1.14",
          "form": "저는 서울이라 가족이 affiliation에서 일해요!",
          "original_form": "저는 서울이라 가족이 &affiliation&에서 일해요!",
          "speaker_id": "2",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100025948.1.15",
          "form": "어라",
          "original_form": "어라",
          "speaker_id": "1",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100025948.1.16",
          "form": "겹치는데ㅋㅋㅋㅋㅋㅋ",
          "original_form": "겹치는데ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:41"
        },
        {
          "id": "MDRW2100025948.1.17",
          "form": "엇 너무 신기하네요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "엇 너무 신기하네요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100025948.1.18",
          "form": "직장은 적성에 잘 맞으세요??",
          "original_form": "직장은 적성에 잘 맞으세요??",
          "speaker_id": "2",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100025948.1.19",
          "form": "ㅋㅋ혹시 아버지가.....",
          "original_form": "ㅋㅋ혹시 아버지가.....",
          "speaker_id": "1",
          "time": "20210901 22:46"
