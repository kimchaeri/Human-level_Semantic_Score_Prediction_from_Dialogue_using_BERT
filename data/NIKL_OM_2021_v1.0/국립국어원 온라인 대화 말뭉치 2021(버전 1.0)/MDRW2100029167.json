      "utterance": [
        {
          "id": "MDRW2100029167.1.1",
          "form": "자 알려주세요",
          "original_form": "자 알려주세요",
          "speaker_id": "2",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100029167.1.2",
          "form": "제가 결혼하기 전 남자친구랑\n헤어지게된 이유중의 하나도\n너무 효자여서였던것도 있는듯해요",
          "original_form": "제가 결혼하기 전 남자친구랑\n헤어지게된 이유중의 하나도\n너무 효자여서였던것도 있는듯해요",
          "speaker_id": "1",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100029167.1.3",
          "form": "그 친구랑도 결혼 이야기까지 오가고\n상견례까진 아니여도 거의 직전까지\n갔었는데... 안녕했었죠",
          "original_form": "그 친구랑도 결혼 이야기까지 오가고\n상견례까진 아니여도 거의 직전까지\n갔었는데... 안녕했었죠",
          "speaker_id": "1",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029167.1.4",
          "form": "저랑은 다른이유지만",
          "original_form": "저랑은 다른이유지만",
          "speaker_id": "2",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029167.1.5",
          "form": "그정도까지 갔다가 헤어지는건",
          "original_form": "그정도까지 갔다가 헤어지는건",
          "speaker_id": "2",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100029167.1.6",
          "form": "흔한가봐요",
          "original_form": "흔한가봐요",
          "speaker_id": "2",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100029167.1.7",
          "form": "이래서 식장 들어가기전까진",
          "original_form": "이래서 식장 들어가기전까진",
          "speaker_id": "1",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100029167.1.8",
          "form": "모른다고들 하나 봅니다.ㅋㅋㅋㅋㅋ",
          "original_form": "모른다고들 하나 봅니다.ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100029167.1.9",
          "form": "사실",
          "original_form": "사실",
          "speaker_id": "2",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100029167.1.10",
          "form": "결혼얘기가 오가는건",
          "original_form": "결혼얘기가 오가는건",
          "speaker_id": "2",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100029167.1.11",
          "form": "아무것도 안한건데",
          "original_form": "아무것도 안한건데",
          "speaker_id": "2",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100029167.1.12",
          "form": "청첩장 찍어놓고도 헤어지는 마당에",
          "original_form": "청첩장 찍어놓고도 헤어지는 마당에",
          "speaker_id": "2",
          "time": "20210903 00:04"
        },
        {
          "id": "MDRW2100029167.1.13",
          "form": "맞아요...  정말 그래요...",
          "original_form": "맞아요...  정말 그래요...",
          "speaker_id": "1",
          "time": "20210903 00:04"
        },
        {
          "id": "MDRW2100029167.1.14",
          "form": "효자가  얼마나 효자였길래",
          "original_form": "효자가  얼마나 효자였길래",
          "speaker_id": "2",
          "time": "20210903 00:06"
        },
        {
          "id": "MDRW2100029167.1.15",
          "form": "헤어질생각까지 하신거에요?",
          "original_form": "헤어질생각까지 하신거에요?",
          "speaker_id": "2",
          "time": "20210903 00:06"
        },
        {
          "id": "MDRW2100029167.1.16",
          "form": "엄마가 그 동안 키우느라\n고생했으니 월급을 거의",
          "original_form": "엄마가 그 동안 키우느라\n고생했으니 월급을 거의",
          "speaker_id": "1",
          "time": "20210903 00:06"
        },
        {
          "id": "MDRW2100029167.1.17",
          "form": "엄마를 위해서 쓰더라구요",
          "original_form": "엄마를 위해서 쓰더라구요",
          "speaker_id": "1",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100029167.1.18",
          "form": "거의 엄마를 위해??",
          "original_form": "거의 엄마를 위해??",
          "speaker_id": "2",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100029167.1.19",
          "form": "엄마가 아들뺐겼다고하기 딱인데",
          "original_form": "엄마가 아들뺐겼다고하기 딱인데",
          "speaker_id": "2",
          "time": "20210903 00:07"
        },
        {
          "id": "MDRW2100029167.1.20",
          "form": "나이도 제가 더 많았ㅋㅋㅋㅋㅋㅋ",
          "original_form": "나이도 제가 더 많았ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029167.1.21",
          "form": "결혼 했겠죠?",
          "original_form": "결혼 했겠죠?",
          "speaker_id": "2",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029167.1.22",
          "form": "헛 연하?",
          "original_form": "헛 연하?",
          "speaker_id": "2",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029167.1.23",
          "form": "모르겠어요....",
          "original_form": "모르겠어요....",
          "speaker_id": "1",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029167.1.24",
          "form": "해서 잘 살고있으면 좋겠어요",
          "original_form": "해서 잘 살고있으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029167.1.25",
          "form": "많이 붙잡았었는데...",
          "original_form": "많이 붙잡았었는데...",
          "speaker_id": "1",
          "time": "20210903 00:08"
        },
        {
          "id": "MDRW2100029167.1.26",
          "form": "님을요?",
          "original_form": "님을요?",
          "speaker_id": "2",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029167.1.27",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029167.1.28",
          "form": "아직도 미안한 마음이 많이 남아서\n잘 살았음 좋겠어요 진심으로",
          "original_form": "아직도 미안한 마음이 많이 남아서\n잘 살았음 좋겠어요 진심으로",
          "speaker_id": "1",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029167.1.29",
          "form": "그게 그렇게 오래 남는군요?",
          "original_form": "그게 그렇게 오래 남는군요?",
          "speaker_id": "2",
          "time": "20210903 00:09"
        },
        {
          "id": "MDRW2100029167.1.30",
          "form": "아버님도 엄청 예뻐해주셨었거든요",
          "original_form": "아버님도 엄청 예뻐해주셨었거든요",
          "speaker_id": "1",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100029167.1.31",
          "form": "부모님께도 상처가 되었을테니\n그것도 죄송한 마음이고",
          "original_form": "부모님께도 상처가 되었을테니\n그것도 죄송한 마음이고",
          "speaker_id": "1",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100029167.1.32",
          "form": "아 부모님도 다 만나셨었구나",
          "original_form": "아 부모님도 다 만나셨었구나",
          "speaker_id": "2",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100029167.1.33",
          "form": "네 ㅎㅎ 결혼 이야기 오갔다고\n했잖아요",
          "original_form": "네 ㅎㅎ 결혼 이야기 오갔다고\n했잖아요",
          "speaker_id": "1",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100029167.1.34",
          "form": "서로 부모님들께 인사도\n드렸었어요... ㅎ",
          "original_form": "서로 부모님들께 인사도\n드렸었어요... ㅎ",
          "speaker_id": "1",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100029167.1.35",
          "form": "그럼 진행이 좀 된거네요",
          "original_form": "그럼 진행이 좀 된거네요",
          "speaker_id": "2",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100029167.1.36",
          "form": "네이트판에서 보던 그런상황인건가",
          "original_form": "네이트판에서 보던 그런상황인건가",
          "speaker_id": "2",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100029167.1.37",
          "form": "그런 상황이 어떤 상황이죵? ㅋㅋ",
          "original_form": "그런 상황이 어떤 상황이죵? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:12"
        }
      ]
    }
  ]
