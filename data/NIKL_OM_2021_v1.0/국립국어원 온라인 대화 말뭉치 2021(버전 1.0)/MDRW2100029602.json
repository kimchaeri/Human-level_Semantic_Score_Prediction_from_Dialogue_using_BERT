      "utterance": [
        {
          "id": "MDRW2100029602.1.1",
          "form": "보아하니 밭일을 도와주러 가시는군요",
          "original_form": "보아하니 밭일을 도와주러 가시는군요",
          "speaker_id": "2",
          "time": "20210903 10:51"
        },
        {
          "id": "MDRW2100029602.1.2",
          "form": "흐규 이번에 블로그안해서",
          "original_form": "흐규 이번에 블로그안해서",
          "speaker_id": "1",
          "time": "20210903 10:57"
        },
        {
          "id": "MDRW2100029602.1.3",
          "form": "많이못팔았어",
          "original_form": "많이못팔았어",
          "speaker_id": "1",
          "time": "20210903 10:58"
        },
        {
          "id": "MDRW2100029602.1.4",
          "form": "엄마아빠 도와야하는데",
          "original_form": "엄마아빠 도와야하는데",
          "speaker_id": "1",
          "time": "20210903 10:58"
        },
        {
          "id": "MDRW2100029602.1.5",
          "form": "효녀네 효녀야..",
          "original_form": "효녀네 효녀야..",
          "speaker_id": "2",
          "time": "20210903 11:36"
        },
        {
          "id": "MDRW2100029602.1.6",
          "form": "그치나효녀야",
          "original_form": "그치나효녀야",
          "speaker_id": "1",
          "time": "20210903 11:58"
        },
        {
          "id": "MDRW2100029602.1.7",
          "form": "도시락 싸달아그럴거",
          "original_form": "도시락 싸달아그럴거",
          "speaker_id": "1",
          "time": "20210903 11:58"
        },
        {
          "id": "MDRW2100029602.1.8",
          "form": "갑자기불효어택",
          "original_form": "갑자기불효어택",
          "speaker_id": "2",
          "time": "20210903 12:21"
        },
        {
          "id": "MDRW2100029602.1.9",
          "form": "주말내내 본가가있으시나요?",
          "original_form": "주말내내 본가가있으시나요?",
          "speaker_id": "2",
          "time": "20210903 12:21"
        },
        {
          "id": "MDRW2100029602.1.10",
          "form": "네..그리고 일요일은 친구들이랑 영화봐요 ^^",
          "original_form": "네..그리고 일요일은 친구들이랑 영화봐요 ^^",
          "speaker_id": "1",
          "time": "20210903 13:00"
        },
        {
          "id": "MDRW2100029602.1.11",
          "form": "주말은 가족이랑 함께",
          "original_form": "주말은 가족이랑 함께",
          "speaker_id": "1",
          "time": "20210903 13:01"
        },
        {
          "id": "MDRW2100029602.1.12",
          "form": "ㅋㅋㅋㅋ가정적이시네요..ㅎ",
          "original_form": "ㅋㅋㅋㅋ가정적이시네요..ㅎ",
          "speaker_id": "2",
          "time": "20210903 13:19"
        },
        {
          "id": "MDRW2100029602.1.13",
          "form": "자취집 방세 아깝지않나욤...대신 살아드릴게요..ㅎ",
          "original_form": "자취집 방세 아깝지않나욤...대신 살아드릴게요..ㅎ",
          "speaker_id": "2",
          "time": "20210903 13:20"
        },
        {
          "id": "MDRW2100029602.1.14",
          "form": "아깝네요 ㅎ 주말마다 오세요",
          "original_form": "아깝네요 ㅎ 주말마다 오세요",
          "speaker_id": "1",
          "time": "20210903 13:21"
        },
        {
          "id": "MDRW2100029602.1.15",
          "form": "그래도 가족이 최고",
          "original_form": "그래도 가족이 최고",
          "speaker_id": "1",
          "time": "20210903 13:21"
        },
        {
          "id": "MDRW2100029602.1.16",
          "form": "쿠키곰돌이봐야지",
          "original_form": "쿠키곰돌이봐야지",
          "speaker_id": "1",
          "time": "20210903 13:21"
        },
        {
          "id": "MDRW2100029602.1.17",
          "form": "그건맞아요... 봐야쥐 내새꾸들..",
          "original_form": "그건맞아요... 봐야쥐 내새꾸들..",
          "speaker_id": "2",
          "time": "20210903 13:21"
        },
        {
          "id": "MDRW2100029602.1.18",
          "form": "맞아 쿠키가 언니오기만을 기다릴텐데",
          "original_form": "맞아 쿠키가 언니오기만을 기다릴텐데",
          "speaker_id": "1",
          "time": "20210903 13:28"
        },
        {
          "id": "MDRW2100029602.1.19",
          "form": "맞아 ㅎㅎ 머 안사가?본가방문시 두손 무겁게..",
          "original_form": "맞아 ㅎㅎ 머 안사가?본가방문시 두손 무겁게..",
          "speaker_id": "2",
          "time": "20210903 13:31"
        },
        {
          "id": "MDRW2100029602.1.20",
          "form": "미안한데 돈 없어",
          "original_form": "미안한데 돈 없어",
          "speaker_id": "1",
          "time": "20210903 13:37"
        },
        {
          "id": "MDRW2100029602.1.21",
          "form": "대신 재난지원금 받으면 나 영양제 하나 사고 나머지 엄마 줄 예정..",
          "original_form": "대신 재난지원금 받으면 나 영양제 하나 사고 나머지 엄마 줄 예정..",
          "speaker_id": "1",
          "time": "20210903 13:37"
        },
        {
          "id": "MDRW2100029602.1.22",
          "form": "ㅋㅋㅋ ㅜㅜ난안나와서 부모님드릴것도없어",
          "original_form": "ㅋㅋㅋ ㅜㅜ난안나와서 부모님드릴것도없어",
          "speaker_id": "2",
