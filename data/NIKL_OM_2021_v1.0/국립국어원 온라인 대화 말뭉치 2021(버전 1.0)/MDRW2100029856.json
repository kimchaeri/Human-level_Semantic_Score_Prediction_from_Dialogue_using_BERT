      "utterance": [
        {
          "id": "MDRW2100029856.1.1",
          "form": "번데기 좋아함",
          "original_form": "번데기 좋아함",
          "speaker_id": "2",
          "time": "20210903 15:36"
        },
        {
          "id": "MDRW2100029856.1.2",
          "form": "번데기 먹고싶어요 갑자기 ㅋㅋ",
          "original_form": "번데기 먹고싶어요 갑자기 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:36"
        },
        {
          "id": "MDRW2100029856.1.3",
          "form": "오징어초무침은 막걸린데 ㅋㅋ",
          "original_form": "오징어초무침은 막걸린데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:36"
        },
        {
          "id": "MDRW2100029856.1.4",
          "form": "ㅋㅋ 소막은 자제할게요",
          "original_form": "ㅋㅋ 소막은 자제할게요",
          "speaker_id": "1",
          "time": "20210903 15:39"
        },
        {
          "id": "MDRW2100029856.1.5",
          "form": "그래도 백신 다음날 몸 사려야죠ㅎ",
          "original_form": "그래도 백신 다음날 몸 사려야죠ㅎ",
          "speaker_id": "1",
          "time": "20210903 15:39"
        },
        {
          "id": "MDRW2100029856.1.6",
          "form": "번데기 멍게 이런거 다 너무 좋아하는데",
          "original_form": "번데기 멍게 이런거 다 너무 좋아하는데",
          "speaker_id": "1",
          "time": "20210903 15:39"
        },
        {
          "id": "MDRW2100029856.1.7",
          "form": "딴사람들과 술자리서 먹기엔 호불호 갈리는 메뉴라 아쉽",
          "original_form": "딴사람들과 술자리서 먹기엔 호불호 갈리는 메뉴라 아쉽",
          "speaker_id": "1",
          "time": "20210903 15:40"
        },
        {
          "id": "MDRW2100029856.1.8",
          "form": "맞습니다ㅎㅎ",
          "original_form": "맞습니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 15:40"
        },
        {
          "id": "MDRW2100029856.1.9",
          "form": "딴사람들과 술자리는 멍게까지는",
          "original_form": "딴사람들과 술자리는 멍게까지는",
          "speaker_id": "2",
          "time": "20210903 15:41"
        },
        {
          "id": "MDRW2100029856.1.10",
          "form": "굿굿?",
          "original_form": "굿굿?",
          "speaker_id": "2",
          "time": "20210903 15:41"
        },
        {
          "id": "MDRW2100029856.1.11",
          "form": "술자리 얼마 주기로 하시나요",
          "original_form": "술자리 얼마 주기로 하시나요",
          "speaker_id": "2",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100029856.1.12",
          "form": "평일 기준 일주일에 두번?",
          "original_form": "평일 기준 일주일에 두번?",
          "speaker_id": "1",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100029856.1.13",
          "form": "혼술은 안 셉니다ㅎㅎ",
          "original_form": "혼술은 안 셉니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 15:43"
        },
        {
          "id": "MDRW2100029856.1.14",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210903 15:43"
        },
        {
          "id": "MDRW2100029856.1.15",
          "form": "코로나 덕에 담날 멀쩡하네요 ㅎㅎ",
          "original_form": "코로나 덕에 담날 멀쩡하네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 15:43"
        },
        {
          "id": "MDRW2100029856.1.16",
          "form": "이차 삼차가 없으니",
          "original_form": "이차 삼차가 없으니",
          "speaker_id": "1",
          "time": "20210903 15:43"
        },
        {
          "id": "MDRW2100029856.1.17",
          "form": "그럼 매일 술이네요",
          "original_form": "그럼 매일 술이네요",
          "speaker_id": "2",
          "time": "20210903 15:43"
        },
        {
          "id": "MDRW2100029856.1.18",
          "form": "님은욫?",
          "original_form": "님은욫?",
          "speaker_id": "1",
          "time": "20210903 15:43"
        },
        {
          "id": "MDRW2100029856.1.19",
          "form": "저도 매일 술이엇는데",
          "original_form": "저도 매일 술이엇는데",
          "speaker_id": "2",
          "time": "20210903 15:43"
        },
        {
          "id": "MDRW2100029856.1.20",
          "form": "저녁 겸사겸사인 경우가 많죠",
          "original_form": "저녁 겸사겸사인 경우가 많죠",
          "speaker_id": "1",
          "time": "20210903 15:44"
        },
        {
          "id": "MDRW2100029856.1.21",
          "form": "최근에 합치고 나서",
          "original_form": "최근에 합치고 나서",
          "speaker_id": "2",
          "time": "20210903 15:44"
        },
        {
          "id": "MDRW2100029856.1.22",
          "form": "저 살림합친지 한달반?",
          "original_form": "저 살림합친지 한달반?",
          "speaker_id": "2",
          "time": "20210903 15:44"
        },
        {
          "id": "MDRW2100029856.1.23",
          "form": "자제했어요",
          "original_form": "자제했어요",
          "speaker_id": "2",
          "time": "20210903 15:44"
        },
        {
          "id": "MDRW2100029856.1.24",
          "form": "ㅋㄱㅋㅋㅋ",
          "original_form": "ㅋㄱㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:44"
        },
        {
          "id": "MDRW2100029856.1.25",
          "form": "원래 격주부부였군요ㅎ",
          "original_form": "원래 격주부부였군요ㅎ",
          "speaker_id": "1",
          "time": "20210903 15:45"
        },
        {
          "id": "MDRW2100029856.1.26",
          "form": "지금 나름의 허니문기간이네요ㅎㅎ",
          "original_form": "지금 나름의 허니문기간이네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 15:45"
        },
        {
          "id": "MDRW2100029856.1.27",
          "form": "주말부부햇엇거든요",
          "original_form": "주말부부햇엇거든요",
          "speaker_id": "2",
