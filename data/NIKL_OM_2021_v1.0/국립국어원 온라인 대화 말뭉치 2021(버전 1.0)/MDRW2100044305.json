        {
          "id": "MDRW2100044305.1.1",
          "form": "님 야식 안드심 ?",
          "original_form": "님 야식 안드심 ?",
          "speaker_id": "1",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044305.1.2",
          "form": "만두먹었더니 속안좋아요ㅠ",
          "original_form": "만두먹었더니 속안좋아요ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044305.1.3",
          "form": "야식먹을거에요?",
          "original_form": "야식먹을거에요?",
          "speaker_id": "2",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044305.1.4",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044305.1.5",
          "form": "만두 머요?",
          "original_form": "만두 머요?",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044305.1.6",
          "form": "얇은피만두요",
          "original_form": "얇은피만두요",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044305.1.7",
          "form": "오 무슨맛 ?",
          "original_form": "오 무슨맛 ?",
          "speaker_id": "1",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044305.1.8",
          "form": "김치맛이오",
          "original_form": "김치맛이오",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044305.1.9",
          "form": "에프에 돌렸어요?",
          "original_form": "에프에 돌렸어요?",
          "speaker_id": "1",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044305.1.10",
          "form": "네 에어프라이어요!!",
          "original_form": "네 에어프라이어요!!",
          "speaker_id": "2",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044305.1.11",
          "form": "몇분돌려요 ?",
          "original_form": "몇분돌려요 ?",
          "speaker_id": "1",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044305.1.12",
          "form": "저는 10분하고 뒤집어서 5분더해요",
          "original_form": "저는 10분하고 뒤집어서 5분더해요",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044305.1.13",
          "form": "잘 익나요?",
          "original_form": "잘 익나요?",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044305.1.14",
          "form": "네 뒤집어줘야해요",
          "original_form": "네 뒤집어줘야해요",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044305.1.15",
