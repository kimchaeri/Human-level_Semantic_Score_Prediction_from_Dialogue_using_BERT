      "utterance": [
        {
          "id": "MDRW2100039756.1.1",
          "form": "그래도 눈사람 만드는건 쟈밌지않나용",
          "original_form": "그래도 눈사람 만드는건 쟈밌지않나용",
          "speaker_id": "1",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100039756.1.2",
          "form": "네네 눈싸움하고 눈사람만드는건 재밌어요ㅋㅋ",
          "original_form": "네네 눈싸움하고 눈사람만드는건 재밌어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:12"
        },
        {
          "id": "MDRW2100039756.1.3",
          "form": "썰매도 타고싶내요 ㅎㅎ",
          "original_form": "썰매도 타고싶내요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100039756.1.4",
          "form": "스키는 탈줄아새요?",
          "original_form": "스키는 탈줄아새요?",
          "speaker_id": "1",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100039756.1.5",
          "form": "잘은 못타지만 뚝딱이면서 타긴타요..ㅎㅎ",
          "original_form": "잘은 못타지만 뚝딱이면서 타긴타요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100039756.1.6",
          "form": "탈줄아는게 어디얘요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "탈줄아는게 어디얘요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 09:31"
        },
        {
          "id": "MDRW2100039756.1.7",
          "form": "저 사실 한 번도 안 타봤거든요",
          "original_form": "저 사실 한 번도 안 타봤거든요",
          "speaker_id": "1",
          "time": "20210904 09:31"
        },
        {
          "id": "MDRW2100039756.1.8",
          "form": "저도 한두번밖에 안타봤어요..!!",
          "original_form": "저도 한두번밖에 안타봤어요..!!",
          "speaker_id": "2",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100039756.1.9",
          "form": "한번 가보셔요 뚝딱이게 되지만 은근 재밌더라구요",
          "original_form": "한번 가보셔요 뚝딱이게 되지만 은근 재밌더라구요",
          "speaker_id": "2",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100039756.1.10",
          "form": "너무 가고싶네요...",
          "original_form": "너무 가고싶네요...",
          "speaker_id": "1",
          "time": "20210906 16:43"
        },
        {
          "id": "MDRW2100039756.1.11",
          "form": "코로나.. 이좌식",
          "original_form": "코로나.. 이좌식",
          "speaker_id": "1",
          "time": "20210906 16:43"
        },
        {
          "id": "MDRW2100039756.1.12",
          "form": "놀로가고싶어도 맘대로 안되는게 속상허네요 ㅠ",
          "original_form": "놀로가고싶어도 맘대로 안되는게 속상허네요 ㅠ",
          "speaker_id": "1",
          "time": "20210906 16:43"
        },
        {
          "id": "MDRW2100039756.1.13",
          "form": "맞아여... 슬프네요..",
          "original_form": "맞아여... 슬프네요..",
          "speaker_id": "2",
          "time": "20210906 17:30"
        },
        {
          "id": "MDRW2100039756.1.14",
          "form": "지금 비오나요? 아까부터 비가 오네요",
          "original_form": "지금 비오나요? 아까부터 비가 오네요",
          "speaker_id": "2",
          "time": "20210906 17:30"
        },
        {
          "id": "MDRW2100039756.1.15",
          "form": "어유 진짜 많이와요",
          "original_form": "어유 진짜 많이와요",
          "speaker_id": "1",
          "time": "20210906 22:48"
        },
        {
          "id": "MDRW2100039756.1.16",
          "form": "집오다가 다젖엇어요",
          "original_form": "집오다가 다젖엇어요",
          "speaker_id": "1",
          "time": "20210906 22:48"
        },
        {
          "id": "MDRW2100039756.1.17",
          "form": "오늘도 비오네여...",
          "original_form": "오늘도 비오네여...",
          "speaker_id": "2",
          "time": "20210907 17:28"
        },
        {
          "id": "MDRW2100039756.1.18",
          "form": "주말에 날이 좋더니 평일에는 비가 이렇게 오네요..",
          "original_form": "주말에 날이 좋더니 평일에는 비가 이렇게 오네요..",
          "speaker_id": "2",
          "time": "20210907 17:29"
        },
        {
          "id": "MDRW2100039756.1.19",
          "form": "뭔가 더 힘든기분이에요 ..",
          "original_form": "뭔가 더 힘든기분이에요 ..",
          "speaker_id": "1",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100039756.1.20",
          "form": "사람이 같이 눅눅..",
          "original_form": "사람이 같이 눅눅..",
          "speaker_id": "1",
          "time": "20210907 22:45"
        },
        {
          "id": "MDRW2100039756.1.21",
          "form": "그러니까요 시원은 한데 약간 습해서 답답하네여..",
          "original_form": "그러니까요 시원은 한데 약간 습해서 답답하네여..",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100039756.1.22",
          "form": "저는 너무 나가고싶어요",
          "original_form": "저는 너무 나가고싶어요",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100039756.1.23",
          "form": "먀일 산책안하고는 못사는사람 ㅠㅠ",
          "original_form": "먀일 산책안하고는 못사는사람 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:48"
        },
        {
          "id": "MDRW2100039756.1.24",
          "form": "그니까여 지금날씨에 산책하면 딱인데 말이죠",
          "original_form": "그니까여 지금날씨에 산책하면 딱인데 말이죠",
          "speaker_id": "2",
          "time": "20210907 22:49"
        }
      ]
    }
  ]
