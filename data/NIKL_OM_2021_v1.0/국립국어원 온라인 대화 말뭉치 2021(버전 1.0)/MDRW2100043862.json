          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043862.1.6",
          "form": "전 닭볶음탕이요!",
          "original_form": "전 닭볶음탕이요!",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043862.1.7",
          "form": "뭐 드시고 계세요?",
          "original_form": "뭐 드시고 계세요?",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043862.1.8",
          "form": "우와 저도요!!!!!",
          "original_form": "우와 저도요!!!!!",
          "speaker_id": "2",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043862.1.9",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "2",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043862.1.10",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043862.1.11",
          "form": "사드신 건가요?",
          "original_form": "사드신 건가요?",
          "speaker_id": "1",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043862.1.12",
          "form": "집에서 요리해 먹었어요",
          "original_form": "집에서 요리해 먹었어요",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043862.1.13",
          "form": "헙 저돈데",
          "original_form": "헙 저돈데",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043862.1.14",
          "form": "제가 한 건 아니지만..",
          "original_form": "제가 한 건 아니지만..",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043862.1.15",
          "form": "저는 사촌동생이 해줫어요",
          "original_form": "저는 사촌동생이 해줫어요",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043862.1.16",
          "form": "비 오는 날엔 역시 닭볶음탕이죠!",
          "original_form": "비 오는 날엔 역시 닭볶음탕이죠!",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043862.1.17",
          "form": "크흐 혹시",
          "original_form": "크흐 혹시",
