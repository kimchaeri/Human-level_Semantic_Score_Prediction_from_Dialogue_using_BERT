      "utterance": [
        {
          "id": "MDRW2100012657.1.1",
          "form": "응 운동하는데 재미있어 보이더라",
          "original_form": "응 운동하는데 재미있어 보이더라",
          "speaker_id": "2",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100012657.1.2",
          "form": "건강도 챙기고 재미도 챙기고",
          "original_form": "건강도 챙기고 재미도 챙기고",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100012657.1.3",
          "form": "무릎은 좀 조심해야할거 같은데",
          "original_form": "무릎은 좀 조심해야할거 같은데",
          "speaker_id": "1",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100012657.1.4",
          "form": "생각보다 재미지네",
          "original_form": "생각보다 재미지네",
          "speaker_id": "1",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100012657.1.5",
          "form": "ㅎㅎㅎ무릎 건강 챙기면서 해",
          "original_form": "ㅎㅎㅎ무릎 건강 챙기면서 해",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100012657.1.6",
          "form": "건강엔 숙면이 최고니 곧 자야겠어",
          "original_form": "건강엔 숙면이 최고니 곧 자야겠어",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100012657.1.7",
          "form": "눈이 감긴다~~",
          "original_form": "눈이 감긴다~~",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100012657.1.8",
          "form": "그려 꿀잠자~~",
          "original_form": "그려 꿀잠자~~",
          "speaker_id": "1",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100012657.1.9",
          "form": "언니도 건강을 위해 일찍 자~",
          "original_form": "언니도 건강을 위해 일찍 자~",
          "speaker_id": "2",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100012657.1.10",
          "form": "오늘 필라테스 가는 날이야?",
          "original_form": "오늘 필라테스 가는 날이야?",
          "speaker_id": "1",
          "time": "20210825 10:18"
        },
        {
          "id": "MDRW2100012657.1.11",
          "form": "응 오늘 필라테스",
          "original_form": "응 오늘 필라테스",
          "speaker_id": "2",
          "time": "20210825 10:50"
        },
        {
          "id": "MDRW2100012657.1.12",
          "form": "건강을 위해 필라테스하는 동생 대단하구만~~",
          "original_form": "건강을 위해 필라테스하는 동생 대단하구만~~",
          "speaker_id": "1",
          "time": "20210825 11:23"
        },
        {
          "id": "MDRW2100012657.1.13",
          "form": "후후 건강을 챙겨야죠",
          "original_form": "후후 건강을 챙겨야죠",
          "speaker_id": "2",
          "time": "20210825 11:25"
        },
        {
          "id": "MDRW2100012657.1.14",
          "form": "언닌 오늘도 사이클 운동?",
          "original_form": "언닌 오늘도 사이클 운동?",
          "speaker_id": "2",
          "time": "20210825 11:25"
        },
        {
          "id": "MDRW2100012657.1.15",
          "form": "이따 해보려고 ㅋㅋ 싸이클 유산소가 되겠지?",
          "original_form": "이따 해보려고 ㅋㅋ 싸이클 유산소가 되겠지?",
          "speaker_id": "1",
          "time": "20210825 11:27"
        },
        {
          "id": "MDRW2100012657.1.16",
          "form": "그럼 다리도 튼튼해지고 심장도 튼튼해지는 운동",
          "original_form": "그럼 다리도 튼튼해지고 심장도 튼튼해지는 운동",
          "speaker_id": "2",
          "time": "20210825 11:35"
        },
        {
          "id": "MDRW2100012657.1.17",
          "form": "오늘도 14키로 달렸당",
          "original_form": "오늘도 14키로 달렸당",
          "speaker_id": "1",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100012657.1.18",
          "form": "오오 건강 제대로 챙기는군!!",
          "original_form": "오오 건강 제대로 챙기는군!!",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100012657.1.19",
          "form": "나는 이제 퇴근하고 운동 가고 있어",
          "original_form": "나는 이제 퇴근하고 운동 가고 있어",
