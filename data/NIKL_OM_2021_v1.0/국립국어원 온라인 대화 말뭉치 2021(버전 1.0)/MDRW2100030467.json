      "utterance": [
        {
          "id": "MDRW2100030467.1.1",
          "form": "이렇게까지 했는데 모를 줄은몰랐다 칭구야..",
          "original_form": "이렇게까지 했는데 모를 줄은몰랐다 칭구야..",
          "speaker_id": "1",
          "time": "20210904 01:32"
        },
        {
          "id": "MDRW2100030467.1.2",
          "form": "출석번호 1번 칭구야",
          "original_form": "출석번호 1번 칭구야",
          "speaker_id": "1",
          "time": "20210904 01:32"
        },
        {
          "id": "MDRW2100030467.1.3",
          "form": "학교는 다시 안 가고 싶어?",
          "original_form": "학교는 다시 안 가고 싶어?",
          "speaker_id": "1",
          "time": "20210904 01:33"
        },
        {
          "id": "MDRW2100030467.1.4",
          "form": "학교를 왜 다시 갘",
          "original_form": "학교를 왜 다시 갘",
          "speaker_id": "2",
          "time": "20210904 01:35"
        },
        {
          "id": "MDRW2100030467.1.5",
          "form": "왜? 학교 안 가고 싶어?",
          "original_form": "왜? 학교 안 가고 싶어?",
          "speaker_id": "1",
          "time": "20210904 01:36"
        },
        {
          "id": "MDRW2100030467.1.6",
          "form": "대학교 다시 다니면 재미있을 것 같은데 수업도 듣고",
          "original_form": "대학교 다시 다니면 재미있을 것 같은데 수업도 듣고",
          "speaker_id": "1",
          "time": "20210904 01:36"
        },
        {
          "id": "MDRW2100030467.1.7",
          "form": "제대로 된 학교로 간다면 모르겠다만",
          "original_form": "제대로 된 학교로 간다면 모르겠다만",
          "speaker_id": "2",
          "time": "20210904 01:39"
        },
        {
          "id": "MDRW2100030467.1.8",
          "form": "모교가 이번에 부실대학 선정되었다는 얘길 들었더니 ㅋㅋㅋ",
          "original_form": "모교가 이번에 부실대학 선정되었다는 얘길 들었더니 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:39"
        },
        {
          "id": "MDRW2100030467.1.9",
          "form": "아 매번 들려오는 소리에 마음이 아프군요",
          "original_form": "아 매번 들려오는 소리에 마음이 아프군요",
          "speaker_id": "1",
          "time": "20210904 01:45"
        },
        {
          "id": "MDRW2100030467.1.10",
          "form": "비교과 과목에 승마가 생겼다는 소식을 아는지",
          "original_form": "비교과 과목에 승마가 생겼다는 소식을 아는지",
          "speaker_id": "1",
          "time": "20210904 01:45"
        },
        {
          "id": "MDRW2100030467.1.11",
          "form": "오..",
          "original_form": "오..",
          "speaker_id": "2",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100030467.1.12",
          "form": "졸업할때쯤 뭐 승마 뭐시기 생긴다더니",
          "original_form": "졸업할때쯤 뭐 승마 뭐시기 생긴다더니",
          "speaker_id": "2",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100030467.1.13",
          "form": "진짜 생겻군여",
          "original_form": "진짜 생겻군여",
          "speaker_id": "2",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100030467.1.14",
          "form": "ㅇㅇㅇ 비교과로 승마가 생겼데",
          "original_form": "ㅇㅇㅇ 비교과로 승마가 생겼데",
          "speaker_id": "1",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100030467.1.15",
          "form": "그학교도 대다내",
          "original_form": "그학교도 대다내",
          "speaker_id": "2",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100030467.1.16",
          "form": "오늘 서류 필요해서 들어갔다가 발견했는데",
          "original_form": "오늘 서류 필요해서 들어갔다가 발견했는데",
          "speaker_id": "1",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100030467.1.17",
          "form": "진작 해주지",
          "original_form": "진작 해주지",
          "speaker_id": "2",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100030467.1.18",
          "form": "내가 학교 다닐때 좀 만들지.. 꼭 졸업할 때 무렵에 만들더라",
          "original_form": "내가 학교 다닐때 좀 만들지.. 꼭 졸업할 때 무렵에 만들더라",
          "speaker_id": "1",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100030467.1.19",
          "form": "나도 승마 해보게",
          "original_form": "나도 승마 해보게",
          "speaker_id": "2",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100030467.1.20",
          "form": "한 4년만 더 일찍 햇으면",
          "original_form": "한 4년만 더 일찍 햇으면",
          "speaker_id": "2",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100030467.1.21",
          "form": "솔직히 동아리도 마찬가지야 동아리... 넉넉하게 있었으면",
          "original_form": "솔직히 동아리도 마찬가지야 동아리... 넉넉하게 있었으면",
          "speaker_id": "1",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100030467.1.22",
          "form": "아마 이것 저것 해봤을 것 같은데",
          "original_form": "아마 이것 저것 해봤을 것 같은데",
          "speaker_id": "1",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100030467.1.23",
          "form": "없기는 또 엄청 없어가지고",
          "original_form": "없기는 또 엄청 없어가지고",
          "speaker_id": "1",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100030467.1.24",
          "form": "할만한것도 없고",
          "original_form": "할만한것도 없고",
          "speaker_id": "2",
          "time": "20210904 01:52"
        },
        {
          "id": "MDRW2100030467.1.25",
          "form": "특색도 없고",
          "original_form": "특색도 없고",
          "speaker_id": "2",
          "time": "20210904 01:52"
        },
        {
          "id": "MDRW2100030467.1.26",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210904 01:53"
        },
        {
          "id": "MDRW2100030467.1.27",
          "form": "만들 생각을 안해봤네",
          "original_form": "만들 생각을 안해봤네",
          "speaker_id": "1",
          "time": "20210904 01:53"
        },
        {
          "id": "MDRW2100030467.1.28",
          "form": "만들어봤어도 괜찮았을 것 같은데",
          "original_form": "만들어봤어도 괜찮았을 것 같은데",
          "speaker_id": "1",
          "time": "20210904 01:53"
        }
      ]
