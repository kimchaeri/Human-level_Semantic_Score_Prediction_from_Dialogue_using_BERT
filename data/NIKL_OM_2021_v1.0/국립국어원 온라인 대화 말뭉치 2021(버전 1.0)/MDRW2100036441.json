      "utterance": [
        {
          "id": "MDRW2100036441.1.1",
          "form": "그러게 너는 백신접종했니?",
          "original_form": "그러게 너는 백신접종했니?",
          "speaker_id": "2",
          "time": "20210830 23:10"
        },
        {
          "id": "MDRW2100036441.1.2",
          "form": "어머 통했구나",
          "original_form": "어머 통했구나",
          "speaker_id": "1",
          "time": "20210830 23:10"
        },
        {
          "id": "MDRW2100036441.1.3",
          "form": "나는 1차만 맞았어",
          "original_form": "나는 1차만 맞았어",
          "speaker_id": "1",
          "time": "20210830 23:10"
        },
        {
          "id": "MDRW2100036441.1.4",
          "form": "너는 뭐 맞았니",
          "original_form": "너는 뭐 맞았니",
          "speaker_id": "1",
          "time": "20210830 23:10"
        },
        {
          "id": "MDRW2100036441.1.5",
          "form": "나는 화이자! 너는?",
          "original_form": "나는 화이자! 너는?",
          "speaker_id": "2",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100036441.1.6",
          "form": "나는 모더나!",
          "original_form": "나는 모더나!",
          "speaker_id": "1",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100036441.1.7",
          "form": "화이자 어ㄸㅐ?",
          "original_form": "화이자 어ㄸㅐ?",
          "speaker_id": "1",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100036441.1.8",
          "form": "팔 아팠지?",
          "original_form": "팔 아팠지?",
          "speaker_id": "1",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100036441.1.9",
          "form": "응 그건 모든 백신이 다 그런가봐",
          "original_form": "응 그건 모든 백신이 다 그런가봐",
          "speaker_id": "2",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100036441.1.10",
          "form": "너는 잔여를 잡은거야?",
          "original_form": "너는 잔여를 잡은거야?",
          "speaker_id": "2",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100036441.1.11",
          "form": "모더나는 팔이 아프더라",
          "original_form": "모더나는 팔이 아프더라",
          "speaker_id": "1",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100036441.1.12",
          "form": "아니!",
          "original_form": "아니!",
          "speaker_id": "1",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100036441.1.13",
          "form": "나는 그냥.. 맞았어",
          "original_form": "나는 그냥.. 맞았어",
          "speaker_id": "1",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100036441.1.14",
          "form": "때가 빨리 오더라",
          "original_form": "때가 빨리 오더라",
          "speaker_id": "1",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100036441.1.15",
          "form": "그렇게 주어 없이 말하니까",
          "original_form": "그렇게 주어 없이 말하니까",
          "speaker_id": "2",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036441.1.16",
          "form": "너는 어떻게 맞은거야?",
          "original_form": "너는 어떻게 맞은거야?",
          "speaker_id": "1",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036441.1.17",
          "form": "진짜로 어디 맞은거같구나",
          "original_form": "진짜로 어디 맞은거같구나",
          "speaker_id": "2",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036441.1.18",
          "form": "아 흐름을 잘 기억해줘",
          "original_form": "아 흐름을 잘 기억해줘",
          "speaker_id": "1",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036441.1.19",
          "form": "아무튼 곧 2차 접종이야",
          "original_form": "아무튼 곧 2차 접종이야",
          "speaker_id": "1",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036441.1.20",
          "form": "아플까?",
          "original_form": "아플까?",
          "speaker_id": "1",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036441.1.21",
          "form": "나는 기숙사에서 다같이 우선권을 줬어",
          "original_form": "나는 기숙사에서 다같이 우선권을 줬어",
          "speaker_id": "2",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036441.1.22",
          "form": "그러게 시간이 빠르다",
          "original_form": "그러게 시간이 빠르다",
          "speaker_id": "2",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036441.1.23",
          "form": "대단하네",
          "original_form": "대단하네",
          "speaker_id": "1",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036441.1.24",
          "form": "기숙사가",
          "original_form": "기숙사가",
          "speaker_id": "1",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036441.1.25",
          "form": "2차는 아프대 대비 단단히해!!",
          "original_form": "2차는 아프대 대비 단단히해!!",
          "speaker_id": "2",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100036441.1.26",
          "form": "있잖아",
          "original_form": "있잖아",
          "speaker_id": "1",
          "time": "20210830 23:13"
        }
      ]
