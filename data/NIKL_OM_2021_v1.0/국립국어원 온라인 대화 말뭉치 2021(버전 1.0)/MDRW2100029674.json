        },
        {
          "id": "MDRW2100029674.1.2",
          "form": "긍정긍정",
          "original_form": "긍정긍정",
          "speaker_id": "1",
          "time": "20210903 11:35"
        },
        {
          "id": "MDRW2100029674.1.3",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 11:35"
        },
        {
          "id": "MDRW2100029674.1.4",
          "form": "소의 고기요~",
          "original_form": "소의 고기요~",
          "speaker_id": "2",
          "time": "20210903 11:35"
        },
        {
          "id": "MDRW2100029674.1.5",
          "form": "단백질도 필요하니까~",
          "original_form": "단백질도 필요하니까~",
          "speaker_id": "2",
          "time": "20210903 11:35"
        },
        {
          "id": "MDRW2100029674.1.6",
          "form": "저는 점심에 물냉먹습니다",
          "original_form": "저는 점심에 물냉먹습니다",
          "speaker_id": "1",
          "time": "20210903 11:35"
        },
        {
          "id": "MDRW2100029674.1.7",
          "form": "코다리냉면ㅋㄱㅋ",
          "original_form": "코다리냉면ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210903 11:35"
        },
        {
          "id": "MDRW2100029674.1.8",
          "form": "맛있겠어요",
          "original_form": "맛있겠어요",
          "speaker_id": "2",
          "time": "20210903 11:36"
        },
        {
          "id": "MDRW2100029674.1.9",
          "form": "고기 좋죠",
          "original_form": "고기 좋죠",
          "speaker_id": "1",
          "time": "20210903 11:36"
        },
        {
          "id": "MDRW2100029674.1.10",
          "form": "제가 아직 코다리냉면에 매력은 못 느껴봤어요",
          "original_form": "제가 아직 코다리냉면에 매력은 못 느껴봤어요",
          "speaker_id": "2",
          "time": "20210903 11:36"
        },
        {
          "id": "MDRW2100029674.1.11",
          "form": "맛집이라 한번씩 꼭 가요",
          "original_form": "맛집이라 한번씩 꼭 가요",
          "speaker_id": "1",
          "time": "20210903 11:36"
        },
        {
          "id": "MDRW2100029674.1.12",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "1",
          "time": "20210903 11:37"
        },
        {
          "id": "MDRW2100029674.1.13",
          "form": "원조 비빔냉면 매니아인 관계로 ..",
          "original_form": "원조 비빔냉면 매니아인 관계로 ..",
          "speaker_id": "2",
          "time": "20210903 11:37"
        },
        {
          "id": "MDRW2100029674.1.14",
          "form": "그럼 평양냉면좋아하세요?",
          "original_form": "그럼 평양냉면좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 11:37"
        },
        {
          "id": "MDRW2100029674.1.15",
          "form": "아하하",
          "original_form": "아하하",
          "speaker_id": "1",
          "time": "20210903 11:37"
        },
        {
          "id": "MDRW2100029674.1.16",
          "form": "비냉도 맛나죠",
          "original_form": "비냉도 맛나죠",
          "speaker_id": "1",
          "time": "20210903 11:37"
        },
        {
          "id": "MDRW2100029674.1.17",
          "form": "그것도 아직..",
          "original_form": "그것도 아직..",
          "speaker_id": "2",
          "time": "20210903 11:37"
        },
        {
          "id": "MDRW2100029674.1.18",
          "form": "매운거 잘 드세요?",
          "original_form": "매운거 잘 드세요?",
          "speaker_id": "1",
          "time": "20210903 11:37"
        },
        {
          "id": "MDRW2100029674.1.19",
          "form": "초급정도..?",
          "original_form": "초급정도..?",
          "speaker_id": "2",
          "time": "20210903 11:38"
        },
        {
          "id": "MDRW2100029674.1.20",
          "form": "게다가 그것조차도많이 약해졌네요",
          "original_form": "게다가 그것조차도많이 약해졌네요",
          "speaker_id": "2",
          "time": "20210903 11:38"
        },
        {
          "id": "MDRW2100029674.1.21",
          "form": "매운걸 좋아하는데 잘 못 먹는다는 아이러니..",
          "original_form": "매운걸 좋아하는데 잘 못 먹는다는 아이러니..",
          "speaker_id": "2",
          "time": "20210903 11:39"
        },
        {
          "id": "MDRW2100029674.1.22",
          "form": "전 매운거 손도 못대거든요",
          "original_form": "전 매운거 손도 못대거든요",
          "speaker_id": "1",
          "time": "20210903 11:39"
        },
        {
          "id": "MDRW2100029674.1.23",
          "form": "엽떡같은거 먹으면 한개 먹고 쿨피스 500  마셔야돼요",
          "original_form": "엽떡같은거 먹으면 한개 먹고 쿨피스 500  마셔야돼요",
          "speaker_id": "1",
          "time": "20210903 11:39"
        },
        {
          "id": "MDRW2100029674.1.24",
          "form": "클나요.",
          "original_form": "클나요.",
          "speaker_id": "2",
          "time": "20210903 11:40"
        },
        {
          "id": "MDRW2100029674.1.25",
          "form": "쿨피스때문에 목까지 배불러요.ㅎㅎ",
          "original_form": "쿨피스때문에 목까지 배불러요.ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 11:41"
        },
        {
          "id": "MDRW2100029674.1.26",
          "form": "속이 아프더라구요",
          "original_form": "속이 아프더라구요",
          "speaker_id": "1",
          "time": "20210903 11:41"
        },
        {
          "id": "MDRW2100029674.1.27",
          "form": "위장 탈나요",
          "original_form": "위장 탈나요",
          "speaker_id": "2",
          "time": "20210903 11:41"
        },
        {
          "id": "MDRW2100029674.1.28",
          "form": "그쵸 조심해야해",
          "original_form": "그쵸 조심해야해",
          "speaker_id": "1",
          "time": "20210903 11:41"
        },
        {
          "id": "MDRW2100029674.1.29",
          "form": "체질에 맞게 식성이 생기는것같아요",
          "original_form": "체질에 맞게 식성이 생기는것같아요",
          "speaker_id": "2",
          "time": "20210903 11:42"
        },
        {
          "id": "MDRW2100029674.1.30",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210903 11:42"
        },
        {
          "id": "MDRW2100029674.1.31",
          "form": "전 매운맛은 못견디는데 단 맛은",
          "original_form": "전 매운맛은 못견디는데 단 맛은",
          "speaker_id": "1",
          "time": "20210903 11:42"
        },
        {
          "id": "MDRW2100029674.1.32",
          "form": "근데,애들은 젊어서 잘 먹나..그 매운걸..",
          "original_form": "근데,애들은 젊어서 잘 먹나..그 매운걸..",
          "speaker_id": "2",
          "time": "20210903 11:42"
        },
        {
          "id": "MDRW2100029674.1.33",
          "form": "엄청 좋아해요",
          "original_form": "엄청 좋아해요",
          "speaker_id": "1",
          "time": "20210903 11:42"
        },
        {
          "id": "MDRW2100029674.1.34",
          "form": "속도 편안",
          "original_form": "속도 편안",
          "speaker_id": "1",
          "time": "20210903 11:42"
        },
        {
          "id": "MDRW2100029674.1.35",
          "form": "그러게요 신기해요",
          "original_form": "그러게요 신기해요",
          "speaker_id": "1",
          "time": "20210903 11:43"
        },
        {
          "id": "MDRW2100029674.1.36",
          "form": "아예 캡사이신 들이붓잖아요",
          "original_form": "아예 캡사이신 들이붓잖아요",
          "speaker_id": "1",
          "time": "20210903 11:43"
        },
        {
          "id": "MDRW2100029674.1.37",
          "form": "속이 편한쪽으로 드세요.",
          "original_form": "속이 편한쪽으로 드세요.",
          "speaker_id": "2",
          "time": "20210903 11:43"
        },
        {
          "id": "MDRW2100029674.1.38",
          "form": "신기한 수입고츄들 다 들여오고",
          "original_form": "신기한 수입고츄들 다 들여오고",
          "speaker_id": "1",
          "time": "20210903 11:43"
        },
        {
          "id": "MDRW2100029674.1.39",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210903 11:43"
        },
        {
          "id": "MDRW2100029674.1.40",
          "form": "참 대단해요",
          "original_form": "참 대단해요",
          "speaker_id": "1",
          "time": "20210903 11:44"
        }
      ]
    }
