      "utterance": [
        {
          "id": "MDRW2100022424.1.1",
          "form": "저",
          "original_form": "저",
          "speaker_id": "1",
          "time": "20210829 13:49"
        },
        {
          "id": "MDRW2100022424.1.2",
          "form": "징이요ㅋㅋㅋ",
          "original_form": "징이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:49"
        },
        {
          "id": "MDRW2100022424.1.3",
          "form": "징 칠 때 그 울림이 웅웅하는게 참 좋더라구요ㅋㅋ",
          "original_form": "징 칠 때 그 울림이 웅웅하는게 참 좋더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100022424.1.4",
          "form": "정말요?",
          "original_form": "정말요?",
          "speaker_id": "1",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100022424.1.5",
          "form": "보통 북이나..장구..",
          "original_form": "보통 북이나..장구..",
          "speaker_id": "1",
          "time": "20210829 13:51"
        },
        {
          "id": "MDRW2100022424.1.6",
          "form": "좋아하시던데",
          "original_form": "좋아하시던데",
          "speaker_id": "1",
          "time": "20210829 13:51"
        },
        {
          "id": "MDRW2100022424.1.7",
          "form": "네ㅋㅋ저도 예전에 사물놀이 해본적이 있어서...",
          "original_form": "네ㅋㅋ저도 예전에 사물놀이 해본적이 있어서...",
          "speaker_id": "2",
          "time": "20210829 13:51"
        },
        {
          "id": "MDRW2100022424.1.8",
          "form": "징 한번 쳐봤는데 손을 타고 올라오는 그 울림이 좋았어요",
          "original_form": "징 한번 쳐봤는데 손을 타고 올라오는 그 울림이 좋았어요",
          "speaker_id": "2",
          "time": "20210829 13:51"
        },
        {
          "id": "MDRW2100022424.1.9",
          "form": "봉사는 해본적 있으신가요?",
          "original_form": "봉사는 해본적 있으신가요?",
          "speaker_id": "2",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100022424.1.10",
          "form": "어 최근에는 없는거같아요",
          "original_form": "어 최근에는 없는거같아요",
          "speaker_id": "1",
          "time": "20210829 14:13"
        },
        {
          "id": "MDRW2100022424.1.11",
          "form": "기회가 있으면 해보려구요",
          "original_form": "기회가 있으면 해보려구요",
          "speaker_id": "1",
          "time": "20210829 14:13"
        },
        {
          "id": "MDRW2100022424.1.12",
          "form": "name2은요",
          "original_form": "&name2&은요",
          "speaker_id": "1",
          "time": "20210829 14:13"
        },
        {
          "id": "MDRW2100022424.1.13",
          "form": "저도 최근에는 없고 3년전인가 공부방에서 초등학생애들한테 영어 가르치는 봉사해본 적 있어요ㅎ",
          "original_form": "저도 최근에는 없고 3년전인가 공부방에서 초등학생애들한테 영어 가르치는 봉사해본 적 있어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100022424.1.14",
          "form": "두 달정도 했었네요ㅎ",
          "original_form": "두 달정도 했었네요ㅎ",
          "speaker_id": "2",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100022424.1.15",
          "form": "대단하세요",
          "original_form": "대단하세요",
          "speaker_id": "1",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100022424.1.16",
          "form": "아동센터같은 곳이었나요",
          "original_form": "아동센터같은 곳이었나요",
          "speaker_id": "1",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100022424.1.17",
          "form": "천주교 산하에 있는 공부방으로 알고 있어요. 선생님들이 각자 과목하나씩 맡아서 가르치는...?",
          "original_form": "천주교 산하에 있는 공부방으로 알고 있어요. 선생님들이 각자 과목하나씩 맡아서 가르치는...?",
          "speaker_id": "2",
          "time": "20210829 14:17"
        },
        {
          "id": "MDRW2100022424.1.18",
          "form": "아~ 어떤과목 가르쳤나요",
          "original_form": "아~ 어떤과목 가르쳤나요",
          "speaker_id": "1",
          "time": "20210829 14:17"
        },
        {
          "id": "MDRW2100022424.1.19",
          "form": "영어요ㅎ",
          "original_form": "영어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 14:18"
        },
        {
          "id": "MDRW2100022424.1.20",
          "form": "대박 완전멋져",
          "original_form": "대박 완전멋져",
          "speaker_id": "1",
          "time": "20210829 14:18"
        },
        {
          "id": "MDRW2100022424.1.21",
          "form": "영어잘하시나봐요",
          "original_form": "영어잘하시나봐요",
          "speaker_id": "1",
          "time": "20210829 14:18"
        },
        {
          "id": "MDRW2100022424.1.22",
          "form": "지금은 많이 잊어먹었어요ㅋㅋㅋ저 때는 교육관련 자격증 따려고 하던 때라 열심히 하던 것도 있구요ㅜ",
          "original_form": "지금은 많이 잊어먹었어요ㅋㅋㅋ저 때는 교육관련 자격증 따려고 하던 때라 열심히 하던 것도 있구요ㅜ",
          "speaker_id": "2",
          "time": "20210829 14:23"
        },
        {
          "id": "MDRW2100022424.1.23",
          "form": "그래도 언어를 하신다는게 멋져요",
          "original_form": "그래도 언어를 하신다는게 멋져요",
          "speaker_id": "1",
          "time": "20210829 14:24"
        },
        {
          "id": "MDRW2100022424.1.24",
          "form": "아이들한테 많이 도움이 됐겠어요",
          "original_form": "아이들한테 많이 도움이 됐겠어요",
          "speaker_id": "1",
          "time": "20210829 14:24"
        }
      ]
