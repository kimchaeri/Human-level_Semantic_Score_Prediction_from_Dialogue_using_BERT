      "utterance": [
        {
          "id": "MDRW2100016367.1.1",
          "form": "오늘은 날씨가 맑네요! name2님 계신곳도 맑나요?",
          "original_form": "오늘은 날씨가 맑네요! &name2&님 계신곳도 맑나요?",
          "speaker_id": "1",
          "time": "20210826 12:17"
        },
        {
          "id": "MDRW2100016367.1.2",
          "form": "여기는 조금 흐려요!",
          "original_form": "여기는 조금 흐려요!",
          "speaker_id": "2",
          "time": "20210826 12:19"
        },
        {
          "id": "MDRW2100016367.1.3",
          "form": "드디어 맑은 날씨라니 기분좋으시겠어요",
          "original_form": "드디어 맑은 날씨라니 기분좋으시겠어요",
          "speaker_id": "2",
          "time": "20210826 12:19"
        },
        {
          "id": "MDRW2100016367.1.4",
          "form": "습하진 않으세요?ㅎㅎ",
          "original_form": "습하진 않으세요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 12:19"
        },
        {
          "id": "MDRW2100016367.1.5",
          "form": "조금 습해요ㅠㅠ",
          "original_form": "조금 습해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 12:20"
        },
        {
          "id": "MDRW2100016367.1.6",
          "form": "에어컨을 잠깐 돌리려구요",
          "original_form": "에어컨을 잠깐 돌리려구요",
          "speaker_id": "2",
          "time": "20210826 12:20"
        },
        {
          "id": "MDRW2100016367.1.7",
          "form": "거긴 습한가요?",
          "original_form": "거긴 습한가요?",
          "speaker_id": "2",
          "time": "20210826 12:20"
        },
        {
          "id": "MDRW2100016367.1.8",
          "form": "확실히 어제보단 덜 하네요~ 일기예보 챙겨보세요?",
          "original_form": "확실히 어제보단 덜 하네요~ 일기예보 챙겨보세요?",
          "speaker_id": "1",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016367.1.9",
          "form": "필요할때만 검색해봐요",
          "original_form": "필요할때만 검색해봐요",
          "speaker_id": "2",
          "time": "20210826 12:26"
        },
        {
          "id": "MDRW2100016367.1.10",
          "form": "어떤 계절 좋아하세요?",
          "original_form": "어떤 계절 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 12:33"
        },
        {
          "id": "MDRW2100016367.1.11",
          "form": "저는 가을이요",
          "original_form": "저는 가을이요",
          "speaker_id": "2",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100016367.1.12",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100016367.1.13",
          "form": "저도 가을 좋아해요ㅎㅎ 무슨 날씨 좋아하세요?",
          "original_form": "저도 가을 좋아해요ㅎㅎ 무슨 날씨 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 12:59"
        },
        {
          "id": "MDRW2100016367.1.14",
          "form": "맑고 하늘 깨끗한 날씨요",
          "original_form": "맑고 하늘 깨끗한 날씨요",
          "speaker_id": "2",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016367.1.15",
          "form": "걷기좋은 날씨 좋아해쇼",
          "original_form": "걷기좋은 날씨 좋아해쇼",
          "speaker_id": "2",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016367.1.16",
          "form": "요",
          "original_form": "요",
          "speaker_id": "2",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016367.1.17",
          "form": "저도요! 그쪽지역은 미세먼지 어떤가요?",
          "original_form": "저도요! 그쪽지역은 미세먼지 어떤가요?",
          "speaker_id": "1",
          "time": "20210826 13:24"
        },
        {
          "id": "MDRW2100016367.1.18",
          "form": "최근에는 미세먼지가 괜찮았어요",
          "original_form": "최근에는 미세먼지가 괜찮았어요",
          "speaker_id": "2",
          "time": "20210826 13:45"
        },
        {
          "id": "MDRW2100016367.1.19",
          "form": "더워도 미세먼지덜하니까 너무좋았어요",
          "original_form": "더워도 미세먼지덜하니까 너무좋았어요",
          "speaker_id": "2",
          "time": "20210826 13:45"
        },
        {
          "id": "MDRW2100016367.1.20",
          "form": "공기청정기 보유하고 계세요?",
          "original_form": "공기청정기 보유하고 계세요?",
          "speaker_id": "1",
          "time": "20210826 14:29"
        },
        {
          "id": "MDRW2100016367.1.21",
          "form": "아니요 사고싶은 물건중 하나예요",
          "original_form": "아니요 사고싶은 물건중 하나예요",
          "speaker_id": "2",
          "time": "20210826 14:47"
        },
        {
          "id": "MDRW2100016367.1.22",
          "form": "있으세요?",
          "original_form": "있으세요?",
          "speaker_id": "2",
          "time": "20210826 14:47"
        },
        {
          "id": "MDRW2100016367.1.23",
          "form": "여긴 날씨가 흐리네요!",
          "original_form": "여긴 날씨가 흐리네요!",
          "speaker_id": "1",
          "time": "20210827 13:07"
        }
      ]
