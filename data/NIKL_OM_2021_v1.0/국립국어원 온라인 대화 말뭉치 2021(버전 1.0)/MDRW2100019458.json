      "utterance": [
        {
          "id": "MDRW2100019458.1.1",
          "form": "혹시 최근에 아파본 적 있어요?",
          "original_form": "혹시 최근에 아파본 적 있어요?",
          "speaker_id": "1",
          "time": "20210827 19:31"
        },
        {
          "id": "MDRW2100019458.1.2",
          "form": "장염에 걸렸었어요",
          "original_form": "장염에 걸렸었어요",
          "speaker_id": "2",
          "time": "20210827 19:38"
        },
        {
          "id": "MDRW2100019458.1.3",
          "form": "허걱 ㅜㅜ 장염 언제 걸리셨어요?",
          "original_form": "허걱 ㅜㅜ 장염 언제 걸리셨어요?",
          "speaker_id": "1",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100019458.1.4",
          "form": "한2달전에 간장게장 먹고요 ㅠㅠ",
          "original_form": "한2달전에 간장게장 먹고요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100019458.1.5",
          "form": "간장게장이 상한거에요? ㅜㅜ",
          "original_form": "간장게장이 상한거에요? ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 19:40"
        },
        {
          "id": "MDRW2100019458.1.6",
          "form": "음 그랬던 것같아요 ㅠㅠ",
          "original_form": "음 그랬던 것같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 19:42"
        },
        {
          "id": "MDRW2100019458.1.7",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210827 19:42"
        },
        {
          "id": "MDRW2100019458.1.8",
          "form": "제가 키우진 않구 부모님이 키우세요~",
          "original_form": "제가 키우진 않구 부모님이 키우세요~",
          "speaker_id": "1",
          "time": "20210827 19:43"
        },
        {
          "id": "MDRW2100019458.1.9",
          "form": "강아지 키우는데 사료를 가려먹더라구요..",
          "original_form": "강아지 키우는데 사료를 가려먹더라구요..",
          "speaker_id": "1",
          "time": "20210827 19:43"
        },
        {
          "id": "MDRW2100019458.1.10",
          "form": "ㅋㅋㅋㅋㅋ 뭔가 듣기만해도 귀엽네요",
          "original_form": "ㅋㅋㅋㅋㅋ 뭔가 듣기만해도 귀엽네요",
          "speaker_id": "2",
          "time": "20210827 19:44"
        },
        {
          "id": "MDRW2100019458.1.11",
          "form": "그래서 사료값이 만만치 않게 들더라구요 ㅜㅜ",
          "original_form": "그래서 사료값이 만만치 않게 들더라구요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100019458.1.12",
          "form": "개도 입맛이 있나봐요 고오오급 입맛..",
          "original_form": "개도 입맛이 있나봐요 고오오급 입맛..",
          "speaker_id": "1",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100019458.1.13",
          "form": "아고 ㅠㅠ 그러게요  어떤강아지에요?",
          "original_form": "아고 ㅠㅠ 그러게요  어떤강아지에요?",
          "speaker_id": "2",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100019458.1.14",
          "form": "말티즈랑 비글 섞인 잡종이랄까..",
          "original_form": "말티즈랑 비글 섞인 잡종이랄까..",
          "speaker_id": "1",
          "time": "20210827 19:47"
        },
        {
          "id": "MDRW2100019458.1.15",
          "form": "ㅋㅋㅋㅋ성격은 어느쪽에 가깝나요?",
          "original_form": "ㅋㅋㅋㅋ성격은 어느쪽에 가깝나요?",
          "speaker_id": "2",
          "time": "20210827 19:47"
        },
        {
          "id": "MDRW2100019458.1.16",
          "form": "저한텐 비글이구요.. 보호자한텐 말티즈네요..",
          "original_form": "저한텐 비글이구요.. 보호자한텐 말티즈네요..",
          "speaker_id": "1",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019458.1.17",
          "form": "울고 싶다..ㅜㅜ",
          "original_form": "울고 싶다..ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019458.1.18",
          "form": "앜 ㅋㅋㅋㅋㅋ 너무 웃겨요ㅋㅋㅋㅋㅋㅋㅋㅋㄲ",
          "original_form": "앜 ㅋㅋㅋㅋㅋ 너무 웃겨요ㅋㅋㅋㅋㅋㅋㅋㅋㄲ",
          "speaker_id": "2",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019458.1.19",
          "form": "저는 고양이를 키워요",
          "original_form": "저는 고양이를 키워요",
          "speaker_id": "2",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019458.1.20",
          "form": "오 진짜요 ?",
          "original_form": "오 진짜요 ?",
          "speaker_id": "1",
          "time": "20210827 19:50"
        },
        {
          "id": "MDRW2100019458.1.21",
          "form": "고양이 종이 뭐에요?",
          "original_form": "고양이 종이 뭐에요?",
          "speaker_id": "1",
          "time": "20210827 19:50"
        },
        {
          "id": "MDRW2100019458.1.22",
          "form": "샴이에요",
          "original_form": "샴이에요",
          "speaker_id": "2",
          "time": "20210827 19:50"
        },
        {
          "id": "MDRW2100019458.1.23",
          "form": "샴 고양이 되게 매력적이고 그렇더라구요 ㅎㅎ",
          "original_form": "샴 고양이 되게 매력적이고 그렇더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100019458.1.24",
          "form": "색깔은 무슨 색이에요?",
          "original_form": "색깔은 무슨 색이에요?",
          "speaker_id": "1",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100019458.1.25",
          "form": "애교가 많아서 귀여워요 ㅎㅎㅎ전체적으로 회색에 얼굴은 까매요",
          "original_form": "애교가 많아서 귀여워요 ㅎㅎㅎ전체적으로 회색에 얼굴은 까매요",
          "speaker_id": "2",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100019458.1.26",
          "form": "제가 생각 했던 딱 그 이미지에요!",
          "original_form": "제가 생각 했던 딱 그 이미지에요!",
          "speaker_id": "1",
          "time": "20210827 19:55"
        },
        {
          "id": "MDRW2100019458.1.27",
          "form": "개냥이에요?",
          "original_form": "개냥이에요?",
          "speaker_id": "1",
          "time": "20210827 19:55"
        },
        {
          "id": "MDRW2100019458.1.28",
          "form": "어려서 그런지 개냥이에요 ㅎㅎㅎ",
          "original_form": "어려서 그런지 개냥이에요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:56"
        }
      ]
    }
