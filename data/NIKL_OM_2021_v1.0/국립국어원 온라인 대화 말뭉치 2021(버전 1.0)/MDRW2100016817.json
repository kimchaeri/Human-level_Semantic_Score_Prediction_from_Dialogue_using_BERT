      "utterance": [
        {
          "id": "MDRW2100016817.1.1",
          "form": "너 종교있어?",
          "original_form": "너 종교있어?",
          "speaker_id": "2",
          "time": "20210826 15:34"
        },
        {
          "id": "MDRW2100016817.1.2",
          "form": "나는 어렸을때 성당하고 교회다니다가 지금은 안 다녀",
          "original_form": "나는 어렸을때 성당하고 교회다니다가 지금은 안 다녀",
          "speaker_id": "1",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100016817.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100016817.1.4",
          "form": "난 초딩때 교회 잠깐 다녀봤어",
          "original_form": "난 초딩때 교회 잠깐 다녀봤어",
          "speaker_id": "2",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100016817.1.5",
          "form": "성당은 언제다녔어?",
          "original_form": "성당은 언제다녔어?",
          "speaker_id": "2",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100016817.1.6",
          "form": "성당 초등학교 저학년까지?",
          "original_form": "성당 초등학교 저학년까지?",
          "speaker_id": "1",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100016817.1.7",
          "form": "어릴때 세례도 받아서 세례명도 있어ㅋㅋ",
          "original_form": "어릴때 세례도 받아서 세례명도 있어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100016817.1.8",
          "form": "우와 가족끼리 다닌거야?\r\n세례명 궁금해",
          "original_form": "우와 가족끼리 다닌거야?\r\n세례명 궁금해",
          "speaker_id": "2",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100016817.1.9",
          "form": "엉 가족 다같이?? 우리 삼남매 대천사팸이야..^^ name1 name3 name4",
          "original_form": "엉 가족 다같이?? 우리 삼남매 대천사팸이야..^^ &name1& &name3& &name4&",
          "speaker_id": "1",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100016817.1.10",
          "form": "와 진짜 신기한데  너빼고 다른 사람들은 아직도 다녀?",
          "original_form": "와 진짜 신기한데  너빼고 다른 사람들은 아직도 다녀?",
          "speaker_id": "2",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100016817.1.11",
          "form": "놀랍게도",
          "original_form": "놀랍게도",
          "speaker_id": "1",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100016817.1.12",
          "form": "아무도 안다녀",
          "original_form": "아무도 안다녀",
          "speaker_id": "1",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100016817.1.13",
          "form": "전부 냉담자가 됐어..ㅋㅋㅋㅋㅋㅋ",
          "original_form": "전부 냉담자가 됐어..ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100016817.1.14",
          "form": "아빠는 약간 불교쪽으로 틀기도 했고",
          "original_form": "아빠는 약간 불교쪽으로 틀기도 했고",
          "speaker_id": "1",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100016817.1.15",
          "form": "너희 가족은 아무도 안다녀?",
          "original_form": "너희 가족은 아무도 안다녀?",
          "speaker_id": "1",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100016817.1.16",
          "form": "헐 어떻게 전부 냉담자가?!",
          "original_form": "헐 어떻게 전부 냉담자가?!",
          "speaker_id": "2",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100016817.1.17",
          "form": "응 우린 완전 무교야 ㅋㅋ",
          "original_form": "응 우린 완전 무교야 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100016817.1.18",
          "form": "엄마가 한때 교회다녔다고 하는데.. 관심없음",
          "original_form": "엄마가 한때 교회다녔다고 하는데.. 관심없음",
          "speaker_id": "2",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100016817.1.19",
          "form": "성당 다니다가 이모네가 교회다닌대서 교회 가봤다가 교회에서 정 떼고 다들 무교쪽으로 갔던더 같아ㅋㅋ",
          "original_form": "성당 다니다가 이모네가 교회다닌대서 교회 가봤다가 교회에서 정 떼고 다들 무교쪽으로 갔던더 같아ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100016817.1.20",
          "form": "ㅋㅋㅋ우리나라 무교 비율 높다더니 우리 보고도 알수 있겠더",
          "original_form": "ㅋㅋㅋ우리나라 무교 비율 높다더니 우리 보고도 알수 있겠더",
          "speaker_id": "1",
          "time": "20210826 15:43"
        },
        {
          "id": "MDRW2100016817.1.21",
          "form": "아하 그런 일이 있었구나..",
          "original_form": "아하 그런 일이 있었구나..",
