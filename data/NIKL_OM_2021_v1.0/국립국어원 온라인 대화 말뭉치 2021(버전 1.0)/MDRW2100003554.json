      "utterance": [
        {
          "id": "MDRW2100003554.1.1",
          "form": "동물 키우시나요????",
          "original_form": "동물 키우시나요????",
          "speaker_id": "2",
          "time": "20210821 13:50"
        },
        {
          "id": "MDRW2100003554.1.2",
          "form": "동물 키웠었어요..!",
          "original_form": "동물 키웠었어요..!",
          "speaker_id": "1",
          "time": "20210821 13:50"
        },
        {
          "id": "MDRW2100003554.1.3",
          "form": "털 없는 친구들만요!",
          "original_form": "털 없는 친구들만요!",
          "speaker_id": "1",
          "time": "20210821 13:51"
        },
        {
          "id": "MDRW2100003554.1.4",
          "form": "오 어떤 동물이요??",
          "original_form": "오 어떤 동물이요??",
          "speaker_id": "2",
          "time": "20210821 13:51"
        },
        {
          "id": "MDRW2100003554.1.5",
          "form": "장수풍뎅이",
          "original_form": "장수풍뎅이",
          "speaker_id": "1",
          "time": "20210821 13:51"
        },
        {
          "id": "MDRW2100003554.1.6",
          "form": "금붕어",
          "original_form": "금붕어",
          "speaker_id": "1",
          "time": "20210821 13:51"
        },
        {
          "id": "MDRW2100003554.1.7",
          "form": "구피",
          "original_form": "구피",
          "speaker_id": "1",
          "time": "20210821 13:51"
        },
        {
          "id": "MDRW2100003554.1.8",
          "form": "자라",
          "original_form": "자라",
          "speaker_id": "1",
          "time": "20210821 13:51"
        },
        {
          "id": "MDRW2100003554.1.9",
          "form": "요!",
          "original_form": "요!",
          "speaker_id": "1",
          "time": "20210821 13:51"
        },
        {
          "id": "MDRW2100003554.1.10",
          "form": "우와 되게 많이 키우셨네요 ㅎ",
          "original_form": "우와 되게 많이 키우셨네요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 13:52"
        },
        {
          "id": "MDRW2100003554.1.11",
          "form": "장수풍뎅이 저희도 키웠는데",
          "original_form": "장수풍뎅이 저희도 키웠는데",
          "speaker_id": "2",
          "time": "20210821 13:53"
        },
        {
          "id": "MDRW2100003554.1.12",
          "form": "먹이가 되게 젤리랑 헷갈려요",
          "original_form": "먹이가 되게 젤리랑 헷갈려요",
          "speaker_id": "2",
          "time": "20210821 13:53"
        },
        {
          "id": "MDRW2100003554.1.13",
          "form": "아 맞아요! 젤리뽀 같이 생겼잖아요",
          "original_form": "아 맞아요! 젤리뽀 같이 생겼잖아요",
          "speaker_id": "1",
          "time": "20210821 13:55"
        },
        {
          "id": "MDRW2100003554.1.14",
          "form": "맞아요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 13:55"
        },
        {
          "id": "MDRW2100003554.1.15",
          "form": "근데 냄새 자체가 너무나..그 달작지근 이상한 냄새라 착각 하고 먹을 일은 없더라구요",
          "original_form": "근데 냄새 자체가 너무나..그 달작지근 이상한 냄새라 착각 하고 먹을 일은 없더라구요",
          "speaker_id": "1",
          "time": "20210821 13:55"
        },
        {
          "id": "MDRW2100003554.1.16",
          "form": "착각하고 먹기에 딱이에요",
          "original_form": "착각하고 먹기에 딱이에요",
          "speaker_id": "2",
          "time": "20210821 13:55"
        },
        {
          "id": "MDRW2100003554.1.17",
          "form": "아 정말요???",
          "original_form": "아 정말요???",
          "speaker_id": "2",
          "time": "20210821 13:56"
        },
        {
          "id": "MDRW2100003554.1.18",
          "form": "냄새는 안맡아봤는데 다행이네요",
          "original_form": "냄새는 안맡아봤는데 다행이네요",
          "speaker_id": "2",
          "time": "20210821 13:56"
        },
        {
          "id": "MDRW2100003554.1.19",
          "form": "ㅋㅋㅋㅋㅋ적어도 까면 알게 되어요. 입에 들어가는건 방지 됩니다.",
          "original_form": "ㅋㅋㅋㅋㅋ적어도 까면 알게 되어요. 입에 들어가는건 방지 됩니다.",
          "speaker_id": "1",
          "time": "20210821 13:56"
        },
        {
          "id": "MDRW2100003554.1.20",
          "form": "다행이에요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "다행이에요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 13:56"
        },
        {
          "id": "MDRW2100003554.1.21",
          "form": "긍데 어쩌다가 저렇게 많이 키웠어요?",
          "original_form": "긍데 어쩌다가 저렇게 많이 키웠어요?",
          "speaker_id": "2",
          "time": "20210821 13:56"
        },
        {
          "id": "MDRW2100003554.1.22",
          "form": "동생이 저런 친구들을 좋아했거든요!곤충이나 물고기...같은 친구들...",
          "original_form": "동생이 저런 친구들을 좋아했거든요!곤충이나 물고기...같은 친구들...",
          "speaker_id": "1",
          "time": "20210821 13:58"
        },
        {
          "id": "MDRW2100003554.1.23",
          "form": "근데 하나같이 다 말로가 좋지 않았어요.",
          "original_form": "근데 하나같이 다 말로가 좋지 않았어요.",
          "speaker_id": "1",
          "time": "20210821 13:58"
        },
        {
          "id": "MDRW2100003554.1.24",
          "form": "아하ㅋㅋ ㅋㅋㅋㅋㅋ",
          "original_form": "아하ㅋㅋ ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 13:58"
        },
        {
          "id": "MDRW2100003554.1.25",
          "form": "헉 아쉽네여 ㅠㅠㅠ",
          "original_form": "헉 아쉽네여 ㅠㅠㅠ",
          "speaker_id": "2",
