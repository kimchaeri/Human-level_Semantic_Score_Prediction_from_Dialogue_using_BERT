      "utterance": [
        {
          "id": "MDRW2100008832.1.1",
          "form": "나 요즘 탄수화물 줄이는 중",
          "original_form": "나 요즘 탄수화물 줄이는 중",
          "speaker_id": "1",
          "time": "20210822 19:01"
        },
        {
          "id": "MDRW2100008832.1.2",
          "form": "난 탄수화물 없으면 안돼..",
          "original_form": "난 탄수화물 없으면 안돼..",
          "speaker_id": "2",
          "time": "20210822 19:01"
        },
        {
          "id": "MDRW2100008832.1.3",
          "form": "파스타도 통밀면, 두부면으로 먹고",
          "original_form": "파스타도 통밀면, 두부면으로 먹고",
          "speaker_id": "1",
          "time": "20210822 19:01"
        },
        {
          "id": "MDRW2100008832.1.4",
          "form": "나는 머리 잘라야하는데 귀차늠",
          "original_form": "나는 머리 잘라야하는데 귀차늠",
          "speaker_id": "2",
          "time": "20210822 19:01"
        },
        {
          "id": "MDRW2100008832.1.5",
          "form": "라면도 반만 먹고 채소많이 넣음",
          "original_form": "라면도 반만 먹고 채소많이 넣음",
          "speaker_id": "1",
          "time": "20210822 19:01"
        },
        {
          "id": "MDRW2100008832.1.6",
          "form": "오 나는 그런거 없이 막 먹어ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "오 나는 그런거 없이 막 먹어ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:02"
        },
        {
          "id": "MDRW2100008832.1.7",
          "form": "나는 이틀전에 머리잘랐다",
          "original_form": "나는 이틀전에 머리잘랐다",
          "speaker_id": "1",
          "time": "20210822 19:02"
        },
        {
          "id": "MDRW2100008832.1.8",
          "form": "운동해야하는데",
          "original_form": "운동해야하는데",
          "speaker_id": "2",
          "time": "20210822 19:02"
        },
        {
          "id": "MDRW2100008832.1.9",
          "form": "당뇨 조심해야해",
          "original_form": "당뇨 조심해야해",
          "speaker_id": "1",
          "time": "20210822 19:02"
        },
        {
          "id": "MDRW2100008832.1.10",
          "form": "퇴근하면 기운없음..",
          "original_form": "퇴근하면 기운없음..",
          "speaker_id": "2",
          "time": "20210822 19:02"
        },
        {
          "id": "MDRW2100008832.1.11",
          "form": "탄수화물 단거 먹는거 큰일남",
          "original_form": "탄수화물 단거 먹는거 큰일남",
          "speaker_id": "1",
          "time": "20210822 19:02"
        },
        {
          "id": "MDRW2100008832.1.12",
          "form": "맞아 당뇨 조심해야돼",
          "original_form": "맞아 당뇨 조심해야돼",
          "speaker_id": "2",
          "time": "20210822 19:02"
        },
        {
          "id": "MDRW2100008832.1.13",
          "form": "나는 유전적인것도 있어서",
          "original_form": "나는 유전적인것도 있어서",
          "speaker_id": "2",
          "time": "20210822 19:02"
        },
        {
          "id": "MDRW2100008832.1.14",
          "form": "특히 더",
          "original_form": "특히 더",
          "speaker_id": "2",
          "time": "20210822 19:02"
        },
        {
          "id": "MDRW2100008832.1.15",
          "form": "친가쪽이 당뇨있어서",
          "original_form": "친가쪽이 당뇨있어서",
          "speaker_id": "2",
          "time": "20210822 19:02"
        },
        {
          "id": "MDRW2100008832.1.16",
          "form": "나는 이번에 피검사한거 괜찮다곤 했는데 관리하려고",
          "original_form": "나는 이번에 피검사한거 괜찮다곤 했는데 관리하려고",
          "speaker_id": "1",
          "time": "20210822 19:02"
        },
        {
          "id": "MDRW2100008832.1.17",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210822 19:02"
        },
        {
          "id": "MDRW2100008832.1.18",
          "form": "아빠도 당뇨였거....그래서 더 조심해야함",
          "original_form": "아빠도 당뇨였거....그래서 더 조심해야함",
          "speaker_id": "2",
          "time": "20210822 19:02"
        },
        {
          "id": "MDRW2100008832.1.19",
          "form": "그럼 너는 특히 조심",
          "original_form": "그럼 너는 특히 조심",
          "speaker_id": "1",
          "time": "20210822 19:02"
        },
        {
          "id": "MDRW2100008832.1.20",
          "form": "완전 조심",
          "original_form": "완전 조심",
          "speaker_id": "1",
          "time": "20210822 19:03"
        },
        {
          "id": "MDRW2100008832.1.21",
          "form": "나두 공복혈당 같은거 100미만이어야하니까",
          "original_form": "나두 공복혈당 같은거 100미만이어야하니까",
          "speaker_id": "2",
          "time": "20210822 19:03"
        },
        {
          "id": "MDRW2100008832.1.22",
          "form": "당뇨뮤서움",
          "original_form": "당뇨뮤서움",
          "speaker_id": "2",
          "time": "20210822 19:03"
        },
        {
          "id": "MDRW2100008832.1.23",
          "form": "안과일해서 그런데 눈에 치명적이야",
          "original_form": "안과일해서 그런데 눈에 치명적이야",
          "speaker_id": "2",
          "time": "20210822 19:03"
        },
        {
          "id": "MDRW2100008832.1.24",
          "form": "나 예전에 공복혈당 101떴어서 주의하랬었음",
          "original_form": "나 예전에 공복혈당 101떴어서 주의하랬었음",
          "speaker_id": "1",
          "time": "20210822 19:03"
        },
        {
          "id": "MDRW2100008832.1.25",
          "form": "1년전에",
          "original_form": "1년전에",
          "speaker_id": "1",
          "time": "20210822 19:03"
        },
        {
          "id": "MDRW2100008832.1.26",
          "form": "너도 식사전에 오이같은 채소 먹고 식사해",
          "original_form": "너도 식사전에 오이같은 채소 먹고 식사해",
          "speaker_id": "1",
          "time": "20210822 19:03"
        },
        {
          "id": "MDRW2100008832.1.27",
          "form": "그러면 혈당스파크 안나온대",
          "original_form": "그러면 혈당스파크 안나온대",
          "speaker_id": "1",
          "time": "20210822 19:04"
        },
        {
          "id": "MDRW2100008832.1.28",
          "form": "사촌언니 눈 이상생겨서 걱정",
          "original_form": "사촌언니 눈 이상생겨서 걱정",
          "speaker_id": "1",
          "time": "20210822 19:04"
        },
        {
          "id": "MDRW2100008832.1.29",
          "form": "맞아 그런거 조심해야돼",
          "original_form": "맞아 그런거 조심해야돼",
