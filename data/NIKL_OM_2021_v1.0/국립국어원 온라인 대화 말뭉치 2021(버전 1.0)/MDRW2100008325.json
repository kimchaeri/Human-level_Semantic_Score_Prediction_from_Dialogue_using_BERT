      "utterance": [
        {
          "id": "MDRW2100008325.1.1",
          "form": "저 3명의 배우가 안하는걸 어떻게 맞추나?",
          "original_form": "저 3명의 배우가 안하는걸 어떻게 맞추나?",
          "speaker_id": "2",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008325.1.2",
          "form": "옥탑방 문제아들",
          "original_form": "옥탑방 문제아들",
          "speaker_id": "2",
          "time": "20210822 15:08"
        },
        {
          "id": "MDRW2100008325.1.3",
          "form": "그러니 아무말 대잔치가 되는거지ㅋㅋ",
          "original_form": "그러니 아무말 대잔치가 되는거지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008325.1.4",
          "form": "가만보면 후니가 참 사차원이더라",
          "original_form": "가만보면 후니가 참 사차원이더라",
          "speaker_id": "2",
          "time": "20210822 15:09"
        },
        {
          "id": "MDRW2100008325.1.5",
          "form": "쟤는 아형에서도 사차원이었어",
          "original_form": "쟤는 아형에서도 사차원이었어",
          "speaker_id": "1",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008325.1.6",
          "form": "ㅋㅋ연락을 하지 않는단다 진짜 아무말 대잔치",
          "original_form": "ㅋㅋ연락을 하지 않는단다 진짜 아무말 대잔치",
          "speaker_id": "2",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008325.1.7",
          "form": "답을 말할때보면 사차원",
          "original_form": "답을 말할때보면 사차원",
          "speaker_id": "1",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008325.1.8",
          "form": "ㅋㅋ뒷담화 할까봐 먼저 자리를 뜨지 않는단다 ㅋㅋ",
          "original_form": "ㅋㅋ뒷담화 할까봐 먼저 자리를 뜨지 않는단다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:10"
        },
        {
          "id": "MDRW2100008325.1.9",
          "form": "근데 조승우도 자기 프로 안본다던데",
          "original_form": "근데 조승우도 자기 프로 안본다던데",
          "speaker_id": "1",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008325.1.10",
          "form": "헐 옥탑방 문제아들을 안본다",
          "original_form": "헐 옥탑방 문제아들을 안본다",
          "speaker_id": "2",
          "time": "20210822 15:11"
        },
        {
          "id": "MDRW2100008325.1.11",
          "form": "그럼 아는형님도 안보겠다",
          "original_form": "그럼 아는형님도 안보겠다",
          "speaker_id": "2",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008325.1.12",
          "form": "자기가 나와 민망해서 안보는 사람 많드라",
          "original_form": "자기가 나와 민망해서 안보는 사람 많드라",
          "speaker_id": "1",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008325.1.13",
          "form": "뭐 다 사람 나름이니까",
          "original_form": "뭐 다 사람 나름이니까",
          "speaker_id": "2",
          "time": "20210822 15:12"
        },
        {
          "id": "MDRW2100008325.1.14",
          "form": "정답이네 자신의 작품을 안보는게 ㅋㅋ",
          "original_form": "정답이네 자신의 작품을 안보는게 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008325.1.15",
          "form": "아까 정형돈은 저장까지 한다는데ㅋ",
          "original_form": "아까 정형돈은 저장까지 한다는데ㅋ",
          "speaker_id": "1",
          "time": "20210822 15:13"
        },
        {
          "id": "MDRW2100008325.1.16",
          "form": "그러게 자기애가 넘친다",
          "original_form": "그러게 자기애가 넘친다",
          "speaker_id": "2",
          "time": "20210822 15:13"
