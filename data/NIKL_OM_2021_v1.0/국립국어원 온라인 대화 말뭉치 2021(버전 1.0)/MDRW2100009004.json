      "utterance": [
        {
          "id": "MDRW2100009004.1.1",
          "form": "국밥 좋아하신다니 추천드리고 싶어요",
          "original_form": "국밥 좋아하신다니 추천드리고 싶어요",
          "speaker_id": "2",
          "time": "20210822 20:24"
        },
        {
          "id": "MDRW2100009004.1.2",
          "form": "돼지부속 파는 곳은 꽤 많더라구요",
          "original_form": "돼지부속 파는 곳은 꽤 많더라구요",
          "speaker_id": "2",
          "time": "20210822 20:24"
        },
        {
          "id": "MDRW2100009004.1.3",
          "form": "네 지금 신세계에요",
          "original_form": "네 지금 신세계에요",
          "speaker_id": "1",
          "time": "20210822 20:24"
        },
        {
          "id": "MDRW2100009004.1.4",
          "form": "돈골육수 돼지국밥 요런걸로 검색하시면",
          "original_form": "돈골육수 돼지국밥 요런걸로 검색하시면",
          "speaker_id": "2",
          "time": "20210822 20:25"
        },
        {
          "id": "MDRW2100009004.1.5",
          "form": "저 부산 돼지국밥집 거의 다 다녀봤거든요 맛집 이름은 다 까먹었지만요ㅋㅋ",
          "original_form": "저 부산 돼지국밥집 거의 다 다녀봤거든요 맛집 이름은 다 까먹었지만요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:25"
        },
        {
          "id": "MDRW2100009004.1.6",
          "form": "업체가 여럿이네요",
          "original_form": "업체가 여럿이네요",
          "speaker_id": "2",
          "time": "20210822 20:25"
        },
        {
          "id": "MDRW2100009004.1.7",
          "form": "ㅋㅋㅋ 우와 그럼 맛있게 드실거에요!",
          "original_form": "ㅋㅋㅋ 우와 그럼 맛있게 드실거에요!",
          "speaker_id": "2",
          "time": "20210822 20:25"
        },
        {
          "id": "MDRW2100009004.1.8",
          "form": "전 네이버스토어에서 검색해서 ㅎㅍㄷ라는 곳에서 (왠지 초성) 구입했어요",
          "original_form": "전 네이버스토어에서 검색해서 ㅎㅍㄷ라는 곳에서 (왠지 초성) 구입했어요",
          "speaker_id": "2",
          "time": "20210822 20:25"
        },
        {
          "id": "MDRW2100009004.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210822 20:25"
        },
        {
          "id": "MDRW2100009004.1.10",
          "form": "초성ㅋㅋㅋ",
          "original_form": "초성ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:26"
        },
        {
          "id": "MDRW2100009004.1.11",
          "form": "그런데 비슷한 제품을 파는 곳이 여럿 있네요 ㅋㅋ 한국인의 국밥사랑",
          "original_form": "그런데 비슷한 제품을 파는 곳이 여럿 있네요 ㅋㅋ 한국인의 국밥사랑",
          "speaker_id": "2",
          "time": "20210822 20:26"
        },
        {
          "id": "MDRW2100009004.1.12",
          "form": "ㅎㅜㄴ ㅍ ㅜ ㄷㅡ 군여",
          "original_form": "ㅎㅜㄴ ㅍ ㅜ ㄷㅡ 군여",
          "speaker_id": "1",
          "time": "20210822 20:26"
        },
        {
          "id": "MDRW2100009004.1.13",
          "form": "ㅋㅋㅋ네 거기서 샀어요! 아예 세트상품으로 팔아요",
          "original_form": "ㅋㅋㅋ네 거기서 샀어요! 아예 세트상품으로 팔아요",
          "speaker_id": "2",
          "time": "20210822 20:27"
        },
        {
          "id": "MDRW2100009004.1.14",
          "form": "다대기랑 머릿고기슬라이스까지",
          "original_form": "다대기랑 머릿고기슬라이스까지",
          "speaker_id": "2",
          "time": "20210822 20:27"
        },
        {
          "id": "MDRW2100009004.1.15",
          "form": "세상 참 좋아졌어요",
          "original_form": "세상 참 좋아졌어요",
          "speaker_id": "1",
          "time": "20210822 20:28"
        },
        {
          "id": "MDRW2100009004.1.16",
          "form": "21세기죠",
          "original_form": "21세기죠",
          "speaker_id": "2",
          "time": "20210822 20:28"
        },
        {
          "id": "MDRW2100009004.1.17",
          "form": "이러다 국밥 그릇도 살기세에요",
          "original_form": "이러다 국밥 그릇도 살기세에요",
          "speaker_id": "1",
          "time": "20210822 20:28"
        },
        {
          "id": "MDRW2100009004.1.18",
          "form": "저도 사실 뚝배기 살까 잠깐 고민했어요",
          "original_form": "저도 사실 뚝배기 살까 잠깐 고민했어요",
          "speaker_id": "2",
          "time": "20210822 20:28"
        },
        {
          "id": "MDRW2100009004.1.19",
          "form": "충분히 이해해요",
          "original_form": "충분히 이해해요",
          "speaker_id": "1",
          "time": "20210822 20:28"
        },
        {
          "id": "MDRW2100009004.1.20",
          "form": "하지만 뚝배기.. 닦기 불편해서 그냥 생각에서 멈췄습니다",
          "original_form": "하지만 뚝배기.. 닦기 불편해서 그냥 생각에서 멈췄습니다",
          "speaker_id": "2",
          "time": "20210822 20:28"
        },
        {
          "id": "MDRW2100009004.1.21",
          "form": "그처 무겁기도 하고...",
          "original_form": "그처 무겁기도 하고...",
          "speaker_id": "1",
          "time": "20210822 20:29"
        },
        {
          "id": "MDRW2100009004.1.22",
          "form": "그돈으로 순대를 한 번 더 사겠어요",
          "original_form": "그돈으로 순대를 한 번 더 사겠어요",
          "speaker_id": "2",
          "time": "20210822 20:29"
        },
        {
          "id": "MDRW2100009004.1.23",
          "form": "위장꾸미기",
          "original_form": "위장꾸미기",
          "speaker_id": "2",
          "time": "20210822 20:29"
        },
        {
          "id": "MDRW2100009004.1.24",
          "form": "엘보우에 이어서 손목도 다치시면 안되니까",
          "original_form": "엘보우에 이어서 손목도 다치시면 안되니까",
          "speaker_id": "1",
          "time": "20210822 20:29"
        },
        {
          "id": "MDRW2100009004.1.25",
          "form": "위꾸",
          "original_form": "위꾸",
          "speaker_id": "1",
          "time": "20210822 20:29"
        },
        {
          "id": "MDRW2100009004.1.26",
          "form": "다꾸보다 효율적이네요",
          "original_form": "다꾸보다 효율적이네요",
          "speaker_id": "1",
          "time": "20210822 20:29"
        },
        {
          "id": "MDRW2100009004.1.27",
          "form": "다꾸는 15년 전에 열심히 했던것 같아요",
          "original_form": "다꾸는 15년 전에 열심히 했던것 같아요",
          "speaker_id": "2",
          "time": "20210822 20:30"
        },
        {
          "id": "MDRW2100009004.1.28",
          "form": "이제는 위장을 열심히..",
          "original_form": "이제는 위장을 열심히..",
          "speaker_id": "2",
          "time": "20210822 20:30"
        }
      ]
    }
