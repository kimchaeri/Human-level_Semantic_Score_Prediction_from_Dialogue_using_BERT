      "utterance": [
        {
          "id": "MDRW2100028283.1.1",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028283.1.2",
          "form": "전 고기파요ㅜㅠ",
          "original_form": "전 고기파요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028283.1.3",
          "form": "근데 요즘에는 탄수화물이 더 땡기는것같기도해요",
          "original_form": "근데 요즘에는 탄수화물이 더 땡기는것같기도해요",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028283.1.4",
          "form": "떡볶이라든지",
          "original_form": "떡볶이라든지",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100028283.1.5",
          "form": "떡볶이는 진짜 안질려요 ㅋㅋㅋ 저는 빵도 너무좋아하고 면요리도 너무좋아해요..밥도 좋아하고ㅜㅠㅜ",
          "original_form": "떡볶이는 진짜 안질려요 ㅋㅋㅋ 저는 빵도 너무좋아하고 면요리도 너무좋아해요..밥도 좋아하고ㅜㅠㅜ",
          "speaker_id": "1",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100028283.1.6",
          "form": "고기랑 밀가루랑 평생 둘중 하나만 먹을 수 있다면 밀가루에요 ㅋㅋㅋㅋ 님은 어떠세요",
          "original_form": "고기랑 밀가루랑 평생 둘중 하나만 먹을 수 있다면 밀가루에요 ㅋㅋㅋㅋ 님은 어떠세요",
          "speaker_id": "1",
          "time": "20210902 22:55"
        },
        {
          "id": "MDRW2100028283.1.7",
          "form": "전 고기요ㅋㅋ",
          "original_form": "전 고기요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:13"
        },
        {
          "id": "MDRW2100028283.1.8",
          "form": "밥 없어도 고기는 너무 잘먹어요",
          "original_form": "밥 없어도 고기는 너무 잘먹어요",
          "speaker_id": "2",
          "time": "20210903 09:13"
        },
        {
          "id": "MDRW2100028283.1.9",
          "form": "해산물은요?",
          "original_form": "해산물은요?",
          "speaker_id": "1",
          "time": "20210903 09:37"
        },
        {
          "id": "MDRW2100028283.1.10",
          "form": "고기>탄수화물>>>해산물입니당",
          "original_form": "고기>탄수화물>>>해산물입니당",
          "speaker_id": "2",
          "time": "20210903 11:32"
        },
        {
          "id": "MDRW2100028283.1.11",
          "form": "제가 비린내에 약해요",
          "original_form": "제가 비린내에 약해요",
          "speaker_id": "2",
          "time": "20210903 11:32"
        },
        {
          "id": "MDRW2100028283.1.12",
          "form": "아하 비린내 계란은요?",
          "original_form": "아하 비린내 계란은요?",
          "speaker_id": "1",
          "time": "20210903 14:11"
        },
        {
          "id": "MDRW2100028283.1.13",
          "form": "계란은 좋아해요",
          "original_form": "계란은 좋아해요",
          "speaker_id": "2",
          "time": "20210903 18:22"
        },
        {
          "id": "MDRW2100028283.1.14",
          "form": "바다 비린내를 잘 못맡는것같아요",
          "original_form": "바다 비린내를 잘 못맡는것같아요",
          "speaker_id": "2",
          "time": "20210903 18:22"
        },
        {
          "id": "MDRW2100028283.1.15",
          "form": "글쿤여 육류는 무슨고기를 젤 조아하나요",
          "original_form": "글쿤여 육류는 무슨고기를 젤 조아하나요",
          "speaker_id": "1",
          "time": "20210903 18:43"
        },
        {
          "id": "MDRW2100028283.1.16",
          "form": "닭이요!",
          "original_form": "닭이요!",
          "speaker_id": "2",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100028283.1.17",
          "form": "치킨 최고십니다",
          "original_form": "치킨 최고십니다",
          "speaker_id": "2",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100028283.1.18",
          "form": "치킨 맛있죠... 어디 브랜드를 주로 드시나요",
          "original_form": "치킨 맛있죠... 어디 브랜드를 주로 드시나요",
          "speaker_id": "1",
          "time": "20210904 19:15"
        },
        {
          "id": "MDRW2100028283.1.19",
          "form": "네네치킨이요!",
          "original_form": "네네치킨이요!",
          "speaker_id": "2",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100028283.1.20",
          "form": "네네치킨 무슨메뉴가 맛있어여?",
          "original_form": "네네치킨 무슨메뉴가 맛있어여?",
          "speaker_id": "1",
          "time": "20210906 22:09"
        },
        {
          "id": "MDRW2100028283.1.21",
          "form": "혹시 매운거좋아하시나요?",
          "original_form": "혹시 매운거좋아하시나요?",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100028283.1.22",
          "form": "좋아하시면 매콤치즈스노윙이요!!!",
          "original_form": "좋아하시면 매콤치즈스노윙이요!!!",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100028283.1.23",
          "form": "오오 매운거 좋아해요! 참고할게요 ㅎㅎ",
          "original_form": "오오 매운거 좋아해요! 참고할게요 ㅎㅎ",
          "speaker_id": "1",
