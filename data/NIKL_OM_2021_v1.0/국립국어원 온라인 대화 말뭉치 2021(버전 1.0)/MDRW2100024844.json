      "utterance": [
        {
          "id": "MDRW2100024844.1.1",
          "form": "다녀오셨어요?!",
          "original_form": "다녀오셨어요?!",
          "speaker_id": "2",
          "time": "20210831 19:42"
        },
        {
          "id": "MDRW2100024844.1.2",
          "form": "어디가 좋아요!?",
          "original_form": "어디가 좋아요!?",
          "speaker_id": "2",
          "time": "20210831 19:42"
        },
        {
          "id": "MDRW2100024844.1.3",
          "form": "방콕이랑 파타야 갔었는데 방콕은 쇼핑하기가 참 좋고.. 파타야는 바다가 진짜 끝내주더라고요",
          "original_form": "방콕이랑 파타야 갔었는데 방콕은 쇼핑하기가 참 좋고.. 파타야는 바다가 진짜 끝내주더라고요",
          "speaker_id": "1",
          "time": "20210831 19:47"
        },
        {
          "id": "MDRW2100024844.1.4",
          "form": "ㅜ크으 좋았겠네요",
          "original_form": "ㅜ크으 좋았겠네요",
          "speaker_id": "2",
          "time": "20210831 19:47"
        },
        {
          "id": "MDRW2100024844.1.5",
          "form": "방콕 가보고싶었는데",
          "original_form": "방콕 가보고싶었는데",
          "speaker_id": "2",
          "time": "20210831 19:47"
        },
        {
          "id": "MDRW2100024844.1.6",
          "form": "요즘은 국내여행을 많이들 가더라고요",
          "original_form": "요즘은 국내여행을 많이들 가더라고요",
          "speaker_id": "2",
          "time": "20210831 19:48"
        },
        {
          "id": "MDRW2100024844.1.7",
          "form": "네 너무 좋아요 사람들도 엄청 착해요",
          "original_form": "네 너무 좋아요 사람들도 엄청 착해요",
          "speaker_id": "1",
          "time": "20210831 19:48"
        },
        {
          "id": "MDRW2100024844.1.8",
          "form": "그러게요 아무래도 코로나땜시",
          "original_form": "그러게요 아무래도 코로나땜시",
          "speaker_id": "1",
          "time": "20210831 19:48"
        },
        {
          "id": "MDRW2100024844.1.9",
          "form": "오오 담에 코로나 끝나면 꼭 가봐야겠어요",
          "original_form": "오오 담에 코로나 끝나면 꼭 가봐야겠어요",
          "speaker_id": "2",
          "time": "20210831 19:48"
        },
        {
          "id": "MDRW2100024844.1.10",
          "form": "펜션 잡아서 노는거 같던데",
          "original_form": "펜션 잡아서 노는거 같던데",
          "speaker_id": "2",
          "time": "20210831 19:49"
        },
        {
          "id": "MDRW2100024844.1.11",
          "form": "갈 사람은 다 가더라고요",
          "original_form": "갈 사람은 다 가더라고요",
          "speaker_id": "2",
          "time": "20210831 19:49"
        },
        {
          "id": "MDRW2100024844.1.12",
          "form": "맞아요 전 못 가겠던데.. 뭔가 가도 되나? 할때쯤 다시 심각해지더라고요 ㅋㅋㅋ",
          "original_form": "맞아요 전 못 가겠던데.. 뭔가 가도 되나? 할때쯤 다시 심각해지더라고요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:50"
        },
        {
          "id": "MDRW2100024844.1.13",
          "form": "그니깐요 나도 가볼까?  하면 막터지고",
          "original_form": "그니깐요 나도 가볼까?  하면 막터지고",
          "speaker_id": "2",
          "time": "20210831 19:51"
        },
        {
          "id": "MDRW2100024844.1.14",
          "form": "백신을 다 맞더라도 끝날까 싶고 그르네요",
          "original_form": "백신을 다 맞더라도 끝날까 싶고 그르네요",
          "speaker_id": "2",
          "time": "20210831 19:52"
        },
        {
          "id": "MDRW2100024844.1.15",
          "form": "그쵸.. 여행가고 이러는게 먼얘기 같네요.. 까마득하고",
          "original_form": "그쵸.. 여행가고 이러는게 먼얘기 같네요.. 까마득하고",
          "speaker_id": "1",
          "time": "20210831 19:53"
        },
        {
          "id": "MDRW2100024844.1.16",
          "form": "여행계획 같은 거 짤때 어떤 타입이신가요",
          "original_form": "여행계획 같은 거 짤때 어떤 타입이신가요",
          "speaker_id": "1",
          "time": "20210831 19:54"
        },
        {
          "id": "MDRW2100024844.1.17",
          "form": "진짜 까마득해요 벌써 9월이라는것도 시간이 참ㅜ",
          "original_form": "진짜 까마득해요 벌써 9월이라는것도 시간이 참ㅜ",
          "speaker_id": "2",
          "time": "20210831 20:41"
        },
        {
          "id": "MDRW2100024844.1.18",
          "form": "저는 크게 이곳저곳 찾아보고 딱 몇곳만가요",
          "original_form": "저는 크게 이곳저곳 찾아보고 딱 몇곳만가요",
          "speaker_id": "2",
          "time": "20210831 20:42"
        },
        {
          "id": "MDRW2100024844.1.19",
          "form": "계획을 철저하게 짜지는 못해요 ㅎ 그냥 가서 길잃지않을정도만",
          "original_form": "계획을 철저하게 짜지는 못해요 ㅎ 그냥 가서 길잃지않을정도만",
          "speaker_id": "2",
          "time": "20210831 20:42"
        },
        {
          "id": "MDRW2100024844.1.20",
          "form": "아 저랑 비슷하시네요 ㅋㅋㅋ 저도 가보고싶은 곳 리스트만 뽑아놓고 날짜별로 대충 이때 가고 이런 식이에요 ㅎㅎ",
          "original_form": "아 저랑 비슷하시네요 ㅋㅋㅋ 저도 가보고싶은 곳 리스트만 뽑아놓고 날짜별로 대충 이때 가고 이런 식이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 21:21"
        },
        {
          "id": "MDRW2100024844.1.21",
          "form": "제 동생은 엄청 철저하게 짜더라고요 분별로 ㅋㅋ",
          "original_form": "제 동생은 엄청 철저하게 짜더라고요 분별로 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:21"
        },
        {
          "id": "MDRW2100024844.1.22",
          "form": "동생 분이랑 여행가면 재밌겠네요",
          "original_form": "동생 분이랑 여행가면 재밌겠네요",
          "speaker_id": "2",
          "time": "20210831 21:27"
        },
        {
          "id": "MDRW2100024844.1.23",
          "form": "맡기면 알아서 다 해줘요 ㅋㅋㅋ 대신 리액션을 잘 해줘야되는..",
          "original_form": "맡기면 알아서 다 해줘요 ㅋㅋㅋ 대신 리액션을 잘 해줘야되는..",
          "speaker_id": "1",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100024844.1.24",
          "form": "오ㅋㅋㅋ 뭔가 귀엽네요",
          "original_form": "오ㅋㅋㅋ 뭔가 귀엽네요",
          "speaker_id": "2",
          "time": "20210831 21:31"
        },
        {
          "id": "MDRW2100024844.1.25",
          "form": "부럽다",
          "original_form": "부럽다",
          "speaker_id": "2",
          "time": "20210831 21:31"
        },
        {
          "id": "MDRW2100024844.1.26",
          "form": "자매(남매)끼리 여행가는거 부럽더라고요",
          "original_form": "자매(남매)끼리 여행가는거 부럽더라고요",
          "speaker_id": "2",
          "time": "20210831 21:31"
        },
        {
          "id": "MDRW2100024844.1.27",
          "form": "네 편하긴 해요 ㅋㅋㅋ",
          "original_form": "네 편하긴 해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:31"
        },
        {
          "id": "MDRW2100024844.1.28",
          "form": "친구보다 편한 면이 있어요 근데 서로 이쁜 사진 안 찍어줘요 각자 도생 ㅋㅋㅋ",
          "original_form": "친구보다 편한 면이 있어요 근데 서로 이쁜 사진 안 찍어줘요 각자 도생 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100024844.1.29",
          "form": "저는 보통 혼자 다녀서 될대로 되라ㅋㅋㅋ",
          "original_form": "저는 보통 혼자 다녀서 될대로 되라ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100024844.1.30",
          "form": "아 맞다",
          "original_form": "아 맞다",
          "speaker_id": "2",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100024844.1.31",
          "form": "ㅋㅋㅋ사진",
          "original_form": "ㅋㅋㅋ사진",
          "speaker_id": "2",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100024844.1.32",
          "form": "친구랑 가면 사진찍어줘야하죠",
          "original_form": "친구랑 가면 사진찍어줘야하죠",
          "speaker_id": "2",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100024844.1.33",
          "form": "네 ㅋㅋㅋ 그건 또 그거대로 귀찮고 ㅋㅋ",
          "original_form": "네 ㅋㅋㅋ 그건 또 그거대로 귀찮고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:33"
        }
      ]
