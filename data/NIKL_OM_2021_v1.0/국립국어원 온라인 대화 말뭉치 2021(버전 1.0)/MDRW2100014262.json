      "utterance": [
        {
          "id": "MDRW2100014262.1.1",
          "form": "아뇨 저 거의 저승사자인데요",
          "original_form": "아뇨 저 거의 저승사자인데요",
          "speaker_id": "2",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014262.1.2",
          "form": "블랙맨입니다",
          "original_form": "블랙맨입니다",
          "speaker_id": "2",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014262.1.3",
          "form": "근데 요즘 색 도전하고 있어요",
          "original_form": "근데 요즘 색 도전하고 있어요",
          "speaker_id": "2",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014262.1.4",
          "form": "쿨톤이세요 웜톤이세여?",
          "original_form": "쿨톤이세요 웜톤이세여?",
          "speaker_id": "2",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014262.1.5",
          "form": "어..",
          "original_form": "어..",
          "speaker_id": "1",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014262.1.6",
          "form": "글쎄요 제 톤이 뭘까요",
          "original_form": "글쎄요 제 톤이 뭘까요",
          "speaker_id": "1",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014262.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014262.1.8",
          "form": "어떤색 도전하시나요",
          "original_form": "어떤색 도전하시나요",
          "speaker_id": "1",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014262.1.9",
          "form": "저 노란색이랑 빨간색",
          "original_form": "저 노란색이랑 빨간색",
          "speaker_id": "2",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100014262.1.10",
          "form": "그리고 녹색",
          "original_form": "그리고 녹색",
          "speaker_id": "2",
          "time": "20210825 18:01"
        },
        {
          "id": "MDRW2100014262.1.11",
          "form": "약간 따뜻한계열",
          "original_form": "약간 따뜻한계열",
          "speaker_id": "1",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014262.1.12",
          "form": "쓰읍 무채색 잘어울리시면 쿨톤이신가",
          "original_form": "쓰읍 무채색 잘어울리시면 쿨톤이신가",
          "speaker_id": "2",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014262.1.13",
          "form": "도전하시네요",
          "original_form": "도전하시네요",
          "speaker_id": "1",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014262.1.14",
          "form": "근데 톤이 뭔상관인가요 걍 조으면 입는거죠",
          "original_form": "근데 톤이 뭔상관인가요 걍 조으면 입는거죠",
          "speaker_id": "2",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014262.1.15",
          "form": "어때요 잘 받는 색 같나요",
          "original_form": "어때요 잘 받는 색 같나요",
          "speaker_id": "1",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014262.1.16",
          "form": "맞으요",
          "original_form": "맞으요",
          "speaker_id": "2",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014262.1.17",
          "form": "넘 검정밖에 없다고",
          "original_form": "넘 검정밖에 없다고",
          "speaker_id": "2",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014262.1.18",
          "form": "흰검 이래서 바코드냐고 하더라고요",
          "original_form": "흰검 이래서 바코드냐고 하더라고요",
          "speaker_id": "2",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014262.1.19",
          "form": "바코듴ㅋㅋㅋㄱㅋ",
          "original_form": "바코듴ㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014262.1.20",
          "form": "아 저 여잰에",
          "original_form": "아 저 여잰에",
          "speaker_id": "1",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014262.1.21",
          "form": "예전에",
          "original_form": "예전에",
          "speaker_id": "1",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014262.1.22",
          "form": "친구들이랑 옷을 입고나갔는데",
          "original_form": "친구들이랑 옷을 입고나갔는데",
          "speaker_id": "1",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014262.1.23",
          "form": "빨강 노랑 초록이어서",
          "original_form": "빨강 노랑 초록이어서",
          "speaker_id": "1",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014262.1.24",
          "form": "니들은 소스모임이냐고",
          "original_form": "니들은 소스모임이냐고",
          "speaker_id": "1",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014262.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014262.1.26",
          "form": "케첩 머스타드",
          "original_form": "케첩 머스타드",
          "speaker_id": "2",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014262.1.27",
          "form": "케챱 머스타드 와사비",
          "original_form": "케챱 머스타드 와사비",
          "speaker_id": "1",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014262.1.28",
          "form": "초록은 뭐죠?",
          "original_form": "초록은 뭐죠?",
          "speaker_id": "2",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014262.1.29",
          "form": "아나 와사비",
          "original_form": "아나 와사비",
          "speaker_id": "2",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014262.1.30",
          "form": "여기 다잇다고",
          "original_form": "여기 다잇다고",
          "speaker_id": "1",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014262.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:03"
        },
        {
          "id": "MDRW2100014262.1.32",
          "form": "쓰읍 웃을때가 아니네",
          "original_form": "쓰읍 웃을때가 아니네",
          "speaker_id": "2",
          "time": "20210825 18:04"
        },
