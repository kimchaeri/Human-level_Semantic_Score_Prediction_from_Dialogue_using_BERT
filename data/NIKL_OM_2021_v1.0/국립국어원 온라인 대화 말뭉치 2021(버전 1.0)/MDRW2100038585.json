      "utterance": [
        {
          "id": "MDRW2100038585.1.1",
          "form": "운동하세요?",
          "original_form": "운동하세요?",
          "speaker_id": "2",
          "time": "20210902 16:22"
        },
        {
          "id": "MDRW2100038585.1.2",
          "form": "홈트 가끔이요",
          "original_form": "홈트 가끔이요",
          "speaker_id": "1",
          "time": "20210902 16:34"
        },
        {
          "id": "MDRW2100038585.1.3",
          "form": "어 저두여 ㅎ",
          "original_form": "어 저두여 ㅎ",
          "speaker_id": "2",
          "time": "20210902 16:57"
        },
        {
          "id": "MDRW2100038585.1.4",
          "form": "근데 요새는 아예 안하고 있어요",
          "original_form": "근데 요새는 아예 안하고 있어요",
          "speaker_id": "2",
          "time": "20210902 16:57"
        },
        {
          "id": "MDRW2100038585.1.5",
          "form": "ㅋㅋ매일하기 힘들어요",
          "original_form": "ㅋㅋ매일하기 힘들어요",
          "speaker_id": "1",
          "time": "20210902 17:02"
        },
        {
          "id": "MDRW2100038585.1.6",
          "form": "귀찮기두 하구요",
          "original_form": "귀찮기두 하구요",
          "speaker_id": "1",
          "time": "20210902 17:02"
        },
        {
          "id": "MDRW2100038585.1.7",
          "form": "정말요",
          "original_form": "정말요",
          "speaker_id": "2",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100038585.1.8",
          "form": "그래서 운동을 다녀야하나 고민입니다 ㅠ",
          "original_form": "그래서 운동을 다녀야하나 고민입니다 ㅠ",
          "speaker_id": "2",
          "time": "20210902 18:48"
        },
        {
          "id": "MDRW2100038585.1.9",
          "form": "딱히 할 건 없더라구요",
          "original_form": "딱히 할 건 없더라구요",
          "speaker_id": "1",
          "time": "20210902 20:09"
        },
        {
          "id": "MDRW2100038585.1.10",
          "form": "남들 하는거 보는건 재밌는데ㅋㅋㅋ",
          "original_form": "남들 하는거 보는건 재밌는데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:09"
        },
        {
          "id": "MDRW2100038585.1.11",
          "form": "ㅋㅋㅋㅋㅋ전 필라테스 할까 고민되네요",
          "original_form": "ㅋㅋㅋㅋㅋ전 필라테스 할까 고민되네요",
          "speaker_id": "2",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100038585.1.12",
          "form": "보기보다 힘들다고 하더라구요",
          "original_form": "보기보다 힘들다고 하더라구요",
          "speaker_id": "1",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100038585.1.13",
          "form": "저 한번 해봤는데",
          "original_form": "저 한번 해봤는데",
          "speaker_id": "2",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100038585.1.14",
          "form": "힘들었어요...ㅋㅋ",
          "original_form": "힘들었어요...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100038585.1.15",
          "form": "ㅋㅋㅋ자세교정으로 추천많이하던데요. 효과있나요?",
          "original_form": "ㅋㅋㅋ자세교정으로 추천많이하던데요. 효과있나요?",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100038585.1.16",
          "form": "전 달랑.. 한달망해서",
          "original_form": "전 달랑.. 한달망해서",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100038585.1.17",
          "form": "효과는 못 본 거 같아요ㅋㅋ",
          "original_form": "효과는 못 본 거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100038585.1.18",
          "form": "운동효과 보려면 반년은 해야겠죠? 힘든데..ㅎ",
          "original_form": "운동효과 보려면 반년은 해야겠죠? 힘든데..ㅎ",
          "speaker_id": "1",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100038585.1.19",
          "form": "네 ㅠ 효과를 보려면요",
          "original_form": "네 ㅠ 효과를 보려면요",
