      "utterance": [
        {
          "id": "MDRW2100034098.1.1",
          "form": "슬의생 보세요?",
          "original_form": "슬의생 보세요?",
          "speaker_id": "2",
          "time": "20210827 18:00"
        },
        {
          "id": "MDRW2100034098.1.2",
          "form": "아니요.. 저빼고다보는느낌이에요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니요.. 저빼고다보는느낌이에요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:31"
        },
        {
          "id": "MDRW2100034098.1.3",
          "form": "재밌나요??",
          "original_form": "재밌나요??",
          "speaker_id": "1",
          "time": "20210827 18:31"
        },
        {
          "id": "MDRW2100034098.1.4",
          "form": "부모님 보실때 슬쩍 보는데 노래부르는것만 기억나요ㅋㅋㅋ",
          "original_form": "부모님 보실때 슬쩍 보는데 노래부르는것만 기억나요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:34"
        },
        {
          "id": "MDRW2100034098.1.5",
          "form": "아하~ ㅋㅋ 약간 힐링드잖아요 맞죠?",
          "original_form": "아하~ ㅋㅋ 약간 힐링드잖아요 맞죠?",
          "speaker_id": "1",
          "time": "20210827 18:34"
        },
        {
          "id": "MDRW2100034098.1.6",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100034098.1.7",
          "form": "그런거 같더라구요",
          "original_form": "그런거 같더라구요",
          "speaker_id": "2",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100034098.1.8",
          "form": "배우분들 다 노래잘부르시는거같더라고요",
          "original_form": "배우분들 다 노래잘부르시는거같더라고요",
          "speaker_id": "1",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100034098.1.9",
          "form": "맞아요ㅋㅋ 그래서 우와.. 하네요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ 그래서 우와.. 하네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100034098.1.10",
          "form": "ㅋㅋㅋㅋ맞죠맞죠",
          "original_form": "ㅋㅋㅋㅋ맞죠맞죠",
          "speaker_id": "1",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100034098.1.11",
          "form": "요새 음악 뭐들으세요?",
          "original_form": "요새 음악 뭐들으세요?",
          "speaker_id": "1",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100034098.1.12",
          "form": "요즘은 발라드요!",
          "original_form": "요즘은 발라드요!",
          "speaker_id": "2",
          "time": "20210827 22:29"
        },
        {
          "id": "MDRW2100034098.1.13",
          "form": "아하! 가수 누구좋아하세요?",
          "original_form": "아하! 가수 누구좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 22:31"
        },
        {
          "id": "MDRW2100034098.1.14",
          "form": "아이유요!",
          "original_form": "아이유요!",
          "speaker_id": "2",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100034098.1.15",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100034098.1.16",
          "form": "국민여동생아이유 넘좋죠~ 전 요새 소향님 너무좋더라구요",
          "original_form": "국민여동생아이유 넘좋죠~ 전 요새 소향님 너무좋더라구요",
          "speaker_id": "1",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100034098.1.17",
          "form": "오~  노래 잘부르시죠",
          "original_form": "오~  노래 잘부르시죠",
