        },
        {
          "id": "MDRW2100029516.1.2",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 09:53"
        },
        {
          "id": "MDRW2100029516.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210903 11:35"
        },
        {
          "id": "MDRW2100029516.1.4",
          "form": "겜 좋아하죠. 폰게임이요",
          "original_form": "겜 좋아하죠. 폰게임이요",
          "speaker_id": "2",
          "time": "20210903 11:35"
        },
        {
          "id": "MDRW2100029516.1.5",
          "form": "저는 남하는거 구경만해요",
          "original_form": "저는 남하는거 구경만해요",
          "speaker_id": "1",
          "time": "20210903 11:38"
        },
        {
          "id": "MDRW2100029516.1.6",
          "form": "무슨 겜 좋아해요?",
          "original_form": "무슨 겜 좋아해요?",
          "speaker_id": "1",
          "time": "20210903 11:38"
        },
        {
          "id": "MDRW2100029516.1.7",
          "form": "피쉬돔이라고",
          "original_form": "피쉬돔이라고",
          "speaker_id": "2",
          "time": "20210903 11:40"
        },
        {
          "id": "MDRW2100029516.1.8",
          "form": "퍼즐게임이요",
          "original_form": "퍼즐게임이요",
          "speaker_id": "2",
          "time": "20210903 11:40"
        },
        {
          "id": "MDRW2100029516.1.9",
          "form": "첨 들어봤어요",
          "original_form": "첨 들어봤어요",
          "speaker_id": "1",
          "time": "20210903 11:41"
        },
        {
          "id": "MDRW2100029516.1.10",
          "form": "재밌겠어요",
          "original_form": "재밌겠어요",
          "speaker_id": "1",
          "time": "20210903 11:41"
        },
        {
          "id": "MDRW2100029516.1.11",
          "form": "제가 아는 건 쿠키런킹덤",
          "original_form": "제가 아는 건 쿠키런킹덤",
          "speaker_id": "1",
          "time": "20210903 11:41"
        },
        {
          "id": "MDRW2100029516.1.12",
          "form": "그게 유명하죠",
          "original_form": "그게 유명하죠",
          "speaker_id": "2",
          "time": "20210903 11:42"
        },
        {
          "id": "MDRW2100029516.1.13",
          "form": "튀김키우기라고 아세요",
          "original_form": "튀김키우기라고 아세요",
          "speaker_id": "2",
          "time": "20210903 11:42"
        },
        {
          "id": "MDRW2100029516.1.14",
          "form": "아뇨ㅜㅜ",
          "original_form": "아뇨ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 11:43"
        },
        {
          "id": "MDRW2100029516.1.15",
          "form": "아는게없네요",
          "original_form": "아는게없네요",
          "speaker_id": "1",
          "time": "20210903 11:43"
        },
        {
          "id": "MDRW2100029516.1.16",
          "form": "ㅎㅎ 단순한게임은 시간떼우기좋아요",
          "original_form": "ㅎㅎ 단순한게임은 시간떼우기좋아요",
          "speaker_id": "2",
          "time": "20210903 11:44"
        },
        {
          "id": "MDRW2100029516.1.17",
          "form": "스트레스받을때잊 게해줘요",
          "original_form": "스트레스받을때잊 게해줘요",
          "speaker_id": "2",
          "time": "20210903 11:44"
        },
        {
          "id": "MDRW2100029516.1.18",
          "form": "그럴듯해요",
          "original_form": "그럴듯해요",
          "speaker_id": "1",
          "time": "20210903 11:44"
        },
        {
          "id": "MDRW2100029516.1.19",
          "form": "저는 그거 좋아해요",
          "original_form": "저는 그거 좋아해요",
          "speaker_id": "1",
          "time": "20210903 11:44"
        },
        {
          "id": "MDRW2100029516.1.20",
          "form": "서핑게임",
          "original_form": "서핑게임",
          "speaker_id": "1",
          "time": "20210903 11:45"
        },
        {
          "id": "MDRW2100029516.1.21",
          "form": "이름이 뭐에요",
          "original_form": "이름이 뭐에요",
          "speaker_id": "2",
          "time": "20210903 11:45"
        },
        {
          "id": "MDRW2100029516.1.22",
          "form": "마이크로엣지에",
          "original_form": "마이크로엣지에",
          "speaker_id": "1",
          "time": "20210903 11:45"
        },
        {
          "id": "MDRW2100029516.1.23",
          "form": "깔린건데",
          "original_form": "깔린건데",
          "speaker_id": "1",
          "time": "20210903 11:45"
        },
        {
          "id": "MDRW2100029516.1.24",
          "form": "나름 재밌어요",
          "original_form": "나름 재밌어요",
          "speaker_id": "1",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100029516.1.25",
          "form": "오 휴대폰으로 균형잡나요",
          "original_form": "오 휴대폰으로 균형잡나요",
          "speaker_id": "2",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100029516.1.26",
          "form": "저는 블록맞춰없애는 단순한거 좋아해요",
          "original_form": "저는 블록맞춰없애는 단순한거 좋아해요",
          "speaker_id": "2",
          "time": "20210903 11:47"
        },
        {
          "id": "MDRW2100029516.1.27",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210903 11:48"
        },
        {
          "id": "MDRW2100029516.1.28",
          "form": "고전 게임 중에 테트리스 혹시 아세요?",
          "original_form": "고전 게임 중에 테트리스 혹시 아세요?",
