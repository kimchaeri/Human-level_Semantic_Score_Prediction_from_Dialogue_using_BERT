      "utterance": [
        {
          "id": "MDRW2100042421.1.1",
          "form": "놀러가고싶으신 장소 있으세요?",
          "original_form": "놀러가고싶으신 장소 있으세요?",
          "speaker_id": "1",
          "time": "20210906 22:03"
        },
        {
          "id": "MDRW2100042421.1.2",
          "form": "작년부터 코로나로 못움직였는데",
          "original_form": "작년부터 코로나로 못움직였는데",
          "speaker_id": "2",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100042421.1.3",
          "form": "좀 마음 놓고 다닐 수 있게 되면",
          "original_form": "좀 마음 놓고 다닐 수 있게 되면",
          "speaker_id": "2",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100042421.1.4",
          "form": "내장신 다시 가보고 싶어요",
          "original_form": "내장신 다시 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100042421.1.5",
          "form": "내장신이 어디예요?",
          "original_form": "내장신이 어디예요?",
          "speaker_id": "1",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100042421.1.6",
          "form": "내장산이요\n오타났었네요",
          "original_form": "내장산이요\n오타났었네요",
          "speaker_id": "2",
          "time": "20210907 06:24"
        },
        {
          "id": "MDRW2100042421.1.7",
          "form": "전라도에 있어요",
          "original_form": "전라도에 있어요",
          "speaker_id": "2",
          "time": "20210907 06:24"
        },
        {
          "id": "MDRW2100042421.1.8",
          "form": "아하 등산 좋아하세요?",
          "original_form": "아하 등산 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 08:33"
        },
        {
          "id": "MDRW2100042421.1.9",
          "form": "엄청 좋아했죠",
          "original_form": "엄청 좋아했죠",
          "speaker_id": "2",
          "time": "20210907 08:36"
        },
        {
          "id": "MDRW2100042421.1.10",
          "form": "작년부터는 못가고 있지만요",
          "original_form": "작년부터는 못가고 있지만요",
          "speaker_id": "2",
          "time": "20210907 08:36"
        },
        {
          "id": "MDRW2100042421.1.11",
          "form": "저도 등산좋아해요 자주 가지는 못하고 있지만요ㅠ",
          "original_form": "저도 등산좋아해요 자주 가지는 못하고 있지만요ㅠ",
          "speaker_id": "1",
          "time": "20210907 11:54"
        },
        {
          "id": "MDRW2100042421.1.12",
          "form": "산 타면 피톤치드도 맡고 좋아요",
          "original_form": "산 타면 피톤치드도 맡고 좋아요",
          "speaker_id": "2",
          "time": "20210907 13:38"
        },
        {
          "id": "MDRW2100042421.1.13",
          "form": "그냥 건강해지는 느낌",
          "original_form": "그냥 건강해지는 느낌",
          "speaker_id": "2",
          "time": "20210907 13:38"
        },
        {
          "id": "MDRW2100042421.1.14",
          "form": "몸 움직이는 걸 좋아해서 즐겨 다녔었죠",
          "original_form": "몸 움직이는 걸 좋아해서 즐겨 다녔었죠",
          "speaker_id": "2",
          "time": "20210907 13:38"
        },
        {
          "id": "MDRW2100042421.1.15",
          "form": "저는 움직이는걸 즐기는편은 아닌데 예외로 산책이랑 등산은 구경하는 재미도 있고 좋더라구요ㅎㅎ",
          "original_form": "저는 움직이는걸 즐기는편은 아닌데 예외로 산책이랑 등산은 구경하는 재미도 있고 좋더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 15:23"
        },
        {
          "id": "MDRW2100042421.1.16",
          "form": "풍경 보는 걸 좋아하나 봅니다",
          "original_form": "풍경 보는 걸 좋아하나 봅니다",
          "speaker_id": "2",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100042421.1.17",
          "form": "그럼 풍경 좋은 곳으로 여행 다녀야 겠네요",
          "original_form": "그럼 풍경 좋은 곳으로 여행 다녀야 겠네요",
          "speaker_id": "2",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100042421.1.18",
          "form": "맞아요 생각정리할때 좋더라구요",
          "original_form": "맞아요 생각정리할때 좋더라구요",
          "speaker_id": "1",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100042421.1.19",
          "form": "풍경 좋은곳이 어디어디있을까요",
          "original_form": "풍경 좋은곳이 어디어디있을까요",
          "speaker_id": "1",
          "time": "20210907 15:24"
        },
        {
          "id": "MDRW2100042421.1.20",
          "form": "도시보다는 시골쪽 가야겠죠",
          "original_form": "도시보다는 시골쪽 가야겠죠",
          "speaker_id": "2",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100042421.1.21",
          "form": "아니면 휴양림도 괜찮아요",
          "original_form": "아니면 휴양림도 괜찮아요",
          "speaker_id": "2",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100042421.1.22",
          "form": "숲속에 들어가 있는 느낌이니까요",
          "original_form": "숲속에 들어가 있는 느낌이니까요",
          "speaker_id": "2",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100042421.1.23",
          "form": "휴양림 좋네요! 벌레는 좀 많겠지만요ㅋㅋ",
          "original_form": "휴양림 좋네요! 벌레는 좀 많겠지만요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:39"
        },
        {
          "id": "MDRW2100042421.1.24",
          "form": "벌레 안좋아하는군요",
          "original_form": "벌레 안좋아하는군요",
          "speaker_id": "2",
          "time": "20210907 16:40"
        },
        {
          "id": "MDRW2100042421.1.25",
          "form": "휴양림은 벌레가 눈에 띄는 정도는 아니에요",
          "original_form": "휴양림은 벌레가 눈에 띄는 정도는 아니에요",
          "speaker_id": "2",
          "time": "20210907 16:40"
        },
        {
          "id": "MDRW2100042421.1.26",
          "form": "관광지처럼 해놓은 곳이라 :)",
          "original_form": "관광지처럼 해놓은 곳이라 :)",
          "speaker_id": "2",
          "time": "20210907 16:41"
        },
        {
          "id": "MDRW2100042421.1.27",
          "form": "헉 진짜요? 한번도 안가봐서 몰랐어요",
          "original_form": "헉 진짜요? 한번도 안가봐서 몰랐어요",
          "speaker_id": "1",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100042421.1.28",
          "form": "기회되면 꼭꼭 가보고싶네요",
          "original_form": "기회되면 꼭꼭 가보고싶네요",
          "speaker_id": "1",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100042421.1.29",
          "form": "약간 숲속에 있는 팬션느낌?",
          "original_form": "약간 숲속에 있는 팬션느낌?",
          "speaker_id": "2",
          "time": "20210908 06:15"
        },
        {
          "id": "MDRW2100042421.1.30",
          "form": "가보면 만족할거에요^^",
          "original_form": "가보면 만족할거에요^^",
          "speaker_id": "2",
          "time": "20210908 06:15"
        }
      ]
