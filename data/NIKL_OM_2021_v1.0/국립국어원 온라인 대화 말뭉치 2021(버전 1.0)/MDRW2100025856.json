      "utterance": [
        {
          "id": "MDRW2100025856.1.1",
          "form": "비 오기 전에",
          "original_form": "비 오기 전에",
          "speaker_id": "2",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025856.1.2",
          "form": "나갔다 와서 다행이다",
          "original_form": "나갔다 와서 다행이다",
          "speaker_id": "2",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025856.1.3",
          "form": "ㅇㅇ 잘 버리고 왔음",
          "original_form": "ㅇㅇ 잘 버리고 왔음",
          "speaker_id": "1",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100025856.1.4",
          "form": "요즘 계속 비오네",
          "original_form": "요즘 계속 비오네",
          "speaker_id": "2",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100025856.1.5",
          "form": "오락 가락 한다",
          "original_form": "오락 가락 한다",
          "speaker_id": "2",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100025856.1.6",
          "form": "ㅇㅇ습한데 괜찮기도하고 습함",
          "original_form": "ㅇㅇ습한데 괜찮기도하고 습함",
          "speaker_id": "1",
          "time": "20210901 17:37"
        },
        {
          "id": "MDRW2100025856.1.7",
          "form": "방 제습기 한번씩 돌려",
          "original_form": "방 제습기 한번씩 돌려",
          "speaker_id": "2",
          "time": "20210901 17:37"
        },
        {
          "id": "MDRW2100025856.1.8",
          "form": "그제 돌렸는데 돌릴 때 뿐인 거 가틈",
          "original_form": "그제 돌렸는데 돌릴 때 뿐인 거 가틈",
          "speaker_id": "1",
          "time": "20210901 17:38"
        },
        {
          "id": "MDRW2100025856.1.9",
          "form": "그래도 옷장에 습기 있음 안되니까 자주 돌려라",
          "original_form": "그래도 옷장에 습기 있음 안되니까 자주 돌려라",
          "speaker_id": "2",
          "time": "20210901 17:38"
        },
        {
          "id": "MDRW2100025856.1.10",
          "form": "ㅇㅇ비 이제 좀 그쳤음 좋겠다",
          "original_form": "ㅇㅇ비 이제 좀 그쳤음 좋겠다",
          "speaker_id": "1",
          "time": "20210901 17:38"
        },
        {
          "id": "MDRW2100025856.1.11",
          "form": "여름에 가물더니 이제 계속 오네",
          "original_form": "여름에 가물더니 이제 계속 오네",
          "speaker_id": "2",
          "time": "20210901 17:39"
        },
        {
          "id": "MDRW2100025856.1.12",
          "form": "그래도 비 와서 좀 기온은 낮아진듯",
          "original_form": "그래도 비 와서 좀 기온은 낮아진듯",
          "speaker_id": "1",
          "time": "20210901 17:39"
        },
        {
          "id": "MDRW2100025856.1.13",
          "form": "대신 습해서 힘들어서 그렇지",
          "original_form": "대신 습해서 힘들어서 그렇지",
          "speaker_id": "1",
          "time": "20210901 17:40"
        },
        {
          "id": "MDRW2100025856.1.14",
          "form": "습하니까 빨래가 안 말라서",
          "original_form": "습하니까 빨래가 안 말라서",
          "speaker_id": "2",
          "time": "20210901 17:40"
        },
        {
          "id": "MDRW2100025856.1.15",
          "form": "빨래를 못 하고 있다",
          "original_form": "빨래를 못 하고 있다",
          "speaker_id": "2",
          "time": "20210901 17:40"
        },
        {
          "id": "MDRW2100025856.1.16",
          "form": "맞아 비 그치면 이불갈자ㅠㅜ",
          "original_form": "맞아 비 그치면 이불갈자ㅠㅜ",
          "speaker_id": "1",
          "time": "20210901 17:40"
        },
