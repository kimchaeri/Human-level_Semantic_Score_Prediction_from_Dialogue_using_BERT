      "utterance": [
        {
          "id": "MDRW2100037371.1.1",
          "form": "야식 좋아하세요?",
          "original_form": "야식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100037371.1.2",
          "form": "네 아직 좋아요 살찔까봐 못 먹죠",
          "original_form": "네 아직 좋아요 살찔까봐 못 먹죠",
          "speaker_id": "2",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100037371.1.3",
          "form": "저는 오늘부터 빠이 ㅜㅜ",
          "original_form": "저는 오늘부터 빠이 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100037371.1.4",
          "form": "오늘부터 다이어트 하시나 봐요",
          "original_form": "오늘부터 다이어트 하시나 봐요",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037371.1.5",
          "form": "낼부턴데 야식은 미리 빠이햇어여",
          "original_form": "낼부턴데 야식은 미리 빠이햇어여",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037371.1.6",
          "form": "네 잘하셨어요 오늘부터 빨리 시작하면 빨리 빠지겠죠",
          "original_form": "네 잘하셨어요 오늘부터 빨리 시작하면 빨리 빠지겠죠",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100037371.1.7",
          "form": "글츄   그대신 아침은 든든하게 먹구 ㅋ",
          "original_form": "글츄   그대신 아침은 든든하게 먹구 ㅋ",
          "speaker_id": "1",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100037371.1.8",
          "form": "성공적인 다이어트를 응원합니다",
          "original_form": "성공적인 다이어트를 응원합니다",
          "speaker_id": "2",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100037371.1.9",
          "form": "네네 감사유~",
          "original_form": "네네 감사유~",
          "speaker_id": "1",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100037371.1.10",
          "form": "평소 어떤 음식 즐겨 드세요?",
          "original_form": "평소 어떤 음식 즐겨 드세요?",
          "speaker_id": "1",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100037371.1.11",
          "form": "카페 자주 가세요? 비빔밥.자주먹어요",
          "original_form": "카페 자주 가세요? 비빔밥.자주먹어요",
          "speaker_id": "2",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100037371.1.12",
          "form": "아항 저는  코로나 후론 못갓어여",
          "original_form": "아항 저는  코로나 후론 못갓어여",
          "speaker_id": "1",
          "time": "20210831 23:07"
        },
        {
          "id": "MDRW2100037371.1.13",
          "form": "카페에서 어머니 코로나 걸릴 것 같아요",
          "original_form": "카페에서 어머니 코로나 걸릴 것 같아요",
