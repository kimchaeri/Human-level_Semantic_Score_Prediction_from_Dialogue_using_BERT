        },
        {
          "id": "MDRW2100020584.1.2",
          "form": "캐치를하시다니",
          "original_form": "캐치를하시다니",
          "speaker_id": "1",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020584.1.3",
          "form": "후후",
          "original_form": "후후",
          "speaker_id": "2",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020584.1.4",
          "form": "근데 파인트 취급안하면 보통 뭐에 담아오세요",
          "original_form": "근데 파인트 취급안하면 보통 뭐에 담아오세요",
          "speaker_id": "1",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020584.1.5",
          "form": "패밀리..? 하프갤런..?",
          "original_form": "패밀리..? 하프갤런..?",
          "speaker_id": "2",
          "time": "20210828 10:08"
        },
        {
          "id": "MDRW2100020584.1.6",
          "form": "이야",
          "original_form": "이야",
          "speaker_id": "1",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020584.1.7",
          "form": "남다르다",
          "original_form": "남다르다",
          "speaker_id": "1",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020584.1.8",
          "form": "역시 가방끈이길어",
          "original_form": "역시 가방끈이길어",
          "speaker_id": "1",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020584.1.9",
          "form": "그래서 31데이에 가야한답니다",
          "original_form": "그래서 31데이에 가야한답니다",
          "speaker_id": "2",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020584.1.10",
          "form": "하프갤런통에 한가지맛만 담아오시는",
          "original_form": "하프갤런통에 한가지맛만 담아오시는",
          "speaker_id": "2",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020584.1.11",
          "form": "name1님도 만만치않아요",
          "original_form": "&name1&님도 만만치않아요",
          "speaker_id": "2",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020584.1.12",
          "form": "한수위",
          "original_form": "한수위",
          "speaker_id": "2",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020584.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020584.1.14",
          "form": "저 파인트도 한가지맛만담고",
          "original_form": "저 파인트도 한가지맛만담고",
          "speaker_id": "1",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020584.1.15",
          "form": "name1님은 또",
          "original_form": "&name1&님은 또",
          "speaker_id": "2",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020584.1.16",
          "form": "뭐좋아하셍",
          "original_form": "뭐좋아하셍",
          "speaker_id": "2",
          "time": "20210828 10:09"
        },
        {
          "id": "MDRW2100020584.1.17",
          "form": "사이즈가 뭐가됐던 한가지만담아요",
          "original_form": "사이즈가 뭐가됐던 한가지만담아요",
          "speaker_id": "1",
          "time": "20210828 10:10"
        },
        {
          "id": "MDRW2100020584.1.18",
          "form": "와.. 광기..",
          "original_form": "와.. 광기..",
          "speaker_id": "2",
          "time": "20210828 10:10"
        },
        {
          "id": "MDRW2100020584.1.19",
          "form": "저 그 요거트 좋아합니다",
          "original_form": "저 그 요거트 좋아합니다",
          "speaker_id": "1",
          "time": "20210828 10:10"
        },
        {
          "id": "MDRW2100020584.1.20",
          "form": "요거트를 아시나요",
          "original_form": "요거트를 아시나요",
          "speaker_id": "1",
          "time": "20210828 10:10"
        },
        {
          "id": "MDRW2100020584.1.21",
          "form": "그래서 31때 요거트 하프갤런채워왔습니다",
          "original_form": "그래서 31때 요거트 하프갤런채워왔습니다",
          "speaker_id": "1",
          "time": "20210828 10:10"
        },
        {
          "id": "MDRW2100020584.1.22",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210828 10:10"
        },
        {
          "id": "MDRW2100020584.1.23",
          "form": "투썸 요거트도 먹어보셨나요",
          "original_form": "투썸 요거트도 먹어보셨나요",
          "speaker_id": "2",
          "time": "20210828 10:10"
        },
        {
          "id": "MDRW2100020584.1.24",
          "form": "아니요 거기서 먹어볼 생각을 못했어요",
          "original_form": "아니요 거기서 먹어볼 생각을 못했어요",
          "speaker_id": "1",
          "time": "20210828 10:11"
        },
        {
          "id": "MDRW2100020584.1.25",
          "form": "거기가면 아이스박스만 먹어서ㅋㅋㄲㅋㅋㅋㅋㅋ",
          "original_form": "거기가면 아이스박스만 먹어서ㅋㅋㄲㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:11"
        },
        {
          "id": "MDRW2100020584.1.26",
          "form": "오호 저 아는 사람은 투썸가면 요거트만 먹길래",
          "original_form": "오호 저 아는 사람은 투썸가면 요거트만 먹길래",
