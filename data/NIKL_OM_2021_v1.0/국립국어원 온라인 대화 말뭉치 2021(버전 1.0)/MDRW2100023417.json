      "utterance": [
        {
          "id": "MDRW2100023417.1.1",
          "form": "하찮다니ㅋㅋ 말씀하시는 내용으루 이미지가 상상이 되네요",
          "original_form": "하찮다니ㅋㅋ 말씀하시는 내용으루 이미지가 상상이 되네요",
          "speaker_id": "2",
          "time": "20210830 01:53"
        },
        {
          "id": "MDRW2100023417.1.2",
          "form": "이빨이 없어서 콜덕한테 물려봤자",
          "original_form": "이빨이 없어서 콜덕한테 물려봤자",
          "speaker_id": "1",
          "time": "20210830 01:53"
        },
        {
          "id": "MDRW2100023417.1.3",
          "form": "빨래집게로 물리는 수준이라고 하더라구요",
          "original_form": "빨래집게로 물리는 수준이라고 하더라구요",
          "speaker_id": "1",
          "time": "20210830 01:53"
        },
        {
          "id": "MDRW2100023417.1.4",
          "form": "너무 하찮아요ㅋㅋㅋㅋ 그래도 공격하겠다고 무는게ㅋㅋㅋㅋ",
          "original_form": "너무 하찮아요ㅋㅋㅋㅋ 그래도 공격하겠다고 무는게ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:53"
        },
        {
          "id": "MDRW2100023417.1.5",
          "form": "와 그건 정말 귀여워서 웃음나오겠네요",
          "original_form": "와 그건 정말 귀여워서 웃음나오겠네요",
          "speaker_id": "2",
          "time": "20210830 01:54"
        },
        {
          "id": "MDRW2100023417.1.6",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:54"
        },
        {
          "id": "MDRW2100023417.1.7",
          "form": "유튜브에 오리키우시는분",
          "original_form": "유튜브에 오리키우시는분",
          "speaker_id": "1",
          "time": "20210830 01:54"
        },
        {
          "id": "MDRW2100023417.1.8",
          "form": "도시오리라고 있는데 추천드려용!",
          "original_form": "도시오리라고 있는데 추천드려용!",
          "speaker_id": "1",
          "time": "20210830 01:54"
        },
        {
          "id": "MDRW2100023417.1.9",
          "form": "말랑말랑 찹쌀떡 두마리 키우셔요",
          "original_form": "말랑말랑 찹쌀떡 두마리 키우셔요",
          "speaker_id": "1",
          "time": "20210830 01:54"
        },
        {
          "id": "MDRW2100023417.1.10",
          "form": "오리들은 근데",
          "original_form": "오리들은 근데",
          "speaker_id": "2",
          "time": "20210830 01:54"
        },
        {
          "id": "MDRW2100023417.1.11",
          "form": "물있는곳을",
          "original_form": "물있는곳을",
          "speaker_id": "2",
          "time": "20210830 01:54"
        },
        {
          "id": "MDRW2100023417.1.12",
          "form": "좋아하지않아요?",
          "original_form": "좋아하지않아요?",
          "speaker_id": "2",
          "time": "20210830 01:54"
        },
        {
          "id": "MDRW2100023417.1.13",
          "form": "넹넹 물 받아두면 알아서 들어가용",
          "original_form": "넹넹 물 받아두면 알아서 들어가용",
          "speaker_id": "1",
          "time": "20210830 01:54"
        },
        {
          "id": "MDRW2100023417.1.14",
          "form": "알아서 들어갔다 나와서 몸 말리고ㅋㅋㅋㅋㅋ",
          "original_form": "알아서 들어갔다 나와서 몸 말리고ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:54"
        },
        {
          "id": "MDRW2100023417.1.15",
          "form": "따로 목욕 안시켜도 됩니당",
          "original_form": "따로 목욕 안시켜도 됩니당",
          "speaker_id": "1",
          "time": "20210830 01:54"
        },
        {
          "id": "MDRW2100023417.1.16",
          "form": "아하 반려동물 키우는건 진짜루ㅠㅠ 돈과노력이 많이들어서",
          "original_form": "아하 반려동물 키우는건 진짜루ㅠㅠ 돈과노력이 많이들어서",
          "speaker_id": "2",
          "time": "20210830 01:55"
        },
        {
          "id": "MDRW2100023417.1.17",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 01:55"
        },
        {
          "id": "MDRW2100023417.1.18",
          "form": "오 그건 편하네요",
          "original_form": "오 그건 편하네요",
          "speaker_id": "2",
          "time": "20210830 01:55"
        },
        {
          "id": "MDRW2100023417.1.19",
          "form": "무엇보다 책임감도 중요해요",
          "original_form": "무엇보다 책임감도 중요해요",
          "speaker_id": "1",
          "time": "20210830 01:55"
        },
        {
          "id": "MDRW2100023417.1.20",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "2",
          "time": "20210830 01:55"
        },
        {
          "id": "MDRW2100023417.1.21",
          "form": "산책은",
          "original_form": "산책은",
          "speaker_id": "2",
          "time": "20210830 01:55"
        },
        {
          "id": "MDRW2100023417.1.22",
          "form": "안해도되는거에요?",
          "original_form": "안해도되는거에요?",
          "speaker_id": "2",
          "time": "20210830 01:55"
        },
        {
          "id": "MDRW2100023417.1.23",
          "form": "진짜 많이 공부하고 준비도 됐을때 키워야하는것같아용",
          "original_form": "진짜 많이 공부하고 준비도 됐을때 키워야하는것같아용",
          "speaker_id": "1",
          "time": "20210830 01:55"
        },
        {
          "id": "MDRW2100023417.1.24",
          "form": "넹넹!",
          "original_form": "넹넹!",
          "speaker_id": "1",
          "time": "20210830 01:55"
        },
        {
          "id": "MDRW2100023417.1.25",
          "form": "따로 안시켜도 되어용ㅎㅎㅎ",
          "original_form": "따로 안시켜도 되어용ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:55"
        },
        {
          "id": "MDRW2100023417.1.26",
          "form": "많이 걷는 동물이 아니라서ㅎㅎ",
          "original_form": "많이 걷는 동물이 아니라서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:55"
        },
        {
          "id": "MDRW2100023417.1.27",
          "form": "산책안하는건 최고네요ㅋㅋㅋㅋ",
          "original_form": "산책안하는건 최고네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 01:55"
        },
        {
          "id": "MDRW2100023417.1.28",
          "form": "최고져!ㅋㅋㅋ",
          "original_form": "최고져!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:55"
        },
        {
          "id": "MDRW2100023417.1.29",
          "form": "전 너무 힘들었어요..",
          "original_form": "전 너무 힘들었어요..",
          "speaker_id": "2",
          "time": "20210830 01:55"
        },
        {
          "id": "MDRW2100023417.1.30",
          "form": "강아지 산책..",
          "original_form": "강아지 산책..",
          "speaker_id": "2",
          "time": "20210830 01:56"
        },
        {
          "id": "MDRW2100023417.1.31",
          "form": "아이구ㅠㅠ 많이 힘드셨나봐요ㅠㅠㅠ",
          "original_form": "아이구ㅠㅠ 많이 힘드셨나봐요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 01:56"
        },
        {
          "id": "MDRW2100023417.1.32",
          "form": "사실 많이 번거롭기도 하죠",
          "original_form": "사실 많이 번거롭기도 하죠",
          "speaker_id": "1",
          "time": "20210830 01:56"
        },
        {
          "id": "MDRW2100023417.1.33",
          "form": "ㅠㅠㅋㅋㅋㅋ움직이는걸 싫어해서",
          "original_form": "ㅠㅠㅋㅋㅋㅋ움직이는걸 싫어해서",
          "speaker_id": "2",
          "time": "20210830 01:56"
        },
        {
          "id": "MDRW2100023417.1.34",
          "form": "저도 집순이라",
          "original_form": "저도 집순이라",
          "speaker_id": "1",
          "time": "20210830 01:56"
        },
        {
          "id": "MDRW2100023417.1.35",
          "form": "키우게 된다면 거북이 달팽이 오리 이런쪽으로....",
          "original_form": "키우게 된다면 거북이 달팽이 오리 이런쪽으로....",
          "speaker_id": "1",
          "time": "20210830 01:56"
        }
      ]
    }
