        {
          "id": "MDRW2100038841.1.1",
          "form": "건강관리하세요?",
          "original_form": "건강관리하세요?",
          "speaker_id": "1",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100038841.1.2",
          "form": "해야는데ㅜㅜ",
          "original_form": "해야는데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100038841.1.3",
          "form": "못하고 사네요",
          "original_form": "못하고 사네요",
          "speaker_id": "2",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100038841.1.4",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100038841.1.5",
          "form": "늘 피곤해  절어 사는데",
          "original_form": "늘 피곤해  절어 사는데",
          "speaker_id": "1",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100038841.1.6",
          "form": "약빨로",
          "original_form": "약빨로",
          "speaker_id": "2",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100038841.1.7",
          "form": "버텨야한다는 생각에",
          "original_form": "버텨야한다는 생각에",
          "speaker_id": "2",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100038841.1.8",
          "form": "영양제 드시나 봐요",
          "original_form": "영양제 드시나 봐요",
          "speaker_id": "1",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100038841.1.9",
          "form": "영양제 몇알은",
          "original_form": "영양제 몇알은",
          "speaker_id": "2",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100038841.1.10",
          "form": "먹는편이에요",
          "original_form": "먹는편이에요",
          "speaker_id": "2",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100038841.1.11",
          "form": "너도 챙겨 먹으려고 노력은 하는데",
          "original_form": "너도 챙겨 먹으려고 노력은 하는데",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100038841.1.12",
          "form": "자주 까먹어요",
          "original_form": "자주 까먹어요",
          "speaker_id": "1",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100038841.1.13",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100038841.1.14",
          "form": "운동도 해야하는데",
          "original_form": "운동도 해야하는데",
          "speaker_id": "2",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100038841.1.15",
          "form": "운동은 늘 내일부터죠ㅠ",
          "original_form": "운동은 늘 내일부터죠ㅠ",
          "speaker_id": "1",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100038841.1.16",
          "form": "명언이시네요",
          "original_form": "명언이시네요",
          "speaker_id": "2",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100038841.1.17",
          "form": "맞아요 늘 내일부터",
          "original_form": "맞아요 늘 내일부터",
          "speaker_id": "2",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100038841.1.18",
          "form": "아직은 시간 없다는 핑계가,,,",
          "original_form": "아직은 시간 없다는 핑계가,,,",
          "speaker_id": "1",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100038841.1.19",
          "form": "생존을 위한 운동이 필요해지면",
          "original_form": "생존을 위한 운동이 필요해지면",
          "speaker_id": "2",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100038841.1.20",
          "form": "그때나 움직일듯해요",
          "original_form": "그때나 움직일듯해요",
          "speaker_id": "2",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100038841.1.21",
          "form": "저랑 똑같으시네요ㅋㅋㅋ",
          "original_form": "저랑 똑같으시네요ㅋㅋㅋ",
