      "utterance": [
        {
          "id": "MDRW2100014859.1.1",
          "form": "지금 연애중이신가용 ㅎㅎ",
          "original_form": "지금 연애중이신가용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014859.1.2",
          "form": "네ㅎㅎ 연애하세요?",
          "original_form": "네ㅎㅎ 연애하세요?",
          "speaker_id": "2",
          "time": "20210825 20:58"
        },
        {
          "id": "MDRW2100014859.1.3",
          "form": "오오 저는 아니요 ㅎㅎ",
          "original_form": "오오 저는 아니요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014859.1.4",
          "form": "얼마나 되셨어요 ?!",
          "original_form": "얼마나 되셨어요 ?!",
          "speaker_id": "1",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014859.1.5",
          "form": "저는 한 3년이요ㅎㅎ",
          "original_form": "저는 한 3년이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014859.1.6",
          "form": "헐 엄청 오래 만나셨네요 !",
          "original_form": "헐 엄청 오래 만나셨네요 !",
          "speaker_id": "1",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100014859.1.7",
          "form": "그쵸 근데 그냥 시간이 흘렀어요ㅎㅎ",
          "original_form": "그쵸 근데 그냥 시간이 흘렀어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100014859.1.8",
          "form": "어떻게 만나게 되셨어요 ??",
          "original_form": "어떻게 만나게 되셨어요 ??",
          "speaker_id": "1",
          "time": "20210825 21:02"
        },
        {
          "id": "MDRW2100014859.1.9",
          "form": "원래 그냥 대학동기였어요ㅎㅎ",
          "original_form": "원래 그냥 대학동기였어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014859.1.10",
          "form": "동갑이에요 ??!",
          "original_form": "동갑이에요 ??!",
          "speaker_id": "1",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014859.1.11",
          "form": "네 동갑이에요 ㅎㅎ",
          "original_form": "네 동갑이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014859.1.12",
          "form": "와 근데 동기면",
          "original_form": "와 근데 동기면",
          "speaker_id": "1",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014859.1.13",
          "form": "군대 기다려주신거에요 ??",
          "original_form": "군대 기다려주신거에요 ??",
          "speaker_id": "1",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014859.1.14",
          "form": "아뇨..ㅎㅎㅎ 사귄건 졸업후에요ㅋㅋ",
          "original_form": "아뇨..ㅎㅎㅎ 사귄건 졸업후에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014859.1.15",
          "form": "아아 ㅎㅎ 헐 그게 더 신기해요 !",
          "original_form": "아아 ㅎㅎ 헐 그게 더 신기해요 !",
          "speaker_id": "1",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014859.1.16",
          "form": "보통 학교다닐때 만나는데 졸업후라니 ㅎㅎ",
          "original_form": "보통 학교다닐때 만나는데 졸업후라니 ㅎㅎ",
