        },
        {
          "id": "MDRW2100020155.1.4",
          "form": "완벽해지죠",
          "original_form": "완벽해지죠",
          "speaker_id": "1",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020155.1.5",
          "form": "그럼 비비고 vs 얇은피",
          "original_form": "그럼 비비고 vs 얇은피",
