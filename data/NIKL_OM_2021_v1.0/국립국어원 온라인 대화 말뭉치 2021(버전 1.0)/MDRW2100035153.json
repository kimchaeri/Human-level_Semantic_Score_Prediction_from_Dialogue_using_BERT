        },
        {
          "id": "MDRW2100035153.1.2",
          "form": "점심 드셨나용?",
          "original_form": "점심 드셨나용?",
          "speaker_id": "2",
          "time": "20210829 14:15"
        },
        {
          "id": "MDRW2100035153.1.3",
          "form": "저 신라면 볶음면 먹었어여ㅎㅎㅎ",
          "original_form": "저 신라면 볶음면 먹었어여ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 15:19"
        },
        {
          "id": "MDRW2100035153.1.4",
          "form": "오 그거 어때용?",
          "original_form": "오 그거 어때용?",
          "speaker_id": "2",
          "time": "20210829 19:16"
        },
        {
          "id": "MDRW2100035153.1.5",
          "form": "궁금했는데",
          "original_form": "궁금했는데",
          "speaker_id": "2",
          "time": "20210829 19:16"
        },
        {
          "id": "MDRW2100035153.1.6",
          "form": "맵싹하니 매력있네요ㅋㅋㅋㅋ",
          "original_form": "맵싹하니 매력있네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:33"
        },
        {
          "id": "MDRW2100035153.1.7",
          "form": "ㅋㄱㅋㅋㄱ담에 먹어봐야겠다",
          "original_form": "ㅋㄱㅋㅋㄱ담에 먹어봐야겠다",
          "speaker_id": "2",
          "time": "20210829 23:50"
        },
        {
          "id": "MDRW2100035153.1.8",
          "form": "광고볼때 맛있어보였는데 역시 맛있군요",
          "original_form": "광고볼때 맛있어보였는데 역시 맛있군요",
          "speaker_id": "2",
          "time": "20210829 23:50"
        },
        {
          "id": "MDRW2100035153.1.9",
          "form": "좋아하는 라면 있으세요?",
          "original_form": "좋아하는 라면 있으세요?",
          "speaker_id": "2",
          "time": "20210829 23:50"
        },
        {
          "id": "MDRW2100035153.1.10",
          "form": "불닭볶음면 시리즈 좋아해요",
          "original_form": "불닭볶음면 시리즈 좋아해요",
          "speaker_id": "1",
          "time": "20210829 23:55"
        },
        {
          "id": "MDRW2100035153.1.11",
          "form": "제 내장건강을 위해 자주는 못먹지만",
          "original_form": "제 내장건강을 위해 자주는 못먹지만",
          "speaker_id": "1",
          "time": "20210829 23:55"
        },
        {
          "id": "MDRW2100035153.1.12",
          "form": "ㅋㅋㅋㅋㅋ핳 맞아요 내장건강ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ핳 맞아요 내장건강ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 23:56"
        },
        {
          "id": "MDRW2100035153.1.13",
          "form": "저도 매운거좋아하는데 꼭 한번씩 속아파서",
          "original_form": "저도 매운거좋아하는데 꼭 한번씩 속아파서",
          "speaker_id": "2",
          "time": "20210829 23:56"
        },
        {
          "id": "MDRW2100035153.1.14",
          "form": "요즘은 조심해요ㅋㄱㅋ",
          "original_form": "요즘은 조심해요ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210829 23:56"
        },
        {
          "id": "MDRW2100035153.1.15",
          "form": "그닠가요 위내시경 하러갈 일 만들면 안되겠다 싶어서 ㅋㅋ큐ㅠ",
          "original_form": "그닠가요 위내시경 하러갈 일 만들면 안되겠다 싶어서 ㅋㅋ큐ㅠ",
          "speaker_id": "1",
          "time": "20210829 23:57"
        },
        {
          "id": "MDRW2100035153.1.16",
          "form": "ㅋㅋㅋㄱㅋㅋ맞앙23",
          "original_form": "ㅋㅋㅋㄱㅋㅋ맞앙23",
          "speaker_id": "2",
          "time": "20210829 23:59"
        },
        {
          "id": "MDRW2100035153.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210829 23:59"
        },
        {
          "id": "MDRW2100035153.1.18",
          "form": "제발 입과 위가 소통해서 해결해줬으면...^^",
          "original_form": "제발 입과 위가 소통해서 해결해줬으면...^^",
          "speaker_id": "2",
          "time": "20210829 23:59"
        },
        {
          "id": "MDRW2100035153.1.19",
          "form": "엽떡도 좋아하세요?",
          "original_form": "엽떡도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 23:59"
        },
        {
          "id": "MDRW2100035153.1.20",
          "form": "앜ㅋㅋㅋ네 가끔 먹는데 진짜 맵긴 하더라구여 근데 ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ네 가끔 먹는데 진짜 맵긴 하더라구여 근데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:07"
        },
        {
          "id": "MDRW2100035153.1.21",
          "form": "맞아요  전 이제 착한맛만 먹어요..ㅎ",
          "original_form": "맞아요  전 이제 착한맛만 먹어요..ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:37"
        },
        {
          "id": "MDRW2100035153.1.22",
          "form": "매운게 맛있고 스트레스도 풀리긴 하는데ㅜㅠ",
          "original_form": "매운게 맛있고 스트레스도 풀리긴 하는데ㅜㅠ",
          "speaker_id": "2",
          "time": "20210830 00:38"
        },
        {
          "id": "MDRW2100035153.1.23",
          "form": "저두.. 이제 자극적인거 말고 슴슴한 맛..",
          "original_form": "저두.. 이제 자극적인거 말고 슴슴한 맛..",
          "speaker_id": "1",
          "time": "20210830 00:44"
        },
        {
          "id": "MDRW2100035153.1.24",
          "form": "ㅋㅋㅋㅋㅋ그런맛 뭐가있져",
          "original_form": "ㅋㅋㅋㅋㅋ그런맛 뭐가있져",
          "speaker_id": "2",
          "time": "20210830 00:44"
        },
        {
          "id": "MDRW2100035153.1.25",
