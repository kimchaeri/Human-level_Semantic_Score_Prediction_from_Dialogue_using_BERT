          "time": "20210823 01:44"
        },
        {
          "id": "MDRW2100009610.1.11",
          "form": "빼는거 없어",
          "original_form": "빼는거 없어",
          "speaker_id": "2",
          "time": "20210823 01:46"
        },
        {
          "id": "MDRW2100009610.1.12",
          "form": "너는 뭐 빼?",
          "original_form": "너는 뭐 빼?",
          "speaker_id": "2",
          "time": "20210823 01:46"
        },
        {
          "id": "MDRW2100009610.1.13",
