      "utterance": [
        {
          "id": "MDRW2100026015.1.1",
          "form": "혹시 싱어게인 보셨어요?",
          "original_form": "혹시 싱어게인 보셨어요?",
          "speaker_id": "1",
          "time": "20210901 18:51"
        },
        {
          "id": "MDRW2100026015.1.2",
          "form": "내 저도 싱어게인 너무 좋아해요",
          "original_form": "내 저도 싱어게인 너무 좋아해요",
          "speaker_id": "2",
          "time": "20210901 18:51"
        },
        {
          "id": "MDRW2100026015.1.3",
          "form": "누구 좋아하셨어요 저는 이승윤이요.우승자",
          "original_form": "누구 좋아하셨어요 저는 이승윤이요.우승자",
          "speaker_id": "1",
          "time": "20210901 18:51"
        },
        {
          "id": "MDRW2100026015.1.4",
          "form": "아 저도 처음부터 저는 이승연 좋아했어요",
          "original_form": "아 저도 처음부터 저는 이승연 좋아했어요",
          "speaker_id": "2",
          "time": "20210901 18:52"
        },
        {
          "id": "MDRW2100026015.1.5",
          "form": "그 락커 아저씨도 좋구 ㅋㅋ",
          "original_form": "그 락커 아저씨도 좋구 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:52"
        },
        {
          "id": "MDRW2100026015.1.6",
          "form": "ㅎㅎㅎ 저두요",
          "original_form": "ㅎㅎㅎ 저두요",
          "speaker_id": "2",
          "time": "20210901 18:52"
        },
        {
          "id": "MDRW2100026015.1.7",
          "form": "이름이 정홍일인가? 그분",
          "original_form": "이름이 정홍일인가? 그분",
          "speaker_id": "1",
          "time": "20210901 18:53"
        },
        {
          "id": "MDRW2100026015.1.8",
          "form": "맞을 거에요 저도 그 분 진짜 좋더라구요",
          "original_form": "맞을 거에요 저도 그 분 진짜 좋더라구요",
          "speaker_id": "2",
          "time": "20210901 18:54"
        },
        {
          "id": "MDRW2100026015.1.9",
          "form": "이무진은 처음에 진짜 좋았는데",
          "original_form": "이무진은 처음에 진짜 좋았는데",
          "speaker_id": "1",
          "time": "20210901 18:54"
        },
        {
          "id": "MDRW2100026015.1.10",
          "form": "맨 처음이 제일 좋았던듯",
          "original_form": "맨 처음이 제일 좋았던듯",
          "speaker_id": "1",
          "time": "20210901 18:54"
        },
        {
          "id": "MDRW2100026015.1.11",
          "form": "저는 이무진은 조금 아쉽더라구요",
          "original_form": "저는 이무진은 조금 아쉽더라구요",
          "speaker_id": "2",
          "time": "20210901 18:54"
        },
        {
          "id": "MDRW2100026015.1.12",
          "form": "이번에 이무진 노래 신호등은 좋던데 중독성이 있더라고요",
          "original_form": "이번에 이무진 노래 신호등은 좋던데 중독성이 있더라고요",
          "speaker_id": "1",
          "time": "20210901 18:56"
        },
        {
          "id": "MDRW2100026015.1.13",
          "form": "왜 저도 노래 좋더라구요 근데 이승윤 은 조금 아쉬운 노래를...",
          "original_form": "왜 저도 노래 좋더라구요 근데 이승윤 은 조금 아쉬운 노래를...",
          "speaker_id": "2",
          "time": "20210901 18:56"
        },
        {
          "id": "MDRW2100026015.1.14",
          "form": "이승윤도 좋은 노래 뽑겠죠 기대중",
          "original_form": "이승윤도 좋은 노래 뽑겠죠 기대중",
          "speaker_id": "1",
          "time": "20210901 18:57"
        },
        {
          "id": "MDRW2100026015.1.15",
          "form": "싱어게인 초반에 이승민이 보여줬던 노래들을 다시 발매해도 좋을 것 같은데 아니 그 정도만 뽑아줘도 좋을 것 같아요",
          "original_form": "싱어게인 초반에 이승민이 보여줬던 노래들을 다시 발매해도 좋을 것 같은데 아니 그 정도만 뽑아줘도 좋을 것 같아요",
          "speaker_id": "2",
          "time": "20210901 18:59"
        },
        {
          "id": "MDRW2100026015.1.16",
          "form": "그러게요 자작곡 욕심이 있어서 그런가",
          "original_form": "그러게요 자작곡 욕심이 있어서 그런가",
          "speaker_id": "1",
          "time": "20210901 19:00"
        },
        {
          "id": "MDRW2100026015.1.17",
          "form": "아무튼 정홍 일인가 그 참여하셨던 분도 그렇고 뭔가 활동을 꾸준히 해주었으면 좋겠어요",
          "original_form": "아무튼 정홍 일인가 그 참여하셨던 분도 그렇고 뭔가 활동을 꾸준히 해주었으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210901 19:00"
        }
      ]
