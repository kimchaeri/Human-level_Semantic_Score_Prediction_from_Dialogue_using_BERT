      "utterance": [
        {
          "id": "MDRW2100046308.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211019 13:16"
        },
        {
          "id": "MDRW2100046308.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211019 13:44"
        },
        {
          "id": "MDRW2100046308.1.3",
          "form": "혹시 스우파 보시나요 ㅋㅋㅋ",
          "original_form": "혹시 스우파 보시나요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211019 13:44"
        },
        {
          "id": "MDRW2100046308.1.4",
          "form": "스우파가 뭔가요???",
          "original_form": "스우파가 뭔가요???",
          "speaker_id": "1",
          "time": "20211019 15:20"
        },
        {
          "id": "MDRW2100046308.1.5",
          "form": "스트리트우먼파이터요!",
          "original_form": "스트리트우먼파이터요!",
          "speaker_id": "2",
          "time": "20211019 15:21"
        },
        {
          "id": "MDRW2100046308.1.6",
          "form": "엠넷에서 여성댄스팀들 나와서 미션하고 대결하는..!",
          "original_form": "엠넷에서 여성댄스팀들 나와서 미션하고 대결하는..!",
          "speaker_id": "2",
          "time": "20211019 15:22"
        },
        {
          "id": "MDRW2100046308.1.7",
          "form": "저 잘 모르는거네요... 죄송해요",
          "original_form": "저 잘 모르는거네요... 죄송해요",
          "speaker_id": "1",
          "time": "20211019 15:22"
        },
        {
          "id": "MDRW2100046308.1.8",
          "form": "죄송할건아닌데 ㅎㅎ ㅠㅠ",
          "original_form": "죄송할건아닌데 ㅎㅎ ㅠㅠ",
          "speaker_id": "2",
          "time": "20211019 15:22"
        },
        {
          "id": "MDRW2100046308.1.9",
          "form": "심심하실 때",
          "original_form": "심심하실 때",
          "speaker_id": "2",
          "time": "20211019 15:22"
        },
        {
          "id": "MDRW2100046308.1.10",
          "form": "유튜브에 한 번 검색해서 봐보세요 ㅎㅎ 묘하게",
          "original_form": "유튜브에 한 번 검색해서 봐보세요 ㅎㅎ 묘하게",
          "speaker_id": "2",
          "time": "20211019 15:23"
        },
        {
          "id": "MDRW2100046308.1.11",
          "form": "계속 보게되는...",
          "original_form": "계속 보게되는...",
          "speaker_id": "2",
          "time": "20211019 15:23"
        },
        {
          "id": "MDRW2100046308.1.12",
          "form": "그런게 있더라구요 ㅋㅋㅋ",
          "original_form": "그런게 있더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211019 15:23"
        },
        {
          "id": "MDRW2100046308.1.13",
          "form": "댄스를 별로 안 좋아해요",
          "original_form": "댄스를 별로 안 좋아해요",
          "speaker_id": "1",
          "time": "20211019 15:23"
        },
        {
          "id": "MDRW2100046308.1.14",
          "form": "아하 그러시구나 ㅠㅠ",
          "original_form": "아하 그러시구나 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211019 15:24"
        },
        {
          "id": "MDRW2100046308.1.15",
          "form": "쇼미더머니는 안보세요?",
          "original_form": "쇼미더머니는 안보세요?",
          "speaker_id": "2",
          "time": "20211019 15:24"
        },
        {
          "id": "MDRW2100046308.1.16",
          "form": "그건 랩이죠?",
          "original_form": "그건 랩이죠?",
          "speaker_id": "1",
          "time": "20211019 15:24"
        },
        {
          "id": "MDRW2100046308.1.17",
          "form": "네! 래퍼들 나오는!",
          "original_form": "네! 래퍼들 나오는!",
          "speaker_id": "2",
          "time": "20211019 15:24"
        },
        {
          "id": "MDRW2100046308.1.18",
          "form": "저 아재라서 그런 요즘 방송 잘 안봐요",
          "original_form": "저 아재라서 그런 요즘 방송 잘 안봐요",
          "speaker_id": "1",
          "time": "20211019 15:25"
        },
        {
          "id": "MDRW2100046308.1.19",
          "form": "전 놀면뭐하니 봅니다 ㅋㅋ",
          "original_form": "전 놀면뭐하니 봅니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211019 15:25"
        },
        {
          "id": "MDRW2100046308.1.20",
          "form": "놀면뭐하니 보시는데 아재라니!",
          "original_form": "놀면뭐하니 보시는데 아재라니!",
          "speaker_id": "2",
          "time": "20211019 15:32"
        },
        {
          "id": "MDRW2100046308.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211019 15:32"
        },
        {
          "id": "MDRW2100046308.1.22",
          "form": "저도 놀면뭐하니 가끔 보는데 재밌어요 ㅎㅎㅎ",
          "original_form": "저도 놀면뭐하니 가끔 보는데 재밌어요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211019 15:32"
        },
        {
          "id": "MDRW2100046308.1.23",
          "form": "전 미주 상큼해서.... 삼촌팬 됐어요",
          "original_form": "전 미주 상큼해서.... 삼촌팬 됐어요",
          "speaker_id": "1",
          "time": "20211019 15:32"
        },
        {
          "id": "MDRW2100046308.1.24",
          "form": "미주 너무 열심히하고 이쁘고..",
          "original_form": "미주 너무 열심히하고 이쁘고..",
          "speaker_id": "2",
          "time": "20211019 15:32"
        },
        {
          "id": "MDRW2100046308.1.25",
          "form": "완전 호감인 연예인 ㅎㅎㅎㅎ",
          "original_form": "완전 호감인 연예인 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211019 15:32"
        },
        {
          "id": "MDRW2100046308.1.26",
          "form": "잘됐으면 좋겠어요 더더!",
          "original_form": "잘됐으면 좋겠어요 더더!",
          "speaker_id": "2",
          "time": "20211019 15:33"
        }
      ]
    }
