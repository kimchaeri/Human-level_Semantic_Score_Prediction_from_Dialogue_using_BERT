          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 00:54"
        },
        {
          "id": "MDRW2100012762.1.38",
          "form": "먼지 쌓이지 않게",
          "original_form": "먼지 쌓이지 않게",
          "speaker_id": "2",
          "time": "20210825 00:54"
        },
        {
          "id": "MDRW2100012762.1.39",
          "form": "그건 동의",
          "original_form": "그건 동의",
          "speaker_id": "1",
          "time": "20210825 00:54"
        },
        {
          "id": "MDRW2100012762.1.40",
          "form": "먼지 싫어요",
          "original_form": "먼지 싫어요",
          "speaker_id": "1",
          "time": "20210825 00:55"
        },
        {
          "id": "MDRW2100012762.1.41",
          "form": "음 저는",
          "original_form": "음 저는",
          "speaker_id": "1",
          "time": "20210825 00:55"
        },
        {
          "id": "MDRW2100012762.1.42",
          "form": "청소랑 빨래는 평소에 하고",
