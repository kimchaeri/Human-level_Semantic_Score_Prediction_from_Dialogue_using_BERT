      "utterance": [
        {
          "id": "MDRW2100012762.1.1",
          "form": "이게 그나마 비슷해보여요",
          "original_form": "이게 그나마 비슷해보여요",
          "speaker_id": "1",
          "time": "20210825 00:50"
        },
        {
          "id": "MDRW2100012762.1.2",
          "form": "기숙사에서 집 가는데 짐 쌀 게 있어요?",
          "original_form": "기숙사에서 집 가는데 짐 쌀 게 있어요?",
          "speaker_id": "1",
          "time": "20210825 00:50"
        },
        {
          "id": "MDRW2100012762.1.3",
          "form": "뭐지?",
          "original_form": "뭐지?",
          "speaker_id": "1",
          "time": "20210825 00:50"
        },
        {
          "id": "MDRW2100012762.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:51"
        },
        {
          "id": "MDRW2100012762.1.5",
          "form": "옷 칫솔 로션 펜 노트",
          "original_form": "옷 칫솔 로션 펜 노트",
          "speaker_id": "1",
          "time": "20210825 00:51"
        },
        {
          "id": "MDRW2100012762.1.6",
          "form": "이어폰?",
          "original_form": "이어폰?",
          "speaker_id": "1",
          "time": "20210825 00:51"
        },
        {
          "id": "MDRW2100012762.1.7",
          "form": "나 자신?",
          "original_form": "나 자신?",
          "speaker_id": "1",
          "time": "20210825 00:51"
        },
        {
          "id": "MDRW2100012762.1.8",
          "form": "일단 전 캐리어가 있어요",
          "original_form": "일단 전 캐리어가 있어요",
          "speaker_id": "2",
          "time": "20210825 00:51"
        },
        {
          "id": "MDRW2100012762.1.9",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210825 00:51"
        },
        {
          "id": "MDRW2100012762.1.10",
          "form": "캐리어에 일단 옷들을 개서 집어 넣죠",
          "original_form": "캐리어에 일단 옷들을 개서 집어 넣죠",
          "speaker_id": "2",
          "time": "20210825 00:51"
        },
        {
          "id": "MDRW2100012762.1.11",
          "form": "기숙사에 한번 가면 집에 잘 안 가는군요",
          "original_form": "기숙사에 한번 가면 집에 잘 안 가는군요",
          "speaker_id": "1",
          "time": "20210825 00:51"
        },
        {
          "id": "MDRW2100012762.1.12",
          "form": "나잖아?",
          "original_form": "나잖아?",
          "speaker_id": "1",
          "time": "20210825 00:51"
        },
        {
          "id": "MDRW2100012762.1.13",
          "form": "그리고요?",
          "original_form": "그리고요?",
          "speaker_id": "1",
          "time": "20210825 00:52"
        },
        {
          "id": "MDRW2100012762.1.14",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "2",
          "time": "20210825 00:52"
        },
        {
          "id": "MDRW2100012762.1.15",
          "form": "상자들 몇개를 수집해서",
          "original_form": "상자들 몇개를 수집해서",
          "speaker_id": "2",
          "time": "20210825 00:52"
        },
        {
          "id": "MDRW2100012762.1.16",
          "form": "아 한번 자취방에 가면 집에 잘 안 와서",
          "original_form": "아 한번 자취방에 가면 집에 잘 안 와서",
          "speaker_id": "1",
          "time": "20210825 00:52"
        },
        {
          "id": "MDRW2100012762.1.17",
          "form": "상자요?",
          "original_form": "상자요?",
          "speaker_id": "1",
          "time": "20210825 00:52"
        },
        {
          "id": "MDRW2100012762.1.18",
          "form": "스탠드, 그리고 책들을 정리해서 집어 넣어요",
          "original_form": "스탠드, 그리고 책들을 정리해서 집어 넣어요",
          "speaker_id": "2",
          "time": "20210825 00:52"
        },
        {
          "id": "MDRW2100012762.1.19",
          "form": "빨래망에 세탁 해야할 옷들을 넣고",
          "original_form": "빨래망에 세탁 해야할 옷들을 넣고",
          "speaker_id": "2",
          "time": "20210825 00:53"
        },
        {
          "id": "MDRW2100012762.1.20",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210825 00:53"
        },
        {
          "id": "MDRW2100012762.1.21",
          "form": "방청소를 하면서",
          "original_form": "방청소를 하면서",
          "speaker_id": "2",
          "time": "20210825 00:53"
        },
        {
          "id": "MDRW2100012762.1.22",
          "form": "방금 그 문장에서",
          "original_form": "방금 그 문장에서",
          "speaker_id": "1",
          "time": "20210825 00:53"
        },
        {
          "id": "MDRW2100012762.1.23",
          "form": "새로운 세계를 봤어요",
          "original_form": "새로운 세계를 봤어요",
          "speaker_id": "1",
          "time": "20210825 00:53"
        },
        {
          "id": "MDRW2100012762.1.24",
          "form": "네..",
          "original_form": "네..",
