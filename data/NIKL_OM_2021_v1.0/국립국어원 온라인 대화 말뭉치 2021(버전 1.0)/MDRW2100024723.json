      "utterance": [
        {
          "id": "MDRW2100024723.1.1",
          "form": "제주도 한라산코스도",
          "original_form": "제주도 한라산코스도",
          "speaker_id": "2",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024723.1.2",
          "form": "재밌어보이더라",
          "original_form": "재밌어보이더라",
          "speaker_id": "2",
          "time": "20210831 17:32"
        },
        {
          "id": "MDRW2100024723.1.3",
          "form": "네가 말하는 구경하기 위해 걷는 여행?",
          "original_form": "네가 말하는 구경하기 위해 걷는 여행?",
          "speaker_id": "1",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024723.1.4",
          "form": "살짝  걸어보고싶어  꽃폈을때",
          "original_form": "살짝  걸어보고싶어  꽃폈을때",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024723.1.5",
          "form": "등산은 괜찮니?",
          "original_form": "등산은 괜찮니?",
          "speaker_id": "1",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024723.1.6",
          "form": "응 맞아",
          "original_form": "응 맞아",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024723.1.7",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024723.1.8",
          "form": "ㄷㅈ등산은 무리고 산책길만 걸을래",
          "original_form": "ㄷㅈ등산은 무리고 산책길만 걸을래",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024723.1.9",
          "form": "제주도 한달살기노",
          "original_form": "제주도 한달살기노",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024723.1.10",
          "form": "ㅋㅋㅋㅋㅋ좋다 말 그대로 꽃길 걷는 여행이네",
          "original_form": "ㅋㅋㅋㅋㅋ좋다 말 그대로 꽃길 걷는 여행이네",
          "speaker_id": "1",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024723.1.11",
          "form": "많이하는데",
          "original_form": "많이하는데",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024723.1.12",
          "form": "넌 어때",
          "original_form": "넌 어때",
          "speaker_id": "2",
          "time": "20210831 17:33"
        },
        {
          "id": "MDRW2100024723.1.13",
          "form": "태풍 안 오는 시즌이면 괜찮을 것 같기도 한데",
          "original_form": "태풍 안 오는 시즌이면 괜찮을 것 같기도 한데",
          "speaker_id": "1",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100024723.1.14",
          "form": "날씨 난리나는 계절이면",
          "original_form": "날씨 난리나는 계절이면",
          "speaker_id": "1",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100024723.1.15",
          "form": "고민 좀 해볼 것 같아ㅋㅋㅋ",
          "original_form": "고민 좀 해볼 것 같아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100024723.1.16",
          "form": "여름이나 겨울은",
          "original_form": "여름이나 겨울은",
          "speaker_id": "2",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100024723.1.17",
          "form": "피해야겠다",
          "original_form": "피해야겠다",
          "speaker_id": "2",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100024723.1.18",
          "form": "봄에 꽃필때도 좋겠다",
          "original_form": "봄에 꽃필때도 좋겠다",
          "speaker_id": "2",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100024723.1.19",
          "form": "봄 가을시즌이 좋겠다",
          "original_form": "봄 가을시즌이 좋겠다",
          "speaker_id": "1",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100024723.1.20",
          "form": "그러게 꽃길 걸어야제",
          "original_form": "그러게 꽃길 걸어야제",
          "speaker_id": "1",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100024723.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100024723.1.22",
          "form": "근데 제주도 수학여행은",
          "original_form": "근데 제주도 수학여행은",
          "speaker_id": "1",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100024723.1.23",
          "form": "왜 못갔어?",
          "original_form": "왜 못갔어?",
          "speaker_id": "1",
          "time": "20210831 17:35"
        },
        {
          "id": "MDRW2100024723.1.24",
          "form": "수학여행때 세월호참사가 안타깝게도",
          "original_form": "수학여행때 세월호참사가 안타깝게도",
          "speaker_id": "2",
          "time": "20210831 17:36"
        },
        {
          "id": "MDRW2100024723.1.25",
          "form": "그래서 전면취소 됐었어",
          "original_form": "그래서 전면취소 됐었어",
          "speaker_id": "2",
          "time": "20210831 17:36"
        },
        {
          "id": "MDRW2100024723.1.26",
          "form": "아... 그랬구만",
          "original_form": "아... 그랬구만",
          "speaker_id": "1",
          "time": "20210831 17:36"
        },
        {
          "id": "MDRW2100024723.1.27",
          "form": "맞아 그때 학생들 여행 다 취소됐었지",
          "original_form": "맞아 그때 학생들 여행 다 취소됐었지",
          "speaker_id": "1",
          "time": "20210831 17:37"
        },
        {
          "id": "MDRW2100024723.1.28",
          "form": "다음에 나랑 같이 제주도여행도 가자",
          "original_form": "다음에 나랑 같이 제주도여행도 가자",
          "speaker_id": "1",
          "time": "20210831 17:37"
        },
        {
          "id": "MDRW2100024723.1.29",
          "form": "그래 좋아 너랑갈곳이 늘었다",
          "original_form": "그래 좋아 너랑갈곳이 늘었다",
          "speaker_id": "2",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100024723.1.30",
          "form": "너 캐나다 갈 생각은 없냐",
          "original_form": "너 캐나다 갈 생각은 없냐",
          "speaker_id": "2",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100024723.1.31",
          "form": "아예 탈조 위시리스트로는 넣어놨는데ㅋㅋㅋㅋㅋ",
          "original_form": "아예 탈조 위시리스트로는 넣어놨는데ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100024723.1.32",
          "form": "단기 여행은 아직 생각 안 해봤어",
          "original_form": "단기 여행은 아직 생각 안 해봤어",
          "speaker_id": "1",
          "time": "20210831 17:39"
        }
      ]
    }
