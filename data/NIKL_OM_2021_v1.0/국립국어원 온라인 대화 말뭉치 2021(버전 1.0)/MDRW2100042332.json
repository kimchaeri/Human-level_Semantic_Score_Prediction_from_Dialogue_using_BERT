      "utterance": [
        {
          "id": "MDRW2100042332.1.1",
          "form": "넌 오늘 저녁 뭐먹었니",
          "original_form": "넌 오늘 저녁 뭐먹었니",
          "speaker_id": "2",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042332.1.2",
          "form": "뭐 먹었는지 맞춰볼래?",
          "original_form": "뭐 먹었는지 맞춰볼래?",
          "speaker_id": "1",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042332.1.3",
          "form": "그래 또 매운거 먹었어?",
          "original_form": "그래 또 매운거 먹었어?",
          "speaker_id": "2",
          "time": "20210906 21:38"
        },
        {
          "id": "MDRW2100042332.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ빨갛지만 안 매운 거 먹었어",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ빨갛지만 안 매운 거 먹었어",
          "speaker_id": "1",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100042332.1.5",
          "form": "또 김치볶음밥이니",
          "original_form": "또 김치볶음밥이니",
          "speaker_id": "2",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100042332.1.6",
          "form": "김치볶음밥 아니야ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "김치볶음밥 아니야ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100042332.1.7",
          "form": "위장이 뭐라하겠다",
          "original_form": "위장이 뭐라하겠다",
          "speaker_id": "2",
          "time": "20210906 21:39"
        },
        {
          "id": "MDRW2100042332.1.8",
          "form": "그래도 안 매운..요리였어!",
          "original_form": "그래도 안 매운..요리였어!",
          "speaker_id": "1",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042332.1.9",
          "form": "라면은 어때",
          "original_form": "라면은 어때",
          "speaker_id": "2",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042332.1.10",
          "form": "객관적으로 안 매워",
          "original_form": "객관적으로 안 매워",
          "speaker_id": "1",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042332.1.11",
          "form": "라면 맛있지",
          "original_form": "라면 맛있지",
          "speaker_id": "1",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042332.1.12",
          "form": "먹고싶다 하지만 오늘 저녁은 라면은 아니었어",
          "original_form": "먹고싶다 하지만 오늘 저녁은 라면은 아니었어",
          "speaker_id": "1",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042332.1.13",
          "form": "야채가 들어가니",
          "original_form": "야채가 들어가니",
          "speaker_id": "2",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042332.1.14",
          "form": "응 야채가 조금 들어가는데 주된 재료는 아니야",
          "original_form": "응 야채가 조금 들어가는데 주된 재료는 아니야",
          "speaker_id": "1",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042332.1.15",
          "form": "국물이 있는 요리니",
          "original_form": "국물이 있는 요리니",
          "speaker_id": "2",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100042332.1.16",
          "form": "조금? 국물이 많지는 않아",
          "original_form": "조금? 국물이 많지는 않아",
          "speaker_id": "1",
          "time": "20210906 21:41"
        }
      ]
