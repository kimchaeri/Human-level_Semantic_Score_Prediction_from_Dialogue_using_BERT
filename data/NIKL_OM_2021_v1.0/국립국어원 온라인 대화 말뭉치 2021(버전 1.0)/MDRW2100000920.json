      "utterance": [
        {
          "id": "MDRW2100000920.1.1",
          "form": "코로나 끝나면 어디로 여행하고 싶으신가요",
          "original_form": "코로나 끝나면 어디로 여행하고 싶으신가요",
          "speaker_id": "2",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000920.1.2",
          "form": "저는 파리요!",
          "original_form": "저는 파리요!",
          "speaker_id": "1",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000920.1.3",
          "form": "저도 유럽은 안 가봐서 저는 스페인 가보고 싶어요",
          "original_form": "저도 유럽은 안 가봐서 저는 스페인 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000920.1.4",
          "form": "오오 저는 사실 외국안가봐사",
          "original_form": "오오 저는 사실 외국안가봐사",
          "speaker_id": "1",
          "time": "20210819 20:40"
        },
        {
          "id": "MDRW2100000920.1.5",
          "form": "저도 많이는 안 가봤어요 국내 여행도 좋아하구요",
          "original_form": "저도 많이는 안 가봤어요 국내 여행도 좋아하구요",
          "speaker_id": "2",
          "time": "20210819 20:41"
        },
        {
          "id": "MDRW2100000920.1.6",
          "form": "저두 제주도 자주가요",
          "original_form": "저두 제주도 자주가요",
          "speaker_id": "1",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000920.1.7",
          "form": "제주도 가고싶었는데 작년에 코로나때문에 비행기 취소되서 못 갔어요ㅜ",
          "original_form": "제주도 가고싶었는데 작년에 코로나때문에 비행기 취소되서 못 갔어요ㅜ",
          "speaker_id": "2",
          "time": "20210819 20:53"
        },
        {
          "id": "MDRW2100000920.1.8",
          "form": "ㅠㅠ 저는 지난달에 다녀왔어요!",
          "original_form": "ㅠㅠ 저는 지난달에 다녀왔어요!",
          "speaker_id": "1",
          "time": "20210819 20:54"
        },
        {
          "id": "MDRW2100000920.1.9",
          "form": "제주도에서 어디가 가장 좋으셨나요",
          "original_form": "제주도에서 어디가 가장 좋으셨나요",
          "speaker_id": "2",
          "time": "20210819 21:02"
        },
        {
          "id": "MDRW2100000920.1.10",
          "form": "저는 1988이요! 카트타는곳",
          "original_form": "저는 1988이요! 카트타는곳",
          "speaker_id": "1",
          "time": "20210819 21:02"
        },
        {
          "id": "MDRW2100000920.1.11",
          "form": "오 재미있을것 같아요 사람 많았을것 같아요",
          "original_form": "오 재미있을것 같아요 사람 많았을것 같아요",
          "speaker_id": "2",
          "time": "20210819 21:07"
        },
        {
          "id": "MDRW2100000920.1.12",
          "form": "그다지 없었어요 개장맞춰서 가서",
          "original_form": "그다지 없었어요 개장맞춰서 가서",
          "speaker_id": "1",
          "time": "20210819 21:07"
        },
        {
          "id": "MDRW2100000920.1.13",
          "form": "아 꿀팁이네요 개장 맞춰서 ㅋㅋ",
          "original_form": "아 꿀팁이네요 개장 맞춰서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100000920.1.14",
          "form": "근데 엄청 비싸요,,",
          "original_form": "근데 엄청 비싸요,,",
