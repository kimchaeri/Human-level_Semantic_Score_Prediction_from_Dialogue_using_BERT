      "utterance": [
        {
          "id": "MDRW2100021282.1.1",
          "form": "고기 좋아하시나요?",
          "original_form": "고기 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210828 17:27"
        },
        {
          "id": "MDRW2100021282.1.2",
          "form": "네 고기졸아해요",
          "original_form": "네 고기졸아해요",
          "speaker_id": "1",
          "time": "20210828 17:27"
        },
        {
          "id": "MDRW2100021282.1.3",
          "form": "매운 음식은 잘 드시나요?",
          "original_form": "매운 음식은 잘 드시나요?",
          "speaker_id": "2",
          "time": "20210828 17:28"
        },
        {
          "id": "MDRW2100021282.1.4",
          "form": "아뇨 매운건 싫어해서",
          "original_form": "아뇨 매운건 싫어해서",
          "speaker_id": "1",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100021282.1.5",
          "form": "쪽갈비나 족발 어떠세요?",
          "original_form": "쪽갈비나 족발 어떠세요?",
          "speaker_id": "2",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100021282.1.6",
          "form": "음 쪽갈비는",
          "original_form": "음 쪽갈비는",
          "speaker_id": "1",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100021282.1.7",
          "form": "배달하는데가 없으니",
          "original_form": "배달하는데가 없으니",
          "speaker_id": "1",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100021282.1.8",
          "form": "족발로해야겟네요",
          "original_form": "족발로해야겟네요",
          "speaker_id": "1",
          "time": "20210828 17:33"
        },
        {
          "id": "MDRW2100021282.1.9",
          "form": "족발 맛있죠~ 막국수도 좋아하시나요?",
          "original_form": "족발 맛있죠~ 막국수도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210828 17:35"
        },
        {
          "id": "MDRW2100021282.1.10",
          "form": "막국수 좋죠",
          "original_form": "막국수 좋죠",
          "speaker_id": "1",
          "time": "20210828 17:38"
        },
        {
          "id": "MDRW2100021282.1.11",
          "form": "저는 족발말고 막국수만 먹고 싶을 때도 많아요ㅋㅋ 족발 드실 때 음료는 뭐랑 드세요?",
          "original_form": "저는 족발말고 막국수만 먹고 싶을 때도 많아요ㅋㅋ 족발 드실 때 음료는 뭐랑 드세요?",
          "speaker_id": "2",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100021282.1.12",
          "form": "음 족발이랑은 저는",
          "original_form": "음 족발이랑은 저는",
          "speaker_id": "1",
          "time": "20210828 17:40"
        },
        {
          "id": "MDRW2100021282.1.13",
          "form": "스프라이트 마시는걸 좋아해요",
          "original_form": "스프라이트 마시는걸 좋아해요",
          "speaker_id": "1",
          "time": "20210828 17:40"
        },
        {
          "id": "MDRW2100021282.1.14",
          "form": "저도 콜라보다는 사이다가 더 맛있더라고요!",
          "original_form": "저도 콜라보다는 사이다가 더 맛있더라고요!",
          "speaker_id": "2",
          "time": "20210828 17:41"
        },
        {
          "id": "MDRW2100021282.1.15",
          "form": "술은 안 좋아하시나요?",
          "original_form": "술은 안 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210828 17:41"
        },
        {
          "id": "MDRW2100021282.1.16",
          "form": "술은 안좋아하는데",
          "original_form": "술은 안좋아하는데",
          "speaker_id": "1",
          "time": "20210828 17:43"
        },
        {
          "id": "MDRW2100021282.1.17",
          "form": "가끔 맥주정도는 마셔요",
          "original_form": "가끔 맥주정도는 마셔요",
          "speaker_id": "1",
          "time": "20210828 17:43"
        },
        {
          "id": "MDRW2100021282.1.18",
          "form": "그렇군요!",
          "original_form": "그렇군요!",
          "speaker_id": "2",
          "time": "20210828 17:44"
        },
        {
          "id": "MDRW2100021282.1.19",
          "form": "맥주는 어떤 맥주 드세요?",
          "original_form": "맥주는 어떤 맥주 드세요?",
          "speaker_id": "2",
          "time": "20210828 17:44"
        },
        {
          "id": "MDRW2100021282.1.20",
          "form": "음 맥주는 그냥 처음보는거?",
          "original_form": "음 맥주는 그냥 처음보는거?",
          "speaker_id": "1",
          "time": "20210828 17:45"
        },
        {
          "id": "MDRW2100021282.1.21",
          "form": "편의점에 종류가 많아서",
          "original_form": "편의점에 종류가 많아서",
          "speaker_id": "1",
          "time": "20210828 17:45"
        },
        {
          "id": "MDRW2100021282.1.22",
          "form": "새로운걸로 먹어봐여",
          "original_form": "새로운걸로 먹어봐여",
          "speaker_id": "1",
          "time": "20210828 17:46"
        }
      ]
