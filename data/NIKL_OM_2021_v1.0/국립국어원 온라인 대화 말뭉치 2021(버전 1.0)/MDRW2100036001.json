      "utterance": [
        {
          "id": "MDRW2100036001.1.1",
          "form": "우와 부러워요",
          "original_form": "우와 부러워요",
          "speaker_id": "2",
          "time": "20210830 13:53"
        },
        {
          "id": "MDRW2100036001.1.2",
          "form": "제가 해외 중에선 제일 가고싶어하는 곳이",
          "original_form": "제가 해외 중에선 제일 가고싶어하는 곳이",
          "speaker_id": "2",
          "time": "20210830 13:53"
        },
        {
          "id": "MDRW2100036001.1.3",
          "form": "대만이에요!!!!!",
          "original_form": "대만이에요!!!!!",
          "speaker_id": "2",
          "time": "20210830 13:53"
        },
        {
          "id": "MDRW2100036001.1.4",
          "form": "대만 어디가고싶으세요?",
          "original_form": "대만 어디가고싶으세요?",
          "speaker_id": "1",
          "time": "20210830 13:54"
        },
        {
          "id": "MDRW2100036001.1.5",
          "form": "타이난이 가장 가고싶은데",
          "original_form": "타이난이 가장 가고싶은데",
          "speaker_id": "2",
          "time": "20210830 14:11"
        },
        {
          "id": "MDRW2100036001.1.6",
          "form": "친구가 타이중 살아서",
          "original_form": "친구가 타이중 살아서",
          "speaker_id": "2",
          "time": "20210830 14:11"
        },
        {
          "id": "MDRW2100036001.1.7",
          "form": "타이중부터 가야할 것 같긴 해요 만약 간다면!!!",
          "original_form": "타이중부터 가야할 것 같긴 해요 만약 간다면!!!",
          "speaker_id": "2",
          "time": "20210830 14:12"
        },
        {
          "id": "MDRW2100036001.1.8",
          "form": "와 친구가 타이중 사시는군요 이민가신건가요? 아니면 대만분이신거예요?",
          "original_form": "와 친구가 타이중 사시는군요 이민가신건가요? 아니면 대만분이신거예요?",
          "speaker_id": "1",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100036001.1.9",
          "form": "네네 대만사람이에요!!!",
          "original_form": "네네 대만사람이에요!!!",
          "speaker_id": "2",
          "time": "20210830 14:15"
        },
        {
          "id": "MDRW2100036001.1.10",
          "form": "인스타그램으로 같이 덕질하다가",
          "original_form": "인스타그램으로 같이 덕질하다가",
          "speaker_id": "2",
          "time": "20210830 14:15"
        },
        {
          "id": "MDRW2100036001.1.11",
          "form": "디엠 주고받다가 친해져서 3년째 선물이랑 편지 오가는 사이예요 ㅎㅎ",
          "original_form": "디엠 주고받다가 친해져서 3년째 선물이랑 편지 오가는 사이예요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 14:16"
        },
        {
          "id": "MDRW2100036001.1.12",
          "form": "언제 한 번 진짜로 만나야할텐데...",
          "original_form": "언제 한 번 진짜로 만나야할텐데...",
          "speaker_id": "2",
          "time": "20210830 14:16"
        },
        {
          "id": "MDRW2100036001.1.13",
          "form": "우와 정말 귀한 인연이네요",
          "original_form": "우와 정말 귀한 인연이네요",
          "speaker_id": "1",
          "time": "20210830 14:19"
        },
        {
          "id": "MDRW2100036001.1.14",
          "form": "언제 한번 만나면 너무 재밌으시겠어요 쌓인 기간도 있구",
          "original_form": "언제 한번 만나면 너무 재밌으시겠어요 쌓인 기간도 있구",
          "speaker_id": "1",
          "time": "20210830 14:19"
        },
        {
          "id": "MDRW2100036001.1.15",
          "form": "대만현지분한테 가이드받으시면 그것만한게 없죠",
          "original_form": "대만현지분한테 가이드받으시면 그것만한게 없죠",
          "speaker_id": "1",
          "time": "20210830 14:20"
        },
        {
          "id": "MDRW2100036001.1.16",
          "form": "맞아요 맞아요!",
          "original_form": "맞아요 맞아요!",
          "speaker_id": "2",
          "time": "20210830 14:28"
        },
        {
          "id": "MDRW2100036001.1.17",
          "form": "그 전에 제가 중국어를 좀 배워놔야할텐데...ㅎㅎ",
          "original_form": "그 전에 제가 중국어를 좀 배워놔야할텐데...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 14:28"
        },
        {
          "id": "MDRW2100036001.1.18",
          "form": "대만은 그렇게나 음식이",
          "original_form": "대만은 그렇게나 음식이",
          "speaker_id": "2",
          "time": "20210830 14:28"
        },
        {
          "id": "MDRW2100036001.1.19",
          "form": "맛있다면서요~!",
          "original_form": "맛있다면서요~!",
          "speaker_id": "2",
          "time": "20210830 14:28"
        },
        {
          "id": "MDRW2100036001.1.20",
          "form": "네! 저 대만에서 먹은 딤섬을 아직도 못잊어요",
          "original_form": "네! 저 대만에서 먹은 딤섬을 아직도 못잊어요",
          "speaker_id": "1",
          "time": "20210830 14:29"
        },
        {
          "id": "MDRW2100036001.1.21",
          "form": "딘다이펑이 한국에도 있지만 대만본점이 진짜 맛있어요",
          "original_form": "딘다이펑이 한국에도 있지만 대만본점이 진짜 맛있어요",
          "speaker_id": "1",
          "time": "20210830 14:29"
        },
        {
          "id": "MDRW2100036001.1.22",
          "form": "우와.....",
          "original_form": "우와.....",
          "speaker_id": "2",
          "time": "20210830 14:42"
        },
        {
          "id": "MDRW2100036001.1.23",
          "form": "저는 펑리수랑 버블티 정도만 생각하고 있었는데",
          "original_form": "저는 펑리수랑 버블티 정도만 생각하고 있었는데",
          "speaker_id": "2",
          "time": "20210830 14:43"
        },
        {
          "id": "MDRW2100036001.1.24",
          "form": "딤섬 생각하니까 정말 군침 도네요",
          "original_form": "딤섬 생각하니까 정말 군침 도네요",
          "speaker_id": "2",
          "time": "20210830 14:43"
        },
        {
          "id": "MDRW2100036001.1.25",
          "form": "그리고 대만은 또 물가가 저렴하다지요?",
          "original_form": "그리고 대만은 또 물가가 저렴하다지요?",
          "speaker_id": "2",
          "time": "20210830 14:43"
        },
        {
          "id": "MDRW2100036001.1.26",
          "form": "그죠! 야시장 에서 맛있는거 싹 털어버리세요!",
          "original_form": "그죠! 야시장 에서 맛있는거 싹 털어버리세요!",
          "speaker_id": "1",
          "time": "20210830 14:44"
        },
        {
          "id": "MDRW2100036001.1.27",
          "form": "상상만 해도 너무 좋아요!",
          "original_form": "상상만 해도 너무 좋아요!",
          "speaker_id": "2",
          "time": "20210830 14:47"
        },
        {
          "id": "MDRW2100036001.1.28",
          "form": "그리고 일본 영향을 많이 받아서인지",
          "original_form": "그리고 일본 영향을 많이 받아서인지",
          "speaker_id": "2",
          "time": "20210830 14:47"
        },
        {
          "id": "MDRW2100036001.1.29",
          "form": "캐릭터 상품도 많다고 하더라구요!",
          "original_form": "캐릭터 상품도 많다고 하더라구요!",
          "speaker_id": "2",
          "time": "20210830 14:47"
        },
        {
          "id": "MDRW2100036001.1.30",
          "form": "맞아요!! 은근 엄청 많더라구요",
          "original_form": "맞아요!! 은근 엄청 많더라구요",
          "speaker_id": "1",
          "time": "20210830 14:47"
        },
        {
          "id": "MDRW2100036001.1.31",
          "form": "인형도 많구요",
          "original_form": "인형도 많구요",
          "speaker_id": "1",
          "time": "20210830 14:48"
        },
        {
          "id": "MDRW2100036001.1.32",
          "form": "그쵸그쵸!!!!",
          "original_form": "그쵸그쵸!!!!",
          "speaker_id": "2",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100036001.1.33",
          "form": "그래서 대만이 가고싶어요,,,",
          "original_form": "그래서 대만이 가고싶어요,,,",
          "speaker_id": "2",
          "time": "20210830 14:57"
        },
        {
          "id": "MDRW2100036001.1.34",
          "form": "name1님은 어디 가고 싶으신 데 있으세요?",
          "original_form": "&name1&님은 어디 가고 싶으신 데 있으세요?",
