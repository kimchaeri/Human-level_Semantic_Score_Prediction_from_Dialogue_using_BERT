      "utterance": [
        {
          "id": "MDRW2100029347.1.1",
          "form": "이번주 일요일에 큰이모네 가??",
          "original_form": "이번주 일요일에 큰이모네 가??",
          "speaker_id": "2",
          "time": "20210903 02:17"
        },
        {
          "id": "MDRW2100029347.1.2",
          "form": "가서 컴퓨터에 뭐 들어있나 봐줘",
          "original_form": "가서 컴퓨터에 뭐 들어있나 봐줘",
          "speaker_id": "1",
          "time": "20210903 17:43"
        },
        {
          "id": "MDRW2100029347.1.3",
          "form": "컴퓨터에 뭐가 들었길래",
          "original_form": "컴퓨터에 뭐가 들었길래",
          "speaker_id": "2",
          "time": "20210903 21:48"
        },
        {
          "id": "MDRW2100029347.1.4",
          "form": "아저씨가 쓰던건데 뭐 중요한거 들어있나보게",
          "original_form": "아저씨가 쓰던건데 뭐 중요한거 들어있나보게",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100029347.1.5",
          "form": "아저씨 나이 많지 않았어??",
          "original_form": "아저씨 나이 많지 않았어??",
          "speaker_id": "2",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100029347.1.6",
          "form": "나이 많았지 70",
          "original_form": "나이 많았지 70",
          "speaker_id": "1",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100029347.1.7",
          "form": "그 정도면 별로 중요한것도 안 넣어놨을듯",
          "original_form": "그 정도면 별로 중요한것도 안 넣어놨을듯",
          "speaker_id": "2",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100029347.1.8",
          "form": "그럴거 같긴한데 혹시 모르니까",
          "original_form": "그럴거 같긴한데 혹시 모르니까",
          "speaker_id": "1",
          "time": "20210903 21:52"
        },
        {
          "id": "MDRW2100029347.1.9",
          "form": "유서같은거 있을까봐",
          "original_form": "유서같은거 있을까봐",
          "speaker_id": "1",
          "time": "20210903 21:53"
        },
        {
          "id": "MDRW2100029347.1.10",
          "form": "이미 이모재산은 다 받았자나",
          "original_form": "이미 이모재산은 다 받았자나",
          "speaker_id": "2",
          "time": "20210903 21:53"
        },
        {
          "id": "MDRW2100029347.1.11",
          "form": "유서 나와도 별거 없을거 같은데",
          "original_form": "유서 나와도 별거 없을거 같은데",
          "speaker_id": "2",
          "time": "20210903 21:53"
        },
        {
          "id": "MDRW2100029347.1.12",
          "form": "그럼 저거 없애버릴수 있나",
          "original_form": "그럼 저거 없애버릴수 있나",
          "speaker_id": "1",
          "time": "20210903 21:54"
        },
        {
          "id": "MDRW2100029347.1.13",
          "form": "응 하드 꺼내서 부셔버리면돼",
          "original_form": "응 하드 꺼내서 부셔버리면돼",
          "speaker_id": "2",
          "time": "20210903 21:55"
        },
        {
          "id": "MDRW2100029347.1.14",
          "form": "그럼 그렇게 해줘",
          "original_form": "그럼 그렇게 해줘",
          "speaker_id": "1",
          "time": "20210903 21:55"
        },
        {
          "id": "MDRW2100029347.1.15",
          "form": "귀찮은 일을 너무 많이 시키는구만",
          "original_form": "귀찮은 일을 너무 많이 시키는구만",
          "speaker_id": "2",
          "time": "20210903 21:56"
        },
        {
          "id": "MDRW2100029347.1.16",
          "form": "한번만해줘",
          "original_form": "한번만해줘",
          "speaker_id": "1",
          "time": "20210903 21:56"
        },
        {
          "id": "MDRW2100029347.1.17",
          "form": "한번 아니자나",
          "original_form": "한번 아니자나",
          "speaker_id": "2",
          "time": "20210903 21:56"
        },
        {
          "id": "MDRW2100029347.1.18",
          "form": "엄마가 준돈은 어떻게 됐나",
          "original_form": "엄마가 준돈은 어떻게 됐나",
