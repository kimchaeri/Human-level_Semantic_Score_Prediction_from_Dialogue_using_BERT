      "utterance": [
        {
          "id": "MDRW2100007891.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210822 11:37"
        },
        {
          "id": "MDRW2100007891.1.2",
          "form": "요즘 건강을 위해 뭘 하고 계시나요?",
          "original_form": "요즘 건강을 위해 뭘 하고 계시나요?",
          "speaker_id": "1",
          "time": "20210822 11:37"
        },
        {
          "id": "MDRW2100007891.1.3",
          "form": "저는 액상과당 끊으려고 하고있어요!",
          "original_form": "저는 액상과당 끊으려고 하고있어요!",
          "speaker_id": "2",
          "time": "20210822 11:38"
        },
        {
          "id": "MDRW2100007891.1.4",
          "form": "와 저도요",
          "original_form": "와 저도요",
          "speaker_id": "1",
          "time": "20210822 11:38"
        },
        {
          "id": "MDRW2100007891.1.5",
          "form": "달달한 것들은 당분이 정말 많더라구요...",
          "original_form": "달달한 것들은 당분이 정말 많더라구요...",
          "speaker_id": "2",
          "time": "20210822 11:38"
        },
        {
          "id": "MDRW2100007891.1.6",
          "form": "몇 년 전에 단 커피",
          "original_form": "몇 년 전에 단 커피",
          "speaker_id": "1",
          "time": "20210822 11:38"
        },
        {
          "id": "MDRW2100007891.1.7",
          "form": "카라멜 마끼아또나 바닐라라떼",
          "original_form": "카라멜 마끼아또나 바닐라라떼",
          "speaker_id": "1",
          "time": "20210822 11:38"
        },
        {
          "id": "MDRW2100007891.1.8",
          "form": "이런 거 진짜 많이 마셨는데 후회돼요",
          "original_form": "이런 거 진짜 많이 마셨는데 후회돼요",
          "speaker_id": "1",
          "time": "20210822 11:39"
        },
        {
          "id": "MDRW2100007891.1.9",
          "form": "그치만 저는 아메리카노는 못마셔서",
          "original_form": "그치만 저는 아메리카노는 못마셔서",
          "speaker_id": "2",
          "time": "20210822 11:40"
        },
        {
          "id": "MDRW2100007891.1.10",
          "form": "카페가면 차만 마시고 있네요...ㅎㅎ",
          "original_form": "카페가면 차만 마시고 있네요...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 11:40"
        },
        {
          "id": "MDRW2100007891.1.11",
          "form": "name1님은 커피 좋아하시나요?",
          "original_form": "&name1&님은 커피 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210822 11:40"
        },
        {
          "id": "MDRW2100007891.1.12",
          "form": "지금도 한 잔 마시고 있어요",
          "original_form": "지금도 한 잔 마시고 있어요",
          "speaker_id": "1",
          "time": "20210822 11:40"
        },
        {
          "id": "MDRW2100007891.1.13",
          "form": "하기 싫은 것들 버틸 때 꼭 필요해요",
          "original_form": "하기 싫은 것들 버틸 때 꼭 필요해요",
          "speaker_id": "1",
          "time": "20210822 11:41"
        },
        {
          "id": "MDRW2100007891.1.14",
          "form": "학생 땐 과제.... 지금은 출근... 같은 것들....? ㅎㅎㅎ",
          "original_form": "학생 땐 과제.... 지금은 출근... 같은 것들....? ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 11:41"
        },
        {
          "id": "MDRW2100007891.1.15",
          "form": "그쵸 출근할때 다들 커피 끊임없이 드시더라구요",
          "original_form": "그쵸 출근할때 다들 커피 끊임없이 드시더라구요",
          "speaker_id": "2",
          "time": "20210822 11:41"
        },
        {
          "id": "MDRW2100007891.1.16",
          "form": "없으면 못살겠다고",
          "original_form": "없으면 못살겠다고",
          "speaker_id": "2",
          "time": "20210822 11:41"
        },
        {
          "id": "MDRW2100007891.1.17",
          "form": "직장인의 포션이라고들 하죠 ㅋㅋㅋ",
          "original_form": "직장인의 포션이라고들 하죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:42"
        },
        {
          "id": "MDRW2100007891.1.18",
          "form": "근데 지금은 젊어서 버티는데",
          "original_form": "근데 지금은 젊어서 버티는데",
          "speaker_id": "1",
          "time": "20210822 11:42"
        },
        {
          "id": "MDRW2100007891.1.19",
          "form": "나중에 위가 상할 것 같긴 해요",
          "original_form": "나중에 위가 상할 것 같긴 해요",
          "speaker_id": "1",
          "time": "20210822 11:42"
        },
        {
          "id": "MDRW2100007891.1.20",
          "form": "미래의 건강을 땡겨서 오늘을 버티는 느낌?",
          "original_form": "미래의 건강을 땡겨서 오늘을 버티는 느낌?",
          "speaker_id": "1",
          "time": "20210822 11:42"
        },
        {
          "id": "MDRW2100007891.1.21",
          "form": "ㅠㅠㅠㅠㅠㅠ 좀 나이들면서",
          "original_form": "ㅠㅠㅠㅠㅠㅠ 좀 나이들면서",
          "speaker_id": "2",
          "time": "20210822 11:42"
        },
        {
          "id": "MDRW2100007891.1.22",
          "form": "하루하루 몸의 회복력이 느려지는게 느껴져서...",
          "original_form": "하루하루 몸의 회복력이 느려지는게 느껴져서...",
          "speaker_id": "2",
          "time": "20210822 11:43"
        },
        {
          "id": "MDRW2100007891.1.23",
          "form": "앞으로 어찌 살아야하나 막막하기도 해요",
          "original_form": "앞으로 어찌 살아야하나 막막하기도 해요",
          "speaker_id": "2",
          "time": "20210822 11:43"
        },
        {
          "id": "MDRW2100007891.1.24",
          "form": "운동은 하시는 거 있어요?",
          "original_form": "운동은 하시는 거 있어요?",
          "speaker_id": "1",
          "time": "20210822 11:43"
        },
        {
          "id": "MDRW2100007891.1.25",
          "form": "운동 정말 싫어하는데... 해야되나 싶더라구요",
          "original_form": "운동 정말 싫어하는데... 해야되나 싶더라구요",
          "speaker_id": "2",
          "time": "20210822 11:43"
        },
        {
          "id": "MDRW2100007891.1.26",
          "form": "그쵸 저도",
          "original_form": "그쵸 저도",
          "speaker_id": "1",
          "time": "20210822 11:43"
        },
        {
          "id": "MDRW2100007891.1.27",
          "form": "특히 근력운동이 중요하다는데",
          "original_form": "특히 근력운동이 중요하다는데",
          "speaker_id": "1",
          "time": "20210822 11:43"
        },
        {
          "id": "MDRW2100007891.1.28",
          "form": "코시국이라 쉽지 않네요",
          "original_form": "코시국이라 쉽지 않네요",
          "speaker_id": "1",
          "time": "20210822 11:43"
        },
        {
          "id": "MDRW2100007891.1.29",
          "form": "마스크 끼고 운동하면 피부가 너무 힘들어해요 ㅠㅠ",
          "original_form": "마스크 끼고 운동하면 피부가 너무 힘들어해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 11:44"
        },
        {
          "id": "MDRW2100007891.1.30",
          "form": "그쵸. 저도 PT한번 받아보고 싶은데",
          "original_form": "그쵸. 저도 PT한번 받아보고 싶은데",
          "speaker_id": "2",
          "time": "20210822 11:44"
        },
        {
          "id": "MDRW2100007891.1.31",
          "form": "주변 조건이 안따라주니까...",
          "original_form": "주변 조건이 안따라주니까...",
          "speaker_id": "2",
          "time": "20210822 11:44"
        },
        {
          "id": "MDRW2100007891.1.32",
          "form": "요즘에 바디프로필 같은 게 인기이기도 하고",
          "original_form": "요즘에 바디프로필 같은 게 인기이기도 하고",
          "speaker_id": "1",
          "time": "20210822 11:45"
        },
        {
          "id": "MDRW2100007891.1.33",
          "form": "sns 보면 저 빼고 다 운동하는 느낌!",
          "original_form": "sns 보면 저 빼고 다 운동하는 느낌!",
          "speaker_id": "1",
          "time": "20210822 11:45"
        },
        {
          "id": "MDRW2100007891.1.34",
          "form": "실행은 안 하면서 죄책감만 느끼고 그래요 ㅋㅋ",
          "original_form": "실행은 안 하면서 죄책감만 느끼고 그래요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:45"
        },
        {
          "id": "MDRW2100007891.1.35",
          "form": "맞아요ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:45"
        },
        {
          "id": "MDRW2100007891.1.36",
          "form": "근데 바디프로필은 부작용 많다고해서",
          "original_form": "근데 바디프로필은 부작용 많다고해서",
          "speaker_id": "2",
          "time": "20210822 11:45"
        },
        {
          "id": "MDRW2100007891.1.37",
          "form": "좀 무섭더라구요",
          "original_form": "좀 무섭더라구요",
          "speaker_id": "2",
          "time": "20210822 11:46"
        },
        {
          "id": "MDRW2100007891.1.38",
          "form": "맞아요 요요도 많다고 하고",
          "original_form": "맞아요 요요도 많다고 하고",
          "speaker_id": "1",
          "time": "20210822 11:46"
        },
        {
          "id": "MDRW2100007891.1.39",
          "form": "맞아요ㅠㅠ 열심히 뺐는데 요요라니..",
          "original_form": "맞아요ㅠㅠ 열심히 뺐는데 요요라니..",
          "speaker_id": "2",
          "time": "20210822 11:46"
        },
        {
          "id": "MDRW2100007891.1.40",
          "form": "너무해...",
          "original_form": "너무해...",
          "speaker_id": "2",
          "time": "20210822 11:47"
        },
        {
          "id": "MDRW2100007891.1.41",
          "form": "뭐든지 급하게 하면 역시 탈이 나나봐요",
          "original_form": "뭐든지 급하게 하면 역시 탈이 나나봐요",
          "speaker_id": "1",
          "time": "20210822 11:47"
        },
        {
          "id": "MDRW2100007891.1.42",
          "form": "맞아요..ㅠㅠ",
          "original_form": "맞아요..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 11:47"
        },
        {
          "id": "MDRW2100007891.1.43",
          "form": "운동을 안 하니",
          "original_form": "운동을 안 하니",
          "speaker_id": "1",
          "time": "20210822 11:47"
        },
        {
          "id": "MDRW2100007891.1.44",
          "form": "먹는 거라도 잘 해보려는데",
          "original_form": "먹는 거라도 잘 해보려는데",
