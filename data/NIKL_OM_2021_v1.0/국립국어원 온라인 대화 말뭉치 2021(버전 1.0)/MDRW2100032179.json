      "utterance": [
        {
          "id": "MDRW2100032179.1.1",
          "form": "곧 또 맨투맨도 사야겠군ㅋㅋㅋ",
          "original_form": "곧 또 맨투맨도 사야겠군ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:10"
        },
        {
          "id": "MDRW2100032179.1.2",
          "form": "맨투맨 보고 있니 ㅋㅋㅋ",
          "original_form": "맨투맨 보고 있니 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:10"
        },
        {
          "id": "MDRW2100032179.1.3",
          "form": "점찍어뒀니",
          "original_form": "점찍어뒀니",
          "speaker_id": "1",
          "time": "20210905 14:10"
        },
        {
          "id": "MDRW2100032179.1.4",
          "form": "음 톰보이꺼 봐두긴 했는데 아직 확정짓지는 않음ㅋ",
          "original_form": "음 톰보이꺼 봐두긴 했는데 아직 확정짓지는 않음ㅋ",
          "speaker_id": "2",
          "time": "20210905 14:11"
        },
        {
          "id": "MDRW2100032179.1.5",
          "form": "미리미리 잘 골라두는 군 ㅋㅋㅋ",
          "original_form": "미리미리 잘 골라두는 군 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:13"
        },
        {
          "id": "MDRW2100032179.1.6",
          "form": "골라두지만 결제는 쉽게 하지못한다....ㅋㅋ",
          "original_form": "골라두지만 결제는 쉽게 하지못한다....ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 14:40"
        },
        {
          "id": "MDRW2100032179.1.7",
          "form": "원래 옷은 그렇게 사는거지",
          "original_form": "원래 옷은 그렇게 사는거지",
          "speaker_id": "1",
          "time": "20210905 15:05"
        },
        {
          "id": "MDRW2100032179.1.8",
          "form": "지원금으로 옷사긴 글렀네",
          "original_form": "지원금으로 옷사긴 글렀네",
          "speaker_id": "2",
          "time": "20210905 16:10"
        },
        {
          "id": "MDRW2100032179.1.9",
          "form": "그니까 ㅋㅋㅋ 집에 쓰는거 말고 살게 없어",
          "original_form": "그니까 ㅋㅋㅋ 집에 쓰는거 말고 살게 없어",
          "speaker_id": "1",
          "time": "20210905 16:21"
        },
        {
          "id": "MDRW2100032179.1.10",
          "form": "모다아울렛에서 츄리닝 살까",
          "original_form": "모다아울렛에서 츄리닝 살까",
          "speaker_id": "2",
          "time": "20210905 16:30"
        },
        {
          "id": "MDRW2100032179.1.11",
          "form": "그까이 가야...근데 또 막 대기업하는데는 안되고 그런거있지 않나 되는 매장인지 확인해야할듯",
          "original_form": "그까이 가야...근데 또 막 대기업하는데는 안되고 그런거있지 않나 되는 매장인지 확인해야할듯",
          "speaker_id": "1",
          "time": "20210905 16:31"
        },
        {
          "id": "MDRW2100032179.1.12",
          "form": "하 귀찮넴ㅋㅋㅋ",
          "original_form": "하 귀찮넴ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 16:36"
        },
        {
          "id": "MDRW2100032179.1.13",
          "form": "이동네서 알차게 쓰기 넘 어렵다 ㅋㅋ",
          "original_form": "이동네서 알차게 쓰기 넘 어렵다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:37"
        },
        {
          "id": "MDRW2100032179.1.14",
          "form": "안경이나 새로할까",
          "original_form": "안경이나 새로할까",
          "speaker_id": "2",
          "time": "20210905 16:38"
        },
        {
          "id": "MDRW2100032179.1.15",
          "form": "나두 안경할 생각 ㅋㅋ",
          "original_form": "나두 안경할 생각 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:42"
        },
        {
          "id": "MDRW2100032179.1.16",
          "form": "내가 쓸 곳은 끽해야 커피 플레이스 정도니 ㅋㅋㅋ",
          "original_form": "내가 쓸 곳은 끽해야 커피 플레이스 정도니 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:42"
        },
        {
          "id": "MDRW2100032179.1.17",
          "form": "커피플레이스에서 자주 만나쟈ㅋㅋㅋㅋㅋ",
          "original_form": "커피플레이스에서 자주 만나쟈ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 16:43"
        },
        {
          "id": "MDRW2100032179.1.18",
          "form": "가는 곳은 다 대기업이다 편의점 빵집 다이소 ㅋㅋㅋ",
          "original_form": "가는 곳은 다 대기업이다 편의점 빵집 다이소 ㅋㅋㅋ",
          "speaker_id": "1",
