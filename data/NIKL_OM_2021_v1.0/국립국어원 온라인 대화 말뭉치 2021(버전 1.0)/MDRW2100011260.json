      "utterance": [
        {
          "id": "MDRW2100011260.1.1",
          "form": "커뮤니티 활동하시나요~?",
          "original_form": "커뮤니티 활동하시나요~?",
          "speaker_id": "1",
          "time": "20210824 10:11"
        },
        {
          "id": "MDRW2100011260.1.2",
          "form": "안녕하세요 아영님! 답장이 늦었네요 ㅎㅎ",
          "original_form": "안녕하세요 아영님! 답장이 늦었네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100011260.1.3",
          "form": "네 커뮤니티 활동하고 있어요 ㅎㅎ",
          "original_form": "네 커뮤니티 활동하고 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100011260.1.4",
          "form": "다음카페 하고있어요",
          "original_form": "다음카페 하고있어요",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100011260.1.5",
          "form": "아영님은요?",
          "original_form": "아영님은요?",
          "speaker_id": "2",
          "time": "20210825 17:22"
        },
        {
          "id": "MDRW2100011260.1.6",
          "form": "우와~ 저는 커뮤니티는 따로 안하고 sns만 하고 있습니다~",
          "original_form": "우와~ 저는 커뮤니티는 따로 안하고 sns만 하고 있습니다~",
          "speaker_id": "1",
          "time": "20210826 10:16"
        },
        {
          "id": "MDRW2100011260.1.7",
          "form": "아하 어떤 sns 하세요?",
          "original_form": "아하 어떤 sns 하세요?",
          "speaker_id": "2",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100011260.1.8",
          "form": "저는 인스타랑 페북이요~~",
          "original_form": "저는 인스타랑 페북이요~~",
          "speaker_id": "1",
          "time": "20210826 16:12"
        },
        {
          "id": "MDRW2100011260.1.9",
          "form": "아하 저는 트위터 조금 해요",
          "original_form": "아하 저는 트위터 조금 해요",
          "speaker_id": "2",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100011260.1.10",
          "form": "인스타도 예전에 했었는데 지금은 안해요 ㅎㅎ",
          "original_form": "인스타도 예전에 했었는데 지금은 안해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100011260.1.11",
          "form": "페이스북은 계정은 있긴 한데",
          "original_form": "페이스북은 계정은 있긴 한데",
          "speaker_id": "2",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100011260.1.12",
          "form": "뭔가 어렵더라구요",
          "original_form": "뭔가 어렵더라구요",
          "speaker_id": "2",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100011260.1.13",
          "form": "지금도 어떻게 하는지 잘 모르겠어요 ㅋㅋㅋ",
          "original_form": "지금도 어떻게 하는지 잘 모르겠어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100011260.1.14",
          "form": "ㅋㅋㅋ저도 하기는하는데 그냥 눈팅정도??",
          "original_form": "ㅋㅋㅋ저도 하기는하는데 그냥 눈팅정도??",
          "speaker_id": "1",
          "time": "20210827 10:15"
        },
        {
          "id": "MDRW2100011260.1.15",
          "form": "sns가.꽤 어렵거라그여ㅠㅠ",
          "original_form": "sns가.꽤 어렵거라그여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 10:15"
        },
        {
          "id": "MDRW2100011260.1.16",
          "form": "ㅋㅋㅋㅋㅋ 맞아요.. 저도 트위터 눈팅해요",
          "original_form": "ㅋㅋㅋㅋㅋ 맞아요.. 저도 트위터 눈팅해요",
          "speaker_id": "2",
          "time": "20210827 15:30"
        },
        {
          "id": "MDRW2100011260.1.17",
          "form": "막 활발하게 글올리고 하는 스타일도 아니라 ㅋㅋㅋ",
          "original_form": "막 활발하게 글올리고 하는 스타일도 아니라 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:30"
        },
        {
          "id": "MDRW2100011260.1.18",
          "form": "저랑 비슷하시네용ㅋㅋ",
          "original_form": "저랑 비슷하시네용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:54"
        },
        {
          "id": "MDRW2100011260.1.19",
          "form": "그래도 보면서 맛집?! 이런정보도 얻을수있더라구요~",
          "original_form": "그래도 보면서 맛집?! 이런정보도 얻을수있더라구요~",
          "speaker_id": "1",
          "time": "20210828 12:59"
        },
        {
          "id": "MDRW2100011260.1.20",
          "form": "맞아요 sns를 하면",
          "original_form": "맞아요 sns를 하면",
          "speaker_id": "2",
          "time": "20210828 21:40"
        },
        {
          "id": "MDRW2100011260.1.21",
          "form": "이것저것 정보를 얻을 수 있어서 좋은 것 같아요 ㅎㅎ",
          "original_form": "이것저것 정보를 얻을 수 있어서 좋은 것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 21:40"
        },
        {
          "id": "MDRW2100011260.1.22",
          "form": "맞아용ㅋㅋ sns보고 찾아간 맛집있나용?!",
          "original_form": "맞아용ㅋㅋ sns보고 찾아간 맛집있나용?!",
          "speaker_id": "1",
          "time": "20210829 16:40"
        },
        {
          "id": "MDRW2100011260.1.23",
          "form": "아니요 ㅋㅋㅋ 항상 보고 가고싶다고 생각은 하는데 귀찮아서 안 가게 되더라구요",
          "original_form": "아니요 ㅋㅋㅋ 항상 보고 가고싶다고 생각은 하는데 귀찮아서 안 가게 되더라구요",
          "speaker_id": "2",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100011260.1.24",
          "form": "아영님은 sns에서 본 맛집들 자주 다니세요?",
          "original_form": "아영님은 sns에서 본 맛집들 자주 다니세요?",
