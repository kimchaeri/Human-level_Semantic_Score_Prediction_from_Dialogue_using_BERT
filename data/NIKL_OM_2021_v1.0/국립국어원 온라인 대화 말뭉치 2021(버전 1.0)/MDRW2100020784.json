      "utterance": [
        {
          "id": "MDRW2100020784.1.1",
          "form": "좋아하는 스포츠",
          "original_form": "좋아하는 스포츠",
          "speaker_id": "2",
          "time": "20210828 12:48"
        },
        {
          "id": "MDRW2100020784.1.2",
          "form": "있으세요?",
          "original_form": "있으세요?",
          "speaker_id": "2",
          "time": "20210828 12:48"
        },
        {
          "id": "MDRW2100020784.1.3",
          "form": "네! name2님도 좋아하는 스포츠 있으세요?",
          "original_form": "네! &name2&님도 좋아하는 스포츠 있으세요?",
          "speaker_id": "1",
          "time": "20210828 18:56"
        },
        {
          "id": "MDRW2100020784.1.4",
          "form": "뭔가요??",
          "original_form": "뭔가요??",
          "speaker_id": "2",
          "time": "20210828 19:17"
        },
        {
          "id": "MDRW2100020784.1.5",
          "form": "전 요새 양궁이요",
          "original_form": "전 요새 양궁이요",
          "speaker_id": "2",
          "time": "20210828 19:17"
        },
        {
          "id": "MDRW2100020784.1.6",
          "form": "저도 양궁 좋아해요!",
          "original_form": "저도 양궁 좋아해요!",
          "speaker_id": "1",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100020784.1.7",
          "form": "저는 이번 올림픽으로 여자배구가 좋아졌어요!",
          "original_form": "저는 이번 올림픽으로 여자배구가 좋아졌어요!",
          "speaker_id": "1",
          "time": "20210829 21:38"
        },
        {
          "id": "MDRW2100020784.1.8",
          "form": "ㅋㅋㅋㅋㅋ 맞아요 ㅎ",
          "original_form": "ㅋㅋㅋㅋㅋ 맞아요 ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:52"
        },
        {
          "id": "MDRW2100020784.1.9",
          "form": "양궁 배구 다 !!",
          "original_form": "양궁 배구 다 !!",
          "speaker_id": "2",
          "time": "20210829 21:53"
        },
        {
          "id": "MDRW2100020784.1.10",
          "form": "저는 그래서 저번주에 열린 여자배구 코보컵을 챙겨봤어요 ㅎㅎㅎ",
          "original_form": "저는 그래서 저번주에 열린 여자배구 코보컵을 챙겨봤어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 15:11"
        },
        {
          "id": "MDRW2100020784.1.11",
          "form": "팀이 6개 있는데 저는 그중에서 인삼 팀이 정이 가네용",
          "original_form": "팀이 6개 있는데 저는 그중에서 인삼 팀이 정이 가네용",
          "speaker_id": "1",
          "time": "20210830 15:11"
        },
        {
          "id": "MDRW2100020784.1.12",
          "form": "재밌었나여ㅋㅋㅋㅋㅋㅋ!!",
          "original_form": "재밌었나여ㅋㅋㅋㅋㅋㅋ!!",
          "speaker_id": "2",
          "time": "20210830 15:26"
        },
        {
          "id": "MDRW2100020784.1.13",
          "form": "네!!! 저번 시즌 꼴등팀인 현대건설 팀이 우승했어요",
          "original_form": "네!!! 저번 시즌 꼴등팀인 현대건설 팀이 우승했어요",
          "speaker_id": "1",
          "time": "20210830 15:27"
        },
        {
          "id": "MDRW2100020784.1.14",
          "form": "국대 양효진 정지윤 헌수 있는 팀이에용",
          "original_form": "국대 양효진 정지윤 헌수 있는 팀이에용",
          "speaker_id": "1",
          "time": "20210830 15:27"
        },
        {
          "id": "MDRW2100020784.1.15",
          "form": "선수",
          "original_form": "선수",
          "speaker_id": "1",
          "time": "20210830 15:27"
        },
        {
          "id": "MDRW2100020784.1.16",
          "form": "오 !!! 우승이라니",
          "original_form": "오 !!! 우승이라니",
          "speaker_id": "2",
          "time": "20210830 15:29"
        },
        {
          "id": "MDRW2100020784.1.17",
          "form": "대박이네여!",
          "original_form": "대박이네여!",
          "speaker_id": "2",
          "time": "20210830 15:29"
        },
        {
          "id": "MDRW2100020784.1.18",
          "form": "네 그것도 저번 시즌 우승팀인 GS 킥스팀을 꺾어서 재밌었어요!!!!!",
          "original_form": "네 그것도 저번 시즌 우승팀인 GS 킥스팀을 꺾어서 재밌었어요!!!!!",
          "speaker_id": "1",
          "time": "20210831 12:48"
        },
        {
          "id": "MDRW2100020784.1.19",
          "form": "근데 제가 응원한 인삼팀은 꼴찌해서 마음이 아파용...",
          "original_form": "근데 제가 응원한 인삼팀은 꼴찌해서 마음이 아파용...",
          "speaker_id": "1",
          "time": "20210831 12:48"
        },
        {
          "id": "MDRW2100020784.1.20",
          "form": "연고지가 대전이라 한화랑 같이 가는건가...",
          "original_form": "연고지가 대전이라 한화랑 같이 가는건가...",
          "speaker_id": "1",
          "time": "20210831 12:49"
        },
        {
          "id": "MDRW2100020784.1.21",
          "form": "다른 스포츠도 좋아하시나요?",
          "original_form": "다른 스포츠도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210831 12:49"
        },
        {
          "id": "MDRW2100020784.1.22",
          "form": "오 되게 잘아시네요ㅋㅋㅋㅋㅋ",
          "original_form": "오 되게 잘아시네요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:59"
        },
        {
          "id": "MDRW2100020784.1.23",
