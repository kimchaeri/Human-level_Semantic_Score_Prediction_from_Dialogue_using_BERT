      "utterance": [
        {
          "id": "MDRW2100037850.1.1",
          "form": "저 진짜 피부로 인한 고통",
          "original_form": "저 진짜 피부로 인한 고통",
          "speaker_id": "2",
          "time": "20210901 19:40"
        },
        {
          "id": "MDRW2100037850.1.2",
          "form": "샴푸도 안써본게없어여ㅜ",
          "original_form": "샴푸도 안써본게없어여ㅜ",
          "speaker_id": "1",
          "time": "20210901 19:40"
        },
        {
          "id": "MDRW2100037850.1.3",
          "form": "너무 잘 알아서 속상해요",
          "original_form": "너무 잘 알아서 속상해요",
          "speaker_id": "2",
          "time": "20210901 19:40"
        },
        {
          "id": "MDRW2100037850.1.4",
          "form": "저는 겸사겸사 머리를 짧게 잘랐는대",
          "original_form": "저는 겸사겸사 머리를 짧게 잘랐는대",
          "speaker_id": "2",
          "time": "20210901 19:41"
        },
        {
          "id": "MDRW2100037850.1.5",
          "form": "피부가 눈에 확보이니까 더스트레스받는것같아요ㅜㅜ",
          "original_form": "피부가 눈에 확보이니까 더스트레스받는것같아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 19:41"
        },
        {
          "id": "MDRW2100037850.1.6",
          "form": "그러니까 많이 나아지긴 하더라구요ㅠ",
          "original_form": "그러니까 많이 나아지긴 하더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210901 19:41"
        },
        {
          "id": "MDRW2100037850.1.7",
          "form": "저도 항상 숏컷입니당...",
          "original_form": "저도 항상 숏컷입니당...",
          "speaker_id": "1",
          "time": "20210901 19:41"
        },
        {
          "id": "MDRW2100037850.1.8",
          "form": "맞아요 그리고 상대방 시선이.....",
          "original_form": "맞아요 그리고 상대방 시선이.....",
          "speaker_id": "2",
          "time": "20210901 19:41"
        },
        {
          "id": "MDRW2100037850.1.9",
          "form": "염색 살면서 해본적이없ㅇ어요",
          "original_form": "염색 살면서 해본적이없ㅇ어요",
          "speaker_id": "1",
          "time": "20210901 19:41"
        },
        {
          "id": "MDRW2100037850.1.10",
          "form": "앗 그러시구나 하긴 이게",
          "original_form": "앗 그러시구나 하긴 이게",
          "speaker_id": "2",
          "time": "20210901 19:41"
        },
        {
          "id": "MDRW2100037850.1.11",
          "form": "name1님 같은 경우는",
          "original_form": "&name1&님 같은 경우는",
          "speaker_id": "2",
          "time": "20210901 19:41"
        },
        {
          "id": "MDRW2100037850.1.12",
          "form": "피지가 많이 분비돼서 그런것도 아니라서",
          "original_form": "피지가 많이 분비돼서 그런것도 아니라서",
          "speaker_id": "2",
          "time": "20210901 19:41"
        },
        {
          "id": "MDRW2100037850.1.13",
          "form": "지금은 좀 나아도 만성적인거라 아시져 ㅅ음식잘못먹으면",
          "original_form": "지금은 좀 나아도 만성적인거라 아시져 ㅅ음식잘못먹으면",
          "speaker_id": "1",
          "time": "20210901 19:41"
        },
        {
          "id": "MDRW2100037850.1.14",
          "form": "머리길이는 상관이 없겠네요",
          "original_form": "머리길이는 상관이 없겠네요",
          "speaker_id": "2",
          "time": "20210901 19:41"
        },
        {
          "id": "MDRW2100037850.1.15",
          "form": "확 또올라오는거ㅜㅜ",
          "original_form": "확 또올라오는거ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100037850.1.16",
          "form": "네네네ㅜ",
          "original_form": "네네네ㅜ",
          "speaker_id": "2",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100037850.1.17",
          "form": "병원에서는 그냥 스테로이드만 처방해주던가요?",
          "original_form": "병원에서는 그냥 스테로이드만 처방해주던가요?",
          "speaker_id": "2",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100037850.1.18",
          "form": "저도 짧게자르는게 훨씬나아요",
          "original_form": "저도 짧게자르는게 훨씬나아요",
          "speaker_id": "1",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100037850.1.19",
          "form": "네 스테로이드랑 먹는약",
          "original_form": "네 스테로이드랑 먹는약",
          "speaker_id": "1",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100037850.1.20",
          "form": "그쵸 그리고 머리가 덜 비어보이더라구요 아예 짧으면..",
          "original_form": "그쵸 그리고 머리가 덜 비어보이더라구요 아예 짧으면..",
          "speaker_id": "2",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100037850.1.21",
          "form": "네ㅜ 머리말리기도중요하고..",
          "original_form": "네ㅜ 머리말리기도중요하고..",
          "speaker_id": "1",
