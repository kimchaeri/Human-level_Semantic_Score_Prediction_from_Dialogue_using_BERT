      "utterance": [
        {
          "id": "MDRW2100034524.1.1",
          "form": "오오 어디거요?",
          "original_form": "오오 어디거요?",
          "speaker_id": "2",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100034524.1.2",
          "form": "저는 배떡 맛있더라구요!",
          "original_form": "저는 배떡 맛있더라구요!",
          "speaker_id": "1",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100034524.1.3",
          "form": "사실 배떡이랑 태리로제밖에 안먹어봐서 비교할만한게 없지만ㅋㅋㅋ",
          "original_form": "사실 배떡이랑 태리로제밖에 안먹어봐서 비교할만한게 없지만ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100034524.1.4",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100034524.1.5",
          "form": "전 배떡 신전 삼첩이요 ㅋㅋㅋ",
          "original_form": "전 배떡 신전 삼첩이요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100034524.1.6",
          "form": "삼첩은 처음들어보네요",
          "original_form": "삼첩은 처음들어보네요",
          "speaker_id": "1",
          "time": "20210828 14:31"
        },
        {
          "id": "MDRW2100034524.1.7",
          "form": "서울 사시나요?",
          "original_form": "서울 사시나요?",
          "speaker_id": "1",
          "time": "20210828 14:31"
        },
        {
          "id": "MDRW2100034524.1.8",
          "form": "네 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:31"
        },
        {
          "id": "MDRW2100034524.1.9",
          "form": "삼첩분식이 배떡 뜨기전부터 살짝 핫했는데",
          "original_form": "삼첩분식이 배떡 뜨기전부터 살짝 핫했는데",
          "speaker_id": "2",
          "time": "20210828 14:31"
        },
        {
          "id": "MDRW2100034524.1.10",
          "form": "요샌 배떡이 워낙 인기라 ㅋㅋㅋ",
          "original_form": "요샌 배떡이 워낙 인기라 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:31"
        },
        {
          "id": "MDRW2100034524.1.11",
          "form": "역시 서울 살아야 이것저것 먹어보는데ㅠ",
          "original_form": "역시 서울 살아야 이것저것 먹어보는데ㅠ",
          "speaker_id": "1",
          "time": "20210828 14:31"
        },
        {
          "id": "MDRW2100034524.1.12",
          "form": "아하 삼첩이 원조인가요?",
          "original_form": "아하 삼첩이 원조인가요?",
          "speaker_id": "1",
          "time": "20210828 14:31"
        },
        {
          "id": "MDRW2100034524.1.13",
          "form": "넵 ㅋㅋㅋㅋ 근데 그전에",
          "original_form": "넵 ㅋㅋㅋㅋ 근데 그전에",
          "speaker_id": "2",
          "time": "20210828 14:32"
        },
        {
          "id": "MDRW2100034524.1.14",
          "form": "스쿨푸드아세요?",
          "original_form": "스쿨푸드아세요?",
          "speaker_id": "2",
          "time": "20210828 14:32"
        },
        {
          "id": "MDRW2100034524.1.15",
          "form": "요새 로제떡볶이 여기저기 다 나오더라구요",
          "original_form": "요새 로제떡볶이 여기저기 다 나오더라구요",
          "speaker_id": "1",
          "time": "20210828 14:32"
        },
        {
          "id": "MDRW2100034524.1.16",
          "form": "음..잘모르겠어요",
          "original_form": "음..잘모르겠어요",
          "speaker_id": "1",
          "time": "20210828 14:32"
        },
        {
          "id": "MDRW2100034524.1.17",
          "form": "거기가 원조인가요?",
          "original_form": "거기가 원조인가요?",
          "speaker_id": "1",
          "time": "20210828 14:33"
        },
        {
          "id": "MDRW2100034524.1.18",
          "form": "거기는",
          "original_form": "거기는",
          "speaker_id": "2",
          "time": "20210828 14:33"
        },
        {
          "id": "MDRW2100034524.1.19",
          "form": "로제는 아니구  매콤크림 떡이랑 파스타랑 같이 나오는데",
          "original_form": "로제는 아니구  매콤크림 떡이랑 파스타랑 같이 나오는데",
          "speaker_id": "2",
          "time": "20210828 14:33"
        },
        {
          "id": "MDRW2100034524.1.20",
          "form": "맛있어요 ㅋㅋㅋ",
          "original_form": "맛있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:33"
        },
        {
          "id": "MDRW2100034524.1.21",
          "form": "오 맛있겠네요",
          "original_form": "오 맛있겠네요",
          "speaker_id": "1",
          "time": "20210828 14:34"
        },
        {
          "id": "MDRW2100034524.1.22",
          "form": "떡볶이랑 파스타랑 같이 나오네요",
          "original_form": "떡볶이랑 파스타랑 같이 나오네요",
          "speaker_id": "1",
          "time": "20210828 14:35"
        },
        {
          "id": "MDRW2100034524.1.23",
          "form": "네 ㅋㅋㅋㅋ 좀 비싸여 근데",
          "original_form": "네 ㅋㅋㅋㅋ 좀 비싸여 근데",
          "speaker_id": "2",
          "time": "20210828 14:35"
        },
        {
          "id": "MDRW2100034524.1.24",
          "form": "2만 좀 안했던거같아요 ㅠ",
          "original_form": "2만 좀 안했던거같아요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 14:35"
        },
        {
          "id": "MDRW2100034524.1.25",
          "form": "만사천인가...?!",
          "original_form": "만사천인가...?!",
          "speaker_id": "2",
          "time": "20210828 14:35"
        },
        {
          "id": "MDRW2100034524.1.26",
          "form": "오 그러네요 비싸군요",
          "original_form": "오 그러네요 비싸군요",
          "speaker_id": "1",
          "time": "20210828 14:35"
        },
        {
          "id": "MDRW2100034524.1.27",
          "form": "양념만두도 맛있어보이네요",
          "original_form": "양념만두도 맛있어보이네요",
          "speaker_id": "1",
          "time": "20210828 14:35"
        },
        {
          "id": "MDRW2100034524.1.28",
          "form": "요새 떡볶이값이 너무 비싸요",
          "original_form": "요새 떡볶이값이 너무 비싸요",
          "speaker_id": "2",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100034524.1.29",
          "form": "양념만두도 맛있죠 ㅜㅜ",
          "original_form": "양념만두도 맛있죠 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 14:36"
        },
        {
          "id": "MDRW2100034524.1.30",
          "form": "제가 양념파라서 무조건 양념을 좋아해요 ㅌㅋ",
          "original_form": "제가 양념파라서 무조건 양념을 좋아해요 ㅌㅋ",
          "speaker_id": "2",
          "time": "20210828 14:36"
        }
      ]
    }
