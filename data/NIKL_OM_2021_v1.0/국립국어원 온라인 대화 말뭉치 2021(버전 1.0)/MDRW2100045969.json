      "utterance": [
        {
          "id": "MDRW2100045969.1.1",
          "form": "드라마는 어떤거 보새요",
          "original_form": "드라마는 어떤거 보새요",
          "speaker_id": "1",
          "time": "20210908 09:10"
        },
        {
          "id": "MDRW2100045969.1.2",
          "form": "저 드라마 챙겨보는걸 잘 못해서 ...",
          "original_form": "저 드라마 챙겨보는걸 잘 못해서 ...",
          "speaker_id": "2",
          "time": "20210908 09:10"
        },
        {
          "id": "MDRW2100045969.1.3",
          "form": "최근에 오월의 청춘 봤었어요",
          "original_form": "최근에 오월의 청춘 봤었어요",
          "speaker_id": "2",
          "time": "20210908 09:10"
        },
        {
          "id": "MDRW2100045969.1.4",
          "form": "마지막 2회는 아직 못봤지만요..",
          "original_form": "마지막 2회는 아직 못봤지만요..",
          "speaker_id": "2",
          "time": "20210908 09:11"
        },
        {
          "id": "MDRW2100045969.1.5",
          "form": "오오 저도 봣어요ㅠㅠㅠ",
          "original_form": "오오 저도 봣어요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 09:11"
        },
        {
          "id": "MDRW2100045969.1.6",
          "form": "울면서 봣어요 ㅠ",
          "original_form": "울면서 봣어요 ㅠ",
          "speaker_id": "1",
          "time": "20210908 09:11"
        },
        {
          "id": "MDRW2100045969.1.7",
          "form": "짐짜",
          "original_form": "짐짜",
          "speaker_id": "2",
          "time": "20210908 09:12"
        },
        {
          "id": "MDRW2100045969.1.8",
          "form": "한 4화까지는 너므 달달하고 행복했는데",
          "original_form": "한 4화까지는 너므 달달하고 행복했는데",
          "speaker_id": "2",
          "time": "20210908 09:12"
        },
        {
          "id": "MDRW2100045969.1.9",
          "form": "자꾸.. 5월이 다가온다는걸 암시하더니....",
          "original_form": "자꾸.. 5월이 다가온다는걸 암시하더니....",
          "speaker_id": "2",
          "time": "20210908 09:12"
        },
        {
          "id": "MDRW2100045969.1.10",
          "form": "그것도 설레는 투로ㅠㅋㅋㅋㅋㅋ",
          "original_form": "그것도 설레는 투로ㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:12"
        },
        {
          "id": "MDRW2100045969.1.11",
          "form": "달력을 비춰줄때마다 제뱔...제발..",
          "original_form": "달력을 비춰줄때마다 제뱔...제발..",
          "speaker_id": "2",
          "time": "20210908 09:12"
        },
        {
          "id": "MDRW2100045969.1.12",
          "form": "애초에 5.18을 주제로 한 드라만데 제발 5월이 오지않길 바랬어요",
          "original_form": "애초에 5.18을 주제로 한 드라만데 제발 5월이 오지않길 바랬어요",
          "speaker_id": "2",
          "time": "20210908 09:12"
        },
        {
          "id": "MDRW2100045969.1.13",
          "form": "맞아요 ㅠㅠ 너무 슬퍼요 ㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "맞아요 ㅠㅠ 너무 슬퍼요 ㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 09:18"
        },
        {
          "id": "MDRW2100045969.1.14",
          "form": "휴지 꼭잇어야해요",
          "original_form": "휴지 꼭잇어야해요",
          "speaker_id": "1",
          "time": "20210908 09:18"
        },
        {
          "id": "MDRW2100045969.1.15",
          "form": "ㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 09:18"
        },
        {
          "id": "MDRW2100045969.1.16",
          "form": "얼른 남은 2화도 봐야할텐데요...",
          "original_form": "얼른 남은 2화도 봐야할텐데요...",
          "speaker_id": "2",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100045969.1.17",
          "form": "최근에 무슨 드라마 보셨어요?",
          "original_form": "최근에 무슨 드라마 보셨어요?",
          "speaker_id": "2",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100045969.1.18",
          "form": "저는 갯벌마을 차차차랑",
          "original_form": "저는 갯벌마을 차차차랑",
          "speaker_id": "1",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100045969.1.19",
          "form": "펜트 하우스요!!",
          "original_form": "펜트 하우스요!!",
          "speaker_id": "1",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100045969.1.20",
          "form": "어 그거 신민아 김선호 나오는 것 맞죠!",
          "original_form": "어 그거 신민아 김선호 나오는 것 맞죠!",
          "speaker_id": "2",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100045969.1.21",
          "form": "펜하 ㅋㅋㅋㅋㅋ 다들 ㅋㅋㅋㅋ 욕하면서 보더라구요",
          "original_form": "펜하 ㅋㅋㅋㅋㅋ 다들 ㅋㅋㅋㅋ 욕하면서 보더라구요",
          "speaker_id": "2",
          "time": "20210908 09:19"
        },
        {
          "id": "MDRW2100045969.1.22",
          "form": "맞아요!!! 김선호 너무 잘생겻어요 ㅋㅋㅋ",
          "original_form": "맞아요!!! 김선호 너무 잘생겻어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:20"
        },
        {
          "id": "MDRW2100045969.1.23",
          "form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045969.1.24",
          "form": "둘다 보조개가 깊어서",
          "original_form": "둘다 보조개가 깊어서",
          "speaker_id": "2",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045969.1.25",
          "form": "어",
          "original_form": "어",
          "speaker_id": "1",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045969.1.26",
          "form": "누가 더 깊나 ㅋㅋㅋ 인터뷰하는거 봣어요",
          "original_form": "누가 더 깊나 ㅋㅋㅋ 인터뷰하는거 봣어요",
          "speaker_id": "2",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045969.1.27",
          "form": "그러고보니 둘다 보조개가 ㅋㅋㅋㅋ",
          "original_form": "그러고보니 둘다 보조개가 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045969.1.28",
          "form": "장난아니게 파여있졐ㅋㅋㅋ",
          "original_form": "장난아니게 파여있졐ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045969.1.29",
          "form": "맞아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045969.1.30",
          "form": "신민아 ㅠㅠ",
          "original_form": "신민아 ㅠㅠ",
