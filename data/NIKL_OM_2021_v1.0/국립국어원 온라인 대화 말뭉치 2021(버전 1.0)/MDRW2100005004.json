      "utterance": [
        {
          "id": "MDRW2100005004.1.1",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 19:51"
        },
        {
          "id": "MDRW2100005004.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210821 19:53"
        },
        {
          "id": "MDRW2100005004.1.3",
          "form": "여름에는 집안일하기 힘드네요",
          "original_form": "여름에는 집안일하기 힘드네요",
          "speaker_id": "2",
          "time": "20210821 19:53"
        },
        {
          "id": "MDRW2100005004.1.4",
          "form": "맞아요 특히 요리할때요",
          "original_form": "맞아요 특히 요리할때요",
          "speaker_id": "1",
          "time": "20210821 19:53"
        },
        {
          "id": "MDRW2100005004.1.5",
          "form": "끓이고 볶고 더워서 혼났어요",
          "original_form": "끓이고 볶고 더워서 혼났어요",
          "speaker_id": "1",
          "time": "20210821 19:54"
        },
        {
          "id": "MDRW2100005004.1.6",
          "form": "냄새때문에 환기하니 에어컨 꺼야하니 더워서 힘들었어요",
          "original_form": "냄새때문에 환기하니 에어컨 꺼야하니 더워서 힘들었어요",
          "speaker_id": "1",
          "time": "20210821 19:54"
        },
        {
          "id": "MDRW2100005004.1.7",
          "form": "맞아요 불앞에서 요리하면..휴",
          "original_form": "맞아요 불앞에서 요리하면..휴",
          "speaker_id": "2",
          "time": "20210821 19:55"
        },
        {
          "id": "MDRW2100005004.1.8",
          "form": "요샌 그나마 시원해서 좋네요 ㅎㅎ",
          "original_form": "요샌 그나마 시원해서 좋네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 19:55"
        },
        {
          "id": "MDRW2100005004.1.9",
          "form": "그래서 배달 넘 많이 시켜먹었어요 ㅋㅋ",
          "original_form": "그래서 배달 넘 많이 시켜먹었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:55"
        },
        {
          "id": "MDRW2100005004.1.10",
          "form": "아이들은 있으세요?",
          "original_form": "아이들은 있으세요?",
          "speaker_id": "1",
          "time": "20210821 19:55"
        },
        {
          "id": "MDRW2100005004.1.11",
          "form": "전 초등생 고등학생 있어요 ㅎㅎ",
          "original_form": "전 초등생 고등학생 있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 19:56"
        },
        {
          "id": "MDRW2100005004.1.12",
          "form": "네 4인가족이에요",
          "original_form": "네 4인가족이에요",
          "speaker_id": "2",
          "time": "20210821 19:57"
        },
        {
          "id": "MDRW2100005004.1.13",
          "form": "청소할때도 문열고하니 더워서 하고나면 샤워 바로 했었어요",
          "original_form": "청소할때도 문열고하니 더워서 하고나면 샤워 바로 했었어요",
          "speaker_id": "1",
          "time": "20210821 19:57"
        },
        {
          "id": "MDRW2100005004.1.14",
          "form": "초딩 유딩  있어요",
          "original_form": "초딩 유딩  있어요",
          "speaker_id": "2",
          "time": "20210821 19:57"
        },
        {
          "id": "MDRW2100005004.1.15",
          "form": "저랑같네요 ㅎㅎ",
          "original_form": "저랑같네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 19:57"
        },
        {
          "id": "MDRW2100005004.1.16",
          "form": "아 귀여울 때네요 ㅎㅎ",
          "original_form": "아 귀여울 때네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 19:58"
        },
        {
          "id": "MDRW2100005004.1.17",
          "form": "저희애들커서 사춘기아이도 있어서 눈치보며 살아요",
          "original_form": "저희애들커서 사춘기아이도 있어서 눈치보며 살아요",
          "speaker_id": "1",
          "time": "20210821 19:58"
        },
        {
          "id": "MDRW2100005004.1.18",
          "form": "귀엽긴한데 말을 너무 안들어서 힘들어요ㅠㅠ",
          "original_form": "귀엽긴한데 말을 너무 안들어서 힘들어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 19:58"
        },
        {
          "id": "MDRW2100005004.1.19",
          "form": "커도 안들어요 ㅜㅜ",
          "original_form": "커도 안들어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 19:58"
        },
        {
          "id": "MDRW2100005004.1.20",
          "form": "흑ㅠㅠ 벌써 걱정이네요",
          "original_form": "흑ㅠㅠ 벌써 걱정이네요",
          "speaker_id": "2",
          "time": "20210821 19:58"
        },
        {
          "id": "MDRW2100005004.1.21",
          "form": "말안들으려고 테어났나봐요 ㅋㅋ",
          "original_form": "말안들으려고 테어났나봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:59"
        },
        {
          "id": "MDRW2100005004.1.22",
          "form": "사춘기에는 흑염룡이 돼요",
          "original_form": "사춘기에는 흑염룡이 돼요",
          "speaker_id": "1",
          "time": "20210821 19:59"
        },
        {
          "id": "MDRW2100005004.1.23",
          "form": "불을 뿜어요",
          "original_form": "불을 뿜어요",
          "speaker_id": "1",
          "time": "20210821 19:59"
        },
        {
          "id": "MDRW2100005004.1.24",
          "form": "걱정많으시죠",
          "original_form": "걱정많으시죠",
          "speaker_id": "1",
          "time": "20210821 19:59"
        },
        {
          "id": "MDRW2100005004.1.25",
          "form": "아이들은 커도 어려도 키우기 힘든거 같아요",
          "original_form": "아이들은 커도 어려도 키우기 힘든거 같아요",
          "speaker_id": "1",
          "time": "20210821 19:59"
        },
        {
          "id": "MDRW2100005004.1.26",
          "form": "정말 답을 알고 싶어요",
          "original_form": "정말 답을 알고 싶어요",
          "speaker_id": "1",
          "time": "20210821 20:00"
        },
        {
          "id": "MDRW2100005004.1.27",
          "form": "그러니까요ㅜ 어떻게 키워야할지 항상 고민이네요",
          "original_form": "그러니까요ㅜ 어떻게 키워야할지 항상 고민이네요",
          "speaker_id": "2",
          "time": "20210821 20:00"
        },
        {
          "id": "MDRW2100005004.1.28",
          "form": "아버님이 많이 도와주셔야 겠네요 애들이 어려서 힘드니",
          "original_form": "아버님이 많이 도와주셔야 겠네요 애들이 어려서 힘드니",
          "speaker_id": "1",
          "time": "20210821 20:00"
        },
        {
          "id": "MDRW2100005004.1.29",
          "form": "네 저희딸 유치원전에 바닥에 들어 눕고 그랬어요 ㅜㅜ",
          "original_form": "네 저희딸 유치원전에 바닥에 들어 눕고 그랬어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 20:01"
        },
        {
          "id": "MDRW2100005004.1.30",
          "form": "저 정말 울고싶었죠",
          "original_form": "저 정말 울고싶었죠",
          "speaker_id": "1",
          "time": "20210821 20:01"
        },
        {
          "id": "MDRW2100005004.1.31",
          "form": "근데 지금은 예의가 바르고 그래요",
          "original_form": "근데 지금은 예의가 바르고 그래요",
          "speaker_id": "1",
          "time": "20210821 20:01"
        },
        {
          "id": "MDRW2100005004.1.32",
          "form": "참 지금도 이해가 안가요",
          "original_form": "참 지금도 이해가 안가요",
          "speaker_id": "1",
          "time": "20210821 20:01"
        },
        {
          "id": "MDRW2100005004.1.33",
          "form": "어릴때 애들은 다들 바닥에 눕나보네요",
          "original_form": "어릴때 애들은 다들 바닥에 눕나보네요",
          "speaker_id": "2",
          "time": "20210821 20:02"
        },
        {
          "id": "MDRW2100005004.1.34",
          "form": "네 그런가봐요",
          "original_form": "네 그런가봐요",
          "speaker_id": "1",
          "time": "20210821 20:02"
        },
        {
          "id": "MDRW2100005004.1.35",
          "form": "지금 생각해도 아찔해요",
          "original_form": "지금 생각해도 아찔해요",
          "speaker_id": "1",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100005004.1.36",
          "form": "그럴땐 뭘 어찌 할지 모르겠더라구요",
          "original_form": "그럴땐 뭘 어찌 할지 모르겠더라구요",
          "speaker_id": "1",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100005004.1.37",
          "form": "육아가 힘들긴해요ㅠㅠ",
          "original_form": "육아가 힘들긴해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 20:03"
        }
