      "utterance": [
        {
          "id": "MDRW2100037876.1.1",
          "form": "우아 빠르다증말ㅋ",
          "original_form": "우아 빠르다증말ㅋ",
          "speaker_id": "2",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100037876.1.2",
          "form": "ㅋㅋㅋ언니 나 이제재우러갑니다ㅋ",
          "original_form": "ㅋㅋㅋ언니 나 이제재우러갑니다ㅋ",
          "speaker_id": "1",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100037876.1.3",
          "form": "오냐;",
          "original_form": "오냐;",
          "speaker_id": "2",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100037876.1.4",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100037876.1.5",
          "form": "아ㅡㅡ진짜  둘째 안자서 돌겠네유",
          "original_form": "아ㅡㅡ진짜  둘째 안자서 돌겠네유",
          "speaker_id": "1",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100037876.1.6",
          "form": "우리도30분째  뒹굴이다ㅋ",
          "original_form": "우리도30분째  뒹굴이다ㅋ",
          "speaker_id": "2",
          "time": "20210901 21:35"
        },
        {
          "id": "MDRW2100037876.1.7",
          "form": "10시되야자것다ㅡㅡ",
          "original_form": "10시되야자것다ㅡㅡ",
          "speaker_id": "2",
          "time": "20210901 21:35"
        },
        {
          "id": "MDRW2100037876.1.8",
          "form": "10시도애교죠",
          "original_form": "10시도애교죠",
          "speaker_id": "1",
          "time": "20210901 21:35"
        },
        {
          "id": "MDRW2100037876.1.9",
          "form": "진짜 씅나네요",
          "original_form": "진짜 씅나네요",
          "speaker_id": "1",
          "time": "20210901 21:35"
        },
        {
          "id": "MDRW2100037876.1.10",
          "form": "지안이는 10분만에잠들었는데",
          "original_form": "지안이는 10분만에잠들었는데",
          "speaker_id": "1",
          "time": "20210901 21:35"
        },
        {
          "id": "MDRW2100037876.1.11",
          "form": "두찌는 1시간넘게저카고있음",
          "original_form": "두찌는 1시간넘게저카고있음",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100037876.1.12",
          "form": "우린둘다 뒹굴이다ㅡㅡ",
          "original_form": "우린둘다 뒹굴이다ㅡㅡ",
          "speaker_id": "2",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100037876.1.13",
          "form": "더 피곤하게 해야되나?ㅡㅡ",
          "original_form": "더 피곤하게 해야되나?ㅡㅡ",
          "speaker_id": "2",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100037876.1.14",
          "form": "그러니깐요",
          "original_form": "그러니깐요",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100037876.1.15",
          "form": "운동이라도 보내고싶네요",
          "original_form": "운동이라도 보내고싶네요",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100037876.1.16",
          "form": "진짜ㅋㅋ",
          "original_form": "진짜ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100037876.1.17",
          "form": "우린 운동하는데도 그렇다",
          "original_form": "우린 운동하는데도 그렇다",
          "speaker_id": "2",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100037876.1.18",
          "form": "에휴",
          "original_form": "에휴",
          "speaker_id": "2",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100037876.1.19",
          "form": "진짜자기싫은듯",
          "original_form": "진짜자기싫은듯",
          "speaker_id": "2",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100037876.1.20",
          "form": "아 언니 그래도 30분은 애교라요",
          "original_form": "아 언니 그래도 30분은 애교라요",
          "speaker_id": "1",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100037876.1.21",
          "form": "정말 .....이제 잠들었어요 둘째",
          "original_form": "정말 .....이제 잠들었어요 둘째",
          "speaker_id": "1",
          "time": "20210901 22:43"
        },
        {
          "id": "MDRW2100037876.1.22",
          "form": "축하한다",
          "original_form": "축하한다",
          "speaker_id": "2",
          "time": "20210901 22:44"
        },
        {
          "id": "MDRW2100037876.1.23",
