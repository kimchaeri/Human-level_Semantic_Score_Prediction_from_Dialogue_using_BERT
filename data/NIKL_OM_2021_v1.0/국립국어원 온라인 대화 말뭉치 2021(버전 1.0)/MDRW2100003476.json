      "utterance": [
        {
          "id": "MDRW2100003476.1.1",
          "form": "아근데",
          "original_form": "아근데",
          "speaker_id": "2",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100003476.1.2",
          "form": "나 이번에 수강신청",
          "original_form": "나 이번에 수강신청",
          "speaker_id": "2",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100003476.1.3",
          "form": "완전 성공해서 기분좋다 킼킼ㅋ",
          "original_form": "완전 성공해서 기분좋다 킼킼ㅋ",
          "speaker_id": "2",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100003476.1.4",
          "form": "나도 내년에",
          "original_form": "나도 내년에",
          "speaker_id": "1",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100003476.1.5",
          "form": "수강신청이란걸 하겠지??",
          "original_form": "수강신청이란걸 하겠지??",
          "speaker_id": "1",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100003476.1.6",
          "form": "히히",
          "original_form": "히히",
          "speaker_id": "1",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100003476.1.7",
          "form": "기대된다잉",
          "original_form": "기대된다잉",
          "speaker_id": "1",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100003476.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100003476.1.9",
          "form": "나는 너가",
          "original_form": "나는 너가",
          "speaker_id": "2",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100003476.1.10",
          "form": "어디학교 갈지가 더 기대돼",
          "original_form": "어디학교 갈지가 더 기대돼",
          "speaker_id": "2",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100003476.1.11",
          "form": "우히히",
          "original_form": "우히히",
          "speaker_id": "1",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100003476.1.12",
          "form": "나는 일단 유교과갈거얍",
          "original_form": "나는 일단 유교과갈거얍",
          "speaker_id": "1",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100003476.1.13",
          "form": "어느학교든지!",
          "original_form": "어느학교든지!",
          "speaker_id": "1",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100003476.1.14",
          "form": "유교과 가자으아아아",
          "original_form": "유교과 가자으아아아",
          "speaker_id": "2",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100003476.1.15",
          "form": "좋아좋아",
          "original_form": "좋아좋아",
          "speaker_id": "2",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100003476.1.16",
          "form": "아근데",
          "original_form": "아근데",
          "speaker_id": "2",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100003476.1.17",
          "form": "내가지금 아이스크림을 사려고하거든?",
          "original_form": "내가지금 아이스크림을 사려고하거든?",
          "speaker_id": "2",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100003476.1.18",
          "form": "엉",
          "original_form": "엉",
          "speaker_id": "1",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100003476.1.19",
          "form": "어떤거",
          "original_form": "어떤거",
          "speaker_id": "1",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100003476.1.20",
          "form": "일단 나뚜루랑",
          "original_form": "일단 나뚜루랑",
          "speaker_id": "2",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100003476.1.21",
          "form": "밴앤케이크?",
          "original_form": "밴앤케이크?",
          "speaker_id": "2",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100003476.1.22",
          "form": "거기를 사려고하는데",
          "original_form": "거기를 사려고하는데",
          "speaker_id": "2",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100003476.1.23",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100003476.1.24",
          "form": "맛잇겟다리",
          "original_form": "맛잇겟다리",
          "speaker_id": "1",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100003476.1.25",
          "form": "키킼 맞지",
          "original_form": "키킼 맞지",
          "speaker_id": "2",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100003476.1.26",
          "form": "근데 둘다 배송비가 5000원이야",
          "original_form": "근데 둘다 배송비가 5000원이야",
          "speaker_id": "2",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100003476.1.27",
          "form": "아 비싸다...",
          "original_form": "아 비싸다...",
          "speaker_id": "1",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100003476.1.28",
          "form": "응 그래서",
          "original_form": "응 그래서",
          "speaker_id": "2",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100003476.1.29",
          "form": "지금 고민인게",
          "original_form": "지금 고민인게",
          "speaker_id": "2",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100003476.1.30",
          "form": "둘중에 하나만 산다면 멀살까?",
          "original_form": "둘중에 하나만 산다면 멀살까?",
          "speaker_id": "2",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100003476.1.31",
          "form": "맛은 무슨무슨맛이야",
          "original_form": "맛은 무슨무슨맛이야",
          "speaker_id": "1",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100003476.1.32",
          "form": "???",
          "original_form": "???",
          "speaker_id": "1",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100003476.1.33",
          "form": "나뚜루는",
          "original_form": "나뚜루는",
          "speaker_id": "2",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100003476.1.34",
          "form": "딸기초코바닐라고",
          "original_form": "딸기초코바닐라고",
          "speaker_id": "2",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100003476.1.35",
          "form": "엉엉",
          "original_form": "엉엉",
          "speaker_id": "1",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100003476.1.36",
          "form": "글고",
          "original_form": "글고",
          "speaker_id": "1",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100003476.1.37",
          "form": "밴앤 어쩌고는",
          "original_form": "밴앤 어쩌고는",
          "speaker_id": "2",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100003476.1.38",
          "form": "좀 다양해",
          "original_form": "좀 다양해",
          "speaker_id": "2",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100003476.1.39",
          "form": "무슨맛인데",
          "original_form": "무슨맛인데",
          "speaker_id": "1",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100003476.1.40",
          "form": "베라같은맛??",
          "original_form": "베라같은맛??",
          "speaker_id": "1",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100003476.1.41",
          "form": "노노 베라는아닌대",
          "original_form": "노노 베라는아닌대",
          "speaker_id": "2",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100003476.1.42",
          "form": "데",
          "original_form": "데",
          "speaker_id": "2",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100003476.1.43",
          "form": "얘네만의 특별한맛",
          "original_form": "얘네만의 특별한맛",
          "speaker_id": "2",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100003476.1.44",
          "form": "음...ㅡ",
          "original_form": "음...ㅡ",
          "speaker_id": "1",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100003476.1.45",
          "form": "가격은?",
          "original_form": "가격은?",
          "speaker_id": "1",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100003476.1.46",
          "form": "가격은",
          "original_form": "가격은",
          "speaker_id": "2",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100003476.1.47",
          "form": "밴앤이랑 나뚜루 둘다",
          "original_form": "밴앤이랑 나뚜루 둘다",
          "speaker_id": "2",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100003476.1.48",
          "form": "세일하는데",
          "original_form": "세일하는데",
          "speaker_id": "2",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100003476.1.49",
          "form": "밴앤은 8천원대",
          "original_form": "밴앤은 8천원대",
          "speaker_id": "2",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100003476.1.50",
          "form": "나뚜루는 5천원대야",
          "original_form": "나뚜루는 5천원대야",
          "speaker_id": "2",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100003476.1.51",
          "form": "그럼",
          "original_form": "그럼",
          "speaker_id": "1",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100003476.1.52",
          "form": "나뚜루로하자",
          "original_form": "나뚜루로하자",
          "speaker_id": "1",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100003476.1.53",
          "form": "ㅋㅋㅋㅋㅋ오키",
          "original_form": "ㅋㅋㅋㅋㅋ오키",
          "speaker_id": "2",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100003476.1.54",
          "form": "역시 가격이구만",
          "original_form": "역시 가격이구만",
          "speaker_id": "2",
          "time": "20210825 13:39"
        }
      ]
