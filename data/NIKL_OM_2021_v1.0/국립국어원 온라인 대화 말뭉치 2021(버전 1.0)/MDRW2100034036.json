      "utterance": [
        {
          "id": "MDRW2100034036.1.1",
          "form": "저 카페인이 안받아서 안마셔요",
          "original_form": "저 카페인이 안받아서 안마셔요",
          "speaker_id": "2",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100034036.1.2",
          "form": "드시는편이세요?",
          "original_form": "드시는편이세요?",
          "speaker_id": "2",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100034036.1.3",
          "form": "가끔 마셔요ㅎㅎ 좋아하는 음료수 있으세요?",
          "original_form": "가끔 마셔요ㅎㅎ 좋아하는 음료수 있으세요?",
          "speaker_id": "1",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100034036.1.4",
          "form": "저는 아침햇살이요",
          "original_form": "저는 아침햇살이요",
          "speaker_id": "2",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100034036.1.5",
          "form": "뭐 좋아하세요?",
          "original_form": "뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100034036.1.6",
          "form": "전 딸기우유요! 데자와나 솔의눈도 드시나요?",
          "original_form": "전 딸기우유요! 데자와나 솔의눈도 드시나요?",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100034036.1.7",
          "form": "아니요.. 데자와 솔의눈 안먹어요ㅠㅠ",
          "original_form": "아니요.. 데자와 솔의눈 안먹어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100034036.1.8",
          "form": "생딸기우유 카페에서 많이 파는거 진짜 맛있던데 먹어보셨나요?",
          "original_form": "생딸기우유 카페에서 많이 파는거 진짜 맛있던데 먹어보셨나요?",
          "speaker_id": "2",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100034036.1.9",
          "form": "너무좋아요~ 가격이 좀 부담되지만ㅜㅜ",
          "original_form": "너무좋아요~ 가격이 좀 부담되지만ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 12:41"
        },
        {
          "id": "MDRW2100034036.1.10",
          "form": "맞아요 가격이 조금 부담이죠ㅠㅠ",
          "original_form": "맞아요 가격이 조금 부담이죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 12:41"
        },
        {
          "id": "MDRW2100034036.1.11",
          "form": "겨울되면 집에서 해먹으려구요",
          "original_form": "겨울되면 집에서 해먹으려구요",
          "speaker_id": "2",
          "time": "20210827 12:41"
        },
        {
          "id": "MDRW2100034036.1.12",
          "form": "오 레시피있나요?",
          "original_form": "오 레시피있나요?",
          "speaker_id": "1",
          "time": "20210827 12:43"
        },
        {
          "id": "MDRW2100034036.1.13",
          "form": "생딸기썰구요 설탕하고 1대1로 섞어서 청을 만든후에 우유에 청 타먹으면 그러저럭 맛있어요",
          "original_form": "생딸기썰구요 설탕하고 1대1로 섞어서 청을 만든후에 우유에 청 타먹으면 그러저럭 맛있어요",
          "speaker_id": "2",
          "time": "20210827 12:50"
        },
        {
          "id": "MDRW2100034036.1.14",
          "form": "딸기 떨이하는거 사서 매년 먹어요",
          "original_form": "딸기 떨이하는거 사서 매년 먹어요",
          "speaker_id": "2",
          "time": "20210827 12:51"
        },
        {
          "id": "MDRW2100034036.1.15",
          "form": "다음에 꼭 해먹어볼게요!",
          "original_form": "다음에 꼭 해먹어볼게요!",
          "speaker_id": "1",
          "time": "20210827 12:52"
        },
        {
          "id": "MDRW2100034036.1.16",
          "form": "잘하는 음식 있으세요?",
          "original_form": "잘하는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210827 12:52"
        },
        {
          "id": "MDRW2100034036.1.17",
          "form": "잘은 모르겠는데 자주 해먹는건",
          "original_form": "잘은 모르겠는데 자주 해먹는건",
          "speaker_id": "2",
          "time": "20210827 13:05"
        },
        {
          "id": "MDRW2100034036.1.18",
          "form": "닭도리탕이랑 콩불 자주해먹어요",
          "original_form": "닭도리탕이랑 콩불 자주해먹어요",
          "speaker_id": "2",
          "time": "20210827 13:06"
        },
        {
          "id": "MDRW2100034036.1.19",
          "form": "콩불에 고기 어느부위 넣으세요??",
          "original_form": "콩불에 고기 어느부위 넣으세요??",
          "speaker_id": "1",
          "time": "20210827 13:06"
        },
        {
          "id": "MDRW2100034036.1.20",
          "form": "저는 대패삼겹살이요",
          "original_form": "저는 대패삼겹살이요",
          "speaker_id": "2",
          "time": "20210827 13:06"
        }
