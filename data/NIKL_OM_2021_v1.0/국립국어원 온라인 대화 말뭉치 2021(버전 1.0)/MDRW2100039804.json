      "utterance": [
        {
          "id": "MDRW2100039804.1.1",
          "form": "저 지금 츄천햐쥬신 노래 틀었어요",
          "original_form": "저 지금 츄천햐쥬신 노래 틀었어요",
          "speaker_id": "2",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100039804.1.2",
          "form": "푸싱앤폴링~~",
          "original_form": "푸싱앤폴링~~",
          "speaker_id": "2",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100039804.1.3",
          "form": "플랭크와 함께해보겠습니다",
          "original_form": "플랭크와 함께해보겠습니다",
          "speaker_id": "2",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100039804.1.4",
          "form": "ㅋㅋㅋ파이팅!",
          "original_form": "ㅋㅋㅋ파이팅!",
          "speaker_id": "1",
          "time": "20210903 21:47"
        },
        {
          "id": "MDRW2100039804.1.5",
          "form": "앗 노래 좋아요!!",
          "original_form": "앗 노래 좋아요!!",
          "speaker_id": "2",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039804.1.6",
          "form": "첨엔 아무생각없이 들었는데 뭔가 가사도 재밌는거같아요",
          "original_form": "첨엔 아무생각없이 들었는데 뭔가 가사도 재밌는거같아요",
          "speaker_id": "2",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039804.1.7",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039804.1.8",
          "form": "전 멤버들이 다같이 훅 부르는게 너무 좋더라고요",
          "original_form": "전 멤버들이 다같이 훅 부르는게 너무 좋더라고요",
          "speaker_id": "1",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039804.1.9",
          "form": "그러게요!! 전 첨에 들어갈때",
          "original_form": "그러게요!! 전 첨에 들어갈때",
          "speaker_id": "2",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100039804.1.10",
          "form": "슬기죠?",
          "original_form": "슬기죠?",
          "speaker_id": "2",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100039804.1.11",
          "form": "첨부분도 넘 좋아요",
          "original_form": "첨부분도 넘 좋아요",
          "speaker_id": "2",
          "time": "20210903 21:50"
        },
        {
          "id": "MDRW2100039804.1.12",
          "form": "노래하는거요?",
          "original_form": "노래하는거요?",
          "speaker_id": "1",
          "time": "20210903 22:00"
        },
        {
          "id": "MDRW2100039804.1.13",
          "form": "맞아요 가사맛 잘 살린 것 같아요",
          "original_form": "맞아요 가사맛 잘 살린 것 같아요",
          "speaker_id": "1",
          "time": "20210903 22:00"
        },
        {
          "id": "MDRW2100039804.1.14",
          "form": "맞아용 전 레벨에서 슬기 음색을 제일 좋아하거든요!",
          "original_form": "맞아용 전 레벨에서 슬기 음색을 제일 좋아하거든요!",
          "speaker_id": "2",
          "time": "20210903 22:00"
        },
        {
          "id": "MDRW2100039804.1.15",
          "form": "조이도 좋지만 슬기를 너무 사랑해버렸어요..ㅠㅠ",
          "original_form": "조이도 좋지만 슬기를 너무 사랑해버렸어요..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 22:01"
        },
        {
          "id": "MDRW2100039804.1.16",
          "form": "전 조이 음색 제일 좋아해요",
          "original_form": "전 조이 음색 제일 좋아해요",
          "speaker_id": "1",
          "time": "20210903 22:01"
        },
        {
          "id": "MDRW2100039804.1.17",
          "form": "솔로 앨범 나와서 너무 좋았어요ㅋㅋ",
          "original_form": "솔로 앨범 나와서 너무 좋았어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:01"
        },
        {
          "id": "MDRW2100039804.1.18",
          "form": "맞아요 조이 음색은 진짜 보물인거같아요",
          "original_form": "맞아요 조이 음색은 진짜 보물인거같아요",
          "speaker_id": "2",
          "time": "20210903 22:02"
        },
        {
          "id": "MDRW2100039804.1.19",
          "form": "진짜 누군가가 따라할수없는 특별한 음색..!!",
          "original_form": "진짜 누군가가 따라할수없는 특별한 음색..!!",
          "speaker_id": "2",
          "time": "20210903 22:02"
        },
        {
          "id": "MDRW2100039804.1.20",
          "form": "맞아요 옥구슬 그 자체잖아요",
          "original_form": "맞아요 옥구슬 그 자체잖아요",
          "speaker_id": "1",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100039804.1.21",
          "form": "솔로 앨범 중에서 어떤 노래 제일 좋아하세요?",
          "original_form": "솔로 앨범 중에서 어떤 노래 제일 좋아하세요?",
