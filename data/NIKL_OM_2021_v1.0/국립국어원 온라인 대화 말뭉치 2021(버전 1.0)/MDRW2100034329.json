      "utterance": [
        {
          "id": "MDRW2100034329.1.1",
          "form": "저녁 드셨나요?ㅎㅎ",
          "original_form": "저녁 드셨나요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 21:28"
        },
        {
          "id": "MDRW2100034329.1.2",
          "form": "네 먹엇어요 ㅎㅎ",
          "original_form": "네 먹엇어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 21:28"
        },
        {
          "id": "MDRW2100034329.1.3",
          "form": "저녁식사로 뭐 드셨어요?",
          "original_form": "저녁식사로 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210904 21:29"
        },
        {
          "id": "MDRW2100034329.1.4",
          "form": "nbb 버거요 ㅎㅎ",
          "original_form": "nbb 버거요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 21:29"
        },
        {
          "id": "MDRW2100034329.1.5",
          "form": "오 처음들어봐요",
          "original_form": "오 처음들어봐요",
          "speaker_id": "1",
          "time": "20210904 21:30"
        },
        {
          "id": "MDRW2100034329.1.6",
          "form": "nbb버거",
          "original_form": "nbb버거",
          "speaker_id": "1",
          "time": "20210904 21:30"
        },
        {
          "id": "MDRW2100034329.1.7",
          "form": "노브랜드 버거요 ㅎㅎ",
          "original_form": "노브랜드 버거요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 21:31"
        },
        {
          "id": "MDRW2100034329.1.8",
          "form": "nbb 라고 하더라구요 ㄹㄹ",
          "original_form": "nbb 라고 하더라구요 ㄹㄹ",
          "speaker_id": "2",
          "time": "20210904 21:31"
        },
        {
          "id": "MDRW2100034329.1.9",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210904 21:31"
        },
        {
          "id": "MDRW2100034329.1.10",
          "form": "노브랜드버거 얘기는 많이 들었는데",
          "original_form": "노브랜드버거 얘기는 많이 들었는데",
          "speaker_id": "1",
          "time": "20210904 21:31"
        },
        {
          "id": "MDRW2100034329.1.11",
          "form": "한번도 못먹어봤어요",
          "original_form": "한번도 못먹어봤어요",
          "speaker_id": "1",
          "time": "20210904 21:31"
        },
        {
          "id": "MDRW2100034329.1.12",
          "form": "저도 집근터 생겨서 첨 가봣어여 ㅎㅎ",
          "original_form": "저도 집근터 생겨서 첨 가봣어여 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 21:32"
        },
        {
          "id": "MDRW2100034329.1.13",
          "form": "맛은 soso해요 ㅎㅎ",
          "original_form": "맛은 soso해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 21:32"
        },
        {
          "id": "MDRW2100034329.1.14",
          "form": "가성비는 좋은거 같아요",
          "original_form": "가성비는 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210904 21:33"
        },
        {
          "id": "MDRW2100034329.1.15",
          "form": "아 가성비가 좋은 햄버거라고 하던데",
          "original_form": "아 가성비가 좋은 햄버거라고 하던데",
          "speaker_id": "1",
          "time": "20210904 21:33"
        },
        {
          "id": "MDRW2100034329.1.16",
          "form": "햄버거 좋아하세요?",
          "original_form": "햄버거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 21:33"
        },
        {
          "id": "MDRW2100034329.1.17",
          "form": "차이카드 있음 더 할인 받구요",
          "original_form": "차이카드 있음 더 할인 받구요",
          "speaker_id": "2",
          "time": "20210904 21:33"
        },
        {
          "id": "MDRW2100034329.1.18",
          "form": "아 그렇구나",
          "original_form": "아 그렇구나",
          "speaker_id": "1",
          "time": "20210904 21:35"
        },
        {
          "id": "MDRW2100034329.1.19",
          "form": "딱히 아 좋아는 안니데  새로운거라 ㅎㅎ",
          "original_form": "딱히 아 좋아는 안니데  새로운거라 ㅎㅎ",
