      "utterance": [
        {
          "id": "MDRW2100043031.1.1",
          "form": "마지막 쪽지에",
          "original_form": "마지막 쪽지에",
          "speaker_id": "2",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100043031.1.2",
          "form": "어디로 가야하는지",
          "original_form": "어디로 가야하는지",
          "speaker_id": "2",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100043031.1.3",
          "form": "나와",
          "original_form": "나와",
          "speaker_id": "2",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100043031.1.4",
          "form": "나 이제 스승 죽은거 찾은거 보니",
          "original_form": "나 이제 스승 죽은거 찾은거 보니",
          "speaker_id": "1",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100043031.1.5",
          "form": "이제 첫번째 찾은듯...",
          "original_form": "이제 첫번째 찾은듯...",
          "speaker_id": "1",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100043031.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100043031.1.7",
          "form": "그래도....나름 화려한 보물상자니까......",
          "original_form": "그래도....나름 화려한 보물상자니까......",
          "speaker_id": "2",
          "time": "20210907 13:21"
        },
        {
          "id": "MDRW2100043031.1.8",
          "form": "험난한 상자깡의 과정...",
          "original_form": "험난한 상자깡의 과정...",
          "speaker_id": "1",
          "time": "20210907 13:30"
        },
        {
          "id": "MDRW2100043031.1.9",
          "form": "결국",
          "original_form": "결국",
          "speaker_id": "1",
          "time": "20210907 13:59"
        },
        {
          "id": "MDRW2100043031.1.10",
          "form": "공략을 펼쳣다",
          "original_form": "공략을 펼쳣다",
          "speaker_id": "1",
          "time": "20210907 13:59"
        },
        {
          "id": "MDRW2100043031.1.11",
          "form": "아니 누가 길안내를 이따위로 해요;;;",
          "original_form": "아니 누가 길안내를 이따위로 해요;;;",
          "speaker_id": "1",
          "time": "20210907 13:59"
        },
        {
          "id": "MDRW2100043031.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 14:00"
        },
        {
          "id": "MDRW2100043031.1.13",
          "form": "원소흔적 따라가면 벽인데...",
          "original_form": "원소흔적 따라가면 벽인데...",
          "speaker_id": "1",
          "time": "20210907 14:00"
        },
        {
          "id": "MDRW2100043031.1.14",
          "form": "첫번째꺼는 그나마",
          "original_form": "첫번째꺼는 그나마",
          "speaker_id": "2",
          "time": "20210907 14:00"
        },
        {
          "id": "MDRW2100043031.1.15",
          "form": "찾을만하레",
          "original_form": "찾을만하레",
          "speaker_id": "2",
          "time": "20210907 14:00"
        },
        {
          "id": "MDRW2100043031.1.16",
          "form": "한데",
          "original_form": "한데",
          "speaker_id": "2",
          "time": "20210907 14:00"
        },
        {
          "id": "MDRW2100043031.1.17",
          "form": "두번째 세번째는 진짜",
          "original_form": "두번째 세번째는 진짜",
          "speaker_id": "2",
          "time": "20210907 14:00"
        },
        {
          "id": "MDRW2100043031.1.18",
          "form": "절대 못찾아......",
          "original_form": "절대 못찾아......",
          "speaker_id": "2",
          "time": "20210907 14:00"
        },
        {
          "id": "MDRW2100043031.1.19",
          "form": "원소흔적을 왜 이모양으로 만들어둔...?",
          "original_form": "원소흔적을 왜 이모양으로 만들어둔...?",
          "speaker_id": "1",
          "time": "20210907 14:00"
        },
        {
          "id": "MDRW2100043031.1.20",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210907 14:03"
        },
        {
          "id": "MDRW2100043031.1.21",
          "form": "그리고 끝났으면 끝났다고 하던가",
          "original_form": "그리고 끝났으면 끝났다고 하던가",
          "speaker_id": "2",
          "time": "20210907 14:03"
        },
        {
          "id": "MDRW2100043031.1.22",
          "form": "어느 순간 원소흔적이 없으면",
          "original_form": "어느 순간 원소흔적이 없으면",
          "speaker_id": "2",
          "time": "20210907 14:03"
        },
        {
          "id": "MDRW2100043031.1.23",
          "form": "그 주변을 모조리 둘러봐야대",
          "original_form": "그 주변을 모조리 둘러봐야대",
          "speaker_id": "2",
          "time": "20210907 14:03"
        },
        {
          "id": "MDRW2100043031.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ 원소흔적으로 길찾게하는게",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 원소흔적으로 길찾게하는게",
          "speaker_id": "1",
          "time": "20210907 14:05"
        },
        {
          "id": "MDRW2100043031.1.25",
          "form": "젤 싫어",
          "original_form": "젤 싫어",
          "speaker_id": "1",
          "time": "20210907 14:05"
        },
        {
          "id": "MDRW2100043031.1.26",
          "form": "ㄹㅇ........",
          "original_form": "ㄹㅇ........",
          "speaker_id": "2",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043031.1.27",
          "form": "아니 적어도",
          "original_form": "아니 적어도",
          "speaker_id": "2",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043031.1.28",
          "form": "현상토벌도 그래서 몇번 타임아웃됨",
          "original_form": "현상토벌도 그래서 몇번 타임아웃됨",
          "speaker_id": "1",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043031.1.29",
          "form": "켜놓고 돌아다니게라도",
          "original_form": "켜놓고 돌아다니게라도",
          "speaker_id": "2",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043031.1.30",
          "form": "해주던가",
          "original_form": "해주던가",
          "speaker_id": "2",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043031.1.31",
          "form": "왜 움직이면 꺼지는거야",
          "original_form": "왜 움직이면 꺼지는거야",
          "speaker_id": "2",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043031.1.32",
          "form": "그니까 ㅋㅋㅋㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043031.1.33",
          "form": "길 못찾겟다고...!",
          "original_form": "길 못찾겟다고...!",
          "speaker_id": "1",
          "time": "20210907 14:06"
        },
        {
          "id": "MDRW2100043031.1.34",
