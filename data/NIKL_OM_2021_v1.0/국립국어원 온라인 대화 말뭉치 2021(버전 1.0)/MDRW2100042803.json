      "utterance": [
        {
          "id": "MDRW2100042803.1.1",
          "form": "혼자사시면 본인을위해쇼핑많이하시겠네요",
          "original_form": "혼자사시면 본인을위해쇼핑많이하시겠네요",
          "speaker_id": "2",
          "time": "20210907 09:22"
        },
        {
          "id": "MDRW2100042803.1.2",
          "form": "네 최근에 이사하면서 많이 샀어요",
          "original_form": "네 최근에 이사하면서 많이 샀어요",
          "speaker_id": "1",
          "time": "20210907 09:23"
        },
        {
          "id": "MDRW2100042803.1.3",
          "form": "가장 최근엔 센스맘매트리스 사구요",
          "original_form": "가장 최근엔 센스맘매트리스 사구요",
          "speaker_id": "1",
          "time": "20210907 09:23"
        },
        {
          "id": "MDRW2100042803.1.4",
          "form": "그러시겠네요",
          "original_form": "그러시겠네요",
          "speaker_id": "2",
          "time": "20210907 09:23"
        },
        {
          "id": "MDRW2100042803.1.5",
          "form": "살림사시느라",
          "original_form": "살림사시느라",
          "speaker_id": "2",
          "time": "20210907 09:23"
        },
        {
          "id": "MDRW2100042803.1.6",
          "form": "최근엔 1인 리클라이너랑 조명 보고 있어요",
          "original_form": "최근엔 1인 리클라이너랑 조명 보고 있어요",
          "speaker_id": "1",
          "time": "20210907 09:24"
        },
        {
          "id": "MDRW2100042803.1.7",
          "form": "리클라이너 있음 완전편할것같아요",
          "original_form": "리클라이너 있음 완전편할것같아요",
          "speaker_id": "2",
          "time": "20210907 09:25"
        },
        {
          "id": "MDRW2100042803.1.8",
          "form": "네 1인용 쇼파 리클라이너 중 고민이네",
          "original_form": "네 1인용 쇼파 리클라이너 중 고민이네",
          "speaker_id": "1",
          "time": "20210907 09:26"
        },
        {
          "id": "MDRW2100042803.1.9",
          "form": "비싸더라구요",
          "original_form": "비싸더라구요",
          "speaker_id": "1",
          "time": "20210907 09:26"
        },
        {
          "id": "MDRW2100042803.1.10",
          "form": "리클라이너되는건 비싸긴해요",
          "original_form": "리클라이너되는건 비싸긴해요",
          "speaker_id": "2",
          "time": "20210907 09:27"
        },
        {
          "id": "MDRW2100042803.1.11",
          "form": "근데 편하니 나를 위해 사는것도좋죠",
          "original_form": "근데 편하니 나를 위해 사는것도좋죠",
          "speaker_id": "2",
          "time": "20210907 09:27"
        },
        {
          "id": "MDRW2100042803.1.12",
          "form": "근데 내년에 이사가시려면 짐스러울수도 있을것같네요",
          "original_form": "근데 내년에 이사가시려면 짐스러울수도 있을것같네요",
          "speaker_id": "2",
          "time": "20210907 09:27"
        },
        {
          "id": "MDRW2100042803.1.13",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210907 09:27"
        },
        {
          "id": "MDRW2100042803.1.14",
          "form": "최근에 구입하고 싶은신 물건 있으세여",
          "original_form": "최근에 구입하고 싶은신 물건 있으세여",
          "speaker_id": "1",
          "time": "20210907 09:28"
        },
        {
          "id": "MDRW2100042803.1.15",
          "form": "저두 쇼파요",
          "original_form": "저두 쇼파요",
          "speaker_id": "2",
          "time": "20210907 09:28"
        },
        {
          "id": "MDRW2100042803.1.16",
          "form": "페브릭인데 가죽으로 바꾸고싶네요",
          "original_form": "페브릭인데 가죽으로 바꾸고싶네요",
          "speaker_id": "2",
          "time": "20210907 09:29"
        },
        {
          "id": "MDRW2100042803.1.17",
          "form": "패브릭은 관리가 좀 어렵나요",
          "original_form": "패브릭은 관리가 좀 어렵나요",
          "speaker_id": "1",
          "time": "20210907 09:29"
        },
        {
          "id": "MDRW2100042803.1.18",
          "form": "애들이커서괜찮아요",
          "original_form": "애들이커서괜찮아요",
          "speaker_id": "2",
          "time": "20210907 09:30"
        },
        {
