      "utterance": [
        {
          "id": "MDRW2100039965.1.1",
          "form": "저희 부장님이 애 셋 아버지이신데",
          "original_form": "저희 부장님이 애 셋 아버지이신데",
          "speaker_id": "2",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100039965.1.2",
          "form": "부인 험담을 그렇게 해요",
          "original_form": "부인 험담을 그렇게 해요",
          "speaker_id": "2",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100039965.1.3",
          "form": "아이고 자기얼굴에 침뱉는건데",
          "original_form": "아이고 자기얼굴에 침뱉는건데",
          "speaker_id": "1",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100039965.1.4",
          "form": "그러니 일로도 믿음이 안가더라고요",
          "original_form": "그러니 일로도 믿음이 안가더라고요",
          "speaker_id": "2",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100039965.1.5",
          "form": "일보다 사람이 싫은것 같아요  회사생활은ㅜ",
          "original_form": "일보다 사람이 싫은것 같아요  회사생활은ㅜ",
          "speaker_id": "1",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100039965.1.6",
          "form": "일은 적성에 맞나요",
          "original_form": "일은 적성에 맞나요",
          "speaker_id": "1",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100039965.1.7",
          "form": "어쩌다 전공따라 직장을 구하긴 했는데",
          "original_form": "어쩌다 전공따라 직장을 구하긴 했는데",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100039965.1.8",
          "form": "적성에맞는지는 모르겠어요",
          "original_form": "적성에맞는지는 모르겠어요",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100039965.1.9",
          "form": "오오 전공 맞추기 어려운데",
          "original_form": "오오 전공 맞추기 어려운데",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100039965.1.10",
          "form": "다른 직종 생각한적 있으신가요",
          "original_form": "다른 직종 생각한적 있으신가요",
          "speaker_id": "1",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100039965.1.11",
          "form": "네여러가지 고민을 했어요",
          "original_form": "네여러가지 고민을 했어요",
          "speaker_id": "2",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100039965.1.12",
          "form": "제가 수학전공이라",
          "original_form": "제가 수학전공이라",
          "speaker_id": "2",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100039965.1.13",
          "form": "와 대단하네요",
          "original_form": "와 대단하네요",
          "speaker_id": "1",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100039965.1.14",
          "form": "경제도 공부해서 어느곳으로 갈까 고민도 좀 했고",
          "original_form": "경제도 공부해서 어느곳으로 갈까 고민도 좀 했고",
          "speaker_id": "2",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100039965.1.15",
          "form": "증권가도 가보고싶었는데 그냥 가장 편하게 그냥 회사들어갔어요",
          "original_form": "증권가도 가보고싶었는데 그냥 가장 편하게 그냥 회사들어갔어요",
          "speaker_id": "2",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100039965.1.16",
          "form": "아...지금은 이직 생각 없으세요?",
          "original_form": "아...지금은 이직 생각 없으세요?",
          "speaker_id": "1",
          "time": "20210903 23:43"
        },
        {
          "id": "MDRW2100039965.1.17",
          "form": "네 이직한 곳도 결국엔 하는 일은 같아서 이제다른 일을 시작할 수 없을 것 같아요ㅠ",
          "original_form": "네 이직한 곳도 결국엔 하는 일은 같아서 이제다른 일을 시작할 수 없을 것 같아요ㅠ",
          "speaker_id": "2",
          "time": "20210903 23:43"
        },
        {
          "id": "MDRW2100039965.1.18",
          "form": "ㅜㅇㅜ 저랑 비슷하시네요",
          "original_form": "ㅜㅇㅜ 저랑 비슷하시네요",
          "speaker_id": "1",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100039965.1.19",
          "form": "아 전공과 직종은 안 비슷합니다ㅜ",
          "original_form": "아 전공과 직종은 안 비슷합니다ㅜ",
          "speaker_id": "1",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100039965.1.20",
          "form": "ㅎㅎ맞추기가 진짜 쉽지 않죠",
          "original_form": "ㅎㅎ맞추기가 진짜 쉽지 않죠",
          "speaker_id": "2",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100039965.1.21",
          "form": "수학이 코에 걸면 코걸이 귀에 걸면 귀걸이라 적당히 다 비슷해지더라고요",
          "original_form": "수학이 코에 걸면 코걸이 귀에 걸면 귀걸이라 적당히 다 비슷해지더라고요",
          "speaker_id": "2",
          "time": "20210903 23:45"
        }
      ]
