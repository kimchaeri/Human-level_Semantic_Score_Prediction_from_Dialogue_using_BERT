      "utterance": [
        {
          "id": "MDRW2100019062.1.1",
          "form": "점심드셨나요",
          "original_form": "점심드셨나요",
          "speaker_id": "2",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019062.1.2",
          "form": "네 저는오늘 삼계탕먹었어요 ㅎㄹ",
          "original_form": "네 저는오늘 삼계탕먹었어요 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210827 14:59"
        },
        {
          "id": "MDRW2100019062.1.3",
          "form": "오 삼계탕",
          "original_form": "오 삼계탕",
          "speaker_id": "2",
          "time": "20210827 14:59"
        },
        {
          "id": "MDRW2100019062.1.4",
          "form": "오늘 날도 추운데 딱이죠",
          "original_form": "오늘 날도 추운데 딱이죠",
          "speaker_id": "2",
          "time": "20210827 15:00"
        },
        {
          "id": "MDRW2100019062.1.5",
          "form": "맞아요~ name2님은 뭐드셨어요?",
          "original_form": "맞아요~ &name2&님은 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210827 15:01"
        },
        {
          "id": "MDRW2100019062.1.6",
          "form": "저는 오늘 초밥먹었습니다",
          "original_form": "저는 오늘 초밥먹었습니다",
          "speaker_id": "2",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100019062.1.7",
          "form": "근데 특이한게 밥이 흑미였어요",
          "original_form": "근데 특이한게 밥이 흑미였어요",
          "speaker_id": "2",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019062.1.8",
          "form": "오 진짜신기한데요??",
          "original_form": "오 진짜신기한데요??",
          "speaker_id": "1",
          "time": "20210827 15:30"
        },
        {
          "id": "MDRW2100019062.1.9",
          "form": "생선좋아하시나봐요!",
          "original_form": "생선좋아하시나봐요!",
          "speaker_id": "1",
          "time": "20210827 15:30"
        },
        {
          "id": "MDRW2100019062.1.10",
          "form": "구운 생선은 딱히 안 좋아하는데",
          "original_form": "구운 생선은 딱히 안 좋아하는데",
          "speaker_id": "2",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019062.1.11",
          "form": "회를 좋아해요ㅋㅋ",
          "original_form": "회를 좋아해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019062.1.12",
          "form": "회좋아하세요?",
          "original_form": "회좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100019062.1.13",
          "form": "아하! 전은 연어회랑 참치회만 좋아해용",
          "original_form": "아하! 전은 연어회랑 참치회만 좋아해용",
          "speaker_id": "1",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100019062.1.14",
          "form": "둘다 맛있죠ㅋㅋ",
          "original_form": "둘다 맛있죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019062.1.15",
          "form": "연어는 아보카도랑 잘 어울리는뎁",
          "original_form": "연어는 아보카도랑 잘 어울리는뎁",
          "speaker_id": "2",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019062.1.16",
          "form": "맞아요! 연어덮밥에 아보카도추가해주면 환상이죠",
          "original_form": "맞아요! 연어덮밥에 아보카도추가해주면 환상이죠",
          "speaker_id": "1",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100019062.1.17",
          "form": "맞아요ㅋㅋ타르타르 소스랑도 잘 맞아욥",
          "original_form": "맞아요ㅋㅋ타르타르 소스랑도 잘 맞아욥",
          "speaker_id": "2",
          "time": "20210827 16:13"
        },
        {
          "id": "MDRW2100019062.1.18",
          "form": "참치회는 저랑 잘 안 맞더라고요",
          "original_form": "참치회는 저랑 잘 안 맞더라고요",
          "speaker_id": "2",
          "time": "20210827 16:14"
        }
      ]
