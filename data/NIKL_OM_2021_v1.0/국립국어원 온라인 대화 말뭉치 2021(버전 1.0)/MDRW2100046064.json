      "utterance": [
        {
          "id": "MDRW2100046064.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210908 09:59"
        },
        {
          "id": "MDRW2100046064.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210908 10:04"
        },
        {
          "id": "MDRW2100046064.1.3",
          "form": "반려동물 키우세요 ?",
          "original_form": "반려동물 키우세요 ?",
          "speaker_id": "2",
          "time": "20210908 10:04"
        },
        {
          "id": "MDRW2100046064.1.4",
          "form": "저흰 구피 키워요",
          "original_form": "저흰 구피 키워요",
          "speaker_id": "1",
          "time": "20210908 10:04"
        },
        {
          "id": "MDRW2100046064.1.5",
          "form": "열대어 인거 맞죠",
          "original_form": "열대어 인거 맞죠",
          "speaker_id": "2",
          "time": "20210908 10:06"
        },
        {
          "id": "MDRW2100046064.1.6",
          "form": "네.맞아요",
          "original_form": "네.맞아요",
          "speaker_id": "1",
          "time": "20210908 10:07"
        },
        {
          "id": "MDRW2100046064.1.7",
          "form": "반려동물 한번도 안 키워 보셨나요?",
          "original_form": "반려동물 한번도 안 키워 보셨나요?",
          "speaker_id": "1",
          "time": "20210908 10:08"
        },
        {
          "id": "MDRW2100046064.1.8",
          "form": "구피가 물고기를 많이 낫죠",
          "original_form": "구피가 물고기를 많이 낫죠",
          "speaker_id": "2",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100046064.1.9",
          "form": "저도 구피 키워 봤어요",
          "original_form": "저도 구피 키워 봤어요",
          "speaker_id": "2",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100046064.1.10",
          "form": "네. 많이 낳더라구요",
          "original_form": "네. 많이 낳더라구요",
          "speaker_id": "1",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100046064.1.11",
          "form": "계속 낳다보면 어미가 등이 굽더라고요",
          "original_form": "계속 낳다보면 어미가 등이 굽더라고요",
          "speaker_id": "2",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100046064.1.12",
          "form": "계속 낳아서 그런건지. 병이 든건지 등이 굽더라구요",
          "original_form": "계속 낳아서 그런건지. 병이 든건지 등이 굽더라구요",
          "speaker_id": "1",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100046064.1.13",
          "form": "계속 낳아서 사람처럼 골다공증 인것 같죠",
          "original_form": "계속 낳아서 사람처럼 골다공증 인것 같죠",
          "speaker_id": "2",
          "time": "20210908 10:15"
        },
        {
          "id": "MDRW2100046064.1.14",
          "form": "왠지 안쓰럽더라고요",
          "original_form": "왠지 안쓰럽더라고요",
          "speaker_id": "2",
          "time": "20210908 10:15"
        },
        {
          "id": "MDRW2100046064.1.15",
          "form": "그렇네요.ㅜㅜ",
          "original_form": "그렇네요.ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 10:15"
        },
        {
          "id": "MDRW2100046064.1.16",
          "form": "저흰 애들 어릴 때 과학실험 방과후를 해서 사슴벌레 장수풍뎅이 누에 햄스터 고슴도치 등등 키워 봤어요",
          "original_form": "저흰 애들 어릴 때 과학실험 방과후를 해서 사슴벌레 장수풍뎅이 누에 햄스터 고슴도치 등등 키워 봤어요",
          "speaker_id": "1",
          "time": "20210908 10:16"
        },
        {
          "id": "MDRW2100046064.1.17",
          "form": "다양한 곤충을 키워 보셨네요",
          "original_form": "다양한 곤충을 키워 보셨네요",
          "speaker_id": "2",
          "time": "20210908 10:20"
        },
        {
          "id": "MDRW2100046064.1.18",
          "form": "키우긴 어렵지 않나요",
          "original_form": "키우긴 어렵지 않나요",
          "speaker_id": "2",
          "time": "20210908 10:20"
        },
        {
          "id": "MDRW2100046064.1.19",
          "form": "네. 다 어렵진 않았어요. 인터넷 덕분에요.ㅋ",
          "original_form": "네. 다 어렵진 않았어요. 인터넷 덕분에요.ㅋ",
          "speaker_id": "1",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100046064.1.20",
          "form": "오래 키우기가 어렵다고 주변에서 얘기 하더라고요",
          "original_form": "오래 키우기가 어렵다고 주변에서 얘기 하더라고요",
          "speaker_id": "2",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046064.1.21",
          "form": "그쵸. 사슴벌레랑 장수풍뎅이는 알 낳아서 성충 된 거 분양도 했어요",
          "original_form": "그쵸. 사슴벌레랑 장수풍뎅이는 알 낳아서 성충 된 거 분양도 했어요",
          "speaker_id": "1",
          "time": "20210908 10:26"
        }
      ]
