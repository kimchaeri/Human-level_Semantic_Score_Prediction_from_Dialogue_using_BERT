      "utterance": [
        {
          "id": "MDRW2100037062.1.1",
          "form": "반려동물 키우시나요",
          "original_form": "반려동물 키우시나요",
          "speaker_id": "1",
          "time": "20210831 18:37"
        },
        {
          "id": "MDRW2100037062.1.2",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210831 18:40"
        },
        {
          "id": "MDRW2100037062.1.3",
          "form": "키우시나요?",
          "original_form": "키우시나요?",
          "speaker_id": "2",
          "time": "20210831 18:40"
        },
        {
          "id": "MDRW2100037062.1.4",
          "form": "네네저도 강아지",
          "original_form": "네네저도 강아지{emoji:♡}",
          "speaker_id": "1",
          "time": "20210831 18:40"
        },
        {
          "id": "MDRW2100037062.1.5",
          "form": "어웅 너무 이쁘겠어용",
          "original_form": "어웅 너무 이쁘겠어용",
          "speaker_id": "2",
          "time": "20210831 19:15"
        },
        {
          "id": "MDRW2100037062.1.6",
          "form": "고양이 키우세요?",
          "original_form": "고양이 키우세요?",
          "speaker_id": "1",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100037062.1.7",
          "form": "넹!",
          "original_form": "넹!",
          "speaker_id": "2",
          "time": "20210831 21:28"
        },
        {
          "id": "MDRW2100037062.1.8",
          "form": "진짜 너무 이뻐요",
          "original_form": "진짜 너무 이뻐요",
          "speaker_id": "2",
          "time": "20210831 21:28"
        },
        {
          "id": "MDRW2100037062.1.9",
          "form": "꺄 몇살이에요",
          "original_form": "꺄 몇살이에요",
          "speaker_id": "1",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100037062.1.10",
          "form": "3살이에용! name1네 아이는요?",
          "original_form": "3살이에용! &name1&네 아이는요?",
          "speaker_id": "2",
          "time": "20210831 21:47"
        },
        {
          "id": "MDRW2100037062.1.11",
          "form": "저희는 4살이에영",
          "original_form": "저희는 4살이에영",
          "speaker_id": "1",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100037062.1.12",
          "form": "오옹 한창때네요",
          "original_form": "오옹 한창때네요",
          "speaker_id": "2",
          "time": "20210831 22:12"
        },
        {
          "id": "MDRW2100037062.1.13",
          "form": "간식은 자주 주시나요?",
          "original_form": "간식은 자주 주시나요?",
          "speaker_id": "2",
          "time": "20210831 22:13"
        },
        {
          "id": "MDRW2100037062.1.14",
          "form": "자주는 못주구 가끔줘요",
          "original_form": "자주는 못주구 가끔줘요",
          "speaker_id": "1",
          "time": "20210901 06:57"
        },
        {
          "id": "MDRW2100037062.1.15",
          "form": "맨날 주고픈데ㅠㅠ 탈이나서ㅠㅠ",
          "original_form": "맨날 주고픈데ㅠㅠ 탈이나서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 06:57"
        },
        {
          "id": "MDRW2100037062.1.16",
          "form": "저두요ㅜㅠ",
          "original_form": "저두요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210901 07:29"
        },
        {
          "id": "MDRW2100037062.1.17",
          "form": "간식주믄 밥도 잘 안먹구요..ㅠ",
          "original_form": "간식주믄 밥도 잘 안먹구요..ㅠ",
          "speaker_id": "2",
          "time": "20210901 07:30"
        },
        {
          "id": "MDRW2100037062.1.18",
          "form": "그렇습니다 또륵",
          "original_form": "그렇습니다 또륵",
          "speaker_id": "1",
          "time": "20210901 10:46"
        },
        {
          "id": "MDRW2100037062.1.19",
          "form": "맛낭거마니주고파요",
          "original_form": "맛낭거마니주고파요",
          "speaker_id": "1",
          "time": "20210901 10:46"
        },
        {
          "id": "MDRW2100037062.1.20",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 12:03"
        },
        {
          "id": "MDRW2100037062.1.21",
          "form": "좋아하는거만 주고싶어요",
          "original_form": "좋아하는거만 주고싶어요",
          "speaker_id": "2",
          "time": "20210901 12:03"
        },
        {
          "id": "MDRW2100037062.1.22",
          "form": "그쵸 고기만 맨널 주고싶어요",
          "original_form": "그쵸 고기만 맨널 주고싶어요",
          "speaker_id": "1",
          "time": "20210901 13:14"
        },
        {
          "id": "MDRW2100037062.1.23",
          "form": "간식은 주로 뭐주세요",
          "original_form": "간식은 주로 뭐주세요",
          "speaker_id": "1",
          "time": "20210901 13:14"
        },
        {
          "id": "MDRW2100037062.1.24",
          "form": "추르나 건조 간 소시지요!",
          "original_form": "추르나 건조 간 소시지요!",
          "speaker_id": "2",
          "time": "20210901 13:44"
        },
        {
          "id": "MDRW2100037062.1.25",
          "form": "건조된거 좋아하더라구요",
          "original_form": "건조된거 좋아하더라구요",
          "speaker_id": "2",
          "time": "20210901 13:45"
        },
        {
          "id": "MDRW2100037062.1.26",
          "form": "간소시지",
          "original_form": "간소시지",
          "speaker_id": "1",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100037062.1.27",
          "form": "저희는말랑한 육포 주랴고 해요",
          "original_form": "저희는말랑한 육포 주랴고 해요",
          "speaker_id": "1",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100037062.1.28",
          "form": "껌은 토하더라고요ㅠㅠ",
          "original_form": "껌은 토하더라고요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100037062.1.29",
          "form": "어우ㅠ 껌 주셨을때 놀라셨겠어요",
          "original_form": "어우ㅠ 껌 주셨을때 놀라셨겠어요",
          "speaker_id": "2",
          "time": "20210901 13:56"
        },
        {
          "id": "MDRW2100037062.1.30",
          "form": "저는 싫어하는건 뱉어내더라구요ㅎㅎ",
          "original_form": "저는 싫어하는건 뱉어내더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 13:57"
        },
        {
          "id": "MDRW2100037062.1.31",
          "form": "퉤하고요",
          "original_form": "퉤하고요",
          "speaker_id": "2",
          "time": "20210901 13:57"
        },
        {
          "id": "MDRW2100037062.1.32",
          "form": "앜ㅋㅋㅋ귀여워",
          "original_form": "앜ㅋㅋㅋ귀여워",
          "speaker_id": "1",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100037062.1.33",
          "form": "참지않긔",
          "original_form": "참지않긔",
          "speaker_id": "1",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100037062.1.34",
