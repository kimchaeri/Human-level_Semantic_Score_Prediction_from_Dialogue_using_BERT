      "utterance": [
        {
          "id": "MDRW2100005995.1.1",
          "form": "백신 접종하셨나요",
          "original_form": "백신 접종하셨나요",
          "speaker_id": "1",
          "time": "20210907 18:27"
        },
        {
          "id": "MDRW2100005995.1.2",
          "form": "1차는 했어요",
          "original_form": "1차는 했어요",
          "speaker_id": "2",
          "time": "20210907 18:28"
        },
        {
          "id": "MDRW2100005995.1.3",
          "form": "모더나접종하셨나요?",
          "original_form": "모더나접종하셨나요?",
          "speaker_id": "1",
          "time": "20210907 18:29"
        },
        {
          "id": "MDRW2100005995.1.4",
          "form": "네네 모더나로 접종했어요",
          "original_form": "네네 모더나로 접종했어요",
          "speaker_id": "2",
          "time": "20210907 18:30"
        },
        {
          "id": "MDRW2100005995.1.5",
          "form": "아 모더나가 제일 델타변이에강하다더라구요",
          "original_form": "아 모더나가 제일 델타변이에강하다더라구요",
          "speaker_id": "1",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100005995.1.6",
          "form": "순차공급되고는 있다는데",
          "original_form": "순차공급되고는 있다는데",
          "speaker_id": "1",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100005995.1.7",
          "form": "그래요? 그거까진 몰랐네요ㅋㅋ",
          "original_form": "그래요? 그거까진 몰랐네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:33"
        },
        {
          "id": "MDRW2100005995.1.8",
          "form": "아직 좀 부족한 것 같긴해요",
          "original_form": "아직 좀 부족한 것 같긴해요",
          "speaker_id": "1",
          "time": "20210907 18:33"
        },
        {
          "id": "MDRW2100005995.1.9",
          "form": "넵넵 화이자보다 더 효과가 좋다네요",
          "original_form": "넵넵 화이자보다 더 효과가 좋다네요",
          "speaker_id": "1",
          "time": "20210907 18:33"
        },
        {
          "id": "MDRW2100005995.1.10",
          "form": "빨리 다들 백신 생겨서 코로나 끝나면 좋겠어요",
          "original_form": "빨리 다들 백신 생겨서 코로나 끝나면 좋겠어요",
          "speaker_id": "2",
          "time": "20210907 18:33"
        },
        {
          "id": "MDRW2100005995.1.11",
          "form": "맞아요 자꾸 변이가 생겨서",
          "original_form": "맞아요 자꾸 변이가 생겨서",
          "speaker_id": "1",
          "time": "20210907 18:33"
        },
        {
          "id": "MDRW2100005995.1.12",
          "form": "백신이 제때들어와야할텐데",
          "original_form": "백신이 제때들어와야할텐데",
          "speaker_id": "1",
          "time": "20210907 18:33"
        },
        {
          "id": "MDRW2100005995.1.13",
          "form": "자꾸 모자란단소리가 들려서",
          "original_form": "자꾸 모자란단소리가 들려서",
          "speaker_id": "1",
          "time": "20210907 18:34"
        },
        {
          "id": "MDRW2100005995.1.14",
          "form": "접종주기가 언제 정상화될지모르겠네요",
          "original_form": "접종주기가 언제 정상화될지모르겠네요",
          "speaker_id": "1",
          "time": "20210907 18:34"
        },
        {
          "id": "MDRW2100005995.1.15",
          "form": "맞아요ㅠㅠ 기사도 내용 다 달라서 뭘 믿어야 할지 모르겠고요",
          "original_form": "맞아요ㅠㅠ 기사도 내용 다 달라서 뭘 믿어야 할지 모르겠고요",
          "speaker_id": "2",
          "time": "20210907 18:36"
        },
        {
          "id": "MDRW2100005995.1.16",
          "form": "접종주기부터 빨리 좀 돌려줬으면 좋겠어요",
          "original_form": "접종주기부터 빨리 좀 돌려줬으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210907 18:38"
        },
        {
          "id": "MDRW2100005995.1.17",
          "form": "문제는 없다고 하는데. 빨리 맞고 싶어서 그런가ㅋㅋ 조급해지긴 하네요",
          "original_form": "문제는 없다고 하는데. 빨리 맞고 싶어서 그런가ㅋㅋ 조급해지긴 하네요",
          "speaker_id": "2",
          "time": "20210907 18:41"
        },
        {
          "id": "MDRW2100005995.1.18",
          "form": "문제는 없겠지만",
          "original_form": "문제는 없겠지만",
          "speaker_id": "1",
          "time": "20210907 18:42"
        },
        {
          "id": "MDRW2100005995.1.19",
          "form": "효과가 줄면 어쩌나싶어서요",
          "original_form": "효과가 줄면 어쩌나싶어서요",
