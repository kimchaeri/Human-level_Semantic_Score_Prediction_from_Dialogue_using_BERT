      "utterance": [
        {
          "id": "MDRW2100015556.1.1",
          "form": "name2님은 진짜 name2 조아하세요?",
          "original_form": "&name2&님은 진짜 &name2& 조아하세요?",
          "speaker_id": "1",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015556.1.2",
          "form": "앗 name1님!!!!네!!",
          "original_form": "앗 &name1&님!!!!네!!",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015556.1.3",
          "form": "카페가면 이름은 바꾼",
          "original_form": "카페가면 이름은 바꾼",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015556.1.4",
          "form": "이곡라떼.오곡라떼 이런 미숫가루들이 있거든요",
          "original_form": "이곡라떼.오곡라떼 이런 미숫가루들이 있거든요",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015556.1.5",
          "form": "카페가면 이런거로 마셔요!!!ㅋㅋ",
          "original_form": "카페가면 이런거로 마셔요!!!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015556.1.6",
          "form": "오 저도 오곡라떼 보이면 꼭 먹어봐요!",
          "original_form": "오 저도 오곡라떼 보이면 꼭 먹어봐요!",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015556.1.7",
          "form": "하지만 찜질방에서 먹는게 최고",
          "original_form": "하지만 찜질방에서 먹는게 최고",
          "speaker_id": "1",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015556.1.8",
          "form": "맞죠맞죠!!!!",
          "original_form": "맞죠맞죠!!!!",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015556.1.9",
          "form": "찜질방의 설탕범벅 그맛을 따라올수없어요ㅜㅜ",
          "original_form": "찜질방의 설탕범벅 그맛을 따라올수없어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015556.1.10",
          "form": "혹시 동묘 시장안에 토스트집 가보셨어요?",
          "original_form": "혹시 동묘 시장안에 토스트집 가보셨어요?",
          "speaker_id": "2",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015556.1.11",
          "form": "맞아요! 찜질방에선 다 맛있다..",
          "original_form": "맞아요! 찜질방에선 다 맛있다..",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015556.1.12",
          "form": "아뇨 맛집이에요?!",
          "original_form": "아뇨 맛집이에요?!",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015556.1.13",
          "form": "거기 미숫가루맛집이에요ㅋㅋㅋㅋ",
          "original_form": "거기 미숫가루맛집이에요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100015556.1.14",
          "form": "혈관을 타고흐르는 설탕의맛",
          "original_form": "혈관을 타고흐르는 설탕의맛",
          "speaker_id": "2",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100015556.1.15",
          "form": "메모해놔야겠다.. 저 원래 단음료 잘 안마시는데",
          "original_form": "메모해놔야겠다.. 저 원래 단음료 잘 안마시는데",
          "speaker_id": "1",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015556.1.16",
          "form": "토스트 미숫가루 다 천원이에요",
          "original_form": "토스트 미숫가루 다 천원이에요",
          "speaker_id": "2",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015556.1.17",
          "form": "미숫가루는 보이면 꼬옥 먹어요",
          "original_form": "미숫가루는 보이면 꼬옥 먹어요",
          "speaker_id": "1",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015556.1.18",
          "form": "토스트에 미숫가루? 진짜 도른 조합이네요..",
          "original_form": "토스트에 미숫가루? 진짜 도른 조합이네요..",
          "speaker_id": "1",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015556.1.19",
          "form": "ㅜㅜ근데 코로나라서 여긴 상황봐서가세요ㅅ",
          "original_form": "ㅜㅜ근데 코로나라서 여긴 상황봐서가세요ㅅ",
          "speaker_id": "2",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015556.1.20",
          "form": "시장안쪽에 있어서",
          "original_form": "시장안쪽에 있어서",
          "speaker_id": "2",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015556.1.21",
          "form": "돗때기시장같은데",
          "original_form": "돗때기시장같은데",
          "speaker_id": "2",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015556.1.22",
          "form": "맞아여ㅋㅋ도른조합이에요",
          "original_form": "맞아여ㅋㅋ도른조합이에요",
          "speaker_id": "2",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015556.1.23",
          "form": "네이버에",
          "original_form": "네이버에",
          "speaker_id": "2",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015556.1.24",
          "form": "동묘 토스트 미숫가루",
          "original_form": "동묘 토스트 미숫가루",
          "speaker_id": "2",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015556.1.25",
          "form": "이렇게검색하면 나오는 후기 다 그집이에요ㅋㅋ",
          "original_form": "이렇게검색하면 나오는 후기 다 그집이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015556.1.26",
          "form": "대박 저 찾아볼래요",
          "original_form": "대박 저 찾아볼래요",
          "speaker_id": "1",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015556.1.27",
          "form": "진짜 가성비짱이에요ㅜㅜ",
          "original_form": "진짜 가성비짱이에요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100015556.1.28",
          "form": "헐 저거 계란물에 적신거죠 토스트",
          "original_form": "헐 저거 계란물에 적신거죠 토스트",
          "speaker_id": "1",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100015556.1.29",
          "form": "맛없을 수 없는 조합이다..",
          "original_form": "맛없을 수 없는 조합이다..",
          "speaker_id": "1",
          "time": "20210825 23:39"
        },
        {
          "id": "MDRW2100015556.1.30",
          "form": "저런 기본 토스트가 그리울때가 있더라구요",
          "original_form": "저런 기본 토스트가 그리울때가 있더라구요",
          "speaker_id": "1",
          "time": "20210825 23:40"
        }
      ]
