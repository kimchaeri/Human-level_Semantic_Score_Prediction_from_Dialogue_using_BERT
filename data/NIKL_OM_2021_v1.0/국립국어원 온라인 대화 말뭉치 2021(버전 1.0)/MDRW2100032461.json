      "utterance": [
        {
          "id": "MDRW2100032461.1.1",
          "form": "오늘은 집밖으로 안나가기 프로젝트 실패했어요",
          "original_form": "오늘은 집밖으로 안나가기 프로젝트 실패했어요",
          "speaker_id": "1",
          "time": "20210905 18:31"
        },
        {
          "id": "MDRW2100032461.1.2",
          "form": "장보러 나갔다 왔어요 ㅋㅋㅋ",
          "original_form": "장보러 나갔다 왔어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:31"
        },
        {
          "id": "MDRW2100032461.1.3",
          "form": "사도사도 왜 맨날 먹을게 없나 몰라요",
          "original_form": "사도사도 왜 맨날 먹을게 없나 몰라요",
          "speaker_id": "1",
          "time": "20210905 18:33"
        },
        {
          "id": "MDRW2100032461.1.4",
          "form": "블루님은 저녁식사 하셨나요?",
          "original_form": "블루님은 저녁식사 하셨나요?",
          "speaker_id": "1",
          "time": "20210905 18:34"
        },
        {
          "id": "MDRW2100032461.1.5",
          "form": "와 저도 지금 마트가는 길이에요!",
          "original_form": "와 저도 지금 마트가는 길이에요!",
          "speaker_id": "2",
          "time": "20210905 18:49"
        },
        {
          "id": "MDRW2100032461.1.6",
          "form": "방금 치킨 먹고 나왓엉ㅅ!",
          "original_form": "방금 치킨 먹고 나왓엉ㅅ!",
          "speaker_id": "2",
          "time": "20210905 18:49"
        },
        {
          "id": "MDRW2100032461.1.7",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "2",
          "time": "20210905 18:49"
        },
        {
          "id": "MDRW2100032461.1.8",
          "form": "이제 먹으려구요!",
          "original_form": "이제 먹으려구요!",
          "speaker_id": "1",
          "time": "20210905 18:52"
        },
        {
          "id": "MDRW2100032461.1.9",
          "form": "치킨!",
          "original_form": "치킨!",
          "speaker_id": "1",
          "time": "20210905 18:53"
        },
        {
          "id": "MDRW2100032461.1.10",
          "form": "저는 떡볶이 먹으려구요!",
          "original_form": "저는 떡볶이 먹으려구요!",
          "speaker_id": "1",
          "time": "20210905 18:53"
        },
        {
          "id": "MDRW2100032461.1.11",
          "form": "치킨에 떡볶이 먹우면",
          "original_form": "치킨에 떡볶이 먹우면",
          "speaker_id": "2",
          "time": "20210905 19:04"
        },
        {
          "id": "MDRW2100032461.1.12",
          "form": "넘 맛있엉ㅅ",
          "original_form": "넘 맛있엉ㅅ",
          "speaker_id": "2",
          "time": "20210905 19:04"
        },
        {
          "id": "MDRW2100032461.1.13",
          "form": "저 떡볶이 별로 안좋아하는데 치킨이랑 같이 먹는거 너무 좋아요",
          "original_form": "저 떡볶이 별로 안좋아하는데 치킨이랑 같이 먹는거 너무 좋아요",
          "speaker_id": "2",
          "time": "20210905 19:04"
        },
        {
          "id": "MDRW2100032461.1.14",
          "form": "치킨이랑 떡볶이 같이는 잘 안먹은 것 같아요",
          "original_form": "치킨이랑 떡볶이 같이는 잘 안먹은 것 같아요",
          "speaker_id": "1",
          "time": "20210905 19:44"
        },
        {
          "id": "MDRW2100032461.1.15",
          "form": "그 조합도 좋다고 말씀해주시니 다음번에 꼭 먹어볼게요",
          "original_form": "그 조합도 좋다고 말씀해주시니 다음번에 꼭 먹어볼게요",
          "speaker_id": "1",
          "time": "20210905 19:45"
        },
        {
          "id": "MDRW2100032461.1.16",
          "form": "오늘은 어떤 떡볶이 드셨어요?",
          "original_form": "오늘은 어떤 떡볶이 드셨어요?",
          "speaker_id": "2",
          "time": "20210905 19:58"
        },
        {
          "id": "MDRW2100032461.1.17",
          "form": "엽떡이요 ㅋㅋ",
          "original_form": "엽떡이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100032461.1.18",
          "form": "매운거 잘 드시나요?",
          "original_form": "매운거 잘 드시나요?",
          "speaker_id": "1",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100032461.1.19",
          "form": "아니요! 잘 못 먹는데 치킨이랑 먹을때는 저도 늘 엽떡먹어요!!",
          "original_form": "아니요! 잘 못 먹는데 치킨이랑 먹을때는 저도 늘 엽떡먹어요!!",
          "speaker_id": "2",
          "time": "20210905 20:28"
        },
        {
          "id": "MDRW2100032461.1.20",
          "form": "고세지랑 치즈추가",
          "original_form": "고세지랑 치즈추가",
          "speaker_id": "2",
          "time": "20210905 20:29"
        },
        {
          "id": "MDRW2100032461.1.21",
          "form": "소세지랑 치즈추가 중극당면 추가해서요..ㅠㅠ",
          "original_form": "소세지랑 치즈추가 중극당면 추가해서요..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 20:29"
        },
        {
          "id": "MDRW2100032461.1.22",
          "form": "역시 뭘 좀 아시는 분이군요 ㅋㅋ",
          "original_form": "역시 뭘 좀 아시는 분이군요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:29"
        },
        {
          "id": "MDRW2100032461.1.23",
          "form": "매운걸 잘 못먹는 편이지만 가끔 시켜먹어요",
          "original_form": "매운걸 잘 못먹는 편이지만 가끔 시켜먹어요",
          "speaker_id": "1",
          "time": "20210905 20:30"
        },
        {
          "id": "MDRW2100032461.1.24",
          "form": "저도요..ㅎ 못먹는데 그냥 울면서 먹어요 늘",
          "original_form": "저도요..ㅎ 못먹는데 그냥 울면서 먹어요 늘",
          "speaker_id": "2",
          "time": "20210905 20:31"
        },
        {
          "id": "MDRW2100032461.1.25",
          "form": "그래서 옛날 컵 떡볶이가 그리워요",
          "original_form": "그래서 옛날 컵 떡볶이가 그리워요",
          "speaker_id": "1",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100032461.1.26",
          "form": "저도요 저렴하고 양도 완전 딱이고 맛있었는데",
          "original_form": "저도요 저렴하고 양도 완전 딱이고 맛있었는데",
          "speaker_id": "2",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100032461.1.27",
          "form": "그쵸 요새 그렇게 파는 곳을 찾기가 너무 힘들어요ㅠ",
          "original_form": "그쵸 요새 그렇게 파는 곳을 찾기가 너무 힘들어요ㅠ",
          "speaker_id": "1",
          "time": "20210905 20:54"
        }
      ]
    }
