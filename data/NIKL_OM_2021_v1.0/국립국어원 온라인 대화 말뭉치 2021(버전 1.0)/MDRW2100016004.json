        },
        {
          "id": "MDRW2100016004.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 07:27"
        },
        {
          "id": "MDRW2100016004.1.3",
          "form": "요새 다이어트나 운동 하시나요?",
          "original_form": "요새 다이어트나 운동 하시나요?",
          "speaker_id": "1",
          "time": "20210826 07:27"
        },
        {
          "id": "MDRW2100016004.1.4",
          "form": "시간적 여유가 안되서 못하고있어요",
          "original_form": "시간적 여유가 안되서 못하고있어요",
          "speaker_id": "2",
          "time": "20210826 07:28"
        },
        {
          "id": "MDRW2100016004.1.5",
          "form": "하고 계신 운동 있으신가요",
          "original_form": "하고 계신 운동 있으신가요",
          "speaker_id": "2",
          "time": "20210826 07:28"
        },
        {
          "id": "MDRW2100016004.1.6",
          "form": "아뇨 저두 운동은 안하고있어요, 그냥 가벼운산책정도만해요ㅋㅋ",
          "original_form": "아뇨 저두 운동은 안하고있어요, 그냥 가벼운산책정도만해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 07:29"
        },
        {
          "id": "MDRW2100016004.1.7",
          "form": "걷기가 여러모로 좋은거같아요",
          "original_form": "걷기가 여러모로 좋은거같아요",
          "speaker_id": "2",
          "time": "20210826 07:29"
        },
        {
          "id": "MDRW2100016004.1.8",
          "form": "음 그럼 영양제 드시는 거 있으세요?",
          "original_form": "음 그럼 영양제 드시는 거 있으세요?",
          "speaker_id": "2",
          "time": "20210826 07:29"
        },
        {
          "id": "MDRW2100016004.1.9",
          "form": "맞아요! 저는 유산균이랑 비타민d 두가지 챙겨먹어요~",
          "original_form": "맞아요! 저는 유산균이랑 비타민d 두가지 챙겨먹어요~",
          "speaker_id": "1",
          "time": "20210826 07:30"
        },
        {
          "id": "MDRW2100016004.1.10",
          "form": "비타민D 먹으면 좋더라궁ᆢ",
          "original_form": "비타민D 먹으면 좋더라궁ᆢ",
          "speaker_id": "2",
          "time": "20210826 07:30"
        },
        {
          "id": "MDRW2100016004.1.11",
          "form": "약간 힘이 더 나는 기분",
          "original_form": "약간 힘이 더 나는 기분",
          "speaker_id": "2",
          "time": "20210826 07:30"
        },
        {
          "id": "MDRW2100016004.1.12",
          "form": "유산균은 어디꺼드시나요",
          "original_form": "유산균은 어디꺼드시나요",
          "speaker_id": "2",
          "time": "20210826 07:30"
        },
        {
          "id": "MDRW2100016004.1.13",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210826 07:30"
        },
        {
          "id": "MDRW2100016004.1.14",
          "form": "유산균 종근당 먹어요",
          "original_form": "유산균 종근당 먹어요",
          "speaker_id": "1",
          "time": "20210826 07:30"
        },
        {
          "id": "MDRW2100016004.1.15",
          "form": "노란색",
          "original_form": "노란색",
          "speaker_id": "1",
          "time": "20210826 07:30"
        },
        {
          "id": "MDRW2100016004.1.16",
          "form": "엇 저도 그거 먹어요!!",
          "original_form": "엇 저도 그거 먹어요!!",
          "speaker_id": "2",
          "time": "20210826 07:31"
        },
        {
          "id": "MDRW2100016004.1.17",
          "form": "가볍게 먹기좋더라구요",
          "original_form": "가볍게 먹기좋더라구요",
          "speaker_id": "2",
          "time": "20210826 07:31"
        },
        {
          "id": "MDRW2100016004.1.18",
          "form": "오 이거드시는분 많더라구요~",
          "original_form": "오 이거드시는분 많더라구요~",
          "speaker_id": "1",
          "time": "20210826 07:31"
        },
        {
          "id": "MDRW2100016004.1.19",
          "form": "맛도 나쁘지않구요",
          "original_form": "맛도 나쁘지않구요",
          "speaker_id": "2",
          "time": "20210826 07:31"
        },
        {
          "id": "MDRW2100016004.1.20",
          "form": "가만보면 가루유산균들 다 맛난거같아요",
          "original_form": "가만보면 가루유산균들 다 맛난거같아요",
