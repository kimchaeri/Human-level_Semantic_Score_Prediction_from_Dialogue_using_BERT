      "utterance": [
        {
          "id": "MDRW2100030865.1.1",
          "form": "사고 싶은거 있어?",
          "original_form": "사고 싶은거 있어?",
          "speaker_id": "1",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100030865.1.2",
          "form": "나 최근에 또 포토카드 못 샀어...",
          "original_form": "나 최근에 또 포토카드 못 샀어...",
          "speaker_id": "1",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100030865.1.3",
          "form": "사줄거야?",
          "original_form": "사줄거야?",
          "speaker_id": "2",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100030865.1.4",
          "form": "0.1초 컷이더라",
          "original_form": "0.1초 컷이더라",
          "speaker_id": "1",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100030865.1.5",
          "form": "미안 나도 돈이 없어",
          "original_form": "미안 나도 돈이 없어",
          "speaker_id": "1",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100030865.1.6",
          "form": "그거 너무 경쟁률ㄹ이...",
          "original_form": "그거 너무 경쟁률ㄹ이...",
          "speaker_id": "2",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100030865.1.7",
          "form": "눈물 난다...",
          "original_form": "눈물 난다...",
          "speaker_id": "1",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100030865.1.8",
          "form": "나만 또 못샀지...",
          "original_form": "나만 또 못샀지...",
          "speaker_id": "1",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100030865.1.9",
          "form": "나도 백현이 포토북?그거 사고 싶었는데",
          "original_form": "나도 백현이 포토북?그거 사고 싶었는데",
          "speaker_id": "2",
          "time": "20210904 13:38"
        },
        {
          "id": "MDRW2100030865.1.10",
          "form": "못샀어",
          "original_form": "못샀어",
          "speaker_id": "2",
          "time": "20210904 13:38"
        },
        {
          "id": "MDRW2100030865.1.11",
          "form": "ㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 13:38"
        },
        {
          "id": "MDRW2100030865.1.12",
          "form": "헐..포토북...",
          "original_form": "헐..포토북...",
          "speaker_id": "1",
          "time": "20210904 13:38"
        },
        {
          "id": "MDRW2100030865.1.13",
          "form": "우리도 그런거 잇는데 없어",
          "original_form": "우리도 그런거 잇는데 없어",
          "speaker_id": "1",
          "time": "20210904 13:38"
        },
        {
          "id": "MDRW2100030865.1.14",
          "form": "컨셉 포토북이 있다? 근데 없어",
          "original_form": "컨셉 포토북이 있다? 근데 없어",
          "speaker_id": "1",
          "time": "20210904 13:38"
        },
        {
          "id": "MDRW2100030865.1.15",
          "form": "홈마가 있는데 없어",
          "original_form": "홈마가 있는데 없어",
          "speaker_id": "1",
          "time": "20210904 13:38"
        },
        {
          "id": "MDRW2100030865.1.16",
          "form": "슈뢰딩거의 고양이도 아니고...",
          "original_form": "슈뢰딩거의 고양이도 아니고...",
          "speaker_id": "1",
          "time": "20210904 13:39"
        },
        {
          "id": "MDRW2100030865.1.17",
          "form": "있는데 없고 없는데 없어",
          "original_form": "있는데 없고 없는데 없어",
          "speaker_id": "2",
          "time": "20210904 14:16"
        },
        {
          "id": "MDRW2100030865.1.18",
          "form": "그냥 없어...",
          "original_form": "그냥 없어...",
          "speaker_id": "2",
          "time": "20210904 14:16"
        },
        {
          "id": "MDRW2100030865.1.19",
          "form": "눈물 괄괄",
          "original_form": "눈물 괄괄",
          "speaker_id": "2",
          "time": "20210904 14:16"
        },
        {
          "id": "MDRW2100030865.1.20",
          "form": "나는 작법서 책 사고 싶은데",
          "original_form": "나는 작법서 책 사고 싶은데",
          "speaker_id": "1",
          "time": "20210904 14:16"
        },
        {
          "id": "MDRW2100030865.1.21",
          "form": "이러다 그림은 안구리고 이론만 머릿속에 집어 넣울듯...",
          "original_form": "이러다 그림은 안구리고 이론만 머릿속에 집어 넣울듯...",
          "speaker_id": "1",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030865.1.22",
          "form": "아 나도 인터페이스 사고 싶다",
          "original_form": "아 나도 인터페이스 사고 싶다",
          "speaker_id": "2",
          "time": "20210904 16:33"
        },
        {
          "id": "MDRW2100030865.1.23",
          "form": "맥북 사고 싶다",
          "original_form": "맥북 사고 싶다",
          "speaker_id": "2",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100030865.1.24",
          "form": "애플을 그렇게 좋아하진 않다만 맥북은 진짜 갖고 싶음...",
          "original_form": "애플을 그렇게 좋아하진 않다만 맥북은 진짜 갖고 싶음...",
          "speaker_id": "2",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100030865.1.25",
          "form": "에어팟 프로랑",
          "original_form": "에어팟 프로랑",
          "speaker_id": "2",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100030865.1.26",
          "form": "맥북 있으면 바로 로직 프로부터 사서 설치한다",
          "original_form": "맥북 있으면 바로 로직 프로부터 사서 설치한다",
          "speaker_id": "2",
          "time": "20210904 16:34"
        },
        {
          "id": "MDRW2100030865.1.27",
          "form": "와우...전부 비싼 것...",
          "original_form": "와우...전부 비싼 것...",
          "speaker_id": "1",
          "time": "20210904 17:01"
        },
        {
          "id": "MDRW2100030865.1.28",
          "form": "근데 나도 컴 봏은 걸로 맞투고 싶어",
          "original_form": "근데 나도 컴 봏은 걸로 맞투고 싶어",
          "speaker_id": "1",
          "time": "20210904 17:01"
        },
        {
          "id": "MDRW2100030865.1.29",
          "form": "색감 때문에 스트레스 받아...",
          "original_form": "색감 때문에 스트레스 받아...",
          "speaker_id": "1",
          "time": "20210904 17:01"
        },
        {
          "id": "MDRW2100030865.1.30",
          "form": "확실히 그림 그리는 사람이니",
          "original_form": "확실히 그림 그리는 사람이니",
          "speaker_id": "2",
          "time": "20210904 17:59"
        },
        {
          "id": "MDRW2100030865.1.31",
          "form": "색감에 예민하구나",
          "original_form": "색감에 예민하구나",
          "speaker_id": "2",
          "time": "20210904 17:59"
        },
        {
          "id": "MDRW2100030865.1.32",
          "form": "색감 차이 너무 나면... 빡치지...",
          "original_form": "색감 차이 너무 나면... 빡치지...",
          "speaker_id": "1",
          "time": "20210904 18:44"
        },
        {
          "id": "MDRW2100030865.1.33",
          "form": "내가 칠한 색은 이게 아닌데...",
          "original_form": "내가 칠한 색은 이게 아닌데...",
          "speaker_id": "1",
          "time": "20210904 18:44"
        }
      ]
