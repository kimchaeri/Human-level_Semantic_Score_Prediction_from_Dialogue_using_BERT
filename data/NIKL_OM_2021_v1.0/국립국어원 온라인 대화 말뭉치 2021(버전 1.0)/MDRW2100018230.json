      "utterance": [
        {
          "id": "MDRW2100018230.1.1",
          "form": "최근에 영화 보신 거 있나요?",
          "original_form": "최근에 영화 보신 거 있나요?",
          "speaker_id": "1",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100018230.1.2",
          "form": "음 영화관가서 본 영화는 기억이안나고",
          "original_form": "음 영화관가서 본 영화는 기억이안나고",
          "speaker_id": "2",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100018230.1.3",
          "form": "티빙으로 봤어요",
          "original_form": "티빙으로 봤어요",
          "speaker_id": "2",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018230.1.4",
          "form": "맞아요 요즘 코로나때문에 영화관 가기 쉽지 않죠",
          "original_form": "맞아요 요즘 코로나때문에 영화관 가기 쉽지 않죠",
          "speaker_id": "1",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018230.1.5",
          "form": "티빙으로 무슨 영화 보셨나요?",
          "original_form": "티빙으로 무슨 영화 보셨나요?",
          "speaker_id": "1",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018230.1.6",
          "form": "예전영화인데",
          "original_form": "예전영화인데",
          "speaker_id": "2",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018230.1.7",
          "form": "불한당 봤습니다",
          "original_form": "불한당 봤습니다",
          "speaker_id": "2",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018230.1.8",
          "form": "아 그 임시완 나오는 영화 맞나요?",
          "original_form": "아 그 임시완 나오는 영화 맞나요?",
          "speaker_id": "1",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018230.1.9",
          "form": "보진 않았지만 제목은 들어봤어요",
          "original_form": "보진 않았지만 제목은 들어봤어요",
          "speaker_id": "1",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100018230.1.10",
          "form": "네 임시완분이랑 설경구 배우분 나오는 영화입니다",
          "original_form": "네 임시완분이랑 설경구 배우분 나오는 영화입니다",
          "speaker_id": "2",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018230.1.11",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100018230.1.12",
          "form": "재밌게 보셨나요?",
          "original_form": "재밌게 보셨나요?",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100018230.1.13",
          "form": "누아르 영화라서",
          "original_form": "누아르 영화라서",
          "speaker_id": "2",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100018230.1.14",
          "form": "잔인했지만 재밌었어요",
          "original_form": "잔인했지만 재밌었어요",
          "speaker_id": "2",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100018230.1.15",
          "form": "최근에 보신 영화 있으세요?",
          "original_form": "최근에 보신 영화 있으세요?",
          "speaker_id": "2",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100018230.1.16",
          "form": "저도 최근에 본 영화가 집에서 본 건데요",
          "original_form": "저도 최근에 본 영화가 집에서 본 건데요",
          "speaker_id": "1",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018230.1.17",
          "form": "소울이라는 픽사애니메이션을 봤어요",
          "original_form": "소울이라는 픽사애니메이션을 봤어요",
          "speaker_id": "1",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018230.1.18",
          "form": "처음 들어보는데",
          "original_form": "처음 들어보는데",
          "speaker_id": "2",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018230.1.19",
          "form": "어떤 내용인가요?",
          "original_form": "어떤 내용인가요?",
          "speaker_id": "2",
          "time": "20210826 23:26"
        },
        {
          "id": "MDRW2100018230.1.20",
          "form": "재즈피아니스트가 되고 싶었던 주인공이 사고로 죽게 되었다가 아직 태어나지 않은 영혼의 세계에 가게 되는 이야기에요",
          "original_form": "재즈피아니스트가 되고 싶었던 주인공이 사고로 죽게 되었다가 아직 태어나지 않은 영혼의 세계에 가게 되는 이야기에요",
          "speaker_id": "1",
          "time": "20210826 23:27"
        },
        {
          "id": "MDRW2100018230.1.21",
          "form": "저는 나름 감명깊게 봤어요",
          "original_form": "저는 나름 감명깊게 봤어요",
          "speaker_id": "1",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100018230.1.22",
          "form": "자신의 삶을 찾아가는 여정이랄까 그런 내용이 담겨있거든요",
          "original_form": "자신의 삶을 찾아가는 여정이랄까 그런 내용이 담겨있거든요",
          "speaker_id": "1",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100018230.1.23",
          "form": "저도 시간되면 한번 찾아봐야겠어요",
          "original_form": "저도 시간되면 한번 찾아봐야겠어요",
          "speaker_id": "2",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018230.1.24",
          "form": "재밌을거같네요",
          "original_form": "재밌을거같네요",
          "speaker_id": "2",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100018230.1.25",
          "form": "네 추천드려요!!",
          "original_form": "네 추천드려요!!",
          "speaker_id": "1",
          "time": "20210826 23:30"
        }
      ]
    }
