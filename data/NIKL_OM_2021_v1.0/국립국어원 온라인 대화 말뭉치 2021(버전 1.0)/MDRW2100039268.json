      },
      "utterance": [
        {
          "id": "MDRW2100039268.1.1",
          "form": "고구마 맛있었음 좋겠다",
          "original_form": "고구마 맛있었음 좋겠다",
          "speaker_id": "2",
          "time": "20210903 15:25"
        },
        {
          "id": "MDRW2100039268.1.2",
          "form": "안 그래도 오자마자",
          "original_form": "안 그래도 오자마자",
          "speaker_id": "1",
          "time": "20210903 15:25"
        },
        {
          "id": "MDRW2100039268.1.3",
          "form": "쪄서 먹어봤다",
          "original_form": "쪄서 먹어봤다",
          "speaker_id": "1",
          "time": "20210903 15:25"
        },
        {
          "id": "MDRW2100039268.1.4",
          "form": "ㅋㅋㅋㅋㅋ빠르군",
          "original_form": "ㅋㅋㅋㅋㅋ빠르군",
          "speaker_id": "2",
          "time": "20210903 15:25"
        },
        {
          "id": "MDRW2100039268.1.5",
          "form": "밥솥으로 쪘어?",
          "original_form": "밥솥으로 쪘어?",
          "speaker_id": "2",
          "time": "20210903 15:26"
        },
        {
          "id": "MDRW2100039268.1.6",
          "form": "밤고구마는 에프돌림 안되잖아",
          "original_form": "밤고구마는 에프돌림 안되잖아",
          "speaker_id": "1",
          "time": "20210903 15:26"
        },
        {
          "id": "MDRW2100039268.1.7",
          "form": "밥솥으로 했지",
          "original_form": "밥솥으로 했지",
          "speaker_id": "1",
          "time": "20210903 15:26"
        },
        {
          "id": "MDRW2100039268.1.8",
          "form": "금방 되",
          "original_form": "금방 되",
          "speaker_id": "1",
          "time": "20210903 15:26"
        },
        {
          "id": "MDRW2100039268.1.9",
          "form": "ㅇㅇ어때?",
          "original_form": "ㅇㅇ어때?",
          "speaker_id": "2",
          "time": "20210903 15:26"
        },
        {
          "id": "MDRW2100039268.1.10",
          "form": "밤이야?",
          "original_form": "밤이야?",
          "speaker_id": "2",
          "time": "20210903 15:26"
        },
        {
          "id": "MDRW2100039268.1.11",
          "form": "완전 목막히더라",
          "original_form": "완전 목막히더라",
          "speaker_id": "1",
          "time": "20210903 15:26"
        },
        {
          "id": "MDRW2100039268.1.12",
          "form": "바로 캐서 보내는 거라던데",
          "original_form": "바로 캐서 보내는 거라던데",
          "speaker_id": "2",
          "time": "20210903 15:27"
        },
        {
          "id": "MDRW2100039268.1.13",
          "form": "그러게 완전 밤고구마긴한데",
          "original_form": "그러게 완전 밤고구마긴한데",
          "speaker_id": "1",
          "time": "20210903 15:27"
        },
        {
          "id": "MDRW2100039268.1.14",
          "form": "당도가 없다",
          "original_form": "당도가 없다",
          "speaker_id": "1",
          "time": "20210903 15:27"
        },
        {
          "id": "MDRW2100039268.1.15",
          "form": "좀 놔뒀다 먹어야겠네",
          "original_form": "좀 놔뒀다 먹어야겠네",
          "speaker_id": "2",
          "time": "20210903 15:27"
        },
        {
          "id": "MDRW2100039268.1.16",
          "form": "이주정도 뒀다 먹어야될 거 같애",
          "original_form": "이주정도 뒀다 먹어야될 거 같애",
          "speaker_id": "1",
          "time": "20210903 15:27"
        },
        {
          "id": "MDRW2100039268.1.17",
          "form": "지금은 별로야?",
          "original_form": "지금은 별로야?",
          "speaker_id": "2",
          "time": "20210903 15:27"
        },
        {
          "id": "MDRW2100039268.1.18",
          "form": "지난번에 바로 캔거 먹었을땐 감자같긴 하더라",
          "original_form": "지난번에 바로 캔거 먹었을땐 감자같긴 하더라",
          "speaker_id": "2",
          "time": "20210903 15:28"
        },
        {
          "id": "MDRW2100039268.1.19",
          "form": "이것도 감자다",
          "original_form": "이것도 감자다",
          "speaker_id": "1",
          "time": "20210903 15:28"
        },
        {
          "id": "MDRW2100039268.1.20",
          "form": "헐ㅋㅋㄱㅋ",
          "original_form": "헐ㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210903 15:28"
        },
        {
          "id": "MDRW2100039268.1.21",
          "form": "한참있어야겠네ㅠㅜ",
          "original_form": "한참있어야겠네ㅠㅜ",
          "speaker_id": "2",
          "time": "20210903 15:28"
        },
        {
          "id": "MDRW2100039268.1.22",
          "form": "근데 고구마가 노랗더라",
          "original_form": "근데 고구마가 노랗더라",
          "speaker_id": "1",
          "time": "20210903 15:28"
        },
        {
          "id": "MDRW2100039268.1.23",
          "form": "밤은 꽉찼어",
          "original_form": "밤은 꽉찼어",
          "speaker_id": "1",
          "time": "20210903 15:28"
        },
        {
          "id": "MDRW2100039268.1.24",
          "form": "대박이다",
          "original_form": "대박이다",
          "speaker_id": "2",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100039268.1.25",
          "form": "그럼 당 올라오면 진짜 맛있겠다",
          "original_form": "그럼 당 올라오면 진짜 맛있겠다",
          "speaker_id": "2",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100039268.1.26",
          "form": "그럴거 같애",
          "original_form": "그럴거 같애",
          "speaker_id": "1",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100039268.1.27",
          "form": "일단 그럼 산청꺼부터 먹어야겠다ㅋㅋ",
          "original_form": "일단 그럼 산청꺼부터 먹어야겠다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100039268.1.28",
          "form": "나 그거 아직 안 먹어봄",
          "original_form": "나 그거 아직 안 먹어봄",
          "speaker_id": "2",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100039268.1.29",
          "form": "아직도?",
          "original_form": "아직도?",
          "speaker_id": "1",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100039268.1.30",
          "form": "그게 지금 젤 달아",
          "original_form": "그게 지금 젤 달아",
          "speaker_id": "1",
          "time": "20210903 15:30"
        },
        {
          "id": "MDRW2100039268.1.31",
          "form": "그건 캐놨던 건가봐",
          "original_form": "그건 캐놨던 건가봐",
          "speaker_id": "2",
          "time": "20210903 15:30"
        },
        {
          "id": "MDRW2100039268.1.32",
          "form": "걔도 온지 얼마 안됐는데",
          "original_form": "걔도 온지 얼마 안됐는데",
          "speaker_id": "2",
          "time": "20210903 15:30"
        },
        {
          "id": "MDRW2100039268.1.33",
          "form": "그런가봐 그것도 완전 밤이야",
          "original_form": "그런가봐 그것도 완전 밤이야",
          "speaker_id": "1",
          "time": "20210903 15:30"
        },
        {
          "id": "MDRW2100039268.1.34",
          "form": "진짜 좋다",
          "original_form": "진짜 좋다",
          "speaker_id": "2",
          "time": "20210903 15:30"
        },
        {
          "id": "MDRW2100039268.1.35",
          "form": "올해 농산물 대박이다 진짜",
          "original_form": "올해 농산물 대박이다 진짜",
          "speaker_id": "2",
          "time": "20210903 15:31"
        }
      ]
    }
  ]
