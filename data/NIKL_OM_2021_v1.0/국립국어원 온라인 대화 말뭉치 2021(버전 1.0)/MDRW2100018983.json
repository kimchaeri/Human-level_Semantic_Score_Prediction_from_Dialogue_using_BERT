      "utterance": [
        {
          "id": "MDRW2100018983.1.1",
          "form": "방청소 자주 하시나요?",
          "original_form": "방청소 자주 하시나요?",
          "speaker_id": "1",
          "time": "20210827 14:22"
        },
        {
          "id": "MDRW2100018983.1.2",
          "form": "저 살만큼만요ㅋㅋ name1님은여 ㅋ",
          "original_form": "저 살만큼만요ㅋㅋ &name1&님은여 ㅋ",
          "speaker_id": "2",
          "time": "20210827 14:29"
        },
        {
          "id": "MDRW2100018983.1.3",
          "form": "저두요ㅋㅋㅋㅋ",
          "original_form": "저두요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100018983.1.4",
          "form": "바닥을 닦을 때가 됐는데 너무 귀찮네요",
          "original_form": "바닥을 닦을 때가 됐는데 너무 귀찮네요",
          "speaker_id": "1",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100018983.1.5",
          "form": "저도..",
          "original_form": "저도..",
          "speaker_id": "2",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100018983.1.6",
          "form": "닦아야해요",
          "original_form": "닦아야해요",
          "speaker_id": "2",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100018983.1.7",
          "form": "하.. 바닥에 러그가 깔려져있어서 더 귀찮은듯해요",
          "original_form": "하.. 바닥에 러그가 깔려져있어서 더 귀찮은듯해요",
          "speaker_id": "1",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100018983.1.8",
          "form": "빨래는 좋아하시나요?",
          "original_form": "빨래는 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100018983.1.9",
          "form": "아.. 청소기로",
          "original_form": "아.. 청소기로",
          "speaker_id": "2",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100018983.1.10",
          "form": "걸리네요",
          "original_form": "걸리네요",
          "speaker_id": "2",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100018983.1.11",
          "form": "빨래는 세탁기가 하쥬 ㅋㅋㅋ",
          "original_form": "빨래는 세탁기가 하쥬 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100018983.1.12",
          "form": "귀찮아서 살만큼해요",
          "original_form": "귀찮아서 살만큼해요",
          "speaker_id": "2",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100018983.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ그쵸",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그쵸",
          "speaker_id": "1",
          "time": "20210827 15:01"
        },
        {
          "id": "MDRW2100018983.1.14",
          "form": "저 최근에 건조기를 샀거든요",
          "original_form": "저 최근에 건조기를 샀거든요",
          "speaker_id": "1",
          "time": "20210827 15:01"
        },
        {
          "id": "MDRW2100018983.1.15",
          "form": "왜 이제야 샀나 싶어요 너무 편하네요,,",
          "original_form": "왜 이제야 샀나 싶어요 너무 편하네요,,",
          "speaker_id": "1",
          "time": "20210827 15:01"
        },
        {
          "id": "MDRW2100018983.1.16",
          "form": "빨래가 아주 약간 좋아졌어요",
          "original_form": "빨래가 아주 약간 좋아졌어요",
          "speaker_id": "1",
          "time": "20210827 15:01"
        },
        {
          "id": "MDRW2100018983.1.17",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210827 15:02"
        },
        {
          "id": "MDRW2100018983.1.18",
          "form": "건조기가 삶의질을 높여줘요 ㅎㅎ",
          "original_form": "건조기가 삶의질을 높여줘요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 15:02"
        },
        {
          "id": "MDRW2100018983.1.19",
          "form": "비많이오면 도움되요",
          "original_form": "비많이오면 도움되요",
          "speaker_id": "2",
          "time": "20210827 15:02"
        },
        {
          "id": "MDRW2100018983.1.20",
          "form": "맞아요 엄청 높아지더라구요",
          "original_form": "맞아요 엄청 높아지더라구요",
          "speaker_id": "1",
          "time": "20210827 15:03"
        },
        {
          "id": "MDRW2100018983.1.21",
          "form": "건조기 써보니까 식기세척기도 사고 싶어졌어요",
          "original_form": "건조기 써보니까 식기세척기도 사고 싶어졌어요",
          "speaker_id": "1",
          "time": "20210827 15:03"
        },
        {
          "id": "MDRW2100018983.1.22",
          "form": "저건 얼마나 편할까 싶어서..ㅋㅋㅋ",
          "original_form": "저건 얼마나 편할까 싶어서..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:03"
        },
        {
          "id": "MDRW2100018983.1.23",
          "form": "식기세척기는 안써봤지만 다시 행구거나 큰설거지는 알아서 해야한다구 들었어요",
          "original_form": "식기세척기는 안써봤지만 다시 행구거나 큰설거지는 알아서 해야한다구 들었어요",
          "speaker_id": "2",
          "time": "20210827 15:22"
        },
        {
          "id": "MDRW2100018983.1.24",
          "form": "아 맞아요 ㅠㅠ 그건 좀 귀찮을거 같아요",
          "original_form": "아 맞아요 ㅠㅠ 그건 좀 귀찮을거 같아요",
          "speaker_id": "1",
          "time": "20210827 16:17"
        },
        {
          "id": "MDRW2100018983.1.25",
          "form": "저희집은 후라이팬도 다 크고 그래서",
          "original_form": "저희집은 후라이팬도 다 크고 그래서",
          "speaker_id": "1",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100018983.1.26",
          "form": "효율이 좀 떨어질듯",
          "original_form": "효율이 좀 떨어질듯",
          "speaker_id": "1",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100018983.1.27",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100018983.1.28",
          "form": "손이 힘드네",
          "original_form": "손이 힘드네",
          "speaker_id": "2",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100018983.1.29",
          "form": "에어프라이기 있으세요? 걔도 못들어가잖아요",
          "original_form": "에어프라이기 있으세요? 걔도 못들어가잖아요",
          "speaker_id": "2",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100018983.1.30",
          "form": "에어프라이기 이사오면서 당근에 팔았어요ㅠㅠ",
          "original_form": "에어프라이기 이사오면서 당근에 팔았어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100018983.1.31",
          "form": "팔고나니 후회되더라구요",
          "original_form": "팔고나니 후회되더라구요",
          "speaker_id": "1",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100018983.1.32",
          "form": "유용했는데...",
          "original_form": "유용했는데...",
