      "utterance": [
        {
          "id": "MDRW2100044251.1.1",
          "form": "볶음면이요? 그거 맛있나요?",
          "original_form": "볶음면이요? 그거 맛있나요?",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044251.1.2",
          "form": "맛잇어요 신라면 맛은 아니더라고요",
          "original_form": "맛잇어요 신라면 맛은 아니더라고요",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044251.1.3",
          "form": "집에 있었던 거 같은데 궁금하네요 ㅋㅋ",
          "original_form": "집에 있었던 거 같은데 궁금하네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044251.1.4",
          "form": "밥도 비벼먹으면 맛나요\n드셔보세요",
          "original_form": "밥도 비벼먹으면 맛나요\n드셔보세요",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044251.1.5",
          "form": "위장 상태 좋을 때 먹어봐야겠네요 ㅋㅋㅋ",
          "original_form": "위장 상태 좋을 때 먹어봐야겠네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044251.1.6",
          "form": "네 생각보다 맵더라고요",
          "original_form": "네 생각보다 맵더라고요",
          "speaker_id": "1",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044251.1.7",
          "form": "마라탕도 제일 약한 맛으로 시켜먹고 그래요ㅎㅎ",
          "original_form": "마라탕도 제일 약한 맛으로 시켜먹고 그래요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044251.1.8",
          "form": "마라탕은 약한맛 먹어도 맛잇더라고요",
          "original_form": "마라탕은 약한맛 먹어도 맛잇더라고요",
          "speaker_id": "1",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044251.1.9",
          "form": "맞아요. 넘 매우면 힘들어서ㅋㅋㅠ",
          "original_form": "맞아요. 넘 매우면 힘들어서ㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044251.1.10",
          "form": "저도 마라탕 약한맛으로 배달 자주시켜요",
          "original_form": "저도 마라탕 약한맛으로 배달 자주시켜요",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044251.1.11",
          "form": "밀키트로도 먹어봤는데 괜찮더라구요 \n매운맛은 조절하면 되고",
          "original_form": "밀키트로도 먹어봤는데 괜찮더라구요 \n매운맛은 조절하면 되고",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044251.1.12",
          "form": "요즘은 밀키트가 없는게없네요 진짜",
          "original_form": "요즘은 밀키트가 없는게없네요 진짜",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044251.1.13",
          "form": "편하고 맛있고 좋더라구요 ㅋㅋ",
          "original_form": "편하고 맛있고 좋더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:33"
        }
      ]
