        },
        {
          "id": "MDRW2100000929.1.5",
          "form": "왜요??ㅋㅋㅋㅋㅋㅋㅋ땀흘리셨나욤;;;;;;;",
          "original_form": "왜요??ㅋㅋㅋㅋㅋㅋㅋ땀흘리셨나욤;;;;;;;",
          "speaker_id": "1",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000929.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000929.1.7",
          "form": "네 진짜 혹시나 그럴까봐 긴장하니까",
          "original_form": "네 진짜 혹시나 그럴까봐 긴장하니까",
          "speaker_id": "2",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000929.1.8",
          "form": "더 땀나는 느낌",
          "original_form": "더 땀나는 느낌",
          "speaker_id": "2",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000929.1.9",
          "form": "요즘 날씨 진짜 종잡을 수가 없어요",
          "original_form": "요즘 날씨 진짜 종잡을 수가 없어요",
          "speaker_id": "2",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000929.1.10",
          "form": "밤에는 춥고!!!",
          "original_form": "밤에는 춥고!!!",
          "speaker_id": "2",
          "time": "20210819 20:43"
        },
        {
          "id": "MDRW2100000929.1.11",
          "form": "ㅋㅋㅋㅋㅋ저는 그래서 회색옷은 겨울옷만 있습니당 ㅡ.ㅡ ㅋㅋㅋ크크",
          "original_form": "ㅋㅋㅋㅋㅋ저는 그래서 회색옷은 겨울옷만 있습니당 ㅡ.ㅡ ㅋㅋㅋ크크",
          "speaker_id": "1",
          "time": "20210819 20:44"
        },
        {
          "id": "MDRW2100000929.1.12",
          "form": "ㅠㅠㅠ밤에진짜 추워여",
          "original_form": "ㅠㅠㅠ밤에진짜 추워여",
          "speaker_id": "1",
          "time": "20210819 20:44"
        },
        {
          "id": "MDRW2100000929.1.13",
          "form": "제방에 에어컨이없어가지구",
          "original_form": "제방에 에어컨이없어가지구",
          "speaker_id": "1",
          "time": "20210819 20:44"
        },
        {
          "id": "MDRW2100000929.1.14",
          "form": "침대에 대나무돗자리같은거 깔구 자는데",
          "original_form": "침대에 대나무돗자리같은거 깔구 자는데",
          "speaker_id": "1",
          "time": "20210819 20:44"
        },
        {
          "id": "MDRW2100000929.1.15",
          "form": "요 며칠간 대나무가 ㅈ제 체온을 다 뺏아가서 넘 추웠어요",
          "original_form": "요 며칠간 대나무가 ㅈ제 체온을 다 뺏아가서 넘 추웠어요",
          "speaker_id": "1",
          "time": "20210819 20:44"
        },
        {
          "id": "MDRW2100000929.1.16",
          "form": "ㅋㅋㅌㅋㅋㅋㅋ근데 치우기 귀찮아서 그만..",
          "original_form": "ㅋㅋㅌㅋㅋㅋㅋ근데 치우기 귀찮아서 그만..",
          "speaker_id": "1",
          "time": "20210819 20:45"
        },
        {
          "id": "MDRW2100000929.1.17",
          "form": "저도 거실에만 있어서 인견이불!",
          "original_form": "저도 거실에만 있어서 인견이불!",
          "speaker_id": "2",
          "time": "20210819 20:45"
        },
        {
          "id": "MDRW2100000929.1.18",
          "form": "근데 싸늘하더라고요",
          "original_form": "근데 싸늘하더라고요",
          "speaker_id": "2",
          "time": "20210819 20:45"
        },
        {
          "id": "MDRW2100000929.1.19",
          "form": "맞아요 맞아",
          "original_form": "맞아요 맞아",
          "speaker_id": "2",
          "time": "20210819 20:45"
        },
        {
          "id": "MDRW2100000929.1.20",
          "form": "근데 낮에는 진짜 두피가 따가울 지경!",
          "original_form": "근데 낮에는 진짜 두피가 따가울 지경!",
          "speaker_id": "2",
          "time": "20210819 20:45"
        },
        {
          "id": "MDRW2100000929.1.21",
          "form": "근데 주말부터는 장마라는 아이러니",
          "original_form": "근데 주말부터는 장마라는 아이러니",
          "speaker_id": "2",
          "time": "20210819 20:45"
        },
        {
          "id": "MDRW2100000929.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요 날씨 알수없움...!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요 날씨 알수없움...!!",
          "speaker_id": "1",
          "time": "20210819 20:45"
        },
        {
          "id": "MDRW2100000929.1.23",
          "form": "근데 생각해보니깡",
          "original_form": "근데 생각해보니깡",
          "speaker_id": "1",
          "time": "20210819 20:45"
        },
        {
          "id": "MDRW2100000929.1.24",
          "form": "이번 여름엔 태풍이 안온게 신기해여!!",
          "original_form": "이번 여름엔 태풍이 안온게 신기해여!!",
          "speaker_id": "1",
          "time": "20210819 20:45"
        },
        {
          "id": "MDRW2100000929.1.25",
          "form": "매년 여름에 두개씩은 온거같은데..",
          "original_form": "매년 여름에 두개씩은 온거같은데..",
          "speaker_id": "1",
          "time": "20210819 20:46"
        },
        {
          "id": "MDRW2100000929.1.26",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210819 20:46"
        },
        {
          "id": "MDRW2100000929.1.27",
          "form": "작년 생각해 보면 진짜",
          "original_form": "작년 생각해 보면 진짜",
          "speaker_id": "2",
          "time": "20210819 20:46"
        },
        {
          "id": "MDRW2100000929.1.28",
          "form": "너무 달라요",
          "original_form": "너무 달라요",
          "speaker_id": "2",
          "time": "20210819 20:46"
        },
        {
          "id": "MDRW2100000929.1.29",
          "form": "작년 여름에는 해 보기가 어려웠는데",
          "original_form": "작년 여름에는 해 보기가 어려웠는데",
          "speaker_id": "2",
          "time": "20210819 20:46"
        },
        {
          "id": "MDRW2100000929.1.30",
          "form": "비가 비가 지겹게 왔잖아요",
          "original_form": "비가 비가 지겹게 왔잖아요",
          "speaker_id": "2",
          "time": "20210819 20:46"
        },
        {
          "id": "MDRW2100000929.1.31",
          "form": "으악 맞아요 맨날 비 비 ㅠㅠㅠ",
          "original_form": "으악 맞아요 맨날 비 비 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 20:49"
        },
        {
          "id": "MDRW2100000929.1.32",
          "form": "요새 날씨 종잡을 수 없어여 기온두 넘넘 오르구........",
          "original_form": "요새 날씨 종잡을 수 없어여 기온두 넘넘 오르구........",
          "speaker_id": "1",
          "time": "20210819 20:49"
        },
        {
          "id": "MDRW2100000929.1.33",
          "form": "기후위기가....오고있어여..!!!!",
          "original_form": "기후위기가....오고있어여..!!!!",
          "speaker_id": "1",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000929.1.34",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000929.1.35",
          "form": "사계절이 아니고 이제 두계절",
          "original_form": "사계절이 아니고 이제 두계절",
          "speaker_id": "2",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000929.1.36",
          "form": "여름-겨울 끝!",
          "original_form": "여름-겨울 끝!",
          "speaker_id": "2",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000929.1.37",
          "form": "두계절 ㅠㅠㅠ",
          "original_form": "두계절 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 20:55"
        },
        {
          "id": "MDRW2100000929.1.38",
          "form": "봄이랑 가을이 젤 좋은데",
          "original_form": "봄이랑 가을이 젤 좋은데",
          "speaker_id": "1",
          "time": "20210819 20:55"
        },
        {
          "id": "MDRW2100000929.1.39",
          "form": "어째서 그 두개만 쏙 빠져버린건지 ㅡ0ㅡ",
          "original_form": "어째서 그 두개만 쏙 빠져버린건지 ㅡ0ㅡ",
          "speaker_id": "1",
          "time": "20210819 20:55"
        },
        {
          "id": "MDRW2100000929.1.40",
          "form": "중간이없어용 아주",
          "original_form": "중간이없어용 아주",
          "speaker_id": "1",
          "time": "20210819 20:55"
        },
        {
          "id": "MDRW2100000929.1.41",
          "form": "오늘두 퇴근하다가 갑자기",
          "original_form": "오늘두 퇴근하다가 갑자기",
          "speaker_id": "1",
          "time": "20210819 20:55"
        },
