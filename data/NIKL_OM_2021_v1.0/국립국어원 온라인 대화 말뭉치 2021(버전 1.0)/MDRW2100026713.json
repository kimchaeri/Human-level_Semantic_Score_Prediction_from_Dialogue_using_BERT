      "utterance": [
        {
          "id": "MDRW2100026713.1.1",
          "form": "님 요술 소녀 아세요? ㅋㅋ",
          "original_form": "님 요술 소녀 아세요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100026713.1.2",
          "form": "알아요! 그 자매인가 쌍둥이인가 맞죠",
          "original_form": "알아요! 그 자매인가 쌍둥이인가 맞죠",
          "speaker_id": "1",
          "time": "20210902 10:37"
        },
        {
          "id": "MDRW2100026713.1.3",
          "form": "헐 이거 아는사람 진짜 못봤는데 ㅜㅜ 막 둘이 텔레파시하고 그러잖아요 ㅋㅋ 텔레파시로 시험답 공유하고그러는편이 기억에 남아요 ㅋㅋ",
          "original_form": "헐 이거 아는사람 진짜 못봤는데 ㅜㅜ 막 둘이 텔레파시하고 그러잖아요 ㅋㅋ 텔레파시로 시험답 공유하고그러는편이 기억에 남아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:50"
        },
        {
          "id": "MDRW2100026713.1.4",
          "form": "알죠알죠ㅠ엠비씨에서 한것도 아는걸요",
          "original_form": "알죠알죠ㅠ엠비씨에서 한것도 아는걸요",
          "speaker_id": "1",
          "time": "20210902 11:52"
        },
        {
          "id": "MDRW2100026713.1.5",
          "form": "진짜 재밌게봤는데 ㅠ",
          "original_form": "진짜 재밌게봤는데 ㅠ",
          "speaker_id": "1",
          "time": "20210902 11:52"
        },
        {
          "id": "MDRW2100026713.1.6",
          "form": "파이팅 대운동회는 아시나요?!",
          "original_form": "파이팅 대운동회는 아시나요?!",
          "speaker_id": "2",
          "time": "20210902 11:55"
        },
        {
          "id": "MDRW2100026713.1.7",
          "form": "알죠!!!!!!!! ㅋㅋ진짜 재밌게봤어요",
          "original_form": "알죠!!!!!!!! ㅋㅋ진짜 재밌게봤어요",
          "speaker_id": "1",
          "time": "20210902 12:19"
        },
        {
          "id": "MDRW2100026713.1.8",
          "form": "ㅋㅋㅋ막 엄마도 달리기 선수였고 그랬던거같은제 ㅋㅋㅋ 엄마가 막 레전드 선수 ㅋㅋㅋㅋ우주가 배경이라 꽤나 신선했죠 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ막 엄마도 달리기 선수였고 그랬던거같은제 ㅋㅋㅋ 엄마가 막 레전드 선수 ㅋㅋㅋㅋ우주가 배경이라 꽤나 신선했죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:20"
        },
        {
          "id": "MDRW2100026713.1.9",
          "form": "맞아요ㅋㅋㅋㅋㅋㄱ엄마가",
          "original_form": "맞아요ㅋㅋㅋㅋㅋㄱ엄마가",
          "speaker_id": "1",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100026713.1.10",
          "form": "그거 비슷하게",
          "original_form": "그거 비슷하게",
          "speaker_id": "1",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100026713.1.11",
          "form": "그...인형끼리 싸우는거 아시나요",
          "original_form": "그...인형끼리 싸우는거 아시나요",
          "speaker_id": "1",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100026713.1.12",
          "form": "엔젤릭 파이터인가",
          "original_form": "엔젤릭 파이터인가",
          "speaker_id": "1",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100026713.1.13",
          "form": "헐알죠알죠 엔젤릭레이어 ㅋㅋㅋ저이거 책으로도봤어요 ㅋㅋㅋ",
          "original_form": "헐알죠알죠 엔젤릭레이어 ㅋㅋㅋ저이거 책으로도봤어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:25"
        },
        {
          "id": "MDRW2100026713.1.14",
          "form": "오 맞아요 엔젤릭레이어",
          "original_form": "오 맞아요 엔젤릭레이어",
          "speaker_id": "1",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100026713.1.15",
          "form": "와우 만화책도 보셨구나",
          "original_form": "와우 만화책도 보셨구나",
          "speaker_id": "1",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100026713.1.16",
          "form": "이거 진짜 재밌게봤는데",
          "original_form": "이거 진짜 재밌게봤는데",
          "speaker_id": "1",
          "time": "20210902 12:27"
        },
        {
          "id": "MDRW2100026713.1.17",
          "form": "이것도 엄마가 ㅋ",
          "original_form": "이것도 엄마가 ㅋ",
          "speaker_id": "1",
          "time": "20210902 12:27"
        },
        {
          "id": "MDRW2100026713.1.18",
          "form": "저 분명 봤는데 왜 기억이안나죠 ㅋㅋㅋㅋㅋㅋ어",
          "original_form": "저 분명 봤는데 왜 기억이안나죠 ㅋㅋㅋㅋㅋㅋ어",
          "speaker_id": "2",
          "time": "20210902 12:28"
        },
        {
          "id": "MDRW2100026713.1.19",
          "form": "엄마가 뭐였죠 ㅋㅋㅋ",
          "original_form": "엄마가 뭐였죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:28"
        },
        {
          "id": "MDRW2100026713.1.20",
          "form": "엄마가 제일 쎈 인형",
          "original_form": "엄마가 제일 쎈 인형",
          "speaker_id": "1",
          "time": "20210902 12:28"
        },
        {
          "id": "MDRW2100026713.1.21",
          "form": "다리 마비와서",
          "original_form": "다리 마비와서",
          "speaker_id": "1",
          "time": "20210902 12:28"
        },
        {
          "id": "MDRW2100026713.1.22",
          "form": "치료하려고 주인공 떠났는데",
          "original_form": "치료하려고 주인공 떠났는데",
          "speaker_id": "1",
          "time": "20210902 12:28"
        },
        {
          "id": "MDRW2100026713.1.23",
          "form": "거기서 만난 이야기!?",
          "original_form": "거기서 만난 이야기!?",
          "speaker_id": "1",
          "time": "20210902 12:28"
        },
        {
          "id": "MDRW2100026713.1.24",
          "form": "엄청난 서사가 있었군요 ㅋㅋㅋㅋ",
          "original_form": "엄청난 서사가 있었군요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:29"
        },
        {
          "id": "MDRW2100026713.1.25",
          "form": "막 인형고치고 이랬던 기억만 납니다 ㅋㅋㅋ 주제곡이랑 ㅋㅋㅋ",
          "original_form": "막 인형고치고 이랬던 기억만 납니다 ㅋㅋㅋ 주제곡이랑 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:29"
        },
        {
          "id": "MDRW2100026713.1.26",
          "form": "노래는 생각안나는데 대단하세요",
          "original_form": "노래는 생각안나는데 대단하세요",
          "speaker_id": "1",
          "time": "20210902 12:29"
        },
        {
          "id": "MDRW2100026713.1.27",
          "form": "그리고~ 역시 체리가 빠질수없죠",
          "original_form": "그리고~ 역시 체리가 빠질수없죠",
          "speaker_id": "1",
          "time": "20210902 12:30"
        }
