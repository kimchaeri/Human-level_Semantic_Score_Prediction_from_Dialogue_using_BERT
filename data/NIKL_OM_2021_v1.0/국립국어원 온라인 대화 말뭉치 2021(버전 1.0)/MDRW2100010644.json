      "utterance": [
        {
          "id": "MDRW2100010644.1.1",
          "form": "커피 좋아하시나요?",
          "original_form": "커피 좋아하시나요?",
          "speaker_id": "2",
          "time": "202P10823 20:P18"
        },
        {
          "id": "MDRW2100010644.1.2",
          "form": "커피 먹으면 어지러워요",
          "original_form": "커피 먹으면 어지러워요",
          "speaker_id": "1",
          "time": "202P10823 20:P19"
        },
        {
          "id": "MDRW2100010644.1.3",
          "form": "카페는 자주가는데",
          "original_form": "카페는 자주가는데",
          "speaker_id": "1",
          "time": "202P10823 20:P19"
        },
        {
          "id": "MDRW2100010644.1.4",
          "form": "아 카페인 안 받으시는군요!",
          "original_form": "아 카페인 안 받으시는군요!",
          "speaker_id": "2",
          "time": "202P10823 20:20"
        },
        {
          "id": "MDRW2100010644.1.5",
          "form": "그러더라고요ㅠㅠ 현대인의 마약이...",
          "original_form": "그러더라고요ㅠㅠ 현대인의 마약이...",
          "speaker_id": "1",
          "time": "202P10823 20:20"
        },
        {
          "id": "MDRW2100010644.1.6",
          "form": "카페 자주가세요?",
          "original_form": "카페 자주가세요?",
          "speaker_id": "1",
          "time": "202P10823 20:20"
        },
        {
          "id": "MDRW2100010644.1.7",
          "form": "아 저는 요샌 그냥 포장해서 마시는데 커피는 좋아해요 ㅎㅎ",
          "original_form": "아 저는 요샌 그냥 포장해서 마시는데 커피는 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "202P10823 20:2P1"
        },
        {
          "id": "MDRW2100010644.1.8",
          "form": "그럼 카페가면 뭐 시켜드시나요",
          "original_form": "그럼 카페가면 뭐 시켜드시나요",
          "speaker_id": "2",
          "time": "202P10823 20:2P1"
        },
        {
          "id": "MDRW2100010644.1.9",
          "form": "에이드나 티먹어요!!",
          "original_form": "에이드나 티먹어요!!",
          "speaker_id": "1",
          "time": "202P10823 20:2P1"
        },
        {
          "id": "MDRW2100010644.1.10",
          "form": "카페에 유명디저트 있으면 먹구요!",
          "original_form": "카페에 유명디저트 있으면 먹구요!",
          "speaker_id": "1",
          "time": "202P10823 20:22"
        },
        {
          "id": "MDRW2100010644.1.11",
          "form": "지난 번엔 수플레케이크 먹었는데 맛있더라고요",
          "original_form": "지난 번엔 수플레케이크 먹었는데 맛있더라고요",
          "speaker_id": "1",
          "time": "202P10823 20:22"
        },
        {
          "id": "MDRW2100010644.1.12",
          "form": "오 그거 약간 폭신한 거 맞나요 아직 안 먹어봤는데 ㅎㅎ",
          "original_form": "오 그거 약간 폭신한 거 맞나요 아직 안 먹어봤는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "202P10823 20:23"
        },
        {
          "id": "MDRW2100010644.1.13",
          "form": "맞아요! 맛있더라고요!",
          "original_form": "맞아요! 맛있더라고요!",
          "speaker_id": "1",
          "time": "202P10823 2P1:55"
        },
        {
          "id": "MDRW2100010644.1.14",
          "form": "오 다음에 한번 도전해봐야겠네요",
          "original_form": "오 다음에 한번 도전해봐야겠네요",
          "speaker_id": "2",
          "time": "202P10823 22:P16"
        },
        {
          "id": "MDRW2100010644.1.15",
          "form": "배달음식은 어떤 거 가장 많이 드시나요?",
          "original_form": "배달음식은 어떤 거 가장 많이 드시나요?",
          "speaker_id": "2",
          "time": "202P10823 22:P16"
        },
        {
          "id": "MDRW2100010644.1.16",
          "form": "전 회나 곱창볶음 같은거 많이 시켜요",
          "original_form": "전 회나 곱창볶음 같은거 많이 시켜요",
          "speaker_id": "1",
          "time": "202P10824 00:00"
        },
        {
          "id": "MDRW2100010644.1.17",
          "form": "name2은요?",
          "original_form": "&name2&은요?",
          "speaker_id": "1",
          "time": "202P10824 00:00"
        },
        {
          "id": "MDRW2100010644.1.18",
          "form": "저는 아무래도 치킨을 가장 많이 먹는 것 같네요! ㅎㅎ",
          "original_form": "저는 아무래도 치킨을 가장 많이 먹는 것 같네요! ㅎㅎ",
          "speaker_id": "2",
          "time": "202P10824 00:02"
        },
        {
          "id": "MDRW2100010644.1.19",
          "form": "치킨 맛있죠",
          "original_form": "치킨 맛있죠",
          "speaker_id": "1",
          "time": "202P10824 00:02"
        },
        {
          "id": "MDRW2100010644.1.20",
          "form": "어떤 치킨을 가장 좋아해요?",
          "original_form": "어떤 치킨을 가장 좋아해요?",
          "speaker_id": "1",
          "time": "202P10824 00:02"
        },
        {
          "id": "MDRW2100010644.1.21",
          "form": "전 굽네도 맛있더라고요",
          "original_form": "전 굽네도 맛있더라고요",
          "speaker_id": "1",
          "time": "202P10824 00:02"
        },
        {
          "id": "MDRW2100010644.1.22",
          "form": "아 굽네 맛있죠 첨에 먹을때 신세계 ㅎㅎ",
          "original_form": "아 굽네 맛있죠 첨에 먹을때 신세계 ㅎㅎ",
          "speaker_id": "2",
          "time": "202P10824 00:05"
        },
        {
          "id": "MDRW2100010644.1.23",
          "form": "전 노랑통닭 알싸한 마늘치킨 제일 좋아해요",
          "original_form": "전 노랑통닭 알싸한 마늘치킨 제일 좋아해요",
          "speaker_id": "2",
          "time": "202P10824 00:05"
        },
        {
          "id": "MDRW2100010644.1.24",
          "form": "오 맛있겠어요",
          "original_form": "오 맛있겠어요",
          "speaker_id": "1",
          "time": "202P10824 00:05"
        },
        {
          "id": "MDRW2100010644.1.25",
          "form": "한국인은 마늘이죠",
          "original_form": "한국인은 마늘이죠",
          "speaker_id": "1",
          "time": "202P10824 00:06"
        },
        {
