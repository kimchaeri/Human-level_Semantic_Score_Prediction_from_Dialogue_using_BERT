        },
        {
          "id": "MDRW2100011508.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210824 13:23"
        },
        {
          "id": "MDRW2100011508.1.3",
          "form": "유튜브 자주보시나요?",
          "original_form": "유튜브 자주보시나요?",
          "speaker_id": "1",
          "time": "20210824 13:24"
        },
        {
          "id": "MDRW2100011508.1.4",
          "form": "네 자주 보는 편이에요",
          "original_form": "네 자주 보는 편이에요",
          "speaker_id": "2",
          "time": "20210824 13:25"
        },
        {
          "id": "MDRW2100011508.1.5",
          "form": "어떤 관심사로 많이 보세요?",
          "original_form": "어떤 관심사로 많이 보세요?",
          "speaker_id": "2",
          "time": "20210824 13:25"
        },
        {
          "id": "MDRW2100011508.1.6",
          "form": "저는 여행유튜브요!",
          "original_form": "저는 여행유튜브요!",
          "speaker_id": "1",
          "time": "20210824 13:26"
        },
        {
          "id": "MDRW2100011508.1.7",
          "form": "여행 유튜브 재밌겠네요ㅎㅎ",
          "original_form": "여행 유튜브 재밌겠네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 13:27"
        },
        {
          "id": "MDRW2100011508.1.8",
          "form": "유튜브로 여행지 추천 받으시는거에요?",
          "original_form": "유튜브로 여행지 추천 받으시는거에요?",
          "speaker_id": "2",
          "time": "20210824 13:28"
        },
        {
          "id": "MDRW2100011508.1.9",
          "form": "음 요즘 해외못나가니까 대리만족해요ㅎㅇ",
          "original_form": "음 요즘 해외못나가니까 대리만족해요ㅎㅇ",
          "speaker_id": "1",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100011508.1.10",
          "form": "아 그거 좋죠!",
          "original_form": "아 그거 좋죠!",
          "speaker_id": "2",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100011508.1.11",
          "form": "유튜브 영상을 워낙 다 잘 찍어주시니까 대리만족하기 좋은 것 같아요",
          "original_form": "유튜브 영상을 워낙 다 잘 찍어주시니까 대리만족하기 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210824 13:30"
        },
        {
          "id": "MDRW2100011508.1.12",
          "form": "마자요 빠니보틀 채널 재밌더라구요ㅎㅎ",
          "original_form": "마자요 빠니보틀 채널 재밌더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 13:31"
        },
        {
          "id": "MDRW2100011508.1.13",
          "form": "그 채널에서는 외국 어느 곳을 가나요?",
          "original_form": "그 채널에서는 외국 어느 곳을 가나요?",
          "speaker_id": "2",
          "time": "20210824 13:32"
        },
        {
          "id": "MDRW2100011508.1.14",
          "form": "지금 미국여행중이에요ㅎㅎ",
          "original_form": "지금 미국여행중이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 13:35"
        },
        {
          "id": "MDRW2100011508.1.15",
          "form": "아 그 유투버는 한국에 있는 분인데 현재 미국 여행중인거에요?",
          "original_form": "아 그 유투버는 한국에 있는 분인데 현재 미국 여행중인거에요?",
          "speaker_id": "2",
          "time": "20210824 13:38"
        },
        {
          "id": "MDRW2100011508.1.16",
          "form": "네네 백신맞고 갔더라구요ㅎㅎ",
          "original_form": "네네 백신맞고 갔더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 13:38"
        },
        {
          "id": "MDRW2100011508.1.17",
          "form": "오 여행유튜브에 진심인 분이시네요ㅋㅋ",
          "original_form": "오 여행유튜브에 진심인 분이시네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:39"
        }
      ]
