      "utterance": [
        {
          "id": "MDRW2100037669.1.1",
          "form": "다른동물키워보신적있으세요!?",
          "original_form": "다른동물키워보신적있으세요!?",
          "speaker_id": "2",
          "time": "20210901 15:10"
        },
        {
          "id": "MDRW2100037669.1.2",
          "form": "엄청 어릴때 진돗개 키웠고 다른 동물은 아니요",
          "original_form": "엄청 어릴때 진돗개 키웠고 다른 동물은 아니요",
          "speaker_id": "1",
          "time": "20210901 15:10"
        },
        {
          "id": "MDRW2100037669.1.3",
          "form": "님은 키우셨나요?",
          "original_form": "님은 키우셨나요?",
          "speaker_id": "1",
          "time": "20210901 15:11"
        },
        {
          "id": "MDRW2100037669.1.4",
          "form": "와~ 초딩때 병아리.메추리정도? ㅎㅎ",
          "original_form": "와~ 초딩때 병아리.메추리정도? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 15:11"
        },
        {
          "id": "MDRW2100037669.1.5",
          "form": "병아리, 메추리라니 귀엽네요 ㅋㅋㅋㅋㅋ",
          "original_form": "병아리, 메추리라니 귀엽네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:12"
        },
        {
          "id": "MDRW2100037669.1.6",
          "form": "ㅋㅋㅋㅋ학교앞에서 팔곤했는데;; 넘옛날인가요 ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋ학교앞에서 팔곤했는데;; 넘옛날인가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 15:13"
        },
        {
          "id": "MDRW2100037669.1.7",
          "form": "아하 보통 학교 앞에 아저씨 와서 팔았어요",
          "original_form": "아하 보통 학교 앞에 아저씨 와서 팔았어요",
          "speaker_id": "1",
          "time": "20210901 15:14"
        },
        {
          "id": "MDRW2100037669.1.8",
          "form": "맞아요!! 삐약삐약하다보면 ㅋㅋㅋ 저도모르게 한마리 사서 집에서 집만들어주고 ㅋㅋ",
          "original_form": "맞아요!! 삐약삐약하다보면 ㅋㅋㅋ 저도모르게 한마리 사서 집에서 집만들어주고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:14"
        },
        {
          "id": "MDRW2100037669.1.9",
          "form": "키우진 않고 학교 앞에서 쪼그려서 구경한 기억은 있어요",
          "original_form": "키우진 않고 학교 앞에서 쪼그려서 구경한 기억은 있어요",
          "speaker_id": "1",
          "time": "20210901 15:16"
        },
        {
          "id": "MDRW2100037669.1.10",
          "form": "ㅎㅎ 추억이네여~",
          "original_form": "ㅎㅎ 추억이네여~",
          "speaker_id": "2",
          "time": "20210901 15:18"
        },
        {
          "id": "MDRW2100037669.1.11",
          "form": "나중에 공원에 묻어주고그랬는데ㅠㅠ",
          "original_form": "나중에 공원에 묻어주고그랬는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 15:18"
        },
        {
          "id": "MDRW2100037669.1.12",
          "form": "전 키우던 개는 할머니댁에 보냈어요",
          "original_form": "전 키우던 개는 할머니댁에 보냈어요",
          "speaker_id": "1",
          "time": "20210901 15:19"
        },
        {
          "id": "MDRW2100037669.1.13",
          "form": "한 십년 살았나? 할머니 돌아가신 후 시무룩하다가 결국 1년뒤 댕댕이별 갔대요",
          "original_form": "한 십년 살았나? 할머니 돌아가신 후 시무룩하다가 결국 1년뒤 댕댕이별 갔대요",
          "speaker_id": "1",
          "time": "20210901 15:20"
        },
        {
          "id": "MDRW2100037669.1.14",
          "form": "그런경우가 많죠 키우다보면 ㅠ",
          "original_form": "그런경우가 많죠 키우다보면 ㅠ",
          "speaker_id": "2",
          "time": "20210901 15:20"
        },
        {
          "id": "MDRW2100037669.1.15",
          "form": "에고  진짜 사람이랑 비슷해요ㅠ9",
          "original_form": "에고  진짜 사람이랑 비슷해요ㅠ9",
          "speaker_id": "2",
          "time": "20210901 15:20"
        },
        {
          "id": "MDRW2100037669.1.16",
          "form": "맞아요 눈치도 있고 사람같아요",
          "original_form": "맞아요 눈치도 있고 사람같아요",
          "speaker_id": "1",
          "time": "20210901 16:19"
        },
        {
          "id": "MDRW2100037669.1.17",
          "form": "세월지날수록 사람처럼 병도 다 생기고ㅠ",
          "original_form": "세월지날수록 사람처럼 병도 다 생기고ㅠ",
          "speaker_id": "2",
          "time": "20210901 16:28"
        },
        {
          "id": "MDRW2100037669.1.18",
          "form": "맞아요 병 생기죠 근데 다른 점이 있다면 수명이 길지 않다는거예요",
          "original_form": "맞아요 병 생기죠 근데 다른 점이 있다면 수명이 길지 않다는거예요",
          "speaker_id": "1",
          "time": "20210901 20:58"
        },
        {
          "id": "MDRW2100037669.1.19",
          "form": "동물농장에 엄청오래산 개나왔던데",
          "original_form": "동물농장에 엄청오래산 개나왔던데",
