      "utterance": [
        {
          "id": "MDRW2100013525.1.1",
          "form": "강아지 키우고싶은데",
          "original_form": "강아지 키우고싶은데",
          "speaker_id": "1",
          "time": "20210825 14:52"
        },
        {
          "id": "MDRW2100013525.1.2",
          "form": "강아지는 누가 케어해?",
          "original_form": "강아지는 누가 케어해?",
          "speaker_id": "2",
          "time": "20210825 14:53"
        },
        {
          "id": "MDRW2100013525.1.3",
          "form": "가족끼리 돌아가면서?",
          "original_form": "가족끼리 돌아가면서?",
          "speaker_id": "1",
          "time": "20210825 14:53"
        },
        {
          "id": "MDRW2100013525.1.4",
          "form": "그건아니지 키우자고 한사람이 해야지",
          "original_form": "그건아니지 키우자고 한사람이 해야지",
          "speaker_id": "2",
          "time": "20210825 14:53"
        },
        {
          "id": "MDRW2100013525.1.5",
          "form": "혼자는 못하지",
          "original_form": "혼자는 못하지",
          "speaker_id": "1",
          "time": "20210825 14:54"
        },
        {
          "id": "MDRW2100013525.1.6",
          "form": "왜 혼자서 못해? 하면되지",
          "original_form": "왜 혼자서 못해? 하면되지",
          "speaker_id": "2",
          "time": "20210825 14:54"
        },
        {
          "id": "MDRW2100013525.1.7",
          "form": "ㅇㅇ그럼 종은 뭐가좋을까",
          "original_form": "ㅇㅇ그럼 종은 뭐가좋을까",
          "speaker_id": "1",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013525.1.8",
          "form": "데려오려면 유기견이 좋지",
          "original_form": "데려오려면 유기견이 좋지",
          "speaker_id": "2",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013525.1.9",
          "form": "봉사하는 마음으로",
          "original_form": "봉사하는 마음으로",
          "speaker_id": "2",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013525.1.10",
          "form": "그것도 그렇네 ㅎㅎ",
          "original_form": "그것도 그렇네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 14:56"
        },
        {
          "id": "MDRW2100013525.1.11",
          "form": "강아지는 키우면 병원비가 많이 들어서 ㅠ",
          "original_form": "강아지는 키우면 병원비가 많이 들어서 ㅠ",
          "speaker_id": "2",
          "time": "20210825 14:56"
        },
        {
          "id": "MDRW2100013525.1.12",
          "form": "강아지 보험 괜찮은거 있음 좋겠네",
          "original_form": "강아지 보험 괜찮은거 있음 좋겠네",
          "speaker_id": "1",
          "time": "20210825 14:57"
        },
        {
          "id": "MDRW2100013525.1.13",
          "form": "그러겡 ㅋㅋ 근데 비쌀듯",
          "original_form": "그러겡 ㅋㅋ 근데 비쌀듯",
          "speaker_id": "2",
          "time": "20210825 14:57"
        },
        {
          "id": "MDRW2100013525.1.14",
          "form": "요즘은 인터넷에도 강아지용품 다양하더라",
          "original_form": "요즘은 인터넷에도 강아지용품 다양하더라",
          "speaker_id": "1",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100013525.1.15",
          "form": "ㅇㅇ인터넷이 더 저렴하고 다양한듯",
          "original_form": "ㅇㅇ인터넷이 더 저렴하고 다양한듯",
          "speaker_id": "2",
