          "form": "아돌리고~~",
          "original_form": "아돌리고~~",
          "speaker_id": "2",
          "time": "20210902 06:42"
        },
        {
          "id": "MDRW2100027006.1.6",
          "form": "여름지나갓다고 계곡이 안땡김",
          "original_form": "여름지나갓다고 계곡이 안땡김",
          "speaker_id": "2",
          "time": "20210902 06:42"
        },
        {
          "id": "MDRW2100027006.1.7",
          "form": "이거 이제봣고",
          "original_form": "이거 이제봣고",
          "speaker_id": "1",
          "time": "20210902 19:16"
        },
        {
          "id": "MDRW2100027006.1.8",
          "form": "블스떳고",
          "original_form": "블스떳고",
          "speaker_id": "1",
          "time": "20210902 19:16"
        },
        {
          "id": "MDRW2100027006.1.9",
          "form": "멋진대",
          "original_form": "멋진대",
          "speaker_id": "2",
          "time": "20210902 19:17"
        },
        {
          "id": "MDRW2100027006.1.10",
          "form": "하 라테일",
          "original_form": "하 라테일",
          "speaker_id": "1",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100027006.1.11",
          "form": "빨리켜저~~~~",
          "original_form": "빨리켜저~~~~",
          "speaker_id": "1",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100027006.1.12",
          "form": "맥을 끊네 ㅡㅡ",
          "original_form": "맥을 끊네 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210902 19:19"
        },
        {
          "id": "MDRW2100027006.1.13",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210903 17:16"
        },
        {
          "id": "MDRW2100027006.1.14",
          "form": "계단 내려가니간",
          "original_form": "계단 내려가니간",
          "speaker_id": "1",
          "time": "20210903 17:16"
        },
        {
          "id": "MDRW2100027006.1.15",
          "form": "피아노가 두대잇고",
          "original_form": "피아노가 두대잇고",
          "speaker_id": "1",
          "time": "20210903 17:16"
        },
        {
          "id": "MDRW2100027006.1.16",
          "form": "그냥 칵테일 먹는 사람도 잇어가지고",
          "original_form": "그냥 칵테일 먹는 사람도 잇어가지고",
          "speaker_id": "1",
          "time": "20210903 17:16"
        },
        {
          "id": "MDRW2100027006.1.17",
          "form": "여기에 녹아들려고",
          "original_form": "여기에 녹아들려고",
          "speaker_id": "1",
          "time": "20210903 17:16"
        },
        {
          "id": "MDRW2100027006.1.18",
          "form": "피아노를 다 같이 침",
          "original_form": "피아노를 다 같이 침",
          "speaker_id": "1",
          "time": "20210903 17:16"
        },
        {
          "id": "MDRW2100027006.1.19",
          "form": "근데 치다가",
          "original_form": "근데 치다가",
          "speaker_id": "1",
          "time": "20210903 17:16"
        },
        {
          "id": "MDRW2100027006.1.20",
          "form": "옆에 모르는 사람이",
          "original_form": "옆에 모르는 사람이",
          "speaker_id": "1",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100027006.1.21",
          "form": "올",
          "original_form": "올",
          "speaker_id": "2",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100027006.1.22",
          "form": "ㅇㅇ(음악가) 좋아하세요?",
          "original_form": "ㅇㅇ(음악가) 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100027006.1.23",
          "form": "해서",
          "original_form": "해서",
          "speaker_id": "1",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100027006.1.24",
          "form": "공주가",
          "original_form": "공주가",
          "speaker_id": "1",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100027006.1.25",
          "form": "네 좋아해요 햇는데",
          "original_form": "네 좋아해요 햇는데",
          "speaker_id": "1",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100027006.1.26",
          "form": "그자식이",
          "original_form": "그자식이",
          "speaker_id": "1",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100027006.1.27",
          "form": "구체적으로 뭐좋아하냐고",
          "original_form": "구체적으로 뭐좋아하냐고",
          "speaker_id": "1",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100027006.1.28",
          "form": "연주해달라하는거임",
          "original_form": "연주해달라하는거임",
          "speaker_id": "1",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100027006.1.29",
          "form": "근데 공주는 그 음악가를 모르고 그냥",
          "original_form": "근데 공주는 그 음악가를 모르고 그냥",
          "speaker_id": "1",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100027006.1.30",
          "form": "얼버무리려한거엿는데",
          "original_form": "얼버무리려한거엿는데",
          "speaker_id": "1",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100027006.1.31",
          "form": "뭐야",
          "original_form": "뭐야",
          "speaker_id": "2",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100027006.1.32",
          "form": "그래서 연주를 못해서",
          "original_form": "그래서 연주를 못해서",
          "speaker_id": "1",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100027006.1.33",
          "form": "총든사람들한테",
          "original_form": "총든사람들한테",
          "speaker_id": "1",
          "time": "20210903 17:17"
        },
        {
          "id": "MDRW2100027006.1.34",
          "form": "마 좋아한다고",
          "original_form": "마 좋아한다고",
          "speaker_id": "2",
          "time": "20210903 17:18"
        },
        {
          "id": "MDRW2100027006.1.35",
          "form": "들켜버렷음",
          "original_form": "들켜버렷음",
          "speaker_id": "1",
          "time": "20210903 17:18"
        },
        {
          "id": "MDRW2100027006.1.36",
          "form": "다칠줄아나 ㅡㅡ",
          "original_form": "다칠줄아나 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210903 17:18"
        },
        {
          "id": "MDRW2100027006.1.37",
          "form": "그래서 한번 더 변장을 하면서 도망치는데",
          "original_form": "그래서 한번 더 변장을 하면서 도망치는데",
          "speaker_id": "1",
          "time": "20210903 17:18"
        },
        {
          "id": "MDRW2100027006.1.38",
          "form": "공주느 화려한 드레스를 입고 왜인진 모름",
          "original_form": "공주느 화려한 드레스를 입고 왜인진 모름",
          "speaker_id": "1",
          "time": "20210903 17:18"
        },
        {
          "id": "MDRW2100027006.1.39",
          "form": "근데 기동성이 ㅂ좋아보이는 드레스랑",
          "original_form": "근데 기동성이 ㅂ좋아보이는 드레스랑",
          "speaker_id": "1",
          "time": "20210903 17:18"
        },
        {
          "id": "MDRW2100027006.1.40",
          "form": "할배는 집사옷으로 바뀌고",
          "original_form": "할배는 집사옷으로 바뀌고",
          "speaker_id": "1",
          "time": "20210903 17:18"
        },
        {
          "id": "MDRW2100027006.1.41",
          "form": "셋이 뒷문으로 도망ㅍ칠라다가 잠겨서",
          "original_form": "셋이 뒷문으로 도망ㅍ칠라다가 잠겨서",
          "speaker_id": "1",
          "time": "20210903 17:18"
        },
        {
          "id": "MDRW2100027006.1.42",
          "form": "창고로 도망갓음",
          "original_form": "창고로 도망갓음",
          "speaker_id": "1",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100027006.1.43",
          "form": "둘은 창고에 숨고 내가 마지막에",
          "original_form": "둘은 창고에 숨고 내가 마지막에",
          "speaker_id": "1",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100027006.1.44",
          "form": "문 닫고 잠글라하는데",
          "original_form": "문 닫고 잠글라하는데",
          "speaker_id": "1",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100027006.1.45",
          "form": "누가 잠금장치를 부숴버리는거에요 닫기도전에",
          "original_form": "누가 잠금장치를 부숴버리는거에요 닫기도전에",
          "speaker_id": "1",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100027006.1.46",
          "form": "그래서 이거 망햇다 싶엇는데",
          "original_form": "그래서 이거 망햇다 싶엇는데",
          "speaker_id": "1",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100027006.1.47",
          "form": "어떤 금발 여성이 와서",
          "original_form": "어떤 금발 여성이 와서",
          "speaker_id": "1",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100027006.1.48",
          "form": "근데 옷이 딷봐도 여기 손님으로 온건 아니고 무슨",
          "original_form": "근데 옷이 딷봐도 여기 손님으로 온건 아니고 무슨",
          "speaker_id": "1",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100027006.1.49",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100027006.1.50",
          "form": "헌터직종같이 생김",
          "original_form": "헌터직종같이 생김",
          "speaker_id": "1",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100027006.1.51",
          "form": "암튼",
          "original_form": "암튼",
          "speaker_id": "1",
          "time": "20210903 17:19"
        },
        {
          "id": "MDRW2100027006.1.52",
          "form": "뭐 재밋는 일있냐고 해서",
          "original_form": "뭐 재밋는 일있냐고 해서",
          "speaker_id": "1",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100027006.1.53",
          "form": "그런거 없고 이런저렁 사정이 잇어서 ㅜ쫓기는 중이라고 후다닥 설명햇더니",
          "original_form": "그런거 없고 이런저렁 사정이 잇어서 ㅜ쫓기는 중이라고 후다닥 설명햇더니",
          "speaker_id": "1",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100027006.1.54",
          "form": "우리 셋을 숨켜줌",
          "original_form": "우리 셋을 숨켜줌",
          "speaker_id": "1",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100027006.1.55",
          "form": "근데 대신에 그 사람이",
          "original_form": "근데 대신에 그 사람이",
          "speaker_id": "1",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100027006.1.56",
          "form": "총든 사람들함테 잡혀가서",
          "original_form": "총든 사람들함테 잡혀가서",
          "speaker_id": "1",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100027006.1.57",
          "form": "경찰서에서 사회복무를",
          "original_form": "경찰서에서 사회복무를",
          "speaker_id": "1",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100027006.1.58",
          "form": "하게된거임 봉사활동",
          "original_form": "하게된거임 봉사활동",
          "speaker_id": "1",
          "time": "20210903 17:20"
        },
        {
          "id": "MDRW2100027006.1.59",
          "form": "근데 봉사가 거의 교도소 노동급이어서",
          "original_form": "근데 봉사가 거의 교도소 노동급이어서",
          "speaker_id": "1",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100027006.1.60",
          "form": "그 사람 면회가면서 미안하다고 햇는데",
          "original_form": "그 사람 면회가면서 미안하다고 햇는데",
          "speaker_id": "1",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100027006.1.61",
          "form": "어차피 왓어야하는 곳이라 괜찮다면서 다음 계획 이야기하다가 꿈 깸 끝",
          "original_form": "어차피 왓어야하는 곳이라 괜찮다면서 다음 계획 이야기하다가 꿈 깸 끝",
          "speaker_id": "1",
          "time": "20210903 17:21"
        },
        {
          "id": "MDRW2100027006.1.62",
          "form": "그럴수가",
          "original_form": "그럴수가",
          "speaker_id": "2",
          "time": "20210903 17:21"
        },
