      "utterance": [
        {
          "id": "MDRW2100030759.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210904 12:20"
        },
        {
          "id": "MDRW2100030759.1.2",
          "form": "안녕하세요 학교 다니시나요",
          "original_form": "안녕하세요 학교 다니시나요",
          "speaker_id": "2",
          "time": "20210904 12:49"
        },
        {
          "id": "MDRW2100030759.1.3",
          "form": "저는 직장인이에요! 학생이신가요?",
          "original_form": "저는 직장인이에요! 학생이신가요?",
          "speaker_id": "1",
          "time": "20210904 13:08"
        },
        {
          "id": "MDRW2100030759.1.4",
          "form": "네네 저도 직장인이에요",
          "original_form": "네네 저도 직장인이에요",
          "speaker_id": "2",
          "time": "20210904 13:17"
        },
        {
          "id": "MDRW2100030759.1.5",
          "form": "대학원같이 다니고 있어요",
          "original_form": "대학원같이 다니고 있어요",
          "speaker_id": "2",
          "time": "20210904 13:18"
        },
        {
          "id": "MDRW2100030759.1.6",
          "form": "앗 저도요!",
          "original_form": "앗 저도요!",
          "speaker_id": "1",
          "time": "20210904 13:18"
        },
        {
          "id": "MDRW2100030759.1.7",
          "form": "너무 힘들지않나요ㅎㅎㅎㅎ",
          "original_form": "너무 힘들지않나요ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 13:18"
        },
        {
          "id": "MDRW2100030759.1.8",
          "form": "우와 대단하세요 네 너무 힘들죠 ...",
          "original_form": "우와 대단하세요 네 너무 힘들죠 ...",
          "speaker_id": "2",
          "time": "20210904 13:18"
        },
        {
          "id": "MDRW2100030759.1.9",
          "form": "큐ㅜㅜ아닙니다 같이 다니는걸요",
          "original_form": "큐ㅜㅜ아닙니다 같이 다니는걸요",
          "speaker_id": "1",
          "time": "20210904 13:19"
        },
        {
          "id": "MDRW2100030759.1.10",
          "form": "최근에 등록하셨겠어요",
          "original_form": "최근에 등록하셨겠어요",
          "speaker_id": "1",
          "time": "20210904 13:19"
        },
        {
          "id": "MDRW2100030759.1.11",
          "form": "이제 졸업 얼마남으셨나요",
          "original_form": "이제 졸업 얼마남으셨나요",
          "speaker_id": "2",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100030759.1.12",
          "form": "저는 4학기차 입니다 이제",
          "original_form": "저는 4학기차 입니다 이제",
          "speaker_id": "2",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100030759.1.13",
          "form": "와 저도4학기에요 이번이",
          "original_form": "와 저도4학기에요 이번이",
          "speaker_id": "1",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100030759.1.14",
          "form": "우와 저희 같은 곳 아닌가요 ㅋㅋㅋ",
          "original_form": "우와 저희 같은 곳 아닌가요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100030759.1.15",
          "form": "ㅎㅎㅎ석사2년 박사2년인데",
          "original_form": "ㅎㅎㅎ석사2년 박사2년인데",
          "speaker_id": "1",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100030759.1.16",
          "form": "ㅋㅋㅋ신기하네요진짜",
          "original_form": "ㅋㅋㅋ신기하네요진짜",
          "speaker_id": "1",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100030759.1.17",
          "form": "우와 석박사 통합이세여?",
          "original_form": "우와 석박사 통합이세여?",
          "speaker_id": "2",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100030759.1.18",
          "form": "네네 통합인데 과연 끝까지할수있으련지ㅎㅎ",
          "original_form": "네네 통합인데 과연 끝까지할수있으련지ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 13:21"
        },
        {
          "id": "MDRW2100030759.1.19",
          "form": "석사과정이세요?박사과정이세요?",
          "original_form": "석사과정이세요?박사과정이세요?",
          "speaker_id": "1",
          "time": "20210904 13:21"
        },
        {
          "id": "MDRW2100030759.1.20",
          "form": "전 석사과정입니다",
          "original_form": "전 석사과정입니다",
          "speaker_id": "2",
          "time": "20210904 13:21"
        },
        {
          "id": "MDRW2100030759.1.21",
          "form": "논문 쓰고계세요? 전 ㄴ 아직 시작도 못했어요ㅠㅠ",
          "original_form": "논문 쓰고계세요? 전 ㄴ 아직 시작도 못했어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 13:21"
        },
        {
          "id": "MDRW2100030759.1.22",
          "form": "앗ㅎㅎㅎㅎ동질감..진짜",
          "original_form": "앗ㅎㅎㅎㅎ동질감..진짜",
          "speaker_id": "1",
          "time": "20210904 13:24"
        },
        {
          "id": "MDRW2100030759.1.23",
          "form": "주제는정해지고형식도정해졌지만",
          "original_form": "주제는정해지고형식도정해졌지만",
          "speaker_id": "1",
          "time": "20210904 13:24"
        },
        {
