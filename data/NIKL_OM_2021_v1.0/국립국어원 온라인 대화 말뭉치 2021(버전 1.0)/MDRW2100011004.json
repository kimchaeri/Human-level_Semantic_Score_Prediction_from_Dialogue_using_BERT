      "utterance": [
        {
          "id": "MDRW2100011004.1.1",
          "form": "나 오늘 후참잘ㅇㅣ 먹고싶었어",
          "original_form": "나 오늘 후참잘ㅇㅣ 먹고싶었어",
          "speaker_id": "2",
          "time": "20210823 23:39"
        },
        {
          "id": "MDRW2100011004.1.2",
          "form": "후참잘은 뭘 줄인거야?",
          "original_form": "후참잘은 뭘 줄인거야?",
          "speaker_id": "1",
          "time": "20210823 23:39"
        },
        {
          "id": "MDRW2100011004.1.3",
          "form": "근데 참았어. 돈도 없고 살도 많겨든.",
          "original_form": "근데 참았어. 돈도 없고 살도 많겨든.",
          "speaker_id": "2",
          "time": "20210823 23:39"
        },
        {
          "id": "MDRW2100011004.1.4",
          "form": "후라이드 참 잘하는 집.",
          "original_form": "후라이드 참 잘하는 집.",
          "speaker_id": "2",
          "time": "20210823 23:39"
        },
        {
          "id": "MDRW2100011004.1.5",
          "form": "바삭하고 맛있어.",
          "original_form": "바삭하고 맛있어.",
          "speaker_id": "2",
          "time": "20210823 23:39"
        },
        {
          "id": "MDRW2100011004.1.6",
          "form": "생활맥주의 치킨도 맛있단다.",
          "original_form": "생활맥주의 치킨도 맛있단다.",
          "speaker_id": "2",
          "time": "20210823 23:39"
        },
        {
          "id": "MDRW2100011004.1.7",
          "form": "살도 많다니 정말 재밌는 언어유희다",
          "original_form": "살도 많다니 정말 재밌는 언어유희다",
          "speaker_id": "1",
          "time": "20210823 23:39"
        },
        {
          "id": "MDRW2100011004.1.8",
          "form": "앵그리버드치킨 조금 맵싹해.",
          "original_form": "앵그리버드치킨 조금 맵싹해.",
          "speaker_id": "2",
          "time": "20210823 23:39"
        },
        {
          "id": "MDRW2100011004.1.9",
          "form": "그건 치킨집 이름이야??",
          "original_form": "그건 치킨집 이름이야??",
          "speaker_id": "1",
          "time": "20210823 23:39"
        },
        {
          "id": "MDRW2100011004.1.10",
          "form": "생활맥주라는 맥줏집에 메뉴이름이 앵그리버드치킨이여",
          "original_form": "생활맥주라는 맥줏집에 메뉴이름이 앵그리버드치킨이여",
          "speaker_id": "2",
          "time": "20210823 23:40"
        },
        {
          "id": "MDRW2100011004.1.11",
          "form": "처음들어봐 다음에 먹어보도록할께",
          "original_form": "처음들어봐 다음에 먹어보도록할께",
          "speaker_id": "1",
          "time": "20210823 23:40"
        },
        {
          "id": "MDRW2100011004.1.12",
          "form": "앵그리치킨이었던가",
          "original_form": "앵그리치킨이었던가",
          "speaker_id": "2",
          "time": "20210823 23:40"
        },
        {
          "id": "MDRW2100011004.1.13",
          "form": "응 맛읶어",
          "original_form": "응 맛읶어",
          "speaker_id": "2",
          "time": "20210823 23:40"
        },
        {
          "id": "MDRW2100011004.1.14",
          "form": "다음에 가서도 먹어보자. 네가 원한다면.",
          "original_form": "다음에 가서도 먹어보자. 네가 원한다면.",
          "speaker_id": "2",
          "time": "20210823 23:40"
        },
        {
          "id": "MDRW2100011004.1.15",
          "form": "그건 너의 집 근처에 있는 가게니?",
          "original_form": "그건 너의 집 근처에 있는 가게니?",
          "speaker_id": "1",
          "time": "20210823 23:40"
        },
        {
          "id": "MDRW2100011004.1.16",
          "form": "나는 지금 끄티집도 땡긴다.",
          "original_form": "나는 지금 끄티집도 땡긴다.",
          "speaker_id": "2",
          "time": "20210823 23:40"
        },
        {
          "id": "MDRW2100011004.1.17",
          "form": "3지구애 있어.",
          "original_form": "3지구애 있어.",
          "speaker_id": "2",
          "time": "20210823 23:40"
        },
        {
          "id": "MDRW2100011004.1.18",
          "form": "끄티집 닭갈비 너무 맛있어",
          "original_form": "끄티집 닭갈비 너무 맛있어",
          "speaker_id": "1",
          "time": "20210823 23:40"
        },
        {
          "id": "MDRW2100011004.1.19",
          "form": "화장실이 별로라 너무 아쉽다",
          "original_form": "화장실이 별로라 너무 아쉽다",
          "speaker_id": "2",
          "time": "20210823 23:40"
        },
        {
          "id": "MDRW2100011004.1.20",
          "form": "담다디 닭도리탕도 맛있어",
          "original_form": "담다디 닭도리탕도 맛있어",
          "speaker_id": "1",
          "time": "20210823 23:40"
        }
      ]
