      "utterance": [
        {
          "id": "MDRW2100011966.1.1",
          "form": "공연 보는거 좋아하세요?",
          "original_form": "공연 보는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 22:14"
        },
        {
          "id": "MDRW2100011966.1.2",
          "form": "많이 못봤지만 좋아해요",
          "original_form": "많이 못봤지만 좋아해요",
          "speaker_id": "2",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100011966.1.3",
          "form": "어떤 공연보셨나요?",
          "original_form": "어떤 공연보셨나요?",
          "speaker_id": "1",
          "time": "20210824 22:23"
        },
        {
          "id": "MDRW2100011966.1.4",
          "form": "사실 예전에 본거라 잘 기억나지않아요",
          "original_form": "사실 예전에 본거라 잘 기억나지않아요",
          "speaker_id": "2",
          "time": "20210824 22:23"
        },
        {
          "id": "MDRW2100011966.1.5",
          "form": "최근에 영화 보셨나요",
          "original_form": "최근에 영화 보셨나요",
          "speaker_id": "1",
          "time": "20210824 22:24"
        },
        {
          "id": "MDRW2100011966.1.6",
          "form": "네 클루엘라 봤어요",
          "original_form": "네 클루엘라 봤어요",
          "speaker_id": "2",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100011966.1.7",
          "form": "그 뒤로는 바빠서 못봤어요",
          "original_form": "그 뒤로는 바빠서 못봤어요",
          "speaker_id": "2",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100011966.1.8",
          "form": "저도 클루엘라 보려고 vod 기다리고있어요",
          "original_form": "저도 클루엘라 보려고 vod 기다리고있어요",
          "speaker_id": "1",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100011966.1.9",
          "form": "어떠셨나요?",
          "original_form": "어떠셨나요?",
          "speaker_id": "1",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100011966.1.10",
          "form": "꽤 재밌었어요!",
          "original_form": "꽤 재밌었어요!",
          "speaker_id": "2",
          "time": "20210824 22:26"
        },
        {
          "id": "MDRW2100011966.1.11",
          "form": "인물들이 연기를 잘하고",
          "original_form": "인물들이 연기를 잘하고",
          "speaker_id": "2",
          "time": "20210824 22:26"
        },
        {
          "id": "MDRW2100011966.1.12",
          "form": "각자 배역에 잘 맞아서",
          "original_form": "각자 배역에 잘 맞아서",
          "speaker_id": "2",
          "time": "20210824 22:27"
        },
        {
          "id": "MDRW2100011966.1.13",
          "form": "좋아하는 영화감독 있으신가요?",
          "original_form": "좋아하는 영화감독 있으신가요?",
          "speaker_id": "1",
          "time": "20210824 22:27"
        },
        {
          "id": "MDRW2100011966.1.14",
          "form": "좋았고 확실히 화려해서",
          "original_form": "좋았고 확실히 화려해서",
          "speaker_id": "2",
          "time": "20210824 22:27"
        },
        {
          "id": "MDRW2100011966.1.15",
          "form": "보는 맛이 있더라구요",
          "original_form": "보는 맛이 있더라구요",
          "speaker_id": "2",
          "time": "20210824 22:27"
        },
        {
          "id": "MDRW2100011966.1.16",
          "form": "영화감독은 딱히 없어요",
          "original_form": "영화감독은 딱히 없어요",
          "speaker_id": "2",
          "time": "20210824 22:27"
        },
        {
          "id": "MDRW2100011966.1.17",
          "form": "한국영화를 많이 보진않아서",
          "original_form": "한국영화를 많이 보진않아서",
          "speaker_id": "2",
          "time": "20210824 22:28"
        },
        {
          "id": "MDRW2100011966.1.18",
          "form": "추천하시는 영화감독님 있나요?",
          "original_form": "추천하시는 영화감독님 있나요?",
          "speaker_id": "2",
          "time": "20210824 22:28"
        },
        {
          "id": "MDRW2100011966.1.19",
          "form": "윤가은 감독이요!",
          "original_form": "윤가은 감독이요!",
          "speaker_id": "1",
          "time": "20210824 22:29"
        },
        {
          "id": "MDRW2100011966.1.20",
          "form": "오 작품 보니",
          "original_form": "오 작품 보니",
          "speaker_id": "2",
          "time": "20210824 22:30"
        },
        {
          "id": "MDRW2100011966.1.21",
          "form": "한번쯤 들어봤던 작품들이네요",
          "original_form": "한번쯤 들어봤던 작품들이네요",
          "speaker_id": "2",
          "time": "20210824 22:30"
        },
        {
          "id": "MDRW2100011966.1.22",
          "form": "오 그런가요?",
          "original_form": "오 그런가요?",
          "speaker_id": "1",
          "time": "20210824 22:30"
        }
      ]
