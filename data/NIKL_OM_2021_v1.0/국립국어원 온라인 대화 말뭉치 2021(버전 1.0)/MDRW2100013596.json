      "utterance": [
        {
          "id": "MDRW2100013596.1.1",
          "form": "name2님 점심 드셨나요? 전 부찌 먹었답니다",
          "original_form": "&name2&님 점심 드셨나요? 전 부찌 먹었답니다",
          "speaker_id": "1",
          "time": "20210825 15:14"
        },
        {
          "id": "MDRW2100013596.1.2",
          "form": "저는 쭈꾸미볶음 먹었어요",
          "original_form": "저는 쭈꾸미볶음 먹었어요",
          "speaker_id": "2",
          "time": "20210825 15:17"
        },
        {
          "id": "MDRW2100013596.1.3",
          "form": "헉 맛있겠다 쭈꾸미볶음 진짜 맛있잖아요ㅠ",
          "original_form": "헉 맛있겠다 쭈꾸미볶음 진짜 맛있잖아요ㅠ",
          "speaker_id": "1",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013596.1.4",
          "form": "맞아요 배달시켜서 먹은 거랍니다",
          "original_form": "맞아요 배달시켜서 먹은 거랍니다",
          "speaker_id": "2",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100013596.1.5",
          "form": "와 진짜 맛있겠어요 저녁은 뭘 드실 예정이신가요?",
          "original_form": "와 진짜 맛있겠어요 저녁은 뭘 드실 예정이신가요?",
          "speaker_id": "1",
          "time": "20210825 16:20"
        },
        {
          "id": "MDRW2100013596.1.6",
          "form": "저녁은 아직 생각해두진 않았는데",
          "original_form": "저녁은 아직 생각해두진 않았는데",
          "speaker_id": "2",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100013596.1.7",
          "form": "저녁 뭐 드실 거세요?",
          "original_form": "저녁 뭐 드실 거세요?",
          "speaker_id": "2",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100013596.1.8",
          "form": "저는...짜장면이요.",
          "original_form": "저는...짜장면이요.",
          "speaker_id": "1",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100013596.1.9",
          "form": "오늘 탕수육 먹을거예요 ㅎㅎㅎㅎ",
          "original_form": "오늘 탕수육 먹을거예요 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100013596.1.10",
          "form": "오 탕수육 좋죠",
          "original_form": "오 탕수육 좋죠",
          "speaker_id": "2",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100013596.1.11",
          "form": "저는 짜장보다 짬뽕 좋아해요",
          "original_form": "저는 짜장보다 짬뽕 좋아해요",
          "speaker_id": "2",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100013596.1.12",
          "form": "헐 짬뽕 저도요",
          "original_form": "헐 짬뽕 저도요",
          "speaker_id": "1",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100013596.1.13",
          "form": "그 매콤한게...하..속풀리는 느낌. 너무좋아요",
          "original_form": "그 매콤한게...하..속풀리는 느낌. 너무좋아요",
          "speaker_id": "1",
          "time": "20210825 17:24"
        },
        {
          "id": "MDRW2100013596.1.14",
          "form": "맞아요.. 예전에 신길짬뽕 먹으러 갔었는데",
          "original_form": "맞아요.. 예전에 신길짬뽕 먹으러 갔었는데",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100013596.1.15",
          "form": "진짜 매워서 넘ㆍ 힘들게 먹었었어요",
          "original_form": "진짜 매워서 넘ㆍ 힘들게 먹었었어요",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100013596.1.16",
          "form": "헉 그래요?! 저는 그렇게 엄청 매운 짬뽕은 안 먹어 본 것 같아요",
          "original_form": "헉 그래요?! 저는 그렇게 엄청 매운 짬뽕은 안 먹어 본 것 같아요",
          "speaker_id": "1",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100013596.1.17",
          "form": "적당한...매움?",
          "original_form": "적당한...매움?",
          "speaker_id": "1",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100013596.1.18",
          "form": "적당한 게 최고죠",
          "original_form": "적당한 게 최고죠",
          "speaker_id": "2",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100013596.1.19",
          "form": "저 지금 요거트 먹으려구요",
          "original_form": "저 지금 요거트 먹으려구요",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100013596.1.20",
          "form": "헐 맛있겠다... 어떤 요거트 드셨나요?!",
          "original_form": "헐 맛있겠다... 어떤 요거트 드셨나요?!",
          "speaker_id": "1",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100013596.1.21",
          "form": "딸기 요거트요!",
          "original_form": "딸기 요거트요!",
          "speaker_id": "2",
          "time": "20210825 20:43"
        }
      ]
