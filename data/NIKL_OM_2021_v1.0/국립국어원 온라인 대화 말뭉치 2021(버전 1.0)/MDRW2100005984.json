        },
        {
          "id": "MDRW2100005984.1.2",
          "form": "새로 또 키울꺼야?",
          "original_form": "새로 또 키울꺼야?",
          "speaker_id": "1",
          "time": "20210907 18:21"
        },
        {
          "id": "MDRW2100005984.1.3",
          "form": "그때 가봐야 알지",
          "original_form": "그때 가봐야 알지",
          "speaker_id": "2",
          "time": "20210907 18:22"
        },
        {
          "id": "MDRW2100005984.1.4",
          "form": "그렇긴하지..ㅋㅋ",
          "original_form": "그렇긴하지..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:22"
        },
        {
          "id": "MDRW2100005984.1.5",
          "form": "또 키우게 되지 않을까",
          "original_form": "또 키우게 되지 않을까",
          "speaker_id": "2",
          "time": "20210907 18:22"
        },
        {
          "id": "MDRW2100005984.1.6",
          "form": "그럴것 같당ㅋ",
          "original_form": "그럴것 같당ㅋ",
          "speaker_id": "1",
          "time": "20210907 18:22"
        },
        {
          "id": "MDRW2100005984.1.7",
          "form": "다음에도 포매로 키워야지",
          "original_form": "다음에도 포매로 키워야지",
          "speaker_id": "2",
          "time": "20210907 18:22"
        },
        {
          "id": "MDRW2100005984.1.8",
          "form": "웅웅 근데 유기견으로 입양하자",
          "original_form": "웅웅 근데 유기견으로 입양하자",
          "speaker_id": "1",
          "time": "20210907 18:22"
        },
        {
          "id": "MDRW2100005984.1.9",
          "form": "유기견은 힘든데",
          "original_form": "유기견은 힘든데",
          "speaker_id": "2",
          "time": "20210907 18:22"
        },
        {
          "id": "MDRW2100005984.1.10",
          "form": "그래도! 불쌍하잖아",
          "original_form": "그래도! 불쌍하잖아",
          "speaker_id": "1",
          "time": "20210907 18:22"
        },
        {
          "id": "MDRW2100005984.1.11",
          "form": "그렇긴 하지",
          "original_form": "그렇긴 하지",
          "speaker_id": "2",
          "time": "20210907 18:22"
        },
        {
          "id": "MDRW2100005984.1.12",
          "form": "웅.. 불쌍하니까 더 잘 돌봐줘야한다",
          "original_form": "웅.. 불쌍하니까 더 잘 돌봐줘야한다",
          "speaker_id": "1",
          "time": "20210907 18:22"
        },
        {
          "id": "MDRW2100005984.1.13",
          "form": "그래다음에는유기견 키우자",
          "original_form": "그래다음에는유기견 키우자",
          "speaker_id": "2",
