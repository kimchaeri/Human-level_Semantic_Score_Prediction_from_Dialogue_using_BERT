      "utterance": [
        {
          "id": "MDRW2100000848.1.1",
          "form": "우리 내년에 여행 어디갈까?",
          "original_form": "우리 내년에 여행 어디갈까?",
          "speaker_id": "1",
          "time": "20210819 20:00"
        },
        {
          "id": "MDRW2100000848.1.2",
          "form": "일단 순천이 너무 가구싶어!",
          "original_form": "일단 순천이 너무 가구싶어!",
          "speaker_id": "2",
          "time": "20210819 20:01"
        },
        {
          "id": "MDRW2100000848.1.3",
          "form": "작년에 순천 가려고 했는데 못갔네.. 가을에 갈 수 잇었음 좋겠다",
          "original_form": "작년에 순천 가려고 했는데 못갔네.. 가을에 갈 수 잇었음 좋겠다",
          "speaker_id": "1",
          "time": "20210819 20:02"
        },
        {
          "id": "MDRW2100000848.1.4",
          "form": "10월정도면 갈 수 있지 않을까?",
          "original_form": "10월정도면 갈 수 있지 않을까?",
          "speaker_id": "2",
          "time": "20210819 21:54"
        },
        {
          "id": "MDRW2100000848.1.5",
          "form": "글쎄.. 순천만 갈대가.그렇게 멋지다는데 궁금하다",
          "original_form": "글쎄.. 순천만 갈대가.그렇게 멋지다는데 궁금하다",
          "speaker_id": "1",
          "time": "20210819 21:55"
        },
        {
          "id": "MDRW2100000848.1.6",
          "form": "나도!! 갈대밭에서 인생사진 찍어야되는데ㅎㅎ",
          "original_form": "나도!! 갈대밭에서 인생사진 찍어야되는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 21:56"
        },
        {
          "id": "MDRW2100000848.1.7",
          "form": "너는 순천말고 가고싶은 곳 있어?",
          "original_form": "너는 순천말고 가고싶은 곳 있어?",
          "speaker_id": "2",
          "time": "20210819 21:56"
        },
        {
          "id": "MDRW2100000848.1.8",
          "form": "음..통영도 좋고",
          "original_form": "음..통영도 좋고",
          "speaker_id": "1",
          "time": "20210819 22:00"
        },
        {
          "id": "MDRW2100000848.1.9",
          "form": "그냥 한적한 시골동네 가고시펑",
          "original_form": "그냥 한적한 시골동네 가고시펑",
          "speaker_id": "1",
          "time": "20210819 22:00"
        },
        {
          "id": "MDRW2100000848.1.10",
          "form": "시골동네~ 벌써부터 힐링이다",
          "original_form": "시골동네~ 벌써부터 힐링이다",
          "speaker_id": "2",
          "time": "20210819 22:07"
        },
        {
          "id": "MDRW2100000848.1.11",
          "form": "그러고보니 우리 물놀이도 못했네",
          "original_form": "그러고보니 우리 물놀이도 못했네",
          "speaker_id": "2",
          "time": "20210819 22:07"
        },
        {
          "id": "MDRW2100000848.1.12",
          "form": "내년 여름에는 바다나 계곡 놀러가고싶당",
          "original_form": "내년 여름에는 바다나 계곡 놀러가고싶당",
          "speaker_id": "1",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100000848.1.13",
          "form": "그래 내년에는 가족들이랑 다같이 꼭 계곡가자",
          "original_form": "그래 내년에는 가족들이랑 다같이 꼭 계곡가자",
          "speaker_id": "2",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100000848.1.14",
          "form": "제주도도 너무 가고싶어",
          "original_form": "제주도도 너무 가고싶어",
          "speaker_id": "2",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100000848.1.15",
          "form": "제주도..ㅠㅠㅠ 제주도 바다에서 수영하고싶다",
          "original_form": "제주도..ㅠㅠㅠ 제주도 바다에서 수영하고싶다",
          "speaker_id": "1",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100000848.1.16",
          "form": "내년에 꼭 같이가자",
          "original_form": "내년에 꼭 같이가자",
          "speaker_id": "1",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100000848.1.17",
          "form": "나는 제주도 비자림이 너무 좋았어",
          "original_form": "나는 제주도 비자림이 너무 좋았어",
          "speaker_id": "2",
          "time": "20210819 22:33"
        },
        {
          "id": "MDRW2100000848.1.18",
          "form": "나두 그리고 우도도 다시한번 가고싶어",
          "original_form": "나두 그리고 우도도 다시한번 가고싶어",
          "speaker_id": "1",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100000848.1.19",
          "form": "수상보트 진짜 재미써",
          "original_form": "수상보트 진짜 재미써",
          "speaker_id": "1",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100000848.1.20",
          "form": "그때 수상보트 못탄게 너무 아쉽다",
          "original_form": "그때 수상보트 못탄게 너무 아쉽다",
          "speaker_id": "2",
          "time": "20210819 22:40"
        },
        {
          "id": "MDRW2100000848.1.21",
          "form": "다음번에는 꼭 우리끼리라도 타자",
          "original_form": "다음번에는 꼭 우리끼리라도 타자",
          "speaker_id": "2",
          "time": "20210819 22:40"
        },
        {
          "id": "MDRW2100000848.1.22",
