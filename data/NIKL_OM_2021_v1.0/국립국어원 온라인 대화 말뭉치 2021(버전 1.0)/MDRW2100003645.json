      "utterance": [
        {
          "id": "MDRW2100003645.1.1",
          "form": "하루 종일 내릴 것 같아",
          "original_form": "하루 종일 내릴 것 같아",
          "speaker_id": "2",
          "time": "20210821 09:47"
        },
        {
          "id": "MDRW2100003645.1.2",
          "form": "방충망 청소라도 해야하나 ㅎㅎ",
          "original_form": "방충망 청소라도 해야하나 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 09:47"
        },
        {
          "id": "MDRW2100003645.1.3",
          "form": "ㅎㅎ 이런 날 창틀이랑 방충망 청소하기 좋지",
          "original_form": "ㅎㅎ 이런 날 창틀이랑 방충망 청소하기 좋지",
          "speaker_id": "2",
          "time": "20210821 09:48"
        },
        {
          "id": "MDRW2100003645.1.4",
          "form": "빗소리 듣기 좋다",
          "original_form": "빗소리 듣기 좋다",
          "speaker_id": "1",
          "time": "20210821 09:48"
        },
        {
          "id": "MDRW2100003645.1.5",
          "form": "그러게~시원하게 내리네",
          "original_form": "그러게~시원하게 내리네",
          "speaker_id": "2",
          "time": "20210821 09:48"
        },
        {
          "id": "MDRW2100003645.1.6",
          "form": "어제 이불 세탁기 돌렸는데 ㅠ",
          "original_form": "어제 이불 세탁기 돌렸는데 ㅠ",
          "speaker_id": "1",
          "time": "20210821 09:49"
        },
        {
          "id": "MDRW2100003645.1.7",
          "form": "마르는데 며칠은 걸릴 듯 ㅎㅎ",
          "original_form": "마르는데 며칠은 걸릴 듯 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 09:49"
        },
        {
          "id": "MDRW2100003645.1.8",
          "form": "그러게 말야. 날짜를 잘못 골랐네",
          "original_form": "그러게 말야. 날짜를 잘못 골랐네",
          "speaker_id": "1",
          "time": "20210821 09:49"
        },
        {
          "id": "MDRW2100003645.1.9",
          "form": "이 정도 하늘이면 하루종일 비 당첨이야",
          "original_form": "이 정도 하늘이면 하루종일 비 당첨이야",
          "speaker_id": "2",
          "time": "20210821 09:50"
        },
        {
          "id": "MDRW2100003645.1.10",
          "form": "ㅎㅎ 마음을 비워야지",
          "original_form": "ㅎㅎ 마음을 비워야지",
          "speaker_id": "1",
          "time": "20210821 09:50"
        },
        {
          "id": "MDRW2100003645.1.11",
          "form": "ㅇㅇ 빗소리 들으면서 힐링해",
          "original_form": "ㅇㅇ 빗소리 들으면서 힐링해",
          "speaker_id": "2",
          "time": "20210821 09:50"
        },
        {
          "id": "MDRW2100003645.1.12",
          "form": "좋지 ㅎㅎ",
          "original_form": "좋지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 09:50"
        },
        {
          "id": "MDRW2100003645.1.13",
          "form": "밖에 있을 땐 비오는 게 싫은데 집에서는 참 좋아",
          "original_form": "밖에 있을 땐 비오는 게 싫은데 집에서는 참 좋아",
          "speaker_id": "2",
          "time": "20210821 09:51"
        },
        {
          "id": "MDRW2100003645.1.14",
          "form": "나도 그래 ㅎㅎ",
          "original_form": "나도 그래 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 09:51"
        },
        {
          "id": "MDRW2100003645.1.15",
          "form": "하루종일 비내리니까 너무 축축하다",
