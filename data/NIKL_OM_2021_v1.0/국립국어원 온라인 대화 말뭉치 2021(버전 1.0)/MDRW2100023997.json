        },
        {
          "id": "MDRW2100023997.1.4",
          "form": "모임 좋아하시나요",
          "original_form": "모임 좋아하시나요",
          "speaker_id": "1",
          "time": "20210907 16:01"
        },
        {
          "id": "MDRW2100023997.1.5",
          "form": "아뇨 ㅋㅋ 전 집에 있는걸 좋아해서요",
          "original_form": "아뇨 ㅋㅋ 전 집에 있는걸 좋아해서요",
          "speaker_id": "2",
          "time": "20210907 16:01"
        },
        {
          "id": "MDRW2100023997.1.6",
          "form": "집이 좋죠",
          "original_form": "집이 좋죠",
          "speaker_id": "1",
          "time": "20210907 16:02"
        },
        {
          "id": "MDRW2100023997.1.7",
          "form": "요즘 시국에는 특히 ㅎㅎ",
          "original_form": "요즘 시국에는 특히 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 16:02"
        },
        {
          "id": "MDRW2100023997.1.8",
          "form": "네 맞아요 ㅋㅋ 동호회 드신거 있으신가요",
          "original_form": "네 맞아요 ㅋㅋ 동호회 드신거 있으신가요",
          "speaker_id": "2",
          "time": "20210907 16:03"
        },
        {
          "id": "MDRW2100023997.1.9",
          "form": "아뇨 동호회는 크게 없고",
          "original_form": "아뇨 동호회는 크게 없고",
          "speaker_id": "1",
          "time": "20210907 16:19"
        },
        {
          "id": "MDRW2100023997.1.10",
          "form": "동기 모임정도?",
          "original_form": "동기 모임정도?",
          "speaker_id": "1",
          "time": "20210907 16:19"
        },
        {
          "id": "MDRW2100023997.1.11",
          "form": "요즘 코로나 때문에 모임 못 하셔서 아쉬우시겠어요",
          "original_form": "요즘 코로나 때문에 모임 못 하셔서 아쉬우시겠어요",
          "speaker_id": "2",
          "time": "20210907 16:20"
        },
        {
          "id": "MDRW2100023997.1.12",
          "form": "뭐 아쉬운대로 소규모를..",
          "original_form": "뭐 아쉬운대로 소규모를..",
          "speaker_id": "1",
          "time": "20210907 16:21"
        },
        {
          "id": "MDRW2100023997.1.13",
          "form": "봉사단체 있으신가요?",
          "original_form": "봉사단체 있으신가요?",
          "speaker_id": "2",
          "time": "20210907 16:22"
        },
        {
          "id": "MDRW2100023997.1.14",
          "form": "봉사단체 가입한곳 있으신가요",
          "original_form": "봉사단체 가입한곳 있으신가요",
          "speaker_id": "2",
          "time": "20210907 16:22"
        },
        {
          "id": "MDRW2100023997.1.15",
          "form": "아니요 ㅎㅎ name2님은요?",
          "original_form": "아니요 ㅎㅎ &name2&님은요?",
          "speaker_id": "1",
          "time": "20210907 16:49"
        },
        {
          "id": "MDRW2100023997.1.16",
          "form": "없어요 ㅋㅋ 근데 코로나 끝나면 한번 가입해보고 싶어요",
          "original_form": "없어요 ㅋㅋ 근데 코로나 끝나면 한번 가입해보고 싶어요",
          "speaker_id": "2",
          "time": "20210907 16:59"
        },
        {
          "id": "MDRW2100023997.1.17",
          "form": "가입해보고싶은 동호회 있으신가요?",
          "original_form": "가입해보고싶은 동호회 있으신가요?",
          "speaker_id": "2",
          "time": "20210907 17:00"
        },
        {
          "id": "MDRW2100023997.1.18",
          "form": "음 이제 육아해야되서ㅠㅠ",
          "original_form": "음 이제 육아해야되서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 18:20"
        },
        {
          "id": "MDRW2100023997.1.19",
          "form": "시간 내기가 어려울듯해요",
          "original_form": "시간 내기가 어려울듯해요",
