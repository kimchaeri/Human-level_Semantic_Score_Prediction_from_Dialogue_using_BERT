      "utterance": [
        {
          "id": "MDRW2100032775.1.1",
          "form": "넷플릭스 보시나요",
          "original_form": "넷플릭스 보시나요",
          "speaker_id": "2",
          "time": "20210905 23:24"
        },
        {
          "id": "MDRW2100032775.1.2",
          "form": "네 저 스위트롬 정말 재밌게 봤어요",
          "original_form": "네 저 스위트롬 정말 재밌게 봤어요",
          "speaker_id": "1",
          "time": "20210905 23:25"
        },
        {
          "id": "MDRW2100032775.1.3",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 23:25"
        },
        {
          "id": "MDRW2100032775.1.4",
          "form": "스위트홈 재밌죠",
          "original_form": "스위트홈 재밌죠",
          "speaker_id": "2",
          "time": "20210905 23:25"
        },
        {
          "id": "MDRW2100032775.1.5",
          "form": "name2님은 보시는 작품 있나요?",
          "original_form": "&name2&님은 보시는 작품 있나요?",
          "speaker_id": "1",
          "time": "20210905 23:25"
        },
        {
          "id": "MDRW2100032775.1.6",
          "form": "왕이된남자를 보고있아요",
          "original_form": "왕이된남자를 보고있아요",
          "speaker_id": "2",
          "time": "20210905 23:25"
        },
        {
          "id": "MDRW2100032775.1.7",
          "form": "가을님은요",
          "original_form": "가을님은요",
          "speaker_id": "2",
          "time": "20210905 23:25"
        },
        {
          "id": "MDRW2100032775.1.8",
          "form": "저는 호텔델루나 보고 있어요",
          "original_form": "저는 호텔델루나 보고 있어요",
          "speaker_id": "1",
          "time": "20210905 23:25"
        },
        {
          "id": "MDRW2100032775.1.9",
          "form": "호텔델루나 안봤는데 재미있아요?",
          "original_form": "호텔델루나 안봤는데 재미있아요?",
          "speaker_id": "2",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100032775.1.10",
          "form": "네! 전 재밌더라고요!",
          "original_form": "네! 전 재밌더라고요!",
          "speaker_id": "1",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100032775.1.11",
          "form": "좋아하는 배우가 나와서 재밌어요!",
          "original_form": "좋아하는 배우가 나와서 재밌어요!",
          "speaker_id": "1",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100032775.1.12",
          "form": "좋아하는 배우가 누구인가요",
          "original_form": "좋아하는 배우가 누구인가요",
          "speaker_id": "2",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100032775.1.13",
          "form": "저 이도현 여진구 둘 다 좋아해요 흫",
          "original_form": "저 이도현 여진구 둘 다 좋아해요 흫",
          "speaker_id": "1",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100032775.1.14",
          "form": "ㅎ.ㅎ",
          "original_form": "ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100032775.1.15",
          "form": "두 배우 다 연기 잘 하죠",
          "original_form": "두 배우 다 연기 잘 하죠",
          "speaker_id": "2",
          "time": "20210905 23:27"
        },
        {
          "id": "MDRW2100032775.1.16",
          "form": "네 맞아요 그래서 더 재밋어요!",
          "original_form": "네 맞아요 그래서 더 재밋어요!",
          "speaker_id": "1",
          "time": "20210905 23:27"
        },
        {
          "id": "MDRW2100032775.1.17",
          "form": "다음에 봐야겠네요",
          "original_form": "다음에 봐야겠네요",
          "speaker_id": "2",
          "time": "20210905 23:27"
        }
      ]
