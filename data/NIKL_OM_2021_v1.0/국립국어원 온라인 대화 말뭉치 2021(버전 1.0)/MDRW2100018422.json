      "utterance": [
        {
          "id": "MDRW2100018422.1.1",
          "form": "이상형이 어떻게 되세요?",
          "original_form": "이상형이 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210827 01:46"
        },
        {
          "id": "MDRW2100018422.1.2",
          "form": "190센치 이상의 지창욱을 닮은 남자..?",
          "original_form": "190센치 이상의 지창욱을 닮은 남자..?",
          "speaker_id": "2",
          "time": "20210827 01:47"
        },
        {
          "id": "MDRW2100018422.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210827 01:47"
        },
        {
          "id": "MDRW2100018422.1.4",
          "form": "ㅋㅋㅋㅋㅋ190이요...?",
          "original_form": "ㅋㅋㅋㅋㅋ190이요...?",
          "speaker_id": "1",
          "time": "20210827 01:47"
        },
        {
          "id": "MDRW2100018422.1.5",
          "form": "지창욱 인정합니다 존잘..",
          "original_form": "지창욱 인정합니다 존잘..",
          "speaker_id": "1",
          "time": "20210827 01:47"
        },
        {
          "id": "MDRW2100018422.1.6",
          "form": "김연경 선수가 192더라구요..",
          "original_form": "김연경 선수가 192더라구요..",
          "speaker_id": "2",
          "time": "20210827 01:48"
        },
        {
          "id": "MDRW2100018422.1.7",
          "form": "헉 되게 크네요",
          "original_form": "헉 되게 크네요",
          "speaker_id": "1",
          "time": "20210827 01:48"
        },
        {
          "id": "MDRW2100018422.1.8",
          "form": "그래서 기준을 190이상으로 잡았죠!",
          "original_form": "그래서 기준을 190이상으로 잡았죠!",
          "speaker_id": "2",
          "time": "20210827 01:48"
        },
        {
          "id": "MDRW2100018422.1.9",
          "form": "저는 외관상 깔끔하고 단정한느낌 좋아해요 ㅋㅋㅋㅋ",
          "original_form": "저는 외관상 깔끔하고 단정한느낌 좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 01:48"
        },
        {
          "id": "MDRW2100018422.1.10",
          "form": "쌍커풀없는!",
          "original_form": "쌍커풀없는!",
          "speaker_id": "1",
          "time": "20210827 01:48"
        },
        {
          "id": "MDRW2100018422.1.11",
          "form": "엇 예를 들자면?",
          "original_form": "엇 예를 들자면?",
          "speaker_id": "2",
          "time": "20210827 01:48"
        },
        {
          "id": "MDRW2100018422.1.12",
          "form": "음...뉴가있을가요",
          "original_form": "음...뉴가있을가요",
          "speaker_id": "1",
          "time": "20210827 01:50"
        },
        {
          "id": "MDRW2100018422.1.13",
          "form": "샤이니 키? 우리집준호?",
          "original_form": "샤이니 키? 우리집준호?",
          "speaker_id": "1",
          "time": "20210827 01:50"
        },
        {
          "id": "MDRW2100018422.1.14",
          "form": "아아 나혼산에서 두분 다 봤어요!",
          "original_form": "아아 나혼산에서 두분 다 봤어요!",
          "speaker_id": "2",
          "time": "20210827 01:50"
        },
        {
          "id": "MDRW2100018422.1.15",
          "form": "준호 운동하는거 보니까 저도 운동 뽐뿌오더라구요",
          "original_form": "준호 운동하는거 보니까 저도 운동 뽐뿌오더라구요",
          "speaker_id": "2",
          "time": "20210827 01:51"
        },
        {
          "id": "MDRW2100018422.1.16",
          "form": "근육 굿굿",
          "original_form": "근육 굿굿",
          "speaker_id": "2",
          "time": "20210827 01:51"
        },
        {
          "id": "MDRW2100018422.1.17",
          "form": "아 맞아요 체지방률이 엄청 낫다고..",
          "original_form": "아 맞아요 체지방률이 엄청 낫다고..",
          "speaker_id": "1",
          "time": "20210827 01:51"
        },
        {
          "id": "MDRW2100018422.1.18",
          "form": "그럼 성격적으로 이상형 어떻게되요?",
          "original_form": "그럼 성격적으로 이상형 어떻게되요?",
          "speaker_id": "1",
          "time": "20210827 01:52"
        },
        {
          "id": "MDRW2100018422.1.19",
          "form": "흠.. 자격지심이 없고 긍정적이며 지적인 남자?",
          "original_form": "흠.. 자격지심이 없고 긍정적이며 지적인 남자?",
          "speaker_id": "2",
          "time": "20210827 01:53"
        },
        {
          "id": "MDRW2100018422.1.20",
          "form": "name1은 어떤 성격이 좋으세요?",
          "original_form": "&name1&은 어떤 성격이 좋으세요?",
          "speaker_id": "2",
          "time": "20210827 01:53"
        },
        {
          "id": "MDRW2100018422.1.21",
          "form": "아 자격지심없는거 좋은거같아요",
          "original_form": "아 자격지심없는거 좋은거같아요",
          "speaker_id": "1",
          "time": "20210827 01:53"
        },
        {
          "id": "MDRW2100018422.1.22",
          "form": "저는 센스있고 유머감각있는사람이요!",
          "original_form": "저는 센스있고 유머감각있는사람이요!",
          "speaker_id": "1",
          "time": "20210827 01:54"
        },
        {
          "id": "MDRW2100018422.1.23",
          "form": "대화잘통하면 더 좋은거같아요",
          "original_form": "대화잘통하면 더 좋은거같아요",
          "speaker_id": "1",
          "time": "20210827 01:54"
        },
        {
          "id": "MDRW2100018422.1.24",
          "form": "아 맞아요 ㅠㅠ 뭐든 코드가 맞아야 관계가 이어지는 거 같아요",
          "original_form": "아 맞아요 ㅠㅠ 뭐든 코드가 맞아야 관계가 이어지는 거 같아요",
          "speaker_id": "2",
          "time": "20210827 01:58"
        }
      ]
