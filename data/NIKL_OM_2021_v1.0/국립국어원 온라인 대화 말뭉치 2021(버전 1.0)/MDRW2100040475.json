      "utterance": [
        {
          "id": "MDRW2100040475.1.1",
          "form": "말이 귀여워 꼬집기고 할퀸거죠 비키라고",
          "original_form": "말이 귀여워 꼬집기고 할퀸거죠 비키라고",
          "speaker_id": "2",
          "time": "20210904 21:14"
        },
        {
          "id": "MDRW2100040475.1.2",
          "form": "핸드폰으로 이렇게 좋은 세상이ㅋㅋ",
          "original_form": "핸드폰으로 이렇게 좋은 세상이ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:15"
        },
        {
          "id": "MDRW2100040475.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ악 맞아요 이제는 진짜 ㅋㅋㅋㅋ싸우지도않겠어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ악 맞아요 이제는 진짜 ㅋㅋㅋㅋ싸우지도않겠어요",
          "speaker_id": "1",
          "time": "20210904 21:27"
        },
        {
          "id": "MDRW2100040475.1.4",
          "form": "각자 자기폰보니까 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "각자 자기폰보니까 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:27"
        },
        {
          "id": "MDRW2100040475.1.5",
          "form": "남이죠ㅋㅋ",
          "original_form": "남이죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:30"
        },
        {
          "id": "MDRW2100040475.1.6",
          "form": "그래도 남동생하고 친하신가봐요",
          "original_form": "그래도 남동생하고 친하신가봐요",
          "speaker_id": "2",
          "time": "20210904 21:30"
        },
        {
          "id": "MDRW2100040475.1.7",
          "form": "집안일좀 하나요? 동생분?",
          "original_form": "집안일좀 하나요? 동생분?",
          "speaker_id": "2",
          "time": "20210904 21:30"
        },
        {
          "id": "MDRW2100040475.1.8",
          "form": "ㅋㅋㅋㅋ아니요 남동생 일 잘안해요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ아니요 남동생 일 잘안해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:43"
        },
        {
          "id": "MDRW2100040475.1.9",
          "form": "동생분은 하시나요?",
          "original_form": "동생분은 하시나요?",
          "speaker_id": "1",
          "time": "20210904 21:44"
        },
        {
          "id": "MDRW2100040475.1.10",
          "form": "잘안하는게 아니라 아예 안해요ㅋㅋ",
          "original_form": "잘안하는게 아니라 아예 안해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 21:48"
        },
        {
          "id": "MDRW2100040475.1.11",
          "form": "휴 남동생들이란..ㅋㅋ 부모님도 이제 포기하고 안시키시나요?",
          "original_form": "휴 남동생들이란..ㅋㅋ 부모님도 이제 포기하고 안시키시나요?",
          "speaker_id": "1",
          "time": "20210904 21:48"
        },
        {
          "id": "MDRW2100040475.1.12",
          "form": "애초에 기대를 안하시고 지일이나 잘하기를 바라시고요..",
          "original_form": "애초에 기대를 안하시고 지일이나 잘하기를 바라시고요..",
          "speaker_id": "2",
          "time": "20210904 21:50"
        },
        {
          "id": "MDRW2100040475.1.13",
          "form": "아악ㅋㅋㅋㅋㅋㅋ공감되서 넘웃겨요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아악ㅋㅋㅋㅋㅋㅋ공감되서 넘웃겨요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:51"
        },
        {
          "id": "MDRW2100040475.1.14",
          "form": "하필 남동생이 또 막내라서 ㅋㅋㅋㅋㅋ",
          "original_form": "하필 남동생이 또 막내라서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:51"
        },
        {
          "id": "MDRW2100040475.1.15",
          "form": "사고만 치지마라... ㅋㅋ저도 그것만 바라고ㅜ",
          "original_form": "사고만 치지마라... ㅋㅋ저도 그것만 바라고ㅜ",
          "speaker_id": "2",
          "time": "20210904 22:07"
        },
        {
          "id": "MDRW2100040475.1.16",
          "form": "휴 맞아요 ㅋㅋㅋ 제 동생은 그래도 요즘 좀 철이든 것 같아요",
          "original_form": "휴 맞아요 ㅋㅋㅋ 제 동생은 그래도 요즘 좀 철이든 것 같아요",
          "speaker_id": "1",
          "time": "20210904 22:13"
        },
        {
          "id": "MDRW2100040475.1.17",
          "form": "저보다도 부모님을 더 챙길때도 있어요 신기...",
          "original_form": "저보다도 부모님을 더 챙길때도 있어요 신기...",
          "speaker_id": "1",
          "time": "20210904 22:13"
        },
        {
          "id": "MDRW2100040475.1.18",
          "form": "아 제동생도 사업해서 큰돈벌면 크게 용돈 드리긴하더라고요",
          "original_form": "아 제동생도 사업해서 큰돈벌면 크게 용돈 드리긴하더라고요",
          "speaker_id": "2",
          "time": "20210904 22:17"
        },
        {
          "id": "MDRW2100040475.1.19",
          "form": "효도는 잘할듯싶어서 너무 다행이죠",
          "original_form": "효도는 잘할듯싶어서 너무 다행이죠",
          "speaker_id": "2",
          "time": "20210904 22:17"
        },
        {
          "id": "MDRW2100040475.1.20",
          "form": "오~~~~ 대박!!!! 그래도 착하고 좋네요!!! 용돈이 최고죠 ㅋㅋㅋ",
          "original_form": "오~~~~ 대박!!!! 그래도 착하고 좋네요!!! 용돈이 최고죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:23"
        },
        {
          "id": "MDRW2100040475.1.21",
          "form": "근데 자영업이라 코로나ㅜㅠ",
          "original_form": "근데 자영업이라 코로나ㅜㅠ",
          "speaker_id": "2",
          "time": "20210904 22:30"
        },
        {
          "id": "MDRW2100040475.1.22",
          "form": "헬스장이거든요ㅜ",
          "original_form": "헬스장이거든요ㅜ",
          "speaker_id": "2",
          "time": "20210904 22:30"
        },
        {
          "id": "MDRW2100040475.1.23",
          "form": "아이궁 ㅜㅜㅜㅜㅜㅜ 코로나 기간동안",
          "original_form": "아이궁 ㅜㅜㅜㅜㅜㅜ 코로나 기간동안",
          "speaker_id": "1",
          "time": "20210904 22:31"
        },
        {
          "id": "MDRW2100040475.1.24",
          "form": "동생분 맘고생이 심했겠어요ㅠ",
