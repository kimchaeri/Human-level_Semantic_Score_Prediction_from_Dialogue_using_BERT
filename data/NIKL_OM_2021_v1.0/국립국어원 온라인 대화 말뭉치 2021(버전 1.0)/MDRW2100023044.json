      "utterance": [
        {
          "id": "MDRW2100023044.1.1",
          "form": "키우는 반려동물이 있으신가요 ㅎㅎ",
          "original_form": "키우는 반려동물이 있으신가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:02"
        },
        {
          "id": "MDRW2100023044.1.2",
          "form": "안녕하세요 name1~ 아니요 안 키우고 있어요",
          "original_form": "안녕하세요 &name1&~ 아니요 안 키우고 있어요",
          "speaker_id": "2",
          "time": "20210829 21:07"
        },
        {
          "id": "MDRW2100023044.1.3",
          "form": "키우고 싶으신 동물은 있으신가요?ㅎ",
          "original_form": "키우고 싶으신 동물은 있으신가요?ㅎ",
          "speaker_id": "1",
          "time": "20210829 21:10"
        },
        {
          "id": "MDRW2100023044.1.4",
          "form": "강아지랑 고양이 좋아하긴 하는데 키우고 싶진 않아요",
          "original_form": "강아지랑 고양이 좋아하긴 하는데 키우고 싶진 않아요",
          "speaker_id": "2",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100023044.1.5",
          "form": "보는걸 좋아하는 거랑 키우는 건 천지차이인 것 같아서",
          "original_form": "보는걸 좋아하는 거랑 키우는 건 천지차이인 것 같아서",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023044.1.6",
          "form": "뭔가.. 잘 못 챙겨줄 것 같더라구요",
          "original_form": "뭔가.. 잘 못 챙겨줄 것 같더라구요",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023044.1.7",
          "form": "하긴ㅜ 저도 딱 그마음이긴해요",
          "original_form": "하긴ㅜ 저도 딱 그마음이긴해요",
          "speaker_id": "1",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100023044.1.8",
          "form": "근데 산책시키는 애들보면 넘 귀엽더라고요ㅜ",
          "original_form": "근데 산책시키는 애들보면 넘 귀엽더라고요ㅜ",
          "speaker_id": "1",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100023044.1.9",
          "form": "맞아요... 남의 강아지 고양이 보면 너무 귀여워요 증말",
          "original_form": "맞아요... 남의 강아지 고양이 보면 너무 귀여워요 증말",
          "speaker_id": "2",
          "time": "20210829 22:01"
        },
        {
          "id": "MDRW2100023044.1.10",
          "form": "저는근데 사실 한번도 키워본적이 없어요",
          "original_form": "저는근데 사실 한번도 키워본적이 없어요",
          "speaker_id": "1",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100023044.1.11",
          "form": "혹시 키워보신적은 있으세요?",
          "original_form": "혹시 키워보신적은 있으세요?",
          "speaker_id": "1",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100023044.1.12",
          "form": "강아지 고양이는 아니고",
          "original_form": "강아지 고양이는 아니고",
          "speaker_id": "2",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100023044.1.13",
          "form": "어렸을때 햄스터랑 물고기 키워봤어요",
          "original_form": "어렸을때 햄스터랑 물고기 키워봤어요",
          "speaker_id": "2",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100023044.1.14",
          "form": "오 햄스터 한때 유행 이었어요",
          "original_form": "오 햄스터 한때 유행 이었어요",
          "speaker_id": "1",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100023044.1.15",
          "form": "햄토리..",
          "original_form": "햄토리..",
          "speaker_id": "1",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100023044.1.16",
          "form": "아세요? ㅋ",
          "original_form": "아세요? ㅋ",
          "speaker_id": "1",
          "time": "20210829 22:07"
        },
        {
          "id": "MDRW2100023044.1.17",
          "form": "헉 알죠",
          "original_form": "헉 알죠",
          "speaker_id": "2",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100023044.1.18",
          "form": "방가방가 햄토리",
          "original_form": "방가방가 햄토리",
          "speaker_id": "2",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100023044.1.19",
          "form": "어렸을때 좋아했어요 ㅋㅋㅋㅋ",
          "original_form": "어렸을때 좋아했어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100023044.1.20",
          "form": "지금봐도 너무 귀엽게생겼어요",
          "original_form": "지금봐도 너무 귀엽게생겼어요",
          "speaker_id": "2",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100023044.1.21",
          "form": "그 애니 덕에 햄스터 유행이었지 않나여 ㅋㅋㅋ",
          "original_form": "그 애니 덕에 햄스터 유행이었지 않나여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100023044.1.22",
          "form": "맞아요 ㅋㅋㅋㅋ 저는 리본 제일 좋아했어요",
          "original_form": "맞아요 ㅋㅋㅋㅋ 저는 리본 제일 좋아했어요",
          "speaker_id": "2",
          "time": "20210829 22:51"
        },
        {
          "id": "MDRW2100023044.1.23",
          "form": "암튼.. 그럼 주변에서 반려동물 많이 키우나요?",
          "original_form": "암튼.. 그럼 주변에서 반려동물 많이 키우나요?",
          "speaker_id": "2",
          "time": "20210829 22:51"
        },
        {
          "id": "MDRW2100023044.1.24",
          "form": "요새 1만보 걷기하는데",
          "original_form": "요새 1만보 걷기하는데",
          "speaker_id": "1",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100023044.1.25",
          "form": "동네 강아지란 강아지는 다본거같아요",
