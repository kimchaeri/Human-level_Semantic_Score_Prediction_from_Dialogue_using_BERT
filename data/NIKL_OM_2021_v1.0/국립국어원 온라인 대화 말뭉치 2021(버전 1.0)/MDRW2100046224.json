      "utterance": [
        {
          "id": "MDRW2100046224.1.1",
          "form": "그린티는 크림베이스에 녹차올린거같아요",
          "original_form": "그린티는 크림베이스에 녹차올린거같아요",
          "speaker_id": "1",
          "time": "20210908 11:23"
        },
        {
          "id": "MDRW2100046224.1.2",
          "form": "크림은 시른디 ㅋ",
          "original_form": "크림은 시른디 ㅋ",
          "speaker_id": "2",
          "time": "20210908 11:23"
        },
        {
          "id": "MDRW2100046224.1.3",
          "form": "프라푸치노예요",
          "original_form": "프라푸치노예요",
          "speaker_id": "1",
          "time": "20210908 11:23"
        },
        {
          "id": "MDRW2100046224.1.4",
          "form": "찾아봐야겠어요 ㅎ",
          "original_form": "찾아봐야겠어요 ㅎ",
          "speaker_id": "2",
          "time": "20210908 11:23"
        },
        {
          "id": "MDRW2100046224.1.5",
          "form": "빼고달라하든지",
          "original_form": "빼고달라하든지",
          "speaker_id": "2",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046224.1.6",
          "form": "그냥 글레이즈드 라떼 추천이요",
          "original_form": "그냥 글레이즈드 라떼 추천이요",
          "speaker_id": "1",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046224.1.7",
          "form": "그거부터먹고 ㅋ",
          "original_form": "그거부터먹고 ㅋ",
          "speaker_id": "2",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046224.1.8",
          "form": "어제 아이스로 얼음빼고 먹었는데 진하고 맛나요",
          "original_form": "어제 아이스로 얼음빼고 먹었는데 진하고 맛나요",
          "speaker_id": "1",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046224.1.9",
          "form": "깁콘많지싶어요",
          "original_form": "깁콘많지싶어요",
          "speaker_id": "2",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046224.1.10",
          "form": "양은 적지만ㅋ",
          "original_form": "양은 적지만ㅋ",
          "speaker_id": "1",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046224.1.11",
          "form": "얼음빼면 너무 진한거아니예요?",
          "original_form": "얼음빼면 너무 진한거아니예요?",
          "speaker_id": "2",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100046224.1.12",
          "form": "좀 진하더라구요ㅎ달고",
          "original_form": "좀 진하더라구요ㅎ달고",
          "speaker_id": "1",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100046224.1.13",
          "form": "얼음적게 추천",
          "original_form": "얼음적게 추천",
          "speaker_id": "1",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100046224.1.14",
          "form": "얼음 적게",
          "original_form": "얼음 적게",
          "speaker_id": "2",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100046224.1.15",
          "form": "기억해둘께요",
          "original_form": "기억해둘께요",
          "speaker_id": "2",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100046224.1.16",
          "form": "아님 핫으로  드세요",
          "original_form": "아님 핫으로  드세요",
          "speaker_id": "1",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100046224.1.17",
          "form": "아직 아이스가 더 땡겨요 ㅎ",
          "original_form": "아직 아이스가 더 땡겨요 ㅎ",
