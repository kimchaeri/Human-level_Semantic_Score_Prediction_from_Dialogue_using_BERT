        {
          "id": "MDRW2100038107.1.2",
          "form": "명탐정 코난 아니",
          "original_form": "명탐정 코난 아니",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100038107.1.3",
          "form": "나 그거 어릴 때부터 좋아해",
          "original_form": "나 그거 어릴 때부터 좋아해",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100038107.1.4",
          "form": "추리하는 고교 명탐생이 꼬맹이가 된 내용이지",
          "original_form": "추리하는 고교 명탐생이 꼬맹이가 된 내용이지",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100038107.1.5",
          "form": "근데 코나는 몇 십 년째 고등학생이네 아니 초등학생인가",
          "original_form": "근데 코나는 몇 십 년째 고등학생이네 아니 초등학생인가",
          "speaker_id": "2",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100038107.1.6",
          "form": "그래도 나름 재밌다고",
          "original_form": "그래도 나름 재밌다고",
          "speaker_id": "1",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100038107.1.7",
          "form": "맞아 나 몇 달 전에 영화도 봤어",
          "original_form": "맞아 나 몇 달 전에 영화도 봤어",
          "speaker_id": "2",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100038107.1.8",
          "form": "코난 요즘 휴재",
          "original_form": "코난 요즘 휴재",
          "speaker_id": "1",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100038107.1.9",
          "form": "헐 몰랐어 나는 계속 연재 중인데",
          "original_form": "헐 몰랐어 나는 계속 연재 중인데",
          "speaker_id": "2",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100038107.1.10",
          "form": "언제 중인 줄 알았어",
          "original_form": "언제 중인 줄 알았어",
          "speaker_id": "2",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100038107.1.11",
          "form": "작가인 고 효가 늙었나봐",
          "original_form": "작가인 고 효가 늙었나봐",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100038107.1.12",
          "form": "비색에 타는 재밌게 봤어",
          "original_form": "비색에 타는 재밌게 봤어",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100038107.1.13",
          "form": "맞아 그거 평 좋았어",
          "original_form": "맞아 그거 평 좋았어",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100038107.1.14",
          "form": "약간 올림픽 내용이 잖아",
