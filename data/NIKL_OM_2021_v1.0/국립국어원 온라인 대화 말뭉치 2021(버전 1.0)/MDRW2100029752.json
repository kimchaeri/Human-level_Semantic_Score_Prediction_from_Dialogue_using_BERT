      "utterance": [
        {
          "id": "MDRW2100029752.1.1",
          "form": "아 배부릅니다",
          "original_form": "아 배부릅니다",
          "speaker_id": "2",
          "time": "20210903 14:13"
        },
        {
          "id": "MDRW2100029752.1.2",
          "form": "세공기 드셨나요",
          "original_form": "세공기 드셨나요",
          "speaker_id": "1",
          "time": "20210903 14:14"
        },
        {
          "id": "MDRW2100029752.1.3",
          "form": "두공기요!!",
          "original_form": "두공기요!!",
          "speaker_id": "2",
          "time": "20210903 14:16"
        },
        {
          "id": "MDRW2100029752.1.4",
          "form": "후후",
          "original_form": "후후",
          "speaker_id": "2",
          "time": "20210903 14:16"
        },
        {
          "id": "MDRW2100029752.1.5",
          "form": "실망입니다",
          "original_form": "실망입니다",
          "speaker_id": "1",
          "time": "20210903 14:18"
        },
        {
          "id": "MDRW2100029752.1.6",
          "form": "배포가 작으시군요!",
          "original_form": "배포가 작으시군요!",
          "speaker_id": "1",
          "time": "20210903 14:19"
        },
        {
          "id": "MDRW2100029752.1.7",
          "form": "밥공기가 큰데요 ㅋㅋ",
          "original_form": "밥공기가 큰데요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:14"
        },
        {
          "id": "MDRW2100029752.1.8",
          "form": "비겁한 변명입니다!",
          "original_form": "비겁한 변명입니다!",
          "speaker_id": "1",
          "time": "20210903 15:19"
        },
        {
          "id": "MDRW2100029752.1.9",
          "form": "집밥 메뉴는 뭐였나요ㅎ",
          "original_form": "집밥 메뉴는 뭐였나요ㅎ",
          "speaker_id": "1",
          "time": "20210903 15:19"
        },
        {
          "id": "MDRW2100029752.1.10",
          "form": "그 돼지갈비 같은데",
          "original_form": "그 돼지갈비 같은데",
          "speaker_id": "2",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100029752.1.11",
          "form": "국물 좀 있고",
          "original_form": "국물 좀 있고",
          "speaker_id": "2",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100029752.1.12",
          "form": "산적이라고 하던가요",
          "original_form": "산적이라고 하던가요",
          "speaker_id": "2",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100029752.1.13",
          "form": "헉 거하게 차려주셨군요!",
          "original_form": "헉 거하게 차려주셨군요!",
          "speaker_id": "1",
          "time": "20210903 16:17"
        },
        {
          "id": "MDRW2100029752.1.14",
          "form": "잔치날인가요",
          "original_form": "잔치날인가요",
          "speaker_id": "1",
          "time": "20210903 16:17"
        },
        {
          "id": "MDRW2100029752.1.15",
          "form": "ㅋㅋ제가 name1님이나 갈구지",
          "original_form": "ㅋㅋ제가 &name1&님이나 갈구지",
          "speaker_id": "2",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100029752.1.16",
          "form": "꽤 인정받는 삶을",
          "original_form": "꽤 인정받는 삶을",
          "speaker_id": "2",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100029752.1.17",
          "form": "살고 있습니다",
          "original_form": "살고 있습니다",
          "speaker_id": "2",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100029752.1.18",
          "form": "ㅋㅋ 가진자인데 평생이 갑이시쥬ㅎㅎ",
          "original_form": "ㅋㅋ 가진자인데 평생이 갑이시쥬ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100029752.1.19",
          "form": "소고기도 막 드시구",
          "original_form": "소고기도 막 드시구",
          "speaker_id": "1",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100029752.1.20",
          "form": "돼지갈비도 드시구ㅎㅎ",
          "original_form": "돼지갈비도 드시구ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100029752.1.21",
          "form": "을들한테 원성도 드시구ㅎㅎㅎ",
          "original_form": "을들한테 원성도 드시구ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 16:47"
        },
        {
          "id": "MDRW2100029752.1.22",
          "form": "ㅋㅋㅋㅋㅋ 소고기 돈 없어서 40퍼 할인된거 먹는데요",
          "original_form": "ㅋㅋㅋㅋㅋ 소고기 돈 없어서 40퍼 할인된거 먹는데요",
          "speaker_id": "2",
          "time": "20210903 17:37"
        },
        {
          "id": "MDRW2100029752.1.23",
          "form": "님 때문에 누군가는 40퍼 소고기를 못 먹고 굶었습니다",
          "original_form": "님 때문에 누군가는 40퍼 소고기를 못 먹고 굶었습니다",
