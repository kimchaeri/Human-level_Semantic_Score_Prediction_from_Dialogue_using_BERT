      "utterance": [
        {
          "id": "MDRW2100003375.1.1",
          "form": "저녁 식사는 하셨나요?",
          "original_form": "저녁 식사는 하셨나요?",
          "speaker_id": "1",
          "time": "20210820 22:18"
        },
        {
          "id": "MDRW2100003375.1.2",
          "form": "고기를 맛있게 먹었답니다\r\n저녁메뉴는 어떤거드셨어요?",
          "original_form": "고기를 맛있게 먹었답니다\r\n저녁메뉴는 어떤거드셨어요?",
          "speaker_id": "2",
          "time": "20210820 22:19"
        },
        {
          "id": "MDRW2100003375.1.3",
          "form": "집에서 굽굽하신거에요?",
          "original_form": "집에서 굽굽하신거에요?",
          "speaker_id": "1",
          "time": "20210820 22:19"
        },
        {
          "id": "MDRW2100003375.1.4",
          "form": "저는 오늘 치킨이가 땡겨서\r\n치킨이 먹었어요ㅋㅋ",
          "original_form": "저는 오늘 치킨이가 땡겨서\r\n치킨이 먹었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:20"
        },
        {
          "id": "MDRW2100003375.1.5",
          "form": "기계가 돌돌 돌아가며 구워줘서 좋네요ㅎㅎ\r\n불금에 치킨좋습니다 배달 자주하세요?",
          "original_form": "기계가 돌돌 돌아가며 구워줘서 좋네요ㅎㅎ\r\n불금에 치킨좋습니다 배달 자주하세요?",
          "speaker_id": "2",
          "time": "20210820 22:22"
        },
        {
          "id": "MDRW2100003375.1.6",
          "form": "집밥 좋아하는 분과 살아서\r\n다른 집들에 비함 적은편 같아여ㅠㅋㅋ",
          "original_form": "집밥 좋아하는 분과 살아서\r\n다른 집들에 비함 적은편 같아여ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:27"
        },
        {
          "id": "MDRW2100003375.1.7",
          "form": "요리잘하시겠네요? 무슨요리젤자신있으세요?",
          "original_form": "요리잘하시겠네요? 무슨요리젤자신있으세요?",
          "speaker_id": "2",
          "time": "20210820 22:31"
        },
        {
          "id": "MDRW2100003375.1.8",
          "form": "음... 왠만한 한식은 다 해요 ㅋㅋ",
          "original_form": "음... 왠만한 한식은 다 해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:57"
        },
        {
          "id": "MDRW2100003375.1.9",
          "form": "긍정님은 배달 자주 시키시나여?",
          "original_form": "긍정님은 배달 자주 시키시나여?",
          "speaker_id": "1",
          "time": "20210820 22:57"
        },
        {
          "id": "MDRW2100003375.1.10",
          "form": "네 배달매니아에요",
          "original_form": "네 배달매니아에요",
          "speaker_id": "2",
          "time": "20210820 23:14"
        },
        {
          "id": "MDRW2100003375.1.11",
          "form": "근데 메뉴가 한정적이라 아쉽답니다ㅎ",
          "original_form": "근데 메뉴가 한정적이라 아쉽답니다ㅎ",
          "speaker_id": "2",
          "time": "20210820 23:14"
        },
        {
          "id": "MDRW2100003375.1.12",
          "form": "ㅋㅋ주로 뭐 시켜드세여?",
          "original_form": "ㅋㅋ주로 뭐 시켜드세여?",
          "speaker_id": "1",
          "time": "20210820 23:14"
        },
        {
          "id": "MDRW2100003375.1.13",
          "form": "그쵸ㅠ 한정적이죠",
          "original_form": "그쵸ㅠ 한정적이죠",
          "speaker_id": "1",
          "time": "20210820 23:14"
        },
        {
          "id": "MDRW2100003375.1.14",
          "form": "치킨 피자가 기본이구요!\r\n나머지는 할인쿠폰적용되는거요 ㅎㅎ\r\n아 요리조리법은 따로 배우셨나요?\r\n원래 체질적으로 요리잘하시는건가요?",
          "original_form": "치킨 피자가 기본이구요!\r\n나머지는 할인쿠폰적용되는거요 ㅎㅎ\r\n아 요리조리법은 따로 배우셨나요?\r\n원래 체질적으로 요리잘하시는건가요?",
          "speaker_id": "2",
          "time": "20210820 23:18"
        },
        {
          "id": "MDRW2100003375.1.15",
          "form": "ㅋㅋ정보의 바다에서 보고 합니다ㅋㅋ",
          "original_form": "ㅋㅋ정보의 바다에서 보고 합니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 23:21"
        },
        {
          "id": "MDRW2100003375.1.16",
          "form": "체질적으로는\r\n남이 해준걸 잘 먹는 체질요ㅋㅋ",
          "original_form": "체질적으로는\r\n남이 해준걸 잘 먹는 체질요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 23:21"
        },
        {
          "id": "MDRW2100003375.1.17",
          "form": "아 정말 요리잘하는 분들 젤부럽습니다 ㅎㅎ\r\n맛집은 자주다니세요?",
          "original_form": "아 정말 요리잘하는 분들 젤부럽습니다 ㅎㅎ\r\n맛집은 자주다니세요?",
          "speaker_id": "2",
          "time": "20210820 23:24"
        },
        {
          "id": "MDRW2100003375.1.18",
          "form": "코시국 전에는 그래도 찾아 다녔는데",
          "original_form": "코시국 전에는 그래도 찾아 다녔는데",
          "speaker_id": "1",
          "time": "20210820 23:26"
        },
        {
          "id": "MDRW2100003375.1.19",
          "form": "코시국 이후 외식 한번도 한 적 없어요ㅠㅋ",
          "original_form": "코시국 이후 외식 한번도 한 적 없어요ㅠㅋ",
          "speaker_id": "1",
          "time": "20210820 23:26"
        },
        {
          "id": "MDRW2100003375.1.20",
          "form": "아 정말 칭찬해줄부분이네요\r\n맛집가서 괜히 코로나 걸릴까봐 불안하긴하죠!",
          "original_form": "아 정말 칭찬해줄부분이네요\r\n맛집가서 괜히 코로나 걸릴까봐 불안하긴하죠!",
          "speaker_id": "2",
          "time": "20210820 23:27"
        }
      ]
    }
