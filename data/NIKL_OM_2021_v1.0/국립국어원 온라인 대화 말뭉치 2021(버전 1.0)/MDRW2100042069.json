      "utterance": [
        {
          "id": "MDRW2100042069.1.1",
          "form": "안자고 뭐하노??",
          "original_form": "안자고 뭐하노??",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100042069.1.2",
          "form": "자다깼나??",
          "original_form": "자다깼나??",
          "speaker_id": "2",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100042069.1.3",
          "form": "오늘은 안 잤어요ㅋㅋㅋㅋ",
          "original_form": "오늘은 안 잤어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042069.1.4",
          "form": "신랑이랑 지코바 뜯을라고ㅋㅋㅋㅋ",
          "original_form": "신랑이랑 지코바 뜯을라고ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042069.1.5",
          "form": "다이어트 개나 줘버려ㅋㅋ",
          "original_form": "다이어트 개나 줘버려ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042069.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100042069.1.7",
          "form": "지코바가 잠을 이겼네",
          "original_form": "지코바가 잠을 이겼네",
          "speaker_id": "2",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100042069.1.8",
          "form": "역시 멋지다",
          "original_form": "역시 멋지다",
          "speaker_id": "2",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100042069.1.9",
          "form": "다이어트 따위 뭐",
          "original_form": "다이어트 따위 뭐",
          "speaker_id": "2",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100042069.1.10",
          "form": "항상 식욕은 잠을 이깁니다ㅋㅋㅋㅋㅋ",
          "original_form": "항상 식욕은 잠을 이깁니다ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100042069.1.11",
          "form": "내일 다시 시작하죠 뭐ㅋㅋㅋㅋㅋ",
          "original_form": "내일 다시 시작하죠 뭐ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100042069.1.12",
          "form": "진짜 이제는 약 안먹고는 힘들 거 같네요ㅋㅋㅋ",
          "original_form": "진짜 이제는 약 안먹고는 힘들 거 같네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100042069.1.13",
          "form": "아직 약은 안 된다",
          "original_form": "아직 약은 안 된다",
          "speaker_id": "2",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100042069.1.14",
          "form": "치킨 왔나",
          "original_form": "치킨 왔나",
          "speaker_id": "2",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042069.1.15",
          "form": "아직 안 왔어요 언니",
          "original_form": "아직 안 왔어요 언니",
          "speaker_id": "1",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042069.1.16",
          "form": "내가 답이 없거든 뜯고 있는 줄 알아요",
          "original_form": "내가 답이 없거든 뜯고 있는 줄 알아요",
          "speaker_id": "1",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042069.1.17",
          "form": "그래 알았다 빨리 얘기하자",
          "original_form": "그래 알았다 빨리 얘기하자",
          "speaker_id": "2",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042069.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042069.1.19",
          "form": "스피드 스피드 스피드",
          "original_form": "스피드 스피드 스피드",
          "speaker_id": "1",
          "time": "20210906 21:31"
        },
        {
          "id": "MDRW2100042069.1.20",
          "form": "오늘 저녁은 뭐 먹었어요",
          "original_form": "오늘 저녁은 뭐 먹었어요",
          "speaker_id": "1",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100042069.1.21",
          "form": "오늘은",
          "original_form": "오늘은",
          "speaker_id": "2",
          "time": "20210906 21:32"
        },
        {
          "id": "MDRW2100042069.1.22",
          "form": "반찬 나무 숟가락에서",
