      "utterance": [
        {
          "id": "MDRW2100043788.1.1",
          "form": "물을 주시오...",
          "original_form": "물을 주시오...",
          "speaker_id": "1",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100043788.1.2",
          "form": "물마시면 화장품맛 날텐데",
          "original_form": "물마시면 화장품맛 날텐데",
          "speaker_id": "2",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043788.1.3",
          "form": "좀 참았다 드시오",
          "original_form": "좀 참았다 드시오",
          "speaker_id": "2",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043788.1.4",
          "form": "응.. 맞아",
          "original_form": "응.. 맞아",
          "speaker_id": "1",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043788.1.5",
          "form": "몇분 참고 깨끗한 맹물을 벌컥 드시오",
          "original_form": "몇분 참고 깨끗한 맹물을 벌컥 드시오",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043788.1.6",
          "form": "네 그래야겠습니다",
          "original_form": "네 그래야겠습니다",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043788.1.7",
          "form": "물 벌컥벌컥",
          "original_form": "물 벌컥벌컥",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043788.1.8",
          "form": "못마신다 생각하면 더 목이 마르단 말이지 ㅋㅋ",
          "original_form": "못마신다 생각하면 더 목이 마르단 말이지 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043788.1.9",
          "form": "응 목이 마르오...",
          "original_form": "응 목이 마르오...",
          "speaker_id": "1",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100043788.1.10",
          "form": "관대한 동생이 얼른 목을 축일 수 있길",
          "original_form": "관대한 동생이 얼른 목을 축일 수 있길",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043788.1.11",
          "form": "맥주향 탄산수 있으니까",
          "original_form": "맥주향 탄산수 있으니까",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043788.1.12",
          "form": "술 마실 생각이 안난다",
          "original_form": "술 마실 생각이 안난다",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043788.1.13",
          "form": "원래도 술 잘 안마시지만",
          "original_form": "원래도 술 잘 안마시지만",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043788.1.14",
          "form": "밀키스 있으니까 그건 먹자",
          "original_form": "밀키스 있으니까 그건 먹자",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043788.1.15",
          "form": "딸기보드카",
          "original_form": "딸기보드카",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043788.1.16",
          "form": "아 그래야지",
          "original_form": "아 그래야지",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043788.1.17",
          "form": "2주 지나면",
          "original_form": "2주 지나면",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
