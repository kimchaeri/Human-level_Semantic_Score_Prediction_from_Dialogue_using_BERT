      "utterance": [
        {
          "id": "MDRW2100033635.1.1",
          "form": "라면 좋아하세요?",
          "original_form": "라면 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 19:13"
        },
        {
          "id": "MDRW2100033635.1.2",
          "form": "불닭볶음면이랑 오모리 김치찌개라면 좋아해요 아시나요",
          "original_form": "불닭볶음면이랑 오모리 김치찌개라면 좋아해요 아시나요",
          "speaker_id": "2",
          "time": "20210824 19:13"
        },
        {
          "id": "MDRW2100033635.1.3",
          "form": "네 당연히 알죠",
          "original_form": "네 당연히 알죠",
          "speaker_id": "1",
          "time": "20210824 19:13"
        },
        {
          "id": "MDRW2100033635.1.4",
          "form": "오모리 지에스에만 팔잖아요",
          "original_form": "오모리 지에스에만 팔잖아요",
          "speaker_id": "1",
          "time": "20210824 19:13"
        },
        {
          "id": "MDRW2100033635.1.5",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "2",
          "time": "20210824 19:14"
        },
        {
          "id": "MDRW2100033635.1.6",
          "form": "매운 거 잘드세요?",
          "original_form": "매운 거 잘드세요?",
          "speaker_id": "1",
          "time": "20210824 19:14"
        },
        {
          "id": "MDRW2100033635.1.7",
          "form": "봉 지 보다 컵라면이 맛있더라구요",
          "original_form": "봉 지 보다 컵라면이 맛있더라구요",
          "speaker_id": "2",
          "time": "20210824 19:14"
        },
        {
          "id": "MDRW2100033635.1.8",
          "form": "불닭을 좋아하신다고 해서!!",
          "original_form": "불닭을 좋아하신다고 해서!!",
          "speaker_id": "1",
          "time": "20210824 19:14"
        },
        {
          "id": "MDRW2100033635.1.9",
          "form": "저 맵찔인데",
          "original_form": "저 맵찔인데",
          "speaker_id": "2",
          "time": "20210824 19:14"
        },
        {
          "id": "MDRW2100033635.1.10",
          "form": "불닭은 이제 까르보불닭에 정착했어요",
          "original_form": "불닭은 이제 까르보불닭에 정착했어요",
          "speaker_id": "2",
          "time": "20210824 19:14"
        },
        {
          "id": "MDRW2100033635.1.11",
          "form": "드셔보셧나요",
          "original_form": "드셔보셧나요",
          "speaker_id": "2",
          "time": "20210824 19:14"
        },
        {
          "id": "MDRW2100033635.1.12",
          "form": "아 정말요???",
          "original_form": "아 정말요???",
          "speaker_id": "1",
          "time": "20210824 19:14"
        },
        {
          "id": "MDRW2100033635.1.13",
          "form": "저 까르보불닭 사랑해요",
          "original_form": "저 까르보불닭 사랑해요",
          "speaker_id": "1",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100033635.1.14",
          "form": "제 최애입니다",
          "original_form": "제 최애입니다",
          "speaker_id": "1",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100033635.1.15",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100033635.1.16",
          "form": "뭘좀 아시는분..",
          "original_form": "뭘좀 아시는분..",
          "speaker_id": "2",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100033635.1.17",
          "form": "까르보불닭 면 약간 두꺼운것도 좋고",
          "original_form": "까르보불닭 면 약간 두꺼운것도 좋고",
          "speaker_id": "2",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100033635.1.18",
          "form": "전 갠적으로 불닭은 맛 없어요",
          "original_form": "전 갠적으로 불닭은 맛 없어요",
          "speaker_id": "1",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100033635.1.19",
          "form": "꾸덕한것도 좋고..",
          "original_form": "꾸덕한것도 좋고..",
          "speaker_id": "2",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100033635.1.20",
          "form": "까르보가 최고요",
          "original_form": "까르보가 최고요",
          "speaker_id": "1",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100033635.1.21",
          "form": "그거 소스로 리조또해먹어도 맛잇어요",
          "original_form": "그거 소스로 리조또해먹어도 맛잇어요",
          "speaker_id": "2",
          "time": "20210824 19:16"
        },
        {
          "id": "MDRW2100033635.1.22",
          "form": "불닭은 좀 너무 매워요",
          "original_form": "불닭은 좀 너무 매워요",
          "speaker_id": "2",
          "time": "20210824 19:16"
        },
        {
          "id": "MDRW2100033635.1.23",
          "form": "맞아요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:16"
        }
