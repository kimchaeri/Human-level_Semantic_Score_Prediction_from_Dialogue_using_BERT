      "utterance": [
        {
          "id": "MDRW2100019700.1.1",
          "form": "흠 전 양고기빼고 안 가리는데",
          "original_form": "흠 전 양고기빼고 안 가리는데",
          "speaker_id": "1",
          "time": "20210827 21:22"
        },
        {
          "id": "MDRW2100019700.1.2",
          "form": "좋아하는 음식보면 닭인거같아요 ㅋㅋㅋ",
          "original_form": "좋아하는 음식보면 닭인거같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:23"
        },
        {
          "id": "MDRW2100019700.1.3",
          "form": "아 저도 양고기는 못 먹겠어요 ㅠㅠ 이질감이 있고 냄새가ㅠㅠ",
          "original_form": "아 저도 양고기는 못 먹겠어요 ㅠㅠ 이질감이 있고 냄새가ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 21:28"
        },
        {
          "id": "MDRW2100019700.1.4",
          "form": "맞아요 양꼬치도 한번먹고.. 그담부턴 안먹었어요",
          "original_form": "맞아요 양꼬치도 한번먹고.. 그담부턴 안먹었어요",
          "speaker_id": "1",
          "time": "20210827 21:28"
        },
        {
          "id": "MDRW2100019700.1.5",
          "form": "치킨이 최고 아닌가요!",
          "original_form": "치킨이 최고 아닌가요!",
          "speaker_id": "2",
          "time": "20210827 21:29"
        },
        {
          "id": "MDRW2100019700.1.6",
          "form": "맞아요 ㅋㅋㅋ 어디거 좋아하세요?",
          "original_form": "맞아요 ㅋㅋㅋ 어디거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 21:31"
        },
        {
          "id": "MDRW2100019700.1.7",
          "form": "저는 교촌이랑 자담이요!",
          "original_form": "저는 교촌이랑 자담이요!",
          "speaker_id": "2",
          "time": "20210827 21:31"
        },
        {
          "id": "MDRW2100019700.1.8",
          "form": "자담 맛있나요? 광고만 보고 못 먹어봤네요",
          "original_form": "자담 맛있나요? 광고만 보고 못 먹어봤네요",
          "speaker_id": "1",
          "time": "20210827 21:32"
        },
        {
          "id": "MDRW2100019700.1.9",
          "form": "네 생각보다 맛있더라구요! 근데 점바점이 심해서..엄청 추천은 못 하겠더라구요ㅠㅠㅋㅋ",
          "original_form": "네 생각보다 맛있더라구요! 근데 점바점이 심해서..엄청 추천은 못 하겠더라구요ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100019700.1.10",
          "form": "어디 치킨 좋아하시나요?",
          "original_form": "어디 치킨 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100019700.1.11",
          "form": "아 점바점 심하면 좀 글쵸 ㅠㅠ",
          "original_form": "아 점바점 심하면 좀 글쵸 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100019700.1.12",
          "form": "저는 뿌링클이요 ㅋㅋㅋ",
          "original_form": "저는 뿌링클이요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100019700.1.13",
          "form": "뿌링클 멘보샤 드셔보셨나요?????",
          "original_form": "뿌링클 멘보샤 드셔보셨나요?????",
          "speaker_id": "2",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100019700.1.14",
          "form": "헛 아니요 맛있나요?",
          "original_form": "헛 아니요 맛있나요?",
          "speaker_id": "1",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100019700.1.15",
          "form": "멘보샤에 뿌링클 뿌려져있어요 ㅋㅋㅋㅋ",
          "original_form": "멘보샤에 뿌링클 뿌려져있어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100019700.1.16",
          "form": "한번쯤 먹어볼만 한 것 같아요!",
          "original_form": "한번쯤 먹어볼만 한 것 같아요!",
          "speaker_id": "2",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100019700.1.17",
          "form": "멘보샤도 안 먹어봐서 궁금하네요 ㅋㅋㅋ",
          "original_form": "멘보샤도 안 먹어봐서 궁금하네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100019700.1.18",
          "form": "솔직히 멘보샤 자체는....아직 맛있는 곳 안 먹어봐서 ㅠㅠㅋㅋㅋ",
          "original_form": "솔직히 멘보샤 자체는....아직 맛있는 곳 안 먹어봐서 ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100019700.1.19",
          "form": "저도 잘 모르는 메뉴지만ㅋㅋㅋ 뿌링클 가루가 있어서 그런지",
          "original_form": "저도 잘 모르는 메뉴지만ㅋㅋㅋ 뿌링클 가루가 있어서 그런지",
          "speaker_id": "2",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100019700.1.20",
          "form": "맛날거같아요",
          "original_form": "맛날거같아요",
          "speaker_id": "1",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100019700.1.21",
          "form": "새우+빵+튀김+뿌링클이니까요!ㅌㅋㅋㅋㅋㅋ",
          "original_form": "새우+빵+튀김+뿌링클이니까요!ㅌㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100019700.1.22",
          "form": "맛없없이네요 그렇게 들으니 ㅋㅋㅋㅋ",
          "original_form": "맛없없이네요 그렇게 들으니 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:45"
        }
      ]
    }
