      "utterance": [
        {
          "id": "MDRW2100015518.1.1",
          "form": "책 하루에 몇 권 독서하세요?",
          "original_form": "책 하루에 몇 권 독서하세요?",
          "speaker_id": "1",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015518.1.2",
          "form": "저요 ㅋㅋ저는 하루요?",
          "original_form": "저요 ㅋㅋ저는 하루요?",
          "speaker_id": "2",
          "time": "20210825 23:22"
        },
        {
          "id": "MDRW2100015518.1.3",
          "form": "예선에 많이 읽을때는",
          "original_form": "예선에 많이 읽을때는",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015518.1.4",
          "form": "네~하루에 몇 권 읽어요?",
          "original_form": "네~하루에 몇 권 읽어요?",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015518.1.5",
          "form": "하루에 두권도 읽었는데",
          "original_form": "하루에 두권도 읽었는데",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015518.1.6",
          "form": "요즘은 많이 안보네요",
          "original_form": "요즘은 많이 안보네요",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015518.1.7",
          "form": "독서 좋아하세요?",
          "original_form": "독서 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015518.1.8",
          "form": "그럼 요즘은 얼마나 읽어요?",
          "original_form": "그럼 요즘은 얼마나 읽어요?",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015518.1.9",
          "form": "저는 매일 책 봐요!",
          "original_form": "저는 매일 책 봐요!",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015518.1.10",
          "form": "오 어떤거 보세요??",
          "original_form": "오 어떤거 보세요??",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015518.1.11",
          "form": "요즘은 사실 이북을 많이 보네요",
          "original_form": "요즘은 사실 이북을 많이 보네요",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015518.1.12",
          "form": "요즘엔 마이클샌델의 공정하다는 착각 보구요",
          "original_form": "요즘엔 마이클샌델의 공정하다는 착각 보구요",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015518.1.13",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015518.1.14",
          "form": "누구 먼저 살려야할까? 라는 책도 봤어요~",
          "original_form": "누구 먼저 살려야할까? 라는 책도 봤어요~",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015518.1.15",
          "form": "무슨 내용이에요???",
          "original_form": "무슨 내용이에요???",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015518.1.16",
          "form": "능력주의는 모두에게 같은 기회를 제공하는가?",
          "original_form": "능력주의는 모두에게 같은 기회를 제공하는가?",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015518.1.17",
          "form": "와 철학적이네요",
          "original_form": "와 철학적이네요",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015518.1.18",
          "form": "이거에 초점에 맞춰서 여러 챕터가 있어요!",
          "original_form": "이거에 초점에 맞춰서 여러 챕터가 있어요!",
          "speaker_id": "1",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015518.1.19",
          "form": "오 ㅋㅋㅋ저는 예전에 교양에서 읽은거빼고는 평소에는 그런책잘안읽는데",
          "original_form": "오 ㅋㅋㅋ저는 예전에 교양에서 읽은거빼고는 평소에는 그런책잘안읽는데",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015518.1.20",
          "form": "대단하네요",
          "original_form": "대단하네요",
          "speaker_id": "2",
          "time": "20210825 23:25"
        },
        {
          "id": "MDRW2100015518.1.21",
          "form": "아 ㅎㅎ 요즘 자기계발이나 심리 관련 책도 좋더라구요",
          "original_form": "아 ㅎㅎ 요즘 자기계발이나 심리 관련 책도 좋더라구요",
          "speaker_id": "1",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100015518.1.22",
          "form": "아하 저는 추리소설 많이 읽어요",
          "original_form": "아하 저는 추리소설 많이 읽어요",
          "speaker_id": "2",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100015518.1.23",
          "form": "추리소설로 유명한 작가 누구 있었죠?",
          "original_form": "추리소설로 유명한 작가 누구 있었죠?",
          "speaker_id": "1",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100015518.1.24",
          "form": "일본에 게이고였나요..",
          "original_form": "일본에 게이고였나요..",
          "speaker_id": "1",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100015518.1.25",
          "form": "아니면 베스트셀러 소설 많이 읽었어요",
          "original_form": "아니면 베스트셀러 소설 많이 읽었어요",
          "speaker_id": "2",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100015518.1.26",
          "form": "히가시노게이고 소설",
          "original_form": "히가시노게이고 소설",
          "speaker_id": "2",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100015518.1.27",
          "form": "유명한 작가님 맞죠?",
          "original_form": "유명한 작가님 맞죠?",
          "speaker_id": "1",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100015518.1.28",
          "form": "진짜 반은 읽었는데 워낙 다작가라",
          "original_form": "진짜 반은 읽었는데 워낙 다작가라",
          "speaker_id": "2",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100015518.1.29",
          "form": "네 ㅋㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100015518.1.30",
          "form": "ㅋㅋㅋ책도 많이 내셨던데 다 잘팔리는 게 와우",
          "original_form": "ㅋㅋㅋ책도 많이 내셨던데 다 잘팔리는 게 와우",
          "speaker_id": "1",
          "time": "20210825 23:28"
        },
        {
          "id": "MDRW2100015518.1.31",
          "form": "맞아요 아니면 생존 소설 좋아해요 공상과학소설이요",
          "original_form": "맞아요 아니면 생존 소설 좋아해요 공상과학소설이요",
          "speaker_id": "2",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100015518.1.32",
          "form": "매트로 2033같은거요 ㅋㅋ",
          "original_form": "매트로 2033같은거요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100015518.1.33",
          "form": "오 그런 장르는 전 영화로밖에 안만나봤어요 ㅋㅋ",
          "original_form": "오 그런 장르는 전 영화로밖에 안만나봤어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:29"
        },
        {
          "id": "MDRW2100015518.1.34",
          "form": "앜ㅋㅋ전영화나 미드보다가 그원작소설보고하는 ㅋㅋㅋ",
          "original_form": "앜ㅋㅋ전영화나 미드보다가 그원작소설보고하는 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015518.1.35",
          "form": "아! 그렇구나~",
          "original_form": "아! 그렇구나~",
          "speaker_id": "1",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015518.1.36",
          "form": "기욤 뮈소 프랑스베셀작가도 좋아하고요 ...그냥 드라마 소설 ㅋㅋㅋ네",
          "original_form": "기욤 뮈소 프랑스베셀작가도 좋아하고요 ...그냥 드라마 소설 ㅋㅋㅋ네",
          "speaker_id": "2",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015518.1.37",
          "form": "기욤 뮈소 작가도 유명하죠 진짜 ㅎㅎ",
          "original_form": "기욤 뮈소 작가도 유명하죠 진짜 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015518.1.38",
          "form": "되게 다독가이시네요",
          "original_form": "되게 다독가이시네요",
          "speaker_id": "1",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015518.1.39",
          "form": "좀 많이 읽었어요 한때",
          "original_form": "좀 많이 읽었어요 한때",
          "speaker_id": "2",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015518.1.40",
          "form": "문제는 앤디님처럼 좀 어려운게아니라",
          "original_form": "문제는 앤디님처럼 좀 어려운게아니라",
          "speaker_id": "2",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015518.1.41",
          "form": "다 소설이에요 ㅋㅋㅋ베스트셀러소설 ㅋㅋ",
          "original_form": "다 소설이에요 ㅋㅋㅋ베스트셀러소설 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015518.1.42",
          "form": "에이 어렵긴요 ㅜㅜ 그냥 술술 읽어보는 거죠 뭐",
          "original_form": "에이 어렵긴요 ㅜㅜ 그냥 술술 읽어보는 거죠 뭐",
          "speaker_id": "1",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015518.1.43",
          "form": "이기적 유전자가 젤 어려웠어요 저는..",
          "original_form": "이기적 유전자가 젤 어려웠어요 저는..",
          "speaker_id": "1",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015518.1.44",
          "form": "그걸 고2 때 왜 읽었을까...",
          "original_form": "그걸 고2 때 왜 읽었을까...",
          "speaker_id": "1",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015518.1.45",
          "form": "그러다가 요즘은 그냥 더쉬운 이북으로 넘어오고 ㅋㅋㅋ",
          "original_form": "그러다가 요즘은 그냥 더쉬운 이북으로 넘어오고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015518.1.46",
          "form": "아 ㅋㅋ그때 권장도서였나",
          "original_form": "아 ㅋㅋ그때 권장도서였나",
          "speaker_id": "2",
          "time": "20210825 23:31"
        }
      ]
