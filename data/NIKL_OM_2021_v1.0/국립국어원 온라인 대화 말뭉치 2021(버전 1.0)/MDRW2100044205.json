          "form": "어어~",
          "original_form": "어어~",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044205.1.10",
          "form": "고거 사야겠다~",
          "original_form": "고거 사야겠다~",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044205.1.11",
          "form": "얼마였지??",
          "original_form": "얼마였지??",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044205.1.12",
