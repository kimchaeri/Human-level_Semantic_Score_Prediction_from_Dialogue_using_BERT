      "utterance": [
        {
          "id": "MDRW2100002388.1.1",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 11:00"
        },
        {
          "id": "MDRW2100002388.1.2",
          "form": "좋아해요! 해외여행을 요즘 자주 못가서 아쉬운데",
          "original_form": "좋아해요! 해외여행을 요즘 자주 못가서 아쉬운데",
          "speaker_id": "2",
          "time": "20210820 11:00"
        },
        {
          "id": "MDRW2100002388.1.3",
          "form": "여행 너무 좋죠 여행 좋아하세요?",
          "original_form": "여행 너무 좋죠 여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 11:00"
        },
        {
          "id": "MDRW2100002388.1.4",
          "form": "저도 해외여행 못가서 아쉬워요",
          "original_form": "저도 해외여행 못가서 아쉬워요",
          "speaker_id": "1",
          "time": "20210820 11:01"
        },
        {
          "id": "MDRW2100002388.1.5",
          "form": "해외 가봤던곳중에서 가장 기억에 남는곳은 어디에요?",
          "original_form": "해외 가봤던곳중에서 가장 기억에 남는곳은 어디에요?",
          "speaker_id": "1",
          "time": "20210820 11:01"
        },
        {
          "id": "MDRW2100002388.1.6",
          "form": "어 저는 보라카이요 제가 망고를 좋아해서 망고 실컷먹고 왔어요",
          "original_form": "어 저는 보라카이요 제가 망고를 좋아해서 망고 실컷먹고 왔어요",
          "speaker_id": "2",
          "time": "20210820 11:02"
        },
        {
          "id": "MDRW2100002388.1.7",
          "form": "오는길엔 옻이 올라서..고생했지만",
          "original_form": "오는길엔 옻이 올라서..고생했지만",
          "speaker_id": "2",
          "time": "20210820 11:02"
        },
        {
          "id": "MDRW2100002388.1.8",
          "form": "최근에 어디 여행 갓다오셨나요?",
          "original_form": "최근에 어디 여행 갓다오셨나요?",
          "speaker_id": "2",
          "time": "20210820 11:02"
        },
        {
          "id": "MDRW2100002388.1.9",
          "form": "저는 강원도 양양을 다녀왔어요",
          "original_form": "저는 강원도 양양을 다녀왔어요",
          "speaker_id": "1",
          "time": "20210820 11:02"
        },
        {
          "id": "MDRW2100002388.1.10",
          "form": "바다를 즐기거싶었는데 사람이 진짜 많더라구요",
          "original_form": "바다를 즐기거싶었는데 사람이 진짜 많더라구요",
          "speaker_id": "1",
          "time": "20210820 11:02"
        },
        {
          "id": "MDRW2100002388.1.11",
          "form": "아 저두 양양 갔다왔는데!",
          "original_form": "아 저두 양양 갔다왔는데!",
          "speaker_id": "2",
          "time": "20210820 11:05"
        },
        {
          "id": "MDRW2100002388.1.12",
          "form": "구경은 많이 하셨어요?",
          "original_form": "구경은 많이 하셨어요?",
          "speaker_id": "2",
          "time": "20210820 11:05"
        },
        {
          "id": "MDRW2100002388.1.13",
          "form": "네에!!! 댕댕이들 왔는데 진짜 너무 귀여워서 행복했어요",
          "original_form": "네에!!! 댕댕이들 왔는데 진짜 너무 귀여워서 행복했어요",
          "speaker_id": "1",
          "time": "20210820 11:07"
        },
        {
          "id": "MDRW2100002388.1.14",
          "form": "어느 여행지나 강아지는 꼭 있는것같아요",
          "original_form": "어느 여행지나 강아지는 꼭 있는것같아요",
          "speaker_id": "1",
          "time": "20210820 11:07"
        },
        {
          "id": "MDRW2100002388.1.15",
          "form": "맞아요 요즘은 여행장소에 반려견을 데려갈 수 있더라구요!",
          "original_form": "맞아요 요즘은 여행장소에 반려견을 데려갈 수 있더라구요!",
          "speaker_id": "2",
          "time": "20210820 11:09"
        },
        {
          "id": "MDRW2100002388.1.16",
          "form": "여행계획은 없으세요?",
          "original_form": "여행계획은 없으세요?",
          "speaker_id": "2",
          "time": "20210820 11:09"
        },
        {
          "id": "MDRW2100002388.1.17",
          "form": "제주도를 가볼까 생각중인데",
          "original_form": "제주도를 가볼까 생각중인데",
          "speaker_id": "1",
          "time": "20210820 11:13"
        },
        {
          "id": "MDRW2100002388.1.18",
          "form": "일단 생각만 하고있어요",
          "original_form": "일단 생각만 하고있어요",
          "speaker_id": "1",
          "time": "20210820 11:13"
        },
        {
          "id": "MDRW2100002388.1.19",
          "form": "아 제주도 너무 좋지요 제주도는 가도가도 좋은거같아요",
          "original_form": "아 제주도 너무 좋지요 제주도는 가도가도 좋은거같아요",
          "speaker_id": "2",
          "time": "20210820 11:15"
        },
        {
          "id": "MDRW2100002388.1.20",
          "form": "제주도 가볼만한곳 추천해주실수있나요?",
          "original_form": "제주도 가볼만한곳 추천해주실수있나요?",
          "speaker_id": "1",
          "time": "20210820 11:15"
        },
        {
          "id": "MDRW2100002388.1.21",
          "form": "제주도에 금녕해변이요",
          "original_form": "제주도에 금녕해변이요",
          "speaker_id": "2",
          "time": "20210820 11:17"
        },
        {
          "id": "MDRW2100002388.1.22",
          "form": "해변치고 덜유명한데 파라솔도 대여할 수있어서",
          "original_form": "해변치고 덜유명한데 파라솔도 대여할 수있어서",
          "speaker_id": "2",
          "time": "20210820 11:17"
        },
        {
          "id": "MDRW2100002388.1.23",
          "form": "풍경보면서 맥주 마시기 좋더라구요",
          "original_form": "풍경보면서 맥주 마시기 좋더라구요",
          "speaker_id": "2",
          "time": "20210820 11:17"
        },
        {
          "id": "MDRW2100002388.1.24",
          "form": "오 술이 꿀떡꿀떡 넘어가겠네요",
          "original_form": "오 술이 꿀떡꿀떡 넘어가겠네요",
          "speaker_id": "1",
          "time": "20210820 11:18"
        },
        {
          "id": "MDRW2100002388.1.25",
          "form": "벌써부터 기대가되요",
          "original_form": "벌써부터 기대가되요",
          "speaker_id": "1",
          "time": "20210820 11:18"
        },
        {
          "id": "MDRW2100002388.1.26",
          "form": "가을쯤 가시면 좋겠어요! 제주도여행가시면",
          "original_form": "가을쯤 가시면 좋겠어요! 제주도여행가시면",
          "speaker_id": "2",
          "time": "20210820 11:19"
        },
        {
          "id": "MDRW2100002388.1.27",
          "form": "공항에서 마음쿠키라고 있는데 드셔보세요",
          "original_form": "공항에서 마음쿠키라고 있는데 드셔보세요",
          "speaker_id": "2",
          "time": "20210820 11:19"
        }
      ]
