      "utterance": [
        {
          "id": "MDRW2100029497.1.1",
          "form": "오늘 날씨가 정말좋네요",
          "original_form": "오늘 날씨가 정말좋네요",
          "speaker_id": "1",
          "time": "20210903 09:41"
        },
        {
          "id": "MDRW2100029497.1.2",
          "form": "완연한 가을",
          "original_form": "완연한 가을",
          "speaker_id": "1",
          "time": "20210903 09:41"
        },
        {
          "id": "MDRW2100029497.1.3",
          "form": "아침바람은 춥더라구요~",
          "original_form": "아침바람은 춥더라구요~",
          "speaker_id": "2",
          "time": "20210903 10:34"
        },
        {
          "id": "MDRW2100029497.1.4",
          "form": "어제부터 추웠어요ㅜㅋㅋ",
          "original_form": "어제부터 추웠어요ㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 10:35"
        },
        {
          "id": "MDRW2100029497.1.5",
          "form": "여긴 아침에 나가보니 비가 그쳤지만",
          "original_form": "여긴 아침에 나가보니 비가 그쳤지만",
          "speaker_id": "2",
          "time": "20210903 10:35"
        },
        {
          "id": "MDRW2100029497.1.6",
          "form": "아직 흐리네요 12시쯤 또 비소식이있네용",
          "original_form": "아직 흐리네요 12시쯤 또 비소식이있네용",
          "speaker_id": "2",
          "time": "20210903 10:35"
        },
        {
          "id": "MDRW2100029497.1.7",
          "form": "아 거기 어딘데요? 여기는 충북인데 하늘 정말 예뻐요",
          "original_form": "아 거기 어딘데요? 여기는 충북인데 하늘 정말 예뻐요",
          "speaker_id": "1",
          "time": "20210903 10:50"
        },
        {
          "id": "MDRW2100029497.1.8",
          "form": "며칠동안 내내 오더니",
          "original_form": "며칠동안 내내 오더니",
          "speaker_id": "1",
          "time": "20210903 10:50"
        },
        {
          "id": "MDRW2100029497.1.9",
          "form": "ㅇㅕ긴 경남거제입니당",
          "original_form": "ㅇㅕ긴 경남거제입니당",
          "speaker_id": "2",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100029497.1.10",
          "form": "부러워요 ㅋㅋ 맑은하늘~~저도보고퐈요~~~",
          "original_form": "부러워요 ㅋㅋ 맑은하늘~~저도보고퐈요~~~",
          "speaker_id": "2",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029497.1.11",
          "form": "우와 거제요?? 거기 진짜 가보고싶어요~!",
          "original_form": "우와 거제요?? 거기 진짜 가보고싶어요~!",
          "speaker_id": "1",
          "time": "20210903 11:18"
        },
        {
          "id": "MDRW2100029497.1.12",
          "form": "놀러오세요~~~ㅎㅎㅎ",
          "original_form": "놀러오세요~~~ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 11:18"
        },
        {
          "id": "MDRW2100029497.1.13",
          "form": "남부면 더 따뜻할것같은데",
          "original_form": "남부면 더 따뜻할것같은데",
          "speaker_id": "1",
          "time": "20210903 11:18"
        },
        {
          "id": "MDRW2100029497.1.14",
          "form": "날씨좋을때요 ㅋㅋ",
          "original_form": "날씨좋을때요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:18"
        },
        {
          "id": "MDRW2100029497.1.15",
          "form": "계속살면 몸이 적응되니 똑같아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "계속살면 몸이 적응되니 똑같아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:18"
        },
        {
          "id": "MDRW2100029497.1.16",
          "form": "여긴 게다가 바닷바람이부니까.",
          "original_form": "여긴 게다가 바닷바람이부니까.",
          "speaker_id": "2",
          "time": "20210903 11:18"
        },
        {
          "id": "MDRW2100029497.1.17",
          "form": "오 맞아요 저도 제주도살 때 그랬어요",
          "original_form": "오 맞아요 저도 제주도살 때 그랬어요",
          "speaker_id": "1",
          "time": "20210903 11:57"
        },
        {
          "id": "MDRW2100029497.1.18",
          "form": "덥긴했는데 바닷바람불도 ㅋㅋㅋ",
          "original_form": "덥긴했는데 바닷바람불도 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:57"
        },
        {
          "id": "MDRW2100029497.1.19",
          "form": "거기 더위에적응돼서 이제 더위 많이 안타요",
          "original_form": "거기 더위에적응돼서 이제 더위 많이 안타요",
          "speaker_id": "1",
          "time": "20210903 11:57"
        },
        {
          "id": "MDRW2100029497.1.20",
          "form": "우와 제주도 사셨어요? 제주도 넘가고파요~~",
          "original_form": "우와 제주도 사셨어요? 제주도 넘가고파요~~",
          "speaker_id": "2",
          "time": "20210903 11:58"
        },
        {
          "id": "MDRW2100029497.1.21",
          "form": "몸이 날씨에 적응하는건 신기하네요 ㅋㅋ",
          "original_form": "몸이 날씨에 적응하는건 신기하네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:58"
        },
        {
          "id": "MDRW2100029497.1.22",
          "form": "지역적 날씨인가요 ㅎㅎ;",
          "original_form": "지역적 날씨인가요 ㅎㅎ;",
          "speaker_id": "2",
          "time": "20210903 11:58"
        },
        {
          "id": "MDRW2100029497.1.23",
          "form": "그러니까요ㅋㅋㅋ오ㅐ 그 아프리카사람들은 우리나라오면 춥다잖아요ㅋㅋㅋㅋ",
          "original_form": "그러니까요ㅋㅋㅋ오ㅐ 그 아프리카사람들은 우리나라오면 춥다잖아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 12:10"
        },
        {
          "id": "MDRW2100029497.1.24",
          "form": "신기하더라고요",
          "original_form": "신기하더라고요",
          "speaker_id": "1",
          "time": "20210903 12:10"
        },
        {
          "id": "MDRW2100029497.1.25",
          "form": "ㅋㅋ그렇군요 아프리카사람들은 우리나라가 춥군요...",
          "original_form": "ㅋㅋ그렇군요 아프리카사람들은 우리나라가 춥군요...",
          "speaker_id": "2",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100029497.1.26",
          "form": "전첨알았어요 ㅋㅋ",
          "original_form": "전첨알았어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100029497.1.27",
          "form": "오늘도 여전히  약간은 흐린거같지만 날좋네요~~",
          "original_form": "오늘도 여전히  약간은 흐린거같지만 날좋네요~~",
          "speaker_id": "2",
          "time": "20210904 12:23"
        },
        {
          "id": "MDRW2100029497.1.28",
          "form": "그러게요~ 저는 이제 수면바지입고자요",
          "original_form": "그러게요~ 저는 이제 수면바지입고자요",
