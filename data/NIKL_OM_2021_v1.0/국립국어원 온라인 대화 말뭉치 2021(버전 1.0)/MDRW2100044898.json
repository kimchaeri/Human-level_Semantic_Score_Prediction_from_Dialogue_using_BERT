      "utterance": [
        {
          "id": "MDRW2100044898.1.1",
          "form": "초3정도되면 어느정도 앞가림은 해여 ㅎㅎ",
          "original_form": "초3정도되면 어느정도 앞가림은 해여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044898.1.2",
          "form": "큰 아이가 초3인가요?",
          "original_form": "큰 아이가 초3인가요?",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044898.1.3",
          "form": "네 초3요",
          "original_form": "네 초3요",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044898.1.4",
          "form": "다 키우셨네요~ 부러워요",
          "original_form": "다 키우셨네요~ 부러워요",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044898.1.5",
          "form": "둘쨰가 남았잖아여ㅠㅠㅠㅠ",
          "original_form": "둘쨰가 남았잖아여ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044898.1.6",
          "form": "3살여ㅠㅠㅠㅠ",
          "original_form": "3살여ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044898.1.7",
          "form": "악!!! 어쩌다 ㅠ",
          "original_form": "악!!! 어쩌다 ㅠ",
          "speaker_id": "2",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044898.1.8",
          "form": "한명은 외로울까봐 뒤늦게ㅠㅠㅠ",
          "original_form": "한명은 외로울까봐 뒤늦게ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044898.1.9",
          "form": "맞아요 제가 동생이랑 6살 차이인데 어렷을땐 잘 놀았어요",
          "original_form": "맞아요 제가 동생이랑 6살 차이인데 어렷을땐 잘 놀았어요",
          "speaker_id": "2",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044898.1.10",
          "form": "7살이라 근데 좀 차이지긴한데ㅠㅠ 지금은 좀 되요 그래도 좀 컸다고",
          "original_form": "7살이라 근데 좀 차이지긴한데ㅠㅠ 지금은 좀 되요 그래도 좀 컸다고",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044898.1.11",
          "form": "그래도 다 크면 별 차이 아니던데요",
          "original_form": "그래도 다 크면 별 차이 아니던데요",
          "speaker_id": "2",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044898.1.12",
          "form": "맞아요 !  애기때는 헉 싶더니 그래도 좀 크니 놀아주고 하니 보기는 좋더라구요",
          "original_form": "맞아요 !  애기때는 헉 싶더니 그래도 좀 크니 놀아주고 하니 보기는 좋더라구요",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044898.1.13",
          "form": "큰애가 귀여워하지 않나요?",
          "original_form": "큰애가 귀여워하지 않나요?",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044898.1.14",
          "form": "근데 한창 친구들 하고 놀 나이라 ㅎㅎ",
          "original_form": "근데 한창 친구들 하고 놀 나이라 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044898.1.15",
          "form": "같이있음 귀여워하긴하는데 잘 안 놀아줘요",
          "original_form": "같이있음 귀여워하긴하는데 잘 안 놀아줘요",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044898.1.16",
          "form": "3살이랑 놀기엔 너무 재미가 없을거예요",
          "original_form": "3살이랑 놀기엔 너무 재미가 없을거예요",
          "speaker_id": "2",
          "time": "20210908 00:18"
        }
      ]
