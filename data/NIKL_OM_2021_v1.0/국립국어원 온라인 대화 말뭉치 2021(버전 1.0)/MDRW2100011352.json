      "utterance": [
        {
          "id": "MDRW2100011352.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 11:31"
        },
        {
          "id": "MDRW2100011352.1.2",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "2",
          "time": "20210824 11:32"
        },
        {
          "id": "MDRW2100011352.1.3",
          "form": "유튜브 자주보시나요?",
          "original_form": "유튜브 자주보시나요?",
          "speaker_id": "1",
          "time": "20210824 11:34"
        },
        {
          "id": "MDRW2100011352.1.4",
          "form": "네 자주 들어가죠ㅋㅋ",
          "original_form": "네 자주 들어가죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:53"
        },
        {
          "id": "MDRW2100011352.1.5",
          "form": "요즘 여행 유튜브 많이 봅니다.",
          "original_form": "요즘 여행 유튜브 많이 봅니다.",
          "speaker_id": "2",
          "time": "20210824 11:53"
        },
        {
          "id": "MDRW2100011352.1.6",
          "form": "오 누구좋아하시나요!",
          "original_form": "오 누구좋아하시나요!",
          "speaker_id": "1",
          "time": "20210824 11:54"
        },
        {
          "id": "MDRW2100011352.1.7",
          "form": "저는 빠니보틀",
          "original_form": "저는 빠니보틀",
          "speaker_id": "1",
          "time": "20210824 11:54"
        },
        {
          "id": "MDRW2100011352.1.8",
          "form": "앗 처음 들어보네요",
          "original_form": "앗 처음 들어보네요",
          "speaker_id": "2",
          "time": "20210824 11:56"
        },
        {
          "id": "MDRW2100011352.1.9",
          "form": "전 애청은 없고요",
          "original_form": "전 애청은 없고요",
          "speaker_id": "2",
          "time": "20210824 11:56"
        },
        {
          "id": "MDRW2100011352.1.10",
          "form": "쯔양 섬에서 산거 좋아해요ㅋㅋ",
          "original_form": "쯔양 섬에서 산거 좋아해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:56"
        },
        {
          "id": "MDRW2100011352.1.11",
          "form": "본거 또보고",
          "original_form": "본거 또보고",
          "speaker_id": "2",
          "time": "20210824 11:57"
        },
        {
          "id": "MDRW2100011352.1.12",
          "form": "오 그 욕지도 아닌가요",
          "original_form": "오 그 욕지도 아닌가요",
          "speaker_id": "1",
          "time": "20210824 11:57"
        },
        {
          "id": "MDRW2100011352.1.13",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:58"
        },
        {
          "id": "MDRW2100011352.1.14",
          "form": "그런 느낌 너무 좋아요",
          "original_form": "그런 느낌 너무 좋아요",
          "speaker_id": "2",
          "time": "20210824 11:58"
        },
        {
          "id": "MDRW2100011352.1.15",
          "form": "헐 빠니 보틀은 실시간 여행중이군요,",
          "original_form": "헐 빠니 보틀은 실시간 여행중이군요,",
          "speaker_id": "2",
          "time": "20210824 11:59"
        },
        {
          "id": "MDRW2100011352.1.16",
          "form": "네 지금 미국여행 중이에요ㅋㅋㅋ",
          "original_form": "네 지금 미국여행 중이에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:59"
        },
        {
          "id": "MDRW2100011352.1.17",
          "form": "ㅋㅋ 대단한 유튜버네요",
          "original_form": "ㅋㅋ 대단한 유튜버네요",
          "speaker_id": "2",
          "time": "20210824 11:59"
        },
        {
          "id": "MDRW2100011352.1.18",
          "form": "이시국에 참 대단해요ㅎㅎ",
          "original_form": "이시국에 참 대단해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 12:01"
        },
        {
          "id": "MDRW2100011352.1.19",
          "form": "영상미도 좋네요",
          "original_form": "영상미도 좋네요",
          "speaker_id": "2",
          "time": "20210824 12:08"
        },
        {
          "id": "MDRW2100011352.1.20",
          "form": "능력자시다",
          "original_form": "능력자시다",
          "speaker_id": "2",
          "time": "20210824 12:08"
        },
        {
          "id": "MDRW2100011352.1.21",
          "form": "좋좋소라는 드라마 아시나요",
          "original_form": "좋좋소라는 드라마 아시나요",
          "speaker_id": "1",
          "time": "20210824 12:21"
        },
        {
          "id": "MDRW2100011352.1.22",
          "form": "그거 감독도 했어요ㄷㄷ",
          "original_form": "그거 감독도 했어요ㄷㄷ",
          "speaker_id": "1",
          "time": "20210824 12:22"
        }
      ]
