      "utterance": [
        {
          "id": "MDRW2100019566.1.1",
          "form": "갑자기?",
          "original_form": "갑자기?",
          "speaker_id": "2",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100019566.1.2",
          "form": "전부터 로망이 있었어",
          "original_form": "전부터 로망이 있었어",
          "speaker_id": "1",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100019566.1.3",
          "form": "되게 힘들거 같은데",
          "original_form": "되게 힘들거 같은데",
          "speaker_id": "2",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100019566.1.4",
          "form": "막 모래언덕에서 썰매타던뎈ㅋㅋ",
          "original_form": "막 모래언덕에서 썰매타던뎈ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:22"
        },
        {
          "id": "MDRW2100019566.1.5",
          "form": "ㅋㅋㅋㅋㅋ 내려오다 살이라도 닿으면 화상입을 듯",
          "original_form": "ㅋㅋㅋㅋㅋ 내려오다 살이라도 닿으면 화상입을 듯",
          "speaker_id": "2",
          "time": "20210827 20:22"
        },
        {
          "id": "MDRW2100019566.1.6",
          "form": "뭐 깔고 타지ㅋㅋㅋ재밌어보이더라고",
          "original_form": "뭐 깔고 타지ㅋㅋㅋ재밌어보이더라고",
          "speaker_id": "1",
          "time": "20210827 20:22"
        },
        {
          "id": "MDRW2100019566.1.7",
          "form": "내려오다 옆으로 쓰러지면 ㅋㅋ",
          "original_form": "내려오다 옆으로 쓰러지면 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:23"
        },
        {
          "id": "MDRW2100019566.1.8",
          "form": "쳐박히는 거지ㅋㅋ",
          "original_form": "쳐박히는 거지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:23"
        },
        {
          "id": "MDRW2100019566.1.9",
          "form": "못 찾는거 아냐?ㅋㅋ",
          "original_form": "못 찾는거 아냐?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:23"
        },
        {
          "id": "MDRW2100019566.1.10",
          "form": "ㅋㅋㅋㅋ무슨 늪도 아니고",
          "original_form": "ㅋㅋㅋㅋ무슨 늪도 아니고",
          "speaker_id": "1",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019566.1.11",
          "form": "근데 워낙 사막이 크니까",
          "original_form": "근데 워낙 사막이 크니까",
          "speaker_id": "2",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019566.1.12",
          "form": "크긴하지 사실 무서울 거 같기도 해",
          "original_form": "크긴하지 사실 무서울 거 같기도 해",
          "speaker_id": "1",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019566.1.13",
          "form": "그니까 없어지면 ㄷㄷ",
          "original_form": "그니까 없어지면 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210827 20:25"
        },
        {
          "id": "MDRW2100019566.1.14",
          "form": "모래폭풍 같은 거 오면 눈 없어질 거 같아ㅋㅋㅋ",
          "original_form": "모래폭풍 같은 거 오면 눈 없어질 거 같아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:25"
        },
        {
          "id": "MDRW2100019566.1.15",
          "form": "가보고 싶은 곳 없어?",
          "original_form": "가보고 싶은 곳 없어?",
          "speaker_id": "1",
