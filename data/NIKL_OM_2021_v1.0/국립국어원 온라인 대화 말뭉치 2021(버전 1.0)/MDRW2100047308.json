      "utterance": [
        {
          "id": "MDRW2100047308.1.1",
          "form": "네 미친거죠",
          "original_form": "네 미친거죠",
          "speaker_id": "2",
          "time": "20210826 10:37"
        },
        {
          "id": "MDRW2100047308.1.2",
          "form": "아 먹고싶다",
          "original_form": "아 먹고싶다",
          "speaker_id": "2",
          "time": "20210826 10:37"
        },
        {
          "id": "MDRW2100047308.1.3",
          "form": "돈주고 치킨 사먹어야죠 뭐",
          "original_form": "돈주고 치킨 사먹어야죠 뭐",
          "speaker_id": "2",
          "time": "20210826 10:37"
        },
        {
          "id": "MDRW2100047308.1.4",
          "form": "그건 진짜",
          "original_form": "그건 진짜",
          "speaker_id": "1",
          "time": "20210826 10:38"
        },
        {
          "id": "MDRW2100047308.1.5",
          "form": "며칠 잠안올것같아요",
          "original_form": "며칠 잠안올것같아요",
          "speaker_id": "1",
          "time": "20210826 10:38"
        },
        {
          "id": "MDRW2100047308.1.6",
          "form": "아 오늘 치킨 사먹어야겠네요",
          "original_form": "아 오늘 치킨 사먹어야겠네요",
          "speaker_id": "2",
          "time": "20210826 10:39"
        },
        {
          "id": "MDRW2100047308.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 10:39"
        },
        {
          "id": "MDRW2100047308.1.8",
          "form": "눈에 치킨이 왔다갔다 ㅋㅋ",
          "original_form": "눈에 치킨이 왔다갔다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:39"
        },
        {
          "id": "MDRW2100047308.1.9",
          "form": "ㅎㅎ 오늘 저녁메뉴는 치킨인건가요?ㅎ",
          "original_form": "ㅎㅎ 오늘 저녁메뉴는 치킨인건가요?ㅎ",
          "speaker_id": "1",
          "time": "20210826 10:39"
        },
        {
          "id": "MDRW2100047308.1.10",
          "form": "자나깨나 쿠폰조심입니다",
          "original_form": "자나깨나 쿠폰조심입니다",
          "speaker_id": "2",
          "time": "20210826 10:39"
        },
        {
          "id": "MDRW2100047308.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 10:39"
        },
        {
          "id": "MDRW2100047308.1.12",
          "form": "그럴래요 ㅋㅋ",
          "original_form": "그럴래요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:39"
        },
        {
          "id": "MDRW2100047308.1.13",
          "form": "치킨 언제나 옳죠",
          "original_form": "치킨 언제나 옳죠",
          "speaker_id": "2",
          "time": "20210826 10:39"
        },
        {
          "id": "MDRW2100047308.1.14",
          "form": "치킨은 사랑이죵",
          "original_form": "치킨은 사랑이죵",
          "speaker_id": "1",
          "time": "20210826 10:40"
        },
        {
          "id": "MDRW2100047308.1.15",
          "form": "아 햄버거도 먹어야 하는데 ㅋㅋ",
          "original_form": "아 햄버거도 먹어야 하는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:40"
        },
        {
          "id": "MDRW2100047308.1.16",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 10:40"
        },
        {
          "id": "MDRW2100047308.1.17",
          "form": "깁콘으로요?",
          "original_form": "깁콘으로요?",
          "speaker_id": "1",
          "time": "20210826 10:40"
        },
        {
          "id": "MDRW2100047308.1.18",
          "form": "오늘은 간장 후라이드 두개 먹고싶네요",
          "original_form": "오늘은 간장 후라이드 두개 먹고싶네요",
          "speaker_id": "2",
          "time": "20210826 10:40"
        },
        {
          "id": "MDRW2100047308.1.19",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 10:40"
        },
        {
          "id": "MDRW2100047308.1.20",
          "form": "드셔야할것이 많으시네요 ㅎ",
          "original_form": "드셔야할것이 많으시네요 ㅎ",
          "speaker_id": "1",
          "time": "20210826 10:40"
        },
        {
          "id": "MDRW2100047308.1.21",
          "form": "저번에 세일해서 샀어요",
          "original_form": "저번에 세일해서 샀어요",
          "speaker_id": "2",
          "time": "20210826 10:40"
        },
        {
          "id": "MDRW2100047308.1.22",
          "form": "버거킹요",
          "original_form": "버거킹요",
          "speaker_id": "2",
          "time": "20210826 10:40"
        },
        {
          "id": "MDRW2100047308.1.23",
          "form": "아~~~~간장 후라이드 넘 맛있겠어요",
          "original_form": "아~~~~간장 후라이드 넘 맛있겠어요",
          "speaker_id": "1",
          "time": "20210826 10:40"
        },
        {
          "id": "MDRW2100047308.1.24",
          "form": "햄버거 맛나요 ㅎㅎ",
          "original_form": "햄버거 맛나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 10:41"
        },
        {
          "id": "MDRW2100047308.1.25",
          "form": "내 ㅎㅎ",
          "original_form": "내 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 10:41"
        },
        {
          "id": "MDRW2100047308.1.26",
          "form": "햄버거는 버거킹이 짱 ㅎ",
          "original_form": "햄버거는 버거킹이 짱 ㅎ",
          "speaker_id": "1",
          "time": "20210826 10:41"
        },
        {
          "id": "MDRW2100047308.1.27",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 10:41"
        },
        {
          "id": "MDRW2100047308.1.28",
          "form": "근데 평상시 넘비싸요",
          "original_form": "근데 평상시 넘비싸요",
          "speaker_id": "2",
          "time": "20210826 10:41"
        },
        {
          "id": "MDRW2100047308.1.29",
          "form": "세일하는거 빼고",
          "original_form": "세일하는거 빼고",
          "speaker_id": "2",
          "time": "20210826 10:41"
        },
        {
          "id": "MDRW2100047308.1.30",
          "form": "맞아요 ㅜㅠ\r\n햄버거세트가 만원가까이 하죠?",
          "original_form": "맞아요 ㅜㅠ\r\n햄버거세트가 만원가까이 하죠?",
          "speaker_id": "1",
          "time": "20210826 10:41"
        },
        {
          "id": "MDRW2100047308.1.31",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210826 10:41"
        },
        {
          "id": "MDRW2100047308.1.32",
          "form": "이건뭐",
