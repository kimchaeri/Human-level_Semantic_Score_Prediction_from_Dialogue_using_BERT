      "utterance": [
        {
          "id": "MDRW2100043827.1.1",
          "form": "손목은 좀 어때?",
          "original_form": "손목은 좀 어때?",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043827.1.2",
          "form": "요샌 좀 낫네",
          "original_form": "요샌 좀 낫네",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043827.1.3",
          "form": "다행이네",
          "original_form": "다행이네",
          "speaker_id": "1",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043827.1.4",
          "form": "학기 시작했는데 아프면 큰일",
          "original_form": "학기 시작했는데 아프면 큰일",
          "speaker_id": "1",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100043827.1.5",
          "form": "손목 건강 조심혀~",
          "original_form": "손목 건강 조심혀~",
          "speaker_id": "1",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100043827.1.6",
          "form": "챙겨줘서 고마우이",
          "original_form": "챙겨줘서 고마우이",
          "speaker_id": "2",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043827.1.7",
          "form": "그럼 같이 늙어가는 처지에",
          "original_form": "그럼 같이 늙어가는 처지에",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043827.1.8",
          "form": "우리 서로 건강을 챙겨줘야지",
          "original_form": "우리 서로 건강을 챙겨줘야지",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043827.1.9",
          "form": "아빠 피부를 위해 팩 붙여드리고 와야지",
          "original_form": "아빠 피부를 위해 팩 붙여드리고 와야지",
          "speaker_id": "1",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043827.1.10",
          "form": "잘했내",
          "original_form": "잘했내",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043827.1.11",
          "form": "오래오래 서로 건강챙겨주자",
          "original_form": "오래오래 서로 건강챙겨주자",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043827.1.12",
          "form": "그래 서로의 건강챙김이!",
          "original_form": "그래 서로의 건강챙김이!",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043827.1.13",
          "form": "너는 어디 불편한덴 없고?",
          "original_form": "너는 어디 불편한덴 없고?",
          "speaker_id": "2",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100043827.1.14",
          "form": "응응 몸은 건강합니다~",
          "original_form": "응응 몸은 건강합니다~",
          "speaker_id": "1",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100043827.1.15",
          "form": "피곤해서 그렇지ㅋㅋ",
          "original_form": "피곤해서 그렇지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100043827.1.16",
          "form": "좋네 역시 꾸준히 운동해서 그런가",
          "original_form": "좋네 역시 꾸준히 운동해서 그런가",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043827.1.17",
          "form": "필라테스는 언제 다시가?",
          "original_form": "필라테스는 언제 다시가?",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100043827.1.18",
          "form": "다음주부터 다시 운동 시작~",
          "original_form": "다음주부터 다시 운동 시작~",
          "speaker_id": "1",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100043827.1.19",
          "form": "아하 월욜부터?",
          "original_form": "아하 월욜부터?",
          "speaker_id": "2",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100043827.1.20",
          "form": "응 월욜에 예약해야지ㅋㅋ",
          "original_form": "응 월욜에 예약해야지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100043827.1.21",
          "form": "일단 예약된 건 수욜 개인레슨",
          "original_form": "일단 예약된 건 수욜 개인레슨",
          "speaker_id": "1",
          "time": "20210907 21:05"
        }
