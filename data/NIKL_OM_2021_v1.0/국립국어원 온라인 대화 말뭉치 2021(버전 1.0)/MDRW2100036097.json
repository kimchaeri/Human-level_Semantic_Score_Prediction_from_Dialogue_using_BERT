      "utterance": [
        {
          "id": "MDRW2100036097.1.1",
          "form": "치킨은 어디꺼 드시나여?",
          "original_form": "치킨은 어디꺼 드시나여?",
          "speaker_id": "2",
          "time": "20210830 16:17"
        },
        {
          "id": "MDRW2100036097.1.2",
          "form": "그때그때 할인하는거 위주로 먹는데",
          "original_form": "그때그때 할인하는거 위주로 먹는데",
          "speaker_id": "1",
          "time": "20210830 16:18"
        },
        {
          "id": "MDRW2100036097.1.3",
          "form": "안가리고 먹어용ㅎㅎ",
          "original_form": "안가리고 먹어용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 16:18"
        },
        {
          "id": "MDRW2100036097.1.4",
          "form": "오 저도 안가리고 먹어요 ㅋㅋㅋㅋ",
          "original_form": "오 저도 안가리고 먹어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 16:23"
        },
        {
          "id": "MDRW2100036097.1.5",
          "form": "교촌에선 머드세여?",
          "original_form": "교촌에선 머드세여?",
          "speaker_id": "2",
          "time": "20210830 16:23"
        },
        {
          "id": "MDRW2100036097.1.6",
          "form": "저는 오리지널에 웨지감자 꼭 같이 시켜먹어용",
          "original_form": "저는 오리지널에 웨지감자 꼭 같이 시켜먹어용",
          "speaker_id": "1",
          "time": "20210830 16:24"
        },
        {
          "id": "MDRW2100036097.1.7",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210830 16:24"
        },
        {
          "id": "MDRW2100036097.1.8",
          "form": "오오 맛있죠 저는 레드 조아해서 반반 먹어요 ㅋㅋㅋ웨지감사 가끔시키고요",
          "original_form": "오오 맛있죠 저는 레드 조아해서 반반 먹어요 ㅋㅋㅋ웨지감사 가끔시키고요",
          "speaker_id": "2",
          "time": "20210830 16:26"
        },
        {
          "id": "MDRW2100036097.1.9",
          "form": "레드랑 허니랑 강경파가 많으시더라구요ㅋㅋㅋ",
          "original_form": "레드랑 허니랑 강경파가 많으시더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:26"
        },
        {
          "id": "MDRW2100036097.1.10",
          "form": "ㅋㅋㅋㅋㅋ 맞아요 전 다조아해요..그때그때 땡기는것을 먹는편..",
          "original_form": "ㅋㅋㅋㅋㅋ 맞아요 전 다조아해요..그때그때 땡기는것을 먹는편..",
          "speaker_id": "2",
          "time": "20210830 16:27"
        },
        {
          "id": "MDRW2100036097.1.11",
          "form": "교촌도 이것저것 많이하더라구요 꽈배기도 한번 먹은적있어욬ㅋㅋ",
          "original_form": "교촌도 이것저것 많이하더라구요 꽈배기도 한번 먹은적있어욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 16:28"
        },
        {
          "id": "MDRW2100036097.1.12",
          "form": "그 무슨 스파클링이었나",
          "original_form": "그 무슨 스파클링이었나",
          "speaker_id": "1",
          "time": "20210830 16:29"
        },
        {
          "id": "MDRW2100036097.1.13",
          "form": "콜라대신 주는거",
          "original_form": "콜라대신 주는거",
          "speaker_id": "1",
          "time": "20210830 16:29"
        },
        {
          "id": "MDRW2100036097.1.14",
          "form": "그거 좀 없애버렸으면 좋겠어요ㅋㅋ",
          "original_form": "그거 좀 없애버렸으면 좋겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:29"
        },
        {
          "id": "MDRW2100036097.1.15",
          "form": "ㅋㅋㅋㅋㅋ맞아요 누가먹어진짜..",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 누가먹어진짜..",
          "speaker_id": "2",
          "time": "20210830 16:30"
        },
        {
          "id": "MDRW2100036097.1.16",
          "form": "요즘 또 바뀌었다는 글을 본거같은데..",
          "original_form": "요즘 또 바뀌었다는 글을 본거같은데..",
          "speaker_id": "2",
          "time": "20210830 16:30"
        },
        {
          "id": "MDRW2100036097.1.17",
          "form": "맨날 그거 안먹고 버립니다...ㅠㅠ",
          "original_form": "맨날 그거 안먹고 버립니다...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 16:30"
        },
        {
          "id": "MDRW2100036097.1.18",
          "form": "아 그냥 콜라로 주지 뭘 또 바꾸는걸까요...?ㅋㅋㅋㅋ",
          "original_form": "아 그냥 콜라로 주지 뭘 또 바꾸는걸까요...?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:30"
        },
        {
          "id": "MDRW2100036097.1.19",
          "form": "그러개 말입니닼ㅋㅋㅋㅋ 콜라나주지..",
          "original_form": "그러개 말입니닼ㅋㅋㅋㅋ 콜라나주지..",
          "speaker_id": "2",
          "time": "20210830 16:31"
        },
        {
          "id": "MDRW2100036097.1.20",
          "form": "얼마전에 멕시카나에서 후라이드를 시켜먹었는데",
          "original_form": "얼마전에 멕시카나에서 후라이드를 시켜먹었는데",
          "speaker_id": "1",
          "time": "20210830 16:31"
        },
        {
          "id": "MDRW2100036097.1.21",
          "form": "정말 의외로 맛있었어요",
          "original_form": "정말 의외로 맛있었어요",
          "speaker_id": "1",
          "time": "20210830 16:31"
        },
        {
          "id": "MDRW2100036097.1.22",
          "form": "멕시카나 맛있어요 ㅠㅠ양념도 좋구..",
          "original_form": "멕시카나 맛있어요 ㅠㅠ양념도 좋구..",
          "speaker_id": "2",
          "time": "20210830 16:32"
        },
        {
          "id": "MDRW2100036097.1.23",
          "form": "멕시카나 하면 양념이라 후라이드는 신선하긴하네옄ㅋㅋㅋ",
          "original_form": "멕시카나 하면 양념이라 후라이드는 신선하긴하네옄ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 16:32"
        },
        {
          "id": "MDRW2100036097.1.24",
          "form": "진짜 bbq랑 거의 비슷할정도 맛있어서 깜짝 놀랬네요ㅋㅋㅋㅋㅋ",
          "original_form": "진짜 bbq랑 거의 비슷할정도 맛있어서 깜짝 놀랬네요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
