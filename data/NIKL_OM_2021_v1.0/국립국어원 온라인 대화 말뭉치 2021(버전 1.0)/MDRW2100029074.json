        {
          "id": "MDRW2100029074.1.1",
          "form": "만화카페에서 파는",
          "original_form": "만화카페에서 파는",
          "speaker_id": "1",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100029074.1.2",
          "form": "밥 막걸리 먹어봤음?",
          "original_form": "밥 막걸리 먹어봤음?",
          "speaker_id": "2",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100029074.1.3",
          "form": "쥐포 구이 먹고싶다",
          "original_form": "쥐포 구이 먹고싶다",
          "speaker_id": "1",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100029074.1.4",
          "form": "안먹어봄",
          "original_form": "안먹어봄",
          "speaker_id": "1",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100029074.1.5",
          "form": "궁금",
          "original_form": "궁금",
          "speaker_id": "1",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100029074.1.6",
          "form": "마나 카페에서 파는 라면 먹고 싶음",
          "original_form": "마나 카페에서 파는 라면 먹고 싶음",
          "speaker_id": "2",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100029074.1.7",
          "form": "짜장면도 시켜 먹으면 맛있음",
          "original_form": "짜장면도 시켜 먹으면 맛있음",
          "speaker_id": "2",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100029074.1.8",
          "form": "당구장자장면 진리",
          "original_form": "당구장자장면 진리",
          "speaker_id": "1",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100029074.1.9",
          "form": "당구장에서 먹으면 뭐든 맛있음",
          "original_form": "당구장에서 먹으면 뭐든 맛있음",
          "speaker_id": "2",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100029074.1.10",
          "form": "피씨방은 레스도랑 겸업임",
          "original_form": "피씨방은 레스도랑 겸업임",
          "speaker_id": "1",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100029074.1.11",
          "form": "음료도맛남",
          "original_form": "음료도맛남",
          "speaker_id": "1",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100029074.1.12",
          "form": "커피 에이드 탄산음료",
          "original_form": "커피 에이드 탄산음료",
          "speaker_id": "1",
          "time": "20210902 23:34"
        },
        {
