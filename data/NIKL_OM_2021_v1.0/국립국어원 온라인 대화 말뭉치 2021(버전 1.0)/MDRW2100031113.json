      "utterance": [
        {
          "id": "MDRW2100031113.1.1",
          "form": "즐겨하시는 게임이 뭔가요?",
          "original_form": "즐겨하시는 게임이 뭔가요?",
          "speaker_id": "2",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100031113.1.2",
          "form": "저도 테런 좋아하는데",
          "original_form": "저도 테런 좋아하는데",
          "speaker_id": "2",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100031113.1.3",
          "form": "네 저는 이벤트맵만 주로해서",
          "original_form": "네 저는 이벤트맵만 주로해서",
          "speaker_id": "2",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100031113.1.4",
          "form": "어떤맵 좋아하시나요",
          "original_form": "어떤맵 좋아하시나요",
          "speaker_id": "2",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100031113.1.5",
          "form": "아 그 맵이름이 머였지",
          "original_form": "아 그 맵이름이 머였지",
          "speaker_id": "2",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100031113.1.6",
          "form": "금뚝은뚝?이었나",
          "original_form": "금뚝은뚝?이었나",
          "speaker_id": "2",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100031113.1.7",
          "form": "아! 달밤에 춤추듯이요",
          "original_form": "아! 달밤에 춤추듯이요",
          "speaker_id": "2",
          "time": "20210904 17:00"
        },
        {
          "id": "MDRW2100031113.1.8",
          "form": "그거 진짜 재밌죠 ㅎㅎ",
          "original_form": "그거 진짜 재밌죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 17:00"
        },
        {
          "id": "MDRW2100031113.1.9",
          "form": "맞아요 때리는맛이 좋아요",
          "original_form": "맞아요 때리는맛이 좋아요",
          "speaker_id": "1",
          "time": "20210904 17:01"
        },
        {
          "id": "MDRW2100031113.1.10",
          "form": "저도 그거 한창 했어슨데",
          "original_form": "저도 그거 한창 했어슨데",
          "speaker_id": "2",
          "time": "20210904 17:01"
        },
        {
          "id": "MDRW2100031113.1.11",
          "form": "오 같이해보고 싶네요",
          "original_form": "오 같이해보고 싶네요",
          "speaker_id": "1",
          "time": "20210904 17:01"
        },
        {
          "id": "MDRW2100031113.1.12",
          "form": "유령망치 걸리면 망해요",
          "original_form": "유령망치 걸리면 망해요",
          "speaker_id": "2",
          "time": "20210904 17:02"
        },
        {
          "id": "MDRW2100031113.1.13",
          "form": "그건 진짜 존재감 유령같죠",
          "original_form": "그건 진짜 존재감 유령같죠",
          "speaker_id": "1",
          "time": "20210904 17:02"
        },
        {
          "id": "MDRW2100031113.1.14",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 17:02"
        },
        {
          "id": "MDRW2100031113.1.15",
          "form": "은망치가 제일 좋고",
          "original_form": "은망치가 제일 좋고",
          "speaker_id": "1",
          "time": "20210904 17:05"
        },
        {
          "id": "MDRW2100031113.1.16",
          "form": "은망치로 다 동물 변하게하면 쾌감...",
          "original_form": "은망치로 다 동물 변하게하면 쾌감...",
          "speaker_id": "2",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100031113.1.17",
          "form": "너무 재밌어요 ㅋㅋㅋ",
          "original_form": "너무 재밌어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:08"
        },
        {
          "id": "MDRW2100031113.1.18",
          "form": "아 갑자기 또 하고싶네요",
          "original_form": "아 갑자기 또 하고싶네요",
          "speaker_id": "2",
          "time": "20210904 17:08"
        },
        {
          "id": "MDRW2100031113.1.19",
          "form": "저도요 얘기하다보니 하고싶어졌어요",
          "original_form": "저도요 얘기하다보니 하고싶어졌어요",
          "speaker_id": "1",
          "time": "20210904 17:08"
        },
        {
          "id": "MDRW2100031113.1.20",
          "form": "나중에 오랜만에 접속해야겠어요",
          "original_form": "나중에 오랜만에 접속해야겠어요",
          "speaker_id": "2",
          "time": "20210904 17:09"
        },
        {
          "id": "MDRW2100031113.1.21",
          "form": "오랜만에 접속하시는 건가요?",
          "original_form": "오랜만에 접속하시는 건가요?",
          "speaker_id": "1",
          "time": "20210904 17:10"
        },
        {
          "id": "MDRW2100031113.1.22",
          "form": "오 한달 넘으셨으면 접속보상 받으실 수 있을텐데",
          "original_form": "오 한달 넘으셨으면 접속보상 받으실 수 있을텐데",
          "speaker_id": "1",
          "time": "20210904 17:10"
        }
      ]
