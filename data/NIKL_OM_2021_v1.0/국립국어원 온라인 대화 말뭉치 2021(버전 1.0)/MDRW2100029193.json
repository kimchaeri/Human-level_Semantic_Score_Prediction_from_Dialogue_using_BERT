      "utterance": [
        {
          "id": "MDRW2100029193.1.1",
          "form": "편지쓸때",
          "original_form": "편지쓸때",
          "speaker_id": "2",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100029193.1.2",
          "form": "군대라기보다는 모임이네요",
          "original_form": "군대라기보다는 모임이네요",
          "speaker_id": "1",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100029193.1.3",
          "form": "수신자부담 부담스러워서",
          "original_form": "수신자부담 부담스러워서",
          "speaker_id": "2",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100029193.1.4",
          "form": "통화할수있는",
          "original_form": "통화할수있는",
          "speaker_id": "2",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100029193.1.5",
          "form": "핸드폰 하니깐 편지 쓸 필요가 없네요",
          "original_form": "핸드폰 하니깐 편지 쓸 필요가 없네요",
          "speaker_id": "1",
          "time": "20210903 00:11"
        },
        {
          "id": "MDRW2100029193.1.6",
          "form": "공중전화 카드도 넣어주고",
          "original_form": "공중전화 카드도 넣어주고",
          "speaker_id": "2",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100029193.1.7",
          "form": "첫째때는말이야",
          "original_form": "첫째때는말이야",
          "speaker_id": "2",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100029193.1.8",
          "form": "그런 것들도 다 필요 없겠다",
          "original_form": "그런 것들도 다 필요 없겠다",
          "speaker_id": "1",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100029193.1.9",
          "form": "인터넷으로 검색해서",
          "original_form": "인터넷으로 검색해서",
          "speaker_id": "2",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100029193.1.10",
          "form": "글써서",
          "original_form": "글써서",
          "speaker_id": "2",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100029193.1.11",
          "form": "맞어 수치인 부담하는 통화 도 있었는데",
          "original_form": "맞어 수치인 부담하는 통화 도 있었는데",
          "speaker_id": "1",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100029193.1.12",
          "form": "거기서 뽑아서 읽었댔나",
          "original_form": "거기서 뽑아서 읽었댔나",
          "speaker_id": "2",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100029193.1.13",
          "form": "그랬는데",
          "original_form": "그랬는데",
          "speaker_id": "2",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100029193.1.14",
          "form": "둘째는 낮부터 저녁까지",
          "original_form": "둘째는 낮부터 저녁까지",
          "speaker_id": "2",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100029193.1.15",
          "form": "그런 것들은 다 없어졌겠네요",
          "original_form": "그런 것들은 다 없어졌겠네요",
          "speaker_id": "1",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100029193.1.16",
          "form": "카톡으로 연락 쌉가능",
          "original_form": "카톡으로 연락 쌉가능",
          "speaker_id": "2",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100029193.1.17",
          "form": "계속 통화해",
          "original_form": "계속 통화해",
          "speaker_id": "1",
          "time": "20210903 00:12"
        },
        {
          "id": "MDRW2100029193.1.18",
          "form": "야 집 일반인보다 더 핸드폰 하겠네요",
          "original_form": "야 집 일반인보다 더 핸드폰 하겠네요",
          "speaker_id": "1",
          "time": "20210903 00:13"
        },
        {
          "id": "MDRW2100029193.1.19",
          "form": "게임같은거 몇개는 제한된듯",
          "original_form": "게임같은거 몇개는 제한된듯",
          "speaker_id": "2",
          "time": "20210903 00:13"
        },
        {
          "id": "MDRW2100029193.1.20",
          "form": "ㅇㅇ겁나편하고",
          "original_form": "ㅇㅇ겁나편하고",
          "speaker_id": "2",
          "time": "20210903 00:13"
        }
