      "utterance": [
        {
          "id": "MDRW2100017454.1.1",
          "form": "가끔 시켜먹어요!",
          "original_form": "가끔 시켜먹어요!",
          "speaker_id": "2",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100017454.1.2",
          "form": "어떤거 시켜먹어요?",
          "original_form": "어떤거 시켜먹어요?",
          "speaker_id": "1",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100017454.1.3",
          "form": "떡볶이 많이 시켜먹는것같아요",
          "original_form": "떡볶이 많이 시켜먹는것같아요",
          "speaker_id": "2",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017454.1.4",
          "form": "떡볶이도 종류가 다양하던데 어떤거 좋아해요?",
          "original_form": "떡볶이도 종류가 다양하던데 어떤거 좋아해요?",
          "speaker_id": "1",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100017454.1.5",
          "form": "저 가래떡 떡볶이 좋아해요!",
          "original_form": "저 가래떡 떡볶이 좋아해요!",
          "speaker_id": "2",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100017454.1.6",
          "form": "쫄깃 해서 맛있죠 ㅜㅜ",
          "original_form": "쫄깃 해서 맛있죠 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100017454.1.7",
          "form": "카레, 짜장, 로제 등등 다양하던데 이 중에는요?",
          "original_form": "카레, 짜장, 로제 등등 다양하던데 이 중에는요?",
          "speaker_id": "1",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100017454.1.8",
          "form": "전 로제요!",
          "original_form": "전 로제요!",
          "speaker_id": "2",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100017454.1.9",
          "form": "로제소스 맛있죠ㅜㅜ 파스타도 맛있고..",
          "original_form": "로제소스 맛있죠ㅜㅜ 파스타도 맛있고..",
          "speaker_id": "1",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100017454.1.10",
          "form": "짜장 떡볶이는 어떻게 생각하세요?",
          "original_form": "짜장 떡볶이는 어떻게 생각하세요?",
          "speaker_id": "1",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100017454.1.11",
          "form": "짜장 떡볶이도 맛있죠",
          "original_form": "짜장 떡볶이도 맛있죠",
          "speaker_id": "2",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100017454.1.12",
          "form": "근데 많이 못먹어봐서",
          "original_form": "근데 많이 못먹어봐서",
          "speaker_id": "2",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100017454.1.13",
          "form": "또 아는 떡볶이 있어요?",
          "original_form": "또 아는 떡볶이 있어요?",
          "speaker_id": "1",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100017454.1.14",
          "form": "맛이 잘 기억이 안나요",
          "original_form": "맛이 잘 기억이 안나요",
          "speaker_id": "2",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100017454.1.15",
          "form": "음... 기름떡볶이..?",
          "original_form": "음... 기름떡볶이..?",
          "speaker_id": "2",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017454.1.16",
          "form": "아 ㅋㅋ 그 통인시장에 파는거요?",
          "original_form": "아 ㅋㅋ 그 통인시장에 파는거요?",
          "speaker_id": "1",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017454.1.17",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017454.1.18",
          "form": "먹어보고싶은데 기회가 없어서 궁금했거든요",
          "original_form": "먹어보고싶은데 기회가 없어서 궁금했거든요",
          "speaker_id": "2",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017454.1.19",
          "form": "저도 먹어본 적 없어서 궁금해요.",
          "original_form": "저도 먹어본 적 없어서 궁금해요.",
          "speaker_id": "1",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017454.1.20",
          "form": "떡국 떡으로 만든 떡볶이 어떻게 생각하세요?",
          "original_form": "떡국 떡으로 만든 떡볶이 어떻게 생각하세요?",
          "speaker_id": "1",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017454.1.21",
          "form": "그것도 맛있죠!",
          "original_form": "그것도 맛있죠!",
          "speaker_id": "2",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017454.1.22",
          "form": "그냥 거의 모든 떡볶이를 좋아하는것 같아요",
          "original_form": "그냥 거의 모든 떡볶이를 좋아하는것 같아요",
          "speaker_id": "2",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017454.1.23",
          "form": "학교 다닐 적에 먹던 라볶이가 제일 최고였어요..",
          "original_form": "학교 다닐 적에 먹던 라볶이가 제일 최고였어요..",
          "speaker_id": "1",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017454.1.24",
          "form": "마지막에 밥까지 볶아먹으면 캬 ㅠㅠ",
          "original_form": "마지막에 밥까지 볶아먹으면 캬 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017454.1.25",
          "form": "와... 최고죠",
          "original_form": "와... 최고죠",
          "speaker_id": "2",
          "time": "20210826 19:41"
        },
        {
          "id": "MDRW2100017454.1.26",
          "form": "떡볶이 맛있게 먹는 법 아세요?",
          "original_form": "떡볶이 맛있게 먹는 법 아세요?",
          "speaker_id": "1",
          "time": "20210826 19:42"
        },
        {
          "id": "MDRW2100017454.1.27",
          "form": "어... 아뇨.. 그냥 먹는것 같아요",
          "original_form": "어... 아뇨.. 그냥 먹는것 같아요",
          "speaker_id": "2",
          "time": "20210826 19:42"
        },
        {
          "id": "MDRW2100017454.1.28",
          "form": "사리 같은 건 안먹어요?ㅠ",
          "original_form": "사리 같은 건 안먹어요?ㅠ",
          "speaker_id": "1",
          "time": "20210826 19:42"
        },
        {
          "id": "MDRW2100017454.1.29",
          "form": "전 삶은계란이랑 만두 넣어 먹는 거 좋아해요 ㅜㅜ",
          "original_form": "전 삶은계란이랑 만두 넣어 먹는 거 좋아해요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017454.1.30",
          "form": "아하!",
          "original_form": "아하!",
          "speaker_id": "2",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017454.1.31",
          "form": "전 중당이요!",
          "original_form": "전 중당이요!",
          "speaker_id": "2",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017454.1.32",
          "form": "중당이 뭐에요?",
          "original_form": "중당이 뭐에요?",
          "speaker_id": "1",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017454.1.33",
          "form": "아 중국당면~~!",
          "original_form": "아 중국당면~~!",
          "speaker_id": "1",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017454.1.34",
          "form": "중국당면 이요!",
          "original_form": "중국당면 이요!",
          "speaker_id": "2",
          "time": "20210826 19:44"
        }
      ]
