      "utterance": [
        {
          "id": "MDRW2100035286.1.1",
          "form": "로제떡볶이 드셔보셨나요?",
          "original_form": "로제떡볶이 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100035286.1.2",
          "form": "진짜 맛있더라구요",
          "original_form": "진짜 맛있더라구요",
          "speaker_id": "1",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100035286.1.3",
          "form": "전 로제유행서도 뒤쳐지고",
          "original_form": "전 로제유행서도 뒤쳐지고",
          "speaker_id": "2",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100035286.1.4",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100035286.1.5",
          "form": "ㅠㅜ맵떡 유행서도 뒤쳐지고..",
          "original_form": "ㅠㅜ맵떡 유행서도 뒤쳐지고..",
          "speaker_id": "2",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100035286.1.6",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100035286.1.7",
          "form": "동네서 초등학교앞 취향으로 파는 동네 달달 밀떡볶이만 먹다보니",
          "original_form": "동네서 초등학교앞 취향으로 파는 동네 달달 밀떡볶이만 먹다보니",
          "speaker_id": "2",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100035286.1.8",
          "form": "느끼한거 좋아하시면 잘 맞으실거에요",
          "original_form": "느끼한거 좋아하시면 잘 맞으실거에요",
          "speaker_id": "1",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100035286.1.9",
          "form": "로제는 사진으로 보며 상상해요 ㅋㅋㅋ",
          "original_form": "로제는 사진으로 보며 상상해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100035286.1.10",
          "form": "아 근데 진짜",
          "original_form": "아 근데 진짜",
          "speaker_id": "1",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100035286.1.11",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100035286.1.12",
          "form": "떡볶이는",
          "original_form": "떡볶이는",
          "speaker_id": "1",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100035286.1.13",
          "form": "토마토가 아니라 고추장인거죠? 생크림이랑 섞은거요",
          "original_form": "토마토가 아니라 고추장인거죠? 생크림이랑 섞은거요",
          "speaker_id": "2",
          "time": "20210829 13:51"
        },
        {
          "id": "MDRW2100035286.1.14",
          "form": "야러가지 먹다가도 원래떡볶이로 돌아가는 것 같아요",
          "original_form": "야러가지 먹다가도 원래떡볶이로 돌아가는 것 같아요",
          "speaker_id": "1",
          "time": "20210829 13:51"
        },
        {
          "id": "MDRW2100035286.1.15",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210829 13:51"
        },
        {
          "id": "MDRW2100035286.1.16",
          "form": "진짜 로제보단",
          "original_form": "진짜 로제보단",
          "speaker_id": "1",
          "time": "20210829 13:51"
        },
        {
          "id": "MDRW2100035286.1.17",
          "form": "생크림이나 우유 섞은 맛 나더라구요",
          "original_form": "생크림이나 우유 섞은 맛 나더라구요",
          "speaker_id": "1",
          "time": "20210829 13:51"
        },
        {
          "id": "MDRW2100035286.1.18",
          "form": "궁금은 한데 배달시키면 혼자먹을 양이 아닌 경우가 많더래고요",
          "original_form": "궁금은 한데 배달시키면 혼자먹을 양이 아닌 경우가 많더래고요",
          "speaker_id": "2",
          "time": "20210829 13:51"
        },
        {
          "id": "MDRW2100035286.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100035286.1.20",
          "form": "엽떡은 무슨 감자탕 3인분 담는 용기에",
          "original_form": "엽떡은 무슨 감자탕 3인분 담는 용기에",
          "speaker_id": "2",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100035286.1.21",
          "form": "그래사 놔뒀다가 먹으면",
          "original_form": "그래사 놔뒀다가 먹으면",
          "speaker_id": "1",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100035286.1.22",
          "form": "넣어서 오던데",
          "original_form": "넣어서 오던데",
          "speaker_id": "2",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100035286.1.23",
          "form": "3끼를 떡볶이로ㅜ",
          "original_form": "3끼를 떡볶이로ㅜ",
          "speaker_id": "1",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100035286.1.24",
          "form": "맞아요 1인분 양 파는데도 있긴한데",
          "original_form": "맞아요 1인분 양 파는데도 있긴한데",
          "speaker_id": "1",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100035286.1.25",
          "form": "솔직히 많이 비싸죠",
          "original_form": "솔직히 많이 비싸죠",
          "speaker_id": "1",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100035286.1.26",
          "form": "그래서 동네에 파는 떡볶이집 가게되는데",
          "original_form": "그래서 동네에 파는 떡볶이집 가게되는데",
          "speaker_id": "1",
          "time": "20210829 13:53"
        },
        {
          "id": "MDRW2100035286.1.27",
          "form": "저희동네 있던 떡볶이집이 사라져서 많이 슬퍼요",
          "original_form": "저희동네 있던 떡볶이집이 사라져서 많이 슬퍼요",
          "speaker_id": "1",
          "time": "20210829 13:53"
        },
        {
          "id": "MDRW2100035286.1.28",
          "form": "ㅜ",
          "original_form": "ㅜ",
          "speaker_id": "1",
          "time": "20210829 13:53"
        },
        {
          "id": "MDRW2100035286.1.29",
          "form": "코로나라서 초등학교가 닫으닉단 근처 분식집이 다 망해가요..",
          "original_form": "코로나라서 초등학교가 닫으닉단 근처 분식집이 다 망해가요..",
          "speaker_id": "2",
          "time": "20210829 13:54"
        },
        {
          "id": "MDRW2100035286.1.30",
          "form": "전 엽떡처럼 한솥으로 오는 떡볶이는 싫거든요 1인분씩 먹고 싶단 말이에요",
          "original_form": "전 엽떡처럼 한솥으로 오는 떡볶이는 싫거든요 1인분씩 먹고 싶단 말이에요",
          "speaker_id": "2",
          "time": "20210829 13:55"
        }
      ]
