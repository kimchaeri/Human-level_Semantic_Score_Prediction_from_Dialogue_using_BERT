      "utterance": [
        {
          "id": "MDRW2100028147.1.1",
          "form": "오늘 name1는 샤니돈가스를 먹나",
          "original_form": "오늘 &name1&는 샤니돈가스를 먹나",
          "speaker_id": "2",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028147.1.2",
          "form": "오늘 저녁은 마라탕이야",
          "original_form": "오늘 저녁은 마라탕이야",
          "speaker_id": "1",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028147.1.3",
          "form": "친구들이랑 먹기로 했어",
          "original_form": "친구들이랑 먹기로 했어",
          "speaker_id": "1",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028147.1.4",
          "form": "헐 뭐야",
          "original_form": "헐 뭐야",
          "speaker_id": "2",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028147.1.5",
          "form": "친구라니",
          "original_form": "친구라니",
          "speaker_id": "2",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028147.1.6",
          "form": "누구야",
          "original_form": "누구야",
          "speaker_id": "2",
          "time": "20210902 17:51"
        },
        {
          "id": "MDRW2100028147.1.7",
          "form": "말 안하고 사라져써",
          "original_form": "말 안하고 사라져써",
          "speaker_id": "2",
          "time": "20210902 17:52"
        },
        {
          "id": "MDRW2100028147.1.8",
          "form": "늘 만나는 회사 친구들",
          "original_form": "늘 만나는 회사 친구들",
          "speaker_id": "1",
          "time": "20210902 17:56"
        },
        {
          "id": "MDRW2100028147.1.9",
          "form": "나도 너랑 놀고 싶어",
          "original_form": "나도 너랑 놀고 싶어",
          "speaker_id": "2",
          "time": "20210902 17:57"
        },
        {
          "id": "MDRW2100028147.1.10",
          "form": "힝",
          "original_form": "힝",
          "speaker_id": "2",
          "time": "20210902 17:57"
        },
        {
          "id": "MDRW2100028147.1.11",
          "form": "마라탕을 같이 먹다니",
          "original_form": "마라탕을 같이 먹다니",
          "speaker_id": "2",
          "time": "20210902 17:58"
        },
        {
          "id": "MDRW2100028147.1.12",
          "form": "나는 저녁으로 피자를 시켜 먹을 것이다",
          "original_form": "나는 저녁으로 피자를 시켜 먹을 것이다",
          "speaker_id": "2",
          "time": "20210902 17:58"
        },
        {
          "id": "MDRW2100028147.1.13",
          "form": "마라탕에 케이크도 먹었지",
          "original_form": "마라탕에 케이크도 먹었지",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028147.1.14",
          "form": "피자는 뭐 시켰어?",
          "original_form": "피자는 뭐 시켰어?",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028147.1.15",
          "form": "고민하다가",
          "original_form": "고민하다가",
          "speaker_id": "2",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028147.1.16",
          "form": "케이크ㅠ",
          "original_form": "케이크ㅠ",
          "speaker_id": "2",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028147.1.17",
          "form": "맛있었겠드아",
          "original_form": "맛있었겠드아",
          "speaker_id": "2",
          "time": "20210902 22:11"
        },
        {
          "id": "MDRW2100028147.1.18",
          "form": "불고기피자와 콤비네이션피자로",
          "original_form": "불고기피자와 콤비네이션피자로",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028147.1.19",
          "form": "무난하게 시켰댜",
          "original_form": "무난하게 시켰댜",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028147.1.20",
          "form": "두 개 시켰어?",
          "original_form": "두 개 시켰어?",
          "speaker_id": "1",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028147.1.21",
          "form": "아 반반이었음",
          "original_form": "아 반반이었음",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028147.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028147.1.23",
          "form": "L사이즈로 동생이랑 먹었드아",
          "original_form": "L사이즈로 동생이랑 먹었드아",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028147.1.24",
          "form": "임실치즈피자가 최고",
          "original_form": "임실치즈피자가 최고",
          "speaker_id": "2",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100028147.1.25",
          "form": "아 나도 주말에 그거 먹었어",
          "original_form": "아 나도 주말에 그거 먹었어",
          "speaker_id": "1",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028147.1.26",
          "form": "고르곤졸라 맛있더라",
          "original_form": "고르곤졸라 맛있더라",
          "speaker_id": "1",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028147.1.27",
          "form": "오 삼치에도 있었구나",
          "original_form": "오 삼치에도 있었구나",
          "speaker_id": "2",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028147.1.28",
          "form": "고르곤졸라 한번도 안시켜봤었는데",
          "original_form": "고르곤졸라 한번도 안시켜봤었는데",
          "speaker_id": "2",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028147.1.29",
          "form": "담에 먹어봐야겠다",
          "original_form": "담에 먹어봐야겠다",
          "speaker_id": "2",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028147.1.30",
          "form": "얼마 전에 새로 생겼어",
          "original_form": "얼마 전에 새로 생겼어",
          "speaker_id": "1",
          "time": "20210903 10:07"
        },
        {
          "id": "MDRW2100028147.1.31",
          "form": "오오 그렇군",
          "original_form": "오오 그렇군",
          "speaker_id": "2",
          "time": "20210903 14:17"
        },
        {
          "id": "MDRW2100028147.1.32",
          "form": "꼬리곰탕 먹었어",
          "original_form": "꼬리곰탕 먹었어",
          "speaker_id": "1",
          "time": "20210907 23:44"
        }
      ]
