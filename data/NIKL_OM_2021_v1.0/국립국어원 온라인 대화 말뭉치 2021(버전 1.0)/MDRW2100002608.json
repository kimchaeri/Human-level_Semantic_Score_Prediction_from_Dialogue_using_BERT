      "utterance": [
        {
          "id": "MDRW2100002608.1.1",
          "form": "넌 결혼 할 생각 있어?",
          "original_form": "넌 결혼 할 생각 있어?",
          "speaker_id": "1",
          "time": "20210820 13:53"
        },
        {
          "id": "MDRW2100002608.1.2",
          "form": "아니 없어",
          "original_form": "아니 없어",
          "speaker_id": "2",
          "time": "20210820 13:53"
        },
        {
          "id": "MDRW2100002608.1.3",
          "form": "너는 있어?",
          "original_form": "너는 있어?",
          "speaker_id": "2",
          "time": "20210820 13:54"
        },
        {
          "id": "MDRW2100002608.1.4",
          "form": "나도 전에는 생각 없었는데 최근들어서는 생각 정도는 있어",
          "original_form": "나도 전에는 생각 없었는데 최근들어서는 생각 정도는 있어",
          "speaker_id": "1",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002608.1.5",
          "form": "헉 그렇구나",
          "original_form": "헉 그렇구나",
          "speaker_id": "2",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002608.1.6",
          "form": "난 앞으로도 생각 없을것 같은데",
          "original_form": "난 앞으로도 생각 없을것 같은데",
          "speaker_id": "2",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002608.1.7",
          "form": "음 그럼 연애는 해볼 생각 있어?",
          "original_form": "음 그럼 연애는 해볼 생각 있어?",
          "speaker_id": "1",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002608.1.8",
          "form": "그것도 없는데",
          "original_form": "그것도 없는데",
          "speaker_id": "2",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002608.1.9",
          "form": "한번 해보는것도 나쁘지 않아~",
          "original_form": "한번 해보는것도 나쁘지 않아~",
          "speaker_id": "1",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002608.1.10",
          "form": "그럼 혹시 이상형은 있어?",
          "original_form": "그럼 혹시 이상형은 있어?",
          "speaker_id": "1",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002608.1.11",
          "form": "음.. 어려운 질문인걸",
          "original_form": "음.. 어려운 질문인걸",
          "speaker_id": "2",
          "time": "20210820 14:01"
        },
        {
          "id": "MDRW2100002608.1.12",
          "form": "생각해봤는데 없는듯",
          "original_form": "생각해봤는데 없는듯",
          "speaker_id": "2",
          "time": "20210820 14:01"
        },
        {
          "id": "MDRW2100002608.1.13",
          "form": "에이 그러지 말고 좀더 생각해봐",
          "original_form": "에이 그러지 말고 좀더 생각해봐",
          "speaker_id": "1",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100002608.1.14",
          "form": "안그럼 나는 어때같은 소리 할거야",
          "original_form": "안그럼 나는 어때같은 소리 할거야",
          "speaker_id": "1",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100002608.1.15",
          "form": "막 잘생긴거라던지",
          "original_form": "막 잘생긴거라던지",
          "speaker_id": "1",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002608.1.16",
          "form": "성격같은 거라던지~",
          "original_form": "성격같은 거라던지~",
          "speaker_id": "1",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002608.1.17",
          "form": "어렵네",
          "original_form": "어렵네",
          "speaker_id": "2",
          "time": "20210820 14:06"
        },
        {
          "id": "MDRW2100002608.1.18",
          "form": "칫",
          "original_form": "칫",
          "speaker_id": "1",
          "time": "20210820 14:06"
        },
        {
          "id": "MDRW2100002608.1.19",
          "form": "궁금했었는데 아쉽네",
          "original_form": "궁금했었는데 아쉽네",
          "speaker_id": "1",
          "time": "20210820 14:07"
        },
        {
          "id": "MDRW2100002608.1.20",
          "form": "근데 정말 안떠오르는걸",
          "original_form": "근데 정말 안떠오르는걸",
          "speaker_id": "2",
          "time": "20210820 14:07"
        },
        {
          "id": "MDRW2100002608.1.21",
          "form": "오케이",
          "original_form": "오케이",
          "speaker_id": "1",
          "time": "20210820 14:08"
        },
        {
          "id": "MDRW2100002608.1.22",
          "form": "그럼 농담으로 나는 어떤것 같아",
          "original_form": "그럼 농담으로 나는 어떤것 같아",
          "speaker_id": "1",
          "time": "20210820 14:08"
        },
        {
          "id": "MDRW2100002608.1.23",
          "form": "다른 사람이 보기엔 나는 어떻게 보여?",
          "original_form": "다른 사람이 보기엔 나는 어떻게 보여?",
          "speaker_id": "1",
          "time": "20210820 14:08"
        },
        {
          "id": "MDRW2100002608.1.24",
          "form": "더 어려운 질문인데",
          "original_form": "더 어려운 질문인데",
          "speaker_id": "2",
          "time": "20210820 14:12"
        },
        {
          "id": "MDRW2100002608.1.25",
          "form": "왜 어려워 ㅠㅠ",
          "original_form": "왜 어려워 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 14:13"
        },
        {
          "id": "MDRW2100002608.1.26",
          "form": "날 모르는구나 ㅠ",
          "original_form": "날 모르는구나 ㅠ",
          "speaker_id": "1",
          "time": "20210820 14:13"
        }
