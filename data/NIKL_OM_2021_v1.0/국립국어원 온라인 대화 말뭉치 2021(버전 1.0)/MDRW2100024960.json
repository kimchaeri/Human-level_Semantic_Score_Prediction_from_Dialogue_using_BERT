      "utterance": [
        {
          "id": "MDRW2100024960.1.1",
          "form": "엽떡 착한 맛 시키셨나요?!",
          "original_form": "엽떡 착한 맛 시키셨나요?!",
          "speaker_id": "1",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100024960.1.2",
          "form": "사이드메뉴도 시키셨나요~~~",
          "original_form": "사이드메뉴도 시키셨나요~~~",
          "speaker_id": "1",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100024960.1.3",
          "form": "친구가 시켜서 무슨맛인지모르겠어욬ㅋㅋ중당 추가한것만 알아요",
          "original_form": "친구가 시켜서 무슨맛인지모르겠어욬ㅋㅋ중당 추가한것만 알아요",
          "speaker_id": "2",
          "time": "20210831 21:26"
        },
        {
          "id": "MDRW2100024960.1.4",
          "form": "사이드는 안시킨것같아요!!",
          "original_form": "사이드는 안시킨것같아요!!",
          "speaker_id": "2",
          "time": "20210831 21:26"
        },
        {
          "id": "MDRW2100024960.1.5",
          "form": "중당은 중국당면이죠?!",
          "original_form": "중당은 중국당면이죠?!",
          "speaker_id": "1",
          "time": "20210831 21:27"
        },
        {
          "id": "MDRW2100024960.1.6",
          "form": "으하하 맛나겠다,,,",
          "original_form": "으하하 맛나겠다,,,",
          "speaker_id": "1",
          "time": "20210831 21:27"
        },
        {
          "id": "MDRW2100024960.1.7",
          "form": "친구가 참 먹을 줄 아는 분이네요 ",
          "original_form": "친구가 참 먹을 줄 아는 분이네요 ",
          "speaker_id": "1",
          "time": "20210831 21:27"
        },
        {
          "id": "MDRW2100024960.1.8",
          "form": "네네 친구가 추가한다고 통보했어요ㅋㅋ",
          "original_form": "네네 친구가 추가한다고 통보했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:27"
        },
        {
          "id": "MDRW2100024960.1.9",
          "form": "엽떡너무 오랜만이에요",
          "original_form": "엽떡너무 오랜만이에요",
          "speaker_id": "2",
          "time": "20210831 21:27"
        },
        {
          "id": "MDRW2100024960.1.10",
          "form": "지금쯤 맛나게 드시고 있겠군요!!!!",
          "original_form": "지금쯤 맛나게 드시고 있겠군요!!!!",
          "speaker_id": "1",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100024960.1.11",
          "form": "헷 다먹었어요",
          "original_form": "헷 다먹었어요",
          "speaker_id": "2",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100024960.1.12",
          "form": "맛있었어요!",
          "original_form": "맛있었어요!",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100024960.1.13",
          "form": "우와~~~~",
          "original_form": "우와~~~~",
          "speaker_id": "1",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100024960.1.14",
          "form": "저도 먹고싶어지네요!!",
          "original_form": "저도 먹고싶어지네요!!",
          "speaker_id": "1",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100024960.1.15",
