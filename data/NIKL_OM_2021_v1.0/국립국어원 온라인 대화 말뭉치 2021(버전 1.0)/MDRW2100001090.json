      "utterance": [
        {
          "id": "MDRW2100001090.1.1",
          "form": "점심 뭐먹었어?",
          "original_form": "점심 뭐먹었어?",
          "speaker_id": "1",
          "time": "20210723 16:40"
        },
        {
          "id": "MDRW2100001090.1.2",
          "form": "초콜릿이랑 콜라 먹었엉 넌?",
          "original_form": "초콜릿이랑 콜라 먹었엉 넌?",
          "speaker_id": "2",
          "time": "20210723 16:41"
        },
        {
          "id": "MDRW2100001090.1.3",
          "form": "왜 그거밖에 안먹었어?",
          "original_form": "왜 그거밖에 안먹었어?",
          "speaker_id": "1",
          "time": "20210723 16:42"
        },
        {
          "id": "MDRW2100001090.1.4",
          "form": "난 라면이랑",
          "original_form": "난 라면이랑",
          "speaker_id": "1",
          "time": "20210723 16:42"
        },
        {
          "id": "MDRW2100001090.1.5",
          "form": "김밥!",
          "original_form": "김밥!",
          "speaker_id": "1",
          "time": "20210723 16:42"
        },
        {
          "id": "MDRW2100001090.1.6",
          "form": "귀찮아서! 라면 맛있겠다",
          "original_form": "귀찮아서! 라면 맛있겠다",
          "speaker_id": "2",
          "time": "20210723 16:43"
        },
        {
          "id": "MDRW2100001090.1.7",
          "form": "무슨 라면 좋아해?",
          "original_form": "무슨 라면 좋아해?",
          "speaker_id": "2",
          "time": "20210723 16:43"
        },
        {
          "id": "MDRW2100001090.1.8",
          "form": "난 불닭!",
          "original_form": "난 불닭!",
          "speaker_id": "2",
          "time": "20210723 16:44"
        },
        {
          "id": "MDRW2100001090.1.9",
          "form": "매운거 잘먹나봐?",
          "original_form": "매운거 잘먹나봐?",
          "speaker_id": "1",
          "time": "20210723 16:44"
        },
        {
          "id": "MDRW2100001090.1.10",
          "form": "난 육개장!",
          "original_form": "난 육개장!",
          "speaker_id": "1",
          "time": "20210723 16:44"
        },
        {
          "id": "MDRW2100001090.1.11",
          "form": "육개장 맛있지 매운 거 잘 먹진 못하는데 좋아해 ㅎㅎㅎ",
          "original_form": "육개장 맛있지 매운 거 잘 먹진 못하는데 좋아해 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210723 16:46"
        },
        {
          "id": "MDRW2100001090.1.12",
          "form": "카페 음료는 보통 뭐 먹어??",
          "original_form": "카페 음료는 보통 뭐 먹어??",
          "speaker_id": "2",
          "time": "20210723 16:46"
        },
        {
          "id": "MDRW2100001090.1.13",
          "form": "나는 커피 좋아해",
          "original_form": "나는 커피 좋아해",
          "speaker_id": "1",
          "time": "20210723 16:46"
        },
        {
          "id": "MDRW2100001090.1.14",
          "form": "커피먹어도 잠안오고",
          "original_form": "커피먹어도 잠안오고",
          "speaker_id": "1",
          "time": "20210723 16:47"
        },
        {
          "id": "MDRW2100001090.1.15",
          "form": "이런게 없더라구",
          "original_form": "이런게 없더라구",
          "speaker_id": "1",
          "time": "20210723 16:47"
        },
        {
          "id": "MDRW2100001090.1.16",
          "form": "넌?",
          "original_form": "넌?",
          "speaker_id": "1",
          "time": "20210723 16:47"
        },
        {
          "id": "MDRW2100001090.1.17",
          "form": "아 ㅋㅋㅋ 나도 커피 마셔도 잘자 난 민트음료!!",
          "original_form": "아 ㅋㅋㅋ 나도 커피 마셔도 잘자 난 민트음료!!",
          "speaker_id": "2",
          "time": "20210723 16:47"
        },
        {
          "id": "MDRW2100001090.1.18",
          "form": "시원한 거!!",
          "original_form": "시원한 거!!",
          "speaker_id": "2",
          "time": "20210723 16:47"
        },
        {
          "id": "MDRW2100001090.1.19",
          "form": "민트 좋아해??",
          "original_form": "민트 좋아해??",
          "speaker_id": "2",
          "time": "20210723 16:48"
        },
