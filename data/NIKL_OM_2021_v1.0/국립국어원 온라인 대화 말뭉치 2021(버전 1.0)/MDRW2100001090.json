        {
          "id": "MDRW2100001090.1.24",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210723 16:48"
        },
        {
          "id": "MDRW2100001090.1.25",
          "form": "아 ㅋㅋㅋ ㅠㅠ 치약맛 나지..?",
          "original_form": "아 ㅋㅋㅋ ㅠㅠ 치약맛 나지..?",
          "speaker_id": "2",
          "time": "20210723 16:48"
        },
