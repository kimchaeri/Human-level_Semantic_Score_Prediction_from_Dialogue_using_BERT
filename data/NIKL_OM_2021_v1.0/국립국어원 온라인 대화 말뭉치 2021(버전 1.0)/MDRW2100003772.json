        },
        {
          "id": "MDRW2100003772.1.2",
          "form": "안녕하세요^^",
          "original_form": "안녕하세요^^",
          "speaker_id": "2",
          "time": "20210821 12:00"
        },
        {
          "id": "MDRW2100003772.1.3",
          "form": "혹시 해외여행 가보셨어요?",
          "original_form": "혹시 해외여행 가보셨어요?",
          "speaker_id": "1",
          "time": "20210821 12:10"
        },
        {
          "id": "MDRW2100003772.1.4",
          "form": "네네 코로나 전에 다녀왔어요!",
          "original_form": "네네 코로나 전에 다녀왔어요!",
          "speaker_id": "2",
          "time": "20210821 12:18"
        },
        {
          "id": "MDRW2100003772.1.5",
          "form": "어디어디 가보셨어요?",
          "original_form": "어디어디 가보셨어요?",
          "speaker_id": "1",
          "time": "20210821 12:23"
        },
        {
          "id": "MDRW2100003772.1.6",
          "form": "신혼여행으로 몰디브다녀오고 2년 터울주고 말레이시아랑 싱가폴 태국 다녀왔어요^^",
          "original_form": "신혼여행으로 몰디브다녀오고 2년 터울주고 말레이시아랑 싱가폴 태국 다녀왔어요^^",
          "speaker_id": "2",
          "time": "20210821 12:26"
        },
        {
          "id": "MDRW2100003772.1.7",
          "form": "어디어디 가보셨어요??",
          "original_form": "어디어디 가보셨어요??",
          "speaker_id": "2",
          "time": "20210821 12:26"
        },
        {
          "id": "MDRW2100003772.1.8",
          "form": "저는 아직 못가봐서 부럽네요",
          "original_form": "저는 아직 못가봐서 부럽네요",
          "speaker_id": "1",
          "time": "20210821 12:30"
        },
        {
          "id": "MDRW2100003772.1.9",
          "form": "어디가 제일 좋으셨어요?",
          "original_form": "어디가 제일 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210821 12:30"
        },
        {
          "id": "MDRW2100003772.1.10",
          "form": "몰디브요.",
          "original_form": "몰디브요.",
          "speaker_id": "2",
          "time": "20210821 12:34"
        },
        {
          "id": "MDRW2100003772.1.11",
          "form": "자연환경이 엄청 좋더라고요",
          "original_form": "자연환경이 엄청 좋더라고요",
          "speaker_id": "2",
          "time": "20210821 12:35"
        },
        {
          "id": "MDRW2100003772.1.12",
          "form": "국내 여행지라도 어디 다녀온 곳 없으셔요??",
          "original_form": "국내 여행지라도 어디 다녀온 곳 없으셔요??",
          "speaker_id": "2",
          "time": "20210821 12:35"
        },
        {
          "id": "MDRW2100003772.1.13",
          "form": "제가 경상도 사람이라",
          "original_form": "제가 경상도 사람이라",
          "speaker_id": "1",
          "time": "20210821 12:36"
        },
        {
          "id": "MDRW2100003772.1.14",
          "form": "서울이 제일 좋았던거같아요",
          "original_form": "서울이 제일 좋았던거같아요",
          "speaker_id": "1",
          "time": "20210821 12:36"
        },
        {
          "id": "MDRW2100003772.1.15",
          "form": "애들데리고 애버랜드 너무 가고 싶은데..",
          "original_form": "애들데리고 애버랜드 너무 가고 싶은데..",
          "speaker_id": "2",
          "time": "20210821 12:38"
        },
        {
          "id": "MDRW2100003772.1.16",
          "form": "코로나라 무서워서 움직이질 못하네요.ㅠㅠ",
          "original_form": "코로나라 무서워서 움직이질 못하네요.ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 12:38"
        },
        {
          "id": "MDRW2100003772.1.17",
          "form": "서울에서 어디가 제일 좋으셨어요??",
          "original_form": "서울에서 어디가 제일 좋으셨어요??",
          "speaker_id": "2",
          "time": "20210821 12:38"
        },
        {
          "id": "MDRW2100003772.1.18",
          "form": "건물이 되게 높더라구요",
          "original_form": "건물이 되게 높더라구요",
          "speaker_id": "1",
          "time": "20210821 12:39"
        },
        {
          "id": "MDRW2100003772.1.19",
          "form": "놀이기구 좋아하세요?",
          "original_form": "놀이기구 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 12:40"
        },
        {
          "id": "MDRW2100003772.1.20",
          "form": "네!!!",
          "original_form": "네!!!",
          "speaker_id": "2",
          "time": "20210821 12:41"
        },
        {
          "id": "MDRW2100003772.1.21",
          "form": "지도 지방에 살고 있어서... 놀이기구는 잘 접할 수가 없거든요.ㅠ",
          "original_form": "지도 지방에 살고 있어서... 놀이기구는 잘 접할 수가 없거든요.ㅠ",
          "speaker_id": "2",
          "time": "20210821 12:41"
        },
        {
          "id": "MDRW2100003772.1.22",
          "form": "저도 놀이기구 타려면 몇시간은 가야하는거 같아요...",
          "original_form": "저도 놀이기구 타려면 몇시간은 가야하는거 같아요...",
          "speaker_id": "1",
          "time": "20210821 12:58"
        }
      ]
