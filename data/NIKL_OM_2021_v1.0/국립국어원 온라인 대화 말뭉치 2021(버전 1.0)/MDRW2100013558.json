      "utterance": [
        {
          "id": "MDRW2100013558.1.1",
          "form": "자취 해보신적 있어요?",
          "original_form": "자취 해보신적 있어요?",
          "speaker_id": "1",
          "time": "20210825 15:31"
        },
        {
          "id": "MDRW2100013558.1.2",
          "form": "앗 아뇨",
          "original_form": "앗 아뇨",
          "speaker_id": "2",
          "time": "20210825 15:31"
        },
        {
          "id": "MDRW2100013558.1.3",
          "form": "자취해보신 적 있으세요?",
          "original_form": "자취해보신 적 있으세요?",
          "speaker_id": "2",
          "time": "20210825 15:31"
        },
        {
          "id": "MDRW2100013558.1.4",
          "form": "네 한 6개월정도",
          "original_form": "네 한 6개월정도",
          "speaker_id": "1",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013558.1.5",
          "form": "자취해보고싶지 않아요?",
          "original_form": "자취해보고싶지 않아요?",
          "speaker_id": "1",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013558.1.6",
          "form": "헉 네네",
          "original_form": "헉 네네",
          "speaker_id": "2",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013558.1.7",
          "form": "아니 취업할 때 되면 자연스럽게 자취할 줄알았늗데",
          "original_form": "아니 취업할 때 되면 자연스럽게 자취할 줄알았늗데",
          "speaker_id": "2",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013558.1.8",
          "form": "오 6개월이나 ㄷㄷ",
          "original_form": "오 6개월이나 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013558.1.9",
          "form": "자취하면 다들 환상 가지고 있잖아요",
          "original_form": "자취하면 다들 환상 가지고 있잖아요",
          "speaker_id": "1",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013558.1.10",
          "form": "맞아요 약간 자유",
          "original_form": "맞아요 약간 자유",
          "speaker_id": "2",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013558.1.11",
          "form": "엄청 좋을것이다 이런",
          "original_form": "엄청 좋을것이다 이런",
          "speaker_id": "1",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013558.1.12",
          "form": "이런거",
          "original_form": "이런거",
          "speaker_id": "2",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013558.1.13",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013558.1.14",
          "form": "근데 실상은 달라요",
          "original_form": "근데 실상은 달라요",
          "speaker_id": "1",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013558.1.15",
          "form": "헉 어떤가요?",
          "original_form": "헉 어떤가요?",
          "speaker_id": "2",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013558.1.16",
          "form": "생각보다 열배는 더 좋아요",
          "original_form": "생각보다 열배는 더 좋아요",
          "speaker_id": "1",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013558.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013558.1.18",
          "form": "함정카드였나욬ㅋㅋㅋㅋ 안좋다는 말 나올 줄 알았는뎈ㅋㅋㅋㅋ",
          "original_form": "함정카드였나욬ㅋㅋㅋㅋ 안좋다는 말 나올 줄 알았는뎈ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013558.1.19",
          "form": "어떤점이 좋나요",
          "original_form": "어떤점이 좋나요",
          "speaker_id": "2",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013558.1.20",
          "form": "하 노터치라는 점이?",
          "original_form": "하 노터치라는 점이?",
          "speaker_id": "2",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013558.1.21",
          "form": "일단 뭐",
          "original_form": "일단 뭐",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013558.1.22",
          "form": "모든걸 내맘대로 하는거죠",
          "original_form": "모든걸 내맘대로 하는거죠",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013558.1.23",
          "form": "단 내가 알바라도 해서 생활비랑 월세에서 자유로우면",
          "original_form": "단 내가 알바라도 해서 생활비랑 월세에서 자유로우면",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013558.1.24",
          "form": "아 쓰읍... 젤 어렵네요",
          "original_form": "아 쓰읍... 젤 어렵네요",
          "speaker_id": "2",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013558.1.25",
          "form": "경제적 자유",
          "original_form": "경제적 자유",
          "speaker_id": "2",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013558.1.26",
          "form": "배당금 많이 받아서 월세 충당했으면 좋겠어요.. 하..",
          "original_form": "배당금 많이 받아서 월세 충당했으면 좋겠어요.. 하..",
          "speaker_id": "2",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013558.1.27",
          "form": "그럼 룸메 없이 혼자 사셨던 거죠?!",
          "original_form": "그럼 룸메 없이 혼자 사셨던 거죠?!",
          "speaker_id": "2",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013558.1.28",
          "form": "네. 여자친구랑 동거하려고 나갔는데 한달만에 헤어졌어요",
          "original_form": "네. 여자친구랑 동거하려고 나갔는데 한달만에 헤어졌어요",
          "speaker_id": "1",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013558.1.29",
          "form": "배당금이면 주식하세요?",
          "original_form": "배당금이면 주식하세요?",
          "speaker_id": "1",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013558.1.30",
          "form": "ㅇㄴ 약간 슬픈 이야기였네요?",
          "original_form": "ㅇㄴ 약간 슬픈 이야기였네요?",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013558.1.31",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013558.1.32",
          "form": "하...쓰읍..물린게 많지만요",
          "original_form": "하...쓰읍..물린게 많지만요",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013558.1.33",
          "form": "인생 기니까요...",
          "original_form": "인생 기니까요...",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013558.1.34",
          "form": "미국만 오르네요",
          "original_form": "미국만 오르네요",
          "speaker_id": "2",
          "time": "20210825 15:40"
        }
      ]
    }
