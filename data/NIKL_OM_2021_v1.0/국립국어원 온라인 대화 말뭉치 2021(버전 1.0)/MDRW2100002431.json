      "utterance": [
        {
          "id": "MDRW2100002431.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210820 12:09"
        },
        {
          "id": "MDRW2100002431.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210820 12:12"
        },
        {
          "id": "MDRW2100002431.1.3",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210820 12:12"
        },
        {
          "id": "MDRW2100002431.1.4",
          "form": "아뇨 고양이 키우고싶은데",
          "original_form": "아뇨 고양이 키우고싶은데",
          "speaker_id": "1",
          "time": "20210820 12:13"
        },
        {
          "id": "MDRW2100002431.1.5",
          "form": "자신이 없더라구요ㅜㅜ",
          "original_form": "자신이 없더라구요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 12:13"
        },
        {
          "id": "MDRW2100002431.1.6",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210820 12:13"
        },
        {
          "id": "MDRW2100002431.1.7",
          "form": "저는 본가에서 강아지 키우고 있어요",
          "original_form": "저는 본가에서 강아지 키우고 있어요",
          "speaker_id": "2",
          "time": "20210820 12:13"
        },
        {
          "id": "MDRW2100002431.1.8",
          "form": "저랑 같이 있지는 않구요",
          "original_form": "저랑 같이 있지는 않구요",
          "speaker_id": "2",
          "time": "20210820 12:13"
        },
        {
          "id": "MDRW2100002431.1.9",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "1",
          "time": "20210820 12:14"
        },
        {
          "id": "MDRW2100002431.1.10",
          "form": "고양이 왜요?",
          "original_form": "고양이 왜요?",
          "speaker_id": "2",
          "time": "20210820 12:14"
        },
        {
          "id": "MDRW2100002431.1.11",
          "form": "강아지도 좋구 고양이도 좋은데",
          "original_form": "강아지도 좋구 고양이도 좋은데",
          "speaker_id": "1",
          "time": "20210820 12:14"
        },
        {
          "id": "MDRW2100002431.1.12",
          "form": "예전에 일하던곳 근처에",
          "original_form": "예전에 일하던곳 근처에",
          "speaker_id": "1",
          "time": "20210820 12:14"
        },
        {
          "id": "MDRW2100002431.1.13",
          "form": "길냥이가 있었거든요",
          "original_form": "길냥이가 있었거든요",
          "speaker_id": "1",
          "time": "20210820 12:14"
        },
        {
          "id": "MDRW2100002431.1.14",
          "form": "그때 정이 많이 들었어서",
          "original_form": "그때 정이 많이 들었어서",
          "speaker_id": "1",
          "time": "20210820 12:14"
        },
        {
          "id": "MDRW2100002431.1.15",
          "form": "되게 키우고싶었었어요",
          "original_form": "되게 키우고싶었었어요",
          "speaker_id": "1",
          "time": "20210820 12:14"
        },
        {
          "id": "MDRW2100002431.1.16",
          "form": "정 많이 들었으면 헤어질때 힘드셨겠네요ㅠ",
          "original_form": "정 많이 들었으면 헤어질때 힘드셨겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210820 12:17"
        },
        {
          "id": "MDRW2100002431.1.17",
          "form": "네ㅠ 근데 다행히 좋은분께 입양간거라",
          "original_form": "네ㅠ 근데 다행히 좋은분께 입양간거라",
          "speaker_id": "1",
          "time": "20210820 12:17"
        },
        {
          "id": "MDRW2100002431.1.18",
          "form": "아쉽긴해도 기분좋게 헤어졌어요",
          "original_form": "아쉽긴해도 기분좋게 헤어졌어요",
          "speaker_id": "1",
          "time": "20210820 12:17"
        },
        {
          "id": "MDRW2100002431.1.19",
          "form": "그뒤로는 고양이들 볼때마다 눈에 밟히더라구요",
          "original_form": "그뒤로는 고양이들 볼때마다 눈에 밟히더라구요",
          "speaker_id": "1",
          "time": "20210820 12:18"
        },
        {
          "id": "MDRW2100002431.1.20",
          "form": "그러게요 입양가서 다행이네요ㅠ",
          "original_form": "그러게요 입양가서 다행이네요ㅠ",
          "speaker_id": "2",
          "time": "20210820 12:18"
        },
        {
          "id": "MDRW2100002431.1.21",
          "form": "그럴만하시겠어요ㅠ",
          "original_form": "그럴만하시겠어요ㅠ",
          "speaker_id": "2",
          "time": "20210820 12:18"
        },
        {
          "id": "MDRW2100002431.1.22",
          "form": "정말 키우고는 싶은데 금전적으로 여유도 없고",
          "original_form": "정말 키우고는 싶은데 금전적으로 여유도 없고",
          "speaker_id": "1",
          "time": "20210820 12:18"
        },
        {
          "id": "MDRW2100002431.1.23",
          "form": "고양이는 키우기힘든가요?",
          "original_form": "고양이는 키우기힘든가요?",
          "speaker_id": "2",
          "time": "20210820 12:18"
        },
        {
          "id": "MDRW2100002431.1.24",
          "form": "책임질자신이없어서ㅠ",
          "original_form": "책임질자신이없어서ㅠ",
          "speaker_id": "1",
          "time": "20210820 12:18"
        },
        {
          "id": "MDRW2100002431.1.25",
          "form": "아 맞아요ㅠ",
          "original_form": "아 맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210820 12:18"
        },
        {
          "id": "MDRW2100002431.1.26",
          "form": "음 아무래도 아프면 돈도 많이 들거고",
          "original_form": "음 아무래도 아프면 돈도 많이 들거고",
          "speaker_id": "1",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100002431.1.27",
          "form": "그쵸 괜히 덜컥 입양하는것보다",
          "original_form": "그쵸 괜히 덜컥 입양하는것보다",
          "speaker_id": "2",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100002431.1.28",
          "form": "사실 할퀴는것도..무섭고..ㅎㅎ",
          "original_form": "사실 할퀴는것도..무섭고..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100002431.1.29",
          "form": "가구도 상한다하구",
          "original_form": "가구도 상한다하구",
          "speaker_id": "1",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100002431.1.30",
          "form": "쉽게 결정하긴 힘들더라구여ㅋㅋ",
          "original_form": "쉽게 결정하긴 힘들더라구여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100002431.1.31",
          "form": "준비 많이하고 입양해야죠",
          "original_form": "준비 많이하고 입양해야죠",
          "speaker_id": "2",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100002431.1.32",
          "form": "그러게요 이것저것 생각할게 많네요",
          "original_form": "그러게요 이것저것 생각할게 많네요",
          "speaker_id": "2",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100002431.1.33",
          "form": "그래도 언젠가는 키우고싶어요~",
          "original_form": "그래도 언젠가는 키우고싶어요~",
          "speaker_id": "1",
          "time": "20210820 12:20"
        },
        {
          "id": "MDRW2100002431.1.34",
          "form": "준비 잘 하셔서 키우시면 잘 키우실거같아요",
          "original_form": "준비 잘 하셔서 키우시면 잘 키우실거같아요",
          "speaker_id": "2",
          "time": "20210820 12:21"
        },
        {
          "id": "MDRW2100002431.1.35",
          "form": "ㅎㅎ 조금씩 준비해봐야겠어요",
          "original_form": "ㅎㅎ 조금씩 준비해봐야겠어요",
          "speaker_id": "1",
          "time": "20210820 12:21"
        },
        {
          "id": "MDRW2100002431.1.36",
          "form": "반려동물 키우면 넘 좋을거같아요",
          "original_form": "반려동물 키우면 넘 좋을거같아요",
          "speaker_id": "1",
          "time": "20210820 12:21"
        },
        {
          "id": "MDRW2100002431.1.37",
          "form": "마자요 저도 키우고 싶은데",
          "original_form": "마자요 저도 키우고 싶은데",
          "speaker_id": "2",
          "time": "20210820 12:22"
        },
        {
          "id": "MDRW2100002431.1.38",
          "form": "저 좋자고 키우자니 반려동물은 집에혼자있어야되니까 외로울거같아서",
          "original_form": "저 좋자고 키우자니 반려동물은 집에혼자있어야되니까 외로울거같아서",
          "speaker_id": "2",
          "time": "20210820 12:22"
        },
        {
          "id": "MDRW2100002431.1.39",
          "form": "쉽게 키우진못할거같아요",
          "original_form": "쉽게 키우진못할거같아요",
          "speaker_id": "2",
          "time": "20210820 12:22"
        },
        {
          "id": "MDRW2100002431.1.40",
          "form": "맞아요 혼자서 살면",
          "original_form": "맞아요 혼자서 살면",
          "speaker_id": "1",
          "time": "20210820 12:23"
        },
        {
          "id": "MDRW2100002431.1.41",
          "form": "내가 외출했을때",
          "original_form": "내가 외출했을때",
          "speaker_id": "1",
          "time": "20210820 12:24"
        },
        {
          "id": "MDRW2100002431.1.42",
          "form": "집에서 혼자 기다려야하니까",
          "original_form": "집에서 혼자 기다려야하니까",
          "speaker_id": "1",
          "time": "20210820 12:24"
        },
        {
          "id": "MDRW2100002431.1.43",
          "form": "걱정될거같아요",
          "original_form": "걱정될거같아요",
          "speaker_id": "1",
          "time": "20210820 12:24"
        },
        {
          "id": "MDRW2100002431.1.44",
          "form": "마자요 그것도 일종의 학대일수도 있으니까\r\n키우고 싶어도 자제하고 있어요",
          "original_form": "마자요 그것도 일종의 학대일수도 있으니까\r\n키우고 싶어도 자제하고 있어요",
          "speaker_id": "2",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002431.1.45",
          "form": "그리고 비용도 진짜 많이 들긴하더라구요",
          "original_form": "그리고 비용도 진짜 많이 들긴하더라구요",
          "speaker_id": "2",
          "time": "20210820 12:25"
        },
        {
          "id": "MDRW2100002431.1.46",
          "form": "그쵸 비용이 제일 큰 문제같아요",
          "original_form": "그쵸 비용이 제일 큰 문제같아요",
          "speaker_id": "1",
          "time": "20210820 12:26"
        }
      ]
    }
  ]
