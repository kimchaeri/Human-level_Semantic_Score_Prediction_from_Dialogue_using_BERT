      "utterance": [
        {
          "id": "MDRW2100001947.1.1",
          "form": "데이트 어디서 하세용",
          "original_form": "데이트 어디서 하세용",
          "speaker_id": "2",
          "time": "20210819 22:24"
        },
        {
          "id": "MDRW2100001947.1.2",
          "form": "놀이공윈요",
          "original_form": "놀이공윈요",
          "speaker_id": "1",
          "time": "20210819 22:25"
        },
        {
          "id": "MDRW2100001947.1.3",
          "form": "name2님은요(?)",
          "original_form": "&name2&님은요(?)",
          "speaker_id": "1",
          "time": "20210819 22:25"
        },
        {
          "id": "MDRW2100001947.1.4",
          "form": "저는 여즘 실내데이트!",
          "original_form": "저는 여즘 실내데이트!",
          "speaker_id": "2",
          "time": "20210819 22:26"
        },
        {
          "id": "MDRW2100001947.1.5",
          "form": "좋치요!! 미팅은 해보셨나요(?)",
          "original_form": "좋치요!! 미팅은 해보셨나요(?)",
          "speaker_id": "1",
          "time": "20210819 22:34"
        },
        {
          "id": "MDRW2100001947.1.6",
          "form": "해보고싶었는데 못해봤어요ㅋㅋ",
          "original_form": "해보고싶었는데 못해봤어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001947.1.7",
          "form": "저도 미팅이 소개팅보다는 재미있을듯",
          "original_form": "저도 미팅이 소개팅보다는 재미있을듯",
          "speaker_id": "1",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001947.1.8",
          "form": "ㅋㅋㅋㅋ궁금해여 어떤 분위기인지",
          "original_form": "ㅋㅋㅋㅋ궁금해여 어떤 분위기인지",
          "speaker_id": "2",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001947.1.9",
          "form": "이상형과 연애하시나요(?)",
          "original_form": "이상형과 연애하시나요(?)",
          "speaker_id": "1",
          "time": "20210819 22:41"
        },
        {
          "id": "MDRW2100001947.1.10",
          "form": "그럼요! 지금 남자친구랑 잘 지내고 있답니다",
          "original_form": "그럼요! 지금 남자친구랑 잘 지내고 있답니다",
          "speaker_id": "2",
          "time": "20210819 23:05"
        },
        {
          "id": "MDRW2100001947.1.11",
          "form": "남자친구가 이상형(?)",
          "original_form": "남자친구가 이상형(?)",
          "speaker_id": "1",
          "time": "20210819 23:05"
        },
        {
          "id": "MDRW2100001947.1.12",
          "form": "넹ㅎㅎ제가 좋아하는 스타일",
          "original_form": "넹ㅎㅎ제가 좋아하는 스타일",
          "speaker_id": "2",
          "time": "20210819 23:05"
        },
        {
          "id": "MDRW2100001947.1.13",
          "form": "오~~그럼 결혼까지도  생각하세요(?)",
          "original_form": "오~~그럼 결혼까지도  생각하세요(?)",
          "speaker_id": "1",
          "time": "20210819 23:06"
        },
        {
          "id": "MDRW2100001947.1.14",
          "form": "하면 좋죠~하고싶긴한데 아직 여건이 안되요ㅋㅋ",
          "original_form": "하면 좋죠~하고싶긴한데 아직 여건이 안되요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:07"
        },
        {
          "id": "MDRW2100001947.1.15",
          "form": "연애 결혼 다르다던데.. 혹시 결혼 ㆍ직정 나이는 언제라고 보시나요(?(",
          "original_form": "연애 결혼 다르다던데.. 혹시 결혼 ㆍ직정 나이는 언제라고 보시나요(?(",
          "speaker_id": "1",
          "time": "20210819 23:07"
        },
        {
          "id": "MDRW2100001947.1.16",
          "form": "개인적으로 28살에 하고싶긴 했었는데 현실적으론 30대 초반중반쯤 가게될것같아요",
          "original_form": "개인적으로 28살에 하고싶긴 했었는데 현실적으론 30대 초반중반쯤 가게될것같아요",
          "speaker_id": "2",
          "time": "20210819 23:08"
        },
        {
          "id": "MDRW2100001947.1.17",
          "form": "빨리해서 신혼 즐기는것도 좋은듯 해요",
          "original_form": "빨리해서 신혼 즐기는것도 좋은듯 해요",
          "speaker_id": "1",
          "time": "20210819 23:09"
        },
        {
          "id": "MDRW2100001947.1.18",
          "form": "맞아요 신혼 즐기고싶어서 28살에 하고싶었던거예요ㅋㅋ",
          "original_form": "맞아요 신혼 즐기고싶어서 28살에 하고싶었던거예요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100001947.1.19",
          "form": "부럽네요~~ 결혼 식은 요즘 코로난데",
          "original_form": "부럽네요~~ 결혼 식은 요즘 코로난데",
          "speaker_id": "1",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100001947.1.20",
          "form": "코로나가 빨리 끝나야 결혼식장도 붐빌텐데~",
          "original_form": "코로나가 빨리 끝나야 결혼식장도 붐빌텐데~",
          "speaker_id": "2",
          "time": "20210819 23:21"
        },
        {
          "id": "MDRW2100001947.1.21",
          "form": "지인 식장 가보니 답답해보이더라고요",
          "original_form": "지인 식장 가보니 답답해보이더라고요",
