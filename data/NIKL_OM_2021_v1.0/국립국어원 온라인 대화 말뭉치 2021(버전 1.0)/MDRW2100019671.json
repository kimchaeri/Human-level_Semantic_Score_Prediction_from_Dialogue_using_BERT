      "utterance": [
        {
          "id": "MDRW2100019671.1.1",
          "form": "저 식음료 스몰토크 마스터 될 것 같아요",
          "original_form": "저 식음료 스몰토크 마스터 될 것 같아요",
          "speaker_id": "2",
          "time": "20210827 21:09"
        },
        {
          "id": "MDRW2100019671.1.2",
          "form": "다행이에요",
          "original_form": "다행이에요",
          "speaker_id": "1",
          "time": "20210827 21:23"
        },
        {
          "id": "MDRW2100019671.1.3",
          "form": "저녁뭐드셨죠",
          "original_form": "저녁뭐드셨죠",
          "speaker_id": "1",
          "time": "20210827 21:23"
        },
        {
          "id": "MDRW2100019671.1.4",
          "form": "오늘은 카레",
          "original_form": "오늘은 카레",
          "speaker_id": "2",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100019671.1.5",
          "form": "아까 점심을 너무 과하게 먹어서",
          "original_form": "아까 점심을 너무 과하게 먹어서",
          "speaker_id": "2",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100019671.1.6",
          "form": "소화시키기가 힘드네여",
          "original_form": "소화시키기가 힘드네여",
          "speaker_id": "2",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100019671.1.7",
          "form": "너무 큰 꼬꼬였어요",
          "original_form": "너무 큰 꼬꼬였어요",
          "speaker_id": "1",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100019671.1.8",
          "form": "네 모밀소바도 양 엄청 많았고..",
          "original_form": "네 모밀소바도 양 엄청 많았고..",
          "speaker_id": "2",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100019671.1.9",
          "form": "와 저도 저녁 모밀",
          "original_form": "와 저도 저녁 모밀",
          "speaker_id": "1",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100019671.1.10",
          "form": "모밀 어쩐지 name2님이 쏘아옹린 공이었나봐요 뮤의식중 얼핏 본 모밀",
          "original_form": "모밀 어쩐지 &name2&님이 쏘아옹린 공이었나봐요 뮤의식중 얼핏 본 모밀",
          "speaker_id": "1",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100019671.1.11",
          "form": "넼ㅋㅋㅋ모밀 동지라고 치려다가 말았네여",
          "original_form": "넼ㅋㅋㅋ모밀 동지라고 치려다가 말았네여",
          "speaker_id": "2",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100019671.1.12",
          "form": "모밀ㄷㅇ짘ㅋㅋㅋ",
          "original_form": "모밀ㄷㅇ짘ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:30"
        },
        {
          "id": "MDRW2100019671.1.13",
          "form": "아 카레도머건따 동생 카레돈까스",
          "original_form": "아 카레도머건따 동생 카레돈까스",
          "speaker_id": "1",
          "time": "20210827 21:30"
        },
        {
          "id": "MDRW2100019671.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:31"
        },
        {
          "id": "MDRW2100019671.1.15",
          "form": "오늘 비슷하게 먹었군요",
          "original_form": "오늘 비슷하게 먹었군요",
          "speaker_id": "2",
          "time": "20210827 21:31"
        },
        {
          "id": "MDRW2100019671.1.16",
          "form": "그러네요",
          "original_form": "그러네요",
          "speaker_id": "1",
          "time": "20210827 21:32"
        },
        {
          "id": "MDRW2100019671.1.17",
          "form": "엄마가 통닭시키래서 지금 시켜드렷는데",
          "original_form": "엄마가 통닭시키래서 지금 시켜드렷는데",
          "speaker_id": "2",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100019671.1.18",
          "form": "저는 닭이 오늘 너무 물려서",
          "original_form": "저는 닭이 오늘 너무 물려서",
          "speaker_id": "2",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100019671.1.19",
          "form": "못먹겟습니다",
          "original_form": "못먹겟습니다",
          "speaker_id": "2",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100019671.1.20",
          "form": "정말 그렇겠네요",
          "original_form": "정말 그렇겠네요",
          "speaker_id": "1",
          "time": "20210827 21:36"
        },
        {
          "id": "MDRW2100019671.1.21",
          "form": "잠깐 쉬시나요 그럼",
          "original_form": "잠깐 쉬시나요 그럼",
          "speaker_id": "1",
          "time": "20210827 21:37"
        },
        {
          "id": "MDRW2100019671.1.22",
          "form": "네 산책 열나게 했더니 힘든데",
          "original_form": "네 산책 열나게 했더니 힘든데",
          "speaker_id": "2",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100019671.1.23",
          "form": "배도 부르고",
          "original_form": "배도 부르고",
          "speaker_id": "2",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100019671.1.24",
          "form": "저도요 광안리에서 걸어왔어요",
          "original_form": "저도요 광안리에서 걸어왔어요",
          "speaker_id": "1",
          "time": "20210827 21:39"
        },
        {
          "id": "MDRW2100019671.1.25",
          "form": "오늘 비슷한 하루 보냈네요",
          "original_form": "오늘 비슷한 하루 보냈네요",
          "speaker_id": "1",
          "time": "20210827 21:39"
        }
      ]
