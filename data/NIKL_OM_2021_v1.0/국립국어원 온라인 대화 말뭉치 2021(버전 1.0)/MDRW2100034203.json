      "utterance": [
        {
          "id": "MDRW2100034203.1.1",
          "form": "어렸을때 게임에",
          "original_form": "어렸을때 게임에",
          "speaker_id": "1",
          "time": "20210827 20:58"
        },
        {
          "id": "MDRW2100034203.1.2",
          "form": "문상만 조금 지른정도요 ㅋㅋㅋ",
          "original_form": "문상만 조금 지른정도요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:58"
        },
        {
          "id": "MDRW2100034203.1.3",
          "form": "헛 전 성인되고 엄청 지르고 있어요 ㅎ.ㅎ",
          "original_form": "헛 전 성인되고 엄청 지르고 있어요 ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210827 21:00"
        },
        {
          "id": "MDRW2100034203.1.4",
          "form": "제가 돈 벌어서 그런가 막 쓰게 되더라고요!!!",
          "original_form": "제가 돈 벌어서 그런가 막 쓰게 되더라고요!!!",
          "speaker_id": "2",
          "time": "20210827 21:00"
        },
        {
          "id": "MDRW2100034203.1.5",
          "form": "돈벌면 아끼게된다던데전",
          "original_form": "돈벌면 아끼게된다던데전",
          "speaker_id": "1",
          "time": "20210827 21:00"
        },
        {
          "id": "MDRW2100034203.1.6",
          "form": "오히려 더써요 저도 ㅋㅋㅋㅋ",
          "original_form": "오히려 더써요 저도 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:00"
        },
        {
          "id": "MDRW2100034203.1.7",
          "form": "그니까요 ㅎㅎㅎㅎㅎ 내 돈이란 생각이 들어서 그런가봐요",
          "original_form": "그니까요 ㅎㅎㅎㅎㅎ 내 돈이란 생각이 들어서 그런가봐요",
          "speaker_id": "2",
          "time": "20210827 21:03"
        },
        {
          "id": "MDRW2100034203.1.8",
          "form": "name1님 요새는 온라인 게임은 안 하세요?!",
          "original_form": "&name1&님 요새는 온라인 게임은 안 하세요?!",
          "speaker_id": "2",
          "time": "20210827 21:03"
        },
        {
          "id": "MDRW2100034203.1.9",
          "form": "온라인은 잘 안 하는거같아요 ㅜㅜ",
          "original_form": "온라인은 잘 안 하는거같아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 21:03"
        },
        {
          "id": "MDRW2100034203.1.10",
          "form": "어렸을때 라테일이라 넥슨 정도??",
          "original_form": "어렸을때 라테일이라 넥슨 정도??",
          "speaker_id": "1",
          "time": "20210827 21:04"
        },
        {
          "id": "MDRW2100034203.1.11",
          "form": "아 그렇군요 ㅎ.ㅎ 전 오버워치 해서 여쭤봤어요 ㅎㅎ",
          "original_form": "아 그렇군요 ㅎ.ㅎ 전 오버워치 해서 여쭤봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:06"
        },
        {
          "id": "MDRW2100034203.1.12",
          "form": "오버워치 많이 하시더라구요!",
          "original_form": "오버워치 많이 하시더라구요!",
          "speaker_id": "1",
          "time": "20210827 21:06"
        },
        {
          "id": "MDRW2100034203.1.13",
          "form": "재밌나요??",
          "original_form": "재밌나요??",
          "speaker_id": "1",
          "time": "20210827 21:07"
        },
        {
          "id": "MDRW2100034203.1.14",
          "form": "네!!! 처음 하는 사람들은 화면이 너무 현란해서",
          "original_form": "네!!! 처음 하는 사람들은 화면이 너무 현란해서",
          "speaker_id": "2",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100034203.1.15",
          "form": "어지럽다고 하는데 몇 번 하다보면 괜찮아져요 ㅎㅎ",
          "original_form": "어지럽다고 하는데 몇 번 하다보면 괜찮아져요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100034203.1.16",
          "form": "와 맞아요 그런말 저도 들었어요 ㅋㅋㅋ",
          "original_form": "와 맞아요 그런말 저도 들었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100034203.1.17",
          "form": "멀미난단 말도 들은거같아요",
          "original_form": "멀미난단 말도 들은거같아요",
          "speaker_id": "1",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100034203.1.18",
          "form": "맞아요 ㅎㅎㅎㅎㅎ",
          "original_form": "맞아요 ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:28"
        },
        {
          "id": "MDRW2100034203.1.19",
          "form": "그래도 제 최애 게임은 크아에요!",
          "original_form": "그래도 제 최애 게임은 크아에요!",
          "speaker_id": "2",
          "time": "20210827 21:28"
        },
        {
          "id": "MDRW2100034203.1.20",
          "form": "와 오랜만에 듣네요 ㅋㅋㅋ",
          "original_form": "와 오랜만에 듣네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:29"
        },
        {
          "id": "MDRW2100034203.1.21",
          "form": "자주하세요?",
          "original_form": "자주하세요?",
          "speaker_id": "1",
          "time": "20210827 21:29"
        },
        {
          "id": "MDRW2100034203.1.22",
          "form": "네! 초딩게임이라고는 하는데 전 재밌더라고요 ㅎㅎ",
          "original_form": "네! 초딩게임이라고는 하는데 전 재밌더라고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 21:31"
        },
        {
          "id": "MDRW2100034203.1.23",
          "form": "어렸을때 많이 했죠 크아나 카트 ㅋㅋㅋ",
