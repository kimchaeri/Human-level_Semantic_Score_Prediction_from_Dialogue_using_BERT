      "utterance": [
        {
          "id": "MDRW2100028710.1.1",
          "form": "저는 부모님이 소개팅 안해주셨어요",
          "original_form": "저는 부모님이 소개팅 안해주셨어요",
          "speaker_id": "2",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100028710.1.2",
          "form": "니가 스스로알아서 하던지 말던지 ㅋㅋㅋ",
          "original_form": "니가 스스로알아서 하던지 말던지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100028710.1.3",
          "form": "ㅋㅋㅋㅋㅋ앜ㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ앜ㅋ",
          "speaker_id": "1",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100028710.1.4",
          "form": "제친구가",
          "original_form": "제친구가",
          "speaker_id": "2",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100028710.1.5",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100028710.1.6",
          "form": "원래 사귀던 여친이 간호사였는데",
          "original_form": "원래 사귀던 여친이 간호사였는데",
          "speaker_id": "2",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100028710.1.7",
          "form": "아버지가 공무원이셨거든요",
          "original_form": "아버지가 공무원이셨거든요",
          "speaker_id": "2",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100028710.1.8",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100028710.1.9",
          "form": "친구한테 공무원만나서 결혼해야된다고",
          "original_form": "친구한테 공무원만나서 결혼해야된다고",
          "speaker_id": "2",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100028710.1.10",
          "form": "앜...",
          "original_form": "앜...",
          "speaker_id": "1",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100028710.1.11",
          "form": "그래서 여친있는데",
          "original_form": "그래서 여친있는데",
          "speaker_id": "2",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100028710.1.12",
          "form": "소개팅나가서",
          "original_form": "소개팅나가서",
          "speaker_id": "2",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100028710.1.13",
          "form": "간호사도 괜찮은데...",
          "original_form": "간호사도 괜찮은데...",
          "speaker_id": "1",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100028710.1.14",
          "form": "양다리걸치다가",
          "original_form": "양다리걸치다가",
          "speaker_id": "2",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100028710.1.15",
          "form": "할..",
          "original_form": "할..",
          "speaker_id": "1",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100028710.1.16",
          "form": "간호사 차버리고",
          "original_form": "간호사 차버리고",
          "speaker_id": "2",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100028710.1.17",
          "form": "공무원만나서",
          "original_form": "공무원만나서",
          "speaker_id": "2",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028710.1.18",
          "form": "결혼했는데요",
          "original_form": "결혼했는데요",
          "speaker_id": "2",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028710.1.19",
          "form": "허르.",
          "original_form": "허르.",
          "speaker_id": "1",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028710.1.20",
          "form": "2주전인가",
          "original_form": "2주전인가",
          "speaker_id": "2",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028710.1.21",
          "form": "전화와서",
          "original_form": "전화와서",
          "speaker_id": "2",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028710.1.22",
          "form": "진짜 애만아니면 이혼했다고",
          "original_form": "진짜 애만아니면 이혼했다고",
          "speaker_id": "2",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028710.1.23",
          "form": "와....",
          "original_form": "와....",
          "speaker_id": "1",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028710.1.24",
          "form": "그렇게 가서 행복하지두 않다니 ㅠㅜ",
          "original_form": "그렇게 가서 행복하지두 않다니 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100028710.1.25",
          "form": "부모님하고 와이프랑 사이 안좋아서",
          "original_form": "부모님하고 와이프랑 사이 안좋아서",
          "speaker_id": "2",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100028710.1.26",
          "form": "아예 얼굴 안보고 산대오",
          "original_form": "아예 얼굴 안보고 산대오",
          "speaker_id": "2",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100028710.1.27",
          "form": "아 진짜요 것도 반전이네여",
          "original_form": "아 진짜요 것도 반전이네여",
          "speaker_id": "1",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100028710.1.28",
          "form": "부모님이랑 사이가 안좋다니..",
          "original_form": "부모님이랑 사이가 안좋다니..",
          "speaker_id": "1",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100028710.1.29",
          "form": "첨엔 애도 안보여준다고 그랬대요",
          "original_form": "첨엔 애도 안보여준다고 그랬대요",
          "speaker_id": "2",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100028710.1.30",
          "form": "왜 그렇지.....",
          "original_form": "왜 그렇지.....",
          "speaker_id": "1",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100028710.1.31",
          "form": "제친구 부모님이",
          "original_form": "제친구 부모님이",
          "speaker_id": "2",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100028710.1.32",
          "form": "제사를 중요시하셔서",
          "original_form": "제사를 중요시하셔서",
          "speaker_id": "2",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100028710.1.33",
          "form": "막 연차쓰고  제사준비하라고 그러고",
          "original_form": "막 연차쓰고  제사준비하라고 그러고",
          "speaker_id": "2",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100028710.1.34",
          "form": "아....",
          "original_form": "아....",
          "speaker_id": "1",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100028710.1.35",
          "form": "그러셨거든요",
          "original_form": "그러셨거든요",
          "speaker_id": "2",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100028710.1.36",
          "form": "그건 또 심했네여",
          "original_form": "그건 또 심했네여",
          "speaker_id": "1",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100028710.1.37",
          "form": "간호사님이 운이 좋았네..",
          "original_form": "간호사님이 운이 좋았네..",
          "speaker_id": "1",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100028710.1.38",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210902 21:34"
        },
        {
          "id": "MDRW2100028710.1.39",
          "form": "세상 슬픈 사연 많다...",
          "original_form": "세상 슬픈 사연 많다...",
