      "utterance": [
        {
          "id": "MDRW2100004725.1.1",
          "form": "버블티도좋은데",
          "original_form": "버블티도좋은데",
          "speaker_id": "2",
          "time": "20210821 18:13"
        },
        {
          "id": "MDRW2100004725.1.2",
          "form": "비싸",
          "original_form": "비싸",
          "speaker_id": "2",
          "time": "20210821 18:13"
        },
        {
          "id": "MDRW2100004725.1.3",
          "form": "맞아 버블티 비싸",
          "original_form": "맞아 버블티 비싸",
          "speaker_id": "1",
          "time": "20210821 18:13"
        },
        {
          "id": "MDRW2100004725.1.4",
          "form": "아 동생친구가 타피오카펄을 사서 해먹었는데",
          "original_form": "아 동생친구가 타피오카펄을 사서 해먹었는데",
          "speaker_id": "2",
          "time": "20210821 18:14"
        },
        {
          "id": "MDRW2100004725.1.5",
          "form": "실수로 한번에 너무많이익혀서",
          "original_form": "실수로 한번에 너무많이익혀서",
          "speaker_id": "2",
          "time": "20210821 18:14"
        },
        {
          "id": "MDRW2100004725.1.6",
          "form": "해치우기힘들었다더라",
          "original_form": "해치우기힘들었다더라",
          "speaker_id": "2",
          "time": "20210821 18:14"
        },
        {
          "id": "MDRW2100004725.1.7",
          "form": "와 저런",
          "original_form": "와 저런",
          "speaker_id": "1",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004725.1.8",
          "form": "귀찮아도 쓸때마다 익히는게 좋은거같다",
          "original_form": "귀찮아도 쓸때마다 익히는게 좋은거같다",
          "speaker_id": "2",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004725.1.9",
          "form": "타피오카 많이 먹으면 더부룩한데",
          "original_form": "타피오카 많이 먹으면 더부룩한데",
          "speaker_id": "1",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004725.1.10",
          "form": "나도 소량은 한번사보고싶다",
          "original_form": "나도 소량은 한번사보고싶다",
          "speaker_id": "2",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004725.1.11",
          "form": "음료수는 우유안있으면",
          "original_form": "음료수는 우유안있으면",
          "speaker_id": "2",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004725.1.12",
          "form": "그거 있잖아",
          "original_form": "그거 있잖아",
          "speaker_id": "1",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004725.1.13",
          "form": "어울리는거 만들기쉽지않나",
          "original_form": "어울리는거 만들기쉽지않나",
          "speaker_id": "2",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004725.1.14",
          "form": "뭐",
          "original_form": "뭐",
          "speaker_id": "2",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004725.1.15",
          "form": "펄 1번먹을 분량으로 파는거 있어",
          "original_form": "펄 1번먹을 분량으로 파는거 있어",
          "speaker_id": "1",
          "time": "20210821 18:16"
        },
        {
          "id": "MDRW2100004725.1.16",
          "form": "10g정도씩",
          "original_form": "10g정도씩",
          "speaker_id": "1",
          "time": "20210821 18:16"
        },
        {
          "id": "MDRW2100004725.1.17",
          "form": "근데그건 포장지때문에 또 비싸게쳐서",
          "original_form": "근데그건 포장지때문에 또 비싸게쳐서",
          "speaker_id": "2",
          "time": "20210821 18:16"
        },
        {
          "id": "MDRW2100004725.1.18",
          "form": "고민하게되더라",
          "original_form": "고민하게되더라",
          "speaker_id": "2",
          "time": "20210821 18:16"
        },
        {
          "id": "MDRW2100004725.1.19",
          "form": "그건 그래",
          "original_form": "그건 그래",
          "speaker_id": "1",
          "time": "20210821 18:16"
        },
        {
          "id": "MDRW2100004725.1.20",
          "form": "근데 또 많이 포장 돼 있는거 먹으면",
          "original_form": "근데 또 많이 포장 돼 있는거 먹으면",
          "speaker_id": "2",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004725.1.21",
          "form": "한번먹을양 계량하는게",
          "original_form": "한번먹을양 계량하는게",
          "speaker_id": "2",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004725.1.22",
          "form": "그렇긴하다",
          "original_form": "그렇긴하다",
