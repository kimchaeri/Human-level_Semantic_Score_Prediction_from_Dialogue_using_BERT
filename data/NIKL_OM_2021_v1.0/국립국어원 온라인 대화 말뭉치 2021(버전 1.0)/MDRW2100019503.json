      "utterance": [
        {
          "id": "MDRW2100019503.1.1",
          "form": "지금은 직장인이세요?",
          "original_form": "지금은 직장인이세요?",
          "speaker_id": "1",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100019503.1.2",
          "form": "네 오늘도 퇴근하고 왔어요",
          "original_form": "네 오늘도 퇴근하고 왔어요",
          "speaker_id": "2",
          "time": "20210827 19:54"
        },
        {
          "id": "MDRW2100019503.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 19:54"
        },
        {
          "id": "MDRW2100019503.1.4",
          "form": "저도 오늘 칼퇴 하고 왔죠~",
          "original_form": "저도 오늘 칼퇴 하고 왔죠~",
          "speaker_id": "1",
          "time": "20210827 19:56"
        },
        {
          "id": "MDRW2100019503.1.5",
          "form": "금요일은 무조건 바로 집에 달려가줘야해요 ㅋㅋ",
          "original_form": "금요일은 무조건 바로 집에 달려가줘야해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:56"
        },
        {
          "id": "MDRW2100019503.1.6",
          "form": "전 오늘 오버타임 했어요ㅠ",
          "original_form": "전 오늘 오버타임 했어요ㅠ",
          "speaker_id": "2",
          "time": "20210827 20:00"
        },
        {
          "id": "MDRW2100019503.1.7",
          "form": "금요일엔 칼퇸데...",
          "original_form": "금요일엔 칼퇸데...",
          "speaker_id": "2",
          "time": "20210827 20:01"
        },
        {
          "id": "MDRW2100019503.1.8",
          "form": "허걱 ㅜㅜ 얼마나 늦게 퇴근하셨어요?",
          "original_form": "허걱 ㅜㅜ 얼마나 늦게 퇴근하셨어요?",
          "speaker_id": "1",
          "time": "20210827 20:01"
        },
        {
          "id": "MDRW2100019503.1.9",
          "form": "한 한시간 정도요?",
          "original_form": "한 한시간 정도요?",
          "speaker_id": "2",
          "time": "20210827 20:02"
        },
        {
          "id": "MDRW2100019503.1.10",
          "form": "요즘 바빠서 야근을 자주 하네요..",
          "original_form": "요즘 바빠서 야근을 자주 하네요..",
          "speaker_id": "2",
          "time": "20210827 20:02"
        },
        {
          "id": "MDRW2100019503.1.11",
          "form": "ㅠㅠ 오버타임 자주 있는 편이에요?",
          "original_form": "ㅠㅠ 오버타임 자주 있는 편이에요?",
          "speaker_id": "1",
          "time": "20210827 20:02"
        },
        {
          "id": "MDRW2100019503.1.12",
          "form": "아이고.. 그럼 그만큼 수당 더 챙겨줘요?",
          "original_form": "아이고.. 그럼 그만큼 수당 더 챙겨줘요?",
          "speaker_id": "1",
          "time": "20210827 20:02"
        },
        {
          "id": "MDRW2100019503.1.13",
          "form": "전 원하진 않았는데 그러네요",
          "original_form": "전 원하진 않았는데 그러네요",
          "speaker_id": "2",
          "time": "20210827 20:02"
        },
        {
          "id": "MDRW2100019503.1.14",
          "form": "아뇨... 야근 수당이 없는 곳이라..ㅎㅎ",
          "original_form": "아뇨... 야근 수당이 없는 곳이라..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019503.1.15",
          "form": "빨리 이직을 해야겠죠",
          "original_form": "빨리 이직을 해야겠죠",
          "speaker_id": "2",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019503.1.16",
          "form": "진짜 회사가 너무하네요 ㅜㅜ",
          "original_form": "진짜 회사가 너무하네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019503.1.17",
          "form": "그런 것도 안챙겨주면서 무급으로 일 부려먹고..",
          "original_form": "그런 것도 안챙겨주면서 무급으로 일 부려먹고..",
          "speaker_id": "1",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019503.1.18",
          "form": "그러니까요ㅠㅠ",
          "original_form": "그러니까요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019503.1.19",
          "form": "전 그렇게 생각하는데 회사 윗사람들은 안그런가봐요",
          "original_form": "전 그렇게 생각하는데 회사 윗사람들은 안그런가봐요",
          "speaker_id": "2",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100019503.1.20",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100019503.1.21",
          "form": "근데 그런 회사들이 굉장히 많은 것 같아요",
          "original_form": "근데 그런 회사들이 굉장히 많은 것 같아요",
          "speaker_id": "1",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019503.1.22",
          "form": "요즘엔 처우가 많이 나아졌다고 하지만 여전하더라구요..",
          "original_form": "요즘엔 처우가 많이 나아졌다고 하지만 여전하더라구요..",
          "speaker_id": "1",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019503.1.23",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "2",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019503.1.24",
          "form": "안그러려면 해외를 가야하나봐요...",
          "original_form": "안그러려면 해외를 가야하나봐요...",
          "speaker_id": "2",
