        {
          "id": "MDRW2100029843.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210903 14:51"
        },
        {
          "id": "MDRW2100029843.1.2",
          "form": "청소담당이요",
          "original_form": "청소담당이요",
          "speaker_id": "1",
          "time": "20210904 10:23"
        },
        {
          "id": "MDRW2100029843.1.3",
          "form": "전 방청소만 해요",
          "original_form": "전 방청소만 해요",
          "speaker_id": "2",
          "time": "20210904 10:46"
        },
        {
          "id": "MDRW2100029843.1.4",
          "form": "빨래 개는거랑요",
          "original_form": "빨래 개는거랑요",
          "speaker_id": "2",
          "time": "20210904 10:46"
        },
        {
          "id": "MDRW2100029843.1.5",
          "form": "그쪽은요??",
          "original_form": "그쪽은요??",
          "speaker_id": "2",
          "time": "20210904 10:46"
        },
        {
          "id": "MDRW2100029843.1.6",
          "form": "화장실전담이요",
          "original_form": "화장실전담이요",
          "speaker_id": "1",
          "time": "20210904 10:47"
        },
        {
          "id": "MDRW2100029843.1.7",
          "form": "오 화장실청소 자주하세요?",
          "original_form": "오 화장실청소 자주하세요?",
          "speaker_id": "2",
          "time": "20210904 10:47"
        },
        {
          "id": "MDRW2100029843.1.8",
          "form": "일주일에한번씩요",
          "original_form": "일주일에한번씩요",
          "speaker_id": "1",
          "time": "20210904 10:50"
        },
        {
          "id": "MDRW2100029843.1.9",
          "form": "그거 말고는 아무것도 안하세요?",
          "original_form": "그거 말고는 아무것도 안하세요?",
          "speaker_id": "2",
          "time": "20210904 10:53"
        },
        {
          "id": "MDRW2100029843.1.10",
          "form": "그렇됴",
          "original_form": "그렇됴",
          "speaker_id": "1",
          "time": "20210904 10:54"
        },
        {
          "id": "MDRW2100029843.1.11",
          "form": "전 설거지가 제일 싫어요",
          "original_form": "전 설거지가 제일 싫어요",
          "speaker_id": "2",
          "time": "20210904 10:54"
        },
        {
          "id": "MDRW2100029843.1.12",
          "form": "가끔 엄마 도와드리는데",
          "original_form": "가끔 엄마 도와드리는데",
          "speaker_id": "2",
          "time": "20210904 10:54"
        },
        {
          "id": "MDRW2100029843.1.13",
          "form": "너무 싫어요ㅠㅠㅠ",
          "original_form": "너무 싫어요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 10:54"
        },
        {
          "id": "MDRW2100029843.1.14",
          "form": "전 요릴를 잘 못해요",
          "original_form": "전 요릴를 잘 못해요",
          "speaker_id": "1",
          "time": "20210904 10:57"
        },
        {
          "id": "MDRW2100029843.1.15",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210904 10:58"
        },
        {
          "id": "MDRW2100029843.1.16",
          "form": "요리하면 가족들 아무도 안먹더라구요",
          "original_form": "요리하면 가족들 아무도 안먹더라구요",
          "speaker_id": "2",
          "time": "20210904 10:58"
        },
        {
          "id": "MDRW2100029843.1.17",
          "form": "요리 그래도 젤 자신있는거",
          "original_form": "요리 그래도 젤 자신있는거",
