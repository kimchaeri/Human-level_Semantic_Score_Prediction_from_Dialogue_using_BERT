      "utterance": [
        {
          "id": "MDRW2100047386.1.1",
          "form": "커피 한잔 하셨어요?",
          "original_form": "커피 한잔 하셨어요?",
          "speaker_id": "1",
          "time": "20210826 14:32"
        },
        {
          "id": "MDRW2100047386.1.2",
          "form": "전화 받다 눌려서 넘어갔네요 ㅜㅜ",
          "original_form": "전화 받다 눌려서 넘어갔네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 14:32"
        },
        {
          "id": "MDRW2100047386.1.3",
          "form": "네 아메리카노 한잔 마셧어요 ㅋㅋㅋㅋ",
          "original_form": "네 아메리카노 한잔 마셧어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:32"
        },
        {
          "id": "MDRW2100047386.1.4",
          "form": "저도 마시고 싶은데",
          "original_form": "저도 마시고 싶은데",
          "speaker_id": "1",
          "time": "20210826 14:33"
        },
        {
          "id": "MDRW2100047386.1.5",
          "form": "딸이 안오네요",
          "original_form": "딸이 안오네요",
          "speaker_id": "1",
          "time": "20210826 14:33"
        },
        {
          "id": "MDRW2100047386.1.6",
          "form": "기계를 애가 내려서요",
          "original_form": "기계를 애가 내려서요",
          "speaker_id": "1",
          "time": "20210826 14:34"
        },
        {
          "id": "MDRW2100047386.1.7",
          "form": "ㅋㅋ아 메 먹고 싶어요",
          "original_form": "ㅋㅋ아 메 먹고 싶어요",
          "speaker_id": "1",
          "time": "20210826 14:34"
        },
        {
          "id": "MDRW2100047386.1.8",
          "form": "커피숍 갈라고 했는데",
          "original_form": "커피숍 갈라고 했는데",
          "speaker_id": "1",
          "time": "20210826 14:34"
        },
        {
          "id": "MDRW2100047386.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ딸이랑 같이 커피한잔 좋네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ딸이랑 같이 커피한잔 좋네요",
          "speaker_id": "2",
          "time": "20210826 14:34"
        },
        {
          "id": "MDRW2100047386.1.10",
          "form": "여태 안가고 있어요",
          "original_form": "여태 안가고 있어요",
          "speaker_id": "1",
          "time": "20210826 14:34"
        },
        {
          "id": "MDRW2100047386.1.11",
          "form": "아 딸은 내려만줘요",
          "original_form": "아 딸은 내려만줘요",
          "speaker_id": "1",
          "time": "20210826 14:34"
        },
        {
          "id": "MDRW2100047386.1.12",
          "form": "정다운 모녀지간이네요",
          "original_form": "정다운 모녀지간이네요",
          "speaker_id": "2",
          "time": "20210826 14:34"
        },
        {
          "id": "MDRW2100047386.1.13",
          "form": "제가 기계치 입니다 ㅎㅎ",
          "original_form": "제가 기계치 입니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 14:34"
        },
        {
          "id": "MDRW2100047386.1.14",
          "form": "반전은 꼭",
          "original_form": "반전은 꼭",
          "speaker_id": "1",
          "time": "20210826 14:34"
        },
        {
          "id": "MDRW2100047386.1.15",
          "form": "찬원 받아요 ㅋㅋ",
          "original_form": "찬원 받아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:34"
        },
        {
          "id": "MDRW2100047386.1.16",
          "form": "따님은 그럼 뭘 마시나요?",
          "original_form": "따님은 그럼 뭘 마시나요?",
          "speaker_id": "2",
          "time": "20210826 14:35"
        },
        {
          "id": "MDRW2100047386.1.17",
          "form": "천원",
          "original_form": "천원",
          "speaker_id": "1",
          "time": "20210826 14:35"
        },
        {
          "id": "MDRW2100047386.1.18",
          "form": "음료요",
          "original_form": "음료요",
          "speaker_id": "1",
          "time": "20210826 14:35"
        },
        {
          "id": "MDRW2100047386.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ귀엽",
          "original_form": "ㅋㅋㅋㅋㅋㅋ귀엽",
          "speaker_id": "2",
          "time": "20210826 14:35"
        },
        {
          "id": "MDRW2100047386.1.20",
          "form": "ㅋㅋ어려서요",
          "original_form": "ㅋㅋ어려서요",
          "speaker_id": "1",
          "time": "20210826 14:35"
        },
        {
          "id": "MDRW2100047386.1.21",
          "form": "아주 갑부 되겠어요",
          "original_form": "아주 갑부 되겠어요",
          "speaker_id": "1",
          "time": "20210826 14:35"
        },
        {
          "id": "MDRW2100047386.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ커피 안마시고 싶은데 계속 엄마한테 커피타주셔고 하는거 아니에요? ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ커피 안마시고 싶은데 계속 엄마한테 커피타주셔고 하는거 아니에요? ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:36"
        },
        {
          "id": "MDRW2100047386.1.23",
          "form": "용돈 번다고 일부러 더해요",
          "original_form": "용돈 번다고 일부러 더해요",
          "speaker_id": "1",
          "time": "20210826 14:37"
        },
        {
          "id": "MDRW2100047386.1.24",
          "form": "두번도 내려주고 ㅋㅋ",
          "original_form": "두번도 내려주고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:37"
        },
        {
          "id": "MDRW2100047386.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ귀여워라",
          "original_form": "ㅋㅋㅋㅋㅋㅋ귀여워라",
          "speaker_id": "2",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100047386.1.26",
          "form": "용돈을 딱히 못버니 그걸로 벌어요",
          "original_form": "용돈을 딱히 못버니 그걸로 벌어요",
          "speaker_id": "1",
          "time": "20210826 14:39"
        },
        {
          "id": "MDRW2100047386.1.27",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "1",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100047386.1.28",
          "form": "저 저녁먹고 잠들어서 이제 일어났어요 ㅡㅡ",
          "original_form": "저 저녁먹고 잠들어서 이제 일어났어요 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210826 20:48"
        }
      ]
