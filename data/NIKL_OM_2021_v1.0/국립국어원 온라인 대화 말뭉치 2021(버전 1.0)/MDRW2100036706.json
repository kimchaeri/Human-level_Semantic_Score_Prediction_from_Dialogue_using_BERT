      "utterance": [
        {
          "id": "MDRW2100036706.1.1",
          "form": "ㅋㅋㅋㅋ 여튼 애들 지금 약간",
          "original_form": "ㅋㅋㅋㅋ 여튼 애들 지금 약간",
          "speaker_id": "2",
          "time": "20210831 09:48"
        },
        {
          "id": "MDRW2100036706.1.2",
          "form": "사춘기 1기 정도 되는듯",
          "original_form": "사춘기 1기 정도 되는듯",
          "speaker_id": "2",
          "time": "20210831 09:48"
        },
        {
          "id": "MDRW2100036706.1.3",
          "form": "맞어 ㅋㅋㅋ",
