      "utterance": [
        {
          "id": "MDRW2100012752.1.1",
          "form": "안녕하세요~^^",
          "original_form": "안녕하세요~^^",
          "speaker_id": "1",
          "time": "20210825 00:43"
        },
        {
          "id": "MDRW2100012752.1.2",
          "form": "요즘 날씨 어때요?",
          "original_form": "요즘 날씨 어때요?",
          "speaker_id": "1",
          "time": "20210825 00:43"
        },
        {
          "id": "MDRW2100012752.1.3",
          "form": "날씨가 하루종일 오락가락해요",
          "original_form": "날씨가 하루종일 오락가락해요",
          "speaker_id": "2",
          "time": "20210825 00:44"
        },
        {
          "id": "MDRW2100012752.1.4",
          "form": "name1 지역은 어떤가요",
          "original_form": "&name1& 지역은 어떤가요",
          "speaker_id": "2",
          "time": "20210825 00:44"
        },
        {
          "id": "MDRW2100012752.1.5",
          "form": "전 전남이에요ㅎㅎ",
          "original_form": "전 전남이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 00:46"
        },
        {
          "id": "MDRW2100012752.1.6",
          "form": "어제 태풍불어서 오락가락했죠ㅎ",
          "original_form": "어제 태풍불어서 오락가락했죠ㅎ",
          "speaker_id": "1",
          "time": "20210825 00:46"
        },
        {
          "id": "MDRW2100012752.1.7",
          "form": "아 여기는 수원이요",
          "original_form": "아 여기는 수원이요",
          "speaker_id": "2",
          "time": "20210825 00:46"
        },
        {
          "id": "MDRW2100012752.1.8",
          "form": "부슬비가 내리다 그치다 반복해요",
          "original_form": "부슬비가 내리다 그치다 반복해요",
          "speaker_id": "2",
          "time": "20210825 00:47"
        },
        {
          "id": "MDRW2100012752.1.9",
          "form": "지금은 갰네요",
          "original_form": "지금은 갰네요",
          "speaker_id": "2",
          "time": "20210825 00:47"
        },
        {
          "id": "MDRW2100012752.1.10",
          "form": "아ㅎㅎ 수원이시구나ㅎㅎ",
          "original_form": "아ㅎㅎ 수원이시구나ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 00:47"
        },
        {
          "id": "MDRW2100012752.1.11",
          "form": "날씨가 요즘",
          "original_form": "날씨가 요즘",
          "speaker_id": "1",
          "time": "20210825 00:47"
        },
        {
          "id": "MDRW2100012752.1.12",
          "form": "스콜처럼 내리지않나",
          "original_form": "스콜처럼 내리지않나",
          "speaker_id": "1",
          "time": "20210825 00:47"
        },
        {
          "id": "MDRW2100012752.1.13",
          "form": "태풍이오질 않나 맨날 비네요ㅠ",
          "original_form": "태풍이오질 않나 맨날 비네요ㅠ",
          "speaker_id": "1",
          "time": "20210825 00:47"
        },
        {
          "id": "MDRW2100012752.1.14",
          "form": "기상청도 강수량이나 실시간 예보 감잡기 어려운가봐요ㅜ",
          "original_form": "기상청도 강수량이나 실시간 예보 감잡기 어려운가봐요ㅜ",
          "speaker_id": "2",
          "time": "20210825 00:48"
        },
        {
          "id": "MDRW2100012752.1.15",
          "form": "어쩔수없죠ㅎㅎ 슈퍼컴퓨터인데도 못잡으니ㄹㅎ",
          "original_form": "어쩔수없죠ㅎㅎ 슈퍼컴퓨터인데도 못잡으니ㄹㅎ",
          "speaker_id": "1",
          "time": "20210825 00:49"
        },
        {
          "id": "MDRW2100012752.1.16",
          "form": "그래도 올해는 큰태풍이 없어 다행이에요",
          "original_form": "그래도 올해는 큰태풍이 없어 다행이에요",
          "speaker_id": "2",
          "time": "20210825 00:49"
        },
        {
          "id": "MDRW2100012752.1.17",
          "form": "그래도 일간으로 잡는것도 대단해요ㄹ 예전에는 맨날 틀렸잖아요ㅎ",
          "original_form": "그래도 일간으로 잡는것도 대단해요ㄹ 예전에는 맨날 틀렸잖아요ㅎ",
          "speaker_id": "1",
          "time": "20210825 00:49"
        },
        {
          "id": "MDRW2100012752.1.18",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 00:49"
        },
        {
          "id": "MDRW2100012752.1.19",
          "form": "ㄱㄱㄱㄱㄱ",
          "original_form": "ㄱㄱㄱㄱㄱ",
          "speaker_id": "1",
          "time": "20210825 00:50"
        },
        {
          "id": "MDRW2100012752.1.20",
          "form": "사실 요새도 일간이 잘 맞지는ㅎㅎ",
          "original_form": "사실 요새도 일간이 잘 맞지는ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:51"
        },
        {
          "id": "MDRW2100012752.1.21",
          "form": "ㅎㅎ 맞아요ㅎ 가끔안맞으면",
          "original_form": "ㅎㅎ 맞아요ㅎ 가끔안맞으면",
          "speaker_id": "1",
          "time": "20210825 00:52"
        },
        {
          "id": "MDRW2100012752.1.22",
          "form": "출근할때 낭패보죠ㅎㅎ",
          "original_form": "출근할때 낭패보죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 00:52"
        },
        {
          "id": "MDRW2100012752.1.23",
          "form": "비가올때 옷차림은 어떤게 가장좋을까요?",
          "original_form": "비가올때 옷차림은 어떤게 가장좋을까요?",
          "speaker_id": "1",
          "time": "20210825 00:52"
        },
        {
          "id": "MDRW2100012752.1.24",
          "form": "샌들이 편한데 출근룩이 아니죠ㅜ",
          "original_form": "샌들이 편한데 출근룩이 아니죠ㅜ",
          "speaker_id": "2",
          "time": "20210825 00:53"
        },
        {
          "id": "MDRW2100012752.1.25",
          "form": "어ㅎㅎ 회사는 자유복장아에요?",
          "original_form": "어ㅎㅎ 회사는 자유복장아에요?",
          "speaker_id": "1",
          "time": "20210825 00:54"
        },
        {
          "id": "MDRW2100012752.1.26",
          "form": "저희도 바뀐지 얼마안됫긴한데 수도권은 많이 바뀐걸로알고있는데",
          "original_form": "저희도 바뀐지 얼마안됫긴한데 수도권은 많이 바뀐걸로알고있는데",
          "speaker_id": "1",
          "time": "20210825 00:54"
        },
        {
          "id": "MDRW2100012752.1.27",
          "form": "네 그래도 사람 만나고 그럴땐 조심스러워지니까요",
          "original_form": "네 그래도 사람 만나고 그럴땐 조심스러워지니까요",
          "speaker_id": "2",
          "time": "20210825 00:55"
        },
        {
          "id": "MDRW2100012752.1.28",
          "form": "전 샌들 잘 신고  다니긴 해요",
          "original_form": "전 샌들 잘 신고  다니긴 해요",
          "speaker_id": "2",
          "time": "20210825 00:55"
        },
        {
          "id": "MDRW2100012752.1.29",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:55"
        },
        {
          "id": "MDRW2100012752.1.30",
          "form": "아무래도ㅎㅎ 샌들은 그렇네요ㅎㅎㅎ",
          "original_form": "아무래도ㅎㅎ 샌들은 그렇네요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 00:56"
        },
        {
          "id": "MDRW2100012752.1.31",
          "form": "저도 어제 태풍당직서는데ㅎ 맨발에 슬리퍼여서 아침에 윗분들 오시기 전에 바로 퇴근랬습니자",
          "original_form": "저도 어제 태풍당직서는데ㅎ 맨발에 슬리퍼여서 아침에 윗분들 오시기 전에 바로 퇴근랬습니자",
          "speaker_id": "1",
          "time": "20210825 00:56"
        },
        {
          "id": "MDRW2100012752.1.32",
          "form": "윗분들은 옛날사람들이라 그런거 싫어하셔요ㅠ",
          "original_form": "윗분들은 옛날사람들이라 그런거 싫어하셔요ㅠ",
          "speaker_id": "1",
          "time": "20210825 00:57"
        },
        {
          "id": "MDRW2100012752.1.33",
          "form": "맞아요ㅜ",
          "original_form": "맞아요ㅜ",
          "speaker_id": "2",
          "time": "20210825 00:57"
        },
        {
          "id": "MDRW2100012752.1.34",
          "form": "쿨비즈룩 허용하지만 반바지 금지",
          "original_form": "쿨비즈룩 허용하지만 반바지 금지",
          "speaker_id": "2",
          "time": "20210825 00:57"
        },
        {
          "id": "MDRW2100012752.1.35",
          "form": "라운드티도 별로ㅜ",
          "original_form": "라운드티도 별로ㅜ",
          "speaker_id": "2",
          "time": "20210825 00:58"
        },
        {
          "id": "MDRW2100012752.1.36",
          "form": "그게 무슨 쿨비즈룩인가요",
          "original_form": "그게 무슨 쿨비즈룩인가요",
          "speaker_id": "2",
          "time": "20210825 00:58"
        },
        {
          "id": "MDRW2100012752.1.37",
          "form": "아 그런게 있어요?",
          "original_form": "아 그런게 있어요?",
          "speaker_id": "1",
          "time": "20210825 00:58"
        },
        {
          "id": "MDRW2100012752.1.38",
          "form": "무슨 자율복장이 그런대",
          "original_form": "무슨 자율복장이 그런대",
          "speaker_id": "1",
          "time": "20210825 00:59"
        },
        {
          "id": "MDRW2100012752.1.39",
          "form": "사장님 나오라 해야겟네요ㅎ",
          "original_form": "사장님 나오라 해야겟네요ㅎ",
          "speaker_id": "1",
          "time": "20210825 00:59"
        },
        {
          "id": "MDRW2100012752.1.40",
          "form": "제 직장의 경우 좀ㅎㅎ",
          "original_form": "제 직장의 경우 좀ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:59"
        },
        {
          "id": "MDRW2100012752.1.41",
          "form": "요즘같이 일기예보가 잘발달된 시대에서",
          "original_form": "요즘같이 일기예보가 잘발달된 시대에서",
          "speaker_id": "1",
          "time": "20210825 00:59"
        },
        {
          "id": "MDRW2100012752.1.42",
          "form": "그러게요 너무 고리타분하심",
          "original_form": "그러게요 너무 고리타분하심",
          "speaker_id": "2",
          "time": "20210825 00:59"
        },
