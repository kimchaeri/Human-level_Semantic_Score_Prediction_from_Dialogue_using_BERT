      "utterance": [
        {
          "id": "MDRW2100030959.1.1",
          "form": "주부들 대화의 반은 메뉴걱정이죠..",
          "original_form": "주부들 대화의 반은 메뉴걱정이죠..",
          "speaker_id": "2",
          "time": "20210904 14:49"
        },
        {
          "id": "MDRW2100030959.1.2",
          "form": "그러게나말이어요",
          "original_form": "그러게나말이어요",
          "speaker_id": "1",
          "time": "20210904 14:52"
        },
        {
          "id": "MDRW2100030959.1.3",
          "form": "늘 하던것만 하게되고 ..메뉴돌려막기느낌..?ㅎㅎ",
          "original_form": "늘 하던것만 하게되고 ..메뉴돌려막기느낌..?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 14:54"
        },
        {
          "id": "MDRW2100030959.1.4",
          "form": "그쵸 크크크",
          "original_form": "그쵸 크크크",
          "speaker_id": "1",
          "time": "20210904 14:54"
        },
        {
          "id": "MDRW2100030959.1.5",
          "form": "김 계란 두부 버섯",
          "original_form": "김 계란 두부 버섯",
          "speaker_id": "1",
          "time": "20210904 14:54"
        },
        {
          "id": "MDRW2100030959.1.6",
          "form": "고기",
          "original_form": "고기",
          "speaker_id": "1",
          "time": "20210904 14:54"
        },
        {
          "id": "MDRW2100030959.1.7",
          "form": "멸치 콩나물",
          "original_form": "멸치 콩나물",
          "speaker_id": "1",
          "time": "20210904 14:54"
        },
        {
          "id": "MDRW2100030959.1.8",
          "form": "생선~",
          "original_form": "생선~",
          "speaker_id": "2",
          "time": "20210904 14:55"
        },
        {
          "id": "MDRW2100030959.1.9",
          "form": "굽다가 조리다가",
          "original_form": "굽다가 조리다가",
          "speaker_id": "2",
          "time": "20210904 14:55"
        },
        {
          "id": "MDRW2100030959.1.10",
          "form": "맞아요 굽고 조리고 찌고 탕으로 내고",
          "original_form": "맞아요 굽고 조리고 찌고 탕으로 내고",
          "speaker_id": "1",
          "time": "20210904 14:55"
        },
        {
          "id": "MDRW2100030959.1.11",
          "form": "가족들은 이런  모를거예요.",
          "original_form": "가족들은 이런  모를거예요.",
          "speaker_id": "2",
          "time": "20210904 14:56"
        },
        {
          "id": "MDRW2100030959.1.12",
          "form": "몰라주죠",
          "original_form": "몰라주죠",
          "speaker_id": "1",
          "time": "20210904 14:56"
        },
        {
          "id": "MDRW2100030959.1.13",
          "form": "오늘은 뭐 할거야?",
          "original_form": "오늘은 뭐 할거야?",
          "speaker_id": "2",
          "time": "20210904 14:56"
        },
        {
          "id": "MDRW2100030959.1.14",
          "form": "맛없다 짜다 싱겁다 달다",
          "original_form": "맛없다 짜다 싱겁다 달다",
          "speaker_id": "1",
          "time": "20210904 14:56"
        },
        {
          "id": "MDRW2100030959.1.15",
          "form": "안해주면 그것만으로도 감사",
          "original_form": "안해주면 그것만으로도 감사",
          "speaker_id": "1",
          "time": "20210904 14:56"
        },
        {
          "id": "MDRW2100030959.1.16",
          "form": "이런 질문의 무게감을..",
          "original_form": "이런 질문의 무게감을..",
          "speaker_id": "2",
          "time": "20210904 14:56"
        },
        {
          "id": "MDRW2100030959.1.17",
          "form": "맞아요~",
          "original_form": "맞아요~",
          "speaker_id": "2",
          "time": "20210904 14:57"
        },
        {
          "id": "MDRW2100030959.1.18",
          "form": "ㅋㅋ그니까요",
          "original_form": "ㅋㅋ그니까요",
          "speaker_id": "1",
          "time": "20210904 14:57"
        },
        {
          "id": "MDRW2100030959.1.19",
          "form": "한끼 먹을라믄 시간을 얼마나 들여야하는지..",
          "original_form": "한끼 먹을라믄 시간을 얼마나 들여야하는지..",
          "speaker_id": "2",
          "time": "20210904 14:57"
        },
        {
          "id": "MDRW2100030959.1.20",
          "form": "치우는건 또 어떤지..",
          "original_form": "치우는건 또 어떤지..",
          "speaker_id": "2",
          "time": "20210904 14:57"
        },
        {
          "id": "MDRW2100030959.1.21",
          "form": "음쓰처리에 설거지에",
          "original_form": "음쓰처리에 설거지에",
          "speaker_id": "1",
          "time": "20210904 14:58"
        },
        {
          "id": "MDRW2100030959.1.22",
          "form": "애매하게 남아 버리기도 보관도",
          "original_form": "애매하게 남아 버리기도 보관도",
          "speaker_id": "1",
          "time": "20210904 14:58"
        },
        {
          "id": "MDRW2100030959.1.23",
          "form": "어중간할때 ㅜㅜ",
          "original_form": "어중간할때 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 14:58"
        },
        {
          "id": "MDRW2100030959.1.24",
          "form": "그래서~~~내가 그걸 아까워서 죄다 먹다가 이 몸매되는거지요",
          "original_form": "그래서~~~내가 그걸 아까워서 죄다 먹다가 이 몸매되는거지요",
          "speaker_id": "2",
          "time": "20210904 14:59"
        },
        {
          "id": "MDRW2100030959.1.25",
          "form": "냉장고들어가면 까묵거나,천대받거나 둘중 하나거든요",
          "original_form": "냉장고들어가면 까묵거나,천대받거나 둘중 하나거든요",
          "speaker_id": "2",
          "time": "20210904 14:59"
        },
        {
          "id": "MDRW2100030959.1.26",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210904 15:00"
        },
        {
          "id": "MDRW2100030959.1.27",
          "form": "들어가면 잊어버려서",
          "original_form": "들어가면 잊어버려서",
          "speaker_id": "1",
          "time": "20210904 15:00"
        },
        {
          "id": "MDRW2100030959.1.28",
          "form": "나중에 이게 무슨 통이었지 하고 열어보면",
          "original_form": "나중에 이게 무슨 통이었지 하고 열어보면",
          "speaker_id": "1",
          "time": "20210904 15:00"
        },
        {
          "id": "MDRW2100030959.1.29",
          "form": "그때야 생각이 나는",
          "original_form": "그때야 생각이 나는",
          "speaker_id": "1",
          "time": "20210904 15:00"
        },
        {
          "id": "MDRW2100030959.1.30",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 15:00"
        },
        {
          "id": "MDRW2100030959.1.31",
          "form": "저만 이런게 아니어서 ㅎㅎㅆ",
          "original_form": "저만 이런게 아니어서 ㅎㅎㅆ",
          "speaker_id": "1",
          "time": "20210904 15:00"
        },
        {
          "id": "MDRW2100030959.1.32",
          "form": "저만 이런게 아니라 다행이에요",
          "original_form": "저만 이런게 아니라 다행이에요",
          "speaker_id": "1",
          "time": "20210904 15:01"
        }
      ]
    }
