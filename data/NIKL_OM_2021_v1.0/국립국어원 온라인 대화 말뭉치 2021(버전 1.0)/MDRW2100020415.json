      "utterance": [
        {
          "id": "MDRW2100020415.1.1",
          "form": "해리포터 좋아하니",
          "original_form": "해리포터 좋아하니",
          "speaker_id": "1",
          "time": "20210828 03:11"
        },
        {
          "id": "MDRW2100020415.1.2",
          "form": "안녕 응 책 읽었어",
          "original_form": "안녕 응 책 읽었어",
          "speaker_id": "2",
          "time": "20210828 03:11"
        },
        {
          "id": "MDRW2100020415.1.3",
          "form": "하긴 해리포터 안읽은 사람못봤어",
          "original_form": "하긴 해리포터 안읽은 사람못봤어",
          "speaker_id": "1",
          "time": "20210828 03:12"
        },
        {
          "id": "MDRW2100020415.1.4",
          "form": "영화화 되면서 다시 읽어봤어",
          "original_form": "영화화 되면서 다시 읽어봤어",
          "speaker_id": "2",
          "time": "20210828 03:12"
        },
        {
          "id": "MDRW2100020415.1.5",
          "form": "그 세계관이 너무 재밌어!",
          "original_form": "그 세계관이 너무 재밌어!",
          "speaker_id": "1",
          "time": "20210828 03:12"
        },
        {
          "id": "MDRW2100020415.1.6",
          "form": "마법세계 진짜 흥미로워",
          "original_form": "마법세계 진짜 흥미로워",
          "speaker_id": "2",
          "time": "20210828 03:13"
        },
        {
          "id": "MDRW2100020415.1.7",
          "form": "마법학교가 실존한다면 좋겠다",
          "original_form": "마법학교가 실존한다면 좋겠다",
          "speaker_id": "1",
          "time": "20210828 03:13"
        },
        {
          "id": "MDRW2100020415.1.8",
          "form": "ㅋㅋㅋ어릴때 호그와트 입학편지 기다렸잖아",
          "original_form": "ㅋㅋㅋ어릴때 호그와트 입학편지 기다렸잖아",
          "speaker_id": "2",
          "time": "20210828 03:13"
        },
        {
          "id": "MDRW2100020415.1.9",
          "form": "조앤롤링은 참 뛰어난 작가같아",
          "original_form": "조앤롤링은 참 뛰어난 작가같아",
          "speaker_id": "1",
          "time": "20210828 03:13"
        },
        {
          "id": "MDRW2100020415.1.10",
          "form": "그러고보니 요즘 다른작품 쓰고있지",
          "original_form": "그러고보니 요즘 다른작품 쓰고있지",
          "speaker_id": "2",
          "time": "20210828 03:14"
        },
        {
          "id": "MDRW2100020415.1.11",
          "form": "정말? 소개해줘",
          "original_form": "정말? 소개해줘",
          "speaker_id": "1",
          "time": "20210828 03:14"
        },
        {
          "id": "MDRW2100020415.1.12",
          "form": "신비한 동물 사전!",
          "original_form": "신비한 동물 사전!",
          "speaker_id": "2",
          "time": "20210828 03:15"
        },
        {
          "id": "MDRW2100020415.1.13",
          "form": "아아 들어봤어  동물들임귀엽다던데",
          "original_form": "아아 들어봤어  동물들임귀엽다던데",
          "speaker_id": "1",
          "time": "20210828 03:15"
        },
        {
          "id": "MDRW2100020415.1.14",
          "form": "해리포터 세계관에서 이어져",
          "original_form": "해리포터 세계관에서 이어져",
