      "utterance": [
        {
          "id": "MDRW2100007068.1.1",
          "form": "대학생이신가요?",
          "original_form": "대학생이신가요?",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007068.1.2",
          "form": "아니요~ 추억이네요",
          "original_form": "아니요~ 추억이네요",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007068.1.3",
          "form": "대학생이세요?",
          "original_form": "대학생이세요?",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007068.1.4",
          "form": "저는 대학생이에요 ㅎㅎㅎ",
          "original_form": "저는 대학생이에요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007068.1.5",
          "form": "아직 3학년인데 졸업하고 싶네요 얼른 ㅠㅠㅠ",
          "original_form": "아직 3학년인데 졸업하고 싶네요 얼른 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007068.1.6",
          "form": "아 ㅋㅋㅋㅋ 아직 4학년 남으셨네요",
          "original_form": "아 ㅋㅋㅋㅋ 아직 4학년 남으셨네요",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007068.1.7",
          "form": "대학생활 그립다~~",
          "original_form": "대학생활 그립다~~",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007068.1.8",
          "form": "ㅎㅎㅎ 지금은 직장다니세요?",
          "original_form": "ㅎㅎㅎ 지금은 직장다니세요?",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007068.1.9",
          "form": "네 ㅋㅋㅋ 전 디자인 전공이였어요",
          "original_form": "네 ㅋㅋㅋ 전 디자인 전공이였어요",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007068.1.10",
          "form": "그래서 시험이 늘 실기였었답니다",
          "original_form": "그래서 시험이 늘 실기였었답니다",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007068.1.11",
          "form": "우와 멋져요! 휴학없이 바로 하셨나요?",
          "original_form": "우와 멋져요! 휴학없이 바로 하셨나요?",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007068.1.12",
          "form": "실기 시험 매번 보는거 힘들지 않으셨나요?",
          "original_form": "실기 시험 매번 보는거 힘들지 않으셨나요?",
          "speaker_id": "1",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007068.1.13",
          "form": "휴학은 1번 했어요 후회는 없어요 ㅋㅋㅋ",
          "original_form": "휴학은 1번 했어요 후회는 없어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007068.1.14",
          "form": "휴학 추천하시나요?",
          "original_form": "휴학 추천하시나요?",
          "speaker_id": "1",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007068.1.15",
          "form": "주변에 많이 하시더라고요 ㅎㅎㅎ",
          "original_form": "주변에 많이 하시더라고요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007068.1.16",
          "form": "음.... 저는 추천!",
          "original_form": "음.... 저는 추천!",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007068.1.17",
          "form": "놀기만 해도! 휴학은 대학생때만 가능하니깐요",
          "original_form": "놀기만 해도! 휴학은 대학생때만 가능하니깐요",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007068.1.18",
          "form": "동아리 활동하시나요?",
          "original_form": "동아리 활동하시나요?",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007068.1.19",
          "form": "동아리 활동 1학년 때 하다가 탈동 했어요 ㅎㅎㅎ",
          "original_form": "동아리 활동 1학년 때 하다가 탈동 했어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007068.1.20",
          "form": "저랑 안맞더라고요 그런 문화가ㅠㅠ",
          "original_form": "저랑 안맞더라고요 그런 문화가ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007068.1.21",
          "form": "그럴 수 있죠",
          "original_form": "그럴 수 있죠",
          "speaker_id": "2",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007068.1.22",
          "form": "해봣으면 됬어요 ㅋㅋ",
          "original_form": "해봣으면 됬어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007068.1.23",
          "form": "공부는 할만하시나요?",
          "original_form": "공부는 할만하시나요?",
          "speaker_id": "2",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007068.1.24",
          "form": "공부는 별로 안해서 괜찮은데 ㅎㅎㅎ",
          "original_form": "공부는 별로 안해서 괜찮은데 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007068.1.25",
          "form": "내년에 임용  시험이 걱정이네요 ㅎㅎ",
