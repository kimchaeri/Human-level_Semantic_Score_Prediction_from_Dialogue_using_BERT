      "utterance": [
        {
          "id": "MDRW2100035685.1.1",
          "form": "그리고 난 내몸을 너무사려 ㅋㅋ",
          "original_form": "그리고 난 내몸을 너무사려 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100035685.1.2",
          "form": "엄마한테 납작 엎드려 제가 해보겠슴다 하고",
          "original_form": "엄마한테 납작 엎드려 제가 해보겠슴다 하고",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100035685.1.3",
          "form": "손빨래싫어",
          "original_form": "손빨래싫어",
          "speaker_id": "1",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100035685.1.4",
          "form": "음 나도그래",
          "original_form": "음 나도그래",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100035685.1.5",
          "form": "그냥 세탁기 돌리고파",
          "original_form": "그냥 세탁기 돌리고파",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035685.1.6",
          "form": "그럼 또 엄마는 환장함 ㅋㅋ",
          "original_form": "그럼 또 엄마는 환장함 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035685.1.7",
          "form": "샤워하면서 해",
          "original_form": "샤워하면서 해",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035685.1.8",
          "form": "속 옷 하고 양말",
          "original_form": "속 옷 하고 양말",
          "speaker_id": "2",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100035685.1.9",
          "form": "근데 여름옷은 진짜 스트레스",
          "original_form": "근데 여름옷은 진짜 스트레스",
          "speaker_id": "1",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100035685.1.10",
          "form": "내가 빨면 힘든데",
          "original_form": "내가 빨면 힘든데",
          "speaker_id": "1",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100035685.1.11",
          "form": "냄새나",
          "original_form": "냄새나",
          "speaker_id": "1",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100035685.1.12",
          "form": "세탁기돌려",
          "original_form": "세탁기돌려",
          "speaker_id": "2",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100035685.1.13",
          "form": "엄마가 싫어함",
          "original_form": "엄마가 싫어함",
          "speaker_id": "1",
          "time": "20210829 22:31"
        },
        {
          "id": "MDRW2100035685.1.14",
          "form": "ㅋㅋ에잉",
          "original_form": "ㅋㅋ에잉",
          "speaker_id": "2",
          "time": "20210829 22:31"
        },
        {
          "id": "MDRW2100035685.1.15",
          "form": "여름옷은 천이 약하니 꼭 손빨래해야한다",
          "original_form": "여름옷은 천이 약하니 꼭 손빨래해야한다",
          "speaker_id": "1",
          "time": "20210829 22:31"
        },
        {
          "id": "MDRW2100035685.1.16",
          "form": "아 글쿠낭",
          "original_form": "아 글쿠낭",
          "speaker_id": "2",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100035685.1.17",
          "form": "집의 규율이구나",
          "original_form": "집의 규율이구나",
          "speaker_id": "2",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100035685.1.18",
          "form": "아니 엄마의 스타일 ㅋㅋ",
          "original_form": "아니 엄마의 스타일 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100035685.1.19",
          "form": "운동겸 열심히해 부모님과 집에 있으니",
          "original_form": "운동겸 열심히해 부모님과 집에 있으니",
          "speaker_id": "2",
          "time": "20210829 22:32"
        },
        {
