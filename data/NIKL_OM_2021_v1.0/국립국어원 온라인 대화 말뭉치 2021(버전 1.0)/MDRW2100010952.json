      "utterance": [
        {
          "id": "MDRW2100010952.1.1",
          "form": "어떤 음식 좋아하세요?",
          "original_form": "어떤 음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 23:12"
        },
        {
          "id": "MDRW2100010952.1.2",
          "form": "저는 얼큰한 음식 좋아해요",
          "original_form": "저는 얼큰한 음식 좋아해요",
          "speaker_id": "2",
          "time": "20210823 23:12"
        },
        {
          "id": "MDRW2100010952.1.3",
          "form": "마라탕 최애예요",
          "original_form": "마라탕 최애예요",
          "speaker_id": "2",
          "time": "20210823 23:13"
        },
        {
          "id": "MDRW2100010952.1.4",
          "form": "name1님은 뭐 좋아하세요",
          "original_form": "&name1&님은 뭐 좋아하세요",
          "speaker_id": "2",
          "time": "20210823 23:13"
        },
        {
          "id": "MDRW2100010952.1.5",
          "form": "와 대박",
          "original_form": "와 대박",
          "speaker_id": "1",
          "time": "20210823 23:13"
        },
        {
          "id": "MDRW2100010952.1.6",
          "form": "저 마라탕 진짜 좋아해요",
          "original_form": "저 마라탕 진짜 좋아해요",
          "speaker_id": "1",
          "time": "20210823 23:13"
        },
        {
          "id": "MDRW2100010952.1.7",
          "form": "헉 통했네요",
          "original_form": "헉 통했네요",
          "speaker_id": "2",
          "time": "20210823 23:14"
        },
        {
          "id": "MDRW2100010952.1.8",
          "form": "그리고 버블티도 좋아합니당",
          "original_form": "그리고 버블티도 좋아합니당",
          "speaker_id": "1",
          "time": "20210823 23:14"
        },
        {
          "id": "MDRW2100010952.1.9",
          "form": "ㅎㅎ 저 흑당버블티 광인인데",
          "original_form": "ㅎㅎ 저 흑당버블티 광인인데",
          "speaker_id": "2",
          "time": "20210823 23:14"
        },
        {
          "id": "MDRW2100010952.1.10",
          "form": "매운거 먹고 단거 먹으면 천국",
          "original_form": "매운거 먹고 단거 먹으면 천국",
          "speaker_id": "2",
          "time": "20210823 23:14"
        },
        {
          "id": "MDRW2100010952.1.11",
          "form": "아... 맛잘알",
          "original_form": "아... 맛잘알",
          "speaker_id": "1",
          "time": "20210823 23:15"
        },
        {
          "id": "MDRW2100010952.1.12",
          "form": "마라샹궈도 드세요?",
          "original_form": "마라샹궈도 드세요?",
          "speaker_id": "1",
          "time": "20210823 23:15"
        },
        {
          "id": "MDRW2100010952.1.13",
          "form": "네네 완전 밥도둑ㅜㅜ",
          "original_form": "네네 완전 밥도둑ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 23:15"
        },
        {
          "id": "MDRW2100010952.1.14",
          "form": "소스도 사서 집에서 해봤는데 이프로 부족하더라고요",
          "original_form": "소스도 사서 집에서 해봤는데 이프로 부족하더라고요",
          "speaker_id": "2",
          "time": "20210823 23:15"
        },
        {
          "id": "MDRW2100010952.1.15",
          "form": "재료가 안 다양해서 그런가",
          "original_form": "재료가 안 다양해서 그런가",
          "speaker_id": "2",
          "time": "20210823 23:16"
        },
        {
          "id": "MDRW2100010952.1.16",
          "form": "오 집에서도 해드시다니",
          "original_form": "오 집에서도 해드시다니",
          "speaker_id": "1",
          "time": "20210823 23:16"
        },
        {
          "id": "MDRW2100010952.1.17",
          "form": "진정한 마라러버로 인정해드립니다",
          "original_form": "진정한 마라러버로 인정해드립니다",
          "speaker_id": "1",
          "time": "20210823 23:16"
        },
        {
          "id": "MDRW2100010952.1.18",
          "form": "건대에 매운향솥 괜찮더라구요",
          "original_form": "건대에 매운향솥 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210823 23:16"
        },
        {
          "id": "MDRW2100010952.1.19",
          "form": "ㅎㅎ 소스가 잘 나오더라고요.",
          "original_form": "ㅎㅎ 소스가 잘 나오더라고요.",
          "speaker_id": "2",
          "time": "20210823 23:16"
        },
        {
          "id": "MDRW2100010952.1.20",
          "form": "오오 한번 가봐야겠어요",
          "original_form": "오오 한번 가봐야겠어요",
          "speaker_id": "2",
          "time": "20210823 23:17"
        },
        {
          "id": "MDRW2100010952.1.21",
          "form": "전 부드러운맛은 라화쿵부 매운맛은 라향각 좋아해요",
          "original_form": "전 부드러운맛은 라화쿵부 매운맛은 라향각 좋아해요",
          "speaker_id": "2",
          "time": "20210823 23:17"
        },
        {
          "id": "MDRW2100010952.1.22",
          "form": "매운향솥 이름부터 당기네요",
          "original_form": "매운향솥 이름부터 당기네요",
          "speaker_id": "2",
          "time": "20210823 23:17"
        },
        {
          "id": "MDRW2100010952.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ이렇게 맛으로 나누는게 너무 찐 같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ이렇게 맛으로 나누는게 너무 찐 같아요",
          "speaker_id": "1",
          "time": "20210823 23:17"
        },
        {
          "id": "MDRW2100010952.1.24",
          "form": "버블티..먹고싶네요 흑당버블티 안 먹은지 오랜데",
          "original_form": "버블티..먹고싶네요 흑당버블티 안 먹은지 오랜데",
          "speaker_id": "1",
          "time": "20210823 23:18"
        },
        {
          "id": "MDRW2100010952.1.25",
          "form": "어디가 젤 맛있나요",
          "original_form": "어디가 젤 맛있나요",
          "speaker_id": "1",
          "time": "20210823 23:18"
        },
        {
          "id": "MDRW2100010952.1.26",
          "form": "저는 공차는 입에 잘 맞아요ㅜ",
          "original_form": "저는 공차는 입에 잘 맞아요ㅜ",
          "speaker_id": "2",
          "time": "20210823 23:18"
        },
        {
          "id": "MDRW2100010952.1.27",
          "form": "집근처 일리터만 배달하는 곳이 있는데 거기가 좋더라고요",
          "original_form": "집근처 일리터만 배달하는 곳이 있는데 거기가 좋더라고요",
          "speaker_id": "2",
          "time": "20210823 23:18"
        },
        {
          "id": "MDRW2100010952.1.28",
          "form": "덕분에 과음하죠",
          "original_form": "덕분에 과음하죠",
          "speaker_id": "2",
          "time": "20210823 23:19"
        },
        {
          "id": "MDRW2100010952.1.29",
          "form": "버블티 과음이라 ..",
          "original_form": "버블티 과음이라 ..",
          "speaker_id": "1",
          "time": "20210823 23:19"
        },
        {
          "id": "MDRW2100010952.1.30",
          "form": "완벽하네요",
          "original_form": "완벽하네요",
          "speaker_id": "1",
          "time": "20210823 23:19"
        },
        {
          "id": "MDRW2100010952.1.31",
          "form": "오리지널은 주변은 타로밀크티 좋아하던데 전 블랙쪽이 더 좋더라구여",
          "original_form": "오리지널은 주변은 타로밀크티 좋아하던데 전 블랙쪽이 더 좋더라구여",
          "speaker_id": "1",
          "time": "20210823 23:19"
        },
        {
          "id": "MDRW2100010952.1.32",
          "form": "그쵸. 그래도 타피오카는 꼭 추가하죠. 이게 생명인데.",
          "original_form": "그쵸. 그래도 타피오카는 꼭 추가하죠. 이게 생명인데.",
          "speaker_id": "2",
          "time": "20210823 23:19"
        },
        {
          "id": "MDRW2100010952.1.33",
          "form": "저도 타로는 미묘하게 텁텁해요",
          "original_form": "저도 타로는 미묘하게 텁텁해요",
          "speaker_id": "2",
          "time": "20210823 23:20"
        },
        {
          "id": "MDRW2100010952.1.34",
          "form": "그냥 블랙티가 젤 어울리네요",
          "original_form": "그냥 블랙티가 젤 어울리네요",
          "speaker_id": "2",
          "time": "20210823 23:20"
        },
        {
          "id": "MDRW2100010952.1.35",
          "form": "1리터 짜리면 펄 아주 가득가득",
          "original_form": "1리터 짜리면 펄 아주 가득가득",
          "speaker_id": "1",
          "time": "20210823 23:21"
        },
        {
          "id": "MDRW2100010952.1.36",
          "form": "상상만해도 좋아요",
          "original_form": "상상만해도 좋아요",
          "speaker_id": "1",
          "time": "20210823 23:21"
        },
        {
          "id": "MDRW2100010952.1.37",
          "form": "사실상 펄 때문에 먹는 것...",
          "original_form": "사실상 펄 때문에 먹는 것...",
          "speaker_id": "1",
          "time": "20210823 23:21"
        },
        {
          "id": "MDRW2100010952.1.38",
          "form": "배불러도 펄은 빼놓을 수 없죠ㅎ",
          "original_form": "배불러도 펄은 빼놓을 수 없죠ㅎ",
          "speaker_id": "2",
          "time": "20210823 23:21"
        },
        {
          "id": "MDRW2100010952.1.39",
          "form": "전에 실수로 펄없이 시켰더니 완전 허전ㅜㅜ",
          "original_form": "전에 실수로 펄없이 시켰더니 완전 허전ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 23:22"
        },
        {
          "id": "MDRW2100010952.1.40",
          "form": "맞아요 ㅋㅋㅋㅋ ㅠ",
          "original_form": "맞아요 ㅋㅋㅋㅋ ㅠ",
          "speaker_id": "1",
          "time": "20210823 23:23"
        }
