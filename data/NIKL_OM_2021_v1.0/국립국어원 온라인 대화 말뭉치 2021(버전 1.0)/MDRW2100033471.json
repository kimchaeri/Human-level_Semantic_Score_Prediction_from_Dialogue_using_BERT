        {
          "id": "MDRW2100033471.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210823 23:55"
        },
        {
          "id": "MDRW2100033471.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210823 23:55"
        },
        {
          "id": "MDRW2100033471.1.4",
          "form": "제수타일 이예요 ㅎㅎ",
          "original_form": "제수타일 이예요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 23:55"
        },
        {
          "id": "MDRW2100033471.1.5",
          "form": "너무 머쪄요",
          "original_form": "너무 머쪄요",
          "speaker_id": "1",
          "time": "20210823 23:55"
        },
        {
          "id": "MDRW2100033471.1.6",
          "form": "그리고 그 영상미도 보는 재미가 있어요",
          "original_form": "그리고 그 영상미도 보는 재미가 있어요",
          "speaker_id": "2",
          "time": "20210823 23:56"
        },
        {
          "id": "MDRW2100033471.1.7",
          "form": "나는 미쿡이야 ㅋㅋㅋ",
          "original_form": "나는 미쿡이야 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:56"
        },
        {
          "id": "MDRW2100033471.1.8",
          "form": "끄덕끄덕",
          "original_form": "끄덕끄덕",
          "speaker_id": "1",
          "time": "20210823 23:56"
        },
        {
          "id": "MDRW2100033471.1.9",
          "form": "극공감해요",
          "original_form": "극공감해요",
          "speaker_id": "1",
          "time": "20210823 23:56"
        },
        {
          "id": "MDRW2100033471.1.10",
          "form": "마음만은 캘리포니아였죠",
          "original_form": "마음만은 캘리포니아였죠",
          "speaker_id": "2",
          "time": "20210823 23:57"
        },
        {
          "id": "MDRW2100033471.1.11",
          "form": "그냥 좀 파란바다인데 뭔가 이국적인 ㅋㅋ",
          "original_form": "그냥 좀 파란바다인데 뭔가 이국적인 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:57"
        },
        {
          "id": "MDRW2100033471.1.12",
          "form": "아 브라운신부 영드 추천이요 ㅎㅎ",
          "original_form": "아 브라운신부 영드 추천이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 23:57"
        },
        {
          "id": "MDRW2100033471.1.13",
          "form": "웨이브 같은데서 보실수 있어요",
          "original_form": "웨이브 같은데서 보실수 있어요",
          "speaker_id": "2",
          "time": "20210823 23:57"
        },
        {
          "id": "MDRW2100033471.1.14",
          "form": "오~ 수사극이예요?",
          "original_form": "오~ 수사극이예요?",
          "speaker_id": "1",
          "time": "20210823 23:58"
        },
        {
          "id": "MDRW2100033471.1.15",
          "form": "말그대로 파더브라운이 수사해요",
          "original_form": "말그대로 파더브라운이 수사해요",
          "speaker_id": "2",
          "time": "20210824 00:00"
        },
        {
          "id": "MDRW2100033471.1.16",
          "form": "신부님이요",
          "original_form": "신부님이요",
          "speaker_id": "2",
          "time": "20210824 00:00"
        },
        {
          "id": "MDRW2100033471.1.17",
          "form": "배경이 50년대 영국이에요",
          "original_form": "배경이 50년대 영국이에요",
          "speaker_id": "2",
          "time": "20210824 00:00"
        },
        {
          "id": "MDRW2100033471.1.18",
          "form": "고전고전한 맛이있어요 ㅋㅋ",
          "original_form": "고전고전한 맛이있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 00:00"
        },
        {
          "id": "MDRW2100033471.1.19",
          "form": "오~~~ 완전 땡기네요",
          "original_form": "오~~~ 완전 땡기네요",
          "speaker_id": "1",
          "time": "20210824 00:00"
        },
        {
          "id": "MDRW2100033471.1.20",
          "form": "이것도 시리즈 끝난거예요?",
          "original_form": "이것도 시리즈 끝난거예요?",
          "speaker_id": "1",
          "time": "20210824 00:00"
        },
        {
          "id": "MDRW2100033471.1.21",
          "form": "csi랑 완전 반대로수사한다고 보심되요",
          "original_form": "csi랑 완전 반대로수사한다고 보심되요",
          "speaker_id": "2",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100033471.1.22",
          "form": "이거 시즌 8까지나왔고",
          "original_form": "이거 시즌 8까지나왔고",
          "speaker_id": "2",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100033471.1.23",
          "form": "아직완결안났어요",
          "original_form": "아직완결안났어요",
          "speaker_id": "2",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100033471.1.24",
          "form": "시즌8까지요?",
          "original_form": "시즌8까지요?",
          "speaker_id": "1",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100033471.1.25",
          "form": "대박 잼있나보네요",
          "original_form": "대박 잼있나보네요",
          "speaker_id": "1",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100033471.1.26",
          "form": "메모메모",
          "original_form": "메모메모",
          "speaker_id": "1",
          "time": "20210824 00:02"
        },
        {
          "id": "MDRW2100033471.1.27",
          "form": "에피가 10개나 15개라서요",
          "original_form": "에피가 10개나 15개라서요",
          "speaker_id": "2",
          "time": "20210824 00:05"
        },
        {
          "id": "MDRW2100033471.1.28",
          "form": "원래 21년에 시즌9예정이었는데",
          "original_form": "원래 21년에 시즌9예정이었는데",
          "speaker_id": "2",
          "time": "20210824 00:05"
        },
        {
          "id": "MDRW2100033471.1.29",
          "form": "미뤄졌어요",
          "original_form": "미뤄졌어요",
          "speaker_id": "2",
          "time": "20210824 00:05"
        },
        {
          "id": "MDRW2100033471.1.30",
          "form": "기대되요",
          "original_form": "기대되요",
          "speaker_id": "1",
          "time": "20210824 00:06"
        },
        {
          "id": "MDRW2100033471.1.31",
          "form": "넷플부터찾아봐야겠어요",
          "original_form": "넷플부터찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210824 00:07"
        },
        {
          "id": "MDRW2100033471.1.32",
          "form": "볼것이 많아지네요 ㅎ",
          "original_form": "볼것이 많아지네요 ㅎ",
          "speaker_id": "1",
          "time": "20210824 00:07"
        },
        {
          "id": "MDRW2100033471.1.33",
          "form": "극중 티비없어서 라디오들어요",
          "original_form": "극중 티비없어서 라디오들어요",
          "speaker_id": "2",
          "time": "20210824 00:08"
        },
        {
          "id": "MDRW2100033471.1.34",
          "form": "와 ㅋㄱ",
          "original_form": "와 ㅋㄱ",
          "speaker_id": "2",
          "time": "20210824 00:09"
        },
        {
          "id": "MDRW2100033471.1.35",
          "form": "그리고 곱슬곱슬한 머리에예쁜모자쓰고나오고요 모자핀으로 고정해서",
          "original_form": "그리고 곱슬곱슬한 머리에예쁜모자쓰고나오고요 모자핀으로 고정해서",
          "speaker_id": "2",
          "time": "20210824 00:09"
        },
        {
          "id": "MDRW2100033471.1.36",
          "form": "저는 출퇴근에 라디오 한번씩들어요 ㅎㅎ",
          "original_form": "저는 출퇴근에 라디오 한번씩들어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 00:09"
        },
        {
          "id": "MDRW2100033471.1.37",
          "form": "전이제 자러갈게요 굳밤용",
          "original_form": "전이제 자러갈게요 굳밤용",
