      "utterance": [
        {
          "id": "MDRW2100005226.1.1",
          "form": "여행 너무 좋아요",
          "original_form": "여행 너무 좋아요",
          "speaker_id": "2",
          "time": "20210822 01:54"
        },
        {
          "id": "MDRW2100005226.1.2",
          "form": "여행계획을 짜는 것만으로도 행복해요",
          "original_form": "여행계획을 짜는 것만으로도 행복해요",
          "speaker_id": "2",
          "time": "20210822 01:55"
        },
        {
          "id": "MDRW2100005226.1.3",
          "form": "여행계획...?",
          "original_form": "여행계획...?",
          "speaker_id": "1",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100005226.1.4",
          "form": "혹시 답사여행 좋아하세요?",
          "original_form": "혹시 답사여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100005226.1.5",
          "form": "유적같은거",
          "original_form": "유적같은거",
          "speaker_id": "1",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100005226.1.6",
          "form": "여름이나 더운나라만 아니면 좋을거같네요",
          "original_form": "여름이나 더운나라만 아니면 좋을거같네요",
          "speaker_id": "2",
          "time": "20210822 12:49"
        },
        {
          "id": "MDRW2100005226.1.7",
          "form": "박물관 보는거 좋아요",
          "original_form": "박물관 보는거 좋아요",
          "speaker_id": "2",
          "time": "20210822 12:49"
        },
        {
          "id": "MDRW2100005226.1.8",
          "form": "이럴수가 저도요!!!!!",
          "original_form": "이럴수가 저도요!!!!!",
          "speaker_id": "1",
          "time": "20210822 13:14"
        },
        {
          "id": "MDRW2100005226.1.9",
          "form": "해외여행 딱 한번 갓는디",
          "original_form": "해외여행 딱 한번 갓는디",
          "speaker_id": "1",
          "time": "20210822 13:14"
        },
        {
          "id": "MDRW2100005226.1.10",
          "form": "첫날은 박물관에서 안나갔어요",
          "original_form": "첫날은 박물관에서 안나갔어요",
          "speaker_id": "1",
          "time": "20210822 13:14"
        },
        {
          "id": "MDRW2100005226.1.11",
          "form": "와 정말 좋아하시나봐요",
          "original_form": "와 정말 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210822 13:17"
        },
        {
          "id": "MDRW2100005226.1.12",
          "form": "코로나 종식되면 가고싶네요ㅜㅠ",
          "original_form": "코로나 종식되면 가고싶네요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210822 13:17"
        },
        {
          "id": "MDRW2100005226.1.13",
          "form": "코로나..!!",
          "original_form": "코로나..!!",
          "speaker_id": "1",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100005226.1.14",
          "form": "세계 박물관여행 갑시다",
          "original_form": "세계 박물관여행 갑시다",
          "speaker_id": "1",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100005226.1.15",
          "form": "언젠가",
          "original_form": "언젠가",
          "speaker_id": "1",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100005226.1.16",
          "form": "노년도 좋아",
          "original_form": "노년도 좋아",
          "speaker_id": "1",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100005226.1.17",
          "form": "약속해",
          "original_form": "약속해",
          "speaker_id": "1",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100005226.1.18",
          "form": "약쏙~~!~!",
          "original_form": "약쏙~~!~!",
          "speaker_id": "2",
          "time": "20210822 16:15"
        },
        {
          "id": "MDRW2100005226.1.19",
          "form": "좋아요 세계일주 갑시다",
          "original_form": "좋아요 세계일주 갑시다",
          "speaker_id": "2",
          "time": "20210822 16:15"
        },
        {
          "id": "MDRW2100005226.1.20",
          "form": "할머니돼서 간다면",
          "original_form": "할머니돼서 간다면",
          "speaker_id": "1",
          "time": "20210823 20:21"
        },
        {
          "id": "MDRW2100005226.1.21",
          "form": "전용기로 갈수있게",
          "original_form": "전용기로 갈수있게",
          "speaker_id": "1",
          "time": "20210823 20:21"
        },
        {
          "id": "MDRW2100005226.1.22",
          "form": "재력을 쌓아둡시다",
          "original_form": "재력을 쌓아둡시다",
          "speaker_id": "1",
          "time": "20210823 20:21"
        },
        {
          "id": "MDRW2100005226.1.23",
          "form": "킄킄킄",
          "original_form": "킄킄킄",
          "speaker_id": "1",
          "time": "20210823 20:21"
        },
        {
          "id": "MDRW2100005226.1.24",
          "form": "와 좋아요",
          "original_form": "와 좋아요",
          "speaker_id": "2",
          "time": "20210824 16:03"
        },
        {
          "id": "MDRW2100005226.1.25",
          "form": "생각만해도 좋네요 쿸쿸쿸",
          "original_form": "생각만해도 좋네요 쿸쿸쿸",
          "speaker_id": "2",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100005226.1.26",
          "form": "여행 넘 가고싶어요 안그래도 날씨가 궃어서",
          "original_form": "여행 넘 가고싶어요 안그래도 날씨가 궃어서",
          "speaker_id": "2",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100005226.1.27",
          "form": "밖에 나가기좀 그래서 답답해요",
          "original_form": "밖에 나가기좀 그래서 답답해요",
          "speaker_id": "2",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100005226.1.28",
          "form": "킄킄킄 전 여행대신으로",
          "original_form": "킄킄킄 전 여행대신으로",
          "speaker_id": "1",
          "time": "20210824 17:13"
        },
        {
          "id": "MDRW2100005226.1.29",
          "form": "헬스장투어",
          "original_form": "헬스장투어",
          "speaker_id": "1",
          "time": "20210824 17:13"
        },
        {
          "id": "MDRW2100005226.1.30",
          "form": "나왔읍니다",
          "original_form": "나왔읍니다",
          "speaker_id": "1",
          "time": "20210824 17:13"
        },
        {
          "id": "MDRW2100005226.1.31",
          "form": "여행가려면 돈도 돈이지만",
          "original_form": "여행가려면 돈도 돈이지만",
          "speaker_id": "1",
          "time": "20210824 17:13"
        },
        {
          "id": "MDRW2100005226.1.32",
          "form": "근육도 비축해야만!",
          "original_form": "근육도 비축해야만!",
          "speaker_id": "1",
          "time": "20210824 17:14"
        },
        {
          "id": "MDRW2100005226.1.33",
          "form": "와우 멋져요 하기야 돈만 있다고 여행갈수있는건 아니죠..",
          "original_form": "와우 멋져요 하기야 돈만 있다고 여행갈수있는건 아니죠..",
          "speaker_id": "2",
          "time": "20210824 19:47"
        },
        {
          "id": "MDRW2100005226.1.34",
          "form": "체력도 뒷받침되어야!!!",
          "original_form": "체력도 뒷받침되어야!!!",
          "speaker_id": "2",
          "time": "20210824 19:47"
        },
        {
          "id": "MDRW2100005226.1.35",
          "form": "저도 여행을 위해 열심히 운동해야겠어요",
          "original_form": "저도 여행을 위해 열심히 운동해야겠어요",
          "speaker_id": "2",
          "time": "20210824 19:47"
        },
        {
          "id": "MDRW2100005226.1.36",
          "form": "돈과 근육이 있는 멋진 노년..",
          "original_form": "돈과 근육이 있는 멋진 노년..",
          "speaker_id": "1",
          "time": "20210825 14:48"
        },
        {
          "id": "MDRW2100005226.1.37",
          "form": "생각만해도 행복해요",
          "original_form": "생각만해도 행복해요",
          "speaker_id": "1",
          "time": "20210825 14:48"
        },
        {
          "id": "MDRW2100005226.1.38",
          "form": "돈과 근육만 있다면 정말 뭐든 할수있잖아",
          "original_form": "돈과 근육만 있다면 정말 뭐든 할수있잖아",
          "speaker_id": "1",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100005226.1.39",
          "form": "역시 이집트 답사여행도 가야만",
          "original_form": "역시 이집트 답사여행도 가야만",
          "speaker_id": "1",
          "time": "20210825 14:49"
        },
        {
          "id": "MDRW2100005226.1.40",
          "form": "와 이집트 너무 멋져요",
          "original_form": "와 이집트 너무 멋져요",
          "speaker_id": "2",
          "time": "20210825 17:10"
        },
        {
          "id": "MDRW2100005226.1.41",
          "form": "피라미드 스핑크스!!",
          "original_form": "피라미드 스핑크스!!",
          "speaker_id": "2",
          "time": "20210825 17:10"
        },
        {
          "id": "MDRW2100005226.1.42",
          "form": "돈모아서 꼭 세계일주..",
          "original_form": "돈모아서 꼭 세계일주..",
          "speaker_id": "2",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100005226.1.43",
          "form": "체력관리도 꼭..!!!",
          "original_form": "체력관리도 꼭..!!!",
          "speaker_id": "2",
          "time": "20210825 17:12"
        }
      ]
    }
