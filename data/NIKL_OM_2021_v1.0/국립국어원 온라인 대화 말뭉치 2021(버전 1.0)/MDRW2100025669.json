      "utterance": [
        {
          "id": "MDRW2100025669.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 15:41"
        },
        {
          "id": "MDRW2100025669.1.2",
          "form": "안녕하세요:)",
          "original_form": "안녕하세요:)",
          "speaker_id": "2",
          "time": "20210901 15:42"
        },
        {
          "id": "MDRW2100025669.1.3",
          "form": "가장 최근에 다녀온\n여행지가 어디세요?",
          "original_form": "가장 최근에 다녀온\n여행지가 어디세요?",
          "speaker_id": "2",
          "time": "20210901 15:42"
        },
        {
          "id": "MDRW2100025669.1.4",
          "form": "저는 봄에 서산에 간게 마지막이에요",
          "original_form": "저는 봄에 서산에 간게 마지막이에요",
          "speaker_id": "1",
          "time": "20210901 15:42"
        },
        {
          "id": "MDRW2100025669.1.5",
          "form": "최근에 어디 다녀오신곳 있나요ㅡ",
          "original_form": "최근에 어디 다녀오신곳 있나요ㅡ",
          "speaker_id": "1",
          "time": "20210901 15:43"
        },
        {
          "id": "MDRW2100025669.1.6",
          "form": "속초에 다녀왔었어요^^",
          "original_form": "속초에 다녀왔었어요^^",
          "speaker_id": "2",
          "time": "20210901 15:45"
        },
        {
          "id": "MDRW2100025669.1.7",
          "form": "서산은 어떠셨어요?",
          "original_form": "서산은 어떠셨어요?",
          "speaker_id": "2",
          "time": "20210901 15:45"
        },
        {
          "id": "MDRW2100025669.1.8",
          "form": "청벚꽃 구경하고왔었어요",
          "original_form": "청벚꽃 구경하고왔었어요",
          "speaker_id": "1",
          "time": "20210901 16:14"
        },
        {
          "id": "MDRW2100025669.1.9",
          "form": "우와 좋으셨겠어요:)",
          "original_form": "우와 좋으셨겠어요:)",
          "speaker_id": "2",
          "time": "20210901 16:23"
        },
        {
          "id": "MDRW2100025669.1.10",
          "form": "맛집들도 다녀오셨나요?",
          "original_form": "맛집들도 다녀오셨나요?",
          "speaker_id": "2",
          "time": "20210901 16:23"
        },
        {
          "id": "MDRW2100025669.1.11",
          "form": "아니요  사람들이 많아서  그냥 벚꽃만 구경하고 왔어요 ㅎㅎ",
          "original_form": "아니요  사람들이 많아서  그냥 벚꽃만 구경하고 왔어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 16:23"
        },
        {
          "id": "MDRW2100025669.1.12",
          "form": "잘하셨네요:)",
          "original_form": "잘하셨네요:)",
          "speaker_id": "2",
          "time": "20210901 16:24"
        },
        {
          "id": "MDRW2100025669.1.13",
          "form": "당일치기로 다녀오신건가요?",
          "original_form": "당일치기로 다녀오신건가요?",
          "speaker_id": "2",
          "time": "20210901 16:24"
        },
        {
          "id": "MDRW2100025669.1.14",
          "form": "네 아침에 일찍 출발했어요",
          "original_form": "네 아침에 일찍 출발했어요",
          "speaker_id": "1",
          "time": "20210901 16:29"
        },
        {
          "id": "MDRW2100025669.1.15",
          "form": "그런데도 사람이 많더라고요",
          "original_form": "그런데도 사람이 많더라고요",
          "speaker_id": "1",
          "time": "20210901 16:29"
        },
        {
          "id": "MDRW2100025669.1.16",
          "form": "다들 같은 마음으로\n일찍 출발하셨었나봐요:)",
          "original_form": "다들 같은 마음으로\n일찍 출발하셨었나봐요:)",
          "speaker_id": "2",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100025669.1.17",
          "form": "청벚꽃 사진도 많이 찍으셨어요?",
          "original_form": "청벚꽃 사진도 많이 찍으셨어요?",
          "speaker_id": "2",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100025669.1.18",
          "form": "꽃나무가 군락지를 이룬건아니라 많이 찍지는 않았어요 ㅎ",
          "original_form": "꽃나무가 군락지를 이룬건아니라 많이 찍지는 않았어요 ㅎ",
          "speaker_id": "1",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025669.1.19",
          "form": "저는 진해 군항제에 가보고 싶더라구요:)",
          "original_form": "저는 진해 군항제에 가보고 싶더라구요:)",
          "speaker_id": "2",
          "time": "20210901 18:45"
        },
        {
          "id": "MDRW2100025669.1.20",
          "form": "어릴때 로망스 드라마 본 게\n너무 인상적이었었나 ㅋ",
          "original_form": "어릴때 로망스 드라마 본 게\n너무 인상적이었었나 ㅋ",
          "speaker_id": "2",
          "time": "20210901 18:45"
        },
        {
          "id": "MDRW2100025669.1.21",
          "form": "ㅎㅎ 예쁘죠 군항제 한 번 가봤는데",
          "original_form": "ㅎㅎ 예쁘죠 군항제 한 번 가봤는데",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100025669.1.22",
          "form": "꽃잎만큼이나 사람들이 많았었어요 ㅎ",
          "original_form": "꽃잎만큼이나 사람들이 많았었어요 ㅎ",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100025669.1.23",
