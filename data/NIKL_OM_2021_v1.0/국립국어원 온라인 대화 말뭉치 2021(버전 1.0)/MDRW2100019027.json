      "utterance": [
        {
          "id": "MDRW2100019027.1.1",
          "form": "아직 못정했어요",
          "original_form": "아직 못정했어요",
          "speaker_id": "2",
          "time": "20210827 14:42"
        },
        {
          "id": "MDRW2100019027.1.2",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210827 14:42"
        },
        {
          "id": "MDRW2100019027.1.3",
          "form": "저도 생각중 ㅎㅎ",
          "original_form": "저도 생각중 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 14:42"
        },
        {
          "id": "MDRW2100019027.1.4",
          "form": "아 집에 치킨 있어요",
          "original_form": "아 집에 치킨 있어요",
          "speaker_id": "2",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019027.1.5",
          "form": "엇",
          "original_form": "엇",
          "speaker_id": "1",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019027.1.6",
          "form": "그러고보니 저도 치킨 있네요",
          "original_form": "그러고보니 저도 치킨 있네요",
          "speaker_id": "1",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019027.1.7",
          "form": "어제 사드신거요?",
          "original_form": "어제 사드신거요?",
          "speaker_id": "2",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019027.1.8",
          "form": "네 ㅋㅋ 2마리사서",
          "original_form": "네 ㅋㅋ 2마리사서",
          "speaker_id": "1",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019027.1.9",
          "form": "1마리 고대로 있네요",
          "original_form": "1마리 고대로 있네요",
          "speaker_id": "1",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019027.1.10",
          "form": "식어도 맛있죠 ㅎㅎ",
          "original_form": "식어도 맛있죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019027.1.11",
          "form": "네 케이준 샐러드 해먹고싶은데",
          "original_form": "네 케이준 샐러드 해먹고싶은데",
          "speaker_id": "1",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019027.1.12",
          "form": "샐러드거리가 없는 ㅠㅠ",
          "original_form": "샐러드거리가 없는 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019027.1.13",
          "form": "케이준 맛있죠",
          "original_form": "케이준 맛있죠",
          "speaker_id": "2",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019027.1.14",
          "form": "무슨 치킨 이에요?",
          "original_form": "무슨 치킨 이에요?",
          "speaker_id": "1",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100019027.1.15",
          "form": "전 굽네치킨이요",
          "original_form": "전 굽네치킨이요",
          "speaker_id": "2",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100019027.1.16",
          "form": "굽네 맛있죠",
          "original_form": "굽네 맛있죠",
          "speaker_id": "1",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100019027.1.17",
          "form": "양이 적은 거 빼면 ㅎㅎ",
          "original_form": "양이 적은 거 빼면 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100019027.1.18",
          "form": "네 ㅋㅋㅋ 그래서 저도 굽네 잘 안시켜요 ㅠ",
          "original_form": "네 ㅋㅋㅋ 그래서 저도 굽네 잘 안시켜요 ㅠ",
          "speaker_id": "1",
          "time": "20210827 14:44"
        },
        {
          "id": "MDRW2100019027.1.19",
          "form": "전 페리카나 치킨 제일 자주 시켜먹어요",
          "original_form": "전 페리카나 치킨 제일 자주 시켜먹어요",
          "speaker_id": "1",
          "time": "20210827 14:44"
        }
