        },
        {
          "id": "MDRW2100027298.1.2",
          "form": "놀래라",
          "original_form": "놀래라",
          "speaker_id": "2",
          "time": "20210902 11:13"
        },
        {
          "id": "MDRW2100027298.1.3",
          "form": "커피마셔서요?",
          "original_form": "커피마셔서요?",
          "speaker_id": "1",
          "time": "20210902 11:14"
        },
        {
          "id": "MDRW2100027298.1.4",
          "form": "입안에 있는데ㅋㅋ",
          "original_form": "입안에 있는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:14"
        },
        {
          "id": "MDRW2100027298.1.5",
          "form": "저도마셔요",
          "original_form": "저도마셔요",
          "speaker_id": "1",
          "time": "20210902 11:14"
        },
        {
          "id": "MDRW2100027298.1.6",
          "form": "맛있나요?",
          "original_form": "맛있나요?",
          "speaker_id": "2",
          "time": "20210902 11:14"
        },
        {
          "id": "MDRW2100027298.1.7",
          "form": "전 드립커피마셔요",
          "original_form": "전 드립커피마셔요",
          "speaker_id": "1",
          "time": "20210902 11:14"
        },
        {
          "id": "MDRW2100027298.1.8",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "1",
          "time": "20210902 11:14"
        },
        {
          "id": "MDRW2100027298.1.9",
          "form": "커피종류는 뭔가요",
          "original_form": "커피종류는 뭔가요",
          "speaker_id": "2",
          "time": "20210902 11:14"
        },
        {
          "id": "MDRW2100027298.1.10",
          "form": "고디바커피 섞어서 마셔요",
          "original_form": "고디바커피 섞어서 마셔요",
          "speaker_id": "1",
          "time": "20210902 11:14"
        },
        {
          "id": "MDRW2100027298.1.11",
          "form": "향이좋아요",
          "original_form": "향이좋아요",
          "speaker_id": "1",
          "time": "20210902 11:14"
        },
        {
          "id": "MDRW2100027298.1.12",
          "form": "헤이즐넛 있으면",
          "original_form": "헤이즐넛 있으면",
          "speaker_id": "1",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100027298.1.13",
          "form": "와부럽",
          "original_form": "와부럽",
          "speaker_id": "2",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100027298.1.14",
          "form": "모카랑 섞어드셔보세요",
          "original_form": "모카랑 섞어드셔보세요",
          "speaker_id": "1",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100027298.1.15",
          "form": "존맛",
          "original_form": "존맛",
          "speaker_id": "1",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100027298.1.16",
          "form": "헤이즐넛ㅜㅜ 최애인데",
          "original_form": "헤이즐넛ㅜㅜ 최애인데",
          "speaker_id": "2",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100027298.1.17",
          "form": "모카도 없어서",
          "original_form": "모카도 없어서",
          "speaker_id": "2",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100027298.1.18",
          "form": "카누에 우유타서 마셔요ㅋㅋ",
          "original_form": "카누에 우유타서 마셔요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100027298.1.19",
          "form": "라떼맛있죠",
          "original_form": "라떼맛있죠",
          "speaker_id": "1",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100027298.1.20",
          "form": "부드럽고 고소하고",
          "original_form": "부드럽고 고소하고",
          "speaker_id": "1",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100027298.1.21",
          "form": "카누말고 괜찮은 커피있나요",
          "original_form": "카누말고 괜찮은 커피있나요",
          "speaker_id": "1",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100027298.1.22",
          "form": "인스턴트 아메",
          "original_form": "인스턴트 아메",
          "speaker_id": "1",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100027298.1.23",
          "form": "아뇨..",
          "original_form": "아뇨..",
          "speaker_id": "2",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100027298.1.24",
          "form": "카누도 그닥..",
          "original_form": "카누도 그닥..",
          "speaker_id": "2",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100027298.1.25",
          "form": "이디야에서도",
          "original_form": "이디야에서도",
          "speaker_id": "1",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100027298.1.26",
          "form": "비슷한커피있더라구요",
          "original_form": "비슷한커피있더라구요",
