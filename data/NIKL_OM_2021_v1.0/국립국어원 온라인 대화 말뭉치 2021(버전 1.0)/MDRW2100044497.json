      "utterance": [
        {
          "id": "MDRW2100044497.1.1",
          "form": "안녕하세요 좋아하는 음식 있으신가요?",
          "original_form": "안녕하세요 좋아하는 음식 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044497.1.2",
          "form": "저는치킨좋아해요^^",
          "original_form": "저는치킨좋아해요^^",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044497.1.3",
          "form": "저도 치킨 너무 좋아해요 최애브랜드 있나요?ㅎ",
          "original_form": "저도 치킨 너무 좋아해요 최애브랜드 있나요?ㅎ",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044497.1.4",
          "form": "교촌 허니오리지날이요",
          "original_form": "교촌 허니오리지날이요",
          "speaker_id": "2",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044497.1.5",
          "form": "넘맛있어요",
          "original_form": "넘맛있어요",
          "speaker_id": "2",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044497.1.6",
          "form": "아하 교촌치킨 맛있나요? 저는 주로 지코바를 먹어서요 ㅎㅎ",
          "original_form": "아하 교촌치킨 맛있나요? 저는 주로 지코바를 먹어서요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044497.1.7",
          "form": "아 지코바는 머가맛있어요?",
          "original_form": "아 지코바는 머가맛있어요?",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044497.1.8",
          "form": "저시켰던건 별로였어서ㅠㅠ",
          "original_form": "저시켰던건 별로였어서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044497.1.9",
          "form": "숯불양념치킨 순살이 맛있어요",
          "original_form": "숯불양념치킨 순살이 맛있어요",
          "speaker_id": "1",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044497.1.10",
          "form": "여기도 점바점이 심하다고 하도라고요 ㅎㅎ",
          "original_form": "여기도 점바점이 심하다고 하도라고요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044497.1.11",
          "form": "교촌도 점바점심하던데",
          "original_form": "교촌도 점바점심하던데",
          "speaker_id": "2",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044497.1.12",
          "form": "저희동네지코바가별로인가보네요ㅠ",
          "original_form": "저희동네지코바가별로인가보네요ㅠ",
          "speaker_id": "2",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044497.1.13",
          "form": "아하 진짜 치킨집 점바점 심한 곳 많더라고요ㅠ",
          "original_form": "아하 진짜 치킨집 점바점 심한 곳 많더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044497.1.14",
          "form": "조리하는사람마다달라서그렇겠조?",
          "original_form": "조리하는사람마다달라서그렇겠조?",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044497.1.15",
          "form": "집앞교촌이 맛있어서다행이네요",
          "original_form": "집앞교촌이 맛있어서다행이네요",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044497.1.16",
          "form": "그런 것 같아요ㅠㅎㅎ",
          "original_form": "그런 것 같아요ㅠㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044497.1.17",
          "form": "소스를 본사에서 주는 줄 알았는데 레시피 보고 직접 만드는 방식인가봐요 점바점이 있는 걸 보면요",
          "original_form": "소스를 본사에서 주는 줄 알았는데 레시피 보고 직접 만드는 방식인가봐요 점바점이 있는 걸 보면요",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044497.1.18",
          "form": "아 소스는줄것같아요",
          "original_form": "아 소스는줄것같아요",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044497.1.19",
          "form": "튀기는스킬차이아닐까요",
          "original_form": "튀기는스킬차이아닐까요",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044497.1.20",
          "form": "버무리는스킬이나",
          "original_form": "버무리는스킬이나",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044497.1.21",
          "form": "아 그런가요?",
          "original_form": "아 그런가요?",
          "speaker_id": "1",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044497.1.22",
          "form": "하긴 저 교촌 먹었을때 탄 치킹이 와서 놀랐덩 적 있어요 ㅎㅎ",
          "original_form": "하긴 저 교촌 먹었을때 탄 치킹이 와서 놀랐덩 적 있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044497.1.23",
          "form": "기분 나쁘셨을듯요",
          "original_form": "기분 나쁘셨을듯요",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044497.1.24",
          "form": "네ㅠ 환불도 안되니까 그냥 먹었어요",
          "original_form": "네ㅠ 환불도 안되니까 그냥 먹었어요",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044497.1.25",
          "form": "다시는안시킬듯요",
          "original_form": "다시는안시킬듯요",
          "speaker_id": "2",
