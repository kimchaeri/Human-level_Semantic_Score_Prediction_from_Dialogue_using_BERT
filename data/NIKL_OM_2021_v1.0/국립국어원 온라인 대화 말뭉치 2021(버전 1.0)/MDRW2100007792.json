      "utterance": [
        {
          "id": "MDRW2100007792.1.1",
          "form": "오 중고서점",
          "original_form": "오 중고서점",
          "speaker_id": "1",
          "time": "20210822 10:48"
        },
        {
          "id": "MDRW2100007792.1.2",
          "form": "가서 무슨책 건져오셨나요 ㅎㅎ",
          "original_form": "가서 무슨책 건져오셨나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 10:49"
        },
        {
          "id": "MDRW2100007792.1.3",
          "form": "저는 맨날 당근에서 건져오는데",
          "original_form": "저는 맨날 당근에서 건져오는데",
          "speaker_id": "1",
          "time": "20210822 10:49"
        },
        {
          "id": "MDRW2100007792.1.4",
          "form": "오 저는 알라딘이요! 알라딘에서 자기계발서나 여성학 책 위주로 건져옵니다 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "오 저는 알라딘이요! 알라딘에서 자기계발서나 여성학 책 위주로 건져옵니다 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:36"
        },
        {
          "id": "MDRW2100007792.1.5",
          "form": "알라딘도 좋죠",
          "original_form": "알라딘도 좋죠",
          "speaker_id": "1",
          "time": "20210822 11:47"
        },
        {
          "id": "MDRW2100007792.1.6",
          "form": "일단 책 상태가 좋나서",
          "original_form": "일단 책 상태가 좋나서",
          "speaker_id": "1",
          "time": "20210822 11:47"
        },
        {
          "id": "MDRW2100007792.1.7",
          "form": "중고로 사도 괜찮은 것 같아요",
          "original_form": "중고로 사도 괜찮은 것 같아요",
          "speaker_id": "1",
          "time": "20210822 11:47"
        },
        {
          "id": "MDRW2100007792.1.8",
          "form": "최근에 잘샀다! 하는 책 있어요?",
          "original_form": "최근에 잘샀다! 하는 책 있어요?",
          "speaker_id": "1",
          "time": "20210822 11:47"
        },
        {
          "id": "MDRW2100007792.1.9",
          "form": "맞아요맞아요!!",
          "original_form": "맞아요맞아요!!",
          "speaker_id": "2",
          "time": "20210822 11:56"
        },
        {
          "id": "MDRW2100007792.1.10",
          "form": "저는 1인가구 살림법인가? 그 책이요!",
          "original_form": "저는 1인가구 살림법인가? 그 책이요!",
          "speaker_id": "2",
          "time": "20210822 11:56"
        },
        {
          "id": "MDRW2100007792.1.11",
          "form": "이게 약간 1인가구 절약법같운것도 알려주고 금융 기초지식을 주더라구요 ㅋㅋㅋㅋ",
          "original_form": "이게 약간 1인가구 절약법같운것도 알려주고 금융 기초지식을 주더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:56"
        },
        {
          "id": "MDRW2100007792.1.12",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210822 12:08"
        },
        {
          "id": "MDRW2100007792.1.13",
          "form": "금융기초지식 저한테도 너무 필요해요 ㅋㅋㅋ",
          "original_form": "금융기초지식 저한테도 너무 필요해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:08"
        },
        {
          "id": "MDRW2100007792.1.14",
          "form": "오늘 도서관 가볼 예정이었는데 한번 찾아봐야겠네요",
          "original_form": "오늘 도서관 가볼 예정이었는데 한번 찾아봐야겠네요",
          "speaker_id": "1",
          "time": "20210822 12:09"
        },
        {
          "id": "MDRW2100007792.1.15",
          "form": "그외 따로 사는 물건은 없으새요?",
          "original_form": "그외 따로 사는 물건은 없으새요?",
          "speaker_id": "1",
          "time": "20210822 12:09"
        },
        {
          "id": "MDRW2100007792.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 저 요번에 일회용 안대를 샀는데 너무 좋습니더...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 저 요번에 일회용 안대를 샀는데 너무 좋습니더...",
          "speaker_id": "2",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100007792.1.17",
          "form": "오 쓰고자면 숙면하게되나요?",
          "original_form": "오 쓰고자면 숙면하게되나요?",
          "speaker_id": "1",
          "time": "20210822 13:55"
        },
        {
          "id": "MDRW2100007792.1.18",
          "form": "네 진짜 완전!!! 눈의 피로도 풀리고 따뜻해서 넘 좋더라구요",
          "original_form": "네 진짜 완전!!! 눈의 피로도 풀리고 따뜻해서 넘 좋더라구요",
          "speaker_id": "2",
          "time": "20210822 14:05"
        },
        {
          "id": "MDRW2100007792.1.19",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210822 14:16"
        },
        {
          "id": "MDRW2100007792.1.20",
          "form": "저도 한번 사봐야겠어요",
          "original_form": "저도 한번 사봐야겠어요",
          "speaker_id": "1",
          "time": "20210822 14:16"
        },
        {
          "id": "MDRW2100007792.1.21",
          "form": "요즘 폰만보다보니까 눈이 영 침침한게 ㅋㅋㅋㅋ",
          "original_form": "요즘 폰만보다보니까 눈이 영 침침한게 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 14:16"
        },
        {
          "id": "MDRW2100007792.1.22",
          "form": "저는 이번에 폼롤러샀는데",
          "original_form": "저는 이번에 폼롤러샀는데",
          "speaker_id": "1",
          "time": "20210822 14:16"
        },
        {
          "id": "MDRW2100007792.1.23",
          "form": "너무 시원하네요",
          "original_form": "너무 시원하네요",
          "speaker_id": "1",
          "time": "20210822 14:16"
        },
        {
          "id": "MDRW2100007792.1.24",
          "form": "오 저는 요가학원에서 폼롤러 써보긴했는데",
          "original_form": "오 저는 요가학원에서 폼롤러 써보긴했는데",
          "speaker_id": "2",
          "time": "20210822 15:03"
        },
        {
          "id": "MDRW2100007792.1.25",
          "form": "갬적으로도 갖고싶었어요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "갬적으로도 갖고싶었어요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:03"
        },
        {
          "id": "MDRW2100007792.1.26",
          "form": "유용한가유",
          "original_form": "유용한가유",
          "speaker_id": "2",
          "time": "20210822 15:03"
        },
        {
          "id": "MDRW2100007792.1.27",
          "form": "어우 너무 좋아요",
          "original_form": "어우 너무 좋아요",
          "speaker_id": "1",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100007792.1.28",
          "form": "마사지볼도 같이샀는대",
          "original_form": "마사지볼도 같이샀는대",
          "speaker_id": "1",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100007792.1.29",
          "form": "이건 더좋아요",
          "original_form": "이건 더좋아요",
          "speaker_id": "1",
          "time": "20210822 15:07"
        },
        {
          "id": "MDRW2100007792.1.30",
          "form": "오모나 세상에 저 안그래도 마사지용품 필요한데",
          "original_form": "오모나 세상에 저 안그래도 마사지용품 필요한데",
          "speaker_id": "2",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100007792.1.31",
          "form": "안마기기는 너무 비싸가지구 ㅋㅋㅋㅋㅋ",
          "original_form": "안마기기는 너무 비싸가지구 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100007792.1.32",
          "form": "마사지볼 오천원밖에 안해요",
          "original_form": "마사지볼 오천원밖에 안해요",
          "speaker_id": "1",
          "time": "20210822 15:46"
        },
        {
          "id": "MDRW2100007792.1.33",
          "form": "추천!!",
          "original_form": "추천!!",
          "speaker_id": "1",
          "time": "20210822 15:46"
        },
        {
          "id": "MDRW2100007792.1.34",
          "form": "어깨 풀어주면 너무 좋아요",
          "original_form": "어깨 풀어주면 너무 좋아요",
          "speaker_id": "1",
          "time": "20210822 15:46"
        },
        {
          "id": "MDRW2100007792.1.35",
          "form": "헐 대박싸네요???",
          "original_form": "헐 대박싸네요???",
          "speaker_id": "2",
          "time": "20210822 15:46"
        },
        {
          "id": "MDRW2100007792.1.36",
          "form": "이럴수가",
          "original_form": "이럴수가",
          "speaker_id": "2",
          "time": "20210822 15:46"
        },
        {
          "id": "MDRW2100007792.1.37",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "1",
          "time": "20210822 15:49"
        },
        {
          "id": "MDRW2100007792.1.38",
          "form": "폼롤러 사면서 슬쩍 담았는대",
          "original_form": "폼롤러 사면서 슬쩍 담았는대",
          "speaker_id": "1",
          "time": "20210822 15:49"
        },
        {
          "id": "MDRW2100007792.1.39",
          "form": "얘가 더 좋은것같은 ㅋㅋ",
          "original_form": "얘가 더 좋은것같은 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:49"
        },
        {
          "id": "MDRW2100007792.1.40",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그럼 마사지볼을 하나 사볼까요",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그럼 마사지볼을 하나 사볼까요",
          "speaker_id": "2",
          "time": "20210822 16:15"
        },
        {
          "id": "MDRW2100007792.1.41",
          "form": "쿠팡같은데 잘 나올거같은디",
          "original_form": "쿠팡같은데 잘 나올거같은디",
          "speaker_id": "2",
          "time": "20210822 16:15"
        },
        {
          "id": "MDRW2100007792.1.42",
          "form": "어디서 사셨어요?",
          "original_form": "어디서 사셨어요?",
          "speaker_id": "2",
          "time": "20210822 16:15"
        },
        {
          "id": "MDRW2100007792.1.43",
          "form": "맞아요 쿠팡에서 샀어요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 쿠팡에서 샀어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:18"
        },
        {
          "id": "MDRW2100007792.1.44",
          "form": "근데 로켓배송 시스템 너무 불편이해서",
          "original_form": "근데 로켓배송 시스템 너무 불편이해서",
          "speaker_id": "1",
          "time": "20210822 16:18"
        },
        {
          "id": "MDRW2100007792.1.45",
          "form": "다른곳이 나을것같기도하고",
          "original_form": "다른곳이 나을것같기도하고",
          "speaker_id": "1",
          "time": "20210822 16:18"
        },
        {
          "id": "MDRW2100007792.1.46",
          "form": "오 왜요??? 빠르면 좋을 줄 알았는데!",
          "original_form": "오 왜요??? 빠르면 좋을 줄 알았는데!",
          "speaker_id": "2",
          "time": "20210822 17:01"
        },
        {
          "id": "MDRW2100007792.1.47",
          "form": "만구천 얼마 채워야 보내줘요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "만구천 얼마 채워야 보내줘요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:03"
        },
        {
          "id": "MDRW2100007792.1.48",
          "form": "저 볼도 그렇게 산거였어요 ㅋㅋㅋㅋ",
          "original_form": "저 볼도 그렇게 산거였어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:03"
        },
        {
          "id": "MDRW2100007792.1.49",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 와우 아니시군요! 저는 와우 해둬서 사면 짱 좋을 것 같네유 히히",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 와우 아니시군요! 저는 와우 해둬서 사면 짱 좋을 것 같네유 히히",
          "speaker_id": "2",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100007792.1.50",
          "form": "가족이 로켓와우면 이럴때 좋죠 ^-^",
          "original_form": "가족이 로켓와우면 이럴때 좋죠 ^-^",
