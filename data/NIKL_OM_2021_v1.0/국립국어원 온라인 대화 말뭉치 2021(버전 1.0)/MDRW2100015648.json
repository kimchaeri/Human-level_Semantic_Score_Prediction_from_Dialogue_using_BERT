          "speaker_id": "1",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100015648.1.15",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100015648.1.16",
          "form": "오잌ㅋㅋㅋㅋ 그냥 넣는거요 ㅋㅋㅋㅋ",
          "original_form": "오잌ㅋㅋㅋㅋ 그냥 넣는거요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100015648.1.17",
          "form": "어라 빠졌네 하고 하나 집어넣고ㅋㅋㅋㅋ",
          "original_form": "어라 빠졌네 하고 하나 집어넣고ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100015648.1.18",
          "form": "진짜 웃었어요 그거 보고",
          "original_form": "진짜 웃었어요 그거 보고",
          "speaker_id": "2",
          "time": "20210828 16:25"
        },
        {
          "id": "MDRW2100015648.1.19",
          "form": "ㅋㅋㅋ 스텝을 전부 배구에 몰린 느낌 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 스텝을 전부 배구에 몰린 느낌 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:25"
        },
        {
          "id": "MDRW2100015648.1.20",
          "form": "인간미 있어서 좋아요 ㅋㅋㅋ",
          "original_form": "인간미 있어서 좋아요 ㅋㅋㅋ",
