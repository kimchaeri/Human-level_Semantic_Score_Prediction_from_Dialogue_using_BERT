      "utterance": [
        {
          "id": "MDRW2100009143.1.1",
          "form": "오늘 저녁은 뭐 드셨어요?",
          "original_form": "오늘 저녁은 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210822 21:42"
        },
        {
          "id": "MDRW2100009143.1.2",
          "form": "소고기갈비살이랑부채살먹었어요",
          "original_form": "소고기갈비살이랑부채살먹었어요",
          "speaker_id": "2",
          "time": "20210822 21:42"
        },
        {
          "id": "MDRW2100009143.1.3",
          "form": "버터감자구이도 너무헤비했어요",
          "original_form": "버터감자구이도 너무헤비했어요",
          "speaker_id": "2",
          "time": "20210822 21:43"
        },
        {
          "id": "MDRW2100009143.1.4",
          "form": "오 엄청드셨네요 ㅋㅋㅋ",
          "original_form": "오 엄청드셨네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:44"
        },
        {
          "id": "MDRW2100009143.1.5",
          "form": "직접 만드신거에요? 아님 배달?",
          "original_form": "직접 만드신거에요? 아님 배달?",
          "speaker_id": "1",
          "time": "20210822 21:44"
        },
        {
          "id": "MDRW2100009143.1.6",
          "form": "집에서해먹었어요",
          "original_form": "집에서해먹었어요",
          "speaker_id": "2",
          "time": "20210822 21:44"
        },
        {
          "id": "MDRW2100009143.1.7",
          "form": "뭐드셨나요?",
          "original_form": "뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210822 21:45"
        },
        {
          "id": "MDRW2100009143.1.8",
          "form": "저는 라면끓여먹었어요~ ㅎㅎ",
          "original_form": "저는 라면끓여먹었어요~ ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 21:45"
        },
        {
          "id": "MDRW2100009143.1.9",
          "form": "무슨라면드셨나요?",
          "original_form": "무슨라면드셨나요?",
          "speaker_id": "2",
          "time": "20210822 21:45"
        },
        {
          "id": "MDRW2100009143.1.10",
          "form": "원래는 날씨도 우중충해서 칼국수 먹고싶었는데~ 맛집이라던 동네칼국수집이 휴무더라구요ㅜㅜ",
          "original_form": "원래는 날씨도 우중충해서 칼국수 먹고싶었는데~ 맛집이라던 동네칼국수집이 휴무더라구요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 21:46"
        },
        {
          "id": "MDRW2100009143.1.11",
          "form": "진라면 매운맛이요!",
          "original_form": "진라면 매운맛이요!",
          "speaker_id": "1",
          "time": "20210822 21:46"
        },
        {
          "id": "MDRW2100009143.1.12",
          "form": "어떤 라면 드세요?",
          "original_form": "어떤 라면 드세요?",
          "speaker_id": "1",
          "time": "20210822 21:46"
        },
        {
          "id": "MDRW2100009143.1.13",
          "form": "오 저희집도 진라면매운맛이요!",
          "original_form": "오 저희집도 진라면매운맛이요!",
          "speaker_id": "2",
          "time": "20210822 21:46"
        },
        {
          "id": "MDRW2100009143.1.14",
          "form": "계란풀어먹음 젤맛잇어요",
          "original_form": "계란풀어먹음 젤맛잇어요",
          "speaker_id": "2",
          "time": "20210822 21:46"
        },
        {
          "id": "MDRW2100009143.1.15",
          "form": "맞아요~~ 그냥먹으면 담백하니 좀 짭짤하고 계란넣으면 고소하고 맛있어요! ㅋㅋㅋ",
          "original_form": "맞아요~~ 그냥먹으면 담백하니 좀 짭짤하고 계란넣으면 고소하고 맛있어요! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:47"
        },
        {
          "id": "MDRW2100009143.1.16",
          "form": "원래는 안성탕면이나 다른거 먹었었는데",
          "original_form": "원래는 안성탕면이나 다른거 먹었었는데",
          "speaker_id": "1",
          "time": "20210822 21:47"
        },
        {
          "id": "MDRW2100009143.1.17",
          "form": "요즘 진라면 맛있어졌어요 ㅎㅎ",
          "original_form": "요즘 진라면 맛있어졌어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 21:47"
        },
        {
          "id": "MDRW2100009143.1.18",
          "form": "아! 저희집은 원래 삼양라면 ㅋㅋㅋ 저희 아빠 픽!",
          "original_form": "아! 저희집은 원래 삼양라면 ㅋㅋㅋ 저희 아빠 픽!",
          "speaker_id": "1",
          "time": "20210822 21:47"
        },
        {
          "id": "MDRW2100009143.1.19",
          "form": "우와ㅋㅋ저희집도 아빠만삼양이예요",
          "original_form": "우와ㅋㅋ저희집도 아빠만삼양이예요",
          "speaker_id": "2",
          "time": "20210822 21:49"
        },
        {
          "id": "MDRW2100009143.1.20",
          "form": "아빠들의원픽인가봐요",
          "original_form": "아빠들의원픽인가봐요",
          "speaker_id": "2",
          "time": "20210822 21:49"
        },
        {
          "id": "MDRW2100009143.1.21",
          "form": "진라면은 컵라면도맛있어요ㅎㅎ",
          "original_form": "진라면은 컵라면도맛있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 21:49"
        },
        {
          "id": "MDRW2100009143.1.22",
          "form": "오 ㅋㅋㅋㅋ 아빠픽인가욬ㅋㅋㅋㅋㅋ",
          "original_form": "오 ㅋㅋㅋㅋ 아빠픽인가욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:51"
        },
        {
          "id": "MDRW2100009143.1.23",
          "form": "맞아요! 진라면 컵도 맛있어요~~!!",
          "original_form": "맞아요! 진라면 컵도 맛있어요~~!!",
          "speaker_id": "1",
          "time": "20210822 21:51"
        },
        {
          "id": "MDRW2100009143.1.24",
          "form": "근데 순한맛은 진짜... 별로였어욬ㅋㅋㅋ",
          "original_form": "근데 순한맛은 진짜... 별로였어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:52"
        },
        {
          "id": "MDRW2100009143.1.25",
          "form": "네 순한맛은 제스타일아니예요...",
          "original_form": "네 순한맛은 제스타일아니예요...",
