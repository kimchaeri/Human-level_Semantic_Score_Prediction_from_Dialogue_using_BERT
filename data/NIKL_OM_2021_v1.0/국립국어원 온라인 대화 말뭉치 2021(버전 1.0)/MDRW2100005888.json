      "utterance": [
        {
          "id": "MDRW2100005888.1.1",
          "form": "너 요새 배구 좋아 하지",
          "original_form": "너 요새 배구 좋아 하지",
          "speaker_id": "1",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100005888.1.2",
          "form": "배구보러 갈거야?",
          "original_form": "배구보러 갈거야?",
          "speaker_id": "1",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100005888.1.3",
          "form": "배구 아직은 무관중이야",
          "original_form": "배구 아직은 무관중이야",
          "speaker_id": "2",
          "time": "20210907 15:30"
        },
        {
          "id": "MDRW2100005888.1.4",
          "form": "연콩은 중국 한시즌 뛰고",
          "original_form": "연콩은 중국 한시즌 뛰고",
          "speaker_id": "2",
          "time": "20210907 15:30"
        },
        {
          "id": "MDRW2100005888.1.5",
          "form": "다음은 미국이나 유럽, 터키, 이탈리아 생각중이라고 함",
          "original_form": "다음은 미국이나 유럽, 터키, 이탈리아 생각중이라고 함",
          "speaker_id": "2",
          "time": "20210907 15:30"
        },
        {
          "id": "MDRW2100005888.1.6",
          "form": "미국은 이번 메달도 따고 올해부터 챔스리그가 생긴다해서",
          "original_form": "미국은 이번 메달도 따고 올해부터 챔스리그가 생긴다해서",
          "speaker_id": "2",
          "time": "20210907 15:31"
        },
        {
          "id": "MDRW2100005888.1.7",
          "form": "예전에 연경선수랑 같이 뛴 미국 국대 주장 조던 라슨이 생각 있으면 같이 미국가자고 했으",
          "original_form": "예전에 연경선수랑 같이 뛴 미국 국대 주장 조던 라슨이 생각 있으면 같이 미국가자고 했으",
          "speaker_id": "2",
          "time": "20210907 15:32"
        },
        {
          "id": "MDRW2100005888.1.8",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "1",
          "time": "20210907 15:44"
        },
        {
          "id": "MDRW2100005888.1.9",
          "form": "그 위드코로나 되면 가능하지않을까?",
          "original_form": "그 위드코로나 되면 가능하지않을까?",
          "speaker_id": "1",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100005888.1.10",
          "form": "미국은 지금 어느정도",
          "original_form": "미국은 지금 어느정도",
          "speaker_id": "2",
          "time": "20210907 16:14"
        },
        {
          "id": "MDRW2100005888.1.11",
          "form": "스포츠시합 유관중으로",
          "original_form": "스포츠시합 유관중으로",
          "speaker_id": "2",
          "time": "20210907 16:14"
        },
        {
          "id": "MDRW2100005888.1.12",
          "form": "진행하고있데",
          "original_form": "진행하고있데",
          "speaker_id": "2",
          "time": "20210907 16:14"
        },
        {
          "id": "MDRW2100005888.1.13",
          "form": "그보다 심심이 알람 켜놔도 폰 잠금 안 풀면 안 오는구나",
          "original_form": "그보다 심심이 알람 켜놔도 폰 잠금 안 풀면 안 오는구나",
          "speaker_id": "2",
          "time": "20210907 16:14"
        },
        {
          "id": "MDRW2100005888.1.14",
          "form": "연경신 경기 아니더라도 배구에 관심있는편?",
          "original_form": "연경신 경기 아니더라도 배구에 관심있는편?",
          "speaker_id": "1",
          "time": "20210907 16:18"
        },
        {
          "id": "MDRW2100005888.1.15",
          "form": "있는편 아예 없는건 아닌데 아직 챙겨보는 수준까지는 아닌듯",
          "original_form": "있는편 아예 없는건 아닌데 아직 챙겨보는 수준까지는 아닌듯",
          "speaker_id": "2",
          "time": "20210907 16:21"
        },
        {
          "id": "MDRW2100005888.1.16",
          "form": "얼마전에도 유럽리그? 서양리그? 봤음",
          "original_form": "얼마전에도 유럽리그? 서양리그? 봤음",
          "speaker_id": "2",
          "time": "20210907 16:22"
        },
        {
          "id": "MDRW2100005888.1.17",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210907 16:22"
        },
        {
          "id": "MDRW2100005888.1.18",
          "form": "역시 배구가 좋다기보단 연경신이 좋은거구나",
          "original_form": "역시 배구가 좋다기보단 연경신이 좋은거구나",
          "speaker_id": "1",
          "time": "20210907 16:22"
        },
        {
          "id": "MDRW2100005888.1.19",
          "form": "그게 맞는듯 싶어ㅋㅋㅋ",
          "original_form": "그게 맞는듯 싶어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:22"
        },
        {
          "id": "MDRW2100005888.1.20",
          "form": "아무렴 양궁도 잘 안 보던 사람이",
          "original_form": "아무렴 양궁도 잘 안 보던 사람이",
          "speaker_id": "2",
          "time": "20210907 16:23"
        },
        {
          "id": "MDRW2100005888.1.21",
          "form": "선수들 보다보니 보기 시작하고",
          "original_form": "선수들 보다보니 보기 시작하고",
          "speaker_id": "2",
          "time": "20210907 16:23"
        },
        {
          "id": "MDRW2100005888.1.22",
          "form": "유도도 그렇고ㅋㅋ",
          "original_form": "유도도 그렇고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:23"
        },
        {
          "id": "MDRW2100005888.1.23",
          "form": "사람이 사람을 바꾸다 같은 느낌임",
          "original_form": "사람이 사람을 바꾸다 같은 느낌임",
          "speaker_id": "2",
          "time": "20210907 16:23"
        },
        {
          "id": "MDRW2100005888.1.24",
          "form": "ㅋㅋㅋㅋ좋다 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ좋다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:24"
        },
        {
          "id": "MDRW2100005888.1.25",
          "form": "너 맞다 그 유니폼 왔음?",
          "original_form": "너 맞다 그 유니폼 왔음?",
          "speaker_id": "1",
          "time": "20210907 16:24"
