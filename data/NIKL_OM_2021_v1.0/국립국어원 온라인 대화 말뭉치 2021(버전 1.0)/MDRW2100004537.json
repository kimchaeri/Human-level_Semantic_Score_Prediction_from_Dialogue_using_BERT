      "utterance": [
        {
          "id": "MDRW2100004537.1.1",
          "form": "지금 어떤 일하세요?",
          "original_form": "지금 어떤 일하세요?",
          "speaker_id": "2",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004537.1.2",
          "form": "지금 졸업 예정인 대학샹이에요 ㅎㅎ",
          "original_form": "지금 졸업 예정인 대학샹이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004537.1.3",
          "form": "곧.. 사회로 던져지겠죠",
          "original_form": "곧.. 사회로 던져지겠죠",
          "speaker_id": "1",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004537.1.4",
          "form": "실습 나가서 몇달 일하고 왔었는데",
          "original_form": "실습 나가서 몇달 일하고 왔었는데",
          "speaker_id": "1",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004537.1.5",
          "form": "사회생활은 정말..",
          "original_form": "사회생활은 정말..",
          "speaker_id": "1",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004537.1.6",
          "form": "어렵더라고요",
          "original_form": "어렵더라고요",
          "speaker_id": "1",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004537.1.7",
          "form": "그렇군요  저는 고등학생이라서 사회생활  안해봤지만 사회생활 걱정되네요ㅜㅜ",
          "original_form": "그렇군요  저는 고등학생이라서 사회생활  안해봤지만 사회생활 걱정되네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 17:05"
        },
        {
          "id": "MDRW2100004537.1.8",
          "form": "ㅠㅠ 회사 잠깐 다녔는데도",
          "original_form": "ㅠㅠ 회사 잠깐 다녔는데도",
          "speaker_id": "1",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004537.1.9",
          "form": "피곤해서 너무 힘들었네요",
          "original_form": "피곤해서 너무 힘들었네요",
          "speaker_id": "1",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004537.1.10",
          "form": "제가 아무것도 모른다는 점리 가장 무서웠던것같아요",
          "original_form": "제가 아무것도 모른다는 점리 가장 무서웠던것같아요",
          "speaker_id": "1",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004537.1.11",
          "form": "학교에서 배우기만 했던 것들이랑은 달라서",
          "original_form": "학교에서 배우기만 했던 것들이랑은 달라서",
          "speaker_id": "1",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004537.1.12",
          "form": "그래도 진짜 배우는 건 많거라고요",
          "original_form": "그래도 진짜 배우는 건 많거라고요",
          "speaker_id": "1",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004537.1.13",
          "form": "name2님은 아르바이트 뭐 해보고싶은거 있으세요?",
          "original_form": "&name2&님은 아르바이트 뭐 해보고싶은거 있으세요?",
          "speaker_id": "1",
          "time": "20210821 17:11"
        },
        {
          "id": "MDRW2100004537.1.14",
          "form": "편의점 알바 한번 해보고싶어요",
          "original_form": "편의점 알바 한번 해보고싶어요",
          "speaker_id": "2",
          "time": "20210821 17:15"
        },
        {
          "id": "MDRW2100004537.1.15",
          "form": "편의점 알바..",
          "original_form": "편의점 알바..",
          "speaker_id": "1",
          "time": "20210821 17:22"
        },
        {
          "id": "MDRW2100004537.1.16",
          "form": "좋은데 은근 힘들죠",
          "original_form": "좋은데 은근 힘들죠",
          "speaker_id": "1",
          "time": "20210821 17:22"
        },
        {
          "id": "MDRW2100004537.1.17",
          "form": "야간은 특히 그런거같어요",
          "original_form": "야간은 특히 그런거같어요",
          "speaker_id": "1",
          "time": "20210821 17:22"
        },
        {
          "id": "MDRW2100004537.1.18",
          "form": "아하 그렇군요",
          "original_form": "아하 그렇군요",
          "speaker_id": "2",
          "time": "20210821 17:30"
        },
        {
          "id": "MDRW2100004537.1.19",
          "form": "구래도 돈버는건 행복한 일이죠",
          "original_form": "구래도 돈버는건 행복한 일이죠",
          "speaker_id": "1",
          "time": "20210821 17:32"
        },
        {
          "id": "MDRW2100004537.1.20",
          "form": "행복하게 돈벌수 있으면 너무 좋을텐데",
          "original_form": "행복하게 돈벌수 있으면 너무 좋을텐데",
          "speaker_id": "1",
          "time": "20210821 17:32"
        },
        {
          "id": "MDRW2100004537.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 17:34"
        },
        {
          "id": "MDRW2100004537.1.22",
          "form": "자격증이나 많이 따야죠 뭐",
          "original_form": "자격증이나 많이 따야죠 뭐",
          "speaker_id": "1",
          "time": "20210821 17:35"
        },
        {
          "id": "MDRW2100004537.1.23",
          "form": "이번에 컴활 실기 시험 봐야하는데",
          "original_form": "이번에 컴활 실기 시험 봐야하는데",
          "speaker_id": "1",
          "time": "20210821 17:35"
        },
        {
          "id": "MDRW2100004537.1.24",
          "form": "너무 어려워서 힘드네요 ㅠㅠ",
          "original_form": "너무 어려워서 힘드네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 17:35"
        },
        {
          "id": "MDRW2100004537.1.25",
          "form": "저격증화이팅이에요!",
          "original_form": "저격증화이팅이에요!",
          "speaker_id": "2",
          "time": "20210821 17:36"
        },
        {
          "id": "MDRW2100004537.1.26",
          "form": "고마워요 힘내서",
          "original_form": "고마워요 힘내서",
          "speaker_id": "1",
          "time": "20210821 17:38"
        },
        {
          "id": "MDRW2100004537.1.27",
          "form": "꼭 취직해볼게요",
          "original_form": "꼭 취직해볼게요",
          "speaker_id": "1",
          "time": "20210821 17:38"
        }
      ]
    }
