      "utterance": [
        {
          "id": "MDRW2100024159.1.1",
          "form": "잘생긴 사람 또 누구잇비",
          "original_form": "잘생긴 사람 또 누구잇비",
          "speaker_id": "2",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100024159.1.2",
          "form": "전 약간 좀",
          "original_form": "전 약간 좀",
          "speaker_id": "2",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100024159.1.3",
          "form": "ㅋㅋㅋㅋㅋ진짜 웃겨",
          "original_form": "ㅋㅋㅋㅋㅋ진짜 웃겨",
          "speaker_id": "1",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100024159.1.4",
          "form": "송강 요즘",
          "original_form": "송강 요즘",
          "speaker_id": "2",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100024159.1.5",
          "form": "잘생겼어요",
          "original_form": "잘생겼어요",
          "speaker_id": "2",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100024159.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100024159.1.7",
          "form": "꺅 송강이요!!",
          "original_form": "꺅 송강이요!!",
          "speaker_id": "1",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100024159.1.8",
          "form": "그 나무엑터스 소속사잖아요",
          "original_form": "그 나무엑터스 소속사잖아요",
          "speaker_id": "1",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100024159.1.9",
          "form": "요새 넷플 작품에 많이 보이더라구요???",
          "original_form": "요새 넷플 작품에 많이 보이더라구요???",
          "speaker_id": "1",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100024159.1.10",
          "form": "앜ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100024159.1.11",
          "form": "스위트홈에",
          "original_form": "스위트홈에",
          "speaker_id": "2",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100024159.1.12",
          "form": "송강이 떠올랏네요",
          "original_form": "송강이 떠올랏네요",
          "speaker_id": "2",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100024159.1.13",
          "form": "어어 맞아요 ㅋㅋ 그때부터 사람들이 찾아보더라구요",
          "original_form": "어어 맞아요 ㅋㅋ 그때부터 사람들이 찾아보더라구요",
          "speaker_id": "1",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100024159.1.14",
          "form": "인상깊었어요 분장",
          "original_form": "인상깊었어요 분장",
          "speaker_id": "2",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100024159.1.15",
          "form": "근데 스위트홈은 줄거리가 제 취향 아니여가지고",
          "original_form": "근데 스위트홈은 줄거리가 제 취향 아니여가지고",
          "speaker_id": "1",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100024159.1.16",
          "form": "ㅠㅠ끝까지는 못봤어요",
          "original_form": "ㅠㅠ끝까지는 못봤어요",
          "speaker_id": "1",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100024159.1.17",
          "form": "나름 보다보니 좀 몰입되어서",
          "original_form": "나름 보다보니 좀 몰입되어서",
          "speaker_id": "2",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100024159.1.18",
          "form": "저도 몰입했어야했는데 ADHD의 기분을 느껴버렸네요 ㅜㅜ",
          "original_form": "저도 몰입했어야했는데 ADHD의 기분을 느껴버렸네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100024159.1.19",
          "form": "ㅋㅋㅋㅋㅋ근데 요즘 알고있지만에선",
          "original_form": "ㅋㅋㅋㅋㅋ근데 요즘 알고있지만에선",
          "speaker_id": "2",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100024159.1.20",
          "form": "좀 연기땜에 욕먹길래",
          "original_form": "좀 연기땜에 욕먹길래",
          "speaker_id": "2",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100024159.1.21",
          "form": "안타까웠어요 ㅋㅋㅋ멜로는 좀 안되나",
          "original_form": "안타까웠어요 ㅋㅋㅋ멜로는 좀 안되나",
          "speaker_id": "2",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100024159.1.22",
          "form": "허걱 그래요? 많이 힘드시겠다 ㅠㅠ",
          "original_form": "허걱 그래요? 많이 힘드시겠다 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100024159.1.23",
          "form": "댓글 악플 안보시고 잘 이겨내셨음 좋겠어요",
          "original_form": "댓글 악플 안보시고 잘 이겨내셨음 좋겠어요",
          "speaker_id": "1",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100024159.1.24",
          "form": "요즘 사람들이 연예인에 대해서 말 함부로 해요",
          "original_form": "요즘 사람들이 연예인에 대해서 말 함부로 해요",
          "speaker_id": "1",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100024159.1.25",
          "form": "그러게요 아직 어린 나이라",
          "original_form": "그러게요 아직 어린 나이라",
          "speaker_id": "2",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100024159.1.26",
          "form": "가능성이 많은데",
          "original_form": "가능성이 많은데",
          "speaker_id": "2",
          "time": "20210830 22:50"
        },
        {
          "id": "MDRW2100024159.1.27",
          "form": "송강배우 저랑 동갑이란말이에요 ㅠㅠㅠ",
          "original_form": "송강배우 저랑 동갑이란말이에요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 22:50"
        },
        {
          "id": "MDRW2100024159.1.28",
          "form": "아 진짜요ㅋㅋㅋㅋ",
          "original_form": "아 진짜요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:50"
        },
        {
          "id": "MDRW2100024159.1.29",
          "form": "어라 어쩌다보니 나이 공개해버렸네",
          "original_form": "어라 어쩌다보니 나이 공개해버렸네",
