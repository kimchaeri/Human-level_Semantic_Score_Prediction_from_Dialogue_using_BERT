      "utterance": [
        {
          "id": "MDRW2100026735.1.1",
          "form": "손 살치킨 다 드셨나요",
          "original_form": "손 살치킨 다 드셨나요",
          "speaker_id": "2",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100026735.1.2",
          "form": "아직도.. 부대찌개입니다",
          "original_form": "아직도.. 부대찌개입니다",
          "speaker_id": "1",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100026735.1.3",
          "form": "치킨이 식어가는게 가슴 아프네요ㅜ",
          "original_form": "치킨이 식어가는게 가슴 아프네요ㅜ",
          "speaker_id": "1",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026735.1.4",
          "form": "혹시 소주도 있습니까",
          "original_form": "혹시 소주도 있습니까",
          "speaker_id": "2",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026735.1.5",
          "form": "그렇다고 스팸을 버릴수도 없구ㅜ",
          "original_form": "그렇다고 스팸을 버릴수도 없구ㅜ",
          "speaker_id": "1",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026735.1.6",
          "form": "생각보다 입이 짧으시네요",
          "original_form": "생각보다 입이 짧으시네요",
          "speaker_id": "2",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026735.1.7",
          "form": "ㅎㅎ 두꺼비가 곱게 대기하고 있쥬",
          "original_form": "ㅎㅎ 두꺼비가 곱게 대기하고 있쥬",
          "speaker_id": "1",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026735.1.8",
          "form": "훗",
          "original_form": "훗",
          "speaker_id": "2",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026735.1.9",
          "form": "얼른 박살 내세요",
          "original_form": "얼른 박살 내세요",
          "speaker_id": "2",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026735.1.10",
          "form": "치킨 울고 있습니다",
          "original_form": "치킨 울고 있습니다",
          "speaker_id": "2",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026735.1.11",
          "form": "ㄴㄴ 아직 다 안먹었단 말이쥬",
          "original_form": "ㄴㄴ 아직 다 안먹었단 말이쥬",
          "speaker_id": "1",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026735.1.12",
          "form": "저 원래 집에선 두세시간 먹어요",
          "original_form": "저 원래 집에선 두세시간 먹어요",
          "speaker_id": "1",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026735.1.13",
          "form": "두꺼비가  있으니깐 외롭진 않겠습니다",
          "original_form": "두꺼비가  있으니깐 외롭진 않겠습니다",
          "speaker_id": "2",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026735.1.14",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026735.1.15",
          "form": "먹방 찍으시나요",
          "original_form": "먹방 찍으시나요",
          "speaker_id": "2",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026735.1.16",
          "form": "근데 두꺼비가 한마리뿐이라ㅜ 친구를 만들어줬어야 하는데",
          "original_form": "근데 두꺼비가 한마리뿐이라ㅜ 친구를 만들어줬어야 하는데",
          "speaker_id": "1",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026735.1.17",
          "form": "다음에는 한 박스씩 사 놓으세요",
          "original_form": "다음에는 한 박스씩 사 놓으세요",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026735.1.18",
          "form": "아 저는 피티 병으로 사놓습니다",
          "original_form": "아 저는 피티 병으로 사놓습니다",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026735.1.19",
          "form": "640 미리 짜리 ㅋㅋㅋ",
          "original_form": "640 미리 짜리 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026735.1.20",
          "form": "아니ㅋㅋ",
          "original_form": "아니ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026735.1.21",
          "form": "아 이 동네는  좋은 데 이 먹습니다",
          "original_form": "아 이 동네는  좋은 데 이 먹습니다",
          "speaker_id": "2",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026735.1.22",
          "form": "아까 팀탐때 알아봤쥬?",
          "original_form": "아까 팀탐때 알아봤쥬?",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026735.1.23",
          "form": "술밖에 모르늖ㅎ",
          "original_form": "술밖에 모르늖ㅎ",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026735.1.24",
          "form": "좋은 데 이 먹는게 뭐예요?",
          "original_form": "좋은 데 이 먹는게 뭐예요?",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026735.1.25",
          "form": "좋은데이라고",
          "original_form": "좋은데이라고",
          "speaker_id": "2",
          "time": "20210901 23:58"
        }
