      "utterance": [
        {
          "id": "MDRW2100024187.1.1",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210830 23:01"
        },
        {
          "id": "MDRW2100024187.1.2",
          "form": "그래도 원래 먹던데가 해미 훨씬 다양 하더라고",
          "original_form": "그래도 원래 먹던데가 해미 훨씬 다양 하더라고",
          "speaker_id": "1",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024187.1.3",
          "form": "그건 그래",
          "original_form": "그건 그래",
          "speaker_id": "2",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024187.1.4",
          "form": "멀지만 않으면 참 좋은데",
          "original_form": "멀지만 않으면 참 좋은데",
          "speaker_id": "1",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024187.1.5",
          "form": "그니까 ㅠㅜ",
          "original_form": "그니까 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210830 23:02"
        },
        {
          "id": "MDRW2100024187.1.6",
          "form": "안 멀었으면 자주 가서 포장해 왔을 거야",
          "original_form": "안 멀었으면 자주 가서 포장해 왔을 거야",
          "speaker_id": "1",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100024187.1.7",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100024187.1.8",
          "form": "우리 결국 오늘도 와플 못 먹었네",
          "original_form": "우리 결국 오늘도 와플 못 먹었네",
          "speaker_id": "1",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100024187.1.9",
          "form": "낼 먹어야지",
          "original_form": "낼 먹어야지",
          "speaker_id": "2",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100024187.1.10",
          "form": "배민 들어가서 찾아봐봐 먹고 싶은 곳 없는지",
          "original_form": "배민 들어가서 찾아봐봐 먹고 싶은 곳 없는지",
          "speaker_id": "1",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100024187.1.11",
          "form": "지금 딱히 생각 없어서",
          "original_form": "지금 딱히 생각 없어서",
          "speaker_id": "2",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100024187.1.12",
          "form": "그래도 와플은 우리 시켜 먹는 곳 없어서 새로 뚫어야 된단 말이야",
          "original_form": "그래도 와플은 우리 시켜 먹는 곳 없어서 새로 뚫어야 된단 말이야",
          "speaker_id": "1",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100024187.1.13",
          "form": "아 그래?",
          "original_form": "아 그래?",
          "speaker_id": "2",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100024187.1.14",
          "form": "응 우리 와플 시켜 먹던 곳 없잖아",
          "original_form": "응 우리 와플 시켜 먹던 곳 없잖아",
          "speaker_id": "1",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100024187.1.15",
          "form": "찾아봤던 곳 없어?",
          "original_form": "찾아봤던 곳 없어?",
          "speaker_id": "2",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100024187.1.16",
          "form": "몇 군데 보긴 했는데 잘 모르겠어",
          "original_form": "몇 군데 보긴 했는데 잘 모르겠어",
          "speaker_id": "1",
          "time": "20210830 23:06"
