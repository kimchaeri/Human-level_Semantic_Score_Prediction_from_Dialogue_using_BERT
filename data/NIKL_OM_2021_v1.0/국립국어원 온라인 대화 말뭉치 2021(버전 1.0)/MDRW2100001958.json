      "utterance": [
        {
          "id": "MDRW2100001958.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210819 22:27"
        },
        {
          "id": "MDRW2100001958.1.2",
          "form": "좋아하는 공연있으신가요?",
          "original_form": "좋아하는 공연있으신가요?",
          "speaker_id": "1",
          "time": "20210819 22:27"
        },
        {
          "id": "MDRW2100001958.1.3",
          "form": "전 영화를 좋아해요~",
          "original_form": "전 영화를 좋아해요~",
          "speaker_id": "2",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100001958.1.4",
          "form": "어떤 종류 영화좋아하세요?",
          "original_form": "어떤 종류 영화좋아하세요?",
          "speaker_id": "1",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100001958.1.5",
          "form": "저는 로맨스코미디요~ ㅎ",
          "original_form": "저는 로맨스코미디요~ ㅎ",
          "speaker_id": "2",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100001958.1.6",
          "form": "오 최근에 본 로코영화있으신가여",
          "original_form": "오 최근에 본 로코영화있으신가여",
          "speaker_id": "1",
          "time": "20210819 22:35"
        },
        {
          "id": "MDRW2100001958.1.7",
          "form": "요즘엔 코로나도그렇고 애가어리니.\r\n  영화를못보러다녔네요ㅠ",
          "original_form": "요즘엔 코로나도그렇고 애가어리니.\r\n  영화를못보러다녔네요ㅠ",
          "speaker_id": "2",
          "time": "20210819 22:36"
        },
        {
          "id": "MDRW2100001958.1.8",
          "form": "ㅜㅜ코로나라 그져 전 최근엔 크루엘라가 마지막이었던것같아요",
          "original_form": "ㅜㅜ코로나라 그져 전 최근엔 크루엘라가 마지막이었던것같아요",
          "speaker_id": "1",
          "time": "20210819 22:37"
        },
        {
          "id": "MDRW2100001958.1.9",
          "form": "영화관에서 아무것도못먹고 딱영화만봐야되니까 아쉽긴한데 평일엔 그래도 사람없어서 괜찮더라구여",
          "original_form": "영화관에서 아무것도못먹고 딱영화만봐야되니까 아쉽긴한데 평일엔 그래도 사람없어서 괜찮더라구여",
          "speaker_id": "1",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001958.1.10",
          "form": "그것도 애있는저는 ㅠㅠ",
          "original_form": "그것도 애있는저는 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001958.1.11",
          "form": "마지막으로본건 애니메이션인듯해요ㅠ애꺼",
          "original_form": "마지막으로본건 애니메이션인듯해요ㅠ애꺼",
          "speaker_id": "2",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001958.1.12",
          "form": "ㅜㅜㅜ애기가있으니까 아무래도 애기위주로 많이보겠어요",
          "original_form": "ㅜㅜㅜ애기가있으니까 아무래도 애기위주로 많이보겠어요",
          "speaker_id": "1",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001958.1.13",
          "form": "로코를 좋아해도 애기가보진않으니까ㅜㅜ",
          "original_form": "로코를 좋아해도 애기가보진않으니까ㅜㅜ",
          "speaker_id": "1",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001958.1.14",
          "form": "맞아요ㅠㅠ첫째크고나니 둘째ㅠㅠ 또코로나 끝나면 애기영화보러 다닐것 같아요ㅋㅈㅋ",
          "original_form": "맞아요ㅠㅠ첫째크고나니 둘째ㅠㅠ 또코로나 끝나면 애기영화보러 다닐것 같아요ㅋㅈㅋ",
          "speaker_id": "2",
          "time": "20210819 22:40"
        },
        {
          "id": "MDRW2100001958.1.15",
          "form": "뮤지컬은 명성황후 봤는데 너무 재밌고소름돋게 멋지더라구요",
          "original_form": "뮤지컬은 명성황후 봤는데 너무 재밌고소름돋게 멋지더라구요",
          "speaker_id": "2",
          "time": "20210819 22:41"
        },
        {
          "id": "MDRW2100001958.1.16",
          "form": "오 뮤지컬 보러가고싶은데 재밌을것같아요",
          "original_form": "오 뮤지컬 보러가고싶은데 재밌을것같아요",
          "speaker_id": "1",
          "time": "20210819 22:41"
        },
        {
          "id": "MDRW2100001958.1.17",
          "form": "그 현장에서 주는 감동이 클거같더라구여",
          "original_form": "그 현장에서 주는 감동이 클거같더라구여",
          "speaker_id": "1",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001958.1.18",
          "form": "맞아요 달라요 진짜",
          "original_form": "맞아요 달라요 진짜",
          "speaker_id": "2",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001958.1.19",
          "form": "뮤지컬 요즘 다시 슬슬 공연하는것같던데",
          "original_form": "뮤지컬 요즘 다시 슬슬 공연하는것같던데",
          "speaker_id": "1",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100001958.1.20",
          "form": "꼭 한번 가보고싶어요",
          "original_form": "꼭 한번 가보고싶어요",
          "speaker_id": "1",
          "time": "20210819 22:47"
        }
      ]
