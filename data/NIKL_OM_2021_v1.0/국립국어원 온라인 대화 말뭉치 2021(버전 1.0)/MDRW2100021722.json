      "utterance": [
        {
          "id": "MDRW2100021722.1.1",
          "form": "요즘 뭐봐??!!",
          "original_form": "요즘 뭐봐??!!",
          "speaker_id": "2",
          "time": "20210828 21:55"
        },
        {
          "id": "MDRW2100021722.1.2",
          "form": "나 요즘 역적 !",
          "original_form": "나 요즘 역적 !",
          "speaker_id": "1",
          "time": "20210828 21:55"
        },
        {
          "id": "MDRW2100021722.1.3",
          "form": "드라마?영화? 역적 들어본거같은데",
          "original_form": "드라마?영화? 역적 들어본거같은데",
          "speaker_id": "2",
          "time": "20210828 21:56"
        },
        {
          "id": "MDRW2100021722.1.4",
          "form": "드라마임 엠비씨!",
          "original_form": "드라마임 엠비씨!",
          "speaker_id": "1",
          "time": "20210828 21:57"
        },
        {
          "id": "MDRW2100021722.1.5",
          "form": "누구나오는거야??",
          "original_form": "누구나오는거야??",
          "speaker_id": "2",
          "time": "20210828 21:57"
        },
        {
          "id": "MDRW2100021722.1.6",
          "form": "윤균상! 채수빈! 채수빈너무귀여워 이하늬도 나와 ㅋㅋ",
          "original_form": "윤균상! 채수빈! 채수빈너무귀여워 이하늬도 나와 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:57"
        },
        {
          "id": "MDRW2100021722.1.7",
          "form": "아아앙아아 그거 좀전에껀가?",
          "original_form": "아아앙아아 그거 좀전에껀가?",
          "speaker_id": "2",
          "time": "20210828 21:58"
        },
        {
          "id": "MDRW2100021722.1.8",
          "form": "한 몇 년 전쯤?",
          "original_form": "한 몇 년 전쯤?",
          "speaker_id": "1",
          "time": "20210828 21:58"
        },
        {
          "id": "MDRW2100021722.1.9",
          "form": "무슨내용인뎅? 이하늬도 나왓구나ㅜㅜ",
          "original_form": "무슨내용인뎅? 이하늬도 나왓구나ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 21:59"
        },
        {
          "id": "MDRW2100021722.1.10",
          "form": "홍길동을 새롭게 풀어낸 사극? ㅋㅋㅋ 정통사극이라는데 전혀 아닌듯 ㅋㅋ",
          "original_form": "홍길동을 새롭게 풀어낸 사극? ㅋㅋㅋ 정통사극이라는데 전혀 아닌듯 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:00"
        },
        {
          "id": "MDRW2100021722.1.11",
          "form": "그런거 안좋아하지않았어?!ㅋㅋ재밋음?!",
          "original_form": "그런거 안좋아하지않았어?!ㅋㅋ재밋음?!",
          "speaker_id": "2",
          "time": "20210828 22:00"
        },
        {
          "id": "MDRW2100021722.1.12",
          "form": "어 완전재밌어 세번봤어",
          "original_form": "어 완전재밌어 세번봤어",
          "speaker_id": "1",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100021722.1.13",
          "form": "오ㅔ뭐때문에ㅋㅋ",
          "original_form": "오ㅔ뭐때문에ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100021722.1.14",
          "form": "어떤점이 좋았엉?!",
          "original_form": "어떤점이 좋았엉?!",
          "speaker_id": "2",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100021722.1.15",
          "form": "처음엔 내용이 좋았고 두번째에는 배우와 연기가 좋았고 세번째는 여캐 활용방식이 좋았어 ㅋㅋㅋ",
          "original_form": "처음엔 내용이 좋았고 두번째에는 배우와 연기가 좋았고 세번째는 여캐 활용방식이 좋았어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100021722.1.16",
          "form": "여캐가 어떤데? 방금 봤는데 이하늬는 역시 예쁘네",
