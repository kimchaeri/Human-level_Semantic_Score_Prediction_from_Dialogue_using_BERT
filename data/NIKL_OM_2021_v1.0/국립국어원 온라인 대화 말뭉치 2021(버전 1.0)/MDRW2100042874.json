      "utterance": [
        {
          "id": "MDRW2100042874.1.1",
          "form": "진짜...금방끝난다",
          "original_form": "진짜...금방끝난다",
          "speaker_id": "2",
          "time": "20210907 10:29"
        },
        {
          "id": "MDRW2100042874.1.2",
          "form": "9만원 순식간이네",
          "original_form": "9만원 순식간이네",
          "speaker_id": "2",
          "time": "20210907 10:29"
        },
        {
          "id": "MDRW2100042874.1.3",
          "form": "^~^",
          "original_form": "^~^",
          "speaker_id": "2",
          "time": "20210907 10:29"
        },
        {
          "id": "MDRW2100042874.1.4",
          "form": "아 요즘 페스티벌 가고싶어미치겠어",
          "original_form": "아 요즘 페스티벌 가고싶어미치겠어",
          "speaker_id": "2",
          "time": "20210907 10:30"
        },
        {
          "id": "MDRW2100042874.1.5",
          "form": "벌써 9만원이야? 대박",
          "original_form": "벌써 9만원이야? 대박",
          "speaker_id": "1",
          "time": "20210907 10:30"
        },
        {
          "id": "MDRW2100042874.1.6",
          "form": "락페보내줘~~~~",
          "original_form": "락페보내줘~~~~",
          "speaker_id": "2",
          "time": "20210907 10:30"
        },
        {
          "id": "MDRW2100042874.1.7",
          "form": "페스티벌? 락?",
          "original_form": "페스티벌? 락?",
          "speaker_id": "1",
          "time": "20210907 10:30"
        },
        {
          "id": "MDRW2100042874.1.8",
          "form": "콘서트보내줘",
          "original_form": "콘서트보내줘",
          "speaker_id": "2",
          "time": "20210907 10:30"
        },
        {
          "id": "MDRW2100042874.1.9",
          "form": "ㅇㅎ 락 조아하는구나",
          "original_form": "ㅇㅎ 락 조아하는구나",
          "speaker_id": "1",
          "time": "20210907 10:30"
        },
        {
          "id": "MDRW2100042874.1.10",
          "form": "나는 콘서트...",
          "original_form": "나는 콘서트...",
          "speaker_id": "1",
          "time": "20210907 10:30"
        },
        {
          "id": "MDRW2100042874.1.11",
          "form": "나 둘다조아해",
          "original_form": "나 둘다조아해",
          "speaker_id": "2",
          "time": "20210907 10:30"
        },
        {
          "id": "MDRW2100042874.1.12",
          "form": "콘서트 가고싶어~~~~~~~ 콘서트 얼마나 재밌는데 엉엉",
          "original_form": "콘서트 가고싶어~~~~~~~ 콘서트 얼마나 재밌는데 엉엉",
          "speaker_id": "1",
          "time": "20210907 10:30"
        },
        {
          "id": "MDRW2100042874.1.13",
          "form": "뮤지컬도 좋아함!!!",
          "original_form": "뮤지컬도 좋아함!!!",
          "speaker_id": "2",
          "time": "20210907 10:30"
        },
        {
          "id": "MDRW2100042874.1.14",
          "form": "스탠딩뛰게해줘...",
          "original_form": "스탠딩뛰게해줘...",
          "speaker_id": "2",
          "time": "20210907 10:30"
        },
        {
          "id": "MDRW2100042874.1.15",
          "form": "뮤지컬! 보고싶은데 나중에 보러가야지..",
          "original_form": "뮤지컬! 보고싶은데 나중에 보러가야지..",
          "speaker_id": "1",
          "time": "20210907 10:30"
        },
        {
          "id": "MDRW2100042874.1.16",
          "form": "나 스탠딩은 너무 힘들어서 좌석만 감ㅋㅋㅋㅠㅠㅠㅠㅠ",
          "original_form": "나 스탠딩은 너무 힘들어서 좌석만 감ㅋㅋㅋㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 10:30"
        },
        {
          "id": "MDRW2100042874.1.17",
          "form": "티켓팅 힘들게 하던 때도 그리워졌네...콘서트가 안되다보니까..",
          "original_form": "티켓팅 힘들게 하던 때도 그리워졌네...콘서트가 안되다보니까..",
          "speaker_id": "1",
          "time": "20210907 10:31"
        },
        {
          "id": "MDRW2100042874.1.18",
          "form": "아진짜?!??!",
          "original_form": "아진짜?!??!",
          "speaker_id": "2",
          "time": "20210907 10:33"
        },
        {
          "id": "MDRW2100042874.1.19",
          "form": "난 맨날 좌석끊으면 후회해서",
          "original_form": "난 맨날 좌석끊으면 후회해서",
          "speaker_id": "2",
          "time": "20210907 10:33"
        },
        {
          "id": "MDRW2100042874.1.20",
          "form": "스탠딩가는데",
          "original_form": "스탠딩가는데",
          "speaker_id": "2",
          "time": "20210907 10:33"
        },
        {
          "id": "MDRW2100042874.1.21",
          "form": "좌석이 좋을때도있지",
          "original_form": "좌석이 좋을때도있지",
          "speaker_id": "2",
          "time": "20210907 10:33"
        },
        {
          "id": "MDRW2100042874.1.22",
          "form": "특히 겨울^^...",
          "original_form": "특히 겨울^^...",
          "speaker_id": "2",
          "time": "20210907 10:33"
        },
        {
          "id": "MDRW2100042874.1.23",
          "form": "겨울에 스탠딩 너무힘들어",
          "original_form": "겨울에 스탠딩 너무힘들어",
          "speaker_id": "2",
          "time": "20210907 10:33"
        },
        {
          "id": "MDRW2100042874.1.24",
          "form": "헐 진짜? 나는 여름 스탠딩...진짜 더위먹고 죽을것같애",
          "original_form": "헐 진짜? 나는 여름 스탠딩...진짜 더위먹고 죽을것같애",
          "speaker_id": "1",
          "time": "20210907 10:35"
        },
        {
          "id": "MDRW2100042874.1.25",
          "form": "겨울에는 그런문제는 없는데",
          "original_form": "겨울에는 그런문제는 없는데",
          "speaker_id": "2",
          "time": "20210907 10:50"
        },
        {
          "id": "MDRW2100042874.1.26",
          "form": "짐이....",
          "original_form": "짐이....",
          "speaker_id": "2",
          "time": "20210907 10:50"
        },
        {
          "id": "MDRW2100042874.1.27",
          "form": "^-^.....",
          "original_form": "^-^.....",
          "speaker_id": "2",
          "time": "20210907 10:50"
        },
        {
          "id": "MDRW2100042874.1.28",
          "form": "그래서 여름 저녁 콘서트 좋아해",
          "original_form": "그래서 여름 저녁 콘서트 좋아해",
          "speaker_id": "2",
          "time": "20210907 10:50"
        },
        {
          "id": "MDRW2100042874.1.29",
          "form": "ㅋㅋㅋㅋ그냥 좌석가자 좌석ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ그냥 좌석가자 좌석ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:02"
        },
        {
          "id": "MDRW2100042874.1.30",
          "form": "콘서트 가면 막 나눔하는 분위기도 좋고..들오가기 전에 설레는 것두 조은데..",
          "original_form": "콘서트 가면 막 나눔하는 분위기도 좋고..들오가기 전에 설레는 것두 조은데..",
