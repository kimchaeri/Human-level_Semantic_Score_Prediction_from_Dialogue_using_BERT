        },
        {
          "id": "MDRW2100008714.1.2",
          "form": "쿠팡 ㅋㅋ",
          "original_form": "쿠팡 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:41"
        },
        {
          "id": "MDRW2100008714.1.3",
          "form": "쌀? ㅋㅋ 쿠팡 주문했어요?ㅋㅋ",
          "original_form": "쌀? ㅋㅋ 쿠팡 주문했어요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:04"
        },
        {
          "id": "MDRW2100008714.1.4",
          "form": "아직안했다 오늘사야될거같은데",
          "original_form": "아직안했다 오늘사야될거같은데",
          "speaker_id": "2",
          "time": "20210822 19:07"
        },
        {
          "id": "MDRW2100008714.1.5",
          "form": "핫딜있나?",
          "original_form": "핫딜있나?",
          "speaker_id": "2",
          "time": "20210822 19:07"
        },
        {
          "id": "MDRW2100008714.1.6",
          "form": "핫딜 좀따 함 볼게요ㅋㅋ",
          "original_form": "핫딜 좀따 함 볼게요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:13"
        },
        {
          "id": "MDRW2100008714.1.7",
          "form": "지금 애들재우고 있어유ㅋㅋ",
          "original_form": "지금 애들재우고 있어유ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 20:13"
        },
        {
          "id": "MDRW2100008714.1.8",
          "form": "얼른 육퇴해라ㅎ",
          "original_form": "얼른 육퇴해라ㅎ",
          "speaker_id": "2",
          "time": "20210822 20:17"
        },
        {
          "id": "MDRW2100008714.1.9",
          "form": "둘째는 자고 ㅋㅋ첫째는 좀이따 잔대요ㅋ",
          "original_form": "둘째는 자고 ㅋㅋ첫째는 좀이따 잔대요ㅋ",
          "speaker_id": "1",
          "time": "20210822 21:00"
        },
        {
          "id": "MDRW2100008714.1.10",
          "form": "ㅋㅋ 우린 아직ㅡㅡ",
          "original_form": "ㅋㅋ 우린 아직ㅡㅡ",
          "speaker_id": "2",
          "time": "20210822 21:00"
        },
        {
          "id": "MDRW2100008714.1.11",
          "form": "요즘 쌀이나 다른것도 핫딜 잘안뜨네요.ㅠㅜ",
          "original_form": "요즘 쌀이나 다른것도 핫딜 잘안뜨네요.ㅠㅜ",
          "speaker_id": "1",
          "time": "20210822 21:01"
        },
        {
          "id": "MDRW2100008714.1.12",
          "form": "ㅜㆍ 그냥쿠팡에서 사야겠다",
          "original_form": "ㅜㆍ 그냥쿠팡에서 사야겠다",
          "speaker_id": "2",
          "time": "20210822 21:08"
        },
        {
          "id": "MDRW2100008714.1.13",
          "form": "로켓으로ㅋ",
          "original_form": "로켓으로ㅋ",
          "speaker_id": "2",
          "time": "20210822 21:08"
        },
        {
          "id": "MDRW2100008714.1.14",
          "form": "네네 로켓쿠팡이 제일 빠르니ㅋㅋ",
          "original_form": "네네 로켓쿠팡이 제일 빠르니ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100008714.1.15",
          "form": "급하면 사야죠ㅋㅋ",
          "original_form": "급하면 사야죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:13"
