      "utterance": [
        {
          "id": "MDRW2100018945.1.1",
          "form": "낼사러가야겠다 ㅋㅋ",
          "original_form": "낼사러가야겠다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:51"
        },
        {
          "id": "MDRW2100018945.1.2",
          "form": "네 딸 태블릿 사러가요",
          "original_form": "네 딸 태블릿 사러가요",
          "speaker_id": "2",
          "time": "20210827 13:51"
        },
        {
          "id": "MDRW2100018945.1.3",
          "form": "운전해서 오세요?",
          "original_form": "운전해서 오세요?",
          "speaker_id": "1",
          "time": "20210827 13:52"
        },
        {
          "id": "MDRW2100018945.1.4",
          "form": "전 안하고",
          "original_form": "전 안하고",
          "speaker_id": "2",
          "time": "20210827 13:52"
        },
        {
          "id": "MDRW2100018945.1.5",
          "form": "남편이요",
          "original_form": "남편이요",
          "speaker_id": "2",
          "time": "20210827 13:52"
        },
        {
          "id": "MDRW2100018945.1.6",
          "form": "전 서울 운전 못해뇨",
          "original_form": "전 서울 운전 못해뇨",
          "speaker_id": "2",
          "time": "20210827 13:52"
        },
        {
          "id": "MDRW2100018945.1.7",
          "form": "요",
          "original_form": "요",
          "speaker_id": "2",
          "time": "20210827 13:52"
        },
        {
          "id": "MDRW2100018945.1.8",
          "form": "온가족 총출동 쇼핑",
          "original_form": "온가족 총출동 쇼핑",
          "speaker_id": "1",
          "time": "20210827 13:53"
        },
        {
          "id": "MDRW2100018945.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210827 13:53"
        },
        {
          "id": "MDRW2100018945.1.10",
          "form": "남편분은 뭐 안사시나요ㅎㅎ",
          "original_form": "남편분은 뭐 안사시나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 13:53"
        },
        {
          "id": "MDRW2100018945.1.11",
          "form": "아들은 레트로 게임기 본다는데",
          "original_form": "아들은 레트로 게임기 본다는데",
          "speaker_id": "2",
          "time": "20210827 13:53"
        },
        {
          "id": "MDRW2100018945.1.12",
          "form": "뭘본다는지 모르겠어요",
          "original_form": "뭘본다는지 모르겠어요",
          "speaker_id": "2",
          "time": "20210827 13:53"
        },
        {
          "id": "MDRW2100018945.1.13",
          "form": "팩은 아닐테고",
          "original_form": "팩은 아닐테고",
          "speaker_id": "2",
          "time": "20210827 13:53"
        },
        {
          "id": "MDRW2100018945.1.14",
          "form": "네 살게 없대요",
          "original_form": "네 살게 없대요",
          "speaker_id": "2",
          "time": "20210827 13:53"
        },
        {
          "id": "MDRW2100018945.1.15",
          "form": "뭐 사시고 싶은거 없으세요 ?",
          "original_form": "뭐 사시고 싶은거 없으세요 ?",
          "speaker_id": "2",
          "time": "20210827 13:53"
        },
        {
          "id": "MDRW2100018945.1.16",
          "form": "주말에 쇼핑도 좋아요 ㅎㅎ",
          "original_form": "주말에 쇼핑도 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 13:54"
        },
        {
          "id": "MDRW2100018945.1.17",
          "form": "저는 딱히ㅠ",
          "original_form": "저는 딱히ㅠ",
          "speaker_id": "1",
          "time": "20210827 13:54"
        },
        {
          "id": "MDRW2100018945.1.18",
          "form": "욕심이 없어졌어요",
          "original_form": "욕심이 없어졌어요",
          "speaker_id": "1",
          "time": "20210827 13:54"
        },
        {
          "id": "MDRW2100018945.1.19",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210827 13:54"
        },
        {
          "id": "MDRW2100018945.1.20",
          "form": "저도 제껀 별로 안사요 ㅎㅎ",
          "original_form": "저도 제껀 별로 안사요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 13:54"
        },
        {
          "id": "MDRW2100018945.1.21",
          "form": "취미를 요새 접고 있어서요 ㅎㅎ",
          "original_form": "취미를 요새 접고 있어서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 13:54"
        },
        {
          "id": "MDRW2100018945.1.22",
          "form": "애들꺼 사죠",
          "original_form": "애들꺼 사죠",
          "speaker_id": "2",
          "time": "20210827 13:54"
        },
        {
          "id": "MDRW2100018945.1.23",
          "form": "최근에 산건 뭔가요?",
          "original_form": "최근에 산건 뭔가요?",
          "speaker_id": "2",
          "time": "20210827 13:55"
        },
        {
          "id": "MDRW2100018945.1.24",
          "form": "운동화?",
          "original_form": "운동화?",
          "speaker_id": "1",
          "time": "20210827 13:56"
        },
        {
          "id": "MDRW2100018945.1.25",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:56"
        },
        {
          "id": "MDRW2100018945.1.26",
          "form": "ㅋㅋ운동화",
          "original_form": "ㅋㅋ운동화",
          "speaker_id": "2",
          "time": "20210827 13:57"
        },
        {
          "id": "MDRW2100018945.1.27",
          "form": "그나마 해년마다 사죠",
          "original_form": "그나마 해년마다 사죠",
          "speaker_id": "2",
          "time": "20210827 13:57"
        },
        {
          "id": "MDRW2100018945.1.28",
          "form": "신다보면 달고 계절 바뀌면사고",
          "original_form": "신다보면 달고 계절 바뀌면사고",
          "speaker_id": "2",
          "time": "20210827 13:57"
        },
        {
          "id": "MDRW2100018945.1.29",
          "form": "닳고",
          "original_form": "닳고",
          "speaker_id": "2",
          "time": "20210827 13:57"
        },
        {
          "id": "MDRW2100018945.1.30",
          "form": "참 전 아이들때문에 일년에 자주 사는 품목중 하나네요",
          "original_form": "참 전 아이들때문에 일년에 자주 사는 품목중 하나네요",
          "speaker_id": "2",
          "time": "20210827 13:58"
        },
        {
          "id": "MDRW2100018945.1.31",
          "form": "생필품 식료품이 젤 많이사죠",
          "original_form": "생필품 식료품이 젤 많이사죠",
          "speaker_id": "2",
          "time": "20210827 14:07"
        },
        {
          "id": "MDRW2100018945.1.32",
          "form": "아무래도 먹고 살아야하니",
          "original_form": "아무래도 먹고 살아야하니",
          "speaker_id": "2",
          "time": "20210827 14:07"
        },
        {
          "id": "MDRW2100018945.1.33",
          "form": "잘사는 식품 있나요?",
          "original_form": "잘사는 식품 있나요?",
          "speaker_id": "2",
          "time": "20210827 14:07"
        },
        {
          "id": "MDRW2100018945.1.34",
          "form": "저희는 돈가스 꼭 만들어요ㅋㅋ",
          "original_form": "저희는 돈가스 꼭 만들어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:32"
        },
        {
          "id": "MDRW2100018945.1.35",
          "form": "아 오리고기도 좋아해서 작은 포장 사둬요ㅋ",
          "original_form": "아 오리고기도 좋아해서 작은 포장 사둬요ㅋ",
          "speaker_id": "1",
          "time": "20210827 14:32"
        },
        {
          "id": "MDRW2100018945.1.36",
          "form": "아 저도 오리고기요 ㅋㅋ",
          "original_form": "아 저도 오리고기요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:39"
        },
        {
          "id": "MDRW2100018945.1.37",
          "form": "냉장고에 있어요 ㅎㅎ",
          "original_form": "냉장고에 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 14:39"
        }
      ]
