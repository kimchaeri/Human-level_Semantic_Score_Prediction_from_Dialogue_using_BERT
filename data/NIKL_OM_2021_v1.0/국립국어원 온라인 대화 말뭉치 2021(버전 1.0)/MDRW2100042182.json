      "utterance": [
        {
          "id": "MDRW2100042182.1.1",
          "form": "홈트라니",
          "original_form": "홈트라니",
          "speaker_id": "1",
          "time": "20210906 20:26"
        },
        {
          "id": "MDRW2100042182.1.2",
          "form": "ㅋㅋ 운동 좋아하세요?",
          "original_form": "ㅋㅋ 운동 좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100042182.1.3",
          "form": "저도 홈트 도전했는데",
          "original_form": "저도 홈트 도전했는데",
          "speaker_id": "1",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100042182.1.4",
          "form": "웬만한 의지로는 힘들더라고요",
          "original_form": "웬만한 의지로는 힘들더라고요",
          "speaker_id": "1",
          "time": "20210906 20:27"
        },
        {
          "id": "MDRW2100042182.1.5",
          "form": "맞아요ㅜㅜ 저도 주 2회하고싶은데 어느새 이주에 한번..",
          "original_form": "맞아요ㅜㅜ 저도 주 2회하고싶은데 어느새 이주에 한번..",
          "speaker_id": "2",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042182.1.6",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042182.1.7",
          "form": "저는 그래서 필라테스 등록했어요",
          "original_form": "저는 그래서 필라테스 등록했어요",
          "speaker_id": "1",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042182.1.8",
          "form": "돈 내고 반강제 운동을..",
          "original_form": "돈 내고 반강제 운동을..",
          "speaker_id": "1",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042182.1.9",
          "form": "아 기구요?",
          "original_form": "아 기구요?",
          "speaker_id": "2",
          "time": "20210906 20:30"
        },
        {
          "id": "MDRW2100042182.1.10",
          "form": "네 기구필라테스요",
          "original_form": "네 기구필라테스요",
          "speaker_id": "1",
          "time": "20210906 20:30"
        },
        {
          "id": "MDRW2100042182.1.11",
          "form": "저도 해봤습니다ㅋㅋ 일대일하세요?",
          "original_form": "저도 해봤습니다ㅋㅋ 일대일하세요?",
          "speaker_id": "2",
          "time": "20210906 20:31"
        },
        {
          "id": "MDRW2100042182.1.12",
          "form": "일대일 하다가",
          "original_form": "일대일 하다가",
          "speaker_id": "1",
          "time": "20210906 20:31"
        },
        {
          "id": "MDRW2100042182.1.13",
          "form": "그룹 병행하는 중이예요",
          "original_form": "그룹 병행하는 중이예요",
          "speaker_id": "1",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100042182.1.14",
          "form": "근데 좋긴하죠? ㅋㅋ",
          "original_form": "근데 좋긴하죠? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:33"
        },
        {
          "id": "MDRW2100042182.1.15",
          "form": "네 확실히 건강해지는 느낌",
          "original_form": "네 확실히 건강해지는 느낌",
          "speaker_id": "1",
          "time": "20210906 20:33"
        },
        {
          "id": "MDRW2100042182.1.16",
          "form": "근육통도 줄고요",
          "original_form": "근육통도 줄고요",
          "speaker_id": "1",
          "time": "20210906 20:33"
        },
        {
          "id": "MDRW2100042182.1.17",
          "form": "저도 하다 안하고있는데 차이나요ㅜ",
          "original_form": "저도 하다 안하고있는데 차이나요ㅜ",
          "speaker_id": "2",
          "time": "20210906 20:34"
        },
        {
          "id": "MDRW2100042182.1.18",
          "form": "네 운동하다가 안하면 확실히 느껴지더라고요",
          "original_form": "네 운동하다가 안하면 확실히 느껴지더라고요",
          "speaker_id": "1",
          "time": "20210906 20:36"
        },
        {
          "id": "MDRW2100042182.1.19",
          "form": "엊그제 백신 맞아서 며칠 운동 쉬고 있는데",
          "original_form": "엊그제 백신 맞아서 며칠 운동 쉬고 있는데",
          "speaker_id": "1",
          "time": "20210906 20:36"
        },
        {
          "id": "MDRW2100042182.1.20",
          "form": "확실히 찌뿌둥하네요",
          "original_form": "확실히 찌뿌둥하네요",
          "speaker_id": "1",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042182.1.21",
          "form": "맞아요 저 필라할때 키컸어요",
          "original_form": "맞아요 저 필라할때 키컸어요",
          "speaker_id": "2",
          "time": "20210906 20:38"
        }
