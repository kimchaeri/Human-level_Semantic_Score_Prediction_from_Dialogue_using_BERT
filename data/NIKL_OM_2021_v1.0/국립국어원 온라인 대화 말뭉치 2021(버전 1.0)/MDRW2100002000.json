      "utterance": [
        {
          "id": "MDRW2100002000.1.1",
          "form": "날씨어떼요?",
          "original_form": "날씨어떼요?",
          "speaker_id": "1",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100002000.1.2",
          "form": "시원합니다 아주",
          "original_form": "시원합니다 아주",
          "speaker_id": "2",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100002000.1.3",
          "form": "그쪽은 날씨가 어던가요",
          "original_form": "그쪽은 날씨가 어던가요",
          "speaker_id": "2",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100002000.1.4",
          "form": "여기도요",
          "original_form": "여기도요",
          "speaker_id": "1",
          "time": "20210819 22:43"
        },
        {
          "id": "MDRW2100002000.1.5",
          "form": "가장 좋아하는 계절이 뭔가요",
          "original_form": "가장 좋아하는 계절이 뭔가요",
          "speaker_id": "2",
          "time": "20210819 22:45"
        },
        {
          "id": "MDRW2100002000.1.6",
          "form": "봄이요",
          "original_form": "봄이요",
          "speaker_id": "1",
          "time": "20210819 22:45"
        },
        {
          "id": "MDRW2100002000.1.7",
          "form": "이유는?",
          "original_form": "이유는?",
          "speaker_id": "2",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100002000.1.8",
          "form": "따뜻하니 좋은 거 갘아요",
          "original_form": "따뜻하니 좋은 거 갘아요",
          "speaker_id": "1",
          "time": "20210819 22:47"
        },
        {
          "id": "MDRW2100002000.1.9",
          "form": "여름과 겨울중에서는요?",
          "original_form": "여름과 겨울중에서는요?",
          "speaker_id": "2",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100002000.1.10",
          "form": "겨울이요",
          "original_form": "겨울이요",
          "speaker_id": "1",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100002000.1.11",
          "form": "봄이 따뜻해서 좋은데 여름과 겨울중ㅇ0ㄴ",
          "original_form": "봄이 따뜻해서 좋은데 여름과 겨울중ㅇ0ㄴ",
          "speaker_id": "2",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100002000.1.12",
          "form": "겨울을 조ㅗ아하신다고요?",
          "original_form": "겨울을 조ㅗ아하신다고요?",
          "speaker_id": "2",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100002000.1.13",
          "form": "여름은 너무 뜨겁죠",
          "original_form": "여름은 너무 뜨겁죠",
          "speaker_id": "1",
          "time": "20210819 22:51"
        },
        {
          "id": "MDRW2100002000.1.14",
          "form": "추우몀 운동해서 열내몀 되니까요",
          "original_form": "추우몀 운동해서 열내몀 되니까요",
          "speaker_id": "1",
          "time": "20210819 22:51"
        },
        {
          "id": "MDRW2100002000.1.15",
          "form": "겨울엔 코트와 롱패딩중에 뭘 잘 입으시나요",
          "original_form": "겨울엔 코트와 롱패딩중에 뭘 잘 입으시나요",
          "speaker_id": "2",
          "time": "20210819 22:52"
        },
        {
          "id": "MDRW2100002000.1.16",
          "form": "패딩이 좋죠",
          "original_form": "패딩이 좋죠",
          "speaker_id": "1",
