        },
        {
          "id": "MDRW2100011542.1.2",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "2",
          "time": "20210824 13:44"
        },
        {
          "id": "MDRW2100011542.1.3",
          "form": "이상형 있으신가요?",
          "original_form": "이상형 있으신가요?",
          "speaker_id": "1",
          "time": "20210824 13:44"
        },
        {
          "id": "MDRW2100011542.1.4",
          "form": "이상형은 옛날에는 소지섭이었는데 지금은 송중기요ㅋㅋ",
          "original_form": "이상형은 옛날에는 소지섭이었는데 지금은 송중기요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:46"
        },
        {
          "id": "MDRW2100011542.1.5",
          "form": "이상형 있으신가요?",
          "original_form": "이상형 있으신가요?",
          "speaker_id": "2",
          "time": "20210824 13:47"
        },
        {
          "id": "MDRW2100011542.1.6",
          "form": "저는 이상형은 딱히없고",
          "original_form": "저는 이상형은 딱히없고",
          "speaker_id": "1",
          "time": "20210824 13:47"
        },
        {
          "id": "MDRW2100011542.1.7",
          "form": "그냥 끌리는 사람이요",
          "original_form": "그냥 끌리는 사람이요",
          "speaker_id": "1",
          "time": "20210824 13:47"
        },
        {
          "id": "MDRW2100011542.1.8",
          "form": "끌리는 사람이 제일좋죠ㅋㅋ",
          "original_form": "끌리는 사람이 제일좋죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:47"
        },
        {
          "id": "MDRW2100011542.1.9",
          "form": "저도 그렇긴하더라구요",
          "original_form": "저도 그렇긴하더라구요",
          "speaker_id": "2",
          "time": "20210824 13:47"
        },
        {
          "id": "MDRW2100011542.1.10",
          "form": "결혼은 하고 싶으신가요?",
          "original_form": "결혼은 하고 싶으신가요?",
          "speaker_id": "1",
          "time": "20210824 13:48"
        },
        {
          "id": "MDRW2100011542.1.11",
          "form": "저는 결혼했어요ㅋㅋ",
          "original_form": "저는 결혼했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:51"
        },
        {
          "id": "MDRW2100011542.1.12",
          "form": "연애중이신가요?",
          "original_form": "연애중이신가요?",
          "speaker_id": "2",
          "time": "20210824 13:51"
        },
        {
          "id": "MDRW2100011542.1.13",
          "form": "오 네네 결혼 행복하신가요ㅎㅎ",
          "original_form": "오 네네 결혼 행복하신가요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 13:52"
        },
        {
          "id": "MDRW2100011542.1.14",
          "form": "행복할때도 있고ㅋㅋ 힘들때도 있어요ㅋ",
          "original_form": "행복할때도 있고ㅋㅋ 힘들때도 있어요ㅋ",
          "speaker_id": "2",
          "time": "20210824 13:52"
        },
        {
          "id": "MDRW2100011542.1.15",
          "form": "배우자분 어떻게 만나셨는지 여쭤봐도 될까요",
          "original_form": "배우자분 어떻게 만나셨는지 여쭤봐도 될까요",
          "speaker_id": "1",
          "time": "20210824 13:53"
        },
        {
          "id": "MDRW2100011542.1.16",
          "form": "같은 회사에 다녔어요. 사내연애하다가 저는 퇴사했고 결혼했죠.ㅋㅋ 배우자는 이상형과는 거리가 좀 있어요ㅋㅋ",
          "original_form": "같은 회사에 다녔어요. 사내연애하다가 저는 퇴사했고 결혼했죠.ㅋㅋ 배우자는 이상형과는 거리가 좀 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:56"
        },
        {
          "id": "MDRW2100011542.1.17",
          "form": "와 사내커플 멋있어요!",
          "original_form": "와 사내커플 멋있어요!",
          "speaker_id": "1",
          "time": "20210824 13:58"
        },
        {
          "id": "MDRW2100011542.1.18",
          "form": "사내연애 안한다고 안한다고 했었는데 제가 하게될줄몰랐네요ㅋ",
          "original_form": "사내연애 안한다고 안한다고 했었는데 제가 하게될줄몰랐네요ㅋ",
          "speaker_id": "2",
          "time": "20210824 13:59"
        }
      ]
