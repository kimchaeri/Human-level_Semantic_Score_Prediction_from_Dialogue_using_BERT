      "utterance": [
        {
          "id": "MDRW2100017957.1.1",
          "form": "맞아용",
          "original_form": "맞아용",
          "speaker_id": "2",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100017957.1.2",
          "form": "무슨 브랜드 생각하세요?",
          "original_form": "무슨 브랜드 생각하세요?",
          "speaker_id": "1",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100017957.1.3",
          "form": "최소 100은 모을려고요 그래서 ㅠㅠ",
          "original_form": "최소 100은 모을려고요 그래서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017957.1.4",
          "form": "저는 레노버!",
          "original_form": "저는 레노버!",
          "speaker_id": "2",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017957.1.5",
          "form": "친구가 레노버 좋다고 해서",
          "original_form": "친구가 레노버 좋다고 해서",
          "speaker_id": "2",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017957.1.6",
          "form": "거의 300 넘더라구요",
          "original_form": "거의 300 넘더라구요",
          "speaker_id": "1",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017957.1.7",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017957.1.8",
          "form": "레노버 하나 살려고용",
          "original_form": "레노버 하나 살려고용",
          "speaker_id": "2",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017957.1.9",
          "form": "전 고민하다가",
          "original_form": "전 고민하다가",
          "speaker_id": "1",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017957.1.10",
          "form": "결국 한성 샀어요 ㅠㅠ",
          "original_form": "결국 한성 샀어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017957.1.11",
          "form": "가성비 때문에 어쩔 수 없었어요",
          "original_form": "가성비 때문에 어쩔 수 없었어요",
          "speaker_id": "1",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017957.1.12",
          "form": "아하 한성도 나쁘지 않긴하죠",
          "original_form": "아하 한성도 나쁘지 않긴하죠",
          "speaker_id": "2",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017957.1.13",
          "form": "한성 말이 많기는 많더라구요",
          "original_form": "한성 말이 많기는 많더라구요",
          "speaker_id": "1",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017957.1.14",
          "form": "네 지금 체감상으로는 괜찮아요 ㅎㅎ",
          "original_form": "네 지금 체감상으로는 괜찮아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:52"
        },
        {
          "id": "MDRW2100017957.1.15",
          "form": "만족해요 ㅎㅎ",
          "original_form": "만족해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:52"
        },
        {
          "id": "MDRW2100017957.1.16",
          "form": "다행이네용",
          "original_form": "다행이네용",
          "speaker_id": "2",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100017957.1.17",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100017957.1.18",
          "form": "저도 살때 부디 만족했으면 좋겠네용 ㅜㅜ",
          "original_form": "저도 살때 부디 만족했으면 좋겠네용 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100017957.1.19",
          "form": "우선 11월달까지 150까지 모으는게 최종목표긴해서",
          "original_form": "우선 11월달까지 150까지 모으는게 최종목표긴해서",
          "speaker_id": "2",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100017957.1.20",
          "form": "오 두달 남았네요",
          "original_form": "오 두달 남았네요",
          "speaker_id": "1",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100017957.1.21",
          "form": "무슨 게임하려고 사는 거예요?",
          "original_form": "무슨 게임하려고 사는 거예요?",
          "speaker_id": "1",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100017957.1.22",
          "form": "배그할려고요 ㅎㅎ",
          "original_form": "배그할려고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:54"
