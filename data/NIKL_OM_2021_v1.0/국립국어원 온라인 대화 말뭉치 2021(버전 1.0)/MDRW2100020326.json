      "utterance": [
        {
          "id": "MDRW2100020326.1.1",
          "form": "여행 좋아하시나요 ?!",
          "original_form": "여행 좋아하시나요 ?!",
          "speaker_id": "1",
          "time": "20210828 01:24"
        },
        {
          "id": "MDRW2100020326.1.2",
          "form": "저 근거리로 다니는거 좋아해요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저 근거리로 다니는거 좋아해요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:25"
        },
        {
          "id": "MDRW2100020326.1.3",
          "form": "장거리는 가~끔!",
          "original_form": "장거리는 가~끔!",
          "speaker_id": "2",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100020326.1.4",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100020326.1.5",
          "form": "name1님은 여행 좋아하세요?",
          "original_form": "&name1&님은 여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100020326.1.6",
          "form": "저는 여행 전혀 절대 안가요",
          "original_form": "저는 여행 전혀 절대 안가요",
          "speaker_id": "1",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100020326.1.7",
          "form": "집이좋아요 ,,",
          "original_form": "집이좋아요 ,,",
          "speaker_id": "1",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100020326.1.8",
          "form": "오....",
          "original_form": "오....",
          "speaker_id": "2",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100020326.1.9",
          "form": "나가는게 싫어요 ,,",
          "original_form": "나가는게 싫어요 ,,",
          "speaker_id": "1",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100020326.1.10",
          "form": "사실 저도요.....",
          "original_form": "사실 저도요.....",
          "speaker_id": "2",
          "time": "20210828 01:26"
        },
        {
          "id": "MDRW2100020326.1.11",
          "form": "어디 여행이 제일 좋으셨어요 ?",
          "original_form": "어디 여행이 제일 좋으셨어요 ?",
          "speaker_id": "1",
          "time": "20210828 01:27"
        },
        {
          "id": "MDRW2100020326.1.12",
          "form": "코로나 직전에 친구들이랑 춘천다녀온거요!",
          "original_form": "코로나 직전에 친구들이랑 춘천다녀온거요!",
          "speaker_id": "2",
          "time": "20210828 01:27"
        },
        {
          "id": "MDRW2100020326.1.13",
          "form": "헉 춘천!",
          "original_form": "헉 춘천!",
          "speaker_id": "1",
          "time": "20210828 01:27"
        },
        {
          "id": "MDRW2100020326.1.14",
          "form": "며칠가셨어요 ?",
          "original_form": "며칠가셨어요 ?",
          "speaker_id": "1",
          "time": "20210828 01:27"
        },
        {
          "id": "MDRW2100020326.1.15",
          "form": "당일치기입니다^_^",
          "original_form": "당일치기입니다^_^",
          "speaker_id": "2",
          "time": "20210828 01:28"
        },
        {
          "id": "MDRW2100020326.1.16",
          "form": "헉 가까이 사시나요?!",
          "original_form": "헉 가까이 사시나요?!",
          "speaker_id": "1",
          "time": "20210828 01:28"
        },
        {
          "id": "MDRW2100020326.1.17",
          "form": "음 멀지는 않아요! 수도권이라서 기차로 금방이거든요",
          "original_form": "음 멀지는 않아요! 수도권이라서 기차로 금방이거든요",
          "speaker_id": "2",
          "time": "20210828 01:29"
        },
        {
          "id": "MDRW2100020326.1.18",
          "form": "아침에 기차타고 가서 구경하고 점심먹고 카페 갔다가 돌아왔어요",
          "original_form": "아침에 기차타고 가서 구경하고 점심먹고 카페 갔다가 돌아왔어요",
          "speaker_id": "2",
          "time": "20210828 01:29"
        },
        {
          "id": "MDRW2100020326.1.19",
          "form": "우와 저는 여행 진짜 안가는데 친구들이랑 전주간적있어요",
          "original_form": "우와 저는 여행 진짜 안가는데 친구들이랑 전주간적있어요",
          "speaker_id": "1",
          "time": "20210828 01:30"
        },
        {
          "id": "MDRW2100020326.1.20",
          "form": "대학생때",
          "original_form": "대학생때",
          "speaker_id": "1",
          "time": "20210828 01:30"
        },
        {
          "id": "MDRW2100020326.1.21",
          "form": "뽕뽑겠다고 새벽에가서 저녁에 돌아왔어요",
          "original_form": "뽕뽑겠다고 새벽에가서 저녁에 돌아왔어요",
          "speaker_id": "1",
          "time": "20210828 01:30"
        },
        {
          "id": "MDRW2100020326.1.22",
          "form": "그래서 여행에대한 인식이 더 나빠진게 아닐까싶어요",
          "original_form": "그래서 여행에대한 인식이 더 나빠진게 아닐까싶어요",
          "speaker_id": "1",
          "time": "20210828 01:30"
        },
        {
          "id": "MDRW2100020326.1.23",
          "form": "진짜 미친듯이 빡빡했어요",
          "original_form": "진짜 미친듯이 빡빡했어요",
          "speaker_id": "1",
          "time": "20210828 01:30"
        },
        {
          "id": "MDRW2100020326.1.24",
          "form": "와.... 관광코스 다 도셨나보다ㅋㅋㅋㅋㅋㅋ",
          "original_form": "와.... 관광코스 다 도셨나보다ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:31"
        },
        {
          "id": "MDRW2100020326.1.25",
          "form": "한옥마을만 가도 피곤한데",
          "original_form": "한옥마을만 가도 피곤한데",
          "speaker_id": "2",
          "time": "20210828 01:31"
        },
        {
          "id": "MDRW2100020326.1.26",
          "form": "한창 페이스북에 한옥마을 올라올때",
          "original_form": "한창 페이스북에 한옥마을 올라올때",
          "speaker_id": "1",
          "time": "20210828 01:31"
        },
        {
          "id": "MDRW2100020326.1.27",
          "form": "너무 피곤했어요 ,,",
          "original_form": "너무 피곤했어요 ,,",
