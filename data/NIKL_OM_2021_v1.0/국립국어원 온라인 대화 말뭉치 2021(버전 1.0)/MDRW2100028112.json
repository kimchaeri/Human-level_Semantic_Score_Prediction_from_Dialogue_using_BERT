      "utterance": [
        {
          "id": "MDRW2100028112.1.1",
          "form": "안녕하세요!^^",
          "original_form": "안녕하세요!^^",
          "speaker_id": "1",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100028112.1.2",
          "form": "안녕하세요 식사드셨나요",
          "original_form": "안녕하세요 식사드셨나요",
          "speaker_id": "2",
          "time": "20210902 17:41"
        },
        {
          "id": "MDRW2100028112.1.3",
          "form": "아직 안먹었습니다!",
          "original_form": "아직 안먹었습니다!",
          "speaker_id": "1",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100028112.1.4",
          "form": "혹시 식사 하셨어요?",
          "original_form": "혹시 식사 하셨어요?",
          "speaker_id": "1",
          "time": "20210902 17:42"
        },
        {
          "id": "MDRW2100028112.1.5",
          "form": "고민중이에뇨 뭐드세여",
          "original_form": "고민중이에뇨 뭐드세여",
          "speaker_id": "2",
          "time": "20210902 17:52"
        },
        {
          "id": "MDRW2100028112.1.6",
          "form": "저는 크림우동 먹으려구요 ㅎㅎ",
          "original_form": "저는 크림우동 먹으려구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:53"
        },
        {
          "id": "MDRW2100028112.1.7",
          "form": "우와해서 드시나요",
          "original_form": "우와해서 드시나요",
          "speaker_id": "2",
          "time": "20210902 17:53"
        },
        {
          "id": "MDRW2100028112.1.8",
          "form": "설마요..,,ㅋㅅㅋ",
          "original_form": "설마요..,,ㅋㅅㅋ",
          "speaker_id": "1",
          "time": "20210902 17:53"
        },
        {
          "id": "MDRW2100028112.1.9",
          "form": "사먹으려구요 ㅎㅎ",
          "original_form": "사먹으려구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100028112.1.10",
          "form": "해서 먹을 수 있는 능력이 있었으면 좋겠네요 ㅎㅎ",
          "original_form": "해서 먹을 수 있는 능력이 있었으면 좋겠네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100028112.1.11",
          "form": "자신있게 하실 수 있는 음식 있으세요?",
          "original_form": "자신있게 하실 수 있는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100028112.1.12",
          "form": "저는 김치찌개 된장찌개 미역국 커레요 ㅋㅋㅋ",
          "original_form": "저는 김치찌개 된장찌개 미역국 커레요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:57"
        },
        {
          "id": "MDRW2100028112.1.13",
          "form": "우와~!!",
          "original_form": "우와~!!",
          "speaker_id": "1",
          "time": "20210902 17:57"
        },
        {
          "id": "MDRW2100028112.1.14",
          "form": "자취하새요?",
          "original_form": "자취하새요?",
          "speaker_id": "1",
          "time": "20210902 17:58"
        },
        {
          "id": "MDRW2100028112.1.15",
          "form": "하세요?",
          "original_form": "하세요?",
          "speaker_id": "1",
          "time": "20210902 17:58"
        },
        {
          "id": "MDRW2100028112.1.16",
          "form": "아뇽 가족과같이 살아요",
          "original_form": "아뇽 가족과같이 살아요",
          "speaker_id": "2",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100028112.1.17",
          "form": "엄마가 많이바쁘셔서 요리가 늘었엉6",
          "original_form": "엄마가 많이바쁘셔서 요리가 늘었엉6",
          "speaker_id": "2",
          "time": "20210902 18:51"
        },
        {
          "id": "MDRW2100028112.1.18",
          "form": "우와 엄청 대단해보인다",
          "original_form": "우와 엄청 대단해보인다",
          "speaker_id": "1",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028112.1.19",
          "form": "하다보면 늘더라고요 ㅋㅋ",
          "original_form": "하다보면 늘더라고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100028112.1.20",
          "form": "그래도 되게 멋있으신 거 같아요",
          "original_form": "그래도 되게 멋있으신 거 같아요",
          "speaker_id": "1",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100028112.1.21",
          "form": "오 감사합니다",
          "original_form": "오 감사합니다",
          "speaker_id": "2",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100028112.1.22",
          "form": "저는 과일밖에 못 깍아요 큐ㅠ",
          "original_form": "저는 과일밖에 못 깍아요 큐ㅠ",
          "speaker_id": "1",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100028112.1.23",
          "form": "젤 쉬워요 저요리들",
          "original_form": "젤 쉬워요 저요리들",
          "speaker_id": "2",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028112.1.24",
          "form": "미역국은 소고기 참기름에 볶다가 미역 불린거 볶다가 물붓고 국간장 끝",
          "original_form": "미역국은 소고기 참기름에 볶다가 미역 불린거 볶다가 물붓고 국간장 끝",
          "speaker_id": "2",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100028112.1.25",
          "form": "꼭 요리 잘하는 사람들이 쉽다고..)(",
          "original_form": "꼭 요리 잘하는 사람들이 쉽다고..)(",
          "speaker_id": "1",
          "time": "20210902 21:42"
        },
        {
