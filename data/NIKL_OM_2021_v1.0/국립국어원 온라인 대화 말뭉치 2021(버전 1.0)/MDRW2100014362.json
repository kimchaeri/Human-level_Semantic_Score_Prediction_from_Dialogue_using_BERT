      "utterance": [
        {
          "id": "MDRW2100014362.1.1",
          "form": "좋아하죠 전 아이스아메리카노  좋아해요",
          "original_form": "좋아하죠 전 아이스아메리카노  좋아해요",
          "speaker_id": "2",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100014362.1.2",
          "form": "저는 바닐라라떼요",
          "original_form": "저는 바닐라라떼요",
          "speaker_id": "1",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100014362.1.3",
          "form": "달달하니 맛있어요",
          "original_form": "달달하니 맛있어요",
          "speaker_id": "1",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100014362.1.4",
          "form": "맞아요 전 메가커피를 잘 가요",
          "original_form": "맞아요 전 메가커피를 잘 가요",
          "speaker_id": "2",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100014362.1.5",
          "form": "저도 거기 자주 가요",
          "original_form": "저도 거기 자주 가요",
          "speaker_id": "1",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100014362.1.6",
          "form": "요즘은 제로콜라에 빠져있어요",
          "original_form": "요즘은 제로콜라에 빠져있어요",
          "speaker_id": "2",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100014362.1.7",
          "form": "저도 편의ㅓㅁ 행사 때 종종 사요",
          "original_form": "저도 편의ㅓㅁ 행사 때 종종 사요",
          "speaker_id": "1",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100014362.1.8",
          "form": "맞아요 그게 꿀팁이네요",
          "original_form": "맞아요 그게 꿀팁이네요",
          "speaker_id": "2",
          "time": "20210825 18:39"
        },
        {
          "id": "MDRW2100014362.1.9",
          "form": "펀의점 커피도 맛있는 것 같아요",
          "original_form": "펀의점 커피도 맛있는 것 같아요",
          "speaker_id": "1",
          "time": "20210825 18:39"
        },
        {
          "id": "MDRW2100014362.1.10",
          "form": "얼음이랑 그 커피가 매줄  1등이래요",
          "original_form": "얼음이랑 그 커피가 매줄  1등이래요",
          "speaker_id": "2",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014362.1.11",
          "form": "오 신기하네요",
          "original_form": "오 신기하네요",
          "speaker_id": "1",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014362.1.12",
          "form": "갑자비 스벅 치즈케이크가 먹고싶네요",
          "original_form": "갑자비 스벅 치즈케이크가 먹고싶네요",
          "speaker_id": "2",
          "time": "20210825 18:41"
        },
        {
          "id": "MDRW2100014362.1.13",
          "form": "치즈케이크가 맛있더라구요",
          "original_form": "치즈케이크가 맛있더라구요",
          "speaker_id": "1",
          "time": "20210825 18:42"
        },
        {
          "id": "MDRW2100014362.1.14",
          "form": "다른 음료는 어떤 게 좋이신가요",
          "original_form": "다른 음료는 어떤 게 좋이신가요",
          "speaker_id": "2",
          "time": "20210825 18:42"
        },
        {
          "id": "MDRW2100014362.1.15",
          "form": "전 요구르트요",
          "original_form": "전 요구르트요",
          "speaker_id": "1",
          "time": "20210825 18:42"
