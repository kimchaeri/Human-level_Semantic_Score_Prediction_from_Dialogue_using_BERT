      "utterance": [
        {
          "id": "MDRW2100007451.1.1",
          "form": "가사활동 많이 하시나요",
          "original_form": "가사활동 많이 하시나요",
          "speaker_id": "1",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007451.1.2",
          "form": "저는 1인가구 자취생이지만",
          "original_form": "저는 1인가구 자취생이지만",
          "speaker_id": "2",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007451.1.3",
          "form": "가사활동 거의 안 합니다ㅎㅎ",
          "original_form": "가사활동 거의 안 합니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007451.1.4",
          "form": "밥은 어떻게 드세요?",
          "original_form": "밥은 어떻게 드세요?",
          "speaker_id": "1",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007451.1.5",
          "form": "사드세요?",
          "original_form": "사드세요?",
          "speaker_id": "1",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007451.1.6",
          "form": "네! 배달 자주 시켜요",
          "original_form": "네! 배달 자주 시켜요",
          "speaker_id": "2",
          "time": "20210822 02:14"
        },
        {
          "id": "MDRW2100007451.1.7",
          "form": "오 요즘 배달비도 비싸서 식비 많이 나가시겠어요",
          "original_form": "오 요즘 배달비도 비싸서 식비 많이 나가시겠어요",
          "speaker_id": "1",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007451.1.8",
          "form": "저도 한때 모든 음식을 사서 해결했는데 한달에 계산해보니 식비가 40만원 들더라구요",
          "original_form": "저도 한때 모든 음식을 사서 해결했는데 한달에 계산해보니 식비가 40만원 들더라구요",
          "speaker_id": "1",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007451.1.9",
          "form": "그래서 해먹으려고 노력해요",
          "original_form": "그래서 해먹으려고 노력해요",
          "speaker_id": "1",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007451.1.10",
          "form": "맞아요 진짜 배달음식 시키다보면",
          "original_form": "맞아요 진짜 배달음식 시키다보면",
          "speaker_id": "2",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007451.1.11",
          "form": "4~50은 우습게 나가요",
          "original_form": "4~50은 우습게 나가요",
          "speaker_id": "2",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007451.1.12",
          "form": "해먹어 버릇 들어야하는데",
          "original_form": "해먹어 버릇 들어야하는데",
          "speaker_id": "2",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007451.1.13",
          "form": "사먹어버릇이 너무 단단히 들어버렸어요",
          "original_form": "사먹어버릇이 너무 단단히 들어버렸어요",
          "speaker_id": "2",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007451.1.14",
          "form": "사먹으면 넘 편하죠",
          "original_form": "사먹으면 넘 편하죠",
          "speaker_id": "1",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007451.1.15",
          "form": "맛도있도",
          "original_form": "맛도있도",
          "speaker_id": "1",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007451.1.16",
          "form": "근데 쓰레기 치우는 게 또 일이더라구요",
          "original_form": "근데 쓰레기 치우는 게 또 일이더라구요",
          "speaker_id": "1",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007451.1.17",
          "form": "하 맞아요",
          "original_form": "하 맞아요",
          "speaker_id": "2",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007451.1.18",
          "form": "플라스틱 진짜 많이 나와요",
          "original_form": "플라스틱 진짜 많이 나와요",
          "speaker_id": "2",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007451.1.19",
          "form": "맞아요 진짜 우리가 아니라 음식점이 그런 거 줄여야되는데",
          "original_form": "맞아요 진짜 우리가 아니라 음식점이 그런 거 줄여야되는데",
          "speaker_id": "1",
          "time": "20210822 02:24"
        },
        {
          "id": "MDRW2100007451.1.20",
          "form": "중국집처럼 차라리",
          "original_form": "중국집처럼 차라리",
          "speaker_id": "2",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007451.1.21",
          "form": "그릇으로 주면 좋을거 같아요",
          "original_form": "그릇으로 주면 좋을거 같아요",
          "speaker_id": "2",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007451.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007451.1.23",
          "form": "근데 중국집도 요즘은 다 플라스틱이더라구요",
          "original_form": "근데 중국집도 요즘은 다 플라스틱이더라구요",
          "speaker_id": "1",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007451.1.24",
          "form": "전 겁이 많아서 자취해도",
          "original_form": "전 겁이 많아서 자취해도",
          "speaker_id": "1",
          "time": "20210822 02:26"
        },
        {
          "id": "MDRW2100007451.1.25",
          "form": "친구랑 같이 살아요",
          "original_form": "친구랑 같이 살아요",
          "speaker_id": "1",
          "time": "20210822 02:26"
        },
        {
          "id": "MDRW2100007451.1.26",
          "form": "룸메이트 신기해요",
          "original_form": "룸메이트 신기해요",
          "speaker_id": "2",
