      "utterance": [
        {
          "id": "MDRW2100011704.1.1",
          "form": "직장인 이세요?",
          "original_form": "직장인 이세요?",
          "speaker_id": "2",
          "time": "20210824 16:29"
        },
        {
          "id": "MDRW2100011704.1.2",
          "form": "퇴사했어요 실업급여 신청중이요",
          "original_form": "퇴사했어요 실업급여 신청중이요",
          "speaker_id": "1",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100011704.1.3",
          "form": "네 그러시구낭.. 일이 맞지 않은거에요?",
          "original_form": "네 그러시구낭.. 일이 맞지 않은거에요?",
          "speaker_id": "2",
          "time": "20210824 21:07"
        },
        {
          "id": "MDRW2100011704.1.4",
          "form": "회사가 어려워져서요 ㅠ",
          "original_form": "회사가 어려워져서요 ㅠ",
          "speaker_id": "1",
          "time": "20210824 21:40"
        },
        {
          "id": "MDRW2100011704.1.5",
          "form": "겸사겸사 쉬고 좋네요 ㅎ",
          "original_form": "겸사겸사 쉬고 좋네요 ㅎ",
          "speaker_id": "1",
          "time": "20210824 21:40"
        },
        {
          "id": "MDRW2100011704.1.6",
          "form": "재충전의.기회로.삼으시면 좋겠네요^^",
          "original_form": "재충전의.기회로.삼으시면 좋겠네요^^",
          "speaker_id": "2",
          "time": "20210824 22:29"
        },
        {
          "id": "MDRW2100011704.1.7",
          "form": "네 계속 쉬고싶긴하네요 ㅎ",
          "original_form": "네 계속 쉬고싶긴하네요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 06:13"
        },
        {
          "id": "MDRW2100011704.1.8",
          "form": "일하시나요?",
          "original_form": "일하시나요?",
          "speaker_id": "1",
          "time": "20210825 06:13"
        },
        {
          "id": "MDRW2100011704.1.9",
          "form": "전 일하고 있어요.",
          "original_form": "전 일하고 있어요.",
          "speaker_id": "2",
          "time": "20210825 13:11"
        },
        {
          "id": "MDRW2100011704.1.10",
          "form": "쉬시면서 아르바이트 같은 거 하실 생각인가요?",
          "original_form": "쉬시면서 아르바이트 같은 거 하실 생각인가요?",
          "speaker_id": "2",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100011704.1.11",
          "form": "아뇨 실업급여타면 일못하더라고요",
          "original_form": "아뇨 실업급여타면 일못하더라고요",
          "speaker_id": "1",
          "time": "20210825 13:13"
        },
        {
          "id": "MDRW2100011704.1.12",
          "form": "아 그렇구낭\n전 정규직 되고 12년 근속 중이에요 ㅎ",
          "original_form": "아 그렇구낭\n전 정규직 되고 12년 근속 중이에요 ㅎ",
          "speaker_id": "2",
          "time": "20210825 13:14"
        },
        {
          "id": "MDRW2100011704.1.13",
          "form": "오 저도 10년넘게 일했거든요",
          "original_form": "오 저도 10년넘게 일했거든요",
          "speaker_id": "1",
          "time": "20210825 13:15"
        },
        {
          "id": "MDRW2100011704.1.14",
          "form": "오래일하기 힘든데 사람들도 좋으셨나봐요",
          "original_form": "오래일하기 힘든데 사람들도 좋으셨나봐요",
          "speaker_id": "1",
          "time": "20210825 13:15"
        },
        {
          "id": "MDRW2100011704.1.15",
          "form": "전 혼자 일했어요ㅡ쭈욱\n사장님 한 분이랑^^",
          "original_form": "전 혼자 일했어요ㅡ쭈욱\n사장님 한 분이랑^^",
          "speaker_id": "2",
          "time": "20210825 13:16"
        },
        {
          "id": "MDRW2100011704.1.16",
          "form": "가족같겠네요",
          "original_form": "가족같겠네요",
          "speaker_id": "1",
          "time": "20210825 13:17"
        },
        {
          "id": "MDRW2100011704.1.17",
          "form": "사장님이 좀 연세가 많으신 편이라서 제가 며느리 같다고 하시네요 ^^",
          "original_form": "사장님이 좀 연세가 많으신 편이라서 제가 며느리 같다고 하시네요 ^^",
          "speaker_id": "2",
          "time": "20210825 13:18"
        },
        {
          "id": "MDRW2100011704.1.18",
          "form": "ㅎㅎ좋네요 직종은 어떤건가요?",
          "original_form": "ㅎㅎ좋네요 직종은 어떤건가요?",
          "speaker_id": "1",
          "time": "20210825 13:19"
        },
        {
          "id": "MDRW2100011704.1.19",
          "form": "사무직이에요. 일도 편하고 혼자라서 넘 좋아요",
          "original_form": "사무직이에요. 일도 편하고 혼자라서 넘 좋아요",
