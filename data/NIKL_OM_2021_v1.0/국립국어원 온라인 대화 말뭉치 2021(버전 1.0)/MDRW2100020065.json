      "utterance": [
        {
          "id": "MDRW2100020065.1.1",
          "form": "최근에본영화 있으신가요?",
          "original_form": "최근에본영화 있으신가요?",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020065.1.2",
          "form": "최근에",
          "original_form": "최근에",
          "speaker_id": "2",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020065.1.3",
          "form": "블랙 위도우 봤어요!",
          "original_form": "블랙 위도우 봤어요!",
          "speaker_id": "2",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020065.1.4",
          "form": "헐 저도요",
          "original_form": "헐 저도요",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020065.1.5",
          "form": "너무 재밌죠",
          "original_form": "너무 재밌죠",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020065.1.6",
          "form": "랑종도 볼까했는데 코로나 확진자수땜에 포기하긴했지만",
          "original_form": "랑종도 볼까했는데 코로나 확진자수땜에 포기하긴했지만",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020065.1.7",
          "form": "그래도 블랙위도우 본거에 만족하고있어요 ㅎㅎ",
          "original_form": "그래도 블랙위도우 본거에 만족하고있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020065.1.8",
          "form": "랑종 진짜 잔인할거같아요",
          "original_form": "랑종 진짜 잔인할거같아요",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020065.1.9",
          "form": "마블영화는 영화관에서 봐야죠",
          "original_form": "마블영화는 영화관에서 봐야죠",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020065.1.10",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020065.1.11",
          "form": "이번에 11월달에",
          "original_form": "이번에 11월달에",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020065.1.12",
          "form": "이터널스 마동석 나오는거 함 볼까봐요",
          "original_form": "이터널스 마동석 나오는거 함 볼까봐요",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020065.1.13",
          "form": "오오 곧나오나요?",
          "original_form": "오오 곧나오나요?",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020065.1.14",
          "form": "꼭 봐야겠네요",
          "original_form": "꼭 봐야겠네요",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020065.1.15",
          "form": "스파이더맨 티저봤어요?",
          "original_form": "스파이더맨 티저봤어요?",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020065.1.16",
          "form": "아 맞아요 티저도 이번에 봤죠",
          "original_form": "아 맞아요 티저도 이번에 봤죠",
          "speaker_id": "2",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100020065.1.17",
          "form": "재밌을거같은데 ㅠㅠ",
          "original_form": "재밌을거같은데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100020065.1.18",
          "form": "티저 너무 기대돼요",
          "original_form": "티저 너무 기대돼요",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020065.1.19",
          "form": "계속 유투브 1위더라고요",
          "original_form": "계속 유투브 1위더라고요",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020065.1.20",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020065.1.21",
          "form": "전대스파이더맨도 나온대요",
          "original_form": "전대스파이더맨도 나온대요",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020065.1.22",
          "form": "헐...ㅠㅠ",
          "original_form": "헐...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020065.1.23",
          "form": "대박이네요",
          "original_form": "대박이네요",
          "speaker_id": "2",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020065.1.24",
          "form": "그쵸? ㅋㅋㅋㅋ꼭 영화관에서 봐야겠네요",
          "original_form": "그쵸? ㅋㅋㅋㅋ꼭 영화관에서 봐야겠네요",
          "speaker_id": "1",
          "time": "20210827 23:38"
        }
      ]
