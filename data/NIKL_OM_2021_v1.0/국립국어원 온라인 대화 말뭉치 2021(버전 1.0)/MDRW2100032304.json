      "utterance": [
        {
          "id": "MDRW2100032304.1.1",
          "form": "여행 조아하세요?",
          "original_form": "여행 조아하세요?",
          "speaker_id": "1",
          "time": "20210905 15:05"
        },
        {
          "id": "MDRW2100032304.1.2",
          "form": "네 저 여행쟁이인데ㅠ",
          "original_form": "네 저 여행쟁이인데ㅠ",
          "speaker_id": "2",
          "time": "20210905 20:08"
        },
        {
          "id": "MDRW2100032304.1.3",
          "form": "글쿤요ㅠ 어디가 가장 기억에남나요",
          "original_form": "글쿤요ㅠ 어디가 가장 기억에남나요",
          "speaker_id": "1",
          "time": "20210905 20:30"
        },
        {
          "id": "MDRW2100032304.1.4",
          "form": "저는 동남아 위주라서ㅋㅋ",
          "original_form": "저는 동남아 위주라서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 08:40"
        },
        {
          "id": "MDRW2100032304.1.5",
          "form": "19년에 네번 갔던게",
          "original_form": "19년에 네번 갔던게",
          "speaker_id": "2",
          "time": "20210906 08:40"
        },
        {
          "id": "MDRW2100032304.1.6",
          "form": "기억에 남습니다 ㅋㅋ",
          "original_form": "기억에 남습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 08:40"
        },
        {
          "id": "MDRW2100032304.1.7",
          "form": "와 어디 가셨나요 저는 베트남 !!!!",
          "original_form": "와 어디 가셨나요 저는 베트남 !!!!",
          "speaker_id": "1",
          "time": "20210906 11:39"
        },
        {
          "id": "MDRW2100032304.1.8",
          "form": "어 베트남도 갔어요",
          "original_form": "어 베트남도 갔어요",
          "speaker_id": "2",
          "time": "20210906 11:43"
        },
        {
          "id": "MDRW2100032304.1.9",
          "form": "하노이 갔었어요ㅋㅋ name1님은?",
          "original_form": "하노이 갔었어요ㅋㅋ &name1&님은?",
          "speaker_id": "2",
          "time": "20210906 11:43"
        },
        {
          "id": "MDRW2100032304.1.10",
          "form": "오 저는 호치민이랑 나트랑이요",
          "original_form": "오 저는 호치민이랑 나트랑이요",
          "speaker_id": "1",
          "time": "20210906 11:44"
        },
        {
          "id": "MDRW2100032304.1.11",
          "form": "또 오디가셨어요",
          "original_form": "또 오디가셨어요",
          "speaker_id": "1",
          "time": "20210906 11:44"
        },
        {
          "id": "MDRW2100032304.1.12",
          "form": "홍콩마카오 태국 베트남 대만",
          "original_form": "홍콩마카오 태국 베트남 대만",
          "speaker_id": "2",
          "time": "20210906 11:45"
        },
        {
          "id": "MDRW2100032304.1.13",
          "form": "이렇게 갔습니다 ㅋㅋ",
          "original_form": "이렇게 갔습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 11:45"
        },
        {
          "id": "MDRW2100032304.1.14",
          "form": "와오",
          "original_form": "와오",
          "speaker_id": "1",
          "time": "20210906 11:45"
        },
        {
          "id": "MDRW2100032304.1.15",
          "form": "대만 어때요 너무 가고파요",
          "original_form": "대만 어때요 너무 가고파요",
          "speaker_id": "1",
          "time": "20210906 11:45"
        },
        {
          "id": "MDRW2100032304.1.16",
          "form": "대만 짱좋아요",
          "original_form": "대만 짱좋아요",
          "speaker_id": "2",
          "time": "20210906 11:46"
        },
        {
          "id": "MDRW2100032304.1.17",
          "form": "중국 순한맛?",
          "original_form": "중국 순한맛?",
          "speaker_id": "2",
          "time": "20210906 11:46"
        },
        {
          "id": "MDRW2100032304.1.18",
          "form": "아 순한맛 !!",
          "original_form": "아 순한맛 !!",
          "speaker_id": "1",
          "time": "20210906 11:46"
        },
        {
          "id": "MDRW2100032304.1.19",
          "form": "대만 영화 좋아해서ㅠㅠ 그런 분위기 나요?",
          "original_form": "대만 영화 좋아해서ㅠㅠ 그런 분위기 나요?",
          "speaker_id": "1",
          "time": "20210906 11:46"
        },
        {
          "id": "MDRW2100032304.1.20",
          "form": "네 비슷할겁니다 ㅎㅎ",
          "original_form": "네 비슷할겁니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 11:48"
        },
        {
          "id": "MDRW2100032304.1.21",
          "form": "야시장 진짜 좋아요",
          "original_form": "야시장 진짜 좋아요",
          "speaker_id": "2",
          "time": "20210906 11:48"
        },
        {
          "id": "MDRW2100032304.1.22",
          "form": "오 야시장에 꼬치류 많이 팔죠?",
          "original_form": "오 야시장에 꼬치류 많이 팔죠?",
          "speaker_id": "1",
          "time": "20210906 12:20"
        },
        {
          "id": "MDRW2100032304.1.23",
          "form": "네 꼬치 뭐 만두 등등",
          "original_form": "네 꼬치 뭐 만두 등등",
          "speaker_id": "2",
          "time": "20210906 12:44"
        },
        {
          "id": "MDRW2100032304.1.24",
          "form": "진짜 값도 싸고",
          "original_form": "진짜 값도 싸고",
          "speaker_id": "2",
          "time": "20210906 12:44"
        },
        {
          "id": "MDRW2100032304.1.25",
          "form": "취두부만 빼면!",
          "original_form": "취두부만 빼면!",
          "speaker_id": "2",
          "time": "20210906 12:44"
        },
        {
          "id": "MDRW2100032304.1.26",
          "form": "최고에요",
          "original_form": "최고에요",
          "speaker_id": "2",
          "time": "20210906 12:44"
        }
      ]
    }
