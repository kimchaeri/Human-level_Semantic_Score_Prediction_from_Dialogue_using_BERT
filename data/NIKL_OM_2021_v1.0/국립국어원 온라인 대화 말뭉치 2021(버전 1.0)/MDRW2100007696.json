      "utterance": [
        {
          "id": "MDRW2100007696.1.1",
          "form": "배달먹어요?",
          "original_form": "배달먹어요?",
          "speaker_id": "1",
          "time": "20210822 09:44"
        },
        {
          "id": "MDRW2100007696.1.2",
          "form": "네 배달 좋아하죠.ㅋ",
          "original_form": "네 배달 좋아하죠.ㅋ",
          "speaker_id": "2",
          "time": "20210822 09:45"
        },
        {
          "id": "MDRW2100007696.1.3",
          "form": "배달해서 드세요?",
          "original_form": "배달해서 드세요?",
          "speaker_id": "2",
          "time": "20210822 09:45"
        },
        {
          "id": "MDRW2100007696.1.4",
          "form": "네 자주",
          "original_form": "네 자주",
          "speaker_id": "1",
          "time": "20210822 09:46"
        },
        {
          "id": "MDRW2100007696.1.5",
          "form": "뭐 좋아하세요?",
          "original_form": "뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 09:47"
        },
        {
          "id": "MDRW2100007696.1.6",
          "form": "치킨이요",
          "original_form": "치킨이요",
          "speaker_id": "1",
          "time": "20210822 09:47"
        },
        {
          "id": "MDRW2100007696.1.7",
          "form": "치킨 맛있죠.",
          "original_form": "치킨 맛있죠.",
          "speaker_id": "2",
          "time": "20210822 09:47"
        },
        {
          "id": "MDRW2100007696.1.8",
          "form": "저도 치킨 좋아해요.",
          "original_form": "저도 치킨 좋아해요.",
          "speaker_id": "2",
          "time": "20210822 09:47"
        },
        {
          "id": "MDRW2100007696.1.9",
          "form": "어떤 브랜드 좋아하세요?",
          "original_form": "어떤 브랜드 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 09:48"
        },
        {
          "id": "MDRW2100007696.1.10",
          "form": "네네요",
          "original_form": "네네요",
          "speaker_id": "1",
          "time": "20210822 09:48"
        },
        {
          "id": "MDRW2100007696.1.11",
          "form": "네네 좋죠ㅋㅋ",
          "original_form": "네네 좋죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:49"
        },
        {
          "id": "MDRW2100007696.1.12",
          "form": "저는 교촌치킨좋아해요.",
          "original_form": "저는 교촌치킨좋아해요.",
          "speaker_id": "2",
          "time": "20210822 09:49"
        },
        {
          "id": "MDRW2100007696.1.13",
          "form": "아 좋죠",
          "original_form": "아 좋죠",
          "speaker_id": "1",
          "time": "20210822 09:50"
        },
        {
          "id": "MDRW2100007696.1.14",
          "form": "네  레드교촌시키고 맛있게먹어요.ㅋㅋ",
          "original_form": "네  레드교촌시키고 맛있게먹어요.ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 09:50"
        },
        {
          "id": "MDRW2100007696.1.15",
          "form": "저도 먹어봐야 겠네요",
          "original_form": "저도 먹어봐야 겠네요",
          "speaker_id": "1",
          "time": "20210822 09:52"
        },
        {
          "id": "MDRW2100007696.1.16",
          "form": "저는 매운거 잘 못먹는데",
          "original_form": "저는 매운거 잘 못먹는데",
          "speaker_id": "2",
          "time": "20210822 09:53"
        },
        {
          "id": "MDRW2100007696.1.17",
          "form": "맛있게 맵더라구요, ㅋ",
          "original_form": "맛있게 맵더라구요, ㅋ",
          "speaker_id": "2",
          "time": "20210822 09:53"
        },
        {
          "id": "MDRW2100007696.1.18",
          "form": "군침이 도네요",
          "original_form": "군침이 도네요",
          "speaker_id": "1",
          "time": "20210822 09:54"
