      "utterance": [
        {
          "id": "MDRW2100038407.1.1",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "1",
          "time": "20210902 12:34"
        },
        {
          "id": "MDRW2100038407.1.2",
          "form": "김치찌개 먹엇어요 ㅋㅋㅋ",
          "original_form": "김치찌개 먹엇어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:36"
        },
        {
          "id": "MDRW2100038407.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210902 12:36"
        },
        {
          "id": "MDRW2100038407.1.4",
          "form": "오 저는 된장찌개 먹었어요 ㅎㅍ",
          "original_form": "오 저는 된장찌개 먹었어요 ㅎㅍ",
          "speaker_id": "1",
          "time": "20210902 12:39"
        },
        {
          "id": "MDRW2100038407.1.5",
          "form": "앗 찌개로 통햇군요 ㅋㅋㅋㅋ",
          "original_form": "앗 찌개로 통햇군요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:41"
        },
        {
          "id": "MDRW2100038407.1.6",
          "form": "그러게요 ㅋㅋ 쌀쌀한 날에는 찌개가 잘 어울리죠 ㅎㅎ",
          "original_form": "그러게요 ㅋㅋ 쌀쌀한 날에는 찌개가 잘 어울리죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 12:44"
        },
        {
          "id": "MDRW2100038407.1.7",
          "form": "맞아요 오늘 날이 쌀쌀해서 뜨끈한게 땡기더라구요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 오늘 날이 쌀쌀해서 뜨끈한게 땡기더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:52"
        },
        {
          "id": "MDRW2100038407.1.8",
          "form": "그쵸그쵸 ㅋㅋ 진짜 찌개종류는 다 맛있어요..",
          "original_form": "그쵸그쵸 ㅋㅋ 진짜 찌개종류는 다 맛있어요..",
          "speaker_id": "1",
          "time": "20210902 12:56"
        },
        {
          "id": "MDRW2100038407.1.9",
          "form": "어떤 찌개 제일 좋아하세요?",
          "original_form": "어떤 찌개 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 12:56"
        },
        {
          "id": "MDRW2100038407.1.10",
          "form": "다 좋아하는데 순두부찌개를 제일 좋아하는 것 같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "다 좋아하는데 순두부찌개를 제일 좋아하는 것 같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:57"
        },
        {
          "id": "MDRW2100038407.1.11",
          "form": "저도 찌개종류 다 좋아하는데 제일 좋아하는건 김치찌개고 그다음이 순두부예요",
          "original_form": "저도 찌개종류 다 좋아하는데 제일 좋아하는건 김치찌개고 그다음이 순두부예요",
          "speaker_id": "1",
          "time": "20210902 12:58"
        },
        {
          "id": "MDRW2100038407.1.12",
          "form": "순두부...진짜 칼칼하고 몽글하니 맛나지 않나요 ㅠ",
          "original_form": "순두부...진짜 칼칼하고 몽글하니 맛나지 않나요 ㅠ",
          "speaker_id": "1",
          "time": "20210902 12:58"
        },
        {
          "id": "MDRW2100038407.1.13",
          "form": "맞아요 식감은 몽글몽글한데 맛은 칼칼해서 좋아요",
          "original_form": "맞아요 식감은 몽글몽글한데 맛은 칼칼해서 좋아요",
          "speaker_id": "2",
          "time": "20210902 13:18"
        },
        {
          "id": "MDRW2100038407.1.14",
          "form": "그쵸 순두부에 계란 넣으시나요??",
          "original_form": "그쵸 순두부에 계란 넣으시나요??",
          "speaker_id": "1",
          "time": "20210902 15:01"
        },
        {
          "id": "MDRW2100038407.1.15",
          "form": "네 계란 넣는거 좋아해요",
          "original_form": "네 계란 넣는거 좋아해요",
          "speaker_id": "2",
          "time": "20210902 19:04"
        },
        {
          "id": "MDRW2100038407.1.16",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210902 19:05"
        },
        {
          "id": "MDRW2100038407.1.17",
          "form": "네네 저도 좋아해서 꼭 넣어먹어요ㅎㅎ",
