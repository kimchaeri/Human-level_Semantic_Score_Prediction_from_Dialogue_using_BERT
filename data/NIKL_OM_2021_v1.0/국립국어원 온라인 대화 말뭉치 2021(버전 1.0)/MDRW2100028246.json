      "utterance": [
        {
          "id": "MDRW2100028246.1.1",
          "form": "VR게임 재밌더라고요",
          "original_form": "VR게임 재밌더라고요",
          "speaker_id": "2",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100028246.1.2",
          "form": "비트세이버? 그런것도 해보고",
          "original_form": "비트세이버? 그런것도 해보고",
          "speaker_id": "2",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028246.1.3",
          "form": "네 요새 브이알 많이생겼던데",
          "original_form": "네 요새 브이알 많이생겼던데",
          "speaker_id": "1",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028246.1.4",
          "form": "파판 가상현실생기면 재밌을것같아요",
          "original_form": "파판 가상현실생기면 재밌을것같아요",
          "speaker_id": "1",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028246.1.5",
          "form": "아 맞아요~~",
          "original_form": "아 맞아요~~",
          "speaker_id": "2",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028246.1.6",
          "form": "세계관 거대한 게임이나",
          "original_form": "세계관 거대한 게임이나",
          "speaker_id": "2",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028246.1.7",
          "form": "달빛조각사처럼 게임판타지물에서나오는 게임들 ㅋㅋㅋㅋ",
          "original_form": "달빛조각사처럼 게임판타지물에서나오는 게임들 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028246.1.8",
          "form": "현실 구현이 어려운 게임들은 진짜...",
          "original_form": "현실 구현이 어려운 게임들은 진짜...",
          "speaker_id": "2",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028246.1.9",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028246.1.10",
          "form": "죽기전에 나오지않을까...",
          "original_form": "죽기전에 나오지않을까...",
          "speaker_id": "1",
          "time": "20210902 18:34"
        },
        {
          "id": "MDRW2100028246.1.11",
          "form": "해리포터 VR게임도 나오면 해보고싶어요",
          "original_form": "해리포터 VR게임도 나오면 해보고싶어요",
          "speaker_id": "2",
          "time": "20210902 18:34"
        },
        {
          "id": "MDRW2100028246.1.12",
          "form": "맞아여 ㅋㅋㅋㅋ빗자루타고 막",
          "original_form": "맞아여 ㅋㅋㅋㅋ빗자루타고 막",
          "speaker_id": "1",
          "time": "20210902 18:34"
        },
        {
          "id": "MDRW2100028246.1.13",
          "form": "상상만 해도 재밌겠다 ㅋㅋㅋ",
          "original_form": "상상만 해도 재밌겠다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:34"
        },
        {
          "id": "MDRW2100028246.1.14",
          "form": "퀴디치시합도하고",
          "original_form": "퀴디치시합도하고",
          "speaker_id": "1",
          "time": "20210902 18:34"
        },
        {
          "id": "MDRW2100028246.1.15",
          "form": "스네이프의 마법약 교실 이런것도 하구요 ㅋㅋ",
          "original_form": "스네이프의 마법약 교실 이런것도 하구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:35"
        },
        {
          "id": "MDRW2100028246.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ기숙사탐험도재밌을거같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ기숙사탐험도재밌을거같아요",
