      "utterance": [
        {
          "id": "MDRW2100015389.1.1",
          "form": "다이어트 하고 계세요?",
          "original_form": "다이어트 하고 계세요?",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015389.1.2",
          "form": "영원한 다이어터입니다",
          "original_form": "영원한 다이어터입니다",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015389.1.3",
          "form": "말로만",
          "original_form": "말로만",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015389.1.4",
          "form": "저도 입으로만 다이어터엿는데",
          "original_form": "저도 입으로만 다이어터엿는데",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015389.1.5",
          "form": "코로나 때문에 타격이 커서 ㅋㅋㅋㅋㅋ",
          "original_form": "코로나 때문에 타격이 커서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015389.1.6",
          "form": "확찐자이신가요?",
          "original_form": "확찐자이신가요?",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015389.1.7",
          "form": "코로나니까 덜먹고 덜움직여야하는데",
          "original_form": "코로나니까 덜먹고 덜움직여야하는데",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015389.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ네 오랫동안 확찐자였다가 이제야 겨우 탈출",
          "original_form": "ㅋㅋㅋㅋㅋㅋ네 오랫동안 확찐자였다가 이제야 겨우 탈출",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015389.1.9",
          "form": "고통스런 시간을 보냈어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "고통스런 시간을 보냈어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015389.1.10",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "1",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015389.1.11",
          "form": "비법은요?",
          "original_form": "비법은요?",
          "speaker_id": "1",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015389.1.12",
          "form": "홈트 열심히 하구요",
          "original_form": "홈트 열심히 하구요",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015389.1.13",
          "form": "식단조절도 하구요",
          "original_form": "식단조절도 하구요",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015389.1.14",
          "form": "사실 그냥 살만쪘을때는 다이어트에대한 의지가 생기질 않앗는데",
          "original_form": "사실 그냥 살만쪘을때는 다이어트에대한 의지가 생기질 않앗는데",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015389.1.15",
          "form": "쉬운길은 없군요",
          "original_form": "쉬운길은 없군요",
          "speaker_id": "1",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015389.1.16",
          "form": "확찐자가 되면서 건강도 상하더라구요",
          "original_form": "확찐자가 되면서 건강도 상하더라구요",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015389.1.17",
          "form": "건강이 무너지니까 위기의식이 확 생겨서",
          "original_form": "건강이 무너지니까 위기의식이 확 생겨서",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015389.1.18",
          "form": "그게 원동력이 되었던 것 같아요",
          "original_form": "그게 원동력이 되었던 것 같아요",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015389.1.19",
          "form": "날좀좋아지면 하루 10키로 걷기 해야겠어요",
          "original_form": "날좀좋아지면 하루 10키로 걷기 해야겠어요",
          "speaker_id": "1",
          "time": "20210825 23:19"
        },
        {
          "id": "MDRW2100015389.1.20",
          "form": "맞아요 걷다가 체력붙으면 런데이하시는 분들도 많더라구요",
          "original_form": "맞아요 걷다가 체력붙으면 런데이하시는 분들도 많더라구요",
          "speaker_id": "2",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015389.1.21",
          "form": "뛰는건...해보겠 습니다",
          "original_form": "뛰는건...해보겠 습니다",
          "speaker_id": "1",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100015389.1.22",
          "form": "ㅋㅋㅋㅋㅋ화이팅이에요",
          "original_form": "ㅋㅋㅋㅋㅋ화이팅이에요",
          "speaker_id": "2",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015389.1.23",
          "form": "저도 런닝화 사봐야겟어요",
          "original_form": "저도 런닝화 사봐야겟어요",
          "speaker_id": "2",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015389.1.24",
          "form": "저두요 땀복도 하나 장만해야겠어요",
          "original_form": "저두요 땀복도 하나 장만해야겠어요",
          "speaker_id": "1",
          "time": "20210826 08:11"
        },
        {
          "id": "MDRW2100015389.1.25",
          "form": "저도 바람막이도 사고 싶다 라고 생각하고 있어요 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 바람막이도 사고 싶다 라고 생각하고 있어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:00"
        },
        {
          "id": "MDRW2100015389.1.26",
          "form": "역시 운동은 장비빨...!!",
          "original_form": "역시 운동은 장비빨...!!",
          "speaker_id": "2",
