      "utterance": [
        {
          "id": "MDRW2100022443.1.1",
          "form": "최근 구매 물건은?",
          "original_form": "최근 구매 물건은?",
          "speaker_id": "2",
          "time": "20210829 14:09"
        },
        {
          "id": "MDRW2100022443.1.2",
          "form": "바닐라코 클렌징 밤 샀어",
          "original_form": "바닐라코 클렌징 밤 샀어",
          "speaker_id": "1",
          "time": "20210829 14:14"
        },
        {
          "id": "MDRW2100022443.1.3",
          "form": "그거 좋니?",
          "original_form": "그거 좋니?",
          "speaker_id": "2",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100022443.1.4",
          "form": "예상했던 것보다 클렌징 더 잘 돼서 완전 득템했어",
          "original_form": "예상했던 것보다 클렌징 더 잘 돼서 완전 득템했어",
          "speaker_id": "1",
          "time": "20210829 14:17"
        },
        {
          "id": "MDRW2100022443.1.5",
          "form": "클렌징 밤이면 꾸덕할거같애",
          "original_form": "클렌징 밤이면 꾸덕할거같애",
          "speaker_id": "2",
          "time": "20210829 14:19"
        },
        {
          "id": "MDRW2100022443.1.6",
          "form": "샤베트 제형이야",
          "original_form": "샤베트 제형이야",
          "speaker_id": "1",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100022443.1.7",
          "form": "완전 샤르륵 녹아",
          "original_form": "완전 샤르륵 녹아",
          "speaker_id": "1",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100022443.1.8",
          "form": "갑자기 레인보우샤베트가 생각이 나는군",
          "original_form": "갑자기 레인보우샤베트가 생각이 나는군",
          "speaker_id": "2",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100022443.1.9",
          "form": "난  추워지면 중고거래했던 퍼자켓 입을거야",
          "original_form": "난  추워지면 중고거래했던 퍼자켓 입을거야",
          "speaker_id": "2",
          "time": "20210829 14:22"
        },
        {
          "id": "MDRW2100022443.1.10",
          "form": "오 그거 촉감 너무 좋더라",
          "original_form": "오 그거 촉감 너무 좋더라",
          "speaker_id": "1",
          "time": "20210829 14:23"
        },
        {
          "id": "MDRW2100022443.1.11",
          "form": "난 곧 가을 옷을 구매할 예정이야",
          "original_form": "난 곧 가을 옷을 구매할 예정이야",
          "speaker_id": "1",
          "time": "20210829 14:23"
        },
        {
          "id": "MDRW2100022443.1.12",
          "form": "나도 월급들어오면 구매할거야",
          "original_form": "나도 월급들어오면 구매할거야",
          "speaker_id": "2",
          "time": "20210829 14:23"
        },
        {
          "id": "MDRW2100022443.1.13",
          "form": "쿠팡에 에뛰드 할인하는건 알고 있니?",
          "original_form": "쿠팡에 에뛰드 할인하는건 알고 있니?",
          "speaker_id": "1",
          "time": "20210829 14:25"
        },
        {
          "id": "MDRW2100022443.1.14",
          "form": "틴트랑 아이섀도 2만원짜리 합쳐서 6천 원대야!",
          "original_form": "틴트랑 아이섀도 2만원짜리 합쳐서 6천 원대야!",
          "speaker_id": "1",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100022443.1.15",
          "form": "정말?",
          "original_form": "정말?",
          "speaker_id": "2",
          "time": "20210829 14:27"
        },
        {
          "id": "MDRW2100022443.1.16",
          "form": "하지만 나는 쿠팡가입을 하지않앗어",
          "original_form": "하지만 나는 쿠팡가입을 하지않앗어",
          "speaker_id": "2",
          "time": "20210829 14:27"
        },
        {
          "id": "MDRW2100022443.1.17",
          "form": "정말정말 좋은 찬스라서 지금 고민하고 있어",
          "original_form": "정말정말 좋은 찬스라서 지금 고민하고 있어",
          "speaker_id": "1",
          "time": "20210829 14:27"
        },
        {
          "id": "MDRW2100022443.1.18",
          "form": "얼른 사야지",
          "original_form": "얼른 사야지",
          "speaker_id": "2",
          "time": "20210829 14:44"
        },
        {
          "id": "MDRW2100022443.1.19",
          "form": "조금만 더 고민 해봐야겠어!",
          "original_form": "조금만 더 고민 해봐야겠어!",
          "speaker_id": "1",
          "time": "20210829 14:45"
        }
      ]
