      "utterance": [
        {
          "id": "MDRW2100016777.1.1",
          "form": "네 보아요. name1은요",
          "original_form": "네 보아요. &name1&은요",
          "speaker_id": "2",
          "time": "20210826 16:17"
        },
        {
          "id": "MDRW2100016777.1.2",
          "form": "저도 좋아합니다ㅎㅎ",
          "original_form": "저도 좋아합니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 16:34"
        },
        {
          "id": "MDRW2100016777.1.3",
          "form": "연예인 누구 좋아하나요?",
          "original_form": "연예인 누구 좋아하나요?",
          "speaker_id": "2",
          "time": "20210826 16:56"
        },
        {
          "id": "MDRW2100016777.1.4",
          "form": "에스파요!",
          "original_form": "에스파요!",
          "speaker_id": "1",
          "time": "20210826 17:55"
        },
        {
          "id": "MDRW2100016777.1.5",
          "form": "에스파 요즘 노래 좋던데요",
          "original_form": "에스파 요즘 노래 좋던데요",
          "speaker_id": "2",
          "time": "20210826 18:00"
        },
        {
          "id": "MDRW2100016777.1.6",
          "form": "넥스트레벨 꽂혔어요",
          "original_form": "넥스트레벨 꽂혔어요",
          "speaker_id": "1",
          "time": "20210826 18:03"
        },
        {
          "id": "MDRW2100016777.1.7",
          "form": "한동안 차에서 많이 들었어요",
          "original_form": "한동안 차에서 많이 들었어요",
          "speaker_id": "2",
          "time": "20210826 19:05"
        },
        {
          "id": "MDRW2100016777.1.8",
          "form": "중독성 심해요ㅎㅎ",
          "original_form": "중독성 심해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 20:32"
        },
        {
          "id": "MDRW2100016777.1.9",
          "form": "맞아요. 첫부분 땜에 다운 받아 듣게 됐어요.",
          "original_form": "맞아요. 첫부분 땜에 다운 받아 듣게 됐어요.",
          "speaker_id": "2",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100016777.1.10",
          "form": "지금도듣고있어요ㅎ",
          "original_form": "지금도듣고있어요ㅎ",
          "speaker_id": "1",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100016777.1.11",
          "form": "ㅎㅎ 전 운전할 때 많이 들어서 지금은 다른 노래 들어요 ㅋ",
          "original_form": "ㅎㅎ 전 운전할 때 많이 들어서 지금은 다른 노래 들어요 ㅋ",
          "speaker_id": "2",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100016777.1.12",
          "form": "요즘은 사랑은 유리 같은 것 이 노래 많이 듣고 있어요",
          "original_form": "요즘은 사랑은 유리 같은 것 이 노래 많이 듣고 있어요",
          "speaker_id": "2",
          "time": "20210827 13:33"
        },
        {
          "id": "MDRW2100016777.1.13",
          "form": "오 처음들어보네요",
          "original_form": "오 처음들어보네요",
          "speaker_id": "1",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100016777.1.14",
          "form": "예전 노래인거 같은데 요즘 가수가 부른거 같더라구요.",
          "original_form": "예전 노래인거 같은데 요즘 가수가 부른거 같더라구요.",
          "speaker_id": "2",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100016777.1.15",
          "form": "가을에 듣기 좋아서 많이 듣고 있어요",
          "original_form": "가을에 듣기 좋아서 많이 듣고 있어요",
          "speaker_id": "2",
