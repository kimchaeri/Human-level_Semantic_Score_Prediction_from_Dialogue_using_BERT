          "time": "20210907 10:51"
        },
        {
          "id": "MDRW2100042886.1.13",
          "form": "그거는 뭐에먹어",
          "original_form": "그거는 뭐에먹어",
          "speaker_id": "1",
          "time": "20210907 10:51"
        },
        {
          "id": "MDRW2100042886.1.14",
          "form": "소고기랑 잘어울려",
          "original_form": "소고기랑 잘어울려",
          "speaker_id": "2",
          "time": "20210907 10:51"
