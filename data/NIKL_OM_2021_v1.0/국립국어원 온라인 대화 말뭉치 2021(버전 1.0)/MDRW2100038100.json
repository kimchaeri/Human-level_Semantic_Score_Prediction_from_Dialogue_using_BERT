        {
          "id": "MDRW2100038100.1.1",
          "form": "나 애니메이션 좋아해",
          "original_form": "나 애니메이션 좋아해",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100038100.1.2",
          "form": "나도 디즈니 좋아해",
          "original_form": "나도 디즈니 좋아해",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100038100.1.3",
          "form": "픽사는 최고야",
          "original_form": "픽사는 최고야",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100038100.1.4",
          "form": "토이 스토리가 짱이긴 해",
          "original_form": "토이 스토리가 짱이긴 해",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100038100.1.5",
          "form": "토이 스토리에서 포키 너무 귀여워",
          "original_form": "토이 스토리에서 포키 너무 귀여워",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100038100.1.6",
          "form": "나는 포키 캐릭터는 별로 안 좋아해",
          "original_form": "나는 포키 캐릭터는 별로 안 좋아해",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100038100.1.7",
          "form": "그럼 무슨 캐릭터 좋아해",
          "original_form": "그럼 무슨 캐릭터 좋아해",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100038100.1.8",
          "form": "주인공인 우디",
          "original_form": "주인공인 우디",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100038100.1.9",
          "form": "이번에 신 캐릭터인 더 키 버니도 귀엽던데",
          "original_form": "이번에 신 캐릭터인 더 키 버니도 귀엽던데",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100038100.1.10",
          "form": "맞아 나 걔네 팬 됐어",
          "original_form": "맞아 나 걔네 팬 됐어",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100038100.1.11",
          "form": "픽사에서 업도 재밌어",
          "original_form": "픽사에서 업도 재밌어",
          "speaker_id": "2",
          "time": "20210901 23:13"
        },
