      "utterance": [
        {
          "id": "MDRW2100026903.1.1",
          "form": "올림픽 보셨나요",
          "original_form": "올림픽 보셨나요",
          "speaker_id": "2",
          "time": "20210902 01:11"
        },
        {
          "id": "MDRW2100026903.1.2",
          "form": "네 실시간으로 봤습니다",
          "original_form": "네 실시간으로 봤습니다",
          "speaker_id": "1",
          "time": "20210902 01:11"
        },
        {
          "id": "MDRW2100026903.1.3",
          "form": "양궁, 배구, 펜싱",
          "original_form": "양궁, 배구, 펜싱",
          "speaker_id": "1",
          "time": "20210902 01:11"
        },
        {
          "id": "MDRW2100026903.1.4",
          "form": "전 맘졸여서 실시간으로 못보고",
          "original_form": "전 맘졸여서 실시간으로 못보고",
          "speaker_id": "2",
          "time": "20210902 01:11"
        },
        {
          "id": "MDRW2100026903.1.5",
          "form": "결과만봤어요",
          "original_form": "결과만봤어요",
          "speaker_id": "2",
          "time": "20210902 01:11"
        },
        {
          "id": "MDRW2100026903.1.6",
          "form": "안산선수 짱",
          "original_form": "안산선수 짱",
          "speaker_id": "2",
          "time": "20210902 01:12"
        },
        {
          "id": "MDRW2100026903.1.7",
          "form": "높이뛰기도 재밋었어요",
          "original_form": "높이뛰기도 재밋었어요",
          "speaker_id": "1",
          "time": "20210902 01:12"
        },
        {
          "id": "MDRW2100026903.1.8",
          "form": "양궁선수들 짱멋졌어요",
          "original_form": "양궁선수들 짱멋졌어요",
          "speaker_id": "2",
          "time": "20210902 01:12"
        },
        {
          "id": "MDRW2100026903.1.9",
          "form": "배구 언니들도 짱",
          "original_form": "배구 언니들도 짱",
          "speaker_id": "2",
          "time": "20210902 01:12"
        },
        {
          "id": "MDRW2100026903.1.10",
          "form": "공포영화는 안졸이시나요?  ㅡㅡ",
          "original_form": "공포영화는 안졸이시나요?  ㅡㅡ",
          "speaker_id": "1",
          "time": "20210902 01:12"
        },
        {
          "id": "MDRW2100026903.1.11",
          "form": "영화보다 더 영화같았어요",
          "original_form": "영화보다 더 영화같았어요",
          "speaker_id": "1",
          "time": "20210902 01:12"
        },
        {
          "id": "MDRW2100026903.1.12",
          "form": "영화는 결말이 정해저있지만",
          "original_form": "영화는 결말이 정해저있지만",
          "speaker_id": "2",
          "time": "20210902 01:12"
        },
        {
          "id": "MDRW2100026903.1.13",
          "form": "배구는 저도 생방송 못봤습니다",
          "original_form": "배구는 저도 생방송 못봤습니다",
          "speaker_id": "1",
          "time": "20210902 01:12"
        },
        {
          "id": "MDRW2100026903.1.14",
          "form": "스포츠는 바뀔수 있잖아요",
          "original_form": "스포츠는 바뀔수 있잖아요",
          "speaker_id": "2",
          "time": "20210902 01:12"
        },
        {
          "id": "MDRW2100026903.1.15",
          "form": "인간승리의 결산",
          "original_form": "인간승리의 결산",
          "speaker_id": "2",
          "time": "20210902 01:13"
        },
        {
          "id": "MDRW2100026903.1.16",
          "form": "높이뛰기도 멋졌어요",
          "original_form": "높이뛰기도 멋졌어요",
          "speaker_id": "2",
          "time": "20210902 01:13"
        },
        {
          "id": "MDRW2100026903.1.17",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210902 01:13"
        },
        {
          "id": "MDRW2100026903.1.18",
          "form": "메달을 못따도 노력하는 모습이 좋네요",
          "original_form": "메달을 못따도 노력하는 모습이 좋네요",
          "speaker_id": "2",
          "time": "20210902 01:13"
        },
        {
          "id": "MDRW2100026903.1.19",
          "form": "몇달뒤 동계올림픽도 하죠",
          "original_form": "몇달뒤 동계올림픽도 하죠",
          "speaker_id": "1",
          "time": "20210902 01:13"
        },
        {
          "id": "MDRW2100026903.1.20",
          "form": "기대중이에요",
          "original_form": "기대중이에요",
          "speaker_id": "2",
          "time": "20210902 01:13"
        },
        {
          "id": "MDRW2100026903.1.21",
          "form": "김연아선수 좋아했었는데 그립네요",
          "original_form": "김연아선수 좋아했었는데 그립네요",
