      "utterance": [
        {
          "id": "MDRW2100034611.1.1",
          "form": "ㅠㅠ 걸려서 사망하면 너무 슬픈거고",
          "original_form": "ㅠㅠ 걸려서 사망하면 너무 슬픈거고",
          "speaker_id": "2",
          "time": "20210828 16:18"
        },
        {
          "id": "MDRW2100034611.1.2",
          "form": "넵 ㅜㅜ",
          "original_form": "넵 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 16:18"
        },
        {
          "id": "MDRW2100034611.1.3",
          "form": "저희는 그나마",
          "original_form": "저희는 그나마",
          "speaker_id": "1",
          "time": "20210828 16:18"
        },
        {
          "id": "MDRW2100034611.1.4",
          "form": "사망안하셔도 이게 워너래 상태로 몸이 돌아간단 보장이 없으니",
          "original_form": "사망안하셔도 이게 워너래 상태로 몸이 돌아간단 보장이 없으니",
          "speaker_id": "2",
          "time": "20210828 16:18"
        },
        {
          "id": "MDRW2100034611.1.5",
          "form": "젊어서 나은데 나이드신 분들은 더 걱정돼요",
          "original_form": "젊어서 나은데 나이드신 분들은 더 걱정돼요",
          "speaker_id": "1",
          "time": "20210828 16:18"
        },
        {
          "id": "MDRW2100034611.1.6",
          "form": "맞아요 한혜진도 걸렸었는데 호흡이 70%정도만 된대요 후유증으로..",
          "original_form": "맞아요 한혜진도 걸렸었는데 호흡이 70%정도만 된대요 후유증으로..",
          "speaker_id": "1",
          "time": "20210828 16:18"
        },
        {
          "id": "MDRW2100034611.1.7",
          "form": "아 거기 나오죠 뭐지 예능인데",
          "original_form": "아 거기 나오죠 뭐지 예능인데",
          "speaker_id": "2",
          "time": "20210828 16:19"
        },
        {
          "id": "MDRW2100034611.1.8",
          "form": "축구하는 언니들인가 이름을 까먹었네요",
          "original_form": "축구하는 언니들인가 이름을 까먹었네요",
          "speaker_id": "2",
          "time": "20210828 16:19"
        },
        {
          "id": "MDRW2100034611.1.9",
          "form": "뭔지는 알거같아요",
          "original_form": "뭔지는 알거같아요",
          "speaker_id": "1",
          "time": "20210828 16:19"
        },
        {
          "id": "MDRW2100034611.1.10",
          "form": "기사로 사진만 봤어요",
          "original_form": "기사로 사진만 봤어요",
          "speaker_id": "1",
          "time": "20210828 16:19"
        },
        {
          "id": "MDRW2100034611.1.11",
          "form": "그 예능서 한 얘기래요 축구예능요",
          "original_form": "그 예능서 한 얘기래요 축구예능요",
          "speaker_id": "2",
          "time": "20210828 16:19"
        },
        {
          "id": "MDRW2100034611.1.12",
          "form": "코로나를 완쾌해도 바로 뛰려니 숨이 달리겠죠",
          "original_form": "코로나를 완쾌해도 바로 뛰려니 숨이 달리겠죠",
          "speaker_id": "2",
          "time": "20210828 16:19"
        },
        {
          "id": "MDRW2100034611.1.13",
          "form": "아~ 그럼 운동할때 얘긴가요",
          "original_form": "아~ 그럼 운동할때 얘긴가요",
          "speaker_id": "1",
          "time": "20210828 16:19"
        },
        {
          "id": "MDRW2100034611.1.14",
          "form": "아마 뛸떄 숨이 안 받쳐준단 얘길거에요",
          "original_form": "아마 뛸떄 숨이 안 받쳐준단 얘길거에요",
          "speaker_id": "2",
          "time": "20210828 16:20"
        },
        {
          "id": "MDRW2100034611.1.15",
          "form": "근데 평상시에도 좀 숨가쁜 느낌은 있겠죠",
          "original_form": "근데 평상시에도 좀 숨가쁜 느낌은 있겠죠",
          "speaker_id": "2",
          "time": "20210828 16:20"
        },
        {
          "id": "MDRW2100034611.1.16",
          "form": "평상시에도 숨 못쉬고 앉아서도 헉헉 몰아쉴 정도면",
          "original_form": "평상시에도 숨 못쉬고 앉아서도 헉헉 몰아쉴 정도면",
          "speaker_id": "2",
          "time": "20210828 16:21"
        },
        {
          "id": "MDRW2100034611.1.17",
          "form": "장애등급이 나올 정도로 나쁜거에요",
          "original_form": "장애등급이 나올 정도로 나쁜거에요",
          "speaker_id": "2",
          "time": "20210828 16:21"
        },
        {
          "id": "MDRW2100034611.1.18",
          "form": "전 원래도 평상시에 가끔 숨딸리던데",
          "original_form": "전 원래도 평상시에 가끔 숨딸리던데",
          "speaker_id": "1",
          "time": "20210828 16:21"
        },
        {
          "id": "MDRW2100034611.1.19",
          "form": "힘들겠어요",
          "original_form": "힘들겠어요",
          "speaker_id": "1",
          "time": "20210828 16:22"
        },
        {
          "id": "MDRW2100034611.1.20",
          "form": "그럼 코로나를 조심하셔야겠어요",
          "original_form": "그럼 코로나를 조심하셔야겠어요",
          "speaker_id": "2",
          "time": "20210828 16:22"
        },
        {
          "id": "MDRW2100034611.1.21",
          "form": "네 ㅠㅠ 무서워요",
          "original_form": "네 ㅠㅠ 무서워요",
          "speaker_id": "1",
          "time": "20210828 16:22"
        },
        {
          "id": "MDRW2100034611.1.22",
          "form": "스페인 독감때보단 덜 죽는다곤 해도 많이 죽었거든요",
          "original_form": "스페인 독감때보단 덜 죽는다곤 해도 많이 죽었거든요",
          "speaker_id": "2",
          "time": "20210828 16:22"
        }
      ]
