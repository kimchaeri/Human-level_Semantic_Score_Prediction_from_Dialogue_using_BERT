        },
        {
          "id": "MDRW2100000716.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210819 18:54"
        },
        {
          "id": "MDRW2100000716.1.3",
          "form": "요즘 날씨 진짜 몇년만에 꽤 더운거같아요",
          "original_form": "요즘 날씨 진짜 몇년만에 꽤 더운거같아요",
          "speaker_id": "1",
          "time": "20210819 19:56"
        },
        {
          "id": "MDRW2100000716.1.4",
          "form": "그냥 가만히 잇어도 땀나는건 올해가 처음인거같아요",
          "original_form": "그냥 가만히 잇어도 땀나는건 올해가 처음인거같아요",
          "speaker_id": "1",
          "time": "20210819 20:01"
        },
        {
          "id": "MDRW2100000716.1.5",
          "form": "맞아요 너무 더워요ㅠㅠ",
          "original_form": "맞아요 너무 더워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100000716.1.6",
          "form": "아직 에어컨 틀고잇나요?",
          "original_form": "아직 에어컨 틀고잇나요?",
          "speaker_id": "1",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100000716.1.7",
          "form": "전 틀고있어요 말복이 지났는대 왜이렇게 더운가요",
          "original_form": "전 틀고있어요 말복이 지났는대 왜이렇게 더운가요",
          "speaker_id": "1",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100000716.1.8",
          "form": "너무 더워서 저는 스벅에 놀러왔어요",
          "original_form": "너무 더워서 저는 스벅에 놀러왔어요",
          "speaker_id": "2",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100000716.1.9",
          "form": "여긴 시원해서 계속 있고싶어요",
          "original_form": "여긴 시원해서 계속 있고싶어요",
          "speaker_id": "2",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100000716.1.10",
          "form": "스벅은 시원하죠",
          "original_form": "스벅은 시원하죠",
          "speaker_id": "1",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100000716.1.11",
          "form": "전 아직 덥던대 에어컨 틀 날씨가 아닌가요?",
          "original_form": "전 아직 덥던대 에어컨 틀 날씨가 아닌가요?",
          "speaker_id": "1",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100000716.1.12",
          "form": "에어컨 없이는 못살아요ㅠㅠ",
          "original_form": "에어컨 없이는 못살아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 20:29"
        },
        {
          "id": "MDRW2100000716.1.13",
          "form": "그쵸?",
          "original_form": "그쵸?",
          "speaker_id": "1",
          "time": "20210819 20:29"
        },
        {
          "id": "MDRW2100000716.1.14",
          "form": "지인들은 춥다고하던대",
          "original_form": "지인들은 춥다고하던대",
          "speaker_id": "1",
          "time": "20210819 20:29"
        },
        {
          "id": "MDRW2100000716.1.15",
          "form": "더운게 맞는가봐요",
          "original_form": "더운게 맞는가봐요",
          "speaker_id": "1",
          "time": "20210819 20:29"
        },
        {
          "id": "MDRW2100000716.1.16",
          "form": "지금은 더워요ㅠㅠ",
          "original_form": "지금은 더워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000716.1.17",
          "form": "9월 돼야지 시원해질것같아요",
          "original_form": "9월 돼야지 시원해질것같아요",
          "speaker_id": "2",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000716.1.18",
          "form": "9월초는 지나야할거같죠",
          "original_form": "9월초는 지나야할거같죠",
          "speaker_id": "1",
          "time": "20210819 20:44"
        },
        {
          "id": "MDRW2100000716.1.19",
          "form": "그때쯤이면 날씨가 선선해 지겠죠?",
          "original_form": "그때쯤이면 날씨가 선선해 지겠죠?",
          "speaker_id": "2",
          "time": "20210819 21:05"
        },
        {
          "id": "MDRW2100000716.1.20",
          "form": "밤에 잘때 날씨 괜찮던가요?",
          "original_form": "밤에 잘때 날씨 괜찮던가요?",
          "speaker_id": "2",
          "time": "20210820 09:57"
        },
        {
          "id": "MDRW2100000716.1.21",
          "form": "그래도 아직",
          "original_form": "그래도 아직",
          "speaker_id": "1",
          "time": "20210820 11:50"
        },
        {
          "id": "MDRW2100000716.1.22",
          "form": "더운날씨같아요",
          "original_form": "더운날씨같아요",
          "speaker_id": "1",
          "time": "20210820 11:51"
        },
        {
          "id": "MDRW2100000716.1.23",
          "form": "다음주 27도던대 시원한걸까요",
