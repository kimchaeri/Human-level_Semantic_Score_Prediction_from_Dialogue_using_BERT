        {
          "id": "MDRW2100045686.1.1",
          "form": "나 용산에 유명한 카페 알아냈어",
          "original_form": "나 용산에 유명한 카페 알아냈어",
          "speaker_id": "1",
          "time": "20210908 05:25"
        },
        {
          "id": "MDRW2100045686.1.2",
          "form": "오 어디야",
          "original_form": "오 어디야",
          "speaker_id": "2",
          "time": "20210908 05:26"
        },
        {
          "id": "MDRW2100045686.1.3",
          "form": "요즘 하이브 사옥 근처 맛집이 핫하더라",
          "original_form": "요즘 하이브 사옥 근처 맛집이 핫하더라",
          "speaker_id": "1",
          "time": "20210908 05:26"
        },
        {
          "id": "MDRW2100045686.1.4",
          "form": "아 그주위가 뜨는구나",
          "original_form": "아 그주위가 뜨는구나",
          "speaker_id": "2",
          "time": "20210908 05:26"
        },
        {
          "id": "MDRW2100045686.1.5",
          "form": "어프로치라는 브런치 카페야",
          "original_form": "어프로치라는 브런치 카페야",
          "speaker_id": "1",
          "time": "20210908 05:26"
        },
        {
          "id": "MDRW2100045686.1.6",
          "form": "어프로치? 뭐파는거야",
          "original_form": "어프로치? 뭐파는거야",
          "speaker_id": "2",
          "time": "20210908 05:26"
        },
        {
          "id": "MDRW2100045686.1.7",
          "form": "ㅋㅋ브런지가 유명하대",
          "original_form": "ㅋㅋ브런지가 유명하대",
          "speaker_id": "1",
          "time": "20210908 05:27"
        },
        {
          "id": "MDRW2100045686.1.8",
          "form": "안그래도 그쪽 놀러가야하는데",
          "original_form": "안그래도 그쪽 놀러가야하는데",
          "speaker_id": "2",
          "time": "20210908 05:27"
        },
        {
          "id": "MDRW2100045686.1.9",
          "form": "그니까 하이브 갔다가 거기 가자",
          "original_form": "그니까 하이브 갔다가 거기 가자",
          "speaker_id": "1",
          "time": "20210908 05:27"
        },
        {
          "id": "MDRW2100045686.1.10",
          "form": "다른 맛집도 알아봤어?",
          "original_form": "다른 맛집도 알아봤어?",
          "speaker_id": "2",
          "time": "20210908 05:27"
        },
        {
