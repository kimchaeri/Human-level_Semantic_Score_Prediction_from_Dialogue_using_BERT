      "utterance": [
        {
          "id": "MDRW2100005385.1.1",
          "form": "아 나 유튜브 잘 안보는데 전에 재밌게 본 거 있다",
          "original_form": "아 나 유튜브 잘 안보는데 전에 재밌게 본 거 있다",
          "speaker_id": "2",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005385.1.2",
          "form": "뭔데 나도 알려주라",
          "original_form": "뭔데 나도 알려주라",
          "speaker_id": "1",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005385.1.3",
          "form": "자빱티비라고 게임을 스토리 만들어서 보여주는 건데",
          "original_form": "자빱티비라고 게임을 스토리 만들어서 보여주는 건데",
          "speaker_id": "2",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005385.1.4",
          "form": "오 게임 스토리?",
          "original_form": "오 게임 스토리?",
          "speaker_id": "1",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005385.1.5",
          "form": "마인크레프트라는 게임인데",
          "original_form": "마인크레프트라는 게임인데",
          "speaker_id": "2",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005385.1.6",
          "form": "우정리라고 있어",
          "original_form": "우정리라고 있어",
          "speaker_id": "1",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005385.1.7",
          "form": "우정리가 뭐야?",
          "original_form": "우정리가 뭐야?",
          "speaker_id": "2",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005385.1.8",
          "form": "마을 이름이 우정리야",
          "original_form": "마을 이름이 우정리야",
          "speaker_id": "1",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005385.1.9",
          "form": "오 신기하다 나는 저 게임 몰라",
          "original_form": "오 신기하다 나는 저 게임 몰라",
          "speaker_id": "2",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005385.1.10",
          "form": "영상으로 보기만 했지",
          "original_form": "영상으로 보기만 했지",
          "speaker_id": "2",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005385.1.11",
          "form": "마인크래프트가 마을 짓고 그런 게임이야",
          "original_form": "마인크래프트가 마을 짓고 그런 게임이야",
          "speaker_id": "1",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005385.1.12",
          "form": "아아 우정리",
          "original_form": "아아 우정리",
          "speaker_id": "2",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005385.1.13",
          "form": "자빱티비 나오는 거",
          "original_form": "자빱티비 나오는 거",
          "speaker_id": "2",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005385.1.14",
          "form": "나 저거 영상 캡쳐 봤어",
          "original_form": "나 저거 영상 캡쳐 봤어",
          "speaker_id": "1",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005385.1.15",
          "form": "영상으로 봐봐 더 재미있어",
          "original_form": "영상으로 봐봐 더 재미있어",
          "speaker_id": "2",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005385.1.16",
          "form": "나는 서리사막의 도둑들 추천해",
          "original_form": "나는 서리사막의 도둑들 추천해",
          "speaker_id": "2",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005385.1.17",
          "form": "서리사막에 도둑이 있나",
          "original_form": "서리사막에 도둑이 있나",
          "speaker_id": "1",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005385.1.18",
          "form": "ㅋㅋㅋ 감옥 이야기인데 진짜 웃겨",
          "original_form": "ㅋㅋㅋ 감옥 이야기인데 진짜 웃겨",
