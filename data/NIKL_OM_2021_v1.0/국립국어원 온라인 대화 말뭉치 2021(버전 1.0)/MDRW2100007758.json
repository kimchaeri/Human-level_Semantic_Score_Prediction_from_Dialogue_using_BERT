      "utterance": [
        {
          "id": "MDRW2100007758.1.1",
          "form": "사실 저는 반려동물이 없지만ㅎㅎ",
          "original_form": "사실 저는 반려동물이 없지만ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 10:15"
        },
        {
          "id": "MDRW2100007758.1.2",
          "form": "저도 키우고는 싶은데",
          "original_form": "저도 키우고는 싶은데",
          "speaker_id": "2",
          "time": "20210822 10:15"
        },
        {
          "id": "MDRW2100007758.1.3",
          "form": "털때문에",
          "original_form": "털때문에",
          "speaker_id": "2",
          "time": "20210822 10:15"
        },
        {
          "id": "MDRW2100007758.1.4",
          "form": "전 근데 또 어른되니까 그  마음이 좀 줄어들기도했어요",
          "original_form": "전 근데 또 어른되니까 그  마음이 좀 줄어들기도했어요",
          "speaker_id": "1",
          "time": "20210822 10:17"
        },
        {
          "id": "MDRW2100007758.1.5",
          "form": "요새 랜선집사? 도 가능하니까요 ㅋㅋㅋ",
          "original_form": "요새 랜선집사? 도 가능하니까요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:18"
        },
        {
          "id": "MDRW2100007758.1.6",
          "form": "맞아요 저는 매탈남님 채널 봐요",
          "original_form": "맞아요 저는 매탈남님 채널 봐요",
          "speaker_id": "2",
          "time": "20210822 10:20"
        },
        {
          "id": "MDRW2100007758.1.7",
          "form": "저는 웹툰이요 네이버 웹툰보세요?",
          "original_form": "저는 웹툰이요 네이버 웹툰보세요?",
          "speaker_id": "1",
          "time": "20210822 10:21"
        },
        {
          "id": "MDRW2100007758.1.8",
          "form": "반려동물 웹툰도 있나요?",
          "original_form": "반려동물 웹툰도 있나요?",
          "speaker_id": "2",
          "time": "20210822 10:22"
        },
        {
          "id": "MDRW2100007758.1.9",
          "form": "노곤하개 랑",
          "original_form": "노곤하개 랑",
          "speaker_id": "1",
          "time": "20210822 10:23"
        },
        {
          "id": "MDRW2100007758.1.10",
          "form": "개를 낳았다",
          "original_form": "개를 낳았다",
          "speaker_id": "1",
          "time": "20210822 10:23"
        },
        {
          "id": "MDRW2100007758.1.11",
          "form": "추천이요",
          "original_form": "추천이요",
          "speaker_id": "1",
          "time": "20210822 10:23"
        },
        {
          "id": "MDRW2100007758.1.12",
          "form": "제목부터 재밌을거같네요ㅎㅎ",
          "original_form": "제목부터 재밌을거같네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 10:23"
        },
        {
          "id": "MDRW2100007758.1.13",
          "form": "하나는에피소드형이고 하나는스토리형이요",
          "original_form": "하나는에피소드형이고 하나는스토리형이요",
          "speaker_id": "1",
          "time": "20210822 10:24"
        },
        {
          "id": "MDRW2100007758.1.14",
          "form": "무료니까 심심할때보기좋아요",
          "original_form": "무료니까 심심할때보기좋아요",
          "speaker_id": "1",
          "time": "20210822 10:24"
        },
        {
          "id": "MDRW2100007758.1.15",
          "form": "연재종료되면유료니까 그전에 ㅋㅋ",
          "original_form": "연재종료되면유료니까 그전에 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:24"
        },
        {
          "id": "MDRW2100007758.1.16",
          "form": "네ㅋㅋ",
          "original_form": "네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:24"
        },
        {
          "id": "MDRW2100007758.1.17",
          "form": "반려동물 키워보신적은 없나요?",
          "original_form": "반려동물 키워보신적은 없나요?",
          "speaker_id": "2",
          "time": "20210822 10:25"
        },
        {
          "id": "MDRW2100007758.1.18",
          "form": "어릴때 거북이? 올챙이? 는키워봤어요",
          "original_form": "어릴때 거북이? 올챙이? 는키워봤어요",
          "speaker_id": "1",
          "time": "20210822 10:26"
        },
        {
          "id": "MDRW2100007758.1.19",
          "form": "오 저도 그런건 아주 어릴때 키워본거같아요",
          "original_form": "오 저도 그런건 아주 어릴때 키워본거같아요",
          "speaker_id": "2",
          "time": "20210822 10:28"
        },
        {
          "id": "MDRW2100007758.1.20",
          "form": "혹시 키워보고싶으신 동물이 있으세요?",
          "original_form": "혹시 키워보고싶으신 동물이 있으세요?",
          "speaker_id": "1",
          "time": "20210822 10:31"
        },
        {
          "id": "MDRW2100007758.1.21",
          "form": "전 고양이 키우고싶어요",
          "original_form": "전 고양이 키우고싶어요",
          "speaker_id": "2",
          "time": "20210822 10:32"
        }
      ]
