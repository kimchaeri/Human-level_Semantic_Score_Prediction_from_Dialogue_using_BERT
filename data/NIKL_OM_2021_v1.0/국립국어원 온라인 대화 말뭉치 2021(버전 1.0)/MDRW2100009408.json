        {
          "id": "MDRW2100009408.1.1",
          "form": "화장실 청소가 힘들어요",
          "original_form": "화장실 청소가 힘들어요",
          "speaker_id": "2",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009408.1.2",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009408.1.3",
          "form": "특히 곰팡이제거",
          "original_form": "특히 곰팡이제거",
          "speaker_id": "2",
          "time": "20210822 23:20"
        },
        {
          "id": "MDRW2100009408.1.4",
          "form": "네 문제죠",
          "original_form": "네 문제죠",
          "speaker_id": "1",
          "time": "20210822 23:20"
        },
        {
          "id": "MDRW2100009408.1.5",
          "form": "혼자 사세요?",
          "original_form": "혼자 사세요?",
          "speaker_id": "2",
          "time": "20210822 23:22"
        },
        {
          "id": "MDRW2100009408.1.6",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210822 23:23"
        },
        {
          "id": "MDRW2100009408.1.7",
          "form": "저도 그래서 집안일 부담은 덜해요",
          "original_form": "저도 그래서 집안일 부담은 덜해요",
          "speaker_id": "2",
          "time": "20210822 23:28"
        },
        {
          "id": "MDRW2100009408.1.8",
          "form": "분담해야죠",
          "original_form": "분담해야죠",
          "speaker_id": "1",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100009408.1.9",
          "form": "전 제가 해놓은건 다잔소리하셔서ㅜㅜㅜ",
          "original_form": "전 제가 해놓은건 다잔소리하셔서ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 23:31"
        },
        {
          "id": "MDRW2100009408.1.10",
          "form": "ㅋㅋ 그런가요?",
          "original_form": "ㅋㅋ 그런가요?",
          "speaker_id": "1",
          "time": "20210822 23:31"
        },
        {
          "id": "MDRW2100009408.1.11",
          "form": "성에 안차서 결국 본인이 다시하세요 ㅋㅋ",
          "original_form": "성에 안차서 결국 본인이 다시하세요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:36"
        },
        {
          "id": "MDRW2100009408.1.12",
          "form": "결국 자기 맘에 들어야 하죠",
          "original_form": "결국 자기 맘에 들어야 하죠",
          "speaker_id": "1",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009408.1.13",
          "form": "어휴 그래서 피곤한거같아요",
          "original_form": "어휴 그래서 피곤한거같아요",
          "speaker_id": "2",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009408.1.14",
          "form": "싸우기도해ㅗ",
          "original_form": "싸우기도해ㅗ",
          "speaker_id": "2",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009408.1.15",
          "form": "싸우기도 하고",
