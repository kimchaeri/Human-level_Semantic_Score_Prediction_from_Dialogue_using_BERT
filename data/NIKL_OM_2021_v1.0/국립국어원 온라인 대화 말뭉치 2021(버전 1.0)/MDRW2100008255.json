      "utterance": [
        {
          "id": "MDRW2100008255.1.1",
          "form": "덕분에 배고파졌어요...",
          "original_form": "덕분에 배고파졌어요...",
          "speaker_id": "1",
          "time": "20210822 14:34"
        },
        {
          "id": "MDRW2100008255.1.2",
          "form": "점심 뭐드셨었죠",
          "original_form": "점심 뭐드셨었죠",
          "speaker_id": "1",
          "time": "20210822 14:34"
        },
        {
          "id": "MDRW2100008255.1.3",
          "form": "전... 오징어 젓갈욬ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전... 오징어 젓갈욬ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:34"
        },
        {
          "id": "MDRW2100008255.1.4",
          "form": "맞다...",
          "original_form": "맞다...",
          "speaker_id": "1",
          "time": "20210822 14:34"
        },
        {
          "id": "MDRW2100008255.1.5",
          "form": "아직 배부르신가요",
          "original_form": "아직 배부르신가요",
          "speaker_id": "1",
          "time": "20210822 14:35"
        },
        {
          "id": "MDRW2100008255.1.6",
          "form": "넼ㅋㅋㅋ 먹은지 얼마 안됐잖아요 ㅋㅋㅋㅋ",
          "original_form": "넼ㅋㅋㅋ 먹은지 얼마 안됐잖아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:35"
        },
        {
          "id": "MDRW2100008255.1.7",
          "form": "제 소화기관 텐션이 요즘 미쳐서",
          "original_form": "제 소화기관 텐션이 요즘 미쳐서",
          "speaker_id": "1",
          "time": "20210822 14:35"
        },
        {
          "id": "MDRW2100008255.1.8",
          "form": "금방 꺼지더라고요",
          "original_form": "금방 꺼지더라고요",
          "speaker_id": "1",
          "time": "20210822 14:36"
        },
        {
          "id": "MDRW2100008255.1.9",
          "form": "ㅇㅎㅇㅎ 그럴땐 또 먹어줘야죠ㅠㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅎㅇㅎ 그럴땐 또 먹어줘야죠ㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:40"
        },
        {
          "id": "MDRW2100008255.1.10",
          "form": "저는 복숭아 깎아왔어요",
          "original_form": "저는 복숭아 깎아왔어요",
          "speaker_id": "2",
          "time": "20210822 14:40"
        },
        {
          "id": "MDRW2100008255.1.11",
          "form": "요즘에 복숭아 맛있더라구요",
          "original_form": "요즘에 복숭아 맛있더라구요",
          "speaker_id": "2",
          "time": "20210822 14:40"
        },
        {
          "id": "MDRW2100008255.1.12",
          "form": "오 무슨 복숭아인가요",
          "original_form": "오 무슨 복숭아인가요",
          "speaker_id": "1",
          "time": "20210822 14:42"
        },
        {
          "id": "MDRW2100008255.1.13",
          "form": "그릭요거트 채워먹음 맛있다던데",
          "original_form": "그릭요거트 채워먹음 맛있다던데",
          "speaker_id": "1",
          "time": "20210822 14:43"
        },
        {
          "id": "MDRW2100008255.1.14",
          "form": "백도예요 ㅋㅋㅋㅋㅋ",
          "original_form": "백도예요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:44"
        },
        {
          "id": "MDRW2100008255.1.15",
          "form": "복숭아를 요거트랑 같이 먹을 생각은 못해봤네요",
          "original_form": "복숭아를 요거트랑 같이 먹을 생각은 못해봤네요",
          "speaker_id": "2",
          "time": "20210822 14:44"
        },
        {
          "id": "MDRW2100008255.1.16",
          "form": "요거트도 다음에 사서 먹어봐야겠어요",
          "original_form": "요거트도 다음에 사서 먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210822 14:44"
        },
        {
          "id": "MDRW2100008255.1.17",
          "form": "그래놀라 깔고 복숭아 씨.빼고 요거트 채워서 먹는거 유행이더라고요",
          "original_form": "그래놀라 깔고 복숭아 씨.빼고 요거트 채워서 먹는거 유행이더라고요",
          "speaker_id": "1",
          "time": "20210822 14:46"
        },
        {
          "id": "MDRW2100008255.1.18",
          "form": "그릭요거트 맛있는데 비싸요..",
          "original_form": "그릭요거트 맛있는데 비싸요..",
          "speaker_id": "1",
          "time": "20210822 14:47"
        },
        {
          "id": "MDRW2100008255.1.19",
          "form": "오 그런게 또 유행이었군요 몰랐어요 ㅋㅋㅋㅋ",
          "original_form": "오 그런게 또 유행이었군요 몰랐어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:48"
        },
        {
          "id": "MDRW2100008255.1.20",
          "form": "요즘 유제품들 다 가격이 오르고 있어서 ㅠㅠㅠ",
          "original_form": "요즘 유제품들 다 가격이 오르고 있어서 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 14:48"
        },
        {
          "id": "MDRW2100008255.1.21",
          "form": "이제 우유값도 더 인상한다더라구요... 휴...",
          "original_form": "이제 우유값도 더 인상한다더라구요... 휴...",
          "speaker_id": "2",
          "time": "20210822 14:48"
        },
        {
          "id": "MDRW2100008255.1.22",
          "form": "왜 오르는 걸까요 어쩐지 달라진다 싶더니...",
          "original_form": "왜 오르는 걸까요 어쩐지 달라진다 싶더니...",
          "speaker_id": "1",
          "time": "20210822 14:49"
        },
        {
          "id": "MDRW2100008255.1.23",
          "form": "그릭요거트 백그램이 사천오천 하더라고요",
          "original_form": "그릭요거트 백그램이 사천오천 하더라고요",
          "speaker_id": "1",
          "time": "20210822 14:49"
        },
        {
          "id": "MDRW2100008255.1.24",
          "form": "아이고 ㅠㅠㅠ 비싸다...",
          "original_form": "아이고 ㅠㅠㅠ 비싸다...",
          "speaker_id": "2",
          "time": "20210822 14:49"
        },
        {
          "id": "MDRW2100008255.1.25",
          "form": "코로나19 특수때문에 생산이며 수출입이 좀 제한이 있는지",
          "original_form": "코로나19 특수때문에 생산이며 수출입이 좀 제한이 있는지",
          "speaker_id": "2",
          "time": "20210822 14:50"
        },
        {
          "id": "MDRW2100008255.1.26",
          "form": "우유뿐만 아니고 오징어랑 뭐 이것저것 다 가격 인상을 한다고 하더라구요 ... ㅠㅠ",
          "original_form": "우유뿐만 아니고 오징어랑 뭐 이것저것 다 가격 인상을 한다고 하더라구요 ... ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 14:50"
        },
        {
          "id": "MDRW2100008255.1.27",
          "form": "아이고... 시국이 정말 장난 아니네요",
          "original_form": "아이고... 시국이 정말 장난 아니네요",
          "speaker_id": "1",
          "time": "20210822 14:51"
        },
        {
          "id": "MDRW2100008255.1.28",
          "form": "다들 힘든 시기라 안타까워요",
          "original_form": "다들 힘든 시기라 안타까워요",
          "speaker_id": "1",
          "time": "20210822 14:51"
        }
      ]
    }
