        },
        {
          "id": "MDRW2100029354.1.2",
          "form": "다채롭게 넘어가네요",
          "original_form": "다채롭게 넘어가네요",
          "speaker_id": "2",
          "time": "20210903 02:37"
        },
        {
          "id": "MDRW2100029354.1.3",
          "form": "쌀쌀해지기 시작했어요",
          "original_form": "쌀쌀해지기 시작했어요",
          "speaker_id": "2",
          "time": "20210903 02:37"
        },
        {
          "id": "MDRW2100029354.1.4",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210903 02:37"
        },
        {
          "id": "MDRW2100029354.1.5",
          "form": "옷뭐입으세요",
          "original_form": "옷뭐입으세요",
          "speaker_id": "1",
          "time": "20210903 02:37"
        },
        {
          "id": "MDRW2100029354.1.6",
          "form": "외투를 꺼내입기 시작합니다ㅎㅎ",
          "original_form": "외투를 꺼내입기 시작합니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 02:37"
        },
        {
          "id": "MDRW2100029354.1.7",
          "form": "벌써요? 먼외투?",
          "original_form": "벌써요? 먼외투?",
          "speaker_id": "1",
          "time": "20210903 02:37"
        },
        {
          "id": "MDRW2100029354.1.8",
          "form": "패션에는 모지리라..ㅎ",
          "original_form": "패션에는 모지리라..ㅎ",
          "speaker_id": "2",
          "time": "20210903 02:37"
        },
        {
          "id": "MDRW2100029354.1.9",
          "form": "안덥나요",
          "original_form": "안덥나요",
          "speaker_id": "1",
          "time": "20210903 02:38"
        },
        {
          "id": "MDRW2100029354.1.10",
          "form": "그래봐야 야상이나 야구점퍼정도죠",
          "original_form": "그래봐야 야상이나 야구점퍼정도죠",
          "speaker_id": "2",
          "time": "20210903 02:38"
        },
        {
          "id": "MDRW2100029354.1.11",
          "form": "비가오니 쌀쌀해요..ㅠ",
          "original_form": "비가오니 쌀쌀해요..ㅠ",
          "speaker_id": "2",
          "time": "20210903 02:38"
        },
        {
          "id": "MDRW2100029354.1.12",
          "form": "와 아직 난 반팔인데",
          "original_form": "와 아직 난 반팔인데",
          "speaker_id": "1",
          "time": "20210903 02:39"
        },
        {
          "id": "MDRW2100029354.1.13",
          "form": "저도 지금은 반팔입니당^^",
          "original_form": "저도 지금은 반팔입니당^^",
          "speaker_id": "2",
          "time": "20210903 02:40"
        },
        {
          "id": "MDRW2100029354.1.14",
          "form": "계절 머조아해요",
          "original_form": "계절 머조아해요",
          "speaker_id": "1",
          "time": "20210903 02:44"
        },
        {
          "id": "MDRW2100029354.1.15",
          "form": "가을 좋아합니다",
          "original_form": "가을 좋아합니다",
          "speaker_id": "2",
          "time": "20210903 02:44"
        },
        {
          "id": "MDRW2100029354.1.16",
          "form": "가을이네영 좋겟네여",
          "original_form": "가을이네영 좋겟네여",
          "speaker_id": "1",
          "time": "20210903 02:45"
        },
        {
          "id": "MDRW2100029354.1.17",
          "form": "쓸쓸한 고득의 계절이자 밖에서 책읽기딱좋은",
          "original_form": "쓸쓸한 고득의 계절이자 밖에서 책읽기딱좋은",
