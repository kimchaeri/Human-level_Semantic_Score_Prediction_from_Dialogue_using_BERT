      "utterance": [
        {
          "id": "MDRW2100013646.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 15:29"
        },
        {
          "id": "MDRW2100013646.1.2",
          "form": "안녕하세요 점심 뭐 드셨나요?",
          "original_form": "안녕하세요 점심 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013646.1.3",
          "form": "저 파스타 만들어 먹었어요ㅎㅎ",
          "original_form": "저 파스타 만들어 먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 15:30"
        },
        {
          "id": "MDRW2100013646.1.4",
          "form": "오 무슨 파스타 드셨나요!",
          "original_form": "오 무슨 파스타 드셨나요!",
          "speaker_id": "2",
          "time": "20210825 15:32"
        },
        {
          "id": "MDRW2100013646.1.5",
          "form": "크림 파스타 먹었어요ㅎㅎ",
          "original_form": "크림 파스타 먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 15:33"
        },
        {
          "id": "MDRW2100013646.1.6",
          "form": "크림 파스타 맛있죠! 소스도 만들어드셨나요?",
          "original_form": "크림 파스타 맛있죠! 소스도 만들어드셨나요?",
          "speaker_id": "2",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013646.1.7",
          "form": "소스는 마트에서 샀어요ㅋㅋ면이랑ㅋㅋ",
          "original_form": "소스는 마트에서 샀어요ㅋㅋ면이랑ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013646.1.8",
          "form": "소스 만들 줄 아시나요?",
          "original_form": "소스 만들 줄 아시나요?",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013646.1.9",
          "form": "아뇨ㅋㅋㅋ 소스는 만들기 힘들죠ㅋㅋㅋ",
          "original_form": "아뇨ㅋㅋㅋ 소스는 만들기 힘들죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013646.1.10",
          "form": "크림파스타를 제일 좋아하시나요?",
          "original_form": "크림파스타를 제일 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013646.1.11",
          "form": "ㅋㅋ네네 그런데 로제파스타도 좋아해요ㅋㅋ",
          "original_form": "ㅋㅋ네네 그런데 로제파스타도 좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013646.1.12",
          "form": "오 그렇군요ㅋㅋㅋ 전 알리오올리오나 냉파스타를 좋아해요!",
          "original_form": "오 그렇군요ㅋㅋㅋ 전 알리오올리오나 냉파스타를 좋아해요!",
          "speaker_id": "2",
          "time": "20210825 15:45"
        },
        {
          "id": "MDRW2100013646.1.13",
          "form": "알리오올리오도 담백해서 좋아요ㅎㅎ",
          "original_form": "알리오올리오도 담백해서 좋아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013646.1.14",
          "form": "맞아요ㅎㅎ 알리오올리오에 새우랑 소세지 넣어서 먹으면 진짜 맛있더라구요",
          "original_form": "맞아요ㅎㅎ 알리오올리오에 새우랑 소세지 넣어서 먹으면 진짜 맛있더라구요",
          "speaker_id": "2",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013646.1.15",
          "form": "오 소세지도 넣는군요!ㅎㅎ",
          "original_form": "오 소세지도 넣는군요!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013646.1.16",
          "form": "만들어 드셨어요?",
          "original_form": "만들어 드셨어요?",
          "speaker_id": "1",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013646.1.17",
          "form": "네! 당연하죠ㅋㅋ",
          "original_form": "네! 당연하죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:50"
        }
      ]
