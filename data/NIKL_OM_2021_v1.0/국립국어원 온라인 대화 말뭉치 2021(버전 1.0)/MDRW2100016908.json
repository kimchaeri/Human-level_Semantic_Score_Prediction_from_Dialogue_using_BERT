      "utterance": [
        {
          "id": "MDRW2100016908.1.1",
          "form": "뭐 사고 싶은거 잇어요?",
          "original_form": "뭐 사고 싶은거 잇어요?",
          "speaker_id": "1",
          "time": "20210826 16:09"
        },
        {
          "id": "MDRW2100016908.1.2",
          "form": "딱히 요샌 없어요",
          "original_form": "딱히 요샌 없어요",
          "speaker_id": "2",
          "time": "20210826 16:10"
        },
        {
          "id": "MDRW2100016908.1.3",
          "form": "위시템 잇나요?",
          "original_form": "위시템 잇나요?",
          "speaker_id": "2",
          "time": "20210826 16:10"
        },
        {
          "id": "MDRW2100016908.1.4",
          "form": "저도 물욕이 없어서...",
          "original_form": "저도 물욕이 없어서...",
          "speaker_id": "1",
          "time": "20210826 16:10"
        },
        {
          "id": "MDRW2100016908.1.5",
          "form": "아.. 다이슨 에어랩.. ㅋㅋㅋㅋ",
          "original_form": "아.. 다이슨 에어랩.. ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:10"
        },
        {
          "id": "MDRW2100016908.1.6",
          "form": "아ㅜㅜ 진짜 비싸더라구요",
          "original_form": "아ㅜㅜ 진짜 비싸더라구요",
          "speaker_id": "2",
          "time": "20210826 16:11"
        },
        {
          "id": "MDRW2100016908.1.7",
          "form": "저건 엄청 이쁘더라구요..",
          "original_form": "저건 엄청 이쁘더라구요..",
          "speaker_id": "1",
          "time": "20210826 16:11"
        },
        {
          "id": "MDRW2100016908.1.8",
          "form": "오.. 아시눈구나..",
          "original_form": "오.. 아시눈구나..",
          "speaker_id": "1",
          "time": "20210826 16:11"
        },
        {
          "id": "MDRW2100016908.1.9",
          "form": "ㅋㅋ근데 넘비싸서 위시템 못넣어요",
          "original_form": "ㅋㅋ근데 넘비싸서 위시템 못넣어요",
          "speaker_id": "2",
          "time": "20210826 16:14"
        },
        {
          "id": "MDRW2100016908.1.10",
          "form": "귀차나서 활용도 못할거같아요",
          "original_form": "귀차나서 활용도 못할거같아요",
          "speaker_id": "2",
          "time": "20210826 16:14"
        },
        {
          "id": "MDRW2100016908.1.11",
          "form": "그쵸... 드라이기 주제에 50만원이라니..",
          "original_form": "그쵸... 드라이기 주제에 50만원이라니..",
          "speaker_id": "1",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016908.1.12",
          "form": "차라리 삭발을 하고 말지.. ㅋㅋㅋ",
          "original_form": "차라리 삭발을 하고 말지.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016908.1.13",
          "form": "저는 샤오미 드라이기쓰는데",
          "original_form": "저는 샤오미 드라이기쓰는데",
          "speaker_id": "2",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016908.1.14",
          "form": "가성비 최고임다",
          "original_form": "가성비 최고임다",
          "speaker_id": "2",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016908.1.15",
          "form": "다이슨 저리가라 자기위안하면서 잘써요",
          "original_form": "다이슨 저리가라 자기위안하면서 잘써요",
          "speaker_id": "2",
          "time": "20210826 16:17"
        },
        {
          "id": "MDRW2100016908.1.16",
          "form": "오? 샤오미에서 드라이기도 나와요?",
          "original_form": "오? 샤오미에서 드라이기도 나와요?",
          "speaker_id": "1",
          "time": "20210826 16:17"
        },
        {
          "id": "MDRW2100016908.1.17",
          "form": "쿠팡에서 삿는데",
          "original_form": "쿠팡에서 삿는데",
          "speaker_id": "2",
          "time": "20210826 16:18"
        },
        {
          "id": "MDRW2100016908.1.18",
          "form": "싸고 좋아요",
          "original_form": "싸고 좋아요",
          "speaker_id": "2",
          "time": "20210826 16:18"
        },
        {
          "id": "MDRW2100016908.1.19",
          "form": "오.. 모양 지슷하네여...",
          "original_form": "오.. 모양 지슷하네여...",
          "speaker_id": "1",
          "time": "20210826 16:18"
        },
        {
          "id": "MDRW2100016908.1.20",
          "form": "이거도 엄청 이쁘당..",
          "original_form": "이거도 엄청 이쁘당..",
          "speaker_id": "1",
          "time": "20210826 16:18"
        },
        {
          "id": "MDRW2100016908.1.21",
          "form": "그쳐 강츄",
          "original_form": "그쳐 강츄",
          "speaker_id": "2",
          "time": "20210826 16:18"
        },
        {
          "id": "MDRW2100016908.1.22",
          "form": "바람 어때유? 잘 말라유?",
          "original_form": "바람 어때유? 잘 말라유?",
          "speaker_id": "1",
          "time": "20210826 16:19"
        },
        {
          "id": "MDRW2100016908.1.23",
          "form": "눈감으면 다이슨임돠",
          "original_form": "눈감으면 다이슨임돠",
          "speaker_id": "2",
          "time": "20210826 16:20"
        },
        {
          "id": "MDRW2100016908.1.24",
          "form": "저 미디엄 길이라 드라이기 민감해요.. ㅋㅋ",
          "original_form": "저 미디엄 길이라 드라이기 민감해요.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:20"
        },
        {
          "id": "MDRW2100016908.1.25",
          "form": "저는 곱슬 머리숱부자라서",
          "original_form": "저는 곱슬 머리숱부자라서",
          "speaker_id": "2",
          "time": "20210826 16:20"
        },
        {
          "id": "MDRW2100016908.1.26",
          "form": "바람세기 중요함",
          "original_form": "바람세기 중요함",
          "speaker_id": "2",
          "time": "20210826 16:20"
        },
        {
          "id": "MDRW2100016908.1.27",
          "form": "갑자기 이거 땡기는데...",
          "original_form": "갑자기 이거 땡기는데...",
          "speaker_id": "1",
          "time": "20210826 16:20"
        },
        {
          "id": "MDRW2100016908.1.28",
          "form": "대출받아서 지를까여? ㅋㅋㅋㅋ",
          "original_form": "대출받아서 지를까여? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100016908.1.29",
          "form": "고고고",
          "original_form": "고고고",
          "speaker_id": "2",
          "time": "20210826 16:21"
        },
        {
          "id": "MDRW2100016908.1.30",
