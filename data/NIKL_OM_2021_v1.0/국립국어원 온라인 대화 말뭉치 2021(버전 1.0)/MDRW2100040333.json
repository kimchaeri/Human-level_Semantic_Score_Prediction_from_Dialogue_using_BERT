      "utterance": [
        {
          "id": "MDRW2100040333.1.1",
          "form": "베라 민초안 먹어 봤어요ㅜㅜ",
          "original_form": "베라 민초안 먹어 봤어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 16:25"
        },
        {
          "id": "MDRW2100040333.1.2",
          "form": "민초단으로서 아쉽",
          "original_form": "민초단으로서 아쉽",
          "speaker_id": "1",
          "time": "20210904 16:26"
        },
        {
          "id": "MDRW2100040333.1.3",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210904 16:26"
        },
        {
          "id": "MDRW2100040333.1.4",
          "form": "베라는 역시 피스타치오라ㅜㅜ",
          "original_form": "베라는 역시 피스타치오라ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 16:26"
        },
        {
          "id": "MDRW2100040333.1.5",
          "form": "도전하기 겁나더라구요",
          "original_form": "도전하기 겁나더라구요",
          "speaker_id": "2",
          "time": "20210904 16:26"
        },
        {
          "id": "MDRW2100040333.1.6",
          "form": "그럼 뭘로 접하셨나요 민초를",
          "original_form": "그럼 뭘로 접하셨나요 민초를",
          "speaker_id": "1",
          "time": "20210904 16:26"
        },
        {
          "id": "MDRW2100040333.1.7",
          "form": "아..피스타치오아몬드는 스테디죠ㅎㅎ",
          "original_form": "아..피스타치오아몬드는 스테디죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 16:26"
        },
        {
          "id": "MDRW2100040333.1.8",
          "form": "뜬금없이 우유요ㅋㅋㅋ",
          "original_form": "뜬금없이 우유요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:26"
        },
        {
          "id": "MDRW2100040333.1.9",
          "form": "편의점 우유였는데 나쁘지않더라구요ㅎㅎㅎ",
          "original_form": "편의점 우유였는데 나쁘지않더라구요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 16:26"
        },
        {
          "id": "MDRW2100040333.1.10",
          "form": "아 저도 편의점 민초우유는 또 괜찮아요",
          "original_form": "아 저도 편의점 민초우유는 또 괜찮아요",
          "speaker_id": "1",
          "time": "20210904 16:27"
        },
        {
          "id": "MDRW2100040333.1.11",
          "form": "글\r\n\r\n\r\n\r\n\r\n아이스크림이 훨 맛있지만",
          "original_form": "글\r\n\r\n\r\n\r\n\r\n아이스크림이 훨 맛있지만",
          "speaker_id": "1",
          "time": "20210904 16:27"
        },
        {
          "id": "MDRW2100040333.1.12",
          "form": "배라 민초 매년매월 상위권 픽입니다",
          "original_form": "배라 민초 매년매월 상위권 픽입니다",
          "speaker_id": "1",
          "time": "20210904 16:28"
        },
        {
          "id": "MDRW2100040333.1.13",
          "form": "그게 파인애플피자보다 더 안전한 도전일수도ㅎ",
          "original_form": "그게 파인애플피자보다 더 안전한 도전일수도ㅎ",
          "speaker_id": "1",
          "time": "20210904 16:28"
        },
        {
          "id": "MDRW2100040333.1.14",
          "form": "아! 그렇다면 꼭 먹어봐야겠네요",
          "original_form": "아! 그렇다면 꼭 먹어봐야겠네요",
          "speaker_id": "2",
          "time": "20210904 16:28"
        },
        {
          "id": "MDRW2100040333.1.15",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210904 16:28"
        },
        {
          "id": "MDRW2100040333.1.16",
          "form": "민초단 박해받아도",
          "original_form": "민초단 박해받아도",
          "speaker_id": "1",
          "time": "20210904 16:28"
        },
        {
          "id": "MDRW2100040333.1.17",
          "form": "피스타치오랑 민초 두 개 선택할려니까 뭔가 이상해서ㅋㅋ",
          "original_form": "피스타치오랑 민초 두 개 선택할려니까 뭔가 이상해서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:29"
        },
        {
          "id": "MDRW2100040333.1.18",
          "form": "세손가락에 드는 인기멤인 걸요11",
          "original_form": "세손가락에 드는 인기멤인 걸요11",
          "speaker_id": "1",
          "time": "20210904 16:29"
        },
        {
          "id": "MDRW2100040333.1.19",
          "form": "안시켰었는데ㅋㅋㅋ",
          "original_form": "안시켰었는데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:29"
        },
        {
          "id": "MDRW2100040333.1.20",
