      "utterance": [
        {
          "id": "MDRW2100024120.1.1",
          "form": "지금 학교에 역사쌤도",
          "original_form": "지금 학교에 역사쌤도",
          "speaker_id": "2",
          "time": "20210830 22:19"
        },
        {
          "id": "MDRW2100024120.1.2",
          "form": "곤룡포입고 도포입고 그래요",
          "original_form": "곤룡포입고 도포입고 그래요",
          "speaker_id": "2",
          "time": "20210830 22:19"
        },
        {
          "id": "MDRW2100024120.1.3",
          "form": "ㅋㅋㅋ그런 옷 도대체 어디서 사시는거죠",
          "original_form": "ㅋㅋㅋ그런 옷 도대체 어디서 사시는거죠",
          "speaker_id": "1",
          "time": "20210830 22:19"
        },
        {
          "id": "MDRW2100024120.1.4",
          "form": "오 자사중",
          "original_form": "오 자사중",
          "speaker_id": "2",
          "time": "20210830 22:19"
        },
        {
          "id": "MDRW2100024120.1.5",
          "form": "어느 학교는 훈장님 옷차림이라던데 너무 웃겨서 ㅋㅋ",
          "original_form": "어느 학교는 훈장님 옷차림이라던데 너무 웃겨서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:19"
        },
        {
          "id": "MDRW2100024120.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:19"
        },
        {
          "id": "MDRW2100024120.1.7",
          "form": "네 아 특목고도 자사고였어요",
          "original_form": "네 아 특목고도 자사고였어요",
          "speaker_id": "1",
          "time": "20210830 22:19"
        },
        {
          "id": "MDRW2100024120.1.8",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210830 22:19"
        },
        {
          "id": "MDRW2100024120.1.9",
          "form": "그래서 영어시간에 원어민 선생님이랑 수업했거든요",
          "original_form": "그래서 영어시간에 원어민 선생님이랑 수업했거든요",
          "speaker_id": "1",
          "time": "20210830 22:19"
        },
        {
          "id": "MDRW2100024120.1.10",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "2",
          "time": "20210830 22:19"
        },
        {
          "id": "MDRW2100024120.1.11",
          "form": "그 때 뭔가 재밌는데 괜히 긴장하고 막 그랬어옄ㅋㅋ",
          "original_form": "그 때 뭔가 재밌는데 괜히 긴장하고 막 그랬어옄ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:20"
        },
        {
          "id": "MDRW2100024120.1.12",
          "form": "원어민 수업 좋지않아요?",
          "original_form": "원어민 수업 좋지않아요?",
          "speaker_id": "2",
          "time": "20210830 22:20"
        },
        {
          "id": "MDRW2100024120.1.13",
          "form": "저는 좋았는데 교과서식으로 공부하다가",
          "original_form": "저는 좋았는데 교과서식으로 공부하다가",
          "speaker_id": "1",
          "time": "20210830 22:20"
        },
        {
          "id": "MDRW2100024120.1.14",
          "form": "회화를 만나니까 좀 힘들었어요 ㅋㅋㅋㅋ",
          "original_form": "회화를 만나니까 좀 힘들었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:20"
        },
        {
          "id": "MDRW2100024120.1.15",
          "form": "아ㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:20"
        },
        {
          "id": "MDRW2100024120.1.16",
          "form": "지금 학교에 원어민 잇어요?",
          "original_form": "지금 학교에 원어민 잇어요?",
          "speaker_id": "2",
          "time": "20210830 22:20"
        },
        {
          "id": "MDRW2100024120.1.17",
          "form": "막 하이 하와유 ?하면 아임파인땡큐 이런거 자동반사 나오는 것처럼",
          "original_form": "막 하이 하와유 ?하면 아임파인땡큐 이런거 자동반사 나오는 것처럼",
          "speaker_id": "1",
          "time": "20210830 22:21"
        },
        {
          "id": "MDRW2100024120.1.18",
          "form": "원어민 선생님 만나고 고쳐졌네여..",
          "original_form": "원어민 선생님 만나고 고쳐졌네여..",
          "speaker_id": "1",
          "time": "20210830 22:21"
        },
        {
          "id": "MDRW2100024120.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:21"
        },
        {
          "id": "MDRW2100024120.1.20",
          "form": "저희 학교는 없어용",
          "original_form": "저희 학교는 없어용",
          "speaker_id": "1",
          "time": "20210830 22:21"
        },
        {
          "id": "MDRW2100024120.1.21",
          "form": "원어민 강사 있으면 업무 가중된다구 ^.^",
          "original_form": "원어민 강사 있으면 업무 가중된다구 ^.^",
          "speaker_id": "1",
          "time": "20210830 22:21"
        },
        {
          "id": "MDRW2100024120.1.22",
          "form": "아하 마자요 뭐가 잇더라구요",
          "original_form": "아하 마자요 뭐가 잇더라구요",
          "speaker_id": "2",
          "time": "20210830 22:21"
        },
        {
          "id": "MDRW2100024120.1.23",
          "form": "원어민 주거/생활/복지 이런거 다 책임져야된대서 기피 하시네요?",
          "original_form": "원어민 주거/생활/복지 이런거 다 책임져야된대서 기피 하시네요?",
          "speaker_id": "1",
          "time": "20210830 22:21"
        },
        {
          "id": "MDRW2100024120.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:21"
        },
        {
          "id": "MDRW2100024120.1.25",
          "form": "원어민이랑 친해지고 싶은데",
          "original_form": "원어민이랑 친해지고 싶은데",
          "speaker_id": "2",
          "time": "20210830 22:21"
        },
        {
          "id": "MDRW2100024120.1.26",
          "form": "되게 사생활 침해도 많이 당하신대요 밤이고 주말이고 문제 생기면 막 전화하고 그래서 ㅜㅜ",
          "original_form": "되게 사생활 침해도 많이 당하신대요 밤이고 주말이고 문제 생기면 막 전화하고 그래서 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 22:22"
        },
        {
          "id": "MDRW2100024120.1.27",
          "form": "진짜 너무 조용하셔서 ㅋㅋㅋ",
          "original_form": "진짜 너무 조용하셔서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:22"
        },
        {
          "id": "MDRW2100024120.1.28",
          "form": "아 학교에 있어요 원어민쌤?",
          "original_form": "아 학교에 있어요 원어민쌤?",
          "speaker_id": "1",
