      "utterance": [
        {
          "id": "MDRW2100041230.1.1",
          "form": "다이어트나 운동하시나요?",
          "original_form": "다이어트나 운동하시나요?",
          "speaker_id": "1",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100041230.1.2",
          "form": "아뇨 안하는데 살이쪄서",
          "original_form": "아뇨 안하는데 살이쪄서",
          "speaker_id": "2",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100041230.1.3",
          "form": "운동해야할거같아요 금이님은요?",
          "original_form": "운동해야할거같아요 금이님은요?",
          "speaker_id": "2",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100041230.1.4",
          "form": "저 엊그제 홈트 한판하고 어젠 산책했는데 낼 생리할거같아요ㅜ",
          "original_form": "저 엊그제 홈트 한판하고 어젠 산책했는데 낼 생리할거같아요ㅜ",
          "speaker_id": "1",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100041230.1.5",
          "form": "아이고 그러시군요",
          "original_form": "아이고 그러시군요",
          "speaker_id": "2",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100041230.1.6",
          "form": "저는 이틀전에 생리 시작했어요",
          "original_form": "저는 이틀전에 생리 시작했어요",
          "speaker_id": "2",
          "time": "20210905 20:42"
        },
        {
          "id": "MDRW2100041230.1.7",
          "form": "아...ㅜ 부럽습니다 곧 끝나시겠어요!",
          "original_form": "아...ㅜ 부럽습니다 곧 끝나시겠어요!",
          "speaker_id": "1",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100041230.1.8",
          "form": "네 ㅎㅎ 생리 안아프시고 무사히 지나가시길 빌게요",
          "original_form": "네 ㅎㅎ 생리 안아프시고 무사히 지나가시길 빌게요",
          "speaker_id": "2",
          "time": "20210905 20:43"
        },
        {
          "id": "MDRW2100041230.1.9",
          "form": "생리통약 필수예요",
          "original_form": "생리통약 필수예요",
          "speaker_id": "1",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100041230.1.10",
          "form": "생리통 있으세요?",
          "original_form": "생리통 있으세요?",
          "speaker_id": "1",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100041230.1.11",
          "form": "많이는 없어요",
          "original_form": "많이는 없어요",
          "speaker_id": "2",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100041230.1.12",
          "form": "아 건강하시네요!",
          "original_form": "아 건강하시네요!",
          "speaker_id": "1",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100041230.1.13",
          "form": "안아플때도 있고 아플때도 있는데 참을만한 정도로 아파서요..",
          "original_form": "안아플때도 있고 아플때도 있는데 참을만한 정도로 아파서요..",
          "speaker_id": "2",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100041230.1.14",
          "form": "생리통 한약 한번 드셔보세요",
          "original_form": "생리통 한약 한번 드셔보세요",
          "speaker_id": "2",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100041230.1.15",
          "form": "의료보험 되어서 싸대요",
          "original_form": "의료보험 되어서 싸대요",
          "speaker_id": "2",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100041230.1.16",
          "form": "전 그냥 아플거같다싶으면 바로 먹어요",
          "original_form": "전 그냥 아플거같다싶으면 바로 먹어요",
          "speaker_id": "1",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100041230.1.17",
          "form": "한약이요? 예전에 먹어봤는데 소용없더라고요ㅜ",
          "original_form": "한약이요? 예전에 먹어봤는데 소용없더라고요ㅜ",
          "speaker_id": "1",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100041230.1.18",
          "form": "아 그런가요 ㅜㅠ 저 어릴때 한약 환으로 된거먹고",
          "original_form": "아 그런가요 ㅜㅠ 저 어릴때 한약 환으로 된거먹고",
          "speaker_id": "2",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100041230.1.19",
          "form": "좋아져서요",
          "original_form": "좋아져서요",
          "speaker_id": "2",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100041230.1.20",
          "form": "효과 있으셔서 다행이에요!",
          "original_form": "효과 있으셔서 다행이에요!",
          "speaker_id": "1",
          "time": "20210905 20:50"
        },
        {
          "id": "MDRW2100041230.1.21",
          "form": "지금 따로 챙기시는거 있으세요?",
          "original_form": "지금 따로 챙기시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210905 20:50"
        }
      ]
