      "utterance": [
        {
          "id": "MDRW2100020158.1.1",
          "form": "로제떡볶이 좋아하시나요",
          "original_form": "로제떡볶이 좋아하시나요",
          "speaker_id": "1",
          "time": "20210828 00:00"
        },
        {
          "id": "MDRW2100020158.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020158.1.3",
          "form": "드셔보셨어요?",
          "original_form": "드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020158.1.4",
          "form": "그럼요 전 엽떡 로제먹어봤어요",
          "original_form": "그럼요 전 엽떡 로제먹어봤어요",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020158.1.5",
          "form": "엽떡은 전 아직인데 맛나나요?",
          "original_form": "엽떡은 전 아직인데 맛나나요?",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020158.1.6",
          "form": "생각보다 괜찮앗어요",
          "original_form": "생각보다 괜찮앗어요",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020158.1.7",
          "form": "근데 칼로리 폭탄이란 소리듣고 먹을때마다 망설여졍ㅎ",
          "original_form": "근데 칼로리 폭탄이란 소리듣고 먹을때마다 망설여졍ㅎ",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020158.1.8",
          "form": "져요 ㅎㅎ",
          "original_form": "져요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020158.1.9",
          "form": "맞아요 ㅜ",
          "original_form": "맞아요 ㅜ",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020158.1.10",
          "form": "맛잇는거는 그치만 0칼로리에요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맛잇는거는 그치만 0칼로리에요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020158.1.11",
          "form": "맞는말 ㅠㅠ",
          "original_form": "맞는말 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020158.1.12",
          "form": "name2은 로제떡볶이 어디거 드셔보셧어요??ㅋㅋㅋ",
          "original_form": "&name2&은 로제떡볶이 어디거 드셔보셧어요??ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020158.1.13",
          "form": "전 삼첩 신전 배떡요!",
          "original_form": "전 삼첩 신전 배떡요!",
          "speaker_id": "2",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020158.1.14",
          "form": "3곳중에 어디가 제일 마싯었나요??",
          "original_form": "3곳중에 어디가 제일 마싯었나요??",
          "speaker_id": "1",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020158.1.15",
          "form": "저는 신전이요~",
          "original_form": "저는 신전이요~",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020158.1.16",
          "form": "양도 적당하고 꾸덕해요",
          "original_form": "양도 적당하고 꾸덕해요",
          "speaker_id": "2",
          "time": "20210828 00:06"
        },
        {
          "id": "MDRW2100020158.1.17",
          "form": "오 담에 한번 먹어봐야겠어요",
          "original_form": "오 담에 한번 먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020158.1.18",
          "form": "전 청년다방 로제도 먹어봤는데",
          "original_form": "전 청년다방 로제도 먹어봤는데",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020158.1.19",
          "form": "아거기 궁금하더라구요",
          "original_form": "아거기 궁금하더라구요",
          "speaker_id": "2",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020158.1.20",
          "form": "청년다방 로제는 꾸덕하지가 않았어요...ㅠ",
          "original_form": "청년다방 로제는 꾸덕하지가 않았어요...ㅠ",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020158.1.21",
          "form": "아 ㅜㅜ",
          "original_form": "아 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020158.1.22",
          "form": "로제는 꾸덕함이 생명이죠",
          "original_form": "로제는 꾸덕함이 생명이죠",
          "speaker_id": "2",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020158.1.23",
          "form": "맞아여ㅠㅠ 뭔가 꾸덕한거 좋아하시면 청년다방은 안먹는걸 추천...",
