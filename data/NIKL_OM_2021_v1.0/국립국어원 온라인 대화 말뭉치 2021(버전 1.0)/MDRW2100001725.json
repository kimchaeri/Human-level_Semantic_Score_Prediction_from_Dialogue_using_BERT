      "utterance": [
        {
          "id": "MDRW2100001725.1.1",
          "form": "야식 좋아하시나요?",
          "original_form": "야식 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100001725.1.2",
          "form": "야식 거의 먹어 본적 없는 것 같아요",
          "original_form": "야식 거의 먹어 본적 없는 것 같아요",
          "speaker_id": "2",
          "time": "20210822 18:56"
        },
        {
          "id": "MDRW2100001725.1.3",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "2",
          "time": "20210822 18:56"
        },
        {
          "id": "MDRW2100001725.1.4",
          "form": "가끔 라면정도 먹는거 같아요",
          "original_form": "가끔 라면정도 먹는거 같아요",
          "speaker_id": "1",
          "time": "20210822 18:56"
        },
        {
          "id": "MDRW2100001725.1.5",
          "form": "얍님은 즐겨드시는 음식있으신가요",
          "original_form": "얍님은 즐겨드시는 음식있으신가요",
          "speaker_id": "1",
          "time": "20210822 18:56"
        },
        {
          "id": "MDRW2100001725.1.6",
          "form": "음...그냥 그날따라 되는대로 먹는편이라",
          "original_form": "음...그냥 그날따라 되는대로 먹는편이라",
          "speaker_id": "2",
          "time": "20210822 18:59"
        },
        {
          "id": "MDRW2100001725.1.7",
          "form": "자주 먹는건 없는데",
          "original_form": "자주 먹는건 없는데",
          "speaker_id": "2",
          "time": "20210822 18:59"
        },
        {
          "id": "MDRW2100001725.1.8",
          "form": "아..네..",
          "original_form": "아..네..",
          "speaker_id": "1",
          "time": "20210822 18:59"
        },
        {
          "id": "MDRW2100001725.1.9",
          "form": "회덮밥은 뭔가 보이면 꼭 시키게 돼요",
          "original_form": "회덮밥은 뭔가 보이면 꼭 시키게 돼요",
          "speaker_id": "2",
          "time": "20210822 18:59"
        },
        {
          "id": "MDRW2100001725.1.10",
          "form": "회덮밥 맛있죠!",
          "original_form": "회덮밥 맛있죠!",
          "speaker_id": "1",
          "time": "20210822 19:00"
        },
        {
          "id": "MDRW2100001725.1.11",
          "form": "회덥밥에 큐브회 좋아하시나요",
          "original_form": "회덥밥에 큐브회 좋아하시나요",
          "speaker_id": "1",
          "time": "20210822 19:00"
        },
        {
          "id": "MDRW2100001725.1.12",
          "form": "아니면 활어 넣는거 좋아하시나요",
          "original_form": "아니면 활어 넣는거 좋아하시나요",
          "speaker_id": "1",
          "time": "20210822 19:00"
        },
        {
          "id": "MDRW2100001725.1.13",
          "form": "활어가 더 좋은 것 같아요",
          "original_form": "활어가 더 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210822 19:01"
        },
        {
          "id": "MDRW2100001725.1.14",
          "form": "아무래도 활어가 더 맛있는거 같아요",
          "original_form": "아무래도 활어가 더 맛있는거 같아요",
          "speaker_id": "1",
          "time": "20210822 19:02"
        },
        {
          "id": "MDRW2100001725.1.15",
          "form": "어렸을때는 큐브도 가성비가 좋아서 먹었는데 크니까 양은 적어지고 뭔가 한번에 더 큰 만족감을 원하게 되서...",
          "original_form": "어렸을때는 큐브도 가성비가 좋아서 먹었는데 크니까 양은 적어지고 뭔가 한번에 더 큰 만족감을 원하게 되서...",
          "speaker_id": "2",
          "time": "20210822 19:02"
        },
        {
          "id": "MDRW2100001725.1.16",
          "form": "조금 비싸더라도 활어가 좋은 것 같아요.",
          "original_form": "조금 비싸더라도 활어가 좋은 것 같아요.",
          "speaker_id": "2",
          "time": "20210822 19:02"
        },
        {
          "id": "MDRW2100001725.1.17",
          "form": "뭔지알거같아요  약간 양보다는 질을 우선시 하게되더라구요",
          "original_form": "뭔지알거같아요  약간 양보다는 질을 우선시 하게되더라구요",
          "speaker_id": "1",
          "time": "20210822 19:02"
        },
        {
          "id": "MDRW2100001725.1.18",
          "form": "네네 어렸을 때는 뷔페도 엄청 좋아했거든요.",
          "original_form": "네네 어렸을 때는 뷔페도 엄청 좋아했거든요.",
          "speaker_id": "2",
          "time": "20210822 19:03"
        },
        {
          "id": "MDRW2100001725.1.19",
          "form": "근데 커서는 정말 잘 안 가게 되는 것 같아요.",
          "original_form": "근데 커서는 정말 잘 안 가게 되는 것 같아요.",
          "speaker_id": "2",
          "time": "20210822 19:03"
        },
        {
          "id": "MDRW2100001725.1.20",
          "form": "맞아요 공감합니다",
          "original_form": "맞아요 공감합니다",
          "speaker_id": "1",
          "time": "20210822 19:03"
        }
