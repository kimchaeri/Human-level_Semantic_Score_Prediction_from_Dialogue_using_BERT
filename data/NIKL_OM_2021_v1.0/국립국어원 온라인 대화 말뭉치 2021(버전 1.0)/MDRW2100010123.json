      "utterance": [
        {
          "id": "MDRW2100010123.1.1",
          "form": "가고싶은 여행지가 있나요?",
          "original_form": "가고싶은 여행지가 있나요?",
          "speaker_id": "1",
          "time": "20210823 14:35"
        },
        {
          "id": "MDRW2100010123.1.2",
          "form": "코로나 끝나면 스페인 가보고싶네요!",
          "original_form": "코로나 끝나면 스페인 가보고싶네요!",
          "speaker_id": "2",
          "time": "20210823 14:36"
        },
        {
          "id": "MDRW2100010123.1.3",
          "form": "사실 어디든 떠나고싶어요 ㅠㅠ",
          "original_form": "사실 어디든 떠나고싶어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 14:37"
        },
        {
          "id": "MDRW2100010123.1.4",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "1",
          "time": "20210823 14:37"
        },
        {
          "id": "MDRW2100010123.1.5",
          "form": "저는 가깝게 제주도다녀오고싶어요",
          "original_form": "저는 가깝게 제주도다녀오고싶어요",
          "speaker_id": "1",
          "time": "20210823 14:37"
        },
        {
          "id": "MDRW2100010123.1.6",
          "form": "제주도 좋죠.. 제주도바다 보고싶어요 ..",
          "original_form": "제주도 좋죠.. 제주도바다 보고싶어요 ..",
          "speaker_id": "2",
          "time": "20210823 14:41"
        },
        {
          "id": "MDRW2100010123.1.7",
          "form": "바다좋아하세요?",
          "original_form": "바다좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 14:42"
        },
        {
          "id": "MDRW2100010123.1.8",
          "form": "네! 좋아해요",
          "original_form": "네! 좋아해요",
          "speaker_id": "1",
          "time": "20210823 14:43"
        },
        {
          "id": "MDRW2100010123.1.9",
          "form": "저도 좋아해요ㅎㅎ 제주도 바다는 푸른색이라 더 예쁘더라구요",
          "original_form": "저도 좋아해요ㅎㅎ 제주도 바다는 푸른색이라 더 예쁘더라구요",
          "speaker_id": "2",
          "time": "20210823 14:46"
        },
        {
          "id": "MDRW2100010123.1.10",
          "form": "맞아요! 영롱한 색감이에요ㅎㅎ",
          "original_form": "맞아요! 영롱한 색감이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 14:47"
        },
        {
          "id": "MDRW2100010123.1.11",
          "form": "스페인에도 바다명소가 있나용?",
          "original_form": "스페인에도 바다명소가 있나용?",
          "speaker_id": "1",
          "time": "20210823 14:48"
        },
        {
          "id": "MDRW2100010123.1.12",
          "form": "스페인이 바다랑 닿아있어서 명소가 꽤 있나봐요ㅎㅎ",
          "original_form": "스페인이 바다랑 닿아있어서 명소가 꽤 있나봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 14:52"
        },
        {
          "id": "MDRW2100010123.1.13",
          "form": "여건되면 서핑도 한번해보고싶어요 !",
          "original_form": "여건되면 서핑도 한번해보고싶어요 !",
          "speaker_id": "2",
          "time": "20210823 14:52"
        },
        {
          "id": "MDRW2100010123.1.14",
          "form": "우와 서핑도 너무 좋아요! 바람과 속도감ㅋㅋㅋ",
          "original_form": "우와 서핑도 너무 좋아요! 바람과 속도감ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 14:59"
        },
        {
          "id": "MDRW2100010123.1.15",
          "form": "균형감각이 부족해서 잘은 못하지만 해외여행가면 해보고싶네요ㅋㅋ",
          "original_form": "균형감각이 부족해서 잘은 못하지만 해외여행가면 해보고싶네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 15:02"
        },
        {
          "id": "MDRW2100010123.1.16",
          "form": "여행가보신곳 중에 다시가보고싶은 곳 있으신가요?",
          "original_form": "여행가보신곳 중에 다시가보고싶은 곳 있으신가요?",
          "speaker_id": "2",
          "time": "20210823 15:02"
        },
        {
          "id": "MDRW2100010123.1.17",
          "form": "저도 아직 해본적은 없지만 액티비티는 다좋은거같아요ㅋㅋ",
          "original_form": "저도 아직 해본적은 없지만 액티비티는 다좋은거같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:02"
        },
        {
          "id": "MDRW2100010123.1.18",
          "form": "저는 베트남이용 과일이 맛있어서!",
          "original_form": "저는 베트남이용 과일이 맛있어서!",
          "speaker_id": "1",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010123.1.19",
          "form": "와 저도 베트남 너무 좋았어요 열대과일 좋아하시나봐요!!",
          "original_form": "와 저도 베트남 너무 좋았어요 열대과일 좋아하시나봐요!!",
          "speaker_id": "2",
          "time": "20210823 15:05"
        },
        {
          "id": "MDRW2100010123.1.20",
          "form": "망고 더 먹고올걸 후회돼요ㅠㅠ",
          "original_form": "망고 더 먹고올걸 후회돼요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 15:05"
        },
        {
          "id": "MDRW2100010123.1.21",
          "form": "맞아요!! 망고도 좋고 망고스틴이 너무 맛있었어요ㅎㅎ",
          "original_form": "맞아요!! 망고도 좋고 망고스틴이 너무 맛있었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 16:19"
        }
      ]
    }
