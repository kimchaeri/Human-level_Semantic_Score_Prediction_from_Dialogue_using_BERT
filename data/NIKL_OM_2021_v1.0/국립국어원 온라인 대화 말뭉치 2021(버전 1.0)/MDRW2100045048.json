      "utterance": [
        {
          "id": "MDRW2100045048.1.1",
          "form": "샌드위치가게가 제일 아쉬워",
          "original_form": "샌드위치가게가 제일 아쉬워",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045048.1.2",
          "form": "아 거긴기억남ㅋㅋ",
          "original_form": "아 거긴기억남ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045048.1.3",
          "form": "살먼서 처음 먹어본 크로크무슈였어 ㅋㅋ",
          "original_form": "살먼서 처음 먹어본 크로크무슈였어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045048.1.4",
          "form": "난기억도안나....왜이럼ㅜㅜㅜㅋㅋㅋ",
          "original_form": "난기억도안나....왜이럼ㅜㅜㅜㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045048.1.5",
          "form": "야 거기 샌드위치 진짜 많이 팔았잖아 ㅋㅋㅋ",
          "original_form": "야 거기 샌드위치 진짜 많이 팔았잖아 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045048.1.6",
          "form": "종류많았어 거의 섭웨 ㅋㅋ",
          "original_form": "종류많았어 거의 섭웨 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045048.1.7",
          "form": "그건그렇지만....앞으로는 사진을 많이 찍어놔야지",
          "original_form": "그건그렇지만....앞으로는 사진을 많이 찍어놔야지",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045048.1.8",
          "form": "진짜 그러자 ㅋㅋㅋㅋㅋ 야 그 치즈케이크 기억나나",
          "original_form": "진짜 그러자 ㅋㅋㅋㅋㅋ 야 그 치즈케이크 기억나나",
          "speaker_id": "1",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045048.1.9",
          "form": "마듀? 이런느낌의 이른이었는데",
          "original_form": "마듀? 이런느낌의 이른이었는데",
          "speaker_id": "1",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045048.1.10",
          "form": "어떤거??",
          "original_form": "어떤거??",
          "speaker_id": "2",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100045048.1.11",
          "form": "학교바로앞에 있는 케익집에파는거였능데 완전 몰랑몰랑",
          "original_form": "학교바로앞에 있는 케익집에파는거였능데 완전 몰랑몰랑",
          "speaker_id": "1",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045048.1.12",
          "form": "헝 난안갔었나봐",
          "original_form": "헝 난안갔었나봐",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045048.1.13",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045048.1.14",
          "form": "아니야 ㅋㅋ뭔소리야 ㅋㅋㅋ같이갔어",
          "original_form": "아니야 ㅋㅋ뭔소리야 ㅋㅋㅋ같이갔어",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045048.1.15",
          "form": "던킨과 스킨푸드 사이에",
          "original_form": "던킨과 스킨푸드 사이에",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045048.1.16",
          "form": "이렇게 기억이 안날수가?ㅜㅜ",
          "original_form": "이렇게 기억이 안날수가?ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045048.1.17",
          "form": "거기 막 케잌도 팔고 샌드위치도 팔았잖아",
          "original_form": "거기 막 케잌도 팔고 샌드위치도 팔았잖아",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045048.1.18",
          "form": "ㅋㅋㅋ 우리과애들 우리말고 다른애들 완전단골",
          "original_form": "ㅋㅋㅋ 우리과애들 우리말고 다른애들 완전단골",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045048.1.19",
          "form": "난 아무래도 너랑 다른 학교를 다녔나봐",
          "original_form": "난 아무래도 너랑 다른 학교를 다녔나봐",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045048.1.20",
          "form": "이렇게 기억력이 나쁠수가ㅋㅋ",
          "original_form": "이렇게 기억력이 나쁠수가ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100045048.1.21",
