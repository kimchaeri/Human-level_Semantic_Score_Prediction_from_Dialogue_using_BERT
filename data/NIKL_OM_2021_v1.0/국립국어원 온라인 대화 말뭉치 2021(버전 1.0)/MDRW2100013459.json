          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013459.1.3",
          "form": "고구마??",
          "original_form": "고구마??",
          "speaker_id": "1",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013459.1.4",
          "form": "릉응",
          "original_form": "릉응",
          "speaker_id": "2",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013459.1.5",
          "form": "ㄴ난 불고기!",
          "original_form": "ㄴ난 불고기!",
          "speaker_id": "1",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013459.1.6",
          "form": "역시 시그니처구만",
          "original_form": "역시 시그니처구만",
          "speaker_id": "2",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013459.1.7",
          "form": "글치!",
          "original_form": "글치!",
          "speaker_id": "1",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013459.1.8",
          "form": "2위는 고구마",
          "original_form": "2위는 고구마",
          "speaker_id": "1",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013459.1.9",
          "form": "아 고구마묵구싶다",
          "original_form": "아 고구마묵구싶다",
          "speaker_id": "2",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013459.1.10",
          "form": "너두? 나두",
          "original_form": "너두? 나두",
          "speaker_id": "1",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013459.1.11",
          "form": "피자 먹고싶자아",
          "original_form": "피자 먹고싶자아",
          "speaker_id": "2",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013459.1.12",
          "form": "책임져",
          "original_form": "책임져",
          "speaker_id": "2",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013459.1.13",
          "form": "먹자먹자",
          "original_form": "먹자먹자",
          "speaker_id": "1",
          "time": "20210825 14:35"
        },
        {
          "id": "MDRW2100013459.1.14",
          "form": "집에오면서 사와",
