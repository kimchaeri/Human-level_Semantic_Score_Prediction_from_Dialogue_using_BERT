      "utterance": [
        {
          "id": "MDRW2100018020.1.1",
          "form": "여긴 비 오는데 거기는요?",
          "original_form": "여긴 비 오는데 거기는요?",
          "speaker_id": "1",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018020.1.2",
          "form": "안녕하세요 여기도 계속 비오고 있어요",
          "original_form": "안녕하세요 여기도 계속 비오고 있어요",
          "speaker_id": "2",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100018020.1.3",
          "form": "여기도요 하루종일 꾸준하게 오네요",
          "original_form": "여기도요 하루종일 꾸준하게 오네요",
          "speaker_id": "1",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100018020.1.4",
          "form": "비오는거 좋아하세요?",
          "original_form": "비오는거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100018020.1.5",
          "form": "여름 소나기는 좋아해요 짧고 굵게 오는거요",
          "original_form": "여름 소나기는 좋아해요 짧고 굵게 오는거요",
          "speaker_id": "1",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100018020.1.6",
          "form": "어떤 날씨 좋아하나요",
          "original_form": "어떤 날씨 좋아하나요",
          "speaker_id": "1",
          "time": "20210827 15:53"
        },
        {
          "id": "MDRW2100018020.1.7",
          "form": "저는 비오는 날씨에 실내에 있는것만 좋아해요 ㅋㅋ",
          "original_form": "저는 비오는 날씨에 실내에 있는것만 좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:54"
        },
        {
          "id": "MDRW2100018020.1.8",
          "form": "비오는거 보는건 좋죠. 나가는건 별로",
          "original_form": "비오는거 보는건 좋죠. 나가는건 별로",
          "speaker_id": "1",
          "time": "20210827 15:56"
        },
        {
          "id": "MDRW2100018020.1.9",
          "form": "저는 가을 날씨 좋아해요!",
          "original_form": "저는 가을 날씨 좋아해요!",
          "speaker_id": "2",
          "time": "20210827 15:57"
        },
        {
          "id": "MDRW2100018020.1.10",
          "form": "저도요 더운거 보다 선선한게 좋아요",
          "original_form": "저도요 더운거 보다 선선한게 좋아요",
          "speaker_id": "1",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100018020.1.11",
          "form": "가을이 너무 짧아서 아쉬워요 ㅜㅜ",
          "original_form": "가을이 너무 짧아서 아쉬워요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 16:03"
        },
        {
          "id": "MDRW2100018020.1.12",
          "form": "맞아요. 코스모스 피면 이쁜데",
          "original_form": "맞아요. 코스모스 피면 이쁜데",
          "speaker_id": "1",
          "time": "20210827 16:15"
        },
        {
          "id": "MDRW2100018020.1.13",
          "form": "아 꽃구경 한지 오래네요",
          "original_form": "아 꽃구경 한지 오래네요",
          "speaker_id": "2",
          "time": "20210827 19:37"
        },
        {
          "id": "MDRW2100018020.1.14",
          "form": "아직 길거리에 코스모스가 안 보이는거 같아요. 빨리 보고 싶은데",
          "original_form": "아직 길거리에 코스모스가 안 보이는거 같아요. 빨리 보고 싶은데",
          "speaker_id": "1",
          "time": "20210827 20:06"
        }
      ]
