      "utterance": [
        {
          "id": "MDRW2100033434.1.1",
          "form": "오 볼께요",
          "original_form": "오 볼께요",
          "speaker_id": "2",
          "time": "20210823 21:40"
        },
        {
          "id": "MDRW2100033434.1.2",
          "form": "누룽지가 진짜 정보가 없었는데 감사해요",
          "original_form": "누룽지가 진짜 정보가 없었는데 감사해요",
          "speaker_id": "2",
          "time": "20210823 21:40"
        },
        {
          "id": "MDRW2100033434.1.3",
          "form": "제가 어제 부추 레시피보고 무치는데",
          "original_form": "제가 어제 부추 레시피보고 무치는데",
          "speaker_id": "2",
          "time": "20210823 21:41"
        },
        {
          "id": "MDRW2100033434.1.4",
          "form": "매실청 부어버렸어요 ㅋㅋ",
          "original_form": "매실청 부어버렸어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:41"
        },
        {
          "id": "MDRW2100033434.1.5",
          "form": "항시 그래서 요리가 애매해 져요 ㅋㅋ",
          "original_form": "항시 그래서 요리가 애매해 져요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:41"
        },
        {
          "id": "MDRW2100033434.1.6",
          "form": "아들이 속 쓰리데요",
          "original_form": "아들이 속 쓰리데요",
          "speaker_id": "2",
          "time": "20210823 21:41"
        },
        {
          "id": "MDRW2100033434.1.7",
          "form": "레시피데로 하면 되는데 엇나가요",
          "original_form": "레시피데로 하면 되는데 엇나가요",
          "speaker_id": "2",
          "time": "20210823 21:42"
        },
        {
          "id": "MDRW2100033434.1.8",
          "form": "아놔 ㅋㅋ생각보다 많이 집어먹었어요 ㅋㅋ암만 현미대로",
          "original_form": "아놔 ㅋㅋ생각보다 많이 집어먹었어요 ㅋㅋ암만 현미대로",
          "speaker_id": "1",
          "time": "20210823 21:44"
        },
        {
          "id": "MDRW2100033434.1.9",
          "form": "현미래도",
          "original_form": "현미래도",
          "speaker_id": "1",
          "time": "20210823 21:44"
        },
        {
          "id": "MDRW2100033434.1.10",
          "form": "이래먹으면 ㅋㅋ",
          "original_form": "이래먹으면 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:44"
        },
        {
          "id": "MDRW2100033434.1.11",
          "form": "ㅋㅋ 저녁으로 좀 많이 드셨네요",
          "original_form": "ㅋㅋ 저녁으로 좀 많이 드셨네요",
          "speaker_id": "2",
          "time": "20210823 21:56"
        },
        {
          "id": "MDRW2100033434.1.12",
          "form": "맛난 음식은 원래 야금야금 먹가보면 과자 누룽지 금방 다먹고 엄청 먹어요",
          "original_form": "맛난 음식은 원래 야금야금 먹가보면 과자 누룽지 금방 다먹고 엄청 먹어요",
          "speaker_id": "2",
          "time": "20210823 21:57"
        },
        {
          "id": "MDRW2100033434.1.13",
          "form": "저녁으로는 누룽지 토마토 바나나면 좋은데요",
          "original_form": "저녁으로는 누룽지 토마토 바나나면 좋은데요",
          "speaker_id": "2",
          "time": "20210823 21:57"
        },
        {
          "id": "MDRW2100033434.1.14",
          "form": "현미가 몸에좋고",
          "original_form": "현미가 몸에좋고",
          "speaker_id": "2",
          "time": "20210823 21:57"
        },
        {
          "id": "MDRW2100033434.1.15",
          "form": "이거 근데 진짜 맛있네요",
          "original_form": "이거 근데 진짜 맛있네요",
          "speaker_id": "1",
          "time": "20210823 21:57"
        },
        {
          "id": "MDRW2100033434.1.16",
          "form": "현미 누룽지라길래 무시했는데 ㅋㅋ",
          "original_form": "현미 누룽지라길래 무시했는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:58"
        },
        {
          "id": "MDRW2100033434.1.17",
          "form": "전 좀 전에 막창남은거 포도주 까지 불량 저녁입니다",
          "original_form": "전 좀 전에 막창남은거 포도주 까지 불량 저녁입니다",
          "speaker_id": "2",
          "time": "20210823 21:58"
        },
        {
          "id": "MDRW2100033434.1.18",
          "form": "ㅋㅋ무시하면 안돼요 쭉쭉 들어갑니다",
          "original_form": "ㅋㅋ무시하면 안돼요 쭉쭉 들어갑니다",
          "speaker_id": "2",
          "time": "20210823 21:58"
        },
        {
          "id": "MDRW2100033434.1.19",
          "form": "오늘 사실 비와서 저녁에 전 술 땡겼어요",
          "original_form": "오늘 사실 비와서 저녁에 전 술 땡겼어요",
          "speaker_id": "2",
          "time": "20210823 21:58"
        },
        {
          "id": "MDRW2100033434.1.20",
          "form": "크 비오는 날은 전이 국룰이죠",
          "original_form": "크 비오는 날은 전이 국룰이죠",
          "speaker_id": "1",
          "time": "20210823 21:59"
        },
        {
          "id": "MDRW2100033434.1.21",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210823 21:59"
        },
        {
          "id": "MDRW2100033434.1.22",
          "form": "배달 첨시키는데",
          "original_form": "배달 첨시키는데",
          "speaker_id": "2",
          "time": "20210823 21:59"
        },
        {
          "id": "MDRW2100033434.1.23",
          "form": "저는 엄마가 자칭 전을 잘굽는 분이신데 ㅋㅋ",
          "original_form": "저는 엄마가 자칭 전을 잘굽는 분이신데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:59"
        },
        {
          "id": "MDRW2100033434.1.24",
          "form": "리뷰가 좋더니",
          "original_form": "리뷰가 좋더니",
          "speaker_id": "2",
          "time": "20210823 21:59"
        },
        {
          "id": "MDRW2100033434.1.25",
          "form": "와 진짜 많이 줰ㅅ러요",
          "original_form": "와 진짜 많이 줰ㅅ러요",
          "speaker_id": "2",
          "time": "20210823 21:59"
        },
        {
          "id": "MDRW2100033434.1.26",
          "form": "줬어요",
          "original_form": "줬어요",
          "speaker_id": "2",
          "time": "20210823 21:59"
        },
        {
          "id": "MDRW2100033434.1.27",
          "form": "항상 이상하게 양이 ㅋㅋ 엄청나다는",
          "original_form": "항상 이상하게 양이 ㅋㅋ 엄청나다는",
          "speaker_id": "1",
          "time": "20210823 21:59"
        },
        {
          "id": "MDRW2100033434.1.28",
          "form": "배터져요",
          "original_form": "배터져요",
          "speaker_id": "2",
          "time": "20210823 21:59"
        },
        {
          "id": "MDRW2100033434.1.29",
          "form": "오 큰손 이시네요",
          "original_form": "오 큰손 이시네요",
          "speaker_id": "2",
          "time": "20210823 22:00"
        },
        {
          "id": "MDRW2100033434.1.30",
          "form": "부럽네요 엄마전",
          "original_form": "부럽네요 엄마전",
          "speaker_id": "2",
          "time": "20210823 22:00"
        },
        {
          "id": "MDRW2100033434.1.31",
          "form": "요 시킨곳 진짜 돈 안아깝게 줬어요",
          "original_form": "요 시킨곳 진짜 돈 안아깝게 줬어요",
          "speaker_id": "2",
          "time": "20210823 22:00"
        },
        {
          "id": "MDRW2100033434.1.32",
          "form": "그럼 뿌듯하죠 ㅋㅋ",
          "original_form": "그럼 뿌듯하죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:00"
        },
        {
          "id": "MDRW2100033434.1.33",
          "form": "요새 진짜 배달안되는 메뉴가없어요",
          "original_form": "요새 진짜 배달안되는 메뉴가없어요",
          "speaker_id": "1",
          "time": "20210823 22:00"
        },
        {
          "id": "MDRW2100033434.1.34",
          "form": "첨봐요 이런집 고기 생선 호박 동그랑땡 두부 고추전",
          "original_form": "첨봐요 이런집 고기 생선 호박 동그랑땡 두부 고추전",
          "speaker_id": "2",
          "time": "20210823 22:00"
        },
        {
          "id": "MDRW2100033434.1.35",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210823 22:01"
        },
        {
          "id": "MDRW2100033434.1.36",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210823 22:01"
        },
        {
          "id": "MDRW2100033434.1.37",
          "form": "고추전 항상 조금이라 아쉽네요",
          "original_form": "고추전 항상 조금이라 아쉽네요",
          "speaker_id": "2",
          "time": "20210823 22:01"
        },
        {
          "id": "MDRW2100033434.1.38",
          "form": "근데 전 항상 같은것만 먹고 식상해요",
          "original_form": "근데 전 항상 같은것만 먹고 식상해요",
          "speaker_id": "2",
          "time": "20210823 22:01"
        },
        {
          "id": "MDRW2100033434.1.39",
          "form": "근데 또 잘먹긴해요",
          "original_form": "근데 또 잘먹긴해요",
