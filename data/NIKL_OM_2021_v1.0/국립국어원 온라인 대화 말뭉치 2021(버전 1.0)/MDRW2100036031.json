      "utterance": [
        {
          "id": "MDRW2100036031.1.1",
          "form": "안녕하세용 최근에 재밌게보신 영화나 드라마 있으신가용?",
          "original_form": "안녕하세용 최근에 재밌게보신 영화나 드라마 있으신가용?",
          "speaker_id": "1",
          "time": "20210830 14:38"
        },
        {
          "id": "MDRW2100036031.1.2",
          "form": "빈센조까지는 봤어요ㅋ",
          "original_form": "빈센조까지는 봤어요ㅋ",
          "speaker_id": "2",
          "time": "20210830 14:41"
        },
        {
          "id": "MDRW2100036031.1.3",
          "form": "오 이거 인기 많던데 결말까지 잘 났나용?ㅎㅎ",
          "original_form": "오 이거 인기 많던데 결말까지 잘 났나용?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 14:41"
        },
        {
          "id": "MDRW2100036031.1.4",
          "form": "끝이 좀 그랬는데 그래도 괜찮았어요",
          "original_form": "끝이 좀 그랬는데 그래도 괜찮았어요",
          "speaker_id": "2",
          "time": "20210830 14:43"
        },
        {
          "id": "MDRW2100036031.1.5",
          "form": "오오 시청률도 완전 높던데 봐야겠네용ㅎㅎㅎ",
          "original_form": "오오 시청률도 완전 높던데 봐야겠네용ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 14:44"
        },
        {
          "id": "MDRW2100036031.1.6",
          "form": "저는 드라마 결말까지 난거 위주로 보거든용",
          "original_form": "저는 드라마 결말까지 난거 위주로 보거든용",
          "speaker_id": "1",
          "time": "20210830 14:44"
        },
        {
          "id": "MDRW2100036031.1.7",
          "form": "아 그래요?  끝이 좀 애매하긴 해요ㅋ",
          "original_form": "아 그래요?  끝이 좀 애매하긴 해요ㅋ",
          "speaker_id": "2",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100036031.1.8",
          "form": "ㅋㅋㅋㅋ 으흠~ 고민되네요ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 으흠~ 고민되네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100036031.1.9",
          "form": "저는 아직까지도 사랑의 불시착을",
          "original_form": "저는 아직까지도 사랑의 불시착을",
          "speaker_id": "1",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100036031.1.10",
          "form": "몇번씩 다시봐요ㅋㅋㅋㅋ",
          "original_form": "몇번씩 다시봐요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:46"
        },
        {
          "id": "MDRW2100036031.1.11",
          "form": "ㅋㅋ 전 도깨비 볼때마다 재밌어요",
          "original_form": "ㅋㅋ 전 도깨비 볼때마다 재밌어요",
          "speaker_id": "2",
          "time": "20210830 14:49"
        },
        {
          "id": "MDRW2100036031.1.12",
          "form": "도깨비 재밌게봤는데 어딘가모르게",
          "original_form": "도깨비 재밌게봤는데 어딘가모르게",
          "speaker_id": "1",
          "time": "20210830 14:50"
        },
        {
          "id": "MDRW2100036031.1.13",
          "form": "슬퍼요ㅠㅠ",
          "original_form": "슬퍼요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 14:50"
        },
        {
          "id": "MDRW2100036031.1.14",
          "form": "그쵸 어찌보면 슬프죠ㅠ",
          "original_form": "그쵸 어찌보면 슬프죠ㅠ",
          "speaker_id": "2",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100036031.1.15",
          "form": "메인 커플보다 서브커플",
          "original_form": "메인 커플보다 서브커플",
          "speaker_id": "1",
          "time": "20210830 14:51"
        },
        {
          "id": "MDRW2100036031.1.16",
          "form": "보는맛에 봤는데",
          "original_form": "보는맛에 봤는데",
          "speaker_id": "1",
          "time": "20210830 14:52"
        },
        {
          "id": "MDRW2100036031.1.17",
          "form": "서브커플이 슬퍼요ㅠㅠ",
          "original_form": "서브커플이 슬퍼요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 14:52"
        },
        {
          "id": "MDRW2100036031.1.18",
          "form": "아 서브커플...  그래도 환생해서 잘 됐자나요",
          "original_form": "아 서브커플...  그래도 환생해서 잘 됐자나요",
          "speaker_id": "2",
          "time": "20210830 14:52"
        },
        {
          "id": "MDRW2100036031.1.19",
          "form": "넹넹 근데 그 서사가...ㅠㅠ",
          "original_form": "넹넹 근데 그 서사가...ㅠㅠ",
          "speaker_id": "1",
