      "utterance": [
        {
          "id": "MDRW2100043645.1.1",
          "form": "ㅋㅋ감사해요",
          "original_form": "ㅋㅋ감사해요",
          "speaker_id": "1",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043645.1.2",
          "form": "퇴근하셨나요!",
          "original_form": "퇴근하셨나요!",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043645.1.3",
          "form": "넵다행히도 퇴근했습니다 넘좋네요",
          "original_form": "넵다행히도 퇴근했습니다 넘좋네요",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043645.1.4",
          "form": "저녁무렵에 갑자기 비가 쏟아져서 퇴근길 고되시진 않았을지",
          "original_form": "저녁무렵에 갑자기 비가 쏟아져서 퇴근길 고되시진 않았을지",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043645.1.5",
          "form": "무사히 돌아가셔서 다행입니다",
          "original_form": "무사히 돌아가셔서 다행입니다",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043645.1.6",
          "form": "ㅎㅎ네 비가 추적추적오더 다행히 퇴근때는 타이밍좋게 안오더라고요",
          "original_form": "ㅎㅎ네 비가 추적추적오더 다행히 퇴근때는 타이밍좋게 안오더라고요",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043645.1.7",
          "form": "name1님은 재택이셨나요",
          "original_form": "&name1&님은 재택이셨나요",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043645.1.8",
          "form": "아 다행이에요",
          "original_form": "아 다행이에요",
          "speaker_id": "1",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043645.1.9",
          "form": "네 이번달은 재택입니다 후후",
          "original_form": "네 이번달은 재택입니다 후후",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043645.1.10",
          "form": "개강 첫주가 폭풍같이 지나갔어요",
          "original_form": "개강 첫주가 폭풍같이 지나갔어요",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043645.1.11",
          "form": "맞네요 바쁘셨겠어요",
          "original_form": "맞네요 바쁘셨겠어요",
          "speaker_id": "2",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043645.1.12",
          "form": "우리 학생들은 잘계시던가요",
          "original_form": "우리 학생들은 잘계시던가요",
          "speaker_id": "2",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043645.1.13",
          "form": "네 새학기 새친구들 만나서 이제 인사했습니다",
          "original_form": "네 새학기 새친구들 만나서 이제 인사했습니다",
          "speaker_id": "1",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100043645.1.14",
          "form": "성실한 친구들이 많은 것 같아요 한 삼백명 중 열 명도 채 안빠지고 다 참여했더라고요",
          "original_form": "성실한 친구들이 많은 것 같아요 한 삼백명 중 열 명도 채 안빠지고 다 참여했더라고요",
          "speaker_id": "1",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100043645.1.15",
          "form": "(여러 반 합쳐서요 ㅋㅋ )",
          "original_form": "(여러 반 합쳐서요 ㅋㅋ )",
          "speaker_id": "1",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043645.1.16",
          "form": "ㅋㅋㅋㅋㅋ와 첫주 슬그머니 빠질법도한데",
          "original_form": "ㅋㅋㅋㅋㅋ와 첫주 슬그머니 빠질법도한데",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043645.1.17",
          "form": "요즘 다들성실하시군요",
          "original_form": "요즘 다들성실하시군요",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043645.1.18",
          "form": "그쵸 저도 첫주는 꽤 게으르게 참여한 느낌인데",
          "original_form": "그쵸 저도 첫주는 꽤 게으르게 참여한 느낌인데",
          "speaker_id": "1",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043645.1.19",
          "form": "다들 열심히 카메라 앞에서..",
          "original_form": "다들 열심히 카메라 앞에서..",
          "speaker_id": "1",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043645.1.20",
          "form": "줌이라서",
          "original_form": "줌이라서",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043645.1.21",
          "form": "출첵은 편하시겠어요",
          "original_form": "출첵은 편하시겠어요",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043645.1.22",
          "form": "하지만 몰랐겠죠 아래는 츄리닝 바지였단걸!",
          "original_form": "하지만 몰랐겠죠 아래는 츄리닝 바지였단걸!",
          "speaker_id": "1",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043645.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043645.1.24",
          "form": "ㅋㅋ네 편한 부분이 있어요",
          "original_form": "ㅋㅋ네 편한 부분이 있어요",
          "speaker_id": "1",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043645.1.25",
          "form": "하지만 학생들도 하의는 잠옷이지 않았을까요!",
          "original_form": "하지만 학생들도 하의는 잠옷이지 않았을까요!",
          "speaker_id": "2",
          "time": "20210907 20:01"
        }
      ]
