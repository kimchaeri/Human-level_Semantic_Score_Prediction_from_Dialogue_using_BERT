      "utterance": [
        {
          "id": "MDRW2100022562.1.1",
          "form": "그쪽도 날씨 흐린가요?",
          "original_form": "그쪽도 날씨 흐린가요?",
          "speaker_id": "1",
          "time": "20210829 15:43"
        },
        {
          "id": "MDRW2100022562.1.2",
          "form": "아뇨 여긴 맑아요",
          "original_form": "아뇨 여긴 맑아요",
          "speaker_id": "2",
          "time": "20210829 15:44"
        },
        {
          "id": "MDRW2100022562.1.3",
          "form": "거긴 흐린가보군요 비 오나요?",
          "original_form": "거긴 흐린가보군요 비 오나요?",
          "speaker_id": "2",
          "time": "20210829 15:44"
        },
        {
          "id": "MDRW2100022562.1.4",
          "form": "새벽까지 비왓어요..",
          "original_form": "새벽까지 비왓어요..",
          "speaker_id": "1",
          "time": "20210829 15:45"
        },
        {
          "id": "MDRW2100022562.1.5",
          "form": "오 여긴 비가 안왔어요.. 비오는날 좋아하세요?",
          "original_form": "오 여긴 비가 안왔어요.. 비오는날 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 15:46"
        },
        {
          "id": "MDRW2100022562.1.6",
          "form": "아뇨.. 너무 실어요.. 축축하고 음침하고..",
          "original_form": "아뇨.. 너무 실어요.. 축축하고 음침하고..",
          "speaker_id": "1",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100022562.1.7",
          "form": "비 좋아하세요?",
          "original_form": "비 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 15:47"
        },
        {
          "id": "MDRW2100022562.1.8",
          "form": "비는 싫은데 비 오기 직전은 좋아요",
          "original_form": "비는 싫은데 비 오기 직전은 좋아요",
          "speaker_id": "2",
          "time": "20210829 15:48"
        },
        {
          "id": "MDRW2100022562.1.9",
          "form": "시원하고 습한거요...",
          "original_form": "시원하고 습한거요...",
          "speaker_id": "2",
          "time": "20210829 15:49"
        },
        {
          "id": "MDRW2100022562.1.10",
          "form": "헉.. 진짜요?",
          "original_form": "헉.. 진짜요?",
          "speaker_id": "1",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100022562.1.11",
          "form": "습한걸 좋아하시다니...",
          "original_form": "습한걸 좋아하시다니...",
          "speaker_id": "1",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100022562.1.12",
          "form": "특이하시네요",
          "original_form": "특이하시네요",
          "speaker_id": "1",
          "time": "20210829 15:51"
        },
        {
          "id": "MDRW2100022562.1.13",
          "form": "습하면 좀 숨이 잘 쉬어지는것 같아서요..ㅋㅋ 근데 덥고 습한건 저도 싫어요",
          "original_form": "습하면 좀 숨이 잘 쉬어지는것 같아서요..ㅋㅋ 근데 덥고 습한건 저도 싫어요",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022562.1.14",
          "form": "제일 좋아하는 계절이 뭐예요?",
          "original_form": "제일 좋아하는 계절이 뭐예요?",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022562.1.15",
          "form": "가을이 젤 좋아요.. 쾌청하고 하늘고 높고",
          "original_form": "가을이 젤 좋아요.. 쾌청하고 하늘고 높고",
          "speaker_id": "1",
          "time": "20210829 17:36"
        },
        {
          "id": "MDRW2100022562.1.16",
          "form": "아무래도 가을이 제일 살기 편하죠",
          "original_form": "아무래도 가을이 제일 살기 편하죠",
          "speaker_id": "2",
          "time": "20210829 19:15"
        },
        {
          "id": "MDRW2100022562.1.17",
          "form": "이제 거의 가을 날씨 돼가는것같지 않나요",
          "original_form": "이제 거의 가을 날씨 돼가는것같지 않나요",
          "speaker_id": "2",
          "time": "20210829 19:15"
        },
        {
          "id": "MDRW2100022562.1.18",
          "form": "어제비오고 오늘 급 선선하네요..",
          "original_form": "어제비오고 오늘 급 선선하네요..",
          "speaker_id": "1",
          "time": "20210829 19:17"
        },
        {
          "id": "MDRW2100022562.1.19",
          "form": "오늘은 딱 가을 같아요",
          "original_form": "오늘은 딱 가을 같아요",
          "speaker_id": "1",
          "time": "20210829 19:17"
        },
        {
          "id": "MDRW2100022562.1.20",
          "form": "맞아요 이제 슬슬 긴팔옷을 입어도 될 것 같아요",
          "original_form": "맞아요 이제 슬슬 긴팔옷을 입어도 될 것 같아요",
          "speaker_id": "2",
          "time": "20210829 20:06"
        },
        {
          "id": "MDRW2100022562.1.21",
          "form": "그런데 이러다가 또 급격하게 추워지겠죠 가 무슨 문제일까요..",
          "original_form": "그런데 이러다가 또 급격하게 추워지겠죠 가 무슨 문제일까요..",
