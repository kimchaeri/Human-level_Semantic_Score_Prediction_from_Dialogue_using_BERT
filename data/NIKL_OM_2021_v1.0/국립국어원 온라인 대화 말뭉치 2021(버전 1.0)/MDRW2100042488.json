      "utterance": [
        {
          "id": "MDRW2100042488.1.1",
          "form": "ㅋㅋㅋㅋ난 웃긴 게",
          "original_form": "ㅋㅋㅋㅋ난 웃긴 게",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042488.1.2",
          "form": "쌈 재료 다 갖다놓고 쌈을 안 싸",
          "original_form": "쌈 재료 다 갖다놓고 쌈을 안 싸",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042488.1.3",
          "form": "헐 왜???",
          "original_form": "헐 왜???",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042488.1.4",
          "form": "재료를 각각 따로 먹어",
          "original_form": "재료를 각각 따로 먹어",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042488.1.5",
          "form": "진짜웃긴다",
          "original_form": "진짜웃긴다",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042488.1.6",
          "form": "쌈 쌌을 때 야채에 고기맛이 가려지는 게 분해",
          "original_form": "쌈 쌌을 때 야채에 고기맛이 가려지는 게 분해",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042488.1.7",
          "form": "채식동물마냥",
          "original_form": "채식동물마냥",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042488.1.8",
          "form": "왜 따로먹냐",
          "original_form": "왜 따로먹냐",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042488.1.9",
          "form": "입에 넣으면",
          "original_form": "입에 넣으면",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042488.1.10",
          "form": "고기맛이 확 풍겨야 하는데",
          "original_form": "고기맛이 확 풍겨야 하는데",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042488.1.11",
          "form": "상추잎이 나와 고기 사이를 방해해",
          "original_form": "상추잎이 나와 고기 사이를 방해해",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042488.1.12",
          "form": "하긴 똥 잘안나오는것보다",
          "original_form": "하긴 똥 잘안나오는것보다",
          "speaker_id": "1",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042488.1.13",
          "form": "맛있는게 났지",
          "original_form": "맛있는게 났지",
          "speaker_id": "1",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042488.1.14",
          "form": "넌 쌈 잘 먹어?",
          "original_form": "넌 쌈 잘 먹어?",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042488.1.15",
          "form": "아니 난 쌈 싫어해",
          "original_form": "아니 난 쌈 싫어해",
          "speaker_id": "1",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042488.1.16",
          "form": "왜? 야채때문에?",
          "original_form": "왜? 야채때문에?",
          "speaker_id": "2",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042488.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㄱ근데 쌈안에 밥넣어서 주는건 맛있더라",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱ근데 쌈안에 밥넣어서 주는건 맛있더라",
          "speaker_id": "1",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042488.1.18",
          "form": "야채는 항상 천대받아",
          "original_form": "야채는 항상 천대받아",
          "speaker_id": "1",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042488.1.19",
          "form": "내 밥상에서",
          "original_form": "내 밥상에서",
          "speaker_id": "1",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042488.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ야채가 울겠어",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ야채가 울겠어",
          "speaker_id": "2",
          "time": "20210906 22:28"
        }
      ]
