      "utterance": [
        {
          "id": "MDRW2100004581.1.1",
          "form": "비가 너무 많이 와요",
          "original_form": "비가 너무 많이 와요",
          "speaker_id": "2",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004581.1.2",
          "form": "자다가 헐레벌떡 창문 닫았어요",
          "original_form": "자다가 헐레벌떡 창문 닫았어요",
          "speaker_id": "2",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004581.1.3",
          "form": "그리고 입추지나서 추워요",
          "original_form": "그리고 입추지나서 추워요",
          "speaker_id": "1",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004581.1.4",
          "form": "더워요",
          "original_form": "더워요",
          "speaker_id": "2",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004581.1.5",
          "form": "추워요",
          "original_form": "추워요",
          "speaker_id": "1",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004581.1.6",
          "form": "당신을 이해할수없어요",
          "original_form": "당신을 이해할수없어요",
          "speaker_id": "1",
          "time": "20210821 17:16"
        },
        {
          "id": "MDRW2100004581.1.7",
          "form": "골방...케췌엑~!",
          "original_form": "골방...케췌엑~!",
          "speaker_id": "2",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004581.1.8",
          "form": "츠암ㄴ나~!!~!!",
          "original_form": "츠암ㄴ나~!!~!!",
          "speaker_id": "1",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004581.1.9",
          "form": "제 방은 남향이라서 더워요",
          "original_form": "제 방은 남향이라서 더워요",
          "speaker_id": "2",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004581.1.10",
          "form": "제방은완벽한북향이라",
          "original_form": "제방은완벽한북향이라",
          "speaker_id": "1",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004581.1.11",
          "form": "추워요",
          "original_form": "추워요",
          "speaker_id": "1",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004581.1.12",
          "form": "안타까워요",
          "original_form": "안타까워요",
          "speaker_id": "2",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004581.1.13",
          "form": "절조롱하시나요?",
          "original_form": "절조롱하시나요?",
          "speaker_id": "1",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004581.1.14",
          "form": "대신 저희집 거실이 북향이에요..",
          "original_form": "대신 저희집 거실이 북향이에요..",
          "speaker_id": "2",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004581.1.15",
          "form": "굿굿",
          "original_form": "굿굿",
          "speaker_id": "1",
          "time": "20210821 17:17"
        },
        {
          "id": "MDRW2100004581.1.16",
          "form": "얼마전에 반팔셔츠 샀는데",
          "original_form": "얼마전에 반팔셔츠 샀는데",
          "speaker_id": "2",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004581.1.17",
          "form": "얼마 못입고 집어넣네요",
          "original_form": "얼마 못입고 집어넣네요",
          "speaker_id": "2",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004581.1.18",
          "form": "풉킼",
          "original_form": "풉킼",
          "speaker_id": "1",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004581.1.19",
          "form": "대화를 하라고 이쉑",
          "original_form": "대화를 하라고 이쉑",
          "speaker_id": "2",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004581.1.20",
          "form": "조롱멈춰~!",
          "original_form": "조롱멈춰~!",
          "speaker_id": "2",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004581.1.21",
          "form": "저는 알로하셔츠샀는데",
          "original_form": "저는 알로하셔츠샀는데",
          "speaker_id": "1",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004581.1.22",
          "form": "조롱특화인간",
          "original_form": "조롱특화인간",
          "speaker_id": "2",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004581.1.23",
          "form": "추워도입고다닐거에요",
          "original_form": "추워도입고다닐거에요",
          "speaker_id": "1",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004581.1.24",
          "form": "저도 알로하셔츠 샀어요",
          "original_form": "저도 알로하셔츠 샀어요",
          "speaker_id": "2",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004581.1.25",
          "form": "왜냐면 그게 간지니까",
          "original_form": "왜냐면 그게 간지니까",
          "speaker_id": "1",
          "time": "20210821 17:18"
        },
        {
          "id": "MDRW2100004581.1.26",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "1",
          "time": "20210821 17:19"
        },
        {
          "id": "MDRW2100004581.1.27",
          "form": "간지작살",
          "original_form": "간지작살",
          "speaker_id": "2",
          "time": "20210821 17:19"
        },
        {
          "id": "MDRW2100004581.1.28",
          "form": "내일은 비 안온대요",
          "original_form": "내일은 비 안온대요",
          "speaker_id": "1",
          "time": "20210821 17:19"
        },
        {
          "id": "MDRW2100004581.1.29",
          "form": "그래서 오랜만에 본가갈려고요",
          "original_form": "그래서 오랜만에 본가갈려고요",
          "speaker_id": "1",
          "time": "20210821 17:19"
        },
        {
          "id": "MDRW2100004581.1.30",
          "form": "아 내일 운동가는데 다행이네요",
          "original_form": "아 내일 운동가는데 다행이네요",
          "speaker_id": "2",
          "time": "20210821 17:19"
        },
        {
          "id": "MDRW2100004581.1.31",
          "form": "성마 가다가 비오는건아니겠죠",
          "original_form": "성마 가다가 비오는건아니겠죠",
          "speaker_id": "1",
          "time": "20210821 17:19"
        },
        {
          "id": "MDRW2100004581.1.32",
          "form": "집에 있을 때만 비오면 좋겠어요",
          "original_form": "집에 있을 때만 비오면 좋겠어요",
          "speaker_id": "2",
          "time": "20210821 17:19"
        },
        {
          "id": "MDRW2100004581.1.33",
          "form": "그럼 또..무력하게...비를 맞으며",
          "original_form": "그럼 또..무력하게...비를 맞으며",
          "speaker_id": "1",
          "time": "20210821 17:19"
        },
        {
          "id": "MDRW2100004581.1.34",
          "form": "걸어가겠죠...",
          "original_form": "걸어가겠죠...",
          "speaker_id": "1",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004581.1.35",
          "form": "키히힉 너무 웃기고 좋아요 기대하고있어요",
          "original_form": "키히힉 너무 웃기고 좋아요 기대하고있어요",
          "speaker_id": "2",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004581.1.36",
          "form": "비 왔으면 좋겠어요",
          "original_form": "비 왔으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004581.1.37",
          "form": "주위에는 논 밖에없는 길을",
          "original_form": "주위에는 논 밖에없는 길을",
          "speaker_id": "1",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004581.1.38",
          "form": "혼자...",
          "original_form": "혼자...",
          "speaker_id": "1",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004581.1.39",
          "form": "저벅저벅",
          "original_form": "저벅저벅",
          "speaker_id": "1",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004581.1.40",
          "form": "논두렁 시체 안 되게 조심하세요",
          "original_form": "논두렁 시체 안 되게 조심하세요",
          "speaker_id": "2",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004581.1.41",
          "form": "우산 챙기시고",
          "original_form": "우산 챙기시고",
          "speaker_id": "2",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004581.1.42",
          "form": "오늘도 비때문에 늦잠잤어요",
          "original_form": "오늘도 비때문에 늦잠잤어요",
          "speaker_id": "1",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004581.1.43",
          "form": "저도요^^",
          "original_form": "저도요^^",
          "speaker_id": "2",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004581.1.44",
          "form": "비가 몸을 축 처지게 하네요",
          "original_form": "비가 몸을 축 처지게 하네요",
          "speaker_id": "2",
          "time": "20210821 17:21"
        },
        {
          "id": "MDRW2100004581.1.45",
          "form": "태풍 제정신 아니에요",
          "original_form": "태풍 제정신 아니에요",
          "speaker_id": "2",
          "time": "20210825 16:59"
        }
      ]
