          "speaker_id": "2",
          "time": "20210903 07:41"
        },
        {
          "id": "MDRW2100029422.1.8",
          "form": "전 보드게임좋아해요",
          "original_form": "전 보드게임좋아해요",
          "speaker_id": "1",
          "time": "20210903 07:41"
        },
        {
