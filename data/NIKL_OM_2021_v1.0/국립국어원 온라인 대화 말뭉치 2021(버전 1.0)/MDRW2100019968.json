      "utterance": [
        {
          "id": "MDRW2100019968.1.1",
          "form": "디저트 어떤거 좋아하세요?",
          "original_form": "디저트 어떤거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019968.1.2",
          "form": "케이크도 좋고 아이스크림도 좋아요!",
          "original_form": "케이크도 좋고 아이스크림도 좋아요!",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100019968.1.3",
          "form": "케이크중 어떤게 좋으신가요?",
          "original_form": "케이크중 어떤게 좋으신가요?",
          "speaker_id": "1",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100019968.1.4",
          "form": "저는 딸기 쇼트랑 초코무스 좋아해요",
          "original_form": "저는 딸기 쇼트랑 초코무스 좋아해요",
          "speaker_id": "1",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100019968.1.5",
          "form": "둘다 맛있는거다ㅠㅠ",
          "original_form": "둘다 맛있는거다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100019968.1.6",
          "form": "저도 초코케이크 좋아하구 치즈케이크도 좋아하고 생크림 케이크도 좋아하고",
          "original_form": "저도 초코케이크 좋아하구 치즈케이크도 좋아하고 생크림 케이크도 좋아하고",
          "speaker_id": "2",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100019968.1.7",
          "form": "옛날 시장에 팔던 버터케이크인가? 그런거 빼고는 다 괜찮은거같아요ㅋㅋㅋ",
          "original_form": "옛날 시장에 팔던 버터케이크인가? 그런거 빼고는 다 괜찮은거같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100019968.1.8",
          "form": "사실 다맛있긴 해요 케익은 그죠ㅋㅋㅋㄱㅋ",
          "original_form": "사실 다맛있긴 해요 케익은 그죠ㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100019968.1.9",
          "form": "아이스크림도 다맛있죠..",
          "original_form": "아이스크림도 다맛있죠..",
          "speaker_id": "1",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100019968.1.10",
          "form": "맞아요ㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100019968.1.11",
          "form": "민초 좋아하시나요?ㅋㅋㅋ",
          "original_form": "민초 좋아하시나요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100019968.1.12",
          "form": "혹시 베라픽 뭐에요??!!",
          "original_form": "혹시 베라픽 뭐에요??!!",
          "speaker_id": "2",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100019968.1.13",
          "form": "저는 이상한 나라의 솜사탕 좋아해요!!",
          "original_form": "저는 이상한 나라의 솜사탕 좋아해요!!",
          "speaker_id": "2",
          "time": "20210827 23:54"
        },
        {
          "id": "MDRW2100019968.1.14",
          "form": "헉 대박 저두요!",
          "original_form": "헉 대박 저두요!",
          "speaker_id": "1",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100019968.1.15",
          "form": "이상한나라의 솜사탕 좋아하는사람 처음봐요",
          "original_form": "이상한나라의 솜사탕 좋아하는사람 처음봐요",
          "speaker_id": "1",
          "time": "20210827 23:57"
        },
        {
          "id": "MDRW2100019968.1.16",
          "form": "뭐라 딱집을수 없는 맛이지만 맛있지않나요ㅠ",
          "original_form": "뭐라 딱집을수 없는 맛이지만 맛있지않나요ㅠ",
          "speaker_id": "1",
          "time": "20210827 23:58"
        },
        {
          "id": "MDRW2100019968.1.17",
          "form": "헐 우왕ㅋㅋㅋㅋㅋㅋ",
          "original_form": "헐 우왕ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100019968.1.18",
          "form": "맞아요ㅠㅠㅠㅠ 달달하니 맛있거",
          "original_form": "맞아요ㅠㅠㅠㅠ 달달하니 맛있거",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100019968.1.19",
          "form": "톡톡 터지는거도 좋아해요!!",
          "original_form": "톡톡 터지는거도 좋아해요!!",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100019968.1.20",
          "form": "그죠! 너무 달지 않은 단맛!",
          "original_form": "그죠! 너무 달지 않은 단맛!",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100019968.1.21",
          "form": "근데 진짜 뭐라 설명할수 없는 맛이긴 하네요ㅋㅋㅋㅋㅋ",
          "original_form": "근데 진짜 뭐라 설명할수 없는 맛이긴 하네요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100019968.1.22",
          "form": "어쩜 색도 이뻐요",
          "original_form": "어쩜 색도 이뻐요",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100019968.1.23",
          "form": "솜사탕 맛은 또 아닌거같아서ㅋㅋㅋㅋ",
          "original_form": "솜사탕 맛은 또 아닌거같아서ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100019968.1.24",
          "form": "그죠ㅋㅋㅋㅋㄱ",
          "original_form": "그죠ㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100019968.1.25",
          "form": "맞아요 색도 너무 이뻐요 알록달록 파스텔파스텔하고ㅋㅋㅋ",
          "original_form": "맞아요 색도 너무 이뻐요 알록달록 파스텔파스텔하고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100019968.1.26",
          "form": "저도 친구들에게 설명해보려다 포기했어요",
          "original_form": "저도 친구들에게 설명해보려다 포기했어요",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100019968.1.27",
          "form": "무튼 이거 단종되면 안돼요ㅠㅠㅠ",
          "original_form": "무튼 이거 단종되면 안돼요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100019968.1.28",
          "form": "열심히 사먹읍시다ㅋㅋㅋㅋㅋ",
          "original_form": "열심히 사먹읍시다ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100019968.1.29",
          "form": "아자아자! 화이팅ㅋㅋㅋㄱㄱ",
          "original_form": "아자아자! 화이팅ㅋㅋㅋㄱㄱ",
