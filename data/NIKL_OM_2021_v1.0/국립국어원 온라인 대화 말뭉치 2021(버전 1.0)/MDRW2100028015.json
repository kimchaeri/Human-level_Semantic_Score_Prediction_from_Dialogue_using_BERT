      "utterance": [
        {
          "id": "MDRW2100028015.1.1",
          "form": "뭔데?",
          "original_form": "뭔데?",
          "speaker_id": "2",
          "time": "20210902 16:56"
        },
        {
          "id": "MDRW2100028015.1.2",
          "form": "근데 그게 영강이야",
          "original_form": "근데 그게 영강이야",
          "speaker_id": "1",
          "time": "20210902 16:57"
        },
        {
          "id": "MDRW2100028015.1.3",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210902 16:57"
        },
        {
          "id": "MDRW2100028015.1.4",
          "form": "심지어 영문 전공",
          "original_form": "심지어 영문 전공",
          "speaker_id": "1",
          "time": "20210902 16:57"
        },
        {
          "id": "MDRW2100028015.1.5",
          "form": "이게 맞냐",
          "original_form": "이게 맞냐",
          "speaker_id": "1",
          "time": "20210902 16:57"
        },
        {
          "id": "MDRW2100028015.1.6",
          "form": "오~~",
          "original_form": "오~~",
          "speaker_id": "2",
          "time": "20210902 16:57"
        },
        {
          "id": "MDRW2100028015.1.7",
          "form": "일단 담아 봐",
          "original_form": "일단 담아 봐",
          "speaker_id": "2",
          "time": "20210902 16:57"
        },
        {
          "id": "MDRW2100028015.1.8",
          "form": "담고 생각해",
          "original_form": "담고 생각해",
          "speaker_id": "2",
          "time": "20210902 16:57"
        },
        {
          "id": "MDRW2100028015.1.9",
          "form": "장미칼이라는데",
          "original_form": "장미칼이라는데",
          "speaker_id": "1",
          "time": "20210902 16:57"
        },
        {
          "id": "MDRW2100028015.1.10",
          "form": "하... 일단 담아",
          "original_form": "하... 일단 담아",
          "speaker_id": "1",
          "time": "20210902 16:58"
        },
        {
          "id": "MDRW2100028015.1.11",
          "form": "물론 그건 하고 싶다고 할 수 있는 게",
          "original_form": "물론 그건 하고 싶다고 할 수 있는 게",
          "speaker_id": "1",
          "time": "20210902 16:58"
        },
        {
          "id": "MDRW2100028015.1.12",
          "form": "장미칼은 뭐야",
          "original_form": "장미칼은 뭐야",
          "speaker_id": "2",
          "time": "20210902 16:58"
        },
        {
          "id": "MDRW2100028015.1.13",
          "form": "장미칼 몰라?",
          "original_form": "장미칼 몰라?",
          "speaker_id": "1",
          "time": "20210902 16:58"
        },
        {
          "id": "MDRW2100028015.1.14",
          "form": "우리 학교에서만 쓰는 말인가",
          "original_form": "우리 학교에서만 쓰는 말인가",
          "speaker_id": "1",
          "time": "20210902 16:58"
        },
        {
          "id": "MDRW2100028015.1.15",
          "form": "그런 듯??",
          "original_form": "그런 듯??",
          "speaker_id": "2",
          "time": "20210902 16:58"
        },
        {
          "id": "MDRW2100028015.1.16",
          "form": "학점 컷 약간 빡빡한...",
          "original_form": "학점 컷 약간 빡빡한...",
          "speaker_id": "1",
          "time": "20210902 16:58"
        },
        {
          "id": "MDRW2100028015.1.17",
          "form": "헐~~ 완전 처음 듣는 말",
          "original_form": "헐~~ 완전 처음 듣는 말",
          "speaker_id": "2",
          "time": "20210902 16:58"
        },
        {
          "id": "MDRW2100028015.1.18",
          "form": "검색해보니까 많이들 쓰는데",
          "original_form": "검색해보니까 많이들 쓰는데",
          "speaker_id": "1",
          "time": "20210902 16:59"
        },
        {
          "id": "MDRW2100028015.1.19",
          "form": "님네가 안 쓰나봄",
          "original_form": "님네가 안 쓰나봄",
          "speaker_id": "1",
          "time": "20210902 16:59"
        },
        {
          "id": "MDRW2100028015.1.20",
          "form": "우리 학교 에타에 검색해봤는데",
          "original_form": "우리 학교 에타에 검색해봤는데",
          "speaker_id": "2",
          "time": "20210902 17:00"
        },
        {
          "id": "MDRW2100028015.1.21",
          "form": "하나도 안 나옴",
          "original_form": "하나도 안 나옴",
          "speaker_id": "2",
          "time": "20210902 17:00"
        },
        {
          "id": "MDRW2100028015.1.22",
          "form": "신기하다",
          "original_form": "신기하다",
          "speaker_id": "2",
          "time": "20210902 17:00"
        },
        {
          "id": "MDRW2100028015.1.23",
          "form": "충격적이다",
          "original_form": "충격적이다",
          "speaker_id": "1",
          "time": "20210902 17:00"
        },
        {
          "id": "MDRW2100028015.1.24",
          "form": "근데 더 충격적인 건",
          "original_form": "근데 더 충격적인 건",
          "speaker_id": "1",
          "time": "20210902 17:00"
        },
        {
          "id": "MDRW2100028015.1.25",
          "form": "놓쳤어",
          "original_form": "놓쳤어",
          "speaker_id": "1",
          "time": "20210902 17:00"
        },
        {
          "id": "MDRW2100028015.1.26",
          "form": "이 망할 시스템",
          "original_form": "이 망할 시스템",
          "speaker_id": "1",
          "time": "20210902 17:00"
        },
        {
          "id": "MDRW2100028015.1.27",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210902 17:00"
        },
        {
          "id": "MDRW2100028015.1.28",
          "form": "듣고 싶다고 들을 수 있던 것도 아녓네",
          "original_form": "듣고 싶다고 들을 수 있던 것도 아녓네",
          "speaker_id": "2",
          "time": "20210902 17:00"
        },
        {
          "id": "MDRW2100028015.1.29",
          "form": "그러니까...",
          "original_form": "그러니까...",
          "speaker_id": "1",
          "time": "20210902 17:01"
        },
        {
          "id": "MDRW2100028015.1.30",
          "form": "휴학만이 답인가봐...",
          "original_form": "휴학만이 답인가봐...",
          "speaker_id": "1",
          "time": "20210902 17:01"
        },
        {
          "id": "MDRW2100028015.1.31",
          "form": "망할 시스템",
          "original_form": "망할 시스템",
          "speaker_id": "2",
          "time": "20210902 17:01"
        },
        {
          "id": "MDRW2100028015.1.32",
          "form": "안타깝다",
