        },
        {
          "id": "MDRW2100046542.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20211028 15:00"
        },
        {
          "id": "MDRW2100046542.1.3",
          "form": "나무에서 감 따보신 적 있으세요?",
          "original_form": "나무에서 감 따보신 적 있으세요?",
          "speaker_id": "2",
          "time": "20211028 15:00"
        },
        {
          "id": "MDRW2100046542.1.4",
          "form": "아뇨 없습니다",
          "original_form": "아뇨 없습니다",
          "speaker_id": "1",
          "time": "20211028 15:00"
        },
        {
          "id": "MDRW2100046542.1.5",
          "form": "아아 그러시군요",
          "original_form": "아아 그러시군요",
          "speaker_id": "2",
          "time": "20211028 15:00"
        },
        {
          "id": "MDRW2100046542.1.6",
          "form": "저희 할머니 댁에 감나무가 엄청 많아서",
          "original_form": "저희 할머니 댁에 감나무가 엄청 많아서",
          "speaker_id": "2",
          "time": "20211028 15:01"
        },
        {
          "id": "MDRW2100046542.1.7",
          "form": "이번에 감을 엄청 많이 따왔어요 ㅎㅎ",
          "original_form": "이번에 감을 엄청 많이 따왔어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211028 15:01"
        },
        {
          "id": "MDRW2100046542.1.8",
          "form": "아하 요즘 감 맛있던데 부럽네요",
          "original_form": "아하 요즘 감 맛있던데 부럽네요",
          "speaker_id": "1",
          "time": "20211028 15:01"
        },
        {
          "id": "MDRW2100046542.1.9",
          "form": "감 좋아하세요~?",
          "original_form": "감 좋아하세요~?",
          "speaker_id": "2",
          "time": "20211028 15:01"
        },
        {
          "id": "MDRW2100046542.1.10",
          "form": "네네 요즘 자주 먹어요",
          "original_form": "네네 요즘 자주 먹어요",
          "speaker_id": "1",
          "time": "20211028 15:02"
        },
        {
          "id": "MDRW2100046542.1.11",
          "form": "감은 엄청 다양하게 먹을 수 있는 것 같아요 ㅋㅋㅋ",
          "original_form": "감은 엄청 다양하게 먹을 수 있는 것 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211028 15:02"
        },
        {
          "id": "MDRW2100046542.1.12",
          "form": "맞아요 그리고 다 맛있어요!",
          "original_form": "맞아요 그리고 다 맛있어요!",
          "speaker_id": "1",
          "time": "20211028 15:02"
        },
        {
          "id": "MDRW2100046542.1.13",
          "form": "단감도 있고 홍시도 있고 또 곶감도 있고요",
          "original_form": "단감도 있고 홍시도 있고 또 곶감도 있고요",
          "speaker_id": "2",
          "time": "20211028 15:02"
        },
        {
          "id": "MDRW2100046542.1.14",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20211028 15:02"
        },
        {
          "id": "MDRW2100046542.1.15",
          "form": "뭐 제일 좋아하세요?",
          "original_form": "뭐 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20211028 15:03"
        },
        {
          "id": "MDRW2100046542.1.16",
          "form": "저는 단감이요!",
          "original_form": "저는 단감이요!",
          "speaker_id": "2",
          "time": "20211028 15:03"
        },
        {
          "id": "MDRW2100046542.1.17",
          "form": "와그작 와그작 식감이 좋아요 ㅋㅋ",
          "original_form": "와그작 와그작 식감이 좋아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211028 15:03"
        },
        {
          "id": "MDRW2100046542.1.18",
          "form": "오 저도요! 홍시는 별로 안좋아해요",
          "original_form": "오 저도요! 홍시는 별로 안좋아해요",
          "speaker_id": "1",
          "time": "20211028 15:04"
        },
        {
          "id": "MDRW2100046542.1.19",
          "form": "헉 저도요",
          "original_form": "헉 저도요",
          "speaker_id": "2",
          "time": "20211028 15:04"
        },
        {
          "id": "MDRW2100046542.1.20",
          "form": "물컹물컹해요",
          "original_form": "물컹물컹해요",
          "speaker_id": "2",
          "time": "20211028 15:04"
        },
        {
          "id": "MDRW2100046542.1.21",
          "form": "맞아요ㅋㅋ 얼려먹는건 그나마 나은데 그냥은 잘 안먹게돼요",
          "original_form": "맞아요ㅋㅋ 얼려먹는건 그나마 나은데 그냥은 잘 안먹게돼요",
          "speaker_id": "1",
          "time": "20211028 15:05"
        },
        {
          "id": "MDRW2100046542.1.22",
          "form": "아이스홍시는 맛있죠 ㅋㅋㅋ 그냥 홍시랑 다른 과일 같아요",
          "original_form": "아이스홍시는 맛있죠 ㅋㅋㅋ 그냥 홍시랑 다른 과일 같아요",
