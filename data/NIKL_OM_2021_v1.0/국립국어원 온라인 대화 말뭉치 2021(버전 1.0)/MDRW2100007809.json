      "utterance": [
        {
          "id": "MDRW2100007809.1.1",
          "form": "나도 ㅎㅎ",
          "original_form": "나도 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 10:59"
        },
        {
          "id": "MDRW2100007809.1.2",
          "form": "점심엔 뭐먹지?",
          "original_form": "점심엔 뭐먹지?",
          "speaker_id": "1",
          "time": "20210822 10:59"
        },
        {
          "id": "MDRW2100007809.1.3",
          "form": "냉동실에 닭갈비 있던데 그거 먹을까?",
          "original_form": "냉동실에 닭갈비 있던데 그거 먹을까?",
          "speaker_id": "2",
          "time": "20210822 10:59"
        },
        {
          "id": "MDRW2100007809.1.4",
          "form": "좋지 ㅎㅎ",
          "original_form": "좋지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 10:59"
        },
        {
          "id": "MDRW2100007809.1.5",
          "form": "깻잎도 같이 볶으면 되겠다",
          "original_form": "깻잎도 같이 볶으면 되겠다",
          "speaker_id": "2",
          "time": "20210822 11:00"
        },
        {
          "id": "MDRW2100007809.1.6",
          "form": "고구마도 있어",
          "original_form": "고구마도 있어",
          "speaker_id": "1",
          "time": "20210822 11:00"
        },
        {
          "id": "MDRW2100007809.1.7",
          "form": "오 딱이네",
          "original_form": "오 딱이네",
          "speaker_id": "2",
          "time": "20210822 11:01"
        },
        {
          "id": "MDRW2100007809.1.8",
          "form": "우동사리도 넣을까?",
          "original_form": "우동사리도 넣을까?",
          "speaker_id": "1",
          "time": "20210822 11:01"
        },
        {
          "id": "MDRW2100007809.1.9",
          "form": "좋~지. 완전 맛있겠다 ㅎㅎ",
          "original_form": "좋~지. 완전 맛있겠다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 11:01"
        },
        {
          "id": "MDRW2100007809.1.10",
          "form": "구구닭갈비 진짜 맛있는 것 같아",
          "original_form": "구구닭갈비 진짜 맛있는 것 같아",
          "speaker_id": "1",
          "time": "20210822 11:01"
        },
        {
          "id": "MDRW2100007809.1.11",
          "form": "ㅇㅇ 질기지 않고 부드럽지",
          "original_form": "ㅇㅇ 질기지 않고 부드럽지",
          "speaker_id": "2",
          "time": "20210822 11:01"
        },
        {
          "id": "MDRW2100007809.1.12",
          "form": "양념도 따로 보내주니 편하고",
          "original_form": "양념도 따로 보내주니 편하고",
          "speaker_id": "1",
          "time": "20210822 11:02"
        },
        {
          "id": "MDRW2100007809.1.13",
          "form": "가격도 적당해서 할인할 때 사면 너무 좋은 듯",
          "original_form": "가격도 적당해서 할인할 때 사면 너무 좋은 듯",
          "speaker_id": "2",
          "time": "20210822 11:02"
        },
        {
          "id": "MDRW2100007809.1.14",
          "form": "오늘 저녁메뉴도 미리 정해놔야겠다.",
          "original_form": "오늘 저녁메뉴도 미리 정해놔야겠다.",
