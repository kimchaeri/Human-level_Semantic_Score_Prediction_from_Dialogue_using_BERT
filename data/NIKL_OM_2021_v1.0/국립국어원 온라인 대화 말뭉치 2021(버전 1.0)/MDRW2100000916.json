      "utterance": [
        {
          "id": "MDRW2100000916.1.1",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210819 20:45"
        },
        {
          "id": "MDRW2100000916.1.2",
          "form": "여행 좋아하는데 요즘엔 갈 수 없네요",
          "original_form": "여행 좋아하는데 요즘엔 갈 수 없네요",
          "speaker_id": "2",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000916.1.3",
          "form": "코시국이라 슬프내요 마지막으로간 해외여행지가 어디에요?",
          "original_form": "코시국이라 슬프내요 마지막으로간 해외여행지가 어디에요?",
          "speaker_id": "1",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000916.1.4",
          "form": "코타키나발루요",
          "original_form": "코타키나발루요",
          "speaker_id": "2",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000916.1.5",
          "form": "그게 벌써 2년전이네요 ㅠ",
          "original_form": "그게 벌써 2년전이네요 ㅠ",
          "speaker_id": "2",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000916.1.6",
          "form": "우와아~ 너무 좋았겟내요 이름만들어도 설래요",
          "original_form": "우와아~ 너무 좋았겟내요 이름만들어도 설래요",
          "speaker_id": "1",
          "time": "20210819 20:53"
        },
        {
          "id": "MDRW2100000916.1.7",
          "form": "마지막으로 어디 가보셨어요?",
          "original_form": "마지막으로 어디 가보셨어요?",
          "speaker_id": "2",
          "time": "20210819 20:53"
        },
        {
          "id": "MDRW2100000916.1.8",
          "form": "마지막으로간게 일본 후쿠오카 아니면 태국인데 가물가물하내요ㅋㅋㅋ",
          "original_form": "마지막으로간게 일본 후쿠오카 아니면 태국인데 가물가물하내요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 20:54"
        },
        {
          "id": "MDRW2100000916.1.9",
          "form": "진짜 코로나가 끝나야 어디로든 갈텐데",
          "original_form": "진짜 코로나가 끝나야 어디로든 갈텐데",
          "speaker_id": "2",
          "time": "20210819 20:55"
        },
        {
          "id": "MDRW2100000916.1.10",
          "form": "그 전에 여권 만료되는거 아닌가 몰라요 ㅋㅋ",
          "original_form": "그 전에 여권 만료되는거 아닌가 몰라요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:55"
        },
        {
          "id": "MDRW2100000916.1.11",
          "form": "ㅋㅋㅋ코로나 끝날때쯤엔 여권도 갱신해야겟네요 은근아깝네요",
          "original_form": "ㅋㅋㅋ코로나 끝날때쯤엔 여권도 갱신해야겟네요 은근아깝네요",
          "speaker_id": "1",
          "time": "20210819 20:56"
        },
        {
          "id": "MDRW2100000916.1.12",
          "form": "그러니깐요 아까워요",
          "original_form": "그러니깐요 아까워요",
          "speaker_id": "2",
          "time": "20210819 20:59"
        },
        {
          "id": "MDRW2100000916.1.13",
          "form": "끝나시면 어디가시고 싶으세요?",
          "original_form": "끝나시면 어디가시고 싶으세요?",
          "speaker_id": "1",
          "time": "20210819 21:00"
        },
        {
          "id": "MDRW2100000916.1.14",
          "form": "코로나 끝나면 유럽 가고 싶어요",
          "original_form": "코로나 끝나면 유럽 가고 싶어요",
          "speaker_id": "2",
          "time": "20210819 21:01"
        },
        {
          "id": "MDRW2100000916.1.15",
          "form": "갈 수만 있다면요 ㅎㅎ",
          "original_form": "갈 수만 있다면요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 21:01"
        },
        {
          "id": "MDRW2100000916.1.16",
          "form": "꼭 해외 아니더라도 가까운데서 쉬다오고 싶네요",
          "original_form": "꼭 해외 아니더라도 가까운데서 쉬다오고 싶네요",
          "speaker_id": "2",
          "time": "20210819 21:02"
        },
        {
          "id": "MDRW2100000916.1.17",
          "form": "갈수있을때와 없을때 다르긴하죠ㅠㅠ 가까운곳이라도 가고싶내요",
          "original_form": "갈수있을때와 없을때 다르긴하죠ㅠㅠ 가까운곳이라도 가고싶내요",
          "speaker_id": "1",
          "time": "20210819 21:04"
        },
        {
          "id": "MDRW2100000916.1.18",
          "form": "저는 태국이나 베트남 가고싶어요",
          "original_form": "저는 태국이나 베트남 가고싶어요",
          "speaker_id": "1",
          "time": "20210819 21:04"
        },
        {
          "id": "MDRW2100000916.1.19",
          "form": "좋네요. 어디든 가고 싶어요 이번 휴가 때도 집콕이리 슬프네요.",
          "original_form": "좋네요. 어디든 가고 싶어요 이번 휴가 때도 집콕이리 슬프네요.",
          "speaker_id": "2",
          "time": "20210819 21:05"
        },
        {
          "id": "MDRW2100000916.1.20",
          "form": "휴가는 다녀오셨어요?",
          "original_form": "휴가는 다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210819 21:14"
        },
        {
          "id": "MDRW2100000916.1.21",
          "form": "아니요~ 불안해서 가지는 못했어요ㅠ 괜히 회사에 일생기면 큰일이라서요",
          "original_form": "아니요~ 불안해서 가지는 못했어요ㅠ 괜히 회사에 일생기면 큰일이라서요",
          "speaker_id": "1",
          "time": "20210819 21:15"
        }
      ]
    }
