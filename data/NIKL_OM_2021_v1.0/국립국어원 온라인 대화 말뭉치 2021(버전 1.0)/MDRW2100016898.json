      "utterance": [
        {
          "id": "MDRW2100016898.1.1",
          "form": "오늘 점심 뭐 드셨나요?",
          "original_form": "오늘 점심 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210826 16:07"
        },
        {
          "id": "MDRW2100016898.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 16:08"
        },
        {
          "id": "MDRW2100016898.1.3",
          "form": "직딩이라 식당에서 주는거 먹는데",
          "original_form": "직딩이라 식당에서 주는거 먹는데",
          "speaker_id": "2",
          "time": "20210826 16:09"
        },
        {
          "id": "MDRW2100016898.1.4",
          "form": "오늘은 뼈해장국 나왔어요",
          "original_form": "오늘은 뼈해장국 나왔어요",
          "speaker_id": "2",
          "time": "20210826 16:09"
        },
        {
          "id": "MDRW2100016898.1.5",
          "form": "뭐 드셨나요??",
          "original_form": "뭐 드셨나요??",
          "speaker_id": "2",
          "time": "20210826 16:09"
        },
        {
          "id": "MDRW2100016898.1.6",
          "form": "저는 김치찌개 한그릇 했습니다ㅋㅋㅋ",
          "original_form": "저는 김치찌개 한그릇 했습니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:09"
        },
        {
          "id": "MDRW2100016898.1.7",
          "form": "돼지고기김치찌개인가요",
          "original_form": "돼지고기김치찌개인가요",
          "speaker_id": "2",
          "time": "20210826 16:22"
        },
        {
          "id": "MDRW2100016898.1.8",
          "form": "계란후라이도 하나 있어야하는데",
          "original_form": "계란후라이도 하나 있어야하는데",
          "speaker_id": "2",
          "time": "20210826 16:23"
        },
        {
          "id": "MDRW2100016898.1.9",
          "form": "크으 뭘 좀 아시는군요",
          "original_form": "크으 뭘 좀 아시는군요",
          "speaker_id": "1",
          "time": "20210826 16:24"
        },
        {
          "id": "MDRW2100016898.1.10",
          "form": "아쉽게도 후라이는 없었지만 돼지고기 들어간 김찌 먹었슴다 ㅋㅋㅋ",
          "original_form": "아쉽게도 후라이는 없었지만 돼지고기 들어간 김찌 먹었슴다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:24"
        },
        {
          "id": "MDRW2100016898.1.11",
          "form": "아 아쉽네요 맛집들은 후라이 하나씩 주던데",
          "original_form": "아 아쉽네요 맛집들은 후라이 하나씩 주던데",
          "speaker_id": "2",
          "time": "20210826 16:52"
        },
        {
          "id": "MDRW2100016898.1.12",
          "form": "이시간이면 꼭 커피가 땡기네요",
          "original_form": "이시간이면 꼭 커피가 땡기네요",
          "speaker_id": "2",
          "time": "20210826 16:52"
        },
        {
          "id": "MDRW2100016898.1.13",
          "form": "저도 그래요ㅋㅋ 저는 단 커피를 많이 먹어서 큰일입니다",
          "original_form": "저도 그래요ㅋㅋ 저는 단 커피를 많이 먹어서 큰일입니다",
          "speaker_id": "1",
          "time": "20210826 16:53"
        },
        {
          "id": "MDRW2100016898.1.14",
          "form": "단 커피는 안땡기는데 나이먹으니 슬슬 생각나더라구요?ㅋㅋ",
          "original_form": "단 커피는 안땡기는데 나이먹으니 슬슬 생각나더라구요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:54"
        },
        {
          "id": "MDRW2100016898.1.15",
          "form": "맥심 주로 드시나요?",
          "original_form": "맥심 주로 드시나요?",
          "speaker_id": "2",
          "time": "20210826 16:55"
        },
        {
          "id": "MDRW2100016898.1.16",
          "form": "넼ㅋㅋㅋㅋ 늘 있는 맥심,,,",
          "original_form": "넼ㅋㅋㅋㅋ 늘 있는 맥심,,,{emoji:☆}",
          "speaker_id": "1",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100016898.1.17",
          "form": "ㅋㅋㅋ저도 출근하면 한잔씩 마셨는데",
          "original_form": "ㅋㅋㅋ저도 출근하면 한잔씩 마셨는데",
          "speaker_id": "2",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100016898.1.18",
          "form": "자제하는 중입니다",
          "original_form": "자제하는 중입니다",
          "speaker_id": "2",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100016898.1.19",
          "form": "마시더라도 차를 끄내고",
          "original_form": "마시더라도 차를 끄내고",
          "speaker_id": "2",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100016898.1.20",
          "form": "저도 줄여야 하는데 큰일이네요\n저는 한번에 확 끊어야 진전이 있는 타입이라 이참에 단커피 좀 끊어야겠어요",
          "original_form": "저도 줄여야 하는데 큰일이네요\n저는 한번에 확 끊어야 진전이 있는 타입이라 이참에 단커피 좀 끊어야겠어요",
          "speaker_id": "1",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100016898.1.21",
          "form": "좋아하는 차 있으신가요?",
          "original_form": "좋아하는 차 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100016898.1.22",
          "form": "회사에 메밀차 있는데 이게 속이 편하더라구요?",
          "original_form": "회사에 메밀차 있는데 이게 속이 편하더라구요?",
          "speaker_id": "2",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100016898.1.23",
          "form": "그래서 이거 자주 마시구요",
          "original_form": "그래서 이거 자주 마시구요",
          "speaker_id": "2",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100016898.1.24",
          "form": "집엔 설록차랑 twg? 조금 있어요",
          "original_form": "집엔 설록차랑 twg? 조금 있어요",
