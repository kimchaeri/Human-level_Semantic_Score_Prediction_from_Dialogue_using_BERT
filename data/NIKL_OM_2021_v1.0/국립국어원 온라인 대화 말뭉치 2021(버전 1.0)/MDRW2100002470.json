      "utterance": [
        {
          "id": "MDRW2100002470.1.1",
          "form": "민트초코 좋아하시나요??",
          "original_form": "민트초코 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210820 12:36"
        },
        {
          "id": "MDRW2100002470.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100002470.1.3",
          "form": "네 좋아합니다",
          "original_form": "네 좋아합니다",
          "speaker_id": "2",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100002470.1.4",
          "form": "재프리님은요?",
          "original_form": "재프리님은요?",
          "speaker_id": "2",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100002470.1.5",
          "form": "저도 좋아해요",
          "original_form": "저도 좋아해요",
          "speaker_id": "1",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002470.1.6",
          "form": "민트초코 어디꺼를 좋아하나요?",
          "original_form": "민트초코 어디꺼를 좋아하나요?",
          "speaker_id": "1",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100002470.1.7",
          "form": "민트초코면 그냥 다 좋아요",
          "original_form": "민트초코면 그냥 다 좋아요",
          "speaker_id": "2",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002470.1.8",
          "form": "딱히 브랜드 가리진 않구요",
          "original_form": "딱히 브랜드 가리진 않구요",
          "speaker_id": "2",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002470.1.9",
          "form": "아이스크림을 제일 좋아합니다",
          "original_form": "아이스크림을 제일 좋아합니다",
          "speaker_id": "2",
          "time": "20210820 12:39"
        },
        {
          "id": "MDRW2100002470.1.10",
          "form": "오 아이스크림이 맛있죠",
          "original_form": "오 아이스크림이 맛있죠",
          "speaker_id": "1",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002470.1.11",
          "form": "배라꺼 민트초코 정말 맛있더라구요",
          "original_form": "배라꺼 민트초코 정말 맛있더라구요",
          "speaker_id": "1",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002470.1.12",
          "form": "오 저도",
          "original_form": "오 저도",
          "speaker_id": "2",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002470.1.13",
          "form": "배라꺼가",
          "original_form": "배라꺼가",
          "speaker_id": "2",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002470.1.14",
          "form": "제일좋아요",
          "original_form": "제일좋아요",
          "speaker_id": "2",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002470.1.15",
          "form": "역시 클래식이 최고죠",
          "original_form": "역시 클래식이 최고죠",
          "speaker_id": "2",
          "time": "20210820 12:40"
        },
        {
          "id": "MDRW2100002470.1.16",
          "form": "그쵸 민트초코는 사랑입니다",
          "original_form": "그쵸 민트초코는 사랑입니다",
          "speaker_id": "1",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002470.1.17",
          "form": "저는 스타벅스 민트초코 음료도 좋아해요",
          "original_form": "저는 스타벅스 민트초코 음료도 좋아해요",
          "speaker_id": "1",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002470.1.18",
          "form": "아 전 그거",
          "original_form": "아 전 그거",
          "speaker_id": "2",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002470.1.19",
          "form": "못마셔봤어요",
          "original_form": "못마셔봤어요",
          "speaker_id": "2",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002470.1.20",
          "form": "맛있나요",
          "original_form": "맛있나요",
          "speaker_id": "2",
          "time": "20210820 12:42"
        },
        {
          "id": "MDRW2100002470.1.21",
          "form": "네 맛있어요 추천드려요",
          "original_form": "네 맛있어요 추천드려요",
          "speaker_id": "1",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002470.1.22",
          "form": "공차꺼도 맛있다는데 저도 아직 공차는 ...!",
          "original_form": "공차꺼도 맛있다는데 저도 아직 공차는 ...!",
          "speaker_id": "1",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002470.1.23",
          "form": "공차도 기억하고 있어야겠어요",
          "original_form": "공차도 기억하고 있어야겠어요",
          "speaker_id": "2",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002470.1.24",
          "form": "추천감사해요",
          "original_form": "추천감사해요",
          "speaker_id": "2",
          "time": "20210820 12:43"
        },
        {
          "id": "MDRW2100002470.1.25",
          "form": "민트초코 좋아하는 사람끼리 공유하니까 좋네요",
          "original_form": "민트초코 좋아하는 사람끼리 공유하니까 좋네요",
          "speaker_id": "1",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002470.1.26",
          "form": "입맛에 맞으면 좋겠어요",
          "original_form": "입맛에 맞으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210820 12:44"
        },
        {
          "id": "MDRW2100002470.1.27",
          "form": "민트초코 넘 좋죠~",
          "original_form": "민트초코 넘 좋죠~",
          "speaker_id": "2",
          "time": "20210820 12:45"
        },
        {
          "id": "MDRW2100002470.1.28",
          "form": "호불호갈리지만",
          "original_form": "호불호갈리지만",
          "speaker_id": "2",
          "time": "20210820 12:45"
        },
        {
          "id": "MDRW2100002470.1.29",
          "form": "치약 맛이 난다고 하는데",
          "original_form": "치약 맛이 난다고 하는데",
          "speaker_id": "1",
          "time": "20210820 12:48"
        },
        {
          "id": "MDRW2100002470.1.30",
          "form": "전 못느껴봐서 신기해요",
          "original_form": "전 못느껴봐서 신기해요",
          "speaker_id": "1",
          "time": "20210820 12:48"
        }
      ]
    }
