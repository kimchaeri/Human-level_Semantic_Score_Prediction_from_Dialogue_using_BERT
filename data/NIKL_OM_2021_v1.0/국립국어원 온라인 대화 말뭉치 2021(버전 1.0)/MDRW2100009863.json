      "utterance": [
        {
          "id": "MDRW2100009863.1.1",
          "form": "오전에 너무 조용하니까 이상하다 ㅎㅎ",
          "original_form": "오전에 너무 조용하니까 이상하다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 11:33"
        },
        {
          "id": "MDRW2100009863.1.2",
          "form": "애들 수업 다 들어가서 조용하니까 좋지? ㅎㅎ",
          "original_form": "애들 수업 다 들어가서 조용하니까 좋지? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 11:33"
        },
        {
          "id": "MDRW2100009863.1.3",
          "form": "뭔가 적응이 안되네 ㅎㅎ",
          "original_form": "뭔가 적응이 안되네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 11:34"
        },
        {
          "id": "MDRW2100009863.1.4",
          "form": "그래도 방학보다는 학기 중이 더 편하지 않아?",
          "original_form": "그래도 방학보다는 학기 중이 더 편하지 않아?",
          "speaker_id": "1",
          "time": "20210823 11:34"
        },
        {
          "id": "MDRW2100009863.1.5",
          "form": "그건 아니궁 ㅎㅎ 오전에도 집안일이 잔뜩이니까",
          "original_form": "그건 아니궁 ㅎㅎ 오전에도 집안일이 잔뜩이니까",
          "speaker_id": "2",
          "time": "20210823 11:34"
        },
        {
          "id": "MDRW2100009863.1.6",
          "form": "좀 쉬어",
          "original_form": "좀 쉬어",
          "speaker_id": "1",
          "time": "20210823 11:34"
        },
        {
          "id": "MDRW2100009863.1.7",
          "form": "기본으로 오전에 청소기 돌리고, 빨래 돌리고, 설거지한 다음",
          "original_form": "기본으로 오전에 청소기 돌리고, 빨래 돌리고, 설거지한 다음",
          "speaker_id": "2",
          "time": "20210823 11:35"
        },
        {
          "id": "MDRW2100009863.1.8",
          "form": "훌..",
          "original_form": "훌..",
          "speaker_id": "1",
          "time": "20210823 11:35"
        },
        {
          "id": "MDRW2100009863.1.9",
          "form": "점심준비하면 그냥 12시야 ㅋㅋ",
          "original_form": "점심준비하면 그냥 12시야 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 11:35"
        },
        {
          "id": "MDRW2100009863.1.10",
          "form": "그러네.. 엄청 바쁘겠다",
          "original_form": "그러네.. 엄청 바쁘겠다",
          "speaker_id": "1",
          "time": "20210823 11:35"
        },
        {
          "id": "MDRW2100009863.1.11",
          "form": "집안일은 참 해도 티도 안나고..",
          "original_form": "집안일은 참 해도 티도 안나고..",
          "speaker_id": "2",
          "time": "20210823 11:35"
        },
        {
          "id": "MDRW2100009863.1.12",
          "form": "안하면 확 티나지 ㅋㅋ",
          "original_form": "안하면 확 티나지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 11:35"
        },
        {
          "id": "MDRW2100009863.1.13",
          "form": "ㅋㅋ 그렇지~안하면 엄청 티나",
          "original_form": "ㅋㅋ 그렇지~안하면 엄청 티나",
          "speaker_id": "2",
          "time": "20210823 11:36"
        },
        {
          "id": "MDRW2100009863.1.14",
          "form": "그냥 하지말고 쉬어",
          "original_form": "그냥 하지말고 쉬어",
          "speaker_id": "1",
          "time": "20210823 11:36"
        },
        {
          "id": "MDRW2100009863.1.15",
          "form": "맨날 쉬면 누가 해 ㅠㅠ",
          "original_form": "맨날 쉬면 누가 해 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 11:36"
        },
        {
          "id": "MDRW2100009863.1.16",
          "form": "내가 할께 ㅎㅎ",
          "original_form": "내가 할께 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 11:36"
        },
        {
          "id": "MDRW2100009863.1.17",
          "form": "ㅎㅎ 집에 있는 사람이 해야지.. 그냥 펜션 주인이라고 생각하고 관리하고 있어",
          "original_form": "ㅎㅎ 집에 있는 사람이 해야지.. 그냥 펜션 주인이라고 생각하고 관리하고 있어",
          "speaker_id": "2",
          "time": "20210823 11:36"
        },
        {
          "id": "MDRW2100009863.1.18",
