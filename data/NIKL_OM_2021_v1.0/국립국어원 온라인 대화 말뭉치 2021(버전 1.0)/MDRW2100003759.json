        {
          "id": "MDRW2100003759.1.1",
          "form": "여기 비 엄청 온다",
          "original_form": "여기 비 엄청 온다",
          "speaker_id": "1",
          "time": "20210821 11:56"
        },
        {
          "id": "MDRW2100003759.1.2",
          "form": "여기도 엄청 와",
          "original_form": "여기도 엄청 와",
          "speaker_id": "2",
          "time": "20210821 12:00"
        },
        {
          "id": "MDRW2100003759.1.3",
          "form": "이번주 내내 온다던데",
          "original_form": "이번주 내내 온다던데",
          "speaker_id": "1",
          "time": "20210821 12:01"
        },
        {
          "id": "MDRW2100003759.1.4",
          "form": "장마야?",
          "original_form": "장마야?",
          "speaker_id": "2",
          "time": "20210821 12:01"
        },
        {
          "id": "MDRW2100003759.1.5",
          "form": "그런가봐",
          "original_form": "그런가봐",
          "speaker_id": "1",
          "time": "20210821 12:01"
        },
        {
          "id": "MDRW2100003759.1.6",
          "form": "대신 시원하네",
          "original_form": "대신 시원하네",
          "speaker_id": "2",
          "time": "20210821 12:01"
        },
        {
          "id": "MDRW2100003759.1.7",
          "form": "맞아 어젯밤에는 덥더니",
          "original_form": "맞아 어젯밤에는 덥더니",
          "speaker_id": "1",
          "time": "20210821 12:01"
        },
        {
          "id": "MDRW2100003759.1.8",
          "form": "비오기 전이라 더웠나봐",
          "original_form": "비오기 전이라 더웠나봐",
          "speaker_id": "2",
          "time": "20210821 12:01"
        },
        {
          "id": "MDRW2100003759.1.9",
          "form": "비 좋아해?",
          "original_form": "비 좋아해?",
          "speaker_id": "1",
          "time": "20210821 12:01"
        },
        {
          "id": "MDRW2100003759.1.10",
          "form": "응 좋아해",
          "original_form": "응 좋아해",
          "speaker_id": "2",
          "time": "20210821 12:02"
        },
        {
          "id": "MDRW2100003759.1.11",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210821 12:02"
        },
        {
          "id": "MDRW2100003759.1.12",
          "form": "나는 가끔 오는 비는 좋은데",
          "original_form": "나는 가끔 오는 비는 좋은데",
          "speaker_id": "1",
          "time": "20210821 12:02"
        },
        {
          "id": "MDRW2100003759.1.13",
          "form": "장마는 별로",
          "original_form": "장마는 별로",
          "speaker_id": "1",
          "time": "20210821 12:02"
        },
        {
          "id": "MDRW2100003759.1.14",
          "form": "하긴 장마는 눅눅하니까",
          "original_form": "하긴 장마는 눅눅하니까",
          "speaker_id": "2",
          "time": "20210821 12:02"
        },
        {
          "id": "MDRW2100003759.1.15",
          "form": "계속 비만 오면 우울해",
          "original_form": "계속 비만 오면 우울해",
          "speaker_id": "1",
          "time": "20210821 12:03"
        }
