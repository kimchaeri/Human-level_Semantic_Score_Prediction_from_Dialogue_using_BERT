      "utterance": [
        {
          "id": "MDRW2100042616.1.1",
          "form": "난처했겠네",
          "original_form": "난처했겠네",
          "speaker_id": "2",
          "time": "20210906 23:45"
        },
        {
          "id": "MDRW2100042616.1.2",
          "form": "엉 근데 나보다 더 신경쓰더라",
          "original_form": "엉 근데 나보다 더 신경쓰더라",
          "speaker_id": "1",
          "time": "20210906 23:46"
        },
        {
          "id": "MDRW2100042616.1.3",
          "form": "그래 웃기네",
          "original_form": "그래 웃기네",
          "speaker_id": "2",
          "time": "20210906 23:46"
        },
        {
          "id": "MDRW2100042616.1.4",
          "form": "직장때문에 그런듯",
          "original_form": "직장때문에 그런듯",
          "speaker_id": "1",
          "time": "20210906 23:46"
        },
        {
          "id": "MDRW2100042616.1.5",
          "form": "오랜만에 봤는데 건강하더나",
          "original_form": "오랜만에 봤는데 건강하더나",
          "speaker_id": "2",
          "time": "20210906 23:47"
        },
        {
          "id": "MDRW2100042616.1.6",
          "form": "여름이라 운동을 안해서 다들 살찜",
          "original_form": "여름이라 운동을 안해서 다들 살찜",
          "speaker_id": "1",
          "time": "20210906 23:47"
        },
        {
          "id": "MDRW2100042616.1.7",
          "form": "저런 어쩌노",
          "original_form": "저런 어쩌노",
          "speaker_id": "2",
          "time": "20210906 23:47"
        },
        {
          "id": "MDRW2100042616.1.8",
          "form": "이제 선선해지면은 운동하겠지",
          "original_form": "이제 선선해지면은 운동하겠지",
          "speaker_id": "1",
          "time": "20210906 23:47"
        },
        {
          "id": "MDRW2100042616.1.9",
          "form": "다이어트가 힘든데 살은 안빠지고",
          "original_form": "다이어트가 힘든데 살은 안빠지고",
          "speaker_id": "2",
          "time": "20210906 23:48"
        },
        {
          "id": "MDRW2100042616.1.10",
          "form": "어  다들 뱃살이 심각하더라",
          "original_form": "어  다들 뱃살이 심각하더라",
          "speaker_id": "1",
          "time": "20210906 23:48"
        },
        {
          "id": "MDRW2100042616.1.11",
          "form": "뱃살 저런",
          "original_form": "뱃살 저런",
          "speaker_id": "2",
          "time": "20210906 23:48"
        },
        {
          "id": "MDRW2100042616.1.12",
          "form": "훌라후프 샀다고 하는데 잘 안빠지나봐",
          "original_form": "훌라후프 샀다고 하는데 잘 안빠지나봐",
          "speaker_id": "1",
          "time": "20210906 23:48"
        },
        {
          "id": "MDRW2100042616.1.13",
          "form": "쉽게 살이 빠지는 부위가 아니다",
          "original_form": "쉽게 살이 빠지는 부위가 아니다",
          "speaker_id": "2",
          "time": "20210906 23:49"
        },
        {
          "id": "MDRW2100042616.1.14",
          "form": "그런가봐  팔뚝 살이랑 같이",
          "original_form": "그런가봐  팔뚝 살이랑 같이",
          "speaker_id": "1",
          "time": "20210906 23:49"
        },
        {
          "id": "MDRW2100042616.1.15",
          "form": "나이살이 있어서 더 빼기힘듬",
          "original_form": "나이살이 있어서 더 빼기힘듬",
          "speaker_id": "2",
          "time": "20210906 23:49"
        },
        {
          "id": "MDRW2100042616.1.16",
          "form": "맞다 나두 나잇살이 찌는듯",
          "original_form": "맞다 나두 나잇살이 찌는듯",
          "speaker_id": "1",
          "time": "20210906 23:49"
        },
        {
          "id": "MDRW2100042616.1.17",
          "form": "집에서 운동하잖아",
          "original_form": "집에서 운동하잖아",
          "speaker_id": "2",
          "time": "20210906 23:50"
        },
        {
          "id": "MDRW2100042616.1.18",
          "form": "응 근데 강도가 약하니까 잘 안빠지는듯",
          "original_form": "응 근데 강도가 약하니까 잘 안빠지는듯",
          "speaker_id": "1",
          "time": "20210906 23:50"
        },
        {
          "id": "MDRW2100042616.1.19",
          "form": "너는 안빼도 된다",
          "original_form": "너는 안빼도 된다",
          "speaker_id": "2",
          "time": "20210906 23:50"
        },
        {
          "id": "MDRW2100042616.1.20",
          "form": "나 피부과 가서 점 빼야할듯",
          "original_form": "나 피부과 가서 점 빼야할듯",
          "speaker_id": "1",
          "time": "20210908 02:42"
        }
