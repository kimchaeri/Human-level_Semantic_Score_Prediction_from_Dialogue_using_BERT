      "utterance": [
        {
          "id": "MDRW2100039082.1.1",
          "form": "이번 추석때 가족끼리 모일 수 있겠죠?",
          "original_form": "이번 추석때 가족끼리 모일 수 있겠죠?",
          "speaker_id": "1",
          "time": "20210903 09:33"
        },
        {
          "id": "MDRW2100039082.1.2",
          "form": "이번 추석도 힘들거 같아요",
          "original_form": "이번 추석도 힘들거 같아요",
          "speaker_id": "2",
          "time": "20210903 09:35"
        },
        {
          "id": "MDRW2100039082.1.3",
          "form": "가족 전원이 백신을 맞은 케이스가 적을거 같아서...",
          "original_form": "가족 전원이 백신을 맞은 케이스가 적을거 같아서...",
          "speaker_id": "2",
          "time": "20210903 09:36"
        },
        {
          "id": "MDRW2100039082.1.4",
          "form": "그럴까요? 슬프네요",
          "original_form": "그럴까요? 슬프네요",
          "speaker_id": "1",
          "time": "20210903 09:40"
        },
        {
          "id": "MDRW2100039082.1.5",
          "form": "작년부터 친척들을 못만나서.. 만나고 싶은데",
          "original_form": "작년부터 친척들을 못만나서.. 만나고 싶은데",
          "speaker_id": "1",
          "time": "20210903 09:40"
        },
        {
          "id": "MDRW2100039082.1.6",
          "form": "거리두기가 어느정도나 풀어질지도 모르겠고",
          "original_form": "거리두기가 어느정도나 풀어질지도 모르겠고",
          "speaker_id": "1",
          "time": "20210903 09:40"
        },
        {
          "id": "MDRW2100039082.1.7",
          "form": "사실 백신을 맞아도 전파는 할 수 있어서",
          "original_form": "사실 백신을 맞아도 전파는 할 수 있어서",
          "speaker_id": "2",
          "time": "20210903 09:42"
        },
        {
          "id": "MDRW2100039082.1.8",
          "form": "아무래도 안될거 같아요",
          "original_form": "아무래도 안될거 같아요",
          "speaker_id": "2",
          "time": "20210903 09:42"
        },
        {
          "id": "MDRW2100039082.1.9",
          "form": "올해까지는 자제해야겠네요",
          "original_form": "올해까지는 자제해야겠네요",
          "speaker_id": "1",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100039082.1.10",
          "form": "가족들이 같이 살고 있나요 따로 사나요?",
          "original_form": "가족들이 같이 살고 있나요 따로 사나요?",
          "speaker_id": "1",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100039082.1.11",
          "form": "같이 살고 있어요",
          "original_form": "같이 살고 있어요",
          "speaker_id": "2",
          "time": "20210903 09:50"
        },
        {
          "id": "MDRW2100039082.1.12",
          "form": "가족끼리는 한집이라 괜찮겠네요",
          "original_form": "가족끼리는 한집이라 괜찮겠네요",
          "speaker_id": "1",
          "time": "20210903 09:52"
        },
        {
          "id": "MDRW2100039082.1.13",
          "form": "친척들 못본지 오래됐겠어요",
          "original_form": "친척들 못본지 오래됐겠어요",
          "speaker_id": "1",
          "time": "20210903 09:52"
        },
        {
          "id": "MDRW2100039082.1.14",
          "form": "가끔 따로따로 만났어요",
          "original_form": "가끔 따로따로 만났어요",
          "speaker_id": "2",
          "time": "20210903 09:54"
        },
        {
          "id": "MDRW2100039082.1.15",
          "form": "따로따로.. 슬프네요... 한번에 모여야 재미있는데",
          "original_form": "따로따로.. 슬프네요... 한번에 모여야 재미있는데",
          "speaker_id": "1",
          "time": "20210903 09:56"
        },
        {
          "id": "MDRW2100039082.1.16",
          "form": "저도 집에 친척들 초대를 못해서 아쉬워요",
          "original_form": "저도 집에 친척들 초대를 못해서 아쉬워요",
          "speaker_id": "1",
          "time": "20210903 09:56"
        },
        {
          "id": "MDRW2100039082.1.17",
          "form": "친척끼리 모이는걸 좋아하시나 봐요",
          "original_form": "친척끼리 모이는걸 좋아하시나 봐요",
          "speaker_id": "2",
          "time": "20210903 09:58"
        },
        {
          "id": "MDRW2100039082.1.18",
          "form": "저는 모이면 제가 막내뻘이라",
          "original_form": "저는 모이면 제가 막내뻘이라",
          "speaker_id": "2",
          "time": "20210903 09:58"
        },
        {
          "id": "MDRW2100039082.1.19",
          "form": "여기저기 불려다녀요",
          "original_form": "여기저기 불려다녀요",
          "speaker_id": "2",
          "time": "20210903 09:58"
        },
        {
          "id": "MDRW2100039082.1.20",
          "form": "불러서 집에서 음식 해먹이는걸 좋아해요",
          "original_form": "불러서 집에서 음식 해먹이는걸 좋아해요",
          "speaker_id": "1",
          "time": "20210903 09:59"
        },
        {
          "id": "MDRW2100039082.1.21",
          "form": "밖에서 모이는 것보다요",
          "original_form": "밖에서 모이는 것보다요",
          "speaker_id": "1",
          "time": "20210903 09:59"
        },
        {
          "id": "MDRW2100039082.1.22",
          "form": "근데 뭐 요새는 밖에서 잠깐 보는 게 다라서요",
          "original_form": "근데 뭐 요새는 밖에서 잠깐 보는 게 다라서요",
          "speaker_id": "1",
          "time": "20210903 10:00"
        },
        {
          "id": "MDRW2100039082.1.23",
          "form": "많이 아쉬우시겠어요",
          "original_form": "많이 아쉬우시겠어요",
          "speaker_id": "2",
          "time": "20210903 10:00"
        },
        {
          "id": "MDRW2100039082.1.24",
          "form": "정말 아쉽죠.. 제가 사람들과 어울리는 걸 좋아하는 성격이라서요",
          "original_form": "정말 아쉽죠.. 제가 사람들과 어울리는 걸 좋아하는 성격이라서요",
          "speaker_id": "1",
          "time": "20210903 10:02"
        },
        {
          "id": "MDRW2100039082.1.25",
          "form": "빨리 코로나가 풀렸으면 좋겠네요",
          "original_form": "빨리 코로나가 풀렸으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210903 10:04"
        }
      ]
    }
