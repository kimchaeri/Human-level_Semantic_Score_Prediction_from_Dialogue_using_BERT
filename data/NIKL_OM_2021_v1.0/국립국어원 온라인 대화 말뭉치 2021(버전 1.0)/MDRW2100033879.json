      "utterance": [
        {
          "id": "MDRW2100033879.1.1",
          "form": "셜록은 확실히 아이언맨이 더 낫네요",
          "original_form": "셜록은 확실히 아이언맨이 더 낫네요",
          "speaker_id": "2",
          "time": "20210825 14:16"
        },
        {
          "id": "MDRW2100033879.1.2",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210825 14:16"
        },
        {
          "id": "MDRW2100033879.1.3",
          "form": "살짝 맛이산 연기 ㅋㅋ",
          "original_form": "살짝 맛이산 연기 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:17"
        },
        {
          "id": "MDRW2100033879.1.4",
          "form": "맛이간",
          "original_form": "맛이간",
          "speaker_id": "1",
          "time": "20210825 14:17"
        },
        {
          "id": "MDRW2100033879.1.5",
          "form": "능청스런 연기랑ㅋㅋㅋ",
          "original_form": "능청스런 연기랑ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:17"
        },
        {
          "id": "MDRW2100033879.1.6",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "2",
          "time": "20210825 14:17"
        },
        {
          "id": "MDRW2100033879.1.7",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 14:17"
        },
        {
          "id": "MDRW2100033879.1.8",
          "form": "아이언맨 이미지 강해서 그렇지 연기 잘해요 ㅎㅎ",
          "original_form": "아이언맨 이미지 강해서 그렇지 연기 잘해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 14:17"
        },
        {
          "id": "MDRW2100033879.1.9",
          "form": "그런거 같애요 나이도 쪼매있지만ㅋㅋ",
          "original_form": "그런거 같애요 나이도 쪼매있지만ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:18"
        },
        {
          "id": "MDRW2100033879.1.10",
          "form": "네 아그래도 멋져요",
          "original_form": "네 아그래도 멋져요",
          "speaker_id": "1",
          "time": "20210825 14:18"
        },
        {
          "id": "MDRW2100033879.1.11",
          "form": "근데 저 아이언맨 안한다해서 ㅜㅜ",
          "original_form": "근데 저 아이언맨 안한다해서 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 14:18"
        },
        {
          "id": "MDRW2100033879.1.12",
          "form": "그림자게임?! 그게 제일 최근건가요?",
          "original_form": "그림자게임?! 그게 제일 최근건가요?",
          "speaker_id": "2",
          "time": "20210825 14:18"
        },
        {
          "id": "MDRW2100033879.1.13",
          "form": "이제 어찌볼지 ㅜㅜ",
          "original_form": "이제 어찌볼지 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 14:18"
        },
        {
          "id": "MDRW2100033879.1.14",
          "form": "슬퍼요",
          "original_form": "슬퍼요",
          "speaker_id": "1",
          "time": "20210825 14:18"
        },
        {
          "id": "MDRW2100033879.1.15",
          "form": "길쭉이아저씨 유령신부요",
          "original_form": "길쭉이아저씨 유령신부요",
          "speaker_id": "1",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100033879.1.16",
          "form": "저도 뒤죽박죽 생각이 안나네요 ㅋㅋ",
          "original_form": "저도 뒤죽박죽 생각이 안나네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100033879.1.17",
          "form": "아이언맨은 그림자가 뒤에요",
          "original_form": "아이언맨은 그림자가 뒤에요",
          "speaker_id": "1",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100033879.1.18",
          "form": "아! ㅋㅋ",
          "original_form": "아! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100033879.1.19",
          "form": "웨이브에 셜록치니깐",
          "original_form": "웨이브에 셜록치니깐",
          "speaker_id": "2",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100033879.1.20",
          "form": "저것 밖에 앉나와서요",
          "original_form": "저것 밖에 앉나와서요",
          "speaker_id": "2",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100033879.1.21",
          "form": "오 다있는게 아니라서요",
          "original_form": "오 다있는게 아니라서요",
          "speaker_id": "1",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100033879.1.22",
          "form": "컨텐츠가 맨날변해요",
          "original_form": "컨텐츠가 맨날변해요",
          "speaker_id": "1",
          "time": "20210825 14:22"
        },
        {
          "id": "MDRW2100033879.1.23",
          "form": "그렇네요",
          "original_form": "그렇네요",
          "speaker_id": "2",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100033879.1.24",
          "form": "폰2개로 보는데..",
          "original_form": "폰2개로 보는데..",
          "speaker_id": "2",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100033879.1.25",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 14:23"
        },
        {
          "id": "MDRW2100033879.1.26",
          "form": "한개폰은 오래되서 티빙이 안되더라구요ㅋㅋㅋ",
          "original_form": "한개폰은 오래되서 티빙이 안되더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100033879.1.27",
          "form": "요것도 버전이 좋아야 보여요 ㅋㅋ",
          "original_form": "요것도 버전이 좋아야 보여요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100033879.1.28",
          "form": "본폰으로는 이것저것 딴것도 해야되니 티빙이  진도가 안나가요ㅠㅠ",
          "original_form": "본폰으로는 이것저것 딴것도 해야되니 티빙이  진도가 안나가요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 14:24"
        },
        {
          "id": "MDRW2100033879.1.29",
          "form": "아직  마이시크릿 호텔..다 못봤는데",
          "original_form": "아직  마이시크릿 호텔..다 못봤는데",
