          "speaker_id": "2",
          "time": "20210822 13:11"
        },
        {
          "id": "MDRW2100008064.1.5",
          "form": "언제요?",
          "original_form": "언제요?",
          "speaker_id": "1",
          "time": "20210822 13:13"
        },
        {
          "id": "MDRW2100008064.1.6",
          "form": "구월이요",
          "original_form": "구월이요",
          "speaker_id": "2",
          "time": "20210822 13:56"
        },
        {
          "id": "MDRW2100008064.1.7",
          "form": "부작용걱정안되시나요?",
          "original_form": "부작용걱정안되시나요?",
          "speaker_id": "1",
          "time": "20210822 13:56"
        },
        {
          "id": "MDRW2100008064.1.8",
          "form": "맞으실거예요?",
          "original_form": "맞으실거예요?",
          "speaker_id": "1",
          "time": "20210822 13:56"
        },
        {
          "id": "MDRW2100008064.1.9",
          "form": "네 맞을랴구뇨",
          "original_form": "네 맞을랴구뇨",
          "speaker_id": "2",
          "time": "20210822 14:12"
        },
        {
          "id": "MDRW2100008064.1.10",
          "form": "주변에 아프거나 부작용있으신분 없나요?",
          "original_form": "주변에 아프거나 부작용있으신분 없나요?",
