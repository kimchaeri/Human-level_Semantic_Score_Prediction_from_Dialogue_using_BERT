          "time": "20210822 14:13"
        },
        {
          "id": "MDRW2100008064.1.11",
          "form": "2차는 다 아팠뎅6ㅛ",
          "original_form": "2차는 다 아팠뎅6ㅛ",
          "speaker_id": "2",
          "time": "20210822 14:14"
        },
        {
          "id": "MDRW2100008064.1.12",
          "form": "화이자인가요?",
          "original_form": "화이자인가요?",
          "speaker_id": "1",
          "time": "20210822 14:15"
        },
        {
          "id": "MDRW2100008064.1.13",
          "form": "모더나 둘중 아직 미정이요",
          "original_form": "모더나 둘중 아직 미정이요",
          "speaker_id": "2",
