      "utterance": [
        {
          "id": "MDRW2100043639.1.1",
          "form": "호프집에서 계란말이 판다구",
          "original_form": "호프집에서 계란말이 판다구",
          "speaker_id": "2",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043639.1.2",
          "form": "정말인거야?",
          "original_form": "정말인거야?",
          "speaker_id": "1",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043639.1.3",
          "form": "근데 비싸",
          "original_form": "근데 비싸",
          "speaker_id": "2",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043639.1.4",
          "form": "계란말이인데??",
          "original_form": "계란말이인데??",
          "speaker_id": "1",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100043639.1.5",
          "form": "한 8천원정도해",
          "original_form": "한 8천원정도해",
          "speaker_id": "2",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043639.1.6",
          "form": "헥",
          "original_form": "헥",
          "speaker_id": "1",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043639.1.7",
          "form": "이건좀 아니다 싶지?",
          "original_form": "이건좀 아니다 싶지?",
          "speaker_id": "2",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043639.1.8",
          "form": "진짠데...?",
          "original_form": "진짠데...?",
          "speaker_id": "1",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043639.1.9",
          "form": "넘 비싸 근데또 부드럽다",
          "original_form": "넘 비싸 근데또 부드럽다",
          "speaker_id": "2",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043639.1.10",
          "form": "계란찜같애",
          "original_form": "계란찜같애",
          "speaker_id": "1",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043639.1.11",
          "form": "??",
          "original_form": "??",
          "speaker_id": "1",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043639.1.12",
          "form": "어 비슷해!",
          "original_form": "어 비슷해!",
          "speaker_id": "2",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043639.1.13",
          "form": "헐 그럼 맛잇겟다리",
          "original_form": "헐 그럼 맛잇겟다리",
          "speaker_id": "1",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043639.1.14",
          "form": "하지만 나라면 걍 묵는다",
          "original_form": "하지만 나라면 걍 묵는다",
          "speaker_id": "2",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043639.1.15",
          "form": "만들어서?",
          "original_form": "만들어서?",
          "speaker_id": "1",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100043639.1.16",
          "form": "당연히 숯불닭갈비지",
          "original_form": "당연히 숯불닭갈비지",
          "speaker_id": "1",
