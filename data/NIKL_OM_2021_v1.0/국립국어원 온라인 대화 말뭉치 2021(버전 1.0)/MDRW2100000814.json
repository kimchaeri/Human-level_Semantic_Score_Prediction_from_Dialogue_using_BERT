        {
          "id": "MDRW2100000814.1.1",
          "form": "싸이월드 하셨어요?",
          "original_form": "싸이월드 하셨어요?",
          "speaker_id": "1",
          "time": "20210819 19:39"
        },
        {
          "id": "MDRW2100000814.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210819 19:40"
        },
        {
          "id": "MDRW2100000814.1.3",
          "form": "싸이월드 이번에 다시 부활한다는데 어떠세요?",
          "original_form": "싸이월드 이번에 다시 부활한다는데 어떠세요?",
          "speaker_id": "1",
          "time": "20210819 19:41"
        },
        {
          "id": "MDRW2100000814.1.4",
          "form": "재미있을거 같아요",
          "original_form": "재미있을거 같아요",
          "speaker_id": "2",
          "time": "20210819 19:41"
        },
        {
          "id": "MDRW2100000814.1.5",
          "form": "전 좋으면서 약간 별로에요",
          "original_form": "전 좋으면서 약간 별로에요",
          "speaker_id": "1",
          "time": "20210819 19:43"
        },
        {
          "id": "MDRW2100000814.1.6",
          "form": "추억은 그냥 추억으로 남기고 싶은 느낌도 있구요",
          "original_form": "추억은 그냥 추억으로 남기고 싶은 느낌도 있구요",
          "speaker_id": "1",
          "time": "20210819 19:43"
        },
        {
          "id": "MDRW2100000814.1.7",
          "form": "ㅎ 그렇죠",
          "original_form": "ㅎ 그렇죠",
          "speaker_id": "2",
          "time": "20210819 19:44"
        },
        {
          "id": "MDRW2100000814.1.8",
          "form": "싸이월드 부활하면 하실거에요? ",
          "original_form": "싸이월드 부활하면 하실거에요? ",
          "speaker_id": "1",
          "time": "20210819 19:47"
        },
        {
          "id": "MDRW2100000814.1.9",
          "form": "안할 것 같아요",
          "original_form": "안할 것 같아요",
          "speaker_id": "2",
          "time": "20210819 19:50"
        },
        {
          "id": "MDRW2100000814.1.10",
          "form": "안하는 이유가 뭔가요?",
          "original_form": "안하는 이유가 뭔가요?",
          "speaker_id": "1",
          "time": "20210819 19:51"
        },
        {
          "id": "MDRW2100000814.1.11",
          "form": "저도 안할 거 같긴한데!!",
          "original_form": "저도 안할 거 같긴한데!!",
          "speaker_id": "1",
          "time": "20210819 19:51"
        },
        {
          "id": "MDRW2100000814.1.12",
          "form": "카톡이 더 좋아요",
          "original_form": "카톡이 더 좋아요",
          "speaker_id": "2",
          "time": "20210819 19:51"
        },
        {
          "id": "MDRW2100000814.1.13",
          "form": "오 저두요 ~ 어릴 때니까 할 수 있었던 거 같아요",
          "original_form": "오 저두요 ~ 어릴 때니까 할 수 있었던 거 같아요",
          "speaker_id": "1",
          "time": "20210819 19:53"
        },
        {
          "id": "MDRW2100000814.1.14",
          "form": "지금은 귀찮아서도 못할 거 같고",
          "original_form": "지금은 귀찮아서도 못할 거 같고",
          "speaker_id": "1",
          "time": "20210819 19:53"
        },
        {
          "id": "MDRW2100000814.1.15",
          "form": "그래도 싸이가 다시 부활 한다니 신기하네요",
          "original_form": "그래도 싸이가 다시 부활 한다니 신기하네요",
          "speaker_id": "1",
          "time": "20210819 19:54"
        },
        {
          "id": "MDRW2100000814.1.16",
          "form": "그르네여",
          "original_form": "그르네여",
          "speaker_id": "2",
          "time": "20210819 19:54"
        },
        {
          "id": "MDRW2100000814.1.17",
          "form": "얼마나 더 흥하게 될지 궁금해여",
          "original_form": "얼마나 더 흥하게 될지 궁금해여",
