      "utterance": [
        {
          "id": "MDRW2100012253.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 20:43"
        },
        {
          "id": "MDRW2100012253.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210824 20:43"
        },
        {
          "id": "MDRW2100012253.1.3",
          "form": "오늘 태풍 온단 예보는 들었는데 사시는 곳엔 피해가 없었나요?",
          "original_form": "오늘 태풍 온단 예보는 들었는데 사시는 곳엔 피해가 없었나요?",
          "speaker_id": "2",
          "time": "20210824 20:44"
        },
        {
          "id": "MDRW2100012253.1.4",
          "form": "서울은 비만조금 왔어요!",
          "original_form": "서울은 비만조금 왔어요!",
          "speaker_id": "1",
          "time": "20210824 20:47"
        },
        {
          "id": "MDRW2100012253.1.5",
          "form": "인천도 비슷했어요!",
          "original_form": "인천도 비슷했어요!",
          "speaker_id": "2",
          "time": "20210824 20:48"
        },
        {
          "id": "MDRW2100012253.1.6",
          "form": "태풍이라서 비가 엄청 오고 바람이 불 줄 알았는데 편하게 지나갔네요ㅎㅎ",
          "original_form": "태풍이라서 비가 엄청 오고 바람이 불 줄 알았는데 편하게 지나갔네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 20:48"
        },
        {
          "id": "MDRW2100012253.1.7",
          "form": "경남지방에 비가많이왔다고 들었어요ㅠ",
          "original_form": "경남지방에 비가많이왔다고 들었어요ㅠ",
          "speaker_id": "1",
          "time": "20210824 20:49"
        },
        {
          "id": "MDRW2100012253.1.8",
          "form": "저런ㅠㅠ 뉴스가 수도권 위주로만 나오다보니",
          "original_form": "저런ㅠㅠ 뉴스가 수도권 위주로만 나오다보니",
          "speaker_id": "2",
          "time": "20210824 20:49"
        },
        {
          "id": "MDRW2100012253.1.9",
          "form": "다른 지역 소식엔 어두웠네요;",
          "original_form": "다른 지역 소식엔 어두웠네요;",
          "speaker_id": "2",
          "time": "20210824 20:50"
        },
        {
          "id": "MDRW2100012253.1.10",
          "form": "올해엔 근데 태풍이 처음인것같네요",
          "original_form": "올해엔 근데 태풍이 처음인것같네요",
          "speaker_id": "1",
          "time": "20210824 20:50"
        },
        {
          "id": "MDRW2100012253.1.11",
          "form": "헉 그런가요??",
          "original_form": "헉 그런가요??",
          "speaker_id": "2",
          "time": "20210824 20:50"
        },
        {
          "id": "MDRW2100012253.1.12",
          "form": "장마도 온듯 만듯 지나갔는데",
          "original_form": "장마도 온듯 만듯 지나갔는데",
          "speaker_id": "2",
          "time": "20210824 20:51"
        },
        {
          "id": "MDRW2100012253.1.13",
          "form": "그러게요 여름은 너무 더웠어요",
          "original_form": "그러게요 여름은 너무 더웠어요",
          "speaker_id": "1",
          "time": "20210824 20:51"
        },
        {
          "id": "MDRW2100012253.1.14",
          "form": "맞아요ㅠㅠ 에어컨을 끈 적이 없어요ㅋㅋ",
          "original_form": "맞아요ㅠㅠ 에어컨을 끈 적이 없어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 20:51"
        },
        {
          "id": "MDRW2100012253.1.15",
          "form": "근데 벌써 서늘해졌네요",
          "original_form": "근데 벌써 서늘해졌네요",
          "speaker_id": "1",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100012253.1.16",
          "form": "맞아요 요즘엔 새벽에 꽤 선선해진 것 같아요!",
          "original_form": "맞아요 요즘엔 새벽에 꽤 선선해진 것 같아요!",
          "speaker_id": "2",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100012253.1.17",
          "form": "금세 겨울이 또 올것같아요",
          "original_form": "금세 겨울이 또 올것같아요",
          "speaker_id": "1",
          "time": "20210824 20:53"
        }
      ]
