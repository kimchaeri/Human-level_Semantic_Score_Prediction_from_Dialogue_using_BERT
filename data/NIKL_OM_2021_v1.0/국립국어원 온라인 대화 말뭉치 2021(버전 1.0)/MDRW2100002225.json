      "utterance": [
        {
          "id": "MDRW2100002225.1.1",
          "form": "너는 콜라 좋아하니",
          "original_form": "너는 콜라 좋아하니",
          "speaker_id": "2",
          "time": "20210820 00:50"
        },
        {
          "id": "MDRW2100002225.1.2",
          "form": "나는 콜라는 많이 좋아하지않아.",
          "original_form": "나는 콜라는 많이 좋아하지않아.",
          "speaker_id": "2",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002225.1.3",
          "form": "콜라 그저 그렇지",
          "original_form": "콜라 그저 그렇지",
          "speaker_id": "1",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002225.1.4",
          "form": "근데 좋아하는 애들은",
          "original_form": "근데 좋아하는 애들은",
          "speaker_id": "1",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002225.1.5",
          "form": "60개씩 쌓아두고 먹더라",
          "original_form": "60개씩 쌓아두고 먹더라",
          "speaker_id": "1",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002225.1.6",
          "form": "맞아. 콜라 사먹는게 너무 신기해 나는",
          "original_form": "맞아. 콜라 사먹는게 너무 신기해 나는",
          "speaker_id": "2",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002225.1.7",
          "form": "콜라같은거 먹으면 뼈삭는 느낌이야",
          "original_form": "콜라같은거 먹으면 뼈삭는 느낌이야",
          "speaker_id": "1",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002225.1.8",
          "form": "나는 그냥 햄버거 먹을 때 목막히니까 세트로 주문하는 정도야.",
          "original_form": "나는 그냥 햄버거 먹을 때 목막히니까 세트로 주문하는 정도야.",
          "speaker_id": "2",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002225.1.9",
          "form": "맞아.이빨은 소중해",
          "original_form": "맞아.이빨은 소중해",
          "speaker_id": "2",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002225.1.10",
          "form": "그리고 너무 달아.",
          "original_form": "그리고 너무 달아.",
          "speaker_id": "2",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002225.1.11",
          "form": "맞아 그거도 다 먹지도못해",
          "original_form": "맞아 그거도 다 먹지도못해",
          "speaker_id": "1",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002225.1.12",
          "form": "맞아.",
          "original_form": "맞아.",
          "speaker_id": "2",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002225.1.13",
          "form": "하지만 에이드는 좋아해",
          "original_form": "하지만 에이드는 좋아해",
          "speaker_id": "2",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002225.1.14",
          "form": "콜라보다는 사이다가 좋은 것 같아",
          "original_form": "콜라보다는 사이다가 좋은 것 같아",
          "speaker_id": "1",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002225.1.15",
          "form": "아그리고 나는 영화관에서는 콜라먹어",
          "original_form": "아그리고 나는 영화관에서는 콜라먹어",
          "speaker_id": "1",
          "time": "20210820 00:52"
        },
        {
          "id": "MDRW2100002225.1.16",
          "form": "에이드는 아무래도 콜라가 아니니까",
          "original_form": "에이드는 아무래도 콜라가 아니니까",
          "speaker_id": "2",
          "time": "20210820 00:52"
        },
        {
          "id": "MDRW2100002225.1.17",
          "form": "물론 다 먹지는 못해",
          "original_form": "물론 다 먹지는 못해",
          "speaker_id": "1",
          "time": "20210820 00:52"
        },
        {
          "id": "MDRW2100002225.1.18",
          "form": "에이드좋지",
          "original_form": "에이드좋지",
          "speaker_id": "1",
          "time": "20210820 00:52"
        },
        {
          "id": "MDRW2100002225.1.19",
          "form": "맞아 그 색깔때문인가? 나는 사이다가 이가 덜 썩는 느낌이야",
          "original_form": "맞아 그 색깔때문인가? 나는 사이다가 이가 덜 썩는 느낌이야",
          "speaker_id": "2",
          "time": "20210820 00:52"
        },
        {
          "id": "MDRW2100002225.1.20",
          "form": "이가 덜 누래지는 느낌이야.",
          "original_form": "이가 덜 누래지는 느낌이야.",
          "speaker_id": "2",
          "time": "20210820 00:52"
        },
        {
          "id": "MDRW2100002225.1.21",
          "form": "어떤 에이드 좋아하니 나는 망고가 좋아",
          "original_form": "어떤 에이드 좋아하니 나는 망고가 좋아",
          "speaker_id": "1",
          "time": "20210820 00:52"
        },
        {
          "id": "MDRW2100002225.1.22",
          "form": "맞앜ㅋㅋㅋㅋㅋ다들 똑같구나",
          "original_form": "맞앜ㅋㅋㅋㅋㅋ다들 똑같구나",
          "speaker_id": "1",
          "time": "20210820 00:52"
        },
        {
          "id": "MDRW2100002225.1.23",
          "form": "망고에이드?",
          "original_form": "망고에이드?",
          "speaker_id": "2",
          "time": "20210820 00:52"
        },
        {
          "id": "MDRW2100002225.1.24",
          "form": "응 맛있어",
          "original_form": "응 맛있어",
          "speaker_id": "1",
          "time": "20210820 00:52"
        },
        {
          "id": "MDRW2100002225.1.25",
          "form": "에이드는 자몽이란다.",
          "original_form": "에이드는 자몽이란다.",
          "speaker_id": "2",
          "time": "20210820 00:52"
        },
        {
          "id": "MDRW2100002225.1.26",
          "form": "자몽은 너무 시큼해",
          "original_form": "자몽은 너무 시큼해",
          "speaker_id": "1",
          "time": "20210820 00:52"
        },
        {
          "id": "MDRW2100002225.1.27",
          "form": "청포도나 블루베리도 괜찮아",
          "original_form": "청포도나 블루베리도 괜찮아",
          "speaker_id": "2",
          "time": "20210820 00:52"
        },
        {
          "id": "MDRW2100002225.1.28",
          "form": "망고는 아니야",
          "original_form": "망고는 아니야",
          "speaker_id": "2",
          "time": "20210820 00:52"
        },
        {
          "id": "MDRW2100002225.1.29",
          "form": "자몽 시큼하지 않아.",
          "original_form": "자몽 시큼하지 않아.",
          "speaker_id": "2",
          "time": "20210820 00:52"
        }
      ]
    }
