      "utterance": [
        {
          "id": "MDRW2100047284.1.1",
          "form": "중고거래 경험 있으세요?",
          "original_form": "중고거래 경험 있으세요?",
          "speaker_id": "2",
          "time": "20210826 03:18"
        },
        {
          "id": "MDRW2100047284.1.2",
          "form": "네 있어요 ㅋㅋ중고로 미니에그팬 사봤어요ㅋㅋㅋ",
          "original_form": "네 있어요 ㅋㅋ중고로 미니에그팬 사봤어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 03:20"
        },
        {
          "id": "MDRW2100047284.1.3",
          "form": "대면거래 하셨어요?",
          "original_form": "대면거래 하셨어요?",
          "speaker_id": "2",
          "time": "20210826 03:22"
        },
        {
          "id": "MDRW2100047284.1.4",
          "form": "아님 택배? ㅋㅋ",
          "original_form": "아님 택배? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 03:22"
        },
        {
          "id": "MDRW2100047284.1.5",
          "form": "대면으뤂당근거래요~",
          "original_form": "대면으뤂당근거래요~",
          "speaker_id": "1",
          "time": "20210826 05:23"
        },
        {
          "id": "MDRW2100047284.1.6",
          "form": "부끄부끄했어요 ㅋㅋ",
          "original_form": "부끄부끄했어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 05:24"
        },
        {
          "id": "MDRW2100047284.1.7",
          "form": "앗 대면으로 하셨었군용!",
          "original_form": "앗 대면으로 하셨었군용!",
          "speaker_id": "2",
          "time": "20210826 08:53"
        },
        {
          "id": "MDRW2100047284.1.8",
          "form": "저도 첫 대면거래는\r\n떨리고 부끄부끄했던 기억이ㅋㅋ",
          "original_form": "저도 첫 대면거래는\r\n떨리고 부끄부끄했던 기억이ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 08:58"
        },
        {
          "id": "MDRW2100047284.1.9",
          "form": "ㅋㅋㅋ그러셨구나~",
          "original_form": "ㅋㅋㅋ그러셨구나~",
          "speaker_id": "1",
          "time": "20210826 14:50"
        },
        {
          "id": "MDRW2100047284.1.10",
          "form": "어떤물건이었나요??",
          "original_form": "어떤물건이었나요??",
          "speaker_id": "1",
          "time": "20210826 14:50"
        },
        {
          "id": "MDRW2100047284.1.11",
          "form": "홈쇼핑에서 샀던\r\n클렌징 제품이었던거 같은데",
          "original_form": "홈쇼핑에서 샀던\r\n클렌징 제품이었던거 같은데",
          "speaker_id": "2",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100047284.1.12",
          "form": "넘 많아서 일부 판매하려했는데",
          "original_form": "넘 많아서 일부 판매하려했는데",
          "speaker_id": "2",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100047284.1.13",
          "form": "제 또래 남자가\r\n엄마 드린다고 사러 왔었어요 ㅋㅋ",
          "original_form": "제 또래 남자가\r\n엄마 드린다고 사러 왔었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100047284.1.14",
          "form": "오 우와! 그분 효심이 대박인걸요?",
          "original_form": "오 우와! 그분 효심이 대박인걸요?",
          "speaker_id": "1",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100047284.1.15",
          "form": "홈쇼핑 또 한번보면 저도모르게 뭔갈 사게되더라구요 햐",
          "original_form": "홈쇼핑 또 한번보면 저도모르게 뭔갈 사게되더라구요 햐",
          "speaker_id": "1",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100047284.1.16",
          "form": "그쵸ㅋㅋ 호스트분들\r\n정말 대단하신거 같아요",
          "original_form": "그쵸ㅋㅋ 호스트분들\r\n정말 대단하신거 같아요",
          "speaker_id": "2",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100047284.1.17",
          "form": "필요하지도 갖고 싶지도 않았던건데\r\n보고 있음 나한테 꼭 필요한거같고ㅋ",
          "original_form": "필요하지도 갖고 싶지도 않았던건데\r\n보고 있음 나한테 꼭 필요한거같고ㅋ",
          "speaker_id": "2",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100047284.1.18",
          "form": "지금 안사면 안될거같은ㅋㅋ",
          "original_form": "지금 안사면 안될거같은ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100047284.1.19",
          "form": "맞아요 호스트분들 입담이 장난아니죠!!",
          "original_form": "맞아요 호스트분들 입담이 장난아니죠!!",
          "speaker_id": "1",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100047284.1.20",
          "form": "홈쇼핑한번봤다하면 탕진해요ㅜ",
          "original_form": "홈쇼핑한번봤다하면 탕진해요ㅜ",
          "speaker_id": "1",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100047284.1.21",
          "form": "ㅋㅋ맞아요ㅋㅋ \r\n그 분들은 진짜 타고 나신듯요",
          "original_form": "ㅋㅋ맞아요ㅋㅋ \r\n그 분들은 진짜 타고 나신듯요",
          "speaker_id": "2",
          "time": "20210826 15:28"
        },
        {
          "id": "MDRW2100047284.1.22",
          "form": "그쵸 장난아니시죠~",
          "original_form": "그쵸 장난아니시죠~",
          "speaker_id": "1",
          "time": "20210826 15:31"
        }
      ]
    }
