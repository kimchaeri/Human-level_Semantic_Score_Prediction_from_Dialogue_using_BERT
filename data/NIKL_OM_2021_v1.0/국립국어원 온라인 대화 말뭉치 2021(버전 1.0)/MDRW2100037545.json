      "utterance": [
        {
          "id": "MDRW2100037545.1.1",
          "form": "토토로요!",
          "original_form": "토토로요!",
          "speaker_id": "2",
          "time": "20210901 10:00"
        },
        {
          "id": "MDRW2100037545.1.2",
          "form": "너무 좋아서 상품 쓸어모으고 싶어요",
          "original_form": "너무 좋아서 상품 쓸어모으고 싶어요",
          "speaker_id": "2",
          "time": "20210901 10:00"
        },
        {
          "id": "MDRW2100037545.1.3",
          "form": "토토로! 저 일본갔을때 토토로 샵 몇번갔었는데",
          "original_form": "토토로! 저 일본갔을때 토토로 샵 몇번갔었는데",
          "speaker_id": "1",
          "time": "20210901 10:31"
        },
        {
          "id": "MDRW2100037545.1.4",
          "form": "name2님이 거기가시면 너무 행복해하실거 같아요",
          "original_form": "&name2&님이 거기가시면 너무 행복해하실거 같아요",
          "speaker_id": "1",
          "time": "20210901 10:31"
        },
        {
          "id": "MDRW2100037545.1.5",
          "form": "오오! 저 전에 갔을때 터질만큼 담아왔었어요!!",
          "original_form": "오오! 저 전에 갔을때 터질만큼 담아왔었어요!!",
          "speaker_id": "2",
          "time": "20210901 12:01"
        },
        {
          "id": "MDRW2100037545.1.6",
          "form": "센치로 말고 좋아하시는거 있으세요?",
          "original_form": "센치로 말고 좋아하시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210901 12:01"
        },
        {
          "id": "MDRW2100037545.1.7",
          "form": "원령공주요!",
          "original_form": "원령공주요!",
          "speaker_id": "1",
          "time": "20210901 12:03"
        },
        {
          "id": "MDRW2100037545.1.8",
          "form": "오오 그것도 좋죠~~",
          "original_form": "오오 그것도 좋죠~~",
          "speaker_id": "2",
          "time": "20210901 12:09"
        },
        {
          "id": "MDRW2100037545.1.9",
          "form": "어릴때 몇번이나 봤던 기억 나네요",
          "original_form": "어릴때 몇번이나 봤던 기억 나네요",
          "speaker_id": "2",
          "time": "20210901 12:09"
        },
        {
          "id": "MDRW2100037545.1.10",
          "form": "자연과 사람이 주제인점도 좋았어요",
          "original_form": "자연과 사람이 주제인점도 좋았어요",
          "speaker_id": "1",
          "time": "20210901 12:15"
        },
        {
          "id": "MDRW2100037545.1.11",
          "form": "지브리치곤 조금 고어? 하긴했지만요",
          "original_form": "지브리치곤 조금 고어? 하긴했지만요",
          "speaker_id": "1",
          "time": "20210901 12:15"
        },
        {
          "id": "MDRW2100037545.1.12",
          "form": "맞아요ㅋㅋ 그래도 진짜 안좋은게 없어요",
          "original_form": "맞아요ㅋㅋ 그래도 진짜 안좋은게 없어요",
          "speaker_id": "2",
          "time": "20210901 12:23"
        },
        {
          "id": "MDRW2100037545.1.13",
          "form": "예전에 다찾아서 봤었는데 지금은 기억이 잘안나요",
          "original_form": "예전에 다찾아서 봤었는데 지금은 기억이 잘안나요",
          "speaker_id": "1",
          "time": "20210901 12:36"
        },
        {
          "id": "MDRW2100037545.1.14",
          "form": "ost 가 하나같이 다 좋아서",
          "original_form": "ost 가 하나같이 다 좋아서",
          "speaker_id": "1",
          "time": "20210901 12:37"
        },
        {
          "id": "MDRW2100037545.1.15",
          "form": "한번 직접 오케스트라 연주 들어보고 싶어요",
          "original_form": "한번 직접 오케스트라 연주 들어보고 싶어요",
          "speaker_id": "1",
          "time": "20210901 12:37"
        },
        {
          "id": "MDRW2100037545.1.16",
          "form": "저두요!",
          "original_form": "저두요!",
          "speaker_id": "2",
          "time": "20210901 12:46"
        },
        {
          "id": "MDRW2100037545.1.17",
          "form": "직접 들어보고 싶어오 진짜 감동적일텐데",
          "original_form": "직접 들어보고 싶어오 진짜 감동적일텐데",
          "speaker_id": "2",
          "time": "20210901 12:47"
        },
        {
          "id": "MDRW2100037545.1.18",
          "form": "그죠 여기서 대화하시던분이 저번에 직접들으셨다는데 너무 부러웠어요",
          "original_form": "그죠 여기서 대화하시던분이 저번에 직접들으셨다는데 너무 부러웠어요",
          "speaker_id": "1",
          "time": "20210901 12:48"
        },
        {
          "id": "MDRW2100037545.1.19",
          "form": "표값이 비싸긴해도 꼭가보고싶네요",
          "original_form": "표값이 비싸긴해도 꼭가보고싶네요",
          "speaker_id": "1",
          "time": "20210901 12:48"
        },
        {
          "id": "MDRW2100037545.1.20",
          "form": "오아.. 어디서 들으셨대요",
          "original_form": "오아.. 어디서 들으셨대요",
          "speaker_id": "2",
          "time": "20210901 12:53"
        },
        {
          "id": "MDRW2100037545.1.21",
          "form": "진짜 가고싶네요",
          "original_form": "진짜 가고싶네요",
          "speaker_id": "2",
          "time": "20210901 12:53"
        },
        {
          "id": "MDRW2100037545.1.22",
          "form": "한국에서도 한대요?",
          "original_form": "한국에서도 한대요?",
          "speaker_id": "2",
          "time": "20210901 12:54"
        },
        {
          "id": "MDRW2100037545.1.23",
          "form": "히사이시조 내한때 가셨대요",
          "original_form": "히사이시조 내한때 가셨대요",
          "speaker_id": "1",
          "time": "20210901 13:53"
        },
        {
          "id": "MDRW2100037545.1.24",
          "form": "아아 부럽네요",
          "original_form": "아아 부럽네요",
          "speaker_id": "2",
          "time": "20210901 13:59"
        },
        {
          "id": "MDRW2100037545.1.25",
          "form": "그때 돈이 없어서 못갔었던때인데.. 한국에서 한줄알았어요",
          "original_form": "그때 돈이 없어서 못갔었던때인데.. 한국에서 한줄알았어요",
          "speaker_id": "2",
