      "utterance": [
        {
          "id": "MDRW2100008749.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210822 18:01"
        },
        {
          "id": "MDRW2100008749.1.2",
          "form": "그럼요 설거지가 제일 힘들어요! 매끼니마다 하니",
          "original_form": "그럼요 설거지가 제일 힘들어요! 매끼니마다 하니",
          "speaker_id": "2",
          "time": "20210822 18:13"
        },
        {
          "id": "MDRW2100008749.1.3",
          "form": "전 화장실 청소요",
          "original_form": "전 화장실 청소요",
          "speaker_id": "1",
          "time": "20210822 18:52"
        },
        {
          "id": "MDRW2100008749.1.4",
          "form": "아ㅋㅋㅋ그것도 정말 싫어요!",
          "original_form": "아ㅋㅋㅋ그것도 정말 싫어요!",
          "speaker_id": "2",
          "time": "20210822 18:55"
        },
        {
          "id": "MDRW2100008749.1.5",
          "form": "락스 어쩔수없이 쓰지만 너무 독하구요",
          "original_form": "락스 어쩔수없이 쓰지만 너무 독하구요",
          "speaker_id": "2",
          "time": "20210822 18:56"
        },
        {
          "id": "MDRW2100008749.1.6",
          "form": "곰팡이걱정은요",
          "original_form": "곰팡이걱정은요",
          "speaker_id": "1",
          "time": "20210822 19:45"
        },
        {
          "id": "MDRW2100008749.1.7",
          "form": "네 곰팡이 물때 너무 싫죠",
          "original_form": "네 곰팡이 물때 너무 싫죠",
          "speaker_id": "2",
          "time": "20210822 20:27"
        },
        {
          "id": "MDRW2100008749.1.8",
          "form": "네 호흡기도 안좋아지구",
          "original_form": "네 호흡기도 안좋아지구",
          "speaker_id": "1",
          "time": "20210822 21:48"
        },
        {
          "id": "MDRW2100008749.1.9",
          "form": "그래도 일하고 청소까지하려니 넘 힘들어요 못할때도 많네요",
          "original_form": "그래도 일하고 청소까지하려니 넘 힘들어요 못할때도 많네요",
          "speaker_id": "2",
          "time": "20210822 21:56"
        },
        {
          "id": "MDRW2100008749.1.10",
          "form": "가족이 도와주시나요?",
          "original_form": "가족이 도와주시나요?",
          "speaker_id": "2",
          "time": "20210822 21:56"
        },
        {
          "id": "MDRW2100008749.1.11",
          "form": "제가 다 해요",
          "original_form": "제가 다 해요",
          "speaker_id": "1",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100008749.1.12",
          "form": "힘드시겠어요ㅜㅜ",
          "original_form": "힘드시겠어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100008749.1.13",
          "form": "집안일은 같이해야하는데...",
          "original_form": "집안일은 같이해야하는데...",
          "speaker_id": "2",
          "time": "20210822 22:23"
        },
        {
          "id": "MDRW2100008749.1.14",
          "form": "요리를 못해서요",
          "original_form": "요리를 못해서요",
          "speaker_id": "1",
          "time": "20210822 22:25"
        },
        {
          "id": "MDRW2100008749.1.15",
          "form": "요리도 자꾸해봐야 느는것같아요 요즘엔 더워서 저도 못하네요ㅜㅠ",
          "original_form": "요리도 자꾸해봐야 느는것같아요 요즘엔 더워서 저도 못하네요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100008749.1.16",
          "form": "익숙해지면괜찮아지는데 어렵네오",
          "original_form": "익숙해지면괜찮아지는데 어렵네오",
          "speaker_id": "1",
