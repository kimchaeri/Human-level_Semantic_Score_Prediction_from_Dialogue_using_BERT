        {
          "id": "MDRW2100025090.1.1",
          "form": "네 여름보다 더 싫은 날씨네요",
          "original_form": "네 여름보다 더 싫은 날씨네요",
          "speaker_id": "2",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100025090.1.2",
          "form": "습하거",
          "original_form": "습하거",
          "speaker_id": "1",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025090.1.3",
          "form": "ㅠㅠ 날씨 최악이죠",
          "original_form": "ㅠㅠ 날씨 최악이죠",
          "speaker_id": "2",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100025090.1.4",
          "form": "오락가락해요",
          "original_form": "오락가락해요",
          "speaker_id": "1",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100025090.1.5",
          "form": "옛날이 그립네요 ㅎㅎ",
          "original_form": "옛날이 그립네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100025090.1.6",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "1",
          "time": "20210831 23:07"
        },
        {
          "id": "MDRW2100025090.1.7",
          "form": "아님 가을이라도 빨리 왓음",
          "original_form": "아님 가을이라도 빨리 왓음",
          "speaker_id": "2",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100025090.1.8",
          "form": "맑은 하늘 버구 싶아여",
          "original_form": "맑은 하늘 버구 싶아여",
          "speaker_id": "1",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100025090.1.9",
          "form": "가을은 되야 볼듯요",
          "original_form": "가을은 되야 볼듯요",
          "speaker_id": "2",
          "time": "20210831 23:13"
        },
        {
          "id": "MDRW2100025090.1.10",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "1",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100025090.1.11",
          "form": "요즘은  그냥 겨율이 나은고 같아요",
          "original_form": "요즘은  그냥 겨율이 나은고 같아요",
          "speaker_id": "2",
          "time": "20210831 23:14"
        },
        {
