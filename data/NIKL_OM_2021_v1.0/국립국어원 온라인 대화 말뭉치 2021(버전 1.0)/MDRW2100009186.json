      "utterance": [
        {
          "id": "MDRW2100009186.1.1",
          "form": "자자 이제 라면 죽 다 드시면 후식은 뭔가요~",
          "original_form": "자자 이제 라면 죽 다 드시면 후식은 뭔가요~",
          "speaker_id": "1",
          "time": "20210822 22:02"
        },
        {
          "id": "MDRW2100009186.1.2",
          "form": "흑당밀크티요",
          "original_form": "흑당밀크티요",
          "speaker_id": "2",
          "time": "20210822 22:03"
        },
        {
          "id": "MDRW2100009186.1.3",
          "form": "주문한 거 도착함ㅎ",
          "original_form": "주문한 거 도착함ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:03"
        },
        {
          "id": "MDRW2100009186.1.4",
          "form": "단 거 안 좋아하는데 흑당밀크티는 좋아해요ㅎ",
          "original_form": "단 거 안 좋아하는데 흑당밀크티는 좋아해요ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:03"
        },
        {
          "id": "MDRW2100009186.1.5",
          "form": "단 거 잘 드시나요",
          "original_form": "단 거 잘 드시나요",
          "speaker_id": "2",
          "time": "20210822 22:04"
        },
        {
          "id": "MDRW2100009186.1.6",
          "form": "단거 원래 완전 좋아하는데 건강에 안좋다고 해서 단음료는 많이 줄였어요!",
          "original_form": "단거 원래 완전 좋아하는데 건강에 안좋다고 해서 단음료는 많이 줄였어요!",
          "speaker_id": "1",
          "time": "20210822 22:06"
        },
        {
          "id": "MDRW2100009186.1.7",
          "form": "원래는 커피빈 모카아이스블랜디드 블랙 완전 초코달달음료",
          "original_form": "원래는 커피빈 모카아이스블랜디드 블랙 완전 초코달달음료",
          "speaker_id": "1",
          "time": "20210822 22:07"
        },
        {
          "id": "MDRW2100009186.1.8",
          "form": "스타벅스 자바칩프라푸치노였나... 그거 초코드리즐 많이 초코칩 많이 이렇게 먹었는데",
          "original_form": "스타벅스 자바칩프라푸치노였나... 그거 초코드리즐 많이 초코칩 많이 이렇게 먹었는데",
          "speaker_id": "1",
          "time": "20210822 22:07"
        },
        {
          "id": "MDRW2100009186.1.9",
          "form": "아주 설탕을 마시고 있었 ㅠㅠㅠ",
          "original_form": "아주 설탕을 마시고 있었 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 22:08"
        },
        {
          "id": "MDRW2100009186.1.10",
          "form": "저는 과일주스도 좋아해서 한통씩 맨날 사다놨었거든요~~",
          "original_form": "저는 과일주스도 좋아해서 한통씩 맨날 사다놨었거든요~~",
          "speaker_id": "1",
          "time": "20210822 22:08"
        },
        {
          "id": "MDRW2100009186.1.11",
          "form": "하지만 단음료만 줄였을뿐 단거=맛있는거",
          "original_form": "하지만 단음료만 줄였을뿐 단거=맛있는거",
          "speaker_id": "1",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009186.1.12",
          "form": "헉 완전 단거 좋아하시네요",
          "original_form": "헉 완전 단거 좋아하시네요",
          "speaker_id": "2",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009186.1.13",
          "form": "모카아이스블렌디드라니ㄷㄷㄷ",
          "original_form": "모카아이스블렌디드라니ㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009186.1.14",
          "form": "아시나요 그 음료! ㅋㅋㅋㅋ",
          "original_form": "아시나요 그 음료! ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009186.1.15",
          "form": "알죠ㅎ",
          "original_form": "알죠ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009186.1.16",
          "form": "블랙으로 먹었습니다...",
          "original_form": "블랙으로 먹었습니다...",
          "speaker_id": "1",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009186.1.17",
          "form": "제 최애음료였어요 ㅋㅋㅋㅋㅋ",
          "original_form": "제 최애음료였어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009186.1.18",
          "form": "저에게 모카가 붙은 음료는 달다는걸 알려준ㅎ",
          "original_form": "저에게 모카가 붙은 음료는 달다는걸 알려준ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:12"
        },
        {
          "id": "MDRW2100009186.1.19",
          "form": "동네에 커피빈이 사라지면서 스벅 자바칩으로 갈아탔지만 ㅜㅜ",
          "original_form": "동네에 커피빈이 사라지면서 스벅 자바칩으로 갈아탔지만 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100009186.1.20",
          "form": "ㅜㅇㅜ 맛이 다르잖아요 ㅜㅇㅜ",
          "original_form": "ㅜㅇㅜ 맛이 다르잖아요 ㅜㅇㅜ",
          "speaker_id": "2",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100009186.1.21",
          "form": "그래도 진짜 단거 잘 드시네요",
          "original_form": "그래도 진짜 단거 잘 드시네요",
          "speaker_id": "2",
          "time": "20210822 22:14"
        },
        {
          "id": "MDRW2100009186.1.22",
          "form": "제 인생의 단 음료는 흑당뿐ㅎㅎ",
          "original_form": "제 인생의 단 음료는 흑당뿐ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 22:14"
        },
        {
          "id": "MDRW2100009186.1.23",
          "form": "과일주스는 뭐가 맛있나요",
          "original_form": "과일주스는 뭐가 맛있나요",
          "speaker_id": "2",
          "time": "20210822 22:15"
        },
        {
          "id": "MDRW2100009186.1.24",
          "form": "과일주스는 오로지 미닛메이드였습니다...ㅋㅋㅋㅋ",
          "original_form": "과일주스는 오로지 미닛메이드였습니다...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:17"
        },
        {
          "id": "MDRW2100009186.1.25",
          "form": "안먹은지 오래돼서 모르겠어요 과일주스는 ㅎㅎㅎ",
          "original_form": "안먹은지 오래돼서 모르겠어요 과일주스는 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:18"
        },
        {
          "id": "MDRW2100009186.1.26",
          "form": "흑당밀크티는 한번도 안먹어봤어요~",
          "original_form": "흑당밀크티는 한번도 안먹어봤어요~",
          "speaker_id": "1",
          "time": "20210822 22:18"
        },
        {
          "id": "MDRW2100009186.1.27",
          "form": "여기에 펄 넣어서 드세요?",
          "original_form": "여기에 펄 넣어서 드세요?",
          "speaker_id": "1",
          "time": "20210822 22:18"
        },
        {
          "id": "MDRW2100009186.1.28",
          "form": "넣는 거 좋아해요ㅎ",
          "original_form": "넣는 거 좋아해요ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:18"
        },
        {
          "id": "MDRW2100009186.1.29",
          "form": "배가 불러지는 게 문제지만",
          "original_form": "배가 불러지는 게 문제지만",
          "speaker_id": "2",
          "time": "20210822 22:18"
        },
        {
          "id": "MDRW2100009186.1.30",
          "form": "동네에 1리터 음료만 파는 곳이 있는데 하필 거기  흑당음료가 제일 취향이에요ㅎㅎ",
          "original_form": "동네에 1리터 음료만 파는 곳이 있는데 하필 거기  흑당음료가 제일 취향이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100009186.1.31",
          "form": "펄은 재료가 뭐 예요?",
          "original_form": "펄은 재료가 뭐 예요?",
          "speaker_id": "1",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100009186.1.32",
          "form": "와우 ㅋㅋㅋㅋㅋ",
          "original_form": "와우 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100009186.1.33",
          "form": "지금 1리터 드시는거...?",
          "original_form": "지금 1리터 드시는거...?",
          "speaker_id": "1",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100009186.1.34",
          "form": "나눠서 드시는거죠? ㅋㅋㅋㅋㅋ",
          "original_form": "나눠서 드시는거죠? ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:19"
        },
        {
          "id": "MDRW2100009186.1.35",
          "form": "펄 재료는 전분이죠 ㅎ",
          "original_form": "펄 재료는 전분이죠 ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009186.1.36",
          "form": "아뇨 낼 되면 얼음 녹아 맛없어요",
          "original_form": "아뇨 낼 되면 얼음 녹아 맛없어요",
          "speaker_id": "2",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009186.1.37",
          "form": "배터져도 먹고 죽어야죠ㅎㅎ",
          "original_form": "배터져도 먹고 죽어야죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009186.1.38",
          "form": "ㅋㅋㅋㅋㅋㅋ 위야..  조금만 힘을내",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 위야..  조금만 힘을내",
          "speaker_id": "1",
          "time": "20210822 22:21"
        },
        {
          "id": "MDRW2100009186.1.39",
          "form": "요즘 음료 크게나와서 좋은데 배부를때 좀 난감해요 ㅎㅎ",
          "original_form": "요즘 음료 크게나와서 좋은데 배부를때 좀 난감해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:21"
        },
        {
          "id": "MDRW2100009186.1.40",
          "form": "ㅎㅎ 그러니까요. 하필 여기가 내 취향이라.",
          "original_form": "ㅎㅎ 그러니까요. 하필 여기가 내 취향이라.",
          "speaker_id": "2",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009186.1.41",
          "form": "위야 미안해ㅜㅜ",
          "original_form": "위야 미안해ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009186.1.42",
          "form": "그래도 위도 행복할 거예요",
          "original_form": "그래도 위도 행복할 거예요",
          "speaker_id": "2",
          "time": "20210822 22:22"
        },
        {
          "id": "MDRW2100009186.1.43",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
