      "utterance": [
        {
          "id": "MDRW2100011010.1.1",
          "form": "나는 밥솥 사는 걸 추천할게",
          "original_form": "나는 밥솥 사는 걸 추천할게",
          "speaker_id": "2",
          "time": "20210823 23:43"
        },
        {
          "id": "MDRW2100011010.1.2",
          "form": "하지만 너 집에 자주 있어?",
          "original_form": "하지만 너 집에 자주 있어?",
          "speaker_id": "2",
          "time": "20210823 23:43"
        },
        {
          "id": "MDRW2100011010.1.3",
          "form": "주변에 물어보면 반반인 것 같더라고",
          "original_form": "주변에 물어보면 반반인 것 같더라고",
          "speaker_id": "1",
          "time": "20210823 23:43"
        },
        {
          "id": "MDRW2100011010.1.4",
          "form": "집을 자주 비우고 자꾸 사먹을거라면 사지마.",
          "original_form": "집을 자주 비우고 자꾸 사먹을거라면 사지마.",
          "speaker_id": "2",
          "time": "20210823 23:43"
        },
        {
          "id": "MDRW2100011010.1.5",
          "form": "나는 잘썼어",
          "original_form": "나는 잘썼어",
          "speaker_id": "2",
          "time": "20210823 23:43"
        },
        {
          "id": "MDRW2100011010.1.6",
          "form": "아마 집에 자주 갈 것 같긴해",
          "original_form": "아마 집에 자주 갈 것 같긴해",
          "speaker_id": "1",
          "time": "20210823 23:43"
        },
        {
          "id": "MDRW2100011010.1.7",
          "form": "아무래도 학교에 친구가 없으니까",
          "original_form": "아무래도 학교에 친구가 없으니까",
          "speaker_id": "1",
          "time": "20210823 23:43"
        },
        {
          "id": "MDRW2100011010.1.8",
          "form": "4~5만원짜리라면 추천해.",
          "original_form": "4~5만원짜리라면 추천해.",
          "speaker_id": "2",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100011010.1.9",
          "form": "그러면 내가 너희집에 가게되면 식사대접해주렴.",
          "original_form": "그러면 내가 너희집에 가게되면 식사대접해주렴.",
          "speaker_id": "2",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100011010.1.10",
          "form": "너의 친구. 나니까.",
          "original_form": "너의 친구. 나니까.",
          "speaker_id": "2",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100011010.1.11",
          "form": "그래그래",
          "original_form": "그래그래",
          "speaker_id": "1",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100011010.1.12",
          "form": "그라고 밥솥을 사면",
          "original_form": "그라고 밥솥을 사면",
          "speaker_id": "1",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100011010.1.13",
          "form": "밥을 얼리는 용기도 사야해",
          "original_form": "밥을 얼리는 용기도 사야해",
          "speaker_id": "1",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100011010.1.14",
          "form": "그건 나름 쌌던 거 같아",
          "original_form": "그건 나름 쌌던 거 같아",
          "speaker_id": "2",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100011010.1.15",
          "form": "나는 기억이 잘 안나지만",
          "original_form": "나는 기억이 잘 안나지만",
          "speaker_id": "2",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100011010.1.16",
          "form": "그건 한꺼번에 많이 사서 반찬통으로도 쓰면되겠지?",
          "original_form": "그건 한꺼번에 많이 사서 반찬통으로도 쓰면되겠지?",
          "speaker_id": "1",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100011010.1.17",
          "form": "일단 본가에서 좀 가져와 봐.",
          "original_form": "일단 본가에서 좀 가져와 봐.",
          "speaker_id": "2",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100011010.1.18",
          "form": "당연하지.",
          "original_form": "당연하지.",
          "speaker_id": "2",
          "time": "20210823 23:45"
        },
        {
          "id": "MDRW2100011010.1.19",
          "form": "흠 그냥 새걸 마음 편하게 쓰고싶기도해",
          "original_form": "흠 그냥 새걸 마음 편하게 쓰고싶기도해",
          "speaker_id": "1",
          "time": "20210823 23:45"
        },
        {
          "id": "MDRW2100011010.1.20",
          "form": "돈이 많이 필요하겠구나.",
          "original_form": "돈이 많이 필요하겠구나.",
          "speaker_id": "2",
          "time": "20210823 23:45"
        },
        {
          "id": "MDRW2100011010.1.21",
          "form": "역시 첫 자취는 비싸.",
          "original_form": "역시 첫 자취는 비싸.",
          "speaker_id": "2",
          "time": "20210823 23:45"
        },
        {
          "id": "MDRW2100011010.1.22",
          "form": "근데 이러다가 거지꼴을 못면하겠지?",
          "original_form": "근데 이러다가 거지꼴을 못면하겠지?",
          "speaker_id": "1",
          "time": "20210823 23:45"
        },
        {
          "id": "MDRW2100011010.1.23",
          "form": "ㅠㅠ너무 돈이 많이든다",
          "original_form": "ㅠㅠ너무 돈이 많이든다",
          "speaker_id": "1",
          "time": "20210823 23:45"
        },
        {
          "id": "MDRW2100011010.1.24",
          "form": "응 ㅠ 자중해.",
          "original_form": "응 ㅠ 자중해.",
          "speaker_id": "2",
          "time": "20210823 23:45"
        }
      ]
    }
