      "utterance": [
        {
          "id": "MDRW2100022139.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210829 09:11"
        },
        {
          "id": "MDRW2100022139.1.2",
          "form": "요즘 무엇을 구매하셨나요",
          "original_form": "요즘 무엇을 구매하셨나요",
          "speaker_id": "2",
          "time": "20210829 09:12"
        },
        {
          "id": "MDRW2100022139.1.3",
          "form": "영양제를 구매했어요",
          "original_form": "영양제를 구매했어요",
          "speaker_id": "1",
          "time": "20210829 09:12"
        },
        {
          "id": "MDRW2100022139.1.4",
          "form": "어디서 구매하셨나요",
          "original_form": "어디서 구매하셨나요",
          "speaker_id": "2",
          "time": "20210829 09:12"
        },
        {
          "id": "MDRW2100022139.1.5",
          "form": "인터넷이요",
          "original_form": "인터넷이요",
          "speaker_id": "1",
          "time": "20210829 09:12"
        },
        {
          "id": "MDRW2100022139.1.6",
          "form": "가격은 얼마나 주셨나요",
          "original_form": "가격은 얼마나 주셨나요",
          "speaker_id": "2",
          "time": "20210829 09:13"
        },
        {
          "id": "MDRW2100022139.1.7",
          "form": "2만원 가량이요",
          "original_form": "2만원 가량이요",
          "speaker_id": "1",
          "time": "20210829 09:13"
        },
        {
          "id": "MDRW2100022139.1.8",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210829 09:13"
        },
        {
          "id": "MDRW2100022139.1.9",
          "form": "저는 화장품 구매했어요",
          "original_form": "저는 화장품 구매했어요",
          "speaker_id": "2",
          "time": "20210829 09:14"
        },
        {
          "id": "MDRW2100022139.1.10",
          "form": "어떤 화장품이요?",
          "original_form": "어떤 화장품이요?",
          "speaker_id": "1",
          "time": "20210829 09:14"
        },
        {
          "id": "MDRW2100022139.1.11",
          "form": "바디로션 구매했어요",
          "original_form": "바디로션 구매했어요",
          "speaker_id": "2",
          "time": "20210829 09:14"
        },
        {
          "id": "MDRW2100022139.1.12",
          "form": "얼마주셨나요",
          "original_form": "얼마주셨나요",
          "speaker_id": "1",
          "time": "20210829 09:14"
        },
        {
          "id": "MDRW2100022139.1.13",
          "form": "저도 2만원 정도 구매했어요",
          "original_form": "저도 2만원 정도 구매했어요",
          "speaker_id": "2",
          "time": "20210829 09:15"
        },
        {
          "id": "MDRW2100022139.1.14",
          "form": "만족스러운 쇼핑이었어요",
          "original_form": "만족스러운 쇼핑이었어요",
          "speaker_id": "2",
          "time": "20210829 09:15"
        },
        {
          "id": "MDRW2100022139.1.15",
          "form": "저도 상당히 만족스러웠어요",
          "original_form": "저도 상당히 만족스러웠어요",
          "speaker_id": "1",
          "time": "20210829 09:15"
        },
        {
          "id": "MDRW2100022139.1.16",
          "form": "할인받아서 너무 좋았어요",
          "original_form": "할인받아서 너무 좋았어요",
          "speaker_id": "2",
          "time": "20210829 09:16"
        },
        {
          "id": "MDRW2100022139.1.17",
          "form": "저렴하게 구매하면 기분좋죠",
          "original_form": "저렴하게 구매하면 기분좋죠",
          "speaker_id": "1",
          "time": "20210829 09:16"
        }
