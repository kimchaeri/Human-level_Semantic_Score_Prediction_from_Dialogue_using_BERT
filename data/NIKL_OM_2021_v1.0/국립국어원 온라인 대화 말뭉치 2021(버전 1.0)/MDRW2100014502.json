      "utterance": [
        {
          "id": "MDRW2100014502.1.1",
          "form": "큰 쇼핑 센터와도 멀어지셨나요?",
          "original_form": "큰 쇼핑 센터와도 멀어지셨나요?",
          "speaker_id": "2",
          "time": "20210825 19:28"
        },
        {
          "id": "MDRW2100014502.1.2",
          "form": "여긴 마트가 다망했어요",
          "original_form": "여긴 마트가 다망했어요",
          "speaker_id": "1",
          "time": "20210825 19:32"
        },
        {
          "id": "MDRW2100014502.1.3",
          "form": "ㅜㅜ뉴코아도 없어요",
          "original_form": "ㅜㅜ뉴코아도 없어요",
          "speaker_id": "1",
          "time": "20210825 19:32"
        },
        {
          "id": "MDRW2100014502.1.4",
          "form": "인터넷 부지런히 이용하셔야겠어요",
          "original_form": "인터넷 부지런히 이용하셔야겠어요",
          "speaker_id": "2",
          "time": "20210825 19:40"
        },
        {
          "id": "MDRW2100014502.1.5",
          "form": "뉴코아는 할인이 대박인데!",
          "original_form": "뉴코아는 할인이 대박인데!",
          "speaker_id": "2",
          "time": "20210825 19:40"
        },
        {
          "id": "MDRW2100014502.1.6",
          "form": "그러니까요 아이옷 사야하는ㄷ0",
          "original_form": "그러니까요 아이옷 사야하는ㄷ0",
          "speaker_id": "1",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100014502.1.7",
          "form": "데",
          "original_form": "데",
          "speaker_id": "1",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100014502.1.8",
          "form": "아 아이옷도 많죠",
          "original_form": "아 아이옷도 많죠",
          "speaker_id": "2",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014502.1.9",
          "form": "한 번씩 쇼핑여행 나오셔야겠네요",
          "original_form": "한 번씩 쇼핑여행 나오셔야겠네요",
          "speaker_id": "2",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014502.1.10",
          "form": "네 가끔 사러가요",
          "original_form": "네 가끔 사러가요",
          "speaker_id": "1",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014502.1.11",
          "form": "그러다 요샌 다행히 코로나로 안사네요",
          "original_form": "그러다 요샌 다행히 코로나로 안사네요",
          "speaker_id": "1",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014502.1.12",
          "form": "이게 다행인지 참나",
          "original_form": "이게 다행인지 참나",
          "speaker_id": "1",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100014502.1.13",
          "form": "덜나가니까요 ㅎㅎ",
          "original_form": "덜나가니까요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100014502.1.14",
          "form": "쇼핑센타 많으세요?",
          "original_form": "쇼핑센타 많으세요?",
          "speaker_id": "1",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014502.1.15",
          "form": "저희도 좀 외곽이에요ㅎㅎ",
          "original_form": "저희도 좀 외곽이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014502.1.16",
          "form": "아 저랑 같겠네요 ㅎㅎ",
          "original_form": "아 저랑 같겠네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014502.1.17",
          "form": "여샌 어차피 인터넷이 싸니 인터넷 사면돼죠",
          "original_form": "여샌 어차피 인터넷이 싸니 인터넷 사면돼죠",
          "speaker_id": "1",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014502.1.18",
          "form": "그래도 아쉬울때 많긴해요",
          "original_form": "그래도 아쉬울때 많긴해요",
          "speaker_id": "1",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014502.1.19",
          "form": "네 꼭 사지 않아도 역시",
          "original_form": "네 꼭 사지 않아도 역시",
          "speaker_id": "2",
          "time": "20210826 08:35"
        },
        {
          "id": "MDRW2100014502.1.20",
          "form": "쇼핑은 재밌으니까요ㅋ",
          "original_form": "쇼핑은 재밌으니까요ㅋ",
          "speaker_id": "2",
          "time": "20210826 08:35"
        },
        {
          "id": "MDRW2100014502.1.21",
          "form": "그렇죠 구경만해도 잼나죠",
          "original_form": "그렇죠 구경만해도 잼나죠",
          "speaker_id": "1",
          "time": "20210826 08:36"
        },
        {
          "id": "MDRW2100014502.1.22",
          "form": "신상도 보고 아이옷도 보고 하는 재미가 있는데",
          "original_form": "신상도 보고 아이옷도 보고 하는 재미가 있는데",
          "speaker_id": "1",
          "time": "20210826 08:37"
        },
        {
          "id": "MDRW2100014502.1.23",
          "form": "전 요새 버즈 사고싶어서",
          "original_form": "전 요새 버즈 사고싶어서",
          "speaker_id": "1",
          "time": "20210826 08:37"
        },
        {
          "id": "MDRW2100014502.1.24",
          "form": "노려보고 있어요",
          "original_form": "노려보고 있어요",
          "speaker_id": "1",
          "time": "20210826 08:37"
        },
        {
          "id": "MDRW2100014502.1.25",
          "form": "다들 귀에꽂고 다니는데 저만 없는거 같아요 ㅜㅜ",
          "original_form": "다들 귀에꽂고 다니는데 저만 없는거 같아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 08:38"
        },
        {
          "id": "MDRW2100014502.1.26",
          "form": "버즈가 갑자기 인기네요ㅋㅋ",
          "original_form": "버즈가 갑자기 인기네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 08:38"
        },
        {
          "id": "MDRW2100014502.1.27",
          "form": "통화도 편해보이고",
          "original_form": "통화도 편해보이고",
          "speaker_id": "1",
          "time": "20210826 08:38"
        },
        {
          "id": "MDRW2100014502.1.28",
          "form": "워치랑",
          "original_form": "워치랑",
          "speaker_id": "2",
          "time": "20210826 08:38"
        }
      ]
    }
