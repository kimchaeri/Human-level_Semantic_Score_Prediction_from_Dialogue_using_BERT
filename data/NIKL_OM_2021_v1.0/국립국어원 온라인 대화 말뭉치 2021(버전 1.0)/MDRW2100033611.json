      "utterance": [
        {
          "id": "MDRW2100033611.1.1",
          "form": "좋아하는 게임 있으세요?",
          "original_form": "좋아하는 게임 있으세요?",
          "speaker_id": "2",
          "time": "20210824 17:05"
        },
        {
          "id": "MDRW2100033611.1.2",
          "form": "저 롤 좋아해요 쿠키런은 하다가 접었고!",
          "original_form": "저 롤 좋아해요 쿠키런은 하다가 접었고!",
          "speaker_id": "1",
          "time": "20210824 17:06"
        },
        {
          "id": "MDRW2100033611.1.3",
          "form": "쿠키런 인기가 되게 많네요",
          "original_form": "쿠키런 인기가 되게 많네요",
          "speaker_id": "2",
          "time": "20210824 17:07"
        },
        {
          "id": "MDRW2100033611.1.4",
          "form": "롤은 재밌나요?",
          "original_form": "롤은 재밌나요?",
          "speaker_id": "2",
          "time": "20210824 17:07"
        },
        {
          "id": "MDRW2100033611.1.5",
          "form": "롤 재밌어요! 아직 6개월 밖에 안됐지만 캐릭터가 많아서 골라하는 재미가 있더라고요",
          "original_form": "롤 재밌어요! 아직 6개월 밖에 안됐지만 캐릭터가 많아서 골라하는 재미가 있더라고요",
          "speaker_id": "1",
          "time": "20210824 17:08"
        },
        {
          "id": "MDRW2100033611.1.6",
          "form": "아하 인기 많았을 땨",
          "original_form": "아하 인기 많았을 땨",
          "speaker_id": "2",
          "time": "20210824 17:08"
        },
        {
          "id": "MDRW2100033611.1.7",
          "form": "궁금했어요ㅋㅋㅋㅋㅋ",
          "original_form": "궁금했어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:08"
        },
        {
          "id": "MDRW2100033611.1.8",
          "form": "앗 한 번도 안 해보셨나요? ㅋㅋㅋㅋ",
          "original_form": "앗 한 번도 안 해보셨나요? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:10"
        },
        {
          "id": "MDRW2100033611.1.9",
          "form": "네ㅋㅋ 한창 유명항 게임은",
          "original_form": "네ㅋㅋ 한창 유명항 게임은",
          "speaker_id": "2",
          "time": "20210824 17:11"
        },
        {
          "id": "MDRW2100033611.1.10",
          "form": "다 안해봤엉6",
          "original_form": "다 안해봤엉6",
          "speaker_id": "2",
          "time": "20210824 17:11"
        },
        {
          "id": "MDRW2100033611.1.11",
          "form": "오버워치도 안해보셨나요??!?!?!",
          "original_form": "오버워치도 안해보셨나요??!?!?!",
          "speaker_id": "1",
          "time": "20210824 17:20"
        },
        {
          "id": "MDRW2100033611.1.12",
          "form": "넵 오버워치도 안해봤어요",
          "original_form": "넵 오버워치도 안해봤어요",
          "speaker_id": "2",
          "time": "20210824 17:21"
        },
        {
          "id": "MDRW2100033611.1.13",
          "form": "완전 핫한게임이잖아요",
          "original_form": "완전 핫한게임이잖아요",
          "speaker_id": "2",
          "time": "20210824 17:21"
        },
        {
          "id": "MDRW2100033611.1.14",
          "form": "배그 옵치 롤",
          "original_form": "배그 옵치 롤",
          "speaker_id": "2",
          "time": "20210824 17:21"
        },
        {
          "id": "MDRW2100033611.1.15",
          "form": "와우 그럼 무슨 게임 해보셨나여?",
          "original_form": "와우 그럼 무슨 게임 해보셨나여?",
          "speaker_id": "1",
          "time": "20210824 17:22"
        },
        {
          "id": "MDRW2100033611.1.16",
          "form": "전 넥슨 게임이요 ㅋㅋㅋㅋㅋ",
          "original_form": "전 넥슨 게임이요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:23"
        },
        {
          "id": "MDRW2100033611.1.17",
          "form": "크아 메이플 이런거요 ㅎ",
          "original_form": "크아 메이플 이런거요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 17:23"
        },
        {
          "id": "MDRW2100033611.1.18",
          "form": "오ㅋㅋㅋㅋㅋㅋ 지금도 하시는 게임 있나요?",
          "original_form": "오ㅋㅋㅋㅋㅋㅋ 지금도 하시는 게임 있나요?",
          "speaker_id": "1",
          "time": "20210824 17:25"
        },
        {
          "id": "MDRW2100033611.1.19",
          "form": "최근게임은",
          "original_form": "최근게임은",
          "speaker_id": "2",
          "time": "20210824 17:27"
        },
        {
          "id": "MDRW2100033611.1.20",
          "form": "심즈에서 멈췄어요ㅋㅋ",
          "original_form": "심즈에서 멈췄어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:27"
        }
      ]
