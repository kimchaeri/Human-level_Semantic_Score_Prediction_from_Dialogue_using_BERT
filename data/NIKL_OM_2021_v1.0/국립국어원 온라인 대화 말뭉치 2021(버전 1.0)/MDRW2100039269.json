        },
        {
          "id": "MDRW2100039269.1.2",
          "form": "게임 좋아해가지고 이번에 게임패드도 하나 질렀어요ㅋㅋ",
          "original_form": "게임 좋아해가지고 이번에 게임패드도 하나 질렀어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:28"
        },
        {
          "id": "MDRW2100039269.1.3",
          "form": "킹짱엑박패드",
          "original_form": "킹짱엑박패드",
          "speaker_id": "2",
          "time": "20210903 15:28"
        },
        {
          "id": "MDRW2100039269.1.4",
          "form": "오 좋은 소비 하셨네요ㅋㅋㅋㅋ",
          "original_form": "오 좋은 소비 하셨네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100039269.1.5",
          "form": "저도 작년에 스위치 샀어요",
          "original_form": "저도 작년에 스위치 샀어요",
          "speaker_id": "1",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100039269.1.6",
          "form": "그쵸그쵸?? 오 스위치도 필수죠",
          "original_form": "그쵸그쵸?? 오 스위치도 필수죠",
          "speaker_id": "2",
          "time": "20210903 15:30"
        },
        {
          "id": "MDRW2100039269.1.7",
          "form": "작년 대유행 때 안산사람들 거의 없겠지만서도ㅋㅋㅋ",
          "original_form": "작년 대유행 때 안산사람들 거의 없겠지만서도ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:30"
        },
        {
          "id": "MDRW2100039269.1.8",
          "form": "너무 유행이라 기계가 없었잖아요ㅠㅠㅠ",
          "original_form": "너무 유행이라 기계가 없었잖아요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 15:31"
        },
        {
          "id": "MDRW2100039269.1.9",
          "form": "맞아욬ㅋㅋㅋ 그놈의 플미업자들",
          "original_form": "맞아욬ㅋㅋㅋ 그놈의 플미업자들",
          "speaker_id": "2",
          "time": "20210903 15:32"
        },
        {
          "id": "MDRW2100039269.1.10",
          "form": "다 재고를 떠안고 침몰하라고 저주를 했는데",
          "original_form": "다 재고를 떠안고 침몰하라고 저주를 했는데",
          "speaker_id": "2",
          "time": "20210903 15:32"
        },
        {
          "id": "MDRW2100039269.1.11",
          "form": "잘 살고 있는 모양입니다",
          "original_form": "잘 살고 있는 모양입니다",
          "speaker_id": "2",
          "time": "20210903 15:32"
        },
        {
          "id": "MDRW2100039269.1.12",
          "form": "너무 잘팔렷서..",
          "original_form": "너무 잘팔렷서..",
          "speaker_id": "2",
          "time": "20210903 15:32"
        },
        {
          "id": "MDRW2100039269.1.13",
          "form": "저도 플미 업자한테 사기 싫어서",
          "original_form": "저도 플미 업자한테 사기 싫어서",
          "speaker_id": "1",
          "time": "20210903 15:33"
        },
        {
          "id": "MDRW2100039269.1.14",
          "form": "언제 판매 시작한다는 거 막 기다리고 그랬어요",
          "original_form": "언제 판매 시작한다는 거 막 기다리고 그랬어요",
          "speaker_id": "1",
          "time": "20210903 15:33"
        },
        {
          "id": "MDRW2100039269.1.15",
          "form": "저는 그때.. 동숲에디션 예판 때 대원샵까지 가서 줄섰었어요",
          "original_form": "저는 그때.. 동숲에디션 예판 때 대원샵까지 가서 줄섰었어요",
          "speaker_id": "2",
          "time": "20210903 15:34"
        },
        {
          "id": "MDRW2100039269.1.16",
          "form": "(광탈)",
          "original_form": "(광탈)",
          "speaker_id": "2",
          "time": "20210903 15:34"
        },
        {
          "id": "MDRW2100039269.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ사람 너무 많아~~~",
          "original_form": "ㅋㅋㅋㅋㅋㅋ사람 너무 많아~~~",
          "speaker_id": "2",
          "time": "20210903 15:34"
        },
        {
          "id": "MDRW2100039269.1.18",
          "form": "와....",
          "original_form": "와....",
          "speaker_id": "1",
          "time": "20210903 15:34"
        },
        {
          "id": "MDRW2100039269.1.19",
          "form": "대단하시다",
          "original_form": "대단하시다",
          "speaker_id": "1",
          "time": "20210903 15:35"
        },
        {
          "id": "MDRW2100039269.1.20",
          "form": "저는 그건 포기하고 걍 네온 샀어욬ㅋㅋ",
          "original_form": "저는 그건 포기하고 걍 네온 샀어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:35"
        },
        {
          "id": "MDRW2100039269.1.21",
          "form": "억울해서 밤샘까지 해볼까 생각했는데",
          "original_form": "억울해서 밤샘까지 해볼까 생각했는데",
          "speaker_id": "2",
          "time": "20210903 15:35"
        },
        {
          "id": "MDRW2100039269.1.22",
          "form": "저녁7시에 이미 15명 줄 다 섰다는거 보고",
          "original_form": "저녁7시에 이미 15명 줄 다 섰다는거 보고",
          "speaker_id": "2",
          "time": "20210903 15:35"
        },
        {
          "id": "MDRW2100039269.1.23",
          "form": "포기...ㅋㅋㅋㅋㅋㅋ",
          "original_form": "포기...ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:35"
        },
        {
          "id": "MDRW2100039269.1.24",
          "form": "저땐 약간 광기였던거같아요",
          "original_form": "저땐 약간 광기였던거같아요",
          "speaker_id": "2",
          "time": "20210903 15:36"
        },
        {
          "id": "MDRW2100039269.1.25",
          "form": "그니까요 가끔 이해못할 광기가 보일 때 있는데",
          "original_form": "그니까요 가끔 이해못할 광기가 보일 때 있는데",
          "speaker_id": "1",
          "time": "20210903 15:38"
        },
        {
          "id": "MDRW2100039269.1.26",
          "form": "그 허버칩 때도...",
          "original_form": "그 허버칩 때도...",
          "speaker_id": "1",
          "time": "20210903 15:38"
        },
        {
