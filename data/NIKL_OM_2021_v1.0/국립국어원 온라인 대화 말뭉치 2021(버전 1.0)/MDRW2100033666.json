      "utterance": [
        {
          "id": "MDRW2100033666.1.1",
          "form": "정말 힘들었었어요",
          "original_form": "정말 힘들었었어요",
          "speaker_id": "1",
          "time": "20210825 01:13"
        },
        {
          "id": "MDRW2100033666.1.2",
          "form": "건강 챙기시는 편인가요?",
          "original_form": "건강 챙기시는 편인가요?",
          "speaker_id": "2",
          "time": "20210825 01:13"
        },
        {
          "id": "MDRW2100033666.1.3",
          "form": "유산균만 먹고 있는데 똥겁도 많아서 건강검진 내시경 이런거 한번도 안해봤는데 ㅠㅠ 애조금 더 크면 해보려구요ㅠㅠ",
          "original_form": "유산균만 먹고 있는데 똥겁도 많아서 건강검진 내시경 이런거 한번도 안해봤는데 ㅠㅠ 애조금 더 크면 해보려구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 01:16"
        },
        {
          "id": "MDRW2100033666.1.4",
          "form": "저도 유산균 먹어요!",
          "original_form": "저도 유산균 먹어요!",
          "speaker_id": "2",
          "time": "20210825 01:17"
        },
        {
          "id": "MDRW2100033666.1.5",
          "form": "운동은 하시나요?",
          "original_form": "운동은 하시나요?",
          "speaker_id": "2",
          "time": "20210825 01:17"
        },
        {
          "id": "MDRW2100033666.1.6",
          "form": "아직 둘째가 또 ㅠㅠ 어려서ㅠㅠ",
          "original_form": "아직 둘째가 또 ㅠㅠ 어려서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 01:19"
        },
        {
          "id": "MDRW2100033666.1.7",
          "form": "저는 집에서 요가랑 스트레칭 해요",
          "original_form": "저는 집에서 요가랑 스트레칭 해요",
          "speaker_id": "2",
          "time": "20210825 01:19"
        },
        {
          "id": "MDRW2100033666.1.8",
          "form": "홈트 좋죠!",
          "original_form": "홈트 좋죠!",
          "speaker_id": "1",
          "time": "20210825 01:20"
        },
        {
          "id": "MDRW2100033666.1.9",
          "form": "다이어트 해보신적 있으세요?",
          "original_form": "다이어트 해보신적 있으세요?",
          "speaker_id": "2",
          "time": "20210825 01:22"
        },
        {
          "id": "MDRW2100033666.1.10",
          "form": "생각만해요 ㅋㅋ 다이어트 하셔요?",
          "original_form": "생각만해요 ㅋㅋ 다이어트 하셔요?",
          "speaker_id": "1",
          "time": "20210825 01:23"
        },
        {
          "id": "MDRW2100033666.1.11",
          "form": "해본적이 없네요",
          "original_form": "해본적이 없네요",
          "speaker_id": "2",
          "time": "20210825 01:24"
        },
        {
          "id": "MDRW2100033666.1.12",
          "form": "그냥 건강만하자라는 주의라서요ㅋㄱ",
          "original_form": "그냥 건강만하자라는 주의라서요ㅋㄱ",
          "speaker_id": "2",
          "time": "20210825 01:24"
        },
        {
          "id": "MDRW2100033666.1.13",
          "form": "ㅋㅋㅋㅋㅋ 운동을 해야건강할껀데 그쳐",
          "original_form": "ㅋㅋㅋㅋㅋ 운동을 해야건강할껀데 그쳐",
