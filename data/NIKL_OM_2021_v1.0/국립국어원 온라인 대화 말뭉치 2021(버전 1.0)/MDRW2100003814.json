        },
        {
          "id": "MDRW2100003814.1.3",
          "form": "장마 끝나면",
          "original_form": "장마 끝나면",
          "speaker_id": "2",
          "time": "20210821 12:26"
        },
        {
          "id": "MDRW2100003814.1.4",
          "form": "장마가 언제 끝나는데",
          "original_form": "장마가 언제 끝나는데",
          "speaker_id": "1",
          "time": "20210821 12:27"
        },
        {
          "id": "MDRW2100003814.1.5",
          "form": "다음 주 내내 비 오고 그 다음 주는 괜찮겠지",
          "original_form": "다음 주 내내 비 오고 그 다음 주는 괜찮겠지",
          "speaker_id": "2",
          "time": "20210821 12:27"
        },
        {
          "id": "MDRW2100003814.1.6",
          "form": "그래 그럼 가자",
          "original_form": "그래 그럼 가자",
          "speaker_id": "1",
          "time": "20210821 12:27"
        },
        {
          "id": "MDRW2100003814.1.7",
          "form": "제주도 가기 딱 좋은 날씨다",
          "original_form": "제주도 가기 딱 좋은 날씨다",
          "speaker_id": "2",
          "time": "20210821 12:27"
        },
        {
          "id": "MDRW2100003814.1.8",
          "form": "가서 맛있는 거 많이 먹자",
          "original_form": "가서 맛있는 거 많이 먹자",
          "speaker_id": "1",
          "time": "20210821 12:27"
        },
        {
          "id": "MDRW2100003814.1.9",
          "form": "늘 계획 좀 짜야겠다",
          "original_form": "늘 계획 좀 짜야겠다",
          "speaker_id": "2",
          "time": "20210821 12:27"
        },
        {
          "id": "MDRW2100003814.1.10",
          "form": "너무 힘들지 않은 루트로",
          "original_form": "너무 힘들지 않은 루트로",
          "speaker_id": "1",
          "time": "20210821 12:28"
        },
        {
          "id": "MDRW2100003814.1.11",
          "form": "그래 렌트카 타고 다녀야지",
          "original_form": "그래 렌트카 타고 다녀야지",
