      "utterance": [
        {
          "id": "MDRW2100044107.1.1",
          "form": "다리 꼬지 마라",
          "original_form": "다리 꼬지 마라",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044107.1.2",
          "form": "꼬는게 넘 편한데 ㅠㅠ",
          "original_form": "꼬는게 넘 편한데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044107.1.3",
          "form": "척추에 안좋음",
          "original_form": "척추에 안좋음",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044107.1.4",
          "form": "흑흑 벌써 틀어져서 편한건가",
          "original_form": "흑흑 벌써 틀어져서 편한건가",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044107.1.5",
          "form": "그럴수 있다",
          "original_form": "그럴수 있다",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044107.1.6",
          "form": "그럼 진짜 안해야겠다",
          "original_form": "그럼 진짜 안해야겠다",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044107.1.7",
          "form": "그래 하지마라",
          "original_form": "그래 하지마라",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044107.1.8",
          "form": "웅웅..",
          "original_form": "웅웅..",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044107.1.9",
          "form": "바른자세 해라",
          "original_form": "바른자세 해라",
          "speaker_id": "2",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044107.1.10",
          "form": "그래야지..!",
          "original_form": "그래야지..!",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044107.1.11",
          "form": "수건으로 다리를 묶어도 좋음",
          "original_form": "수건으로 다리를 묶어도 좋음",
          "speaker_id": "2",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044107.1.12",
          "form": "아 안벌어지게?",
          "original_form": "아 안벌어지게?",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044107.1.13",
          "form": "그래 ㅋㅋ",
          "original_form": "그래 ㅋㅋ",
