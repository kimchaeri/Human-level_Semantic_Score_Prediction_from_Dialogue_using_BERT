      "utterance": [
        {
          "id": "MDRW2100028661.1.1",
          "form": "요즘 배달음식 자주 드시나요?",
          "original_form": "요즘 배달음식 자주 드시나요?",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028661.1.2",
          "form": "아니요!",
          "original_form": "아니요!",
          "speaker_id": "2",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028661.1.3",
          "form": "망게님은요?",
          "original_form": "망게님은요?",
          "speaker_id": "2",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028661.1.4",
          "form": "저는 엄청.. 자주ㅋㅋㅋㅋㅋ",
          "original_form": "저는 엄청.. 자주ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028661.1.5",
          "form": "ㅋㅋㅋㅋㅋ저두 원래 자주 시켜먹었는데",
          "original_form": "ㅋㅋㅋㅋㅋ저두 원래 자주 시켜먹었는데",
          "speaker_id": "2",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028661.1.6",
          "form": "돈이 너무 많이 들어서..",
          "original_form": "돈이 너무 많이 들어서..",
          "speaker_id": "2",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028661.1.7",
          "form": "그건 그렇죠..",
          "original_form": "그건 그렇죠..",
          "speaker_id": "1",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028661.1.8",
          "form": "근데 직접 요리하기가 힘들더라구요",
          "original_form": "근데 직접 요리하기가 힘들더라구요",
          "speaker_id": "1",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028661.1.9",
          "form": "저는 요리를 좋아하는 편이라서",
          "original_form": "저는 요리를 좋아하는 편이라서",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028661.1.10",
          "form": "더 그런가봐요",
          "original_form": "더 그런가봐요",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028661.1.11",
          "form": "요리는 뭘 보고 하시나요?",
          "original_form": "요리는 뭘 보고 하시나요?",
          "speaker_id": "1",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028661.1.12",
          "form": "저는 주로 유튜브에서 보는 것 같아요!",
          "original_form": "저는 주로 유튜브에서 보는 것 같아요!",
          "speaker_id": "2",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028661.1.13",
          "form": "백종원님 요리법을 애용합니다ㅎㅎ",
          "original_form": "백종원님 요리법을 애용합니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028661.1.14",
          "form": "ㅋㅋㅋㅋㅋ추천하시는 간단요리가 있을까요?",
          "original_form": "ㅋㅋㅋㅋㅋ추천하시는 간단요리가 있을까요?",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028661.1.15",
          "form": "저는 버섯을 좋아해서 버섯조림이요!",
          "original_form": "저는 버섯을 좋아해서 버섯조림이요!",
          "speaker_id": "2",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100028661.1.16",
          "form": "다음에 유튜브에 한 번 검색해볼게요ㅋㅋㅋㅋ",
          "original_form": "다음에 유튜브에 한 번 검색해볼게요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100028661.1.17",
          "form": "네네 꼭 드셔보세요!",
          "original_form": "네네 꼭 드셔보세요!",
          "speaker_id": "2",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100028661.1.18",
          "form": "재료는 버섯만 있으면 되나요?",
          "original_form": "재료는 버섯만 있으면 되나요?",
          "speaker_id": "1",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100028661.1.19",
          "form": "주재료는 그렇고! 양파 좋아하시면 같이 조려도 맛있어요",
          "original_form": "주재료는 그렇고! 양파 좋아하시면 같이 조려도 맛있어요",
          "speaker_id": "2",
          "time": "20210902 21:21"
        }
      ]
