      "utterance": [
        {
          "id": "MDRW2100018635.1.1",
          "form": "저녁 뭐 먹었다햇어?",
          "original_form": "저녁 뭐 먹었다햇어?",
          "speaker_id": "1",
          "time": "20210827 18:27"
        },
        {
          "id": "MDRW2100018635.1.2",
          "form": "요즘 운동하나?",
          "original_form": "요즘 운동하나?",
          "speaker_id": "1",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100018635.1.3",
          "form": "아니 안함 ㅋㅋ 찔끔하다 하기싫어서 미루는중",
          "original_form": "아니 안함 ㅋㅋ 찔끔하다 하기싫어서 미루는중",
          "speaker_id": "2",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100018635.1.4",
          "form": "ㅎㅎㅎ운동 귀찮",
          "original_form": "ㅎㅎㅎ운동 귀찮",
          "speaker_id": "1",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100018635.1.5",
          "form": "ㅇㅇ그래도 해야지ㅜㅜ 나도 이제 슬슬 하려구",
          "original_form": "ㅇㅇ그래도 해야지ㅜㅜ 나도 이제 슬슬 하려구",
          "speaker_id": "2",
          "time": "20210829 18:50"
        },
        {
          "id": "MDRW2100018635.1.6",
          "form": "ㅎㅅㅎ",
          "original_form": "ㅎㅅㅎ",
          "speaker_id": "1",
          "time": "20210829 18:52"
        },
        {
          "id": "MDRW2100018635.1.7",
          "form": "하겠나 싶\n하면 좋은건 안다만ㅎ",
          "original_form": "하겠나 싶\n하면 좋은건 안다만ㅎ",
          "speaker_id": "1",
          "time": "20210829 18:53"
        },
        {
          "id": "MDRW2100018635.1.8",
          "form": "ㅇㅇ 난 자전거있으니까 그나마 타는거같아",
          "original_form": "ㅇㅇ 난 자전거있으니까 그나마 타는거같아",
          "speaker_id": "2",
          "time": "20210829 18:59"
        },
        {
          "id": "MDRW2100018635.1.9",
          "form": "그런가보네ㅎ",
          "original_form": "그런가보네ㅎ",
          "speaker_id": "1",
          "time": "20210829 19:05"
        },
        {
          "id": "MDRW2100018635.1.10",
          "form": "운동 어렵",
          "original_form": "운동 어렵",
          "speaker_id": "1",
          "time": "20210829 19:05"
        },
        {
          "id": "MDRW2100018635.1.11",
          "form": "그렇지 운동이 막상 하면 해지는데",
          "original_form": "그렇지 운동이 막상 하면 해지는데",
          "speaker_id": "2",
          "time": "20210829 19:13"
        },
        {
          "id": "MDRW2100018635.1.12",
          "form": "안움직이면 계속 안되더라",
          "original_form": "안움직이면 계속 안되더라",
          "speaker_id": "2",
          "time": "20210829 19:13"
        },
        {
          "id": "MDRW2100018635.1.13",
          "form": "목욕탕 같아ㅋ\n막상 가면 잘 하는데\n갈때 까지 잘 안가지는ㅋ",
          "original_form": "목욕탕 같아ㅋ\n막상 가면 잘 하는데\n갈때 까지 잘 안가지는ㅋ",
          "speaker_id": "1",
          "time": "20210829 19:23"
        },
        {
          "id": "MDRW2100018635.1.14",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:31"
        },
        {
          "id": "MDRW2100018635.1.15",
          "form": "하면 또 한다는\n그기 꾸준히가 어렵지",
          "original_form": "하면 또 한다는\n그기 꾸준히가 어렵지",
          "speaker_id": "1",
          "time": "20210829 19:32"
        },
        {
          "id": "MDRW2100018635.1.16",
          "form": "운동은 요즘 안하지만 음식을 다이어트식으로 바꿨어",
          "original_form": "운동은 요즘 안하지만 음식을 다이어트식으로 바꿨어",
          "speaker_id": "2",
          "time": "20210829 19:32"
        },
        {
          "id": "MDRW2100018635.1.17",
          "form": "효과보임?",
          "original_form": "효과보임?",
          "speaker_id": "1",
          "time": "20210829 19:32"
        },
        {
          "id": "MDRW2100018635.1.18",
          "form": "살이 더 찌지않는정도? 왜냐면 밀가루도 먹고 치킨도 먹어서ㅜㅜ",
          "original_form": "살이 더 찌지않는정도? 왜냐면 밀가루도 먹고 치킨도 먹어서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 19:33"
        }
      ]
