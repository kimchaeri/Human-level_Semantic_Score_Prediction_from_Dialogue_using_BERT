      "utterance": [
        {
          "id": "MDRW2100005974.1.1",
          "form": "아니또 그런 방법이 잇다구요 ㅋㅋㅋ",
          "original_form": "아니또 그런 방법이 잇다구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:00"
        },
        {
          "id": "MDRW2100005974.1.2",
          "form": "그게어케가능하죸ㅋㅋㅋ",
          "original_form": "그게어케가능하죸ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:00"
        },
        {
          "id": "MDRW2100005974.1.3",
          "form": "맛있게 빨리 먹어야됨 ㅋㅋ",
          "original_form": "맛있게 빨리 먹어야됨 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100005974.1.4",
          "form": "취하기 전에 해산!",
          "original_form": "취하기 전에 해산!",
          "speaker_id": "2",
          "time": "20210907 18:01"
        },
        {
          "id": "MDRW2100005974.1.5",
          "form": "zzzzzzzㅋㅋㅋㅋ 취하면 뒷감당 힘들죠",
          "original_form": "zzzzzzzㅋㅋㅋㅋ 취하면 뒷감당 힘들죠",
          "speaker_id": "1",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100005974.1.6",
          "form": "소맥 어떤궁합 조아하세요",
          "original_form": "소맥 어떤궁합 조아하세요",
          "speaker_id": "1",
          "time": "20210907 18:04"
        },
        {
          "id": "MDRW2100005974.1.7",
          "form": "전 튀김류 좋아해요",
          "original_form": "전 튀김류 좋아해요",
          "speaker_id": "2",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100005974.1.8",
          "form": "name1님은?",
          "original_form": "&name1&님은?",
          "speaker_id": "2",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100005974.1.9",
          "form": "고기? ㅋㅋㅋ",
          "original_form": "고기? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100005974.1.10",
          "form": "튀김 먹고싶네요",
          "original_form": "튀김 먹고싶네요",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100005974.1.11",
          "form": "저 공원에서 맥주 빨고있서용",
          "original_form": "저 공원에서 맥주 빨고있서용",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100005974.1.12",
          "form": "대박...ㅋㅋㅋㅋㅋ 친구랑요? 비안오나요",
          "original_form": "대박...ㅋㅋㅋㅋㅋ 친구랑요? 비안오나요",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100005974.1.13",
          "form": "비안와요",
          "original_form": "비안와요",
          "speaker_id": "2",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100005974.1.14",
          "form": "여기 바닷가 옆이라",
          "original_form": "여기 바닷가 옆이라",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100005974.1.15",
          "form": "운치있습니다",
          "original_form": "운치있습니다",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100005974.1.16",
          "form": "와부럽다",
          "original_form": "와부럽다",
          "speaker_id": "1",
