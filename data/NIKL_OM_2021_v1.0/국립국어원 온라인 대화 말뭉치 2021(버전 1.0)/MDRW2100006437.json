      "utterance": [
        {
          "id": "MDRW2100006437.1.1",
          "form": "책은 양보단 질이죠",
          "original_form": "책은 양보단 질이죠",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100006437.1.2",
          "form": "그리고 좋은 도서들ㄹ은 겁나게 잘고르잖아",
          "original_form": "그리고 좋은 도서들ㄹ은 겁나게 잘고르잖아",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100006437.1.3",
          "form": "일단 별로같아보이면 잘 안잡거든ㅋㅋㅋㅋ",
          "original_form": "일단 별로같아보이면 잘 안잡거든ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:16"
        },
        {
          "id": "MDRW2100006437.1.4",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100006437.1.5",
          "form": "1차로 일단 거르고 걸러서 보느라",
          "original_form": "1차로 일단 거르고 걸러서 보느라",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100006437.1.6",
          "form": "그런 사람이 리디에 책을 몇백개 쌓아놨습니다",
          "original_form": "그런 사람이 리디에 책을 몇백개 쌓아놨습니다",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100006437.1.7",
          "form": "지를땐 분명 재밌어보여서 샀는데 언제 이렇게 늘었담",
          "original_form": "지를땐 분명 재밌어보여서 샀는데 언제 이렇게 늘었담",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100006437.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100006437.1.9",
          "form": "그 서재 내가 탐내는 중이라고",
          "original_form": "그 서재 내가 탐내는 중이라고",
          "speaker_id": "1",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100006437.1.10",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100006437.1.11",
          "form": "나도 다 못봤는걸ㅋㅋㅋㅋ",
          "original_form": "나도 다 못봤는걸ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100006437.1.12",
          "form": "그렇다고 그걸 달라고 할 순 없는걸?",
          "original_form": "그렇다고 그걸 달라고 할 순 없는걸?",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100006437.1.13",
          "form": "그거?",
          "original_form": "그거?",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100006437.1.14",
          "form": "너의 책장",
          "original_form": "너의 책장",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100006437.1.15",
          "form": "호시탐탐 뭐가 있나 들여다 보고 싶으니라",
          "original_form": "호시탐탐 뭐가 있나 들여다 보고 싶으니라",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100006437.1.16",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100006437.1.17",
          "form": "에이 뭘 그런걸",
          "original_form": "에이 뭘 그런걸",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100006437.1.18",
          "form": "에이.. 그런거라니 나보다 소장중인 책 많은걸?",
          "original_form": "에이.. 그런거라니 나보다 소장중인 책 많은걸?",
          "speaker_id": "1",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100006437.1.19",
          "form": "그야.. 일단 권수로만 쳐도 몇백권인걸ㅋㅋㅋㅋ",
          "original_form": "그야.. 일단 권수로만 쳐도 몇백권인걸ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100006437.1.20",
          "form": "와 이걸 다 종이책으로 갖고 있었다고 생각하면",
          "original_form": "와 이걸 다 종이책으로 갖고 있었다고 생각하면",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100006437.1.21",
          "form": "난 발 뻗고 잠도 못잤을거야 ㅋㅋㅋ",
          "original_form": "난 발 뻗고 잠도 못잤을거야 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100006437.1.22",
          "form": "와... 책에 파묻혔다",
          "original_form": "와... 책에 파묻혔다",
          "speaker_id": "1",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100006437.1.23",
          "form": "물론 행복한 삶이었다 할 수 있지만",
          "original_form": "물론 행복한 삶이었다 할 수 있지만",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100006437.1.24",
          "form": "안돼... 나 아직 결말 못본책이..",
          "original_form": "안돼... 나 아직 결말 못본책이..",
          "speaker_id": "1",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100006437.1.25",
          "form": "염라대왕님 저 책 결말 봐야해요 아니면 저승 못가요 ㅠ",
          "original_form": "염라대왕님 저 책 결말 봐야해요 아니면 저승 못가요 ㅠ",
          "speaker_id": "1",
