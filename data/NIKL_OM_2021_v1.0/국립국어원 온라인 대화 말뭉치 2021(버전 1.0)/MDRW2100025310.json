        },
        {
          "id": "MDRW2100025310.1.2",
          "form": "안녕!",
          "original_form": "안녕!",
          "speaker_id": "2",
          "time": "20210901 08:55"
        },
        {
          "id": "MDRW2100025310.1.3",
          "form": "아 마자 혹시 넷플요새도 보시나여?",
          "original_form": "아 마자 혹시 넷플요새도 보시나여?",
          "speaker_id": "1",
          "time": "20210901 08:55"
        },
        {
          "id": "MDRW2100025310.1.4",
          "form": "이번에 디피가 아주 핫하던데",
          "original_form": "이번에 디피가 아주 핫하던데",
          "speaker_id": "1",
          "time": "20210901 08:55"
        },
        {
          "id": "MDRW2100025310.1.5",
          "form": "봐보까...",
          "original_form": "봐보까...",
          "speaker_id": "1",
          "time": "20210901 08:55"
        },
        {
          "id": "MDRW2100025310.1.6",
          "form": "앗 아니여... 요새 유튜브도 볼 거 쌓아놓고 제대로 못봐서ㅋㅋㅋㅋㅋ",
          "original_form": "앗 아니여... 요새 유튜브도 볼 거 쌓아놓고 제대로 못봐서ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 08:56"
        },
        {
          "id": "MDRW2100025310.1.7",
          "form": "디피 그거 군대얘기인가?",
          "original_form": "디피 그거 군대얘기인가?",
          "speaker_id": "2",
          "time": "20210901 08:56"
        },
        {
          "id": "MDRW2100025310.1.8",
          "form": "ㅇㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 08:56"
        },
        {
          "id": "MDRW2100025310.1.9",
          "form": "캡쳐된 짤같은걸 본거같은데",
          "original_form": "캡쳐된 짤같은걸 본거같은데",
          "speaker_id": "2",
          "time": "20210901 08:56"
        },
        {
          "id": "MDRW2100025310.1.10",
          "form": "군대 탈영병잡는 얘기던데",
          "original_form": "군대 탈영병잡는 얘기던데",
          "speaker_id": "1",
          "time": "20210901 08:56"
        },
        {
          "id": "MDRW2100025310.1.11",
          "form": "아 헐",
          "original_form": "아 헐",
          "speaker_id": "2",
          "time": "20210901 08:57"
        },
        {
          "id": "MDRW2100025310.1.12",
          "form": "탈영병 잡는 스토리구나",
          "original_form": "탈영병 잡는 스토리구나",
          "speaker_id": "2",
          "time": "20210901 08:57"
        },
        {
          "id": "MDRW2100025310.1.13",
          "form": "ㅇㅇ 그 군대가혹행위때문에",
          "original_form": "ㅇㅇ 그 군대가혹행위때문에",
          "speaker_id": "1",
          "time": "20210901 08:57"
        },
        {
          "id": "MDRW2100025310.1.14",
          "form": "도망간사람 추노하는얘기래",
          "original_form": "도망간사람 추노하는얘기래",
          "speaker_id": "1",
          "time": "20210901 08:57"
        },
        {
          "id": "MDRW2100025310.1.15",
          "form": "띠용ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "띠용ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 08:57"
        },
        {
          "id": "MDRW2100025310.1.16",
          "form": "근데 반응 되게 좋나보던데",
          "original_form": "근데 반응 되게 좋나보던데",
          "speaker_id": "2",
          "time": "20210901 08:57"
        },
        {
          "id": "MDRW2100025310.1.17",
          "form": "ㅋㅋㅋㄱㅋ ㅇㅇㅇ 성별막론하고",
          "original_form": "ㅋㅋㅋㄱㅋ ㅇㅇㅇ 성별막론하고",
          "speaker_id": "1",
          "time": "20210901 08:57"
        },
        {
          "id": "MDRW2100025310.1.18",
          "form": "다 호평이더라고",
          "original_form": "다 호평이더라고",
          "speaker_id": "1",
          "time": "20210901 08:58"
        },
        {
          "id": "MDRW2100025310.1.19",
          "form": "특히 거기나오는 구교환이라는배우있는데",
          "original_form": "특히 거기나오는 구교환이라는배우있는데",
          "speaker_id": "1",
          "time": "20210901 08:58"
        },
        {
          "id": "MDRW2100025310.1.20",
          "form": "그분이모가디슈나왔거든여",
          "original_form": "그분이모가디슈나왔거든여",
          "speaker_id": "1",
          "time": "20210901 08:58"
        },
        {
          "id": "MDRW2100025310.1.21",
          "form": "근데 그사람 걔닮았어",
          "original_form": "근데 그사람 걔닮았어",
          "speaker_id": "1",
          "time": "20210901 08:58"
        },
        {
          "id": "MDRW2100025310.1.22",
          "form": "킹덤영신배우",
          "original_form": "킹덤영신배우",
          "speaker_id": "1",
          "time": "20210901 08:58"
        },
        {
          "id": "MDRW2100025310.1.23",
          "form": "이름이생각안나...",
          "original_form": "이름이생각안나...",
          "speaker_id": "1",
          "time": "20210901 08:58"
        },
        {
          "id": "MDRW2100025310.1.24",
          "form": "김성규?",
          "original_form": "김성규?",
          "speaker_id": "2",
          "time": "20210901 08:59"
        },
        {
          "id": "MDRW2100025310.1.25",
          "form": "맞나?",
          "original_form": "맞나?",
          "speaker_id": "2",
          "time": "20210901 08:59"
        },
        {
          "id": "MDRW2100025310.1.26",
          "form": "아 맞아",
          "original_form": "아 맞아",
          "speaker_id": "1",
          "time": "20210901 08:59"
        },
        {
          "id": "MDRW2100025310.1.27",
          "form": "김성규랑 되게 닮았어",
          "original_form": "김성규랑 되게 닮았어",
          "speaker_id": "1",
          "time": "20210901 08:59"
        },
        {
          "id": "MDRW2100025310.1.28",
          "form": "성규 하면 아직도 인피니트 걔가 먼저 생각나섴ㅋㅋㅋㅋㅋ",
          "original_form": "성규 하면 아직도 인피니트 걔가 먼저 생각나섴ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 08:59"
        },
        {
          "id": "MDRW2100025310.1.29",
          "form": "목소리도비슷해 ㅋㄱㅋㅋㄱㄱ",
          "original_form": "목소리도비슷해 ㅋㄱㅋㅋㄱㄱ",
          "speaker_id": "1",
          "time": "20210901 08:59"
        },
        {
          "id": "MDRW2100025310.1.30",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210901 09:00"
        },
        {
          "id": "MDRW2100025310.1.31",
          "form": "그래서 사람들이 첨보고 오...킹덤과 범죄도시의 그분인가",
          "original_form": "그래서 사람들이 첨보고 오...킹덤과 범죄도시의 그분인가",
          "speaker_id": "1",
          "time": "20210901 09:00"
        },
        {
          "id": "MDRW2100025310.1.32",
          "form": "했다가 나중에 오...아니었네...이렇게된다고",
          "original_form": "했다가 나중에 오...아니었네...이렇게된다고",
