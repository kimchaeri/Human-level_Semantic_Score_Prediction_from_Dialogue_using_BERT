      "utterance": [
        {
          "id": "MDRW2100023124.1.1",
          "form": "이런거 잇다는 소리는 들엇어요",
          "original_form": "이런거 잇다는 소리는 들엇어요",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023124.1.2",
          "form": "남성들 필라테스 학원",
          "original_form": "남성들 필라테스 학원",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023124.1.3",
          "form": "필라테스 남성 학원이요?",
          "original_form": "필라테스 남성 학원이요?",
          "speaker_id": "1",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023124.1.4",
          "form": "오 서울에 있어요?",
          "original_form": "오 서울에 있어요?",
          "speaker_id": "1",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023124.1.5",
          "form": "네네 서울에 있다고 본거같아요",
          "original_form": "네네 서울에 있다고 본거같아요",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023124.1.6",
          "form": "와 찾아봐야겠다",
          "original_form": "와 찾아봐야겠다",
          "speaker_id": "1",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023124.1.7",
          "form": "근데 얼마 정도 해요?",
          "original_form": "근데 얼마 정도 해요?",
          "speaker_id": "1",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023124.1.8",
          "form": "서울엔 찾아보면 웬만한건 다 잇는거 같은데 ㅋㅋ",
          "original_form": "서울엔 찾아보면 웬만한건 다 잇는거 같은데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023124.1.9",
          "form": "학원마다 가격 되게 다른데",
          "original_form": "학원마다 가격 되게 다른데",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100023124.1.10",
          "form": "저는 18회에 55만원으로 듣고 잇어요",
          "original_form": "저는 18회에 55만원으로 듣고 잇어요",
          "speaker_id": "2",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100023124.1.11",
          "form": "오우",
          "original_form": "오우",
          "speaker_id": "1",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100023124.1.12",
          "form": "수업당 3만원 정도선",
          "original_form": "수업당 3만원 정도선",
          "speaker_id": "2",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100023124.1.13",
          "form": "그럼 일주일에 몇번 하시는거죠",
          "original_form": "그럼 일주일에 몇번 하시는거죠",
          "speaker_id": "1",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100023124.1.14",
          "form": "일주일에 두번씩!",
          "original_form": "일주일에 두번씩!",
          "speaker_id": "2",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100023124.1.15",
          "form": "그럼 나머지는 아예 학원에 안가요?",
          "original_form": "그럼 나머지는 아예 학원에 안가요?",
          "speaker_id": "1",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100023124.1.16",
          "form": "아니면 자유롭게 와서 기구 쓸 수 있어요?",
          "original_form": "아니면 자유롭게 와서 기구 쓸 수 있어요?",
          "speaker_id": "1",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100023124.1.17",
          "form": "안가요 ㅋㅋㅋ",
          "original_form": "안가요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100023124.1.18",
          "form": "헬스장같은게 아니라서",
          "original_form": "헬스장같은게 아니라서",
          "speaker_id": "2",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100023124.1.19",
          "form": "수업 있을때만 가요",
          "original_form": "수업 있을때만 가요",
          "speaker_id": "2",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100023124.1.20",
          "form": "ㅋㅋㅋ아.. 그건 좀 아쉽네요 ㅠㅠ",
          "original_form": "ㅋㅋㅋ아.. 그건 좀 아쉽네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100023124.1.21",
          "form": "혹시 목 운동에 좋은 거 알려줄 수 있어요?",
          "original_form": "혹시 목 운동에 좋은 거 알려줄 수 있어요?",
          "speaker_id": "1",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100023124.1.22",
          "form": "목운동이면 뭐 거북목 같은건가요?",
          "original_form": "목운동이면 뭐 거북목 같은건가요?",
          "speaker_id": "2",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100023124.1.23",
          "form": "네 그런거 비슷한거요",
          "original_form": "네 그런거 비슷한거요",
          "speaker_id": "1",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100023124.1.24",
          "form": "전 사실 동작 배워도 정말 힘들어 죽을거같아서 늘 내가 뭔 동작을 하는지 까먹는데",
          "original_form": "전 사실 동작 배워도 정말 힘들어 죽을거같아서 늘 내가 뭔 동작을 하는지 까먹는데",
          "speaker_id": "2",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100023124.1.25",
          "form": "허걱 죽음의 필라테스...",
          "original_form": "허걱 죽음의 필라테스...",
          "speaker_id": "1",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100023124.1.26",
          "form": "목은 아무리 어떻게 해도 시원하지가 않아요 ㅠㅠ",
          "original_form": "목은 아무리 어떻게 해도 시원하지가 않아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100023124.1.27",
          "form": "그 뒷골 근육이",
          "original_form": "그 뒷골 근육이",
          "speaker_id": "2",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100023124.1.28",
          "form": "좀 뭉쳐있으면 안좋다고",
          "original_form": "좀 뭉쳐있으면 안좋다고",
          "speaker_id": "2",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100023124.1.29",
          "form": "뒷골 근육을 풀어주시더라구요",
          "original_form": "뒷골 근육을 풀어주시더라구요",
          "speaker_id": "2",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100023124.1.30",
          "form": "어 맞아요ㅜ 딱 그런 느낌이거든요 제가",
          "original_form": "어 맞아요ㅜ 딱 그런 느낌이거든요 제가",
          "speaker_id": "1",
          "time": "20210829 22:00"
        },
        {
          "id": "MDRW2100023124.1.31",
          "form": "혹시 뒷골  근육 푸는 법 알려줄 수 있어요?",
          "original_form": "혹시 뒷골  근육 푸는 법 알려줄 수 있어요?",
          "speaker_id": "1",
          "time": "20210829 22:01"
        }
      ]
