      "utterance": [
        {
          "id": "MDRW2100009902.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 11:33"
        },
        {
          "id": "MDRW2100009902.1.2",
          "form": "네 엄청좋아요",
          "original_form": "네 엄청좋아요",
          "speaker_id": "2",
          "time": "20210823 11:34"
        },
        {
          "id": "MDRW2100009902.1.3",
          "form": "최근에 어디가셨어요?",
          "original_form": "최근에 어디가셨어요?",
          "speaker_id": "1",
          "time": "20210823 11:38"
        },
        {
          "id": "MDRW2100009902.1.4",
          "form": "전라도다녀왔어요",
          "original_form": "전라도다녀왔어요",
          "speaker_id": "2",
          "time": "20210823 11:41"
        },
        {
          "id": "MDRW2100009902.1.5",
          "form": "아 저도 예전에 내일로로 갔었는데 어디어디 가셨나요?",
          "original_form": "아 저도 예전에 내일로로 갔었는데 어디어디 가셨나요?",
          "speaker_id": "1",
          "time": "20210823 11:47"
        },
        {
          "id": "MDRW2100009902.1.6",
          "form": "진도 쏠비치 다녀왔어요",
          "original_form": "진도 쏠비치 다녀왔어요",
          "speaker_id": "2",
          "time": "20210823 11:48"
        },
        {
          "id": "MDRW2100009902.1.7",
          "form": "아 어떤 곳인지 궁금하네요!",
          "original_form": "아 어떤 곳인지 궁금하네요!",
          "speaker_id": "1",
          "time": "20210823 11:49"
        },
        {
          "id": "MDRW2100009902.1.8",
          "form": "예쁘더라고요 수영도하고싶었는데 코로나라 인피니티풀은 구경만 했어요",
          "original_form": "예쁘더라고요 수영도하고싶었는데 코로나라 인피니티풀은 구경만 했어요",
          "speaker_id": "2",
          "time": "20210823 11:50"
        },
        {
          "id": "MDRW2100009902.1.9",
          "form": "맛집도 가보셨나요?",
          "original_form": "맛집도 가보셨나요?",
          "speaker_id": "1",
          "time": "20210823 12:00"
        },
        {
          "id": "MDRW2100009902.1.10",
          "form": "유명한 곳 몇군데 가봤는데 저는 북적이는게 싫어서 나중엔 그냥 아무 곳이나 들어갔어요",
          "original_form": "유명한 곳 몇군데 가봤는데 저는 북적이는게 싫어서 나중엔 그냥 아무 곳이나 들어갔어요",
          "speaker_id": "2",
          "time": "20210823 12:09"
        },
        {
          "id": "MDRW2100009902.1.11",
          "form": "전라도는 아무데나 가도 다 맛있다고 들은거 같아요",
          "original_form": "전라도는 아무데나 가도 다 맛있다고 들은거 같아요",
          "speaker_id": "1",
          "time": "20210823 12:13"
        },
        {
          "id": "MDRW2100009902.1.12",
          "form": "맞아요 반찬도 다 평타이상이에요",
          "original_form": "맞아요 반찬도 다 평타이상이에요",
          "speaker_id": "2",
          "time": "20210823 12:13"
        },
        {
          "id": "MDRW2100009902.1.13",
          "form": "반찬 많이 나오던가요?",
          "original_form": "반찬 많이 나오던가요?",
          "speaker_id": "1",
          "time": "20210823 12:21"
        },
        {
          "id": "MDRW2100009902.1.14",
          "form": "그럼요 기본 반찬에 젓갈 나물 잘 나와요",
          "original_form": "그럼요 기본 반찬에 젓갈 나물 잘 나와요",
          "speaker_id": "2",
          "time": "20210823 12:22"
        },
        {
          "id": "MDRW2100009902.1.15",
          "form": "와 젓갈 맛있을 거 같아요",
          "original_form": "와 젓갈 맛있을 거 같아요",
          "speaker_id": "1",
          "time": "20210823 12:49"
        }
      ]
