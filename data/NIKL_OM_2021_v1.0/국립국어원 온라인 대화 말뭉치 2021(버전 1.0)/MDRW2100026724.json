      "utterance": [
        {
          "id": "MDRW2100026724.1.1",
          "form": "스위트홈 책 봤니",
          "original_form": "스위트홈 책 봤니",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026724.1.2",
          "form": "그 책 재밌게 읽었어",
          "original_form": "그 책 재밌게 읽었어",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026724.1.3",
          "form": "본 세상이 좀비가 되는 이야기지",
          "original_form": "본 세상이 좀비가 되는 이야기지",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026724.1.4",
          "form": "나는 좀비 장르가 좋더라",
          "original_form": "나는 좀비 장르가 좋더라",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026724.1.5",
          "form": "넷플릭스 드라마가 나오고 나서 책도 잘 팔린다",
          "original_form": "넷플릭스 드라마가 나오고 나서 책도 잘 팔린다",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026724.1.6",
          "form": "하긴 사람들이 원작 도 찾아보니까",
          "original_form": "하긴 사람들이 원작 도 찾아보니까",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026724.1.7",
          "form": "주인공 현 속 캐릭터에 이 입대",
          "original_form": "주인공 현 속 캐릭터에 이 입대",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100026724.1.8",
          "form": "나는 처음에 주인공이 이해가 잘 안돼 더",
          "original_form": "나는 처음에 주인공이 이해가 잘 안돼 더",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026724.1.9",
          "form": "찌질한 면이 있지",
          "original_form": "찌질한 면이 있지",
          "speaker_id": "2",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026724.1.10",
          "form": "가족 없이 혼자 사는 캐릭터라 힘들 듯",
          "original_form": "가족 없이 혼자 사는 캐릭터라 힘들 듯",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026724.1.11",
          "form": "인생 디데이까지 결정했잖아",
          "original_form": "인생 디데이까지 결정했잖아",
          "speaker_id": "2",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026724.1.12",
          "form": "그 책 읽고 인터넷이 안되면 어떨지  상상해 봤어",
          "original_form": "그 책 읽고 인터넷이 안되면 어떨지  상상해 봤어",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026724.1.13",
          "form": "헉 너무 끔찍할 것 같아",
          "original_form": "헉 너무 끔찍할 것 같아",
          "speaker_id": "2",
