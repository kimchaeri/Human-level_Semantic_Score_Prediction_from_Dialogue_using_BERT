      "utterance": [
        {
          "id": "MDRW2100047470.1.1",
          "form": "운동하시는거 있으세요?",
          "original_form": "운동하시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100047470.1.2",
          "form": "집에서 가끔 스트레칭이나 홈트 조금 해요",
          "original_form": "집에서 가끔 스트레칭이나 홈트 조금 해요",
          "speaker_id": "2",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100047470.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100047470.1.4",
          "form": "저는 가끔 산책해요",
          "original_form": "저는 가끔 산책해요",
          "speaker_id": "1",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100047470.1.5",
          "form": "지난주랑 이번주는 비가 자주 와서 산책도 못 했어요",
          "original_form": "지난주랑 이번주는 비가 자주 와서 산책도 못 했어요",
          "speaker_id": "1",
          "time": "20210826 17:15"
        },
        {
          "id": "MDRW2100047470.1.6",
          "form": "아 저도요. 비 때문에 나가질 못하네요",
          "original_form": "아 저도요. 비 때문에 나가질 못하네요",
          "speaker_id": "2",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100047470.1.7",
          "form": "저는 수영을 등록했어요~!9월부터 하려고요",
          "original_form": "저는 수영을 등록했어요~!9월부터 하려고요",
          "speaker_id": "1",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100047470.1.8",
          "form": "와 전 진짜 수영 못 해요 ㅋㅋ 전에 해보신 적 있으신가요?",
          "original_form": "와 전 진짜 수영 못 해요 ㅋㅋ 전에 해보신 적 있으신가요?",
          "speaker_id": "2",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100047470.1.9",
          "form": "아니요 이번이 처음이에요",
          "original_form": "아니요 이번이 처음이에요",
          "speaker_id": "1",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100047470.1.10",
          "form": "한 번 가면 4-50분쯤 하나요?",
          "original_form": "한 번 가면 4-50분쯤 하나요?",
          "speaker_id": "2",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100047470.1.11",
          "form": "네 50분 수업이에요",
          "original_form": "네 50분 수업이에요",
          "speaker_id": "1",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100047470.1.12",
          "form": "수영 하고나면 엄청 힘들다고 하더라구요",
          "original_form": "수영 하고나면 엄청 힘들다고 하더라구요",
          "speaker_id": "2",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100047470.1.13",
          "form": "아 진짜요? 살이 많이 빠지면 좋겠어요",
          "original_form": "아 진짜요? 살이 많이 빠지면 좋겠어요",
          "speaker_id": "1",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100047470.1.14",
          "form": "다이어트 성공 기원합니다! ㅎㅎ",
          "original_form": "다이어트 성공 기원합니다! ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100047470.1.15",
          "form": "네~열심히 해서 살 빼볼게요",
          "original_form": "네~열심히 해서 살 빼볼게요",
