      "utterance": [
        {
          "id": "MDRW2100006374.1.1",
          "form": "요즘 영양제드시나요?",
          "original_form": "요즘 영양제드시나요?",
          "speaker_id": "1",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100006374.1.2",
          "form": "전 잘안챙겨먹어요",
          "original_form": "전 잘안챙겨먹어요",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100006374.1.3",
          "form": "비타민이랑 먹으라고 주는데 먹기싫더라구요",
          "original_form": "비타민이랑 먹으라고 주는데 먹기싫더라구요",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100006374.1.4",
          "form": "드시는거 있으세요?",
          "original_form": "드시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100006374.1.5",
          "form": "전 귀찮아서 안먹는데",
          "original_form": "전 귀찮아서 안먹는데",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100006374.1.6",
          "form": "주변에 다 먹더라구요",
          "original_form": "주변에 다 먹더라구요",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100006374.1.7",
          "form": "요즘 많이들 먹더라구요",
          "original_form": "요즘 많이들 먹더라구요",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100006374.1.8",
          "form": "유산균은 락토핏 많이먹던데",
          "original_form": "유산균은 락토핏 많이먹던데",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100006374.1.9",
          "form": "유산균도 안드세요?",
          "original_form": "유산균도 안드세요?",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100006374.1.10",
          "form": "네ㅎ 언니가준거한통 먹고",
          "original_form": "네ㅎ 언니가준거한통 먹고",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100006374.1.11",
          "form": "저희애들은 알아서 챙겨먹더라구요",
          "original_form": "저희애들은 알아서 챙겨먹더라구요",
          "speaker_id": "2",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100006374.1.12",
          "form": "짧게먹어서 근가 효과를 잘 모르겠어요",
          "original_form": "짧게먹어서 근가 효과를 잘 모르겠어요",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100006374.1.13",
          "form": "40넘으니 챙기긴 해야할꺼같은데...",
          "original_form": "40넘으니 챙기긴 해야할꺼같은데...",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100006374.1.14",
          "form": "그니까요 요즘살도 엄청쪄서",
          "original_form": "그니까요 요즘살도 엄청쪄서",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100006374.1.15",
          "form": "빠질생각을 안해요",
          "original_form": "빠질생각을 안해요",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100006374.1.16",
          "form": "뭐든 3개월은 넘게 먹어야 효과를 알겠더라구요",
          "original_form": "뭐든 3개월은 넘게 먹어야 효과를 알겠더라구요",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100006374.1.17",
          "form": "나잇살은 어쩔수없는거같아요",
          "original_form": "나잇살은 어쩔수없는거같아요",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100006374.1.18",
          "form": "진짜 나잇살이 있나봐요",
          "original_form": "진짜 나잇살이 있나봐요",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100006374.1.19",
          "form": "뱃살 허벅지 옆구리",
          "original_form": "뱃살 허벅지 옆구리",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100006374.1.20",
          "form": "특히 뱃살~~  많이 나왔네요",
          "original_form": "특히 뱃살~~  많이 나왔네요",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100006374.1.21",
          "form": "다이어트는 하기싫고ㅎ 운동은 귀찮고ㅎ",
          "original_form": "다이어트는 하기싫고ㅎ 운동은 귀찮고ㅎ",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100006374.1.22",
          "form": "다들 그럴꺼에요",
          "original_form": "다들 그럴꺼에요",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100006374.1.23",
          "form": "날씨 선선해지니 걷기운동이라도 해야겠어요",
          "original_form": "날씨 선선해지니 걷기운동이라도 해야겠어요",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100006374.1.24",
          "form": "걷기운동좋죠",
          "original_form": "걷기운동좋죠",
          "speaker_id": "2",
          "time": "20210907 23:25"
        }
      ]
    }
