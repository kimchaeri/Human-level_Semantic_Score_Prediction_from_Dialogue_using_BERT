      "utterance": [
        {
          "id": "MDRW2100018797.1.1",
          "form": "해외여행도 좋아하세요?",
          "original_form": "해외여행도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 12:15"
        },
        {
          "id": "MDRW2100018797.1.2",
          "form": "그럼요! 요즘은 당연히 못가고 있지만요",
          "original_form": "그럼요! 요즘은 당연히 못가고 있지만요",
          "speaker_id": "2",
          "time": "20210827 12:17"
        },
        {
          "id": "MDRW2100018797.1.3",
          "form": "어디 가보셨어요?ㅎㅎ",
          "original_form": "어디 가보셨어요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 12:18"
        },
        {
          "id": "MDRW2100018797.1.4",
          "form": "저 대만이랑 일본 가봤어요",
          "original_form": "저 대만이랑 일본 가봤어요",
          "speaker_id": "2",
          "time": "20210827 12:18"
        },
        {
          "id": "MDRW2100018797.1.5",
          "form": "저도 일본가봤어요ㅎ",
          "original_form": "저도 일본가봤어요ㅎ",
          "speaker_id": "1",
          "time": "20210827 12:19"
        },
        {
          "id": "MDRW2100018797.1.6",
          "form": "도쿄!",
          "original_form": "도쿄!",
          "speaker_id": "1",
          "time": "20210827 12:19"
        },
        {
          "id": "MDRW2100018797.1.7",
          "form": "저는 오사카랑 후쿠오카만 가봤는데",
          "original_form": "저는 오사카랑 후쿠오카만 가봤는데",
          "speaker_id": "2",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100018797.1.8",
          "form": "도쿄는 어때요?어디가 가기 좋은가요?",
          "original_form": "도쿄는 어때요?어디가 가기 좋은가요?",
          "speaker_id": "2",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100018797.1.9",
          "form": "디즈니랜드 ?ㅎㅎ",
          "original_form": "디즈니랜드 ?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 12:21"
        },
        {
          "id": "MDRW2100018797.1.10",
          "form": "후쿠오카 온천 유명한 곳이죠?",
          "original_form": "후쿠오카 온천 유명한 곳이죠?",
          "speaker_id": "1",
          "time": "20210827 12:23"
        },
        {
          "id": "MDRW2100018797.1.11",
          "form": "와 디즈니 랜드 다녀오셨구나",
          "original_form": "와 디즈니 랜드 다녀오셨구나",
          "speaker_id": "2",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100018797.1.12",
          "form": "네 료칸 유명한 곳 맞아요",
          "original_form": "네 료칸 유명한 곳 맞아요",
          "speaker_id": "2",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100018797.1.13",
          "form": "후쿠오카도 꼭 가보고 싶어요",
          "original_form": "후쿠오카도 꼭 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210831 22:23"
        },
        {
          "id": "MDRW2100018797.1.14",
          "form": "후쿠오카 유후인 좋았어요 저는",
          "original_form": "후쿠오카 유후인 좋았어요 저는",
          "speaker_id": "2",
          "time": "20210831 22:28"
        },
        {
          "id": "MDRW2100018797.1.15",
          "form": "추천드려요",
          "original_form": "추천드려요",
          "speaker_id": "2",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100018797.1.16",
          "form": "유후인이랑 한군데 더 있죠?",
          "original_form": "유후인이랑 한군데 더 있죠?",
          "speaker_id": "1",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100018797.1.17",
          "form": "유후인이 좋군요!",
          "original_form": "유후인이 좋군요!",
          "speaker_id": "1",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100018797.1.18",
          "form": "저는 그랬어요 시골마을인데 가격대가 다비싼거만빼면요....",
          "original_form": "저는 그랬어요 시골마을인데 가격대가 다비싼거만빼면요....",
          "speaker_id": "2",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100018797.1.19",
          "form": "일본은 물가가 비싸죠ㅠㅠ 근데 우리나라도 요즘 너무 비싸서 비슷할거같기도하고ㅜㅜㅋㅋ",
          "original_form": "일본은 물가가 비싸죠ㅠㅠ 근데 우리나라도 요즘 너무 비싸서 비슷할거같기도하고ㅜㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:34"
        }
      ]
