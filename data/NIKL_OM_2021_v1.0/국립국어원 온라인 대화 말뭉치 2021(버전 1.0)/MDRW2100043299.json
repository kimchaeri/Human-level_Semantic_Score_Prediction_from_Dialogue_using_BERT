      "utterance": [
        {
          "id": "MDRW2100043299.1.1",
          "form": "포도 다 썩었어",
          "original_form": "포도 다 썩었어",
          "speaker_id": "2",
          "time": "20210907 17:07"
        },
        {
          "id": "MDRW2100043299.1.2",
          "form": "잉 드시지 좀 아까워요",
          "original_form": "잉 드시지 좀 아까워요",
          "speaker_id": "1",
          "time": "20210907 17:07"
        },
        {
          "id": "MDRW2100043299.1.3",
          "form": "니가 산건데 니가 먹었어야지",
          "original_form": "니가 산건데 니가 먹었어야지",
          "speaker_id": "2",
          "time": "20210907 17:07"
        },
        {
          "id": "MDRW2100043299.1.4",
          "form": "얼린 포도도 상했어요??",
          "original_form": "얼린 포도도 상했어요??",
          "speaker_id": "1",
          "time": "20210907 17:07"
        },
        {
          "id": "MDRW2100043299.1.5",
          "form": "얼린건 괜찮은데 그것도 너무 오래 놔두면 안되지 않을까",
          "original_form": "얼린건 괜찮은데 그것도 너무 오래 놔두면 안되지 않을까",
          "speaker_id": "2",
          "time": "20210907 17:07"
        },
        {
          "id": "MDRW2100043299.1.6",
          "form": "포도 얼린거 맛있는데... 더울때 하나씩 집어먹어요 ㅠ",
          "original_form": "포도 얼린거 맛있는데... 더울때 하나씩 집어먹어요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 17:07"
        },
        {
          "id": "MDRW2100043299.1.7",
          "form": "구슬아이스크림 같아서 맛있긴 해 근데 먹다보면 추워서 좀 그렇다",
          "original_form": "구슬아이스크림 같아서 맛있긴 해 근데 먹다보면 추워서 좀 그렇다",
          "speaker_id": "2",
          "time": "20210907 17:08"
        },
        {
          "id": "MDRW2100043299.1.8",
          "form": "디저트로 한두개씩 먹으면 괜찮지 않을까요? ㅋㅋㅋ",
          "original_form": "디저트로 한두개씩 먹으면 괜찮지 않을까요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:08"
        },
        {
          "id": "MDRW2100043299.1.9",
          "form": "니가 와서 다 먹도록 해",
          "original_form": "니가 와서 다 먹도록 해",
          "speaker_id": "2",
          "time": "20210907 17:08"
        },
        {
          "id": "MDRW2100043299.1.10",
          "form": "그때까지 방치되어 있는 청포도...",
          "original_form": "그때까지 방치되어 있는 청포도...",
          "speaker_id": "1",
          "time": "20210907 17:08"
        },
        {
          "id": "MDRW2100043299.1.11",
          "form": "청포도 아니고 적포도다 포도주 만들수도 없고 어쩌냐",
          "original_form": "청포도 아니고 적포도다 포도주 만들수도 없고 어쩌냐",
          "speaker_id": "2",
          "time": "20210907 17:09"
        },
        {
          "id": "MDRW2100043299.1.12",
          "form": "포도주스 못만드나 ㅋㅋㅋ",
          "original_form": "포도주스 못만드나 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:11"
        },
        {
          "id": "MDRW2100043299.1.13",
          "form": "무슨 껍질 먹는 포도로 포도주스를 만들어",
          "original_form": "무슨 껍질 먹는 포도로 포도주스를 만들어",
          "speaker_id": "2",
          "time": "20210907 17:12"
        },
        {
          "id": "MDRW2100043299.1.14",
          "form": "포도청 같은거 만드는건 어때요? 녹히는건 좀 그런가 다음에 만들때 해요",
          "original_form": "포도청 같은거 만드는건 어때요? 녹히는건 좀 그런가 다음에 만들때 해요",
          "speaker_id": "1",
          "time": "20210907 17:12"
        },
        {
          "id": "MDRW2100043299.1.15",
          "form": "다 청으로 만들면 맛있긴 하지",
          "original_form": "다 청으로 만들면 맛있긴 하지",
          "speaker_id": "2",
          "time": "20210907 17:12"
        },
        {
          "id": "MDRW2100043299.1.16",
          "form": "살이 쪄서 그렇지...",
          "original_form": "살이 쪄서 그렇지...",
          "speaker_id": "1",
          "time": "20210907 17:12"
        }
      ]
    }
