      "utterance": [
        {
          "id": "MDRW2100042174.1.1",
          "form": "네 여기도 비가 와여",
          "original_form": "네 여기도 비가 와여",
          "speaker_id": "2",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100042174.1.2",
          "form": "내일까지 온다더라구요",
          "original_form": "내일까지 온다더라구요",
          "speaker_id": "1",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100042174.1.3",
          "form": "아ㅠㅠ 비 싫어욧",
          "original_form": "아ㅠㅠ 비 싫어욧",
          "speaker_id": "2",
          "time": "20210906 21:25"
        },
        {
          "id": "MDRW2100042174.1.4",
          "form": "바람도 꽤 불던데요",
          "original_form": "바람도 꽤 불던데요",
          "speaker_id": "1",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100042174.1.5",
          "form": "태풍이 지나가는 걸까요?",
          "original_form": "태풍이 지나가는 걸까요?",
          "speaker_id": "1",
          "time": "20210906 21:28"
        },
        {
          "id": "MDRW2100042174.1.6",
          "form": "그런것같아요",
          "original_form": "그런것같아요",
          "speaker_id": "2",
          "time": "20210906 21:40"
        },
        {
          "id": "MDRW2100042174.1.7",
          "form": "조용히 지나갔으면~~",
          "original_form": "조용히 지나갔으면~~",
          "speaker_id": "2",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100042174.1.8",
          "form": "큰 피해 없이 지나갔으면 좋겠어요",
          "original_form": "큰 피해 없이 지나갔으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100042174.1.9",
          "form": "가을에 은근히 태풍이 올라오더라구요",
          "original_form": "가을에 은근히 태풍이 올라오더라구요",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100042174.1.10",
          "form": "이번 가을은 패스해주면 좋겠네요",
          "original_form": "이번 가을은 패스해주면 좋겠네요",
          "speaker_id": "1",
          "time": "20210906 21:41"
        },
        {
          "id": "MDRW2100042174.1.11",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042174.1.12",
          "form": "좋은점도있지만ㅠ",
          "original_form": "좋은점도있지만ㅠ",
          "speaker_id": "2",
          "time": "20210906 21:43"
        },
        {
          "id": "MDRW2100042174.1.13",
          "form": "태풍이 지나가야 바닷물이 잘 섞인다고 하더라구요",
          "original_form": "태풍이 지나가야 바닷물이 잘 섞인다고 하더라구요",
          "speaker_id": "1",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042174.1.14",
          "form": "바닷쪽으로만 잘 지나갔으면 좋겠어요",
          "original_form": "바닷쪽으로만 잘 지나갔으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210906 21:46"
        },
        {
          "id": "MDRW2100042174.1.15",
          "form": "바다에는 좋더라구요",
          "original_form": "바다에는 좋더라구요",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042174.1.16",
          "form": "바다로 가거라",
          "original_form": "바다로 가거라",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042174.1.17",
          "form": "태풍도 나름의 역할이 있는데 사람에게만 피해를 안주면 좋을텐데요",
          "original_form": "태풍도 나름의 역할이 있는데 사람에게만 피해를 안주면 좋을텐데요",
          "speaker_id": "1",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042174.1.18",
          "form": "특히 가을에 수확전 태풍은 일년 농사를 망치는 거잖아요",
          "original_form": "특히 가을에 수확전 태풍은 일년 농사를 망치는 거잖아요",
          "speaker_id": "1",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042174.1.19",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042174.1.20",
          "form": "공기가 맑아지는것좋지만",
          "original_form": "공기가 맑아지는것좋지만",
          "speaker_id": "2",
          "time": "20210906 22:02"
        },
        {
          "id": "MDRW2100042174.1.21",
          "form": "올해 추석이 좀 이른편이라 그 전에 과일같은게 잘 나와줄지는 모르겠지만",
          "original_form": "올해 추석이 좀 이른편이라 그 전에 과일같은게 잘 나와줄지는 모르겠지만",
          "speaker_id": "1",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042174.1.22",
          "form": "일년 농사지은 것들 잘 출하되었으면 좋겠어요",
          "original_form": "일년 농사지은 것들 잘 출하되었으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042174.1.23",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "2",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100042174.1.24",
          "form": "과일이 많이 비쌀듯요",
          "original_form": "과일이 많이 비쌀듯요",
          "speaker_id": "2",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100042174.1.25",
          "form": "그러니까요",
          "original_form": "그러니까요",
