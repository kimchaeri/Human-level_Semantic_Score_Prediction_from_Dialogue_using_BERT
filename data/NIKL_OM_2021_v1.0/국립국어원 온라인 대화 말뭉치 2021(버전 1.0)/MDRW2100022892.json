      "utterance": [
        {
          "id": "MDRW2100022892.1.1",
          "form": "아르바이트 많이 해보셨나요?",
          "original_form": "아르바이트 많이 해보셨나요?",
          "speaker_id": "2",
          "time": "20210829 19:01"
        },
        {
          "id": "MDRW2100022892.1.2",
          "form": "대학 1~2학년때 많이 해봤죠",
          "original_form": "대학 1~2학년때 많이 해봤죠",
          "speaker_id": "1",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100022892.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210829 19:03"
        },
        {
          "id": "MDRW2100022892.1.4",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "2",
          "time": "20210829 19:07"
        },
        {
          "id": "MDRW2100022892.1.5",
          "form": "공장을 제일 많이 가봤네요ㅋㅋㅋ",
          "original_form": "공장을 제일 많이 가봤네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:07"
        },
        {
          "id": "MDRW2100022892.1.6",
          "form": "공장이요?? ㅋㅋㅋ 공장에서 뭐해요??",
          "original_form": "공장이요?? ㅋㅋㅋ 공장에서 뭐해요??",
          "speaker_id": "1",
          "time": "20210829 19:08"
        },
        {
          "id": "MDRW2100022892.1.7",
          "form": "약품이나 화장품 와인 포장이요ㅋㅋㅋㅋ",
          "original_form": "약품이나 화장품 와인 포장이요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:11"
        },
        {
          "id": "MDRW2100022892.1.8",
          "form": "name1님은 어떤 알바 하셨어요?",
          "original_form": "&name1&님은 어떤 알바 하셨어요?",
          "speaker_id": "2",
          "time": "20210829 19:11"
        },
        {
          "id": "MDRW2100022892.1.9",
          "form": "오 포장 알바 신기하네요",
          "original_form": "오 포장 알바 신기하네요",
          "speaker_id": "1",
          "time": "20210829 19:45"
        },
        {
          "id": "MDRW2100022892.1.10",
          "form": "저는 식당 주방이랑 올리브영이요",
          "original_form": "저는 식당 주방이랑 올리브영이요",
          "speaker_id": "1",
          "time": "20210829 19:45"
        },
        {
          "id": "MDRW2100022892.1.11",
          "form": "오오 식당은 해봤는데 올리브영은 어떤지 궁금하네요",
          "original_form": "오오 식당은 해봤는데 올리브영은 어떤지 궁금하네요",
          "speaker_id": "2",
          "time": "20210829 19:52"
        },
        {
          "id": "MDRW2100022892.1.12",
          "form": "올리브영 좀 혜자예요ㅋㅋ",
          "original_form": "올리브영 좀 혜자예요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:00"
        },
        {
          "id": "MDRW2100022892.1.13",
          "form": "주방에서 알바하고나서 음식냄새에 학을 떼가지고 서비스직 갔는데",
          "original_form": "주방에서 알바하고나서 음식냄새에 학을 떼가지고 서비스직 갔는데",
          "speaker_id": "1",
          "time": "20210829 20:00"
        },
        {
          "id": "MDRW2100022892.1.14",
          "form": "대기업이라 그런가 수당 알아서 딱딱 챙겨주고 쾌적하고",
          "original_form": "대기업이라 그런가 수당 알아서 딱딱 챙겨주고 쾌적하고",
          "speaker_id": "1",
          "time": "20210829 20:01"
        },
        {
          "id": "MDRW2100022892.1.15",
          "form": "간식비도 주고 ㅋㅋ 이게 또 은근 좋더라구요",
          "original_form": "간식비도 주고 ㅋㅋ 이게 또 은근 좋더라구요",
          "speaker_id": "1",
          "time": "20210829 20:01"
        },
        {
          "id": "MDRW2100022892.1.16",
          "form": "오..ㅋㅋㅋ",
          "original_form": "오..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:10"
        },
        {
          "id": "MDRW2100022892.1.17",
          "form": "진짜 괜찮네요ㅋㅋ",
          "original_form": "진짜 괜찮네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:10"
        },
        {
          "id": "MDRW2100022892.1.18",
          "form": "저는 카페 알바하고싶었는데 주변에서 구하는곳은 경력만 구해서 못해봤어요ㅠㅜ",
          "original_form": "저는 카페 알바하고싶었는데 주변에서 구하는곳은 경력만 구해서 못해봤어요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210829 20:11"
        },
        {
          "id": "MDRW2100022892.1.19",
          "form": "맞아요 카페알바은 다 그렇더라구요..",
          "original_form": "맞아요 카페알바은 다 그렇더라구요..",
          "speaker_id": "1",
          "time": "20210829 21:07"
        },
        {
          "id": "MDRW2100022892.1.20",
          "form": "근데 알바 하다보면 다른거 하기에 시간이 부족한 느낌이라 안한지 좀 됐어요ㅎㅎ",
          "original_form": "근데 알바 하다보면 다른거 하기에 시간이 부족한 느낌이라 안한지 좀 됐어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:08"
        },
        {
          "id": "MDRW2100022892.1.21",
          "form": "아ㅎㅎ 그건 그래요",
          "original_form": "아ㅎㅎ 그건 그래요",
          "speaker_id": "2",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100022892.1.22",
          "form": "저 저번에 나라에서 코로나방역 했었는데 그런 알바면 할만하더라구요",
          "original_form": "저 저번에 나라에서 코로나방역 했었는데 그런 알바면 할만하더라구요",
          "speaker_id": "2",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100022892.1.23",
          "form": "그건 뭐 하는거예요?",
          "original_form": "그건 뭐 하는거예요?",
          "speaker_id": "1",
          "time": "20210830 00:05"
        },
        {
          "id": "MDRW2100022892.1.24",
          "form": "열 체크나 소독약 뿌리는 일이었어요!",
          "original_form": "열 체크나 소독약 뿌리는 일이었어요!",
          "speaker_id": "2",
          "time": "20210830 00:27"
        }
      ]
