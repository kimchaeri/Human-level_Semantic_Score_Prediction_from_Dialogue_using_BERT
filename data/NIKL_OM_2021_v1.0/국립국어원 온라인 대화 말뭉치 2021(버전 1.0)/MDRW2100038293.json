      "utterance": [
        {
          "id": "MDRW2100038293.1.1",
          "form": "점심은 뭐 드실건가요",
          "original_form": "점심은 뭐 드실건가요",
          "speaker_id": "2",
          "time": "20210902 08:49"
        },
        {
          "id": "MDRW2100038293.1.2",
          "form": "떡볶이 땡기네요 ㅋㅋ",
          "original_form": "떡볶이 땡기네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:17"
        },
        {
          "id": "MDRW2100038293.1.3",
          "form": "어떤 떡볶이 좋아하세요?",
          "original_form": "어떤 떡볶이 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 09:18"
        },
        {
          "id": "MDRW2100038293.1.4",
          "form": "요샌 로제 떡볶이 좋아해요:)",
          "original_form": "요샌 로제 떡볶이 좋아해요:)",
          "speaker_id": "1",
          "time": "20210902 09:23"
        },
        {
          "id": "MDRW2100038293.1.5",
          "form": "떡볶이 좋아하세용?",
          "original_form": "떡볶이 좋아하세용?",
          "speaker_id": "1",
          "time": "20210902 09:23"
        },
        {
          "id": "MDRW2100038293.1.6",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210902 09:24"
        },
        {
          "id": "MDRW2100038293.1.7",
          "form": "엽떡 로제 사랑합니다",
          "original_form": "엽떡 로제 사랑합니다",
          "speaker_id": "2",
          "time": "20210902 09:24"
        },
        {
          "id": "MDRW2100038293.1.8",
          "form": "크으 사랑이쥬",
          "original_form": "크으 사랑이쥬{emoji:♡}{emoji:♡}",
          "speaker_id": "1",
          "time": "20210902 09:24"
        },
        {
          "id": "MDRW2100038293.1.9",
          "form": "혹시 달쫀도 아세요?",
          "original_form": "혹시 달쫀도 아세요?",
          "speaker_id": "1",
          "time": "20210902 09:24"
        },
        {
          "id": "MDRW2100038293.1.10",
          "form": "아니요 처음 들어봐요",
          "original_form": "아니요 처음 들어봐요",
          "speaker_id": "2",
          "time": "20210902 09:24"
        },
        {
          "id": "MDRW2100038293.1.11",
          "form": "달쫀도 체인인데\r\n아시는 분을 못만나봤네여 ㅠㅋ",
          "original_form": "달쫀도 체인인데\r\n아시는 분을 못만나봤네여 ㅠㅋ",
          "speaker_id": "1",
          "time": "20210902 09:33"
        },
        {
          "id": "MDRW2100038293.1.12",
          "form": "저희 집 주변에는 없는거로 나오네요",
          "original_form": "저희 집 주변에는 없는거로 나오네요",
          "speaker_id": "2",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100038293.1.13",
          "form": "친구가 저희집 근처에서\r\n달쫀매장 운영을 해서\r\n저는 거기서 많이 시켜먹어요 ㅋㅋㅈ",
          "original_form": "친구가 저희집 근처에서\r\n달쫀매장 운영을 해서\r\n저는 거기서 많이 시켜먹어요 ㅋㅋㅈ",
          "speaker_id": "1",
          "time": "20210902 09:40"
        },
        {
          "id": "MDRW2100038293.1.14",
          "form": "엌ㅋㅋ 친구가 운영하면 진짜 편하겠네요",
          "original_form": "엌ㅋㅋ 친구가 운영하면 진짜 편하겠네요",
          "speaker_id": "2",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100038293.1.15",
          "form": "친구 가게 매상 올려주니 좋고\r\n저도 맛있는 떡볶ㅇㅣ 먹어서 좋고 ㅋㅋ",
          "original_form": "친구 가게 매상 올려주니 좋고\r\n저도 맛있는 떡볶ㅇㅣ 먹어서 좋고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:46"
        },
        {
          "id": "MDRW2100038293.1.16",
          "form": "로제시키면\r\n베이컨이랑 양송이버섯이랑\r\n아낌없이 들어가 있어서",
          "original_form": "로제시키면\r\n베이컨이랑 양송이버섯이랑\r\n아낌없이 들어가 있어서",
          "speaker_id": "1",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100038293.1.17",
          "form": "맛없을수가없는 비주얼이에여ㅋ",
          "original_form": "맛없을수가없는 비주얼이에여ㅋ",
          "speaker_id": "1",
          "time": "20210902 09:49"
        },
        {
          "id": "MDRW2100038293.1.18",
          "form": "으어 저도 먹어보고 싶네요",
          "original_form": "으어 저도 먹어보고 싶네요",
          "speaker_id": "2",
          "time": "20210902 09:53"
        },
        {
          "id": "MDRW2100038293.1.19",
          "form": "달쫀도 맛있는데\r\n왜 없는걸까요ㅜㅜ 아쉽",
          "original_form": "달쫀도 맛있는데\r\n왜 없는걸까요ㅜㅜ 아쉽",
          "speaker_id": "1",
          "time": "20210902 09:56"
        },
        {
          "id": "MDRW2100038293.1.20",
          "form": "주변에 꼭 생겼으면 좋겠어요",
          "original_form": "주변에 꼭 생겼으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210902 09:56"
        },
        {
          "id": "MDRW2100038293.1.21",
          "form": "ㅋㅋ꼭 드셔보셨음 좋겠어요",
          "original_form": "ㅋㅋ꼭 드셔보셨음 좋겠어요{emoji:♡}",
          "speaker_id": "1",
