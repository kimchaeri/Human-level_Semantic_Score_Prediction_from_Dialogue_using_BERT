        {
          "id": "MDRW2100021233.1.1",
          "form": "네 덥네요~ㅠㅠ",
          "original_form": "네 덥네요~ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021233.1.2",
          "form": "너무요",
          "original_form": "너무요",
          "speaker_id": "1",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100021233.1.3",
          "form": "그래도 여름은 조금씩 가는거같아요",
          "original_form": "그래도 여름은 조금씩 가는거같아요",
          "speaker_id": "2",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100021233.1.4",
          "form": "밤되면 시원하더라고요~",
          "original_form": "밤되면 시원하더라고요~",
          "speaker_id": "2",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100021233.1.5",
          "form": "창문닫아요",
          "original_form": "창문닫아요",
          "speaker_id": "1",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021233.1.6",
          "form": "아 밤에 창문 닫으시나요?",
          "original_form": "아 밤에 창문 닫으시나요?",
          "speaker_id": "2",
          "time": "20210828 17:09"
        },
        {
          "id": "MDRW2100021233.1.7",
          "form": "내 추워요",
          "original_form": "내 추워요",
          "speaker_id": "1",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100021233.1.8",
          "form": "아하 맞아요 좀 춥기두하죠",
          "original_form": "아하 맞아요 좀 춥기두하죠",
          "speaker_id": "2",
          "time": "20210828 17:27"
        },
        {
          "id": "MDRW2100021233.1.9",
          "form": "감기걸릴ㄲ봐",
          "original_form": "감기걸릴ㄲ봐",
          "speaker_id": "1",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100021233.1.10",
          "form": "맞아요 가을철이 다가올땐",
          "original_form": "맞아요 가을철이 다가올땐",
          "speaker_id": "2",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100021233.1.11",
          "form": "환절기 조심해야죠",
          "original_form": "환절기 조심해야죠",
          "speaker_id": "2",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100021233.1.12",
          "form": "그르니까요",
          "original_form": "그르니까요",
          "speaker_id": "1",
          "time": "20210828 19:42"
        },
        {
          "id": "MDRW2100021233.1.13",
          "form": "딱 이때가",
          "original_form": "딱 이때가",
          "speaker_id": "2",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100021233.1.14",
