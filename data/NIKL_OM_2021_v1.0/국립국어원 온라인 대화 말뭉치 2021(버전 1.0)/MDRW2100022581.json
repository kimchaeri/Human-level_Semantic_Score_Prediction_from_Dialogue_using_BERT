      "utterance": [
        {
          "id": "MDRW2100022581.1.1",
          "form": "음 저도 최근에 이게 다인거같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "음 저도 최근에 이게 다인거같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:58"
        },
        {
          "id": "MDRW2100022581.1.2",
          "form": "ㅋㅋ크흐",
          "original_form": "ㅋㅋ크흐",
          "speaker_id": "1",
          "time": "20210829 15:58"
        },
        {
          "id": "MDRW2100022581.1.3",
          "form": "저 저번에 빕스 할인해서 다녀왔어요",
          "original_form": "저 저번에 빕스 할인해서 다녀왔어요",
          "speaker_id": "1",
          "time": "20210829 15:58"
        },
        {
          "id": "MDRW2100022581.1.4",
          "form": "오 빕스 오랜만이네요",
          "original_form": "오 빕스 오랜만이네요",
          "speaker_id": "2",
          "time": "20210829 15:59"
        },
        {
          "id": "MDRW2100022581.1.5",
          "form": "매달 티맴버십에 꼭 빕스 들어가있는 것 같더라구요",
          "original_form": "매달 티맴버십에 꼭 빕스 들어가있는 것 같더라구요",
          "speaker_id": "1",
          "time": "20210829 15:59"
        },
        {
          "id": "MDRW2100022581.1.6",
          "form": "얼마나 할인해요?",
          "original_form": "얼마나 할인해요?",
          "speaker_id": "2",
          "time": "20210829 15:59"
        },
        {
          "id": "MDRW2100022581.1.7",
          "form": "40%요!!",
          "original_form": "40%요!!",
          "speaker_id": "1",
          "time": "20210829 15:59"
        },
        {
          "id": "MDRW2100022581.1.8",
          "form": "근데 할인한도가 있어요 4만원까진가?",
          "original_form": "근데 할인한도가 있어요 4만원까진가?",
          "speaker_id": "1",
          "time": "20210829 15:59"
        },
        {
          "id": "MDRW2100022581.1.9",
          "form": "대박 많이하네요!! 티멤버십할인 자주봐야겠어요",
          "original_form": "대박 많이하네요!! 티멤버십할인 자주봐야겠어요",
          "speaker_id": "2",
          "time": "20210829 15:59"
        },
        {
          "id": "MDRW2100022581.1.10",
          "form": "ㅋㅋㅋ그거 할인하는 주간이 따로 있어요",
          "original_form": "ㅋㅋㅋ그거 할인하는 주간이 따로 있어요",
          "speaker_id": "1",
          "time": "20210829 15:59"
        },
        {
          "id": "MDRW2100022581.1.11",
          "form": "9월은 언제인지는 잘 모르겠네요 ㅜㅜ",
          "original_form": "9월은 언제인지는 잘 모르겠네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 15:59"
        },
        {
          "id": "MDRW2100022581.1.12",
          "form": "오그렇군요 담달에봐야겟어용",
          "original_form": "오그렇군요 담달에봐야겟어용",
          "speaker_id": "2",
          "time": "20210829 15:59"
        },
        {
          "id": "MDRW2100022581.1.13",
          "form": "예전에 아웃백도 할인 많이하더라구요",
          "original_form": "예전에 아웃백도 할인 많이하더라구요",
          "speaker_id": "2",
          "time": "20210829 15:59"
        },
        {
          "id": "MDRW2100022581.1.14",
          "form": "네네 아마 8월 30일이나 31일쯤 나올거에요~",
          "original_form": "네네 아마 8월 30일이나 31일쯤 나올거에요~",
          "speaker_id": "1",
          "time": "20210829 15:59"
        },
        {
          "id": "MDRW2100022581.1.15",
          "form": "아웃백도 할인 자주하는 편이에요?",
          "original_form": "아웃백도 할인 자주하는 편이에요?",
          "speaker_id": "1",
          "time": "20210829 15:59"
        },
        {
          "id": "MDRW2100022581.1.16",
          "form": "그 생일쿠폰이런거랑 뭐되게 중복으로 할인되나봐요!",
          "original_form": "그 생일쿠폰이런거랑 뭐되게 중복으로 할인되나봐요!",
          "speaker_id": "2",
          "time": "20210829 16:00"
        },
        {
          "id": "MDRW2100022581.1.17",
          "form": "근데 아웃백은 혼자 가기 뭐하더라구요 ㅜㅜ....",
          "original_form": "근데 아웃백은 혼자 가기 뭐하더라구요 ㅜㅜ....",
          "speaker_id": "1",
          "time": "20210829 16:00"
        },
        {
          "id": "MDRW2100022581.1.18",
          "form": "빕스는 혼자가도 눈치 안보이는데 힝",
          "original_form": "빕스는 혼자가도 눈치 안보이는데 힝",
          "speaker_id": "1",
          "time": "20210829 16:00"
        },
        {
          "id": "MDRW2100022581.1.19",
          "form": "블로그같은데에 싸게먹는방법이 돌아다녀요 ㅋㅋㅋㅋ",
          "original_form": "블로그같은데에 싸게먹는방법이 돌아다녀요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:00"
        },
        {
          "id": "MDRW2100022581.1.20",
          "form": "와 ㅋㅋ 역시 블로거들 할인소식 빠삭행",
          "original_form": "와 ㅋㅋ 역시 블로거들 할인소식 빠삭행",
          "speaker_id": "1",
          "time": "20210829 16:00"
        },
        {
          "id": "MDRW2100022581.1.21",
          "form": "얼마전 누나가 생일이여서 빕스 생일쿠폰 달라했는데 2인이상 식사가 조건이더군요 ㅜㅜ",
          "original_form": "얼마전 누나가 생일이여서 빕스 생일쿠폰 달라했는데 2인이상 식사가 조건이더군요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 16:01"
        },
        {
          "id": "MDRW2100022581.1.22",
          "form": "아이고 할인같은거 조건이 은근 빡세더라구요",
