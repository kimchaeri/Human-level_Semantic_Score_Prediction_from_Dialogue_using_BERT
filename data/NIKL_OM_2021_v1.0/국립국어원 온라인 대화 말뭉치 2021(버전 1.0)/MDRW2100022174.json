      "utterance": [
        {
          "id": "MDRW2100022174.1.1",
          "form": "무슨 팩?",
          "original_form": "무슨 팩?",
          "speaker_id": "2",
          "time": "20210829 10:29"
        },
        {
          "id": "MDRW2100022174.1.2",
          "form": "고무팩 진주",
          "original_form": "고무팩 진주",
          "speaker_id": "1",
          "time": "20210829 10:30"
        },
        {
          "id": "MDRW2100022174.1.3",
          "form": "ㅎㅎ 얼굴이 진주처럼 반짝반짝해지겠네",
          "original_form": "ㅎㅎ 얼굴이 진주처럼 반짝반짝해지겠네",
          "speaker_id": "2",
          "time": "20210829 10:30"
        },
        {
          "id": "MDRW2100022174.1.4",
          "form": "ㅇㅇ 근데 주름은 자글자글 ㅋㅋ",
          "original_form": "ㅇㅇ 근데 주름은 자글자글 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:30"
        },
        {
          "id": "MDRW2100022174.1.5",
          "form": "헉.. 여러번 해야 효과 있지",
          "original_form": "헉.. 여러번 해야 효과 있지",
          "speaker_id": "2",
          "time": "20210829 10:30"
        },
        {
          "id": "MDRW2100022174.1.6",
          "form": "고무팩이 귀찮아서 잘 안하게 되네",
          "original_form": "고무팩이 귀찮아서 잘 안하게 되네",
          "speaker_id": "1",
          "time": "20210829 10:31"
        },
        {
          "id": "MDRW2100022174.1.7",
          "form": "아무래도 마스크팩처럼 간단하지가 않으니",
          "original_form": "아무래도 마스크팩처럼 간단하지가 않으니",
          "speaker_id": "2",
          "time": "20210829 10:31"
        },
        {
          "id": "MDRW2100022174.1.8",
          "form": "일단 물에 개어야 하고 또 뒷처리가 아주 불편해 ㅎㅎ",
          "original_form": "일단 물에 개어야 하고 또 뒷처리가 아주 불편해 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 10:31"
        },
        {
          "id": "MDRW2100022174.1.9",
          "form": "고무팩 찌꺼기도 잘 안떨어지더라",
          "original_form": "고무팩 찌꺼기도 잘 안떨어지더라",
          "speaker_id": "2",
          "time": "20210829 10:32"
        },
        {
          "id": "MDRW2100022174.1.10",
          "form": "맞아 얼굴에서 떼어내기도 힘들고",
          "original_form": "맞아 얼굴에서 떼어내기도 힘들고",
          "speaker_id": "1",
          "time": "20210829 10:32"
        },
        {
          "id": "MDRW2100022174.1.11",
          "form": "그럼 마스크팩을 사",
          "original_form": "그럼 마스크팩을 사",
          "speaker_id": "2",
          "time": "20210829 10:32"
        },
        {
          "id": "MDRW2100022174.1.12",
          "form": "고무팩 남은 게 너무 많아서 ㅠㅠ",
          "original_form": "고무팩 남은 게 너무 많아서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 10:32"
        },
        {
          "id": "MDRW2100022174.1.13",
          "form": "얼마나 남았는데?",
          "original_form": "얼마나 남았는데?",
          "speaker_id": "2",
          "time": "20210829 10:32"
        },
        {
          "id": "MDRW2100022174.1.14",
          "form": "큰 거 두봉다리나 남았어 ㅎㅎ",
          "original_form": "큰 거 두봉다리나 남았어 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 10:33"
        },
        {
          "id": "MDRW2100022174.1.15",
          "form": "한 번 할 때 팍팍 써 ㅋㅋ",
          "original_form": "한 번 할 때 팍팍 써 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 10:33"
        },
        {
          "id": "MDRW2100022174.1.16",
          "form": "귀찮아서 안했는데 이제 꾸준히 해봐야겠다",
          "original_form": "귀찮아서 안했는데 이제 꾸준히 해봐야겠다",
          "speaker_id": "1",
          "time": "20210829 10:33"
        },
        {
          "id": "MDRW2100022174.1.17",
          "form": "비 안올 때 애들 줄넘기 좀 부탁해",
          "original_form": "비 안올 때 애들 줄넘기 좀 부탁해",
