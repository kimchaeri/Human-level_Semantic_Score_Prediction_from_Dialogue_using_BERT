      "utterance": [
        {
          "id": "MDRW2100030811.1.1",
          "form": "네 꾸준히 읽어요",
          "original_form": "네 꾸준히 읽어요",
          "speaker_id": "2",
          "time": "20210904 13:07"
        },
        {
          "id": "MDRW2100030811.1.2",
          "form": "네 최근 읽으신 것중 기억에 남는 건요?",
          "original_form": "네 최근 읽으신 것중 기억에 남는 건요?",
          "speaker_id": "1",
          "time": "20210904 13:10"
        },
        {
          "id": "MDRW2100030811.1.3",
          "form": "어떻게 말해줄까요",
          "original_form": "어떻게 말해줄까요",
          "speaker_id": "2",
          "time": "20210904 13:11"
        },
        {
          "id": "MDRW2100030811.1.4",
          "form": "? 전 안전마진읽었어요",
          "original_form": "? 전 안전마진읽었어요",
          "speaker_id": "1",
          "time": "20210904 13:19"
        },
        {
          "id": "MDRW2100030811.1.5",
          "form": "넹 안전마진이면 경제책인가봐요",
          "original_form": "넹 안전마진이면 경제책인가봐요",
          "speaker_id": "2",
          "time": "20210904 13:20"
        },
        {
          "id": "MDRW2100030811.1.6",
          "form": "헉 네 어찌아셨어요",
          "original_form": "헉 네 어찌아셨어요",
          "speaker_id": "1",
          "time": "20210904 13:26"
        },
        {
          "id": "MDRW2100030811.1.7",
          "form": "마진이라는 낱말 때문에요",
          "original_form": "마진이라는 낱말 때문에요",
          "speaker_id": "2",
          "time": "20210904 13:27"
        },
        {
          "id": "MDRW2100030811.1.8",
          "form": "음 마진은 상업용어인데...",
          "original_form": "음 마진은 상업용어인데...",
          "speaker_id": "1",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030811.1.9",
          "form": "그런가요 제가 자세히는 몰라서. .",
          "original_form": "그런가요 제가 자세히는 몰라서. .",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030811.1.10",
          "form": "네 잘 아시는줄..",
          "original_form": "네 잘 아시는줄..",
          "speaker_id": "1",
          "time": "20210904 13:41"
        },
        {
          "id": "MDRW2100030811.1.11",
          "form": "넹 요즘은 시집 읽어요",
          "original_form": "넹 요즘은 시집 읽어요",
          "speaker_id": "2",
          "time": "20210904 13:42"
        },
        {
          "id": "MDRW2100030811.1.12",
          "form": "오호 저는ㅈ경제만",
          "original_form": "오호 저는ㅈ경제만",
          "speaker_id": "1",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100030811.1.13",
          "form": "제가 첨 읽은 시집은 윤동주 시집이에요",
          "original_form": "제가 첨 읽은 시집은 윤동주 시집이에요",
          "speaker_id": "2",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100030811.1.14",
          "form": "헉 고딩필독서..",
          "original_form": "헉 고딩필독서..",
          "speaker_id": "1",
          "time": "20210904 15:09"
        }
