      "utterance": [
        {
          "id": "MDRW2100015288.1.1",
          "form": "카페 좋아하세요?",
          "original_form": "카페 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015288.1.2",
          "form": "완전 좋아해요",
          "original_form": "완전 좋아해요",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015288.1.3",
          "form": "제일 좋아하는 프렌차이즈는요?",
          "original_form": "제일 좋아하는 프렌차이즈는요?",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015288.1.4",
          "form": "스벅 제일 많이 가요",
          "original_form": "스벅 제일 많이 가요",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015288.1.5",
          "form": "헉 저도 스벅 많이 가요 ㅋㅋㅋ",
          "original_form": "헉 저도 스벅 많이 가요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015288.1.6",
          "form": "쿠폰도 자주 주고 별쌓이는게 좋더라구요",
          "original_form": "쿠폰도 자주 주고 별쌓이는게 좋더라구요",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015288.1.7",
          "form": "왜 저는 쿠폰을 안주죠?ㅠㅠ",
          "original_form": "왜 저는 쿠폰을 안주죠?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015288.1.8",
          "form": "깔끔하고 간섭도 안하고 ㅋㅋㅋ",
          "original_form": "깔끔하고 간섭도 안하고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015288.1.9",
          "form": "쿠폰 ㅋㅋ생일쿠폰은 받으시죠?",
          "original_form": "쿠폰 ㅋㅋ생일쿠폰은 받으시죠?",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015288.1.10",
          "form": "어떤 메뉴를 제일 좋아하세요?",
          "original_form": "어떤 메뉴를 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015288.1.11",
          "form": "생일쿠폰 필수죠 ㅋㅋ",
          "original_form": "생일쿠폰 필수죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015288.1.12",
          "form": "저는 프라푸치노 도 좋아하는데",
          "original_form": "저는 프라푸치노 도 좋아하는데",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015288.1.13",
          "form": "콜드브루메뉴도 잘먹어요",
          "original_form": "콜드브루메뉴도 잘먹어요",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015288.1.14",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015288.1.15",
          "form": "오~ 비싼 거 좋아하시구나.",
          "original_form": "오~ 비싼 거 좋아하시구나.",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015288.1.16",
          "form": "전 최애메뉴가 자바칩 프라푸치노에요 ㅋㅋ",
          "original_form": "전 최애메뉴가 자바칩 프라푸치노에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015288.1.17",
          "form": "자바칩 스테디죠",
          "original_form": "자바칩 스테디죠",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015288.1.18",
          "form": "진짜 맛있음",
          "original_form": "진짜 맛있음",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015288.1.19",
          "form": "콜드브루 메뉴 중에선 돌체라떼 괜찮은 것 같아요",
          "original_form": "콜드브루 메뉴 중에선 돌체라떼 괜찮은 것 같아요",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015288.1.20",
          "form": "진짜 맞아요",
          "original_form": "진짜 맞아요",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015288.1.21",
          "form": "돌체 최고존엄",
          "original_form": "돌체 최고존엄",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015288.1.22",
          "form": "자허블도 맛있어요",
          "original_form": "자허블도 맛있어요",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015288.1.23",
          "form": "그거 먹으면 설사한단 사람 있다던데 전 멀쩡하던데용",
          "original_form": "그거 먹으면 설사한단 사람 있다던데 전 멀쩡하던데용",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015288.1.24",
          "form": "유당불내증이 없나봐요..",
          "original_form": "유당불내증이 없나봐요..",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015288.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ축복이네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ축복이네요",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015288.1.26",
          "form": "자허블이 뭐죠 자몽허니블렌드?",
          "original_form": "자허블이 뭐죠 자몽허니블렌드?",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015288.1.27",
          "form": "저도 장이 튼튼한가 괜찮아서",
          "original_form": "저도 장이 튼튼한가 괜찮아서",
          "speaker_id": "2",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015288.1.28",
          "form": "잘먹어요",
          "original_form": "잘먹어요",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015288.1.29",
          "form": "자몽허니 블랙티요",
