      "utterance": [
        {
          "id": "MDRW2100032280.1.1",
          "form": "오늘 안에 나갈 수 있을까요",
          "original_form": "오늘 안에 나갈 수 있을까요",
          "speaker_id": "1",
          "time": "20210905 14:49"
        },
        {
          "id": "MDRW2100032280.1.2",
          "form": "비 안 온다면서요",
          "original_form": "비 안 온다면서요",
          "speaker_id": "2",
          "time": "20210905 14:49"
        },
        {
          "id": "MDRW2100032280.1.3",
          "form": "여긴 비 오니까 못 나갈 핑계라도 있지",
          "original_form": "여긴 비 오니까 못 나갈 핑계라도 있지",
          "speaker_id": "2",
          "time": "20210905 14:49"
        },
        {
          "id": "MDRW2100032280.1.4",
          "form": "거기는 게으름 뿐이군요",
          "original_form": "거기는 게으름 뿐이군요",
          "speaker_id": "2",
          "time": "20210905 14:49"
        },
        {
          "id": "MDRW2100032280.1.5",
          "form": "올 것 같은 날씨인가 봐요 고모가 우산 챙겨서 나 가시더라고요",
          "original_form": "올 것 같은 날씨인가 봐요 고모가 우산 챙겨서 나 가시더라고요",
          "speaker_id": "1",
          "time": "20210905 14:49"
        },
        {
          "id": "MDRW2100032280.1.6",
          "form": "비 오기 전에 후딱 가서",
          "original_form": "비 오기 전에 후딱 가서",
          "speaker_id": "2",
          "time": "20210905 14:49"
        },
        {
          "id": "MDRW2100032280.1.7",
          "form": "그쪽 게임 게으름도 만만치 않습니다",
          "original_form": "그쪽 게임 게으름도 만만치 않습니다",
          "speaker_id": "1",
          "time": "20210905 14:49"
        },
        {
          "id": "MDRW2100032280.1.8",
          "form": "소주도 사고",
          "original_form": "소주도 사고",
          "speaker_id": "2",
          "time": "20210905 14:49"
        },
        {
          "id": "MDRW2100032280.1.9",
          "form": "아마스빈도 가고",
          "original_form": "아마스빈도 가고",
          "speaker_id": "2",
          "time": "20210905 14:49"
        },
        {
          "id": "MDRW2100032280.1.10",
          "form": "씻어야 하잖아요 쇼핑의 길은 멀고도 험난하다군요",
          "original_form": "씻어야 하잖아요 쇼핑의 길은 멀고도 험난하다군요",
          "speaker_id": "1",
          "time": "20210905 14:49"
        },
        {
          "id": "MDRW2100032280.1.11",
          "form": "올리브영 가시죠",
          "original_form": "올리브영 가시죠",
          "speaker_id": "2",
          "time": "20210905 14:50"
        },
        {
          "id": "MDRW2100032280.1.12",
          "form": "1타3피지요",
          "original_form": "1타3피지요",
          "speaker_id": "2",
          "time": "20210905 14:50"
        },
        {
          "id": "MDRW2100032280.1.13",
          "form": "쇼핑을 하기 위해서는 씻어야 해요 하지만 있는 거는 정말 힘들어요 일어나야 하기 때문이죠",
          "original_form": "쇼핑을 하기 위해서는 씻어야 해요 하지만 있는 거는 정말 힘들어요 일어나야 하기 때문이죠",
          "speaker_id": "1",
          "time": "20210905 14:50"
        },
        {
          "id": "MDRW2100032280.1.14",
          "form": "화방에 가서 볼펜을 꼭 사 오고 싶긴 해요 언제 준비를 해 볼까요",
          "original_form": "화방에 가서 볼펜을 꼭 사 오고 싶긴 해요 언제 준비를 해 볼까요",
          "speaker_id": "1",
          "time": "20210905 14:50"
        },
        {
          "id": "MDRW2100032280.1.15",
          "form": "비 오면 우산을 가려서 머리가 떡진지 티가 안나요",
          "original_form": "비 오면 우산을 가려서 머리가 떡진지 티가 안나요",
          "speaker_id": "2",
          "time": "20210905 14:50"
        },
        {
          "id": "MDRW2100032280.1.16",
          "form": "3시에 일어날 준비를 해 볼까요",
          "original_form": "3시에 일어날 준비를 해 볼까요",
          "speaker_id": "1",
          "time": "20210905 14:50"
        },
        {
          "id": "MDRW2100032280.1.17",
          "form": "10분 뒤 이군요",
          "original_form": "10분 뒤 이군요",
          "speaker_id": "1",
          "time": "20210905 14:50"
        },
        {
          "id": "MDRW2100032280.1.18",
          "form": "롸잇나우",
          "original_form": "롸잇나우",
          "speaker_id": "2",
          "time": "20210905 14:50"
        },
        {
          "id": "MDRW2100032280.1.19",
          "form": "과연 오늘 쇼핑을 할 수 있을 것인가",
          "original_form": "과연 오늘 쇼핑을 할 수 있을 것인가",
          "speaker_id": "1",
          "time": "20210905 14:50"
        },
        {
          "id": "MDRW2100032280.1.20",
          "form": "10분 뒤에 기도할게요",
          "original_form": "10분 뒤에 기도할게요",
          "speaker_id": "1",
          "time": "20210905 14:50"
        },
        {
          "id": "MDRW2100032280.1.21",
          "form": "부지런해지세요",
          "original_form": "부지런해지세요",
          "speaker_id": "2",
          "time": "20210905 14:50"
        },
        {
          "id": "MDRW2100032280.1.22",
          "form": "시도해 볼게요",
          "original_form": "시도해 볼게요",
