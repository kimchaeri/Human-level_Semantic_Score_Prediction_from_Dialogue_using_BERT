      "utterance": [
        {
          "id": "MDRW2100014848.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 20:56"
        },
        {
          "id": "MDRW2100014848.1.2",
          "form": "네 좋아해요\r\n어디가 좋으셨어요?",
          "original_form": "네 좋아해요\r\n어디가 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014848.1.3",
          "form": "저는 해외는 한번밖에 못가봤고 국내는 여수 좋았어욤",
          "original_form": "저는 해외는 한번밖에 못가봤고 국내는 여수 좋았어욤",
          "speaker_id": "2",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014848.1.4",
          "form": "아 여수좋죠~~",
          "original_form": "아 여수좋죠~~",
          "speaker_id": "1",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014848.1.5",
          "form": "저도 케이블카...바닥이 투명한...",
          "original_form": "저도 케이블카...바닥이 투명한...",
          "speaker_id": "1",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014848.1.6",
          "form": "좋았어요",
          "original_form": "좋았어요",
          "speaker_id": "1",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014848.1.7",
          "form": "헐ㅠㅠ맞아요",
          "original_form": "헐ㅠㅠ맞아요",
          "speaker_id": "2",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014848.1.8",
          "form": "처음엔 무서웠는데",
          "original_form": "처음엔 무서웠는데",
          "speaker_id": "2",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014848.1.9",
          "form": "금방적응되서 사진찍고 놀았어요ㅎㅎ",
          "original_form": "금방적응되서 사진찍고 놀았어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014848.1.10",
          "form": "여수 다녀오신적 있으세요?",
          "original_form": "여수 다녀오신적 있으세요?",
          "speaker_id": "2",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014848.1.11",
          "form": "네 2~3년전에 다녀왔어요",
          "original_form": "네 2~3년전에 다녀왔어요",
          "speaker_id": "1",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014848.1.12",
          "form": "너무좋았어요",
          "original_form": "너무좋았어요",
          "speaker_id": "1",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014848.1.13",
          "form": "음식도 되게 맛있지 않아요?",
          "original_form": "음식도 되게 맛있지 않아요?",
          "speaker_id": "2",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014848.1.14",
          "form": "뭐드셨어요?\r\n한식당 갔었는데 맛있게잘먹었던 기억이 나네요",
          "original_form": "뭐드셨어요?\r\n한식당 갔었는데 맛있게잘먹었던 기억이 나네요",
          "speaker_id": "1",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014848.1.15",
          "form": "저 회무침도 좋았고 바게트버거 진짜 맛있었는데!",
          "original_form": "저 회무침도 좋았고 바게트버거 진짜 맛있었는데!",
          "speaker_id": "2",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014848.1.16",
          "form": "바게트버거 담에가면 사먹을게요 또가고싶어요",
          "original_form": "바게트버거 담에가면 사먹을게요 또가고싶어요",
          "speaker_id": "1",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014848.1.17",
          "form": "진짜 추천이요ㅎㅎ저도 또 가고 싶네요ㅠㅠ",
          "original_form": "진짜 추천이요ㅎㅎ저도 또 가고 싶네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014848.1.18",
          "form": "2월쯤에 갔는데 동백꽃도 여수에서 처음 보고 되게 좋은 기억으로 남아있거든요",
          "original_form": "2월쯤에 갔는데 동백꽃도 여수에서 처음 보고 되게 좋은 기억으로 남아있거든요",
          "speaker_id": "2",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014848.1.19",
          "form": "아 동백꽃 이뻣겠어요",
          "original_form": "아 동백꽃 이뻣겠어요",
          "speaker_id": "1",
          "time": "20210825 21:13"
        },
        {
          "id": "MDRW2100014848.1.20",
          "form": "저도 여수기억이 좋아요",
          "original_form": "저도 여수기억이 좋아요",
          "speaker_id": "1",
          "time": "20210825 21:13"
        }
      ]
