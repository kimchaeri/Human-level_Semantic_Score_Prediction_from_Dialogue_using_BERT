      "utterance": [
        {
          "id": "MDRW2100041242.1.1",
          "form": "전 치즈요.",
          "original_form": "전 치즈요.",
          "speaker_id": "2",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100041242.1.2",
          "form": "저랑 같네요",
          "original_form": "저랑 같네요",
          "speaker_id": "1",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100041242.1.3",
          "form": "저도 치즈요",
          "original_form": "저도 치즈요",
          "speaker_id": "1",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100041242.1.4",
          "form": "어디 피자 브랜드 중에",
          "original_form": "어디 피자 브랜드 중에",
          "speaker_id": "2",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100041242.1.5",
          "form": "소보로 크러스트도 있었는데",
          "original_form": "소보로 크러스트도 있었는데",
          "speaker_id": "2",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100041242.1.6",
          "form": "다 흐트러져서 별로 였어요",
          "original_form": "다 흐트러져서 별로 였어요",
          "speaker_id": "2",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100041242.1.7",
          "form": "피자헛 아닌가요??!!",
          "original_form": "피자헛 아닌가요??!!",
          "speaker_id": "1",
          "time": "20210905 21:04"
        },
        {
          "id": "MDRW2100041242.1.8",
          "form": "그런가요? 브랜드는 잘 기억안나네요ㅋㅋ",
          "original_form": "그런가요? 브랜드는 잘 기억안나네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100041242.1.9",
          "form": "ㅋㅋㅋ네 뭔가 거기거 같아요",
          "original_form": "ㅋㅋㅋ네 뭔가 거기거 같아요",
          "speaker_id": "1",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100041242.1.10",
          "form": "피자헛은 왠지 너무 비싼 거 같아요.",
          "original_form": "피자헛은 왠지 너무 비싼 거 같아요.",
          "speaker_id": "2",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100041242.1.11",
          "form": "전 피자가 다 비싼느낌 ㅠ",
          "original_form": "전 피자가 다 비싼느낌 ㅠ",
          "speaker_id": "1",
          "time": "20210905 21:36"
        },
        {
          "id": "MDRW2100041242.1.12",
          "form": "세일할 때 먹게되더라구여",
          "original_form": "세일할 때 먹게되더라구여",
          "speaker_id": "1",
          "time": "20210905 21:36"
        },
        {
          "id": "MDRW2100041242.1.13",
          "form": "맞아요 할인 떠있으면 조급해져서 꼭 먹어야되구ㅋㅋ",
          "original_form": "맞아요 할인 떠있으면 조급해져서 꼭 먹어야되구ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:42"
        },
        {
          "id": "MDRW2100041242.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 헐 저랑 같네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 헐 저랑 같네요",
          "speaker_id": "1",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100041242.1.15",
          "form": "저렴하면 뭔가 이렁 기회가",
          "original_form": "저렴하면 뭔가 이렁 기회가",
          "speaker_id": "1",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100041242.1.16",
          "form": "페이앱에서 할인 뜨면 꼭 먹는거 같아요ㅋㅋ",
          "original_form": "페이앱에서 할인 뜨면 꼭 먹는거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:58"
        },
        {
          "id": "MDRW2100041242.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ전 깊티 뜨명요!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ전 깊티 뜨명요!!",
          "speaker_id": "1",
          "time": "20210905 22:17"
        },
        {
          "id": "MDRW2100041242.1.18",
          "form": "깊티 얻기만 하면 쓰는거 같아요ㅋㅋ",
          "original_form": "깊티 얻기만 하면 쓰는거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:43"
        },
        {
          "id": "MDRW2100041242.1.19",
          "form": "선물로 받으면 너무 좋구요ㅋㅋㅋㅋ",
          "original_form": "선물로 받으면 너무 좋구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100041242.1.20",
          "form": "ㅋㅋㅋㅋㅋ저두요",
          "original_form": "ㅋㅋㅋㅋㅋ저두요",
          "speaker_id": "1",
