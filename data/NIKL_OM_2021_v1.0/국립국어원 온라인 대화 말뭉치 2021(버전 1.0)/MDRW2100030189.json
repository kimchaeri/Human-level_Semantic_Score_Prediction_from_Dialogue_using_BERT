      "utterance": [
        {
          "id": "MDRW2100030189.1.1",
          "form": "낼 코스트코 갔다 오는 길에 스벅들렸다 오자",
          "original_form": "낼 코스트코 갔다 오는 길에 스벅들렸다 오자",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030189.1.2",
          "form": "스벅?",
          "original_form": "스벅?",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030189.1.3",
          "form": "오 좋지",
          "original_form": "오 좋지",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030189.1.4",
          "form": "쿠폰도 써야하고",
          "original_form": "쿠폰도 써야하고",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030189.1.5",
          "form": "나 오트라떼 먹고싶었는데 계속",
          "original_form": "나 오트라떼 먹고싶었는데 계속",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030189.1.6",
          "form": "귀리우유 안들어오더니",
          "original_form": "귀리우유 안들어오더니",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030189.1.7",
          "form": "이제 되는듯",
          "original_form": "이제 되는듯",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030189.1.8",
          "form": "그거 맛있냐?",
          "original_form": "그거 맛있냐?",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030189.1.9",
          "form": "계속 노래부르네",
          "original_form": "계속 노래부르네",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030189.1.10",
          "form": "ㅇㅇ고소하고 우유보다 나은듯",
          "original_form": "ㅇㅇ고소하고 우유보다 나은듯",
          "speaker_id": "1",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030189.1.11",
          "form": "나도 담에 그거 마셔볼까?",
          "original_form": "나도 담에 그거 마셔볼까?",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030189.1.12",
          "form": "먹어봐봐",
          "original_form": "먹어봐봐",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030189.1.13",
          "form": "근데 바닐라크림콜드브루만큼 깔끔하진 않음",
          "original_form": "근데 바닐라크림콜드브루만큼 깔끔하진 않음",
          "speaker_id": "1",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030189.1.14",
          "form": "니꺼 한번",
          "original_form": "니꺼 한번",
          "speaker_id": "2",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030189.1.15",
          "form": "뺏어",
          "original_form": "뺏어",
          "speaker_id": "2",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030189.1.16",
          "form": "먹어야겠다",
          "original_form": "먹어야겠다",
          "speaker_id": "2",
          "time": "20210903 22:55"
        },
        {
          "id": "MDRW2100030189.1.17",
          "form": "누가 준대?",
          "original_form": "누가 준대?",
          "speaker_id": "1",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030189.1.18",
          "form": "주지마라 그럼 나도 안사줘",
          "original_form": "주지마라 그럼 나도 안사줘",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030189.1.19",
          "form": "헐 진짜 치사하다",
          "original_form": "헐 진짜 치사하다",
          "speaker_id": "1",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030189.1.20",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "1",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030189.1.21",
          "form": "나 원래 치사해",
          "original_form": "나 원래 치사해",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030189.1.22",
          "form": "아 엄마 짜증남",
          "original_form": "아 엄마 짜증남",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030189.1.23",
          "form": "나 그거 없음 아아에 케이크 먹을거야",
          "original_form": "나 그거 없음 아아에 케이크 먹을거야",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030189.1.24",
          "form": "며칠있음 생일인데 벌써먹냐?",
          "original_form": "며칠있음 생일인데 벌써먹냐?",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030189.1.25",
          "form": "생일케익은 케익이고",
          "original_form": "생일케익은 케익이고",
          "speaker_id": "1",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100030189.1.26",
          "form": "이건이거고ㅡㅡ",
          "original_form": "이건이거고ㅡㅡ",
