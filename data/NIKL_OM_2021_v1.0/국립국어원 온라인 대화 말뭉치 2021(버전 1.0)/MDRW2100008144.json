      "utterance": [
        {
          "id": "MDRW2100008144.1.1",
          "form": "정준하 나오네 오랜만",
          "original_form": "정준하 나오네 오랜만",
          "speaker_id": "2",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100008144.1.2",
          "form": "정준하 요새 장사하고 있나?",
          "original_form": "정준하 요새 장사하고 있나?",
          "speaker_id": "1",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100008144.1.3",
          "form": "엉 장사하고 있다는데 잘 안된다고 했던거 같다 저번 매니저구할때",
          "original_form": "엉 장사하고 있다는데 잘 안된다고 했던거 같다 저번 매니저구할때",
          "speaker_id": "2",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100008144.1.4",
          "form": "요샌 장사가 잘되는데가 있겠나",
          "original_form": "요샌 장사가 잘되는데가 있겠나",
          "speaker_id": "1",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008144.1.5",
          "form": "놀면뭐하니가 2년이란다 벌써 그럼 무도끝난지 엄청 오래된거네",
          "original_form": "놀면뭐하니가 2년이란다 벌써 그럼 무도끝난지 엄청 오래된거네",
          "speaker_id": "2",
          "time": "20210822 13:36"
        },
        {
          "id": "MDRW2100008144.1.6",
          "form": "그래도 유튜브에 올라와서 정겹긴 하다",
          "original_form": "그래도 유튜브에 올라와서 정겹긴 하다",
          "speaker_id": "1",
          "time": "20210822 13:36"
        },
        {
          "id": "MDRW2100008144.1.7",
          "form": "장사만 그렇겠나 공장도 그렇고 다 그렇지",
          "original_form": "장사만 그렇겠나 공장도 그렇고 다 그렇지",
          "speaker_id": "2",
          "time": "20210822 13:36"
        },
        {
          "id": "MDRW2100008144.1.8",
          "form": "요새 안 힘든곳이 없다",
          "original_form": "요새 안 힘든곳이 없다",
          "speaker_id": "1",
          "time": "20210822 13:37"
        },
        {
          "id": "MDRW2100008144.1.9",
          "form": "응 심지어 연예계도 힘든갑더라",
          "original_form": "응 심지어 연예계도 힘든갑더라",
          "speaker_id": "2",
          "time": "20210822 13:37"
        },
        {
          "id": "MDRW2100008144.1.10",
          "form": "벌써 가수들 행사가 없자나",
          "original_form": "벌써 가수들 행사가 없자나",
          "speaker_id": "1",
          "time": "20210822 13:38"
        },
        {
          "id": "MDRW2100008144.1.11",
          "form": "응 다 힘 든 시 기 이다",
          "original_form": "응 다 힘 든 시 기 이다",
          "speaker_id": "2",
          "time": "20210822 13:39"
        },
        {
          "id": "MDRW2100008144.1.12",
          "form": "대학교 축제 한번 할때마다 몇천 번다던데",
          "original_form": "대학교 축제 한번 할때마다 몇천 번다던데",
          "speaker_id": "1",
          "time": "20210822 13:39"
        },
        {
          "id": "MDRW2100008144.1.13",
          "form": "정준하는 라디오 하나보네",
          "original_form": "정준하는 라디오 하나보네",
