      "utterance": [
        {
          "id": "MDRW2100001845.1.1",
          "form": "다시 학교 다니고 싶어요",
          "original_form": "다시 학교 다니고 싶어요",
          "speaker_id": "1",
          "time": "20210819 21:39"
        },
        {
          "id": "MDRW2100001845.1.2",
          "form": "맞아요 학교다닐때가 좋았어요",
          "original_form": "맞아요 학교다닐때가 좋았어요",
          "speaker_id": "2",
          "time": "20210819 21:40"
        },
        {
          "id": "MDRW2100001845.1.3",
          "form": "학교생활 어떻게하셨어요 전 아싸였어서 엄청 이것저것못해본게 아쉬워요",
          "original_form": "학교생활 어떻게하셨어요 전 아싸였어서 엄청 이것저것못해본게 아쉬워요",
          "speaker_id": "2",
          "time": "20210819 21:41"
        },
        {
          "id": "MDRW2100001845.1.4",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210819 21:42"
        },
        {
          "id": "MDRW2100001845.1.5",
          "form": "너무 후회되요 추억이 별로 없구",
          "original_form": "너무 후회되요 추억이 별로 없구",
          "speaker_id": "1",
          "time": "20210819 21:42"
        },
        {
          "id": "MDRW2100001845.1.6",
          "form": "그러니까요 학교생활할때만 할수있는 추억들도 많았을텐데",
          "original_form": "그러니까요 학교생활할때만 할수있는 추억들도 많았을텐데",
          "speaker_id": "2",
          "time": "20210819 21:47"
        },
        {
          "id": "MDRW2100001845.1.7",
          "form": "그땐 왜그렇게 가기가 싫었는지 ㅎㅎ..",
          "original_form": "그땐 왜그렇게 가기가 싫었는지 ㅎㅎ..",
          "speaker_id": "2",
          "time": "20210819 21:47"
        },
        {
          "id": "MDRW2100001845.1.8",
          "form": "잘안다니고 그랬어요",
          "original_form": "잘안다니고 그랬어요",
          "speaker_id": "2",
          "time": "20210819 21:47"
        },
        {
          "id": "MDRW2100001845.1.9",
          "form": "그리 싫었던 교복도 그리워요",
          "original_form": "그리 싫었던 교복도 그리워요",
          "speaker_id": "1",
          "time": "20210819 21:53"
        },
        {
          "id": "MDRW2100001845.1.10",
          "form": "교복입기싫고 급식도먹기싫었는데",
          "original_form": "교복입기싫고 급식도먹기싫었는데",
          "speaker_id": "2",
          "time": "20210819 21:54"
        },
        {
          "id": "MDRW2100001845.1.11",
          "form": "지금은 완전 그때가편했지 싶어요",
          "original_form": "지금은 완전 그때가편했지 싶어요",
          "speaker_id": "2",
          "time": "20210819 21:54"
        },
        {
          "id": "MDRW2100001845.1.12",
          "form": "맞아요 밥주는 학교 너무 좋아요",
          "original_form": "맞아요 밥주는 학교 너무 좋아요",
          "speaker_id": "1",
          "time": "20210819 21:54"
        },
        {
          "id": "MDRW2100001845.1.13",
          "form": "교복도 옷걱정없으니 좋고",
          "original_form": "교복도 옷걱정없으니 좋고",
          "speaker_id": "1",
          "time": "20210819 21:55"
        },
        {
          "id": "MDRW2100001845.1.14",
          "form": "취업걱정안해도 되니 좋고",
          "original_form": "취업걱정안해도 되니 좋고",
          "speaker_id": "1",
          "time": "20210819 21:55"
        },
        {
          "id": "MDRW2100001845.1.15",
          "form": "그니까요 영양맞춰서딱나오고",
          "original_form": "그니까요 영양맞춰서딱나오고",
          "speaker_id": "2",
          "time": "20210819 21:55"
        },
        {
          "id": "MDRW2100001845.1.16",
          "form": "학교집학교집왔다갔다만 하면되고",
          "original_form": "학교집학교집왔다갔다만 하면되고",
          "speaker_id": "2",
          "time": "20210819 21:55"
        },
        {
          "id": "MDRW2100001845.1.17",
          "form": "사회나오니까 설움 투성이에요",
          "original_form": "사회나오니까 설움 투성이에요",
          "speaker_id": "1",
          "time": "20210819 21:55"
        },
        {
          "id": "MDRW2100001845.1.18",
          "form": "학교그립고",
          "original_form": "학교그립고",
          "speaker_id": "1",
          "time": "20210819 21:55"
        },
        {
          "id": "MDRW2100001845.1.19",
          "form": "맞아요 그때가정말좋았어요ㅜ책임질일도없고",
          "original_form": "맞아요 그때가정말좋았어요ㅜ책임질일도없고",
          "speaker_id": "2",
          "time": "20210819 22:01"
        },
        {
          "id": "MDRW2100001845.1.20",
          "form": "돌아가면 뭘 제일 하고싶어요?",
          "original_form": "돌아가면 뭘 제일 하고싶어요?",
          "speaker_id": "1",
          "time": "20210819 22:03"
        },
        {
          "id": "MDRW2100001845.1.21",
          "form": "전 공부요 ㅋㅋㅋㅋㅋㅋ학생의본분을 너무 안했어서",
          "original_form": "전 공부요 ㅋㅋㅋㅋㅋㅋ학생의본분을 너무 안했어서",
          "speaker_id": "2",
          "time": "20210819 22:07"
        },
        {
          "id": "MDRW2100001845.1.22",
          "form": "좀 그래도 할걸 싶네요",
          "original_form": "좀 그래도 할걸 싶네요",
          "speaker_id": "2",
          "time": "20210819 22:07"
        },
        {
          "id": "MDRW2100001845.1.23",
          "form": "어제의 기억이 없어요",
          "original_form": "어제의 기억이 없어요",
          "speaker_id": "1",
          "time": "20210820 08:49"
        },
        {
          "id": "MDRW2100001845.1.24",
          "form": "잘잤나요 name2친구님?",
          "original_form": "잘잤나요 &name2&친구님?",
          "speaker_id": "1",
          "time": "20210820 08:49"
        },
        {
          "id": "MDRW2100001845.1.25",
          "form": "어제 이야기에 이어 저도 공부하고싶어요\r\n그럼 삶이 좀 달라지지않았을까싶어요",
          "original_form": "어제 이야기에 이어 저도 공부하고싶어요\r\n그럼 삶이 좀 달라지지않았을까싶어요",
