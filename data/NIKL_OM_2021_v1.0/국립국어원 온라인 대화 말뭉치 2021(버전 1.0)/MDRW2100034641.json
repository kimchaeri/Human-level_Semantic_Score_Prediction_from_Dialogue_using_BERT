      "utterance": [
        {
          "id": "MDRW2100034641.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210828 16:46"
        },
        {
          "id": "MDRW2100034641.1.2",
          "form": "구피요 name1님은요",
          "original_form": "구피요 &name1&님은요",
          "speaker_id": "2",
          "time": "20210828 16:47"
        },
        {
          "id": "MDRW2100034641.1.3",
          "form": "구피 귀엽겠네요 전 뭐 하나 키워볼까 고민중이에요",
          "original_form": "구피 귀엽겠네요 전 뭐 하나 키워볼까 고민중이에요",
          "speaker_id": "1",
          "time": "20210828 17:57"
        },
        {
          "id": "MDRW2100034641.1.4",
          "form": "식물도 좋은데 이제 겨울이 와서리..",
          "original_form": "식물도 좋은데 이제 겨울이 와서리..",
          "speaker_id": "2",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100034641.1.5",
          "form": "그쵸 name2님은 구피 키운지 오래됐나요?",
          "original_form": "그쵸 &name2&님은 구피 키운지 오래됐나요?",
          "speaker_id": "1",
          "time": "20210828 18:17"
        },
        {
          "id": "MDRW2100034641.1.6",
          "form": "아뇨 삼개월된거같아요 ㅎㅎ 아직 잘모르는애들ㅜㅜ",
          "original_form": "아뇨 삼개월된거같아요 ㅎㅎ 아직 잘모르는애들ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 18:18"
        },
        {
          "id": "MDRW2100034641.1.7",
          "form": "구피가 잘 자란다는 이야기를 들은적이 있어서 저도 관심있어요ㅎ",
          "original_form": "구피가 잘 자란다는 이야기를 들은적이 있어서 저도 관심있어요ㅎ",
          "speaker_id": "1",
          "time": "20210828 18:22"
        },
        {
          "id": "MDRW2100034641.1.8",
          "form": "몇마리 키우세요?",
          "original_form": "몇마리 키우세요?",
          "speaker_id": "1",
          "time": "20210828 18:22"
        },
        {
          "id": "MDRW2100034641.1.9",
          "form": "일곱마리정도있어요",
          "original_form": "일곱마리정도있어요",
          "speaker_id": "2",
          "time": "20210828 18:23"
        },
        {
          "id": "MDRW2100034641.1.10",
          "form": "간편해요 밥만주면되서",
          "original_form": "간편해요 밥만주면되서",
          "speaker_id": "2",
          "time": "20210828 18:23"
        },
        {
          "id": "MDRW2100034641.1.11",
          "form": "구피는 알을 잘 낳는다는데 정말 신기할거같아요",
          "original_form": "구피는 알을 잘 낳는다는데 정말 신기할거같아요",
          "speaker_id": "1",
          "time": "20210828 18:25"
        },
        {
          "id": "MDRW2100034641.1.12",
          "form": "아직은 알안놓고 응아만 싸고있어요",
          "original_form": "아직은 알안놓고 응아만 싸고있어요",
          "speaker_id": "2",
          "time": "20210828 18:25"
        },
        {
          "id": "MDRW2100034641.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ알 낳으면 구피 정말 많아지겠네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ알 낳으면 구피 정말 많아지겠네요",
          "speaker_id": "1",
          "time": "20210828 18:34"
        },
        {
          "id": "MDRW2100034641.1.14",
          "form": "아아..사료를 더많이 줘야겠네요ㅎㅎㅎ",
          "original_form": "아아..사료를 더많이 줘야겠네요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 18:35"
        },
        {
          "id": "MDRW2100034641.1.15",
