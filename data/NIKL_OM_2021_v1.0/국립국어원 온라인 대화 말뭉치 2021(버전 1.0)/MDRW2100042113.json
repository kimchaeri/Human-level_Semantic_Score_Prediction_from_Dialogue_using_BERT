        },
        {
          "id": "MDRW2100042113.1.3",
          "form": "머해야할지몰겟음",
          "original_form": "머해야할지몰겟음",
          "speaker_id": "1",
          "time": "20210906 20:07"
        },
        {
          "id": "MDRW2100042113.1.4",
          "form": "쇼핑 최고지 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "쇼핑 최고지 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:15"
        },
        {
          "id": "MDRW2100042113.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:16"
        },
        {
          "id": "MDRW2100042113.1.6",
          "form": "아 컬리주문해야겟다",
          "original_form": "아 컬리주문해야겟다",
          "speaker_id": "1",
          "time": "20210906 20:16"
        },
        {
          "id": "MDRW2100042113.1.7",
          "form": "비오는대개안나ㅜㅜ",
          "original_form": "비오는대개안나ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 20:16"
        },
        {
          "id": "MDRW2100042113.1.8",
          "form": "컬리쓰?ㅋㅋㅋ 뭐 시키닝",
          "original_form": "컬리쓰?ㅋㅋㅋ 뭐 시키닝",
          "speaker_id": "2",
          "time": "20210906 20:17"
        },
        {
          "id": "MDRW2100042113.1.9",
          "form": "후 난 허닭이나 ㅋㅋㅋㅋ 추가주문해야지",
          "original_form": "후 난 허닭이나 ㅋㅋㅋㅋ 추가주문해야지",
          "speaker_id": "2",
          "time": "20210906 20:17"
        },
        {
          "id": "MDRW2100042113.1.10",
          "form": "컬리가 쿠폰줬도라고ㅋㅋ 육처넌",
          "original_form": "컬리가 쿠폰줬도라고ㅋㅋ 육처넌",
          "speaker_id": "1",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100042113.1.11",
          "form": "과일도좀사규",
          "original_form": "과일도좀사규",
          "speaker_id": "1",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100042113.1.12",
          "form": "허닭혹시있나",
          "original_form": "허닭혹시있나",
          "speaker_id": "1",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100042113.1.13",
          "form": "아님굽네만두사볼가",
          "original_form": "아님굽네만두사볼가",
          "speaker_id": "1",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100042113.1.14",
          "form": "맹두는 내가 허닭먹어좟은게",
          "original_form": "맹두는 내가 허닭먹어좟은게",
          "speaker_id": "2",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042113.1.15",
          "form": "봤는데 굽네야",
          "original_form": "봤는데 굽네야",
          "speaker_id": "2",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042113.1.16",
          "form": "컬리 쿠폰 나두 가볼까",
          "original_form": "컬리 쿠폰 나두 가볼까",
          "speaker_id": "2",
          "time": "20210906 20:37"
        },
        {
          "id": "MDRW2100042113.1.17",
          "form": "컬리 왜 난 쿠폰 아니줌 흥",
          "original_form": "컬리 왜 난 쿠폰 아니줌 흥",
          "speaker_id": "2",
          "time": "20210906 20:38"
        },
        {
          "id": "MDRW2100042113.1.18",
          "form": "호갱만주나봐",
          "original_form": "호갱만주나봐",
          "speaker_id": "1",
          "time": "20210906 20:39"
        },
        {
          "id": "MDRW2100042113.1.19",
          "form": "ㅋㅋㅋㅋㅋ 마이 이용해야주는군",
          "original_form": "ㅋㅋㅋㅋㅋ 마이 이용해야주는군",
          "speaker_id": "2",
          "time": "20210906 20:39"
        },
        {
          "id": "MDRW2100042113.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:39"
        },
        {
          "id": "MDRW2100042113.1.21",
          "form": "허닭 볶음밥이나 조져야지",
          "original_form": "허닭 볶음밥이나 조져야지",
          "speaker_id": "2",
          "time": "20210906 20:42"
        },
        {
          "id": "MDRW2100042113.1.22",
          "form": "낚지볶음 존맛탱",
          "original_form": "낚지볶음 존맛탱",
          "speaker_id": "2",
          "time": "20210906 20:42"
        },
        {
          "id": "MDRW2100042113.1.23",
          "form": "곤약밥이야?",
          "original_form": "곤약밥이야?",
