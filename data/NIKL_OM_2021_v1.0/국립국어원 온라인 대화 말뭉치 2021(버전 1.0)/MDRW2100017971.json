      "utterance": [
        {
          "id": "MDRW2100017971.1.1",
          "form": "어떤 연예인 좋아하세요",
          "original_form": "어떤 연예인 좋아하세요",
          "speaker_id": "1",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100017971.1.2",
          "form": "저는 요즘 좋아하는 연예인 딱히 없긴 한데",
          "original_form": "저는 요즘 좋아하는 연예인 딱히 없긴 한데",
          "speaker_id": "2",
          "time": "20210826 21:55"
        },
        {
          "id": "MDRW2100017971.1.3",
          "form": "오래 좋아한 연예인은 김남길",
          "original_form": "오래 좋아한 연예인은 김남길",
          "speaker_id": "2",
          "time": "20210826 21:55"
        },
        {
          "id": "MDRW2100017971.1.4",
          "form": "이제 차은우 자랑 해보시죠",
          "original_form": "이제 차은우 자랑 해보시죠",
          "speaker_id": "2",
          "time": "20210826 21:55"
        },
        {
          "id": "MDRW2100017971.1.5",
          "form": "김남길씨는 요즘 뭐하시나요",
          "original_form": "김남길씨는 요즘 뭐하시나요",
          "speaker_id": "1",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100017971.1.6",
          "form": "차은우씨는 요즘 향수, 옷, 간장 씨에프를 찍었어요",
          "original_form": "차은우씨는 요즘 향수, 옷, 간장 씨에프를 찍었어요",
          "speaker_id": "1",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100017971.1.7",
          "form": "그러게요 곧 영화 개봉할거 기다리고 드라마도 찍는 걸로 알아요",
          "original_form": "그러게요 곧 영화 개봉할거 기다리고 드라마도 찍는 걸로 알아요",
          "speaker_id": "2",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100017971.1.8",
          "form": "간장도 찍나요",
          "original_form": "간장도 찍나요",
          "speaker_id": "2",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100017971.1.9",
          "form": "삼화 간장인데",
          "original_form": "삼화 간장인데",
          "speaker_id": "1",
          "time": "20210826 22:00"
        },
        {
          "id": "MDRW2100017971.1.10",
          "form": "간장사면 포카 준대요",
          "original_form": "간장사면 포카 준대요",
          "speaker_id": "1",
          "time": "20210826 22:00"
        },
        {
          "id": "MDRW2100017971.1.11",
          "form": "오 드라마 나오나븁니다",
          "original_form": "오 드라마 나오나븁니다",
          "speaker_id": "1",
          "time": "20210826 22:00"
        },
        {
          "id": "MDRW2100017971.1.12",
          "form": "앜ㅋㅋㅋㅋ간장 사야하는군요",
          "original_form": "앜ㅋㅋㅋㅋ간장 사야하는군요",
          "speaker_id": "2",
          "time": "20210826 22:01"
        },
        {
          "id": "MDRW2100017971.1.13",
          "form": "넹 드라마 찍는다는 걸 봤어요",
          "original_form": "넹 드라마 찍는다는 걸 봤어요",
          "speaker_id": "2",
          "time": "20210826 22:01"
        },
        {
          "id": "MDRW2100017971.1.14",
          "form": "오 그렇균요 다행이에요",
          "original_form": "오 그렇균요 다행이에요",
          "speaker_id": "1",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017971.1.15",
          "form": "간장 향수 옷 바쁘다바빠",
          "original_form": "간장 향수 옷 바쁘다바빠",
          "speaker_id": "1",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017971.1.16",
          "form": "간장은 왜 찍었대요",
          "original_form": "간장은 왜 찍었대요",
          "speaker_id": "2",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017971.1.17",
          "form": "이것저것 들어오는건 다하나봐요",
          "original_form": "이것저것 들어오는건 다하나봐요",
          "speaker_id": "1",
          "time": "20210826 22:13"
        },
        {
          "id": "MDRW2100017971.1.18",
          "form": "ㅋㅋㅋㅋ이제 고급짐것만 찍어봅시다",
          "original_form": "ㅋㅋㅋㅋ이제 고급짐것만 찍어봅시다",
          "speaker_id": "2",
          "time": "20210826 22:44"
        },
        {
          "id": "MDRW2100017971.1.19",
          "form": "그니가요",
          "original_form": "그니가요",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100017971.1.20",
          "form": "산별을 해야지",
          "original_form": "산별을 해야지",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100017971.1.21",
          "form": "삼화간장 뭡니까",
          "original_form": "삼화간장 뭡니까",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100017971.1.22",
          "form": "삼화간장인가요 첨들어보긴하네요",
          "original_form": "삼화간장인가요 첨들어보긴하네요",
          "speaker_id": "2",
          "time": "20210827 08:19"
        },
        {
          "id": "MDRW2100017971.1.23",
          "form": "그쵸 저두요ㅠㅠ 판타지오 바보",
          "original_form": "그쵸 저두요ㅠㅠ 판타지오 바보",
          "speaker_id": "1",
          "time": "20210827 08:29"
        },
        {
          "id": "MDRW2100017971.1.24",
          "form": "차은우 명품 브랜드만 찍어줬으면...",
          "original_form": "차은우 명품 브랜드만 찍어줬으면...",
          "speaker_id": "2",
          "time": "20210827 08:37"
        }
      ]
