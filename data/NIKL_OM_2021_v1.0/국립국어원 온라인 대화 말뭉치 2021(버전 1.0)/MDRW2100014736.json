      "utterance": [
        {
          "id": "MDRW2100014736.1.1",
          "form": "오늘 비가 개어서",
          "original_form": "오늘 비가 개어서",
          "speaker_id": "1",
          "time": "20210826 10:29"
        },
        {
          "id": "MDRW2100014736.1.2",
          "form": "빨해좀 해야겠어요 ㅎㅎ",
          "original_form": "빨해좀 해야겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 10:30"
        },
        {
          "id": "MDRW2100014736.1.3",
          "form": "근데 흐려서 막 잘 마르진 않겠어요",
          "original_form": "근데 흐려서 막 잘 마르진 않겠어요",
          "speaker_id": "1",
          "time": "20210826 10:30"
        },
        {
          "id": "MDRW2100014736.1.4",
          "form": "저는 지난주말에",
          "original_form": "저는 지난주말에",
          "speaker_id": "2",
          "time": "20210826 10:30"
        },
        {
          "id": "MDRW2100014736.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 10:30"
        },
        {
          "id": "MDRW2100014736.1.6",
          "form": "이불빨래 3번 돌렸어요 ㅎ",
          "original_form": "이불빨래 3번 돌렸어요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 10:30"
        },
        {
          "id": "MDRW2100014736.1.7",
          "form": "옷은 그냥 건조기에 넣는데",
          "original_form": "옷은 그냥 건조기에 넣는데",
          "speaker_id": "2",
          "time": "20210826 10:31"
        },
        {
          "id": "MDRW2100014736.1.8",
          "form": "그게 좋죠 건조기 ㅎㅎ",
          "original_form": "그게 좋죠 건조기 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 10:31"
        },
        {
          "id": "MDRW2100014736.1.9",
          "form": "인견이불들이라 건조기 넣기 그래서\r\n짱짱한 햇살에 뽀숑하게 말리고싶어서",
          "original_form": "인견이불들이라 건조기 넣기 그래서\r\n짱짱한 햇살에 뽀숑하게 말리고싶어서",
          "speaker_id": "2",
          "time": "20210826 10:31"
        },
        {
          "id": "MDRW2100014736.1.10",
          "form": "시작했는데 다널고나니 하루 다 가고",
          "original_form": "시작했는데 다널고나니 하루 다 가고",
          "speaker_id": "2",
          "time": "20210826 10:31"
        },
        {
          "id": "MDRW2100014736.1.11",
          "form": "오 잘하셨어요",
          "original_form": "오 잘하셨어요",
          "speaker_id": "1",
          "time": "20210826 10:31"
        },
        {
          "id": "MDRW2100014736.1.12",
          "form": "그담날부터 비가 ㅋㅋㅋ",
          "original_form": "그담날부터 비가 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:31"
        },
        {
          "id": "MDRW2100014736.1.13",
          "form": "저도 그때 돌힐걸",
          "original_form": "저도 그때 돌힐걸",
          "speaker_id": "1",
          "time": "20210826 10:31"
        },
        {
          "id": "MDRW2100014736.1.14",
          "form": "한주내내 이러네요",
          "original_form": "한주내내 이러네요",
          "speaker_id": "1",
          "time": "20210826 10:32"
        },
        {
          "id": "MDRW2100014736.1.15",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:32"
        },
        {
          "id": "MDRW2100014736.1.16",
          "form": "요새 빨래",
          "original_form": "요새 빨래",
          "speaker_id": "1",
          "time": "20210826 10:32"
        },
        {
          "id": "MDRW2100014736.1.17",
          "form": "냄새가 나요",
          "original_form": "냄새가 나요",
          "speaker_id": "1",
          "time": "20210826 10:32"
        },
        {
          "id": "MDRW2100014736.1.18",
          "form": "햇실좋을땐 진짜 빨래하고싶어져요",
          "original_form": "햇실좋을땐 진짜 빨래하고싶어져요",
          "speaker_id": "2",
          "time": "20210826 10:32"
        },
        {
          "id": "MDRW2100014736.1.19",
          "form": "집안일 하기 싫으네요 ㅋㅋ",
          "original_form": "집안일 하기 싫으네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:32"
        },
        {
          "id": "MDRW2100014736.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 10:32"
        },
        {
          "id": "MDRW2100014736.1.21",
          "form": "바람도 살랑불어주면 더좋고요",
          "original_form": "바람도 살랑불어주면 더좋고요",
          "speaker_id": "2",
          "time": "20210826 10:32"
        },
        {
          "id": "MDRW2100014736.1.22",
          "form": "그때가 신나서해요",
          "original_form": "그때가 신나서해요",
          "speaker_id": "1",
          "time": "20210826 10:32"
        },
        {
          "id": "MDRW2100014736.1.23",
          "form": "네 아 딱좋네요",
          "original_form": "네 아 딱좋네요",
          "speaker_id": "1",
          "time": "20210826 10:33"
        },
        {
          "id": "MDRW2100014736.1.24",
          "form": "기분도 살랑",
          "original_form": "기분도 살랑",
          "speaker_id": "1",
          "time": "20210826 10:33"
        },
        {
          "id": "MDRW2100014736.1.25",
          "form": "오늘이 그날 ㅋㅋ",
          "original_form": "오늘이 그날 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:33"
        },
        {
          "id": "MDRW2100014736.1.26",
          "form": "요샌 청소해도 바닥이 ㅡㅡ",
          "original_form": "요샌 청소해도 바닥이 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210826 10:33"
        },
        {
          "id": "MDRW2100014736.1.27",
          "form": "끈적 ㅎㅎ",
          "original_form": "끈적 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 10:33"
        },
        {
          "id": "MDRW2100014736.1.28",
          "form": "언능 넣어버리세요 ㅎ",
          "original_form": "언능 넣어버리세요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 10:33"
        },
        {
          "id": "MDRW2100014736.1.29",
          "form": "여긴 흐려요",
          "original_form": "여긴 흐려요",
          "speaker_id": "1",
          "time": "20210826 10:33"
        },
        {
          "id": "MDRW2100014736.1.30",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 10:33"
        },
        {
          "id": "MDRW2100014736.1.31",
          "form": "비와서 습해서 글쵸",
          "original_form": "비와서 습해서 글쵸",
          "speaker_id": "2",
          "time": "20210826 10:33"
        }
      ]
    }
