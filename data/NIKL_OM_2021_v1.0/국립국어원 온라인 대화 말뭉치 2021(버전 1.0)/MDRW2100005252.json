      "utterance": [
        {
          "id": "MDRW2100005252.1.1",
          "form": "코로나 끝나면 가고 싶은 여행지 있으세요?",
          "original_form": "코로나 끝나면 가고 싶은 여행지 있으세요?",
          "speaker_id": "1",
          "time": "20210821 21:27"
        },
        {
          "id": "MDRW2100005252.1.2",
          "form": "해외여행이 가고싶네요",
          "original_form": "해외여행이 가고싶네요",
          "speaker_id": "2",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005252.1.3",
          "form": "가본적 없거든요",
          "original_form": "가본적 없거든요",
          "speaker_id": "2",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005252.1.4",
          "form": "가까운 곳이라도 너무 가고싶어요",
          "original_form": "가까운 곳이라도 너무 가고싶어요",
          "speaker_id": "2",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005252.1.5",
          "form": "저도 가본 적 없어요",
          "original_form": "저도 가본 적 없어요",
          "speaker_id": "1",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005252.1.6",
          "form": "요즘에 바다가 보고 싶어서 괌이나 몰디브 이런 곳 가고 싶어요",
          "original_form": "요즘에 바다가 보고 싶어서 괌이나 몰디브 이런 곳 가고 싶어요",
          "speaker_id": "1",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005252.1.7",
          "form": "와 너무좋죠",
          "original_form": "와 너무좋죠",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005252.1.8",
          "form": "저도 바다를 진짜 좋아해요",
          "original_form": "저도 바다를 진짜 좋아해요",
          "speaker_id": "2",
          "time": "20210821 21:57"
        },
        {
          "id": "MDRW2100005252.1.9",
          "form": "바다는 소리 냄새 반짝거리는거 전부",
          "original_form": "바다는 소리 냄새 반짝거리는거 전부",
          "speaker_id": "2",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005252.1.10",
          "form": "너무 매력적인것같아요",
          "original_form": "너무 매력적인것같아요",
          "speaker_id": "2",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005252.1.11",
          "form": "이번 휴가때 잠깐이라도 다녀오고싶었는데 못가서 아쉽네요 ㅠㅠ",
          "original_form": "이번 휴가때 잠깐이라도 다녀오고싶었는데 못가서 아쉽네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005252.1.12",
          "form": "국내 바다는 어디가 좋으세요?",
          "original_form": "국내 바다는 어디가 좋으세요?",
          "speaker_id": "2",
          "time": "20210821 21:58"
        },
        {
          "id": "MDRW2100005252.1.13",
          "form": "강원도랑 부산 정도만 가봤어요",
          "original_form": "강원도랑 부산 정도만 가봤어요",
          "speaker_id": "1",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005252.1.14",
          "form": "부산이 제일 좋았던 거 같아요",
          "original_form": "부산이 제일 좋았던 거 같아요",
          "speaker_id": "1",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005252.1.15",
          "form": "name2 님은요?",
          "original_form": "&name2& 님은요?",
          "speaker_id": "1",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005252.1.16",
          "form": "저는 부산은 안가봤고 강원도는 가봤었는대",
          "original_form": "저는 부산은 안가봤고 강원도는 가봤었는대",
          "speaker_id": "2",
          "time": "20210821 22:07"
        },
        {
          "id": "MDRW2100005252.1.17",
          "form": "좋았었어요",
          "original_form": "좋았었어요",
          "speaker_id": "2",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005252.1.18",
          "form": "부산은 바다가 예쁘죠?",
          "original_form": "부산은 바다가 예쁘죠?",
          "speaker_id": "2",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005252.1.19",
          "form": "네 조명 같은 것도 잘 해놔서 예쁘더라고요",
          "original_form": "네 조명 같은 것도 잘 해놔서 예쁘더라고요",
          "speaker_id": "1",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005252.1.20",
          "form": "바다가면 횟집 가는 편이신가요?",
          "original_form": "바다가면 횟집 가는 편이신가요?",
          "speaker_id": "1",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005252.1.21",
          "form": "수산시장을 많이가요",
          "original_form": "수산시장을 많이가요",
          "speaker_id": "2",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005252.1.22",
          "form": "횟집은 이박 삼일가서 한끼정도?",
          "original_form": "횟집은 이박 삼일가서 한끼정도?",
          "speaker_id": "2",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005252.1.23",
          "form": "맛집 있었나요?",
          "original_form": "맛집 있었나요?",
          "speaker_id": "2",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005252.1.24",
          "form": "수산 시장은 한번도 안 가봤는데 가보고 싶네요",
          "original_form": "수산 시장은 한번도 안 가봤는데 가보고 싶네요",
          "speaker_id": "1",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005252.1.25",
          "form": "딱히 생각나는 곳은 없는데 여행가서 먹으면 다 맛있더라고요",
          "original_form": "딱히 생각나는 곳은 없는데 여행가서 먹으면 다 맛있더라고요",
          "speaker_id": "1",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005252.1.26",
          "form": "수산시장에서 주로 어떤 거 사시나요?",
          "original_form": "수산시장에서 주로 어떤 거 사시나요?",
          "speaker_id": "1",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005252.1.27",
          "form": "그쵸 여행가면 진짜 맛없는 곳 아니면 다 만족하고 나오는 것 같아요",
          "original_form": "그쵸 여행가면 진짜 맛없는 곳 아니면 다 만족하고 나오는 것 같아요",
          "speaker_id": "2",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005252.1.28",
          "form": "광어회 많이 먹죠! 가격이 횟집보다 싸서 좋았어요",
          "original_form": "광어회 많이 먹죠! 가격이 횟집보다 싸서 좋았어요",
          "speaker_id": "2",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005252.1.29",
          "form": "근처에 꼭 새우 튀김집 있어서 거서도 많이 사갔고요",
          "original_form": "근처에 꼭 새우 튀김집 있어서 거서도 많이 사갔고요",
          "speaker_id": "2",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005252.1.30",
          "form": "저도 다음에는 수산시장 가봐야겠어요",
          "original_form": "저도 다음에는 수산시장 가봐야겠어요",
          "speaker_id": "1",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005252.1.31",
          "form": "빨리 코로나 진정되면 좋겠네요",
          "original_form": "빨리 코로나 진정되면 좋겠네요",
          "speaker_id": "1",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005252.1.32",
          "form": "무서워서 국내여행도 못 가니까 지쳐요",
          "original_form": "무서워서 국내여행도 못 가니까 지쳐요",
          "speaker_id": "1",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005252.1.33",
          "form": "맞아여 ㅠㅠ",
          "original_form": "맞아여 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005252.1.34",
          "form": "휴가철에 놀러못가는 것 만큼 슬픈게 또 없더라고여",
          "original_form": "휴가철에 놀러못가는 것 만큼 슬픈게 또 없더라고여",
          "speaker_id": "2",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005252.1.35",
          "form": "사람이 가끔은 휴식시간을 가져줘야하는데..",
          "original_form": "사람이 가끔은 휴식시간을 가져줘야하는데..",
          "speaker_id": "2",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005252.1.36",
          "form": "저도 진짜 휴가가 이렇게 소중했던 적이 없는 거 같아요",
          "original_form": "저도 진짜 휴가가 이렇게 소중했던 적이 없는 거 같아요",
          "speaker_id": "1",
          "time": "20210821 22:42"
        }
      ]
