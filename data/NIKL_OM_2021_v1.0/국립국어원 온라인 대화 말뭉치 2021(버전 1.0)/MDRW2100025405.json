      "utterance": [
        {
          "id": "MDRW2100025405.1.1",
          "form": "이상형이 어떻게 되세요?",
          "original_form": "이상형이 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210901 10:57"
        },
        {
          "id": "MDRW2100025405.1.2",
          "form": "저는 말 잘듣는 조신남이요",
          "original_form": "저는 말 잘듣는 조신남이요",
          "speaker_id": "2",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100025405.1.3",
          "form": "오오 지금 연애중이세요??",
          "original_form": "오오 지금 연애중이세요??",
          "speaker_id": "1",
          "time": "20210901 11:03"
        },
        {
          "id": "MDRW2100025405.1.4",
          "form": "아뇨! 안하고있어요!",
          "original_form": "아뇨! 안하고있어요!",
          "speaker_id": "2",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100025405.1.5",
          "form": "지금 생활하는것도 벅찬데 신경쓰기 귀찮더라고요ㅠ",
          "original_form": "지금 생활하는것도 벅찬데 신경쓰기 귀찮더라고요ㅠ",
          "speaker_id": "2",
          "time": "20210901 11:04"
        },
        {
          "id": "MDRW2100025405.1.6",
          "form": "연애중이세요~?",
          "original_form": "연애중이세요~?",
          "speaker_id": "2",
          "time": "20210901 11:05"
        },
        {
          "id": "MDRW2100025405.1.7",
          "form": "저는 모솔이에요 ㅠㅠ",
          "original_form": "저는 모솔이에요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 11:05"
        },
        {
          "id": "MDRW2100025405.1.8",
          "form": "소개도 받기 꺼려지더라구요 ㅋㅋ",
          "original_form": "소개도 받기 꺼려지더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:05"
        },
        {
          "id": "MDRW2100025405.1.9",
          "form": "아ㅠㅠ 맞아요 어떤 맘일지 알겠어요 저도 모솔 기간이 길었어서",
          "original_form": "아ㅠㅠ 맞아요 어떤 맘일지 알겠어요 저도 모솔 기간이 길었어서",
          "speaker_id": "2",
          "time": "20210901 11:06"
        },
        {
          "id": "MDRW2100025405.1.10",
          "form": "오 몇살때 탈출 하셨나요!",
          "original_form": "오 몇살때 탈출 하셨나요!",
          "speaker_id": "1",
          "time": "20210901 11:06"
        },
        {
          "id": "MDRW2100025405.1.11",
          "form": "음 이십대 후반?",
          "original_form": "음 이십대 후반?",
          "speaker_id": "2",
          "time": "20210901 11:06"
        },
        {
          "id": "MDRW2100025405.1.12",
          "form": "근데 별거없어서 그냥 혼자삽니다",
          "original_form": "근데 별거없어서 그냥 혼자삽니다",
          "speaker_id": "2",
          "time": "20210901 11:07"
        },
        {
          "id": "MDRW2100025405.1.13",
          "form": "오 저는 26살이예욬ㅋㅋㅋ 아직도 연애 생각는 없어요 ㅋㅋ",
          "original_form": "오 저는 26살이예욬ㅋㅋㅋ 아직도 연애 생각는 없어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:07"
        },
        {
          "id": "MDRW2100025405.1.14",
          "form": "그러시구낭",
          "original_form": "그러시구낭",
          "speaker_id": "2",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100025405.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210901 11:09"
        },
        {
          "id": "MDRW2100025405.1.16",
          "form": "혼자가 편해요!",
          "original_form": "혼자가 편해요!",
          "speaker_id": "2",
          "time": "20210901 11:10"
        },
        {
          "id": "MDRW2100025405.1.17",
          "form": "사실 혼자도 신경쓸게 많은데",
          "original_form": "사실 혼자도 신경쓸게 많은데",
          "speaker_id": "2",
          "time": "20210901 11:10"
        },
        {
          "id": "MDRW2100025405.1.18",
          "form": "어휴",
          "original_form": "어휴",
          "speaker_id": "2",
          "time": "20210901 11:10"
        },
        {
          "id": "MDRW2100025405.1.19",
          "form": "근데 주변에서는 연애도 못하냐고 잔소리가 심하드라구요 ㅜㅜ",
          "original_form": "근데 주변에서는 연애도 못하냐고 잔소리가 심하드라구요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 11:11"
        },
        {
          "id": "MDRW2100025405.1.20",
          "form": "아 그럴땐 그냥 당당하게 말씀하세요",
          "original_form": "아 그럴땐 그냥 당당하게 말씀하세요",
          "speaker_id": "2",
          "time": "20210901 11:11"
        },
        {
          "id": "MDRW2100025405.1.21",
          "form": "저도 그랬는데 뭔상관이냐고 그러니깐 그 이후론 아무말안하는",
          "original_form": "저도 그랬는데 뭔상관이냐고 그러니깐 그 이후론 아무말안하는",
          "speaker_id": "2",
          "time": "20210901 11:12"
        },
        {
          "id": "MDRW2100025405.1.22",
          "form": "저도 그럴려고요 ㅎㄹ",
          "original_form": "저도 그럴려고요 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210901 11:13"
        }
      ]
