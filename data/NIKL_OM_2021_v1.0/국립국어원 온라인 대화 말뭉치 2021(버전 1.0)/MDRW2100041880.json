      "utterance": [
        {
          "id": "MDRW2100041880.1.1",
          "form": "점심드셨어요?",
          "original_form": "점심드셨어요?",
          "speaker_id": "1",
          "time": "20210906 16:59"
        },
        {
          "id": "MDRW2100041880.1.2",
          "form": "넵 아까 죽!",
          "original_form": "넵 아까 죽!",
          "speaker_id": "2",
          "time": "20210906 16:59"
        },
        {
          "id": "MDRW2100041880.1.3",
          "form": "앗 죽?속이불편하세요?",
          "original_form": "앗 죽?속이불편하세요?",
          "speaker_id": "1",
          "time": "20210906 17:00"
        },
        {
          "id": "MDRW2100041880.1.4",
          "form": "넵 ㅜㅜ 그거 때문에 조퇴했네요",
          "original_form": "넵 ㅜㅜ 그거 때문에 조퇴했네요",
          "speaker_id": "2",
          "time": "20210906 17:01"
        },
        {
          "id": "MDRW2100041880.1.5",
          "form": "ㅜㅜ큐ㅠㅠ아휴",
          "original_form": "ㅜㅜ큐ㅠㅠ아휴",
          "speaker_id": "1",
          "time": "20210906 17:07"
        },
        {
          "id": "MDRW2100041880.1.6",
          "form": "주제가좋지않았네요",
          "original_form": "주제가좋지않았네요",
          "speaker_id": "1",
          "time": "20210906 17:07"
        },
        {
          "id": "MDRW2100041880.1.7",
          "form": "지금은좀어떠세요ㅜ",
          "original_form": "지금은좀어떠세요ㅜ",
          "speaker_id": "1",
          "time": "20210906 17:07"
        },
        {
          "id": "MDRW2100041880.1.8",
          "form": "ㅠㅠㅋㅋ 괜찮아요 아마 저녁은 평범하게 먹을수있을듯..",
          "original_form": "ㅠㅠㅋㅋ 괜찮아요 아마 저녁은 평범하게 먹을수있을듯..",
          "speaker_id": "2",
          "time": "20210906 17:07"
        },
        {
          "id": "MDRW2100041880.1.9",
          "form": "힝 입맛은있어요?",
          "original_form": "힝 입맛은있어요?",
          "speaker_id": "1",
          "time": "20210906 17:09"
        },
        {
          "id": "MDRW2100041880.1.10",
          "form": "따뜻한거드세요",
          "original_form": "따뜻한거드세요",
          "speaker_id": "1",
          "time": "20210906 17:09"
        },
        {
          "id": "MDRW2100041880.1.11",
          "form": "넵 갈비탕 데워먹게요",
          "original_form": "넵 갈비탕 데워먹게요",
          "speaker_id": "2",
          "time": "20210906 17:09"
        },
        {
          "id": "MDRW2100041880.1.12",
          "form": "저녁 언제 드세요?ㅎㅎ",
          "original_form": "저녁 언제 드세요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 17:09"
        },
        {
          "id": "MDRW2100041880.1.13",
          "form": "한6시쯤이요ㅎㅎ",
          "original_form": "한6시쯤이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 17:10"
        },
        {
          "id": "MDRW2100041880.1.14",
          "form": "맛있는메뉴네요",
          "original_form": "맛있는메뉴네요",
          "speaker_id": "1",
          "time": "20210906 17:10"
        },
        {
          "id": "MDRW2100041880.1.15",
          "form": "넵 ㅎㅎ 지금 먹을 준비 중!",
          "original_form": "넵 ㅎㅎ 지금 먹을 준비 중!",
          "speaker_id": "2",
          "time": "20210906 17:10"
        },
        {
          "id": "MDRW2100041880.1.16",
          "form": "ㅎㅎ저는분식당겨서고민중이에요",
          "original_form": "ㅎㅎ저는분식당겨서고민중이에요",
          "speaker_id": "1",
          "time": "20210906 17:11"
        },
        {
          "id": "MDRW2100041880.1.17",
          "form": "좋아하세요떡볶이?",
          "original_form": "좋아하세요떡볶이?",
          "speaker_id": "1",
          "time": "20210906 17:11"
        },
        {
          "id": "MDRW2100041880.1.18",
          "form": "오 넵 떡볶이",
          "original_form": "오 넵 떡볶이",
          "speaker_id": "2",
          "time": "20210906 17:11"
        },
        {
          "id": "MDRW2100041880.1.19",
          "form": "비싸도 맛있다.",
          "original_form": "비싸도 맛있다.",
          "speaker_id": "2",
          "time": "20210906 17:11"
        },
        {
          "id": "MDRW2100041880.1.20",
          "form": "ㅎㅎㅎ맞아요",
          "original_form": "ㅎㅎㅎ맞아요",
          "speaker_id": "1",
          "time": "20210906 17:13"
        },
        {
          "id": "MDRW2100041880.1.21",
          "form": "요새로제도핫하던데",
          "original_form": "요새로제도핫하던데",
          "speaker_id": "1",
          "time": "20210906 17:13"
        },
        {
          "id": "MDRW2100041880.1.22",
          "form": "못먹어봤어요",
          "original_form": "못먹어봤어요",
