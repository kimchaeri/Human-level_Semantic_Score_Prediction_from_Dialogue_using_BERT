        {
          "id": "MDRW2100023221.1.1",
          "form": "육류좋아하세요?",
          "original_form": "육류좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100023221.1.2",
          "form": "네 고기는 다 좋아해요 ㅎㅎ",
          "original_form": "네 고기는 다 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100023221.1.3",
          "form": "육류 좋아하시나요?",
          "original_form": "육류 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100023221.1.4",
          "form": "없어서 못먹어요",
          "original_form": "없어서 못먹어요",
          "speaker_id": "1",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100023221.1.5",
          "form": "저녁은 드셨나요?",
          "original_form": "저녁은 드셨나요?",
          "speaker_id": "1",
          "time": "20210831 21:33"
        },
        {
          "id": "MDRW2100023221.1.6",
          "form": "던킨도너츠요 ㅋㅋ name1은 드셨나요?",
          "original_form": "던킨도너츠요 ㅋㅋ &name1&은 드셨나요?",
          "speaker_id": "2",
          "time": "20210831 21:42"
        },
        {
          "id": "MDRW2100023221.1.7",
          "form": "도너츠 좋죠",
          "original_form": "도너츠 좋죠",
          "speaker_id": "1",
          "time": "20210831 21:44"
        },
        {
          "id": "MDRW2100023221.1.8",
          "form": "전 닭갈비쓰~",
          "original_form": "전 닭갈비쓰~",
          "speaker_id": "1",
          "time": "20210831 21:44"
        },
        {
          "id": "MDRW2100023221.1.9",
          "form": "맛있었겠어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맛있었겠어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:58"
        },
        {
          "id": "MDRW2100023221.1.10",
          "form": "카카오닙스 도넛이 맛있어요 ㅎㅎ",
          "original_form": "카카오닙스 도넛이 맛있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100023221.1.11",
          "form": "넵 아침에도 저녁에서 낼아침에도 먹을 예정이에요",
          "original_form": "넵 아침에도 저녁에서 낼아침에도 먹을 예정이에요",
          "speaker_id": "1",
          "time": "20210831 22:00"
        },
        {
          "id": "MDRW2100023221.1.12",
          "form": "많이 하셨나보네요ㅋㅋㄱ 아니면 맛있거나?",
          "original_form": "많이 하셨나보네요ㅋㅋㄱ 아니면 맛있거나?",
          "speaker_id": "2",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100023221.1.13",
          "form": "맛잇는데 다 먹어가요ㅜ",
          "original_form": "맛잇는데 다 먹어가요ㅜ",
          "speaker_id": "1",
          "time": "20210831 22:10"
        },
        {
          "id": "MDRW2100023221.1.14",
          "form": "그럴땐 아쉽죠.. ㅜ",
          "original_form": "그럴땐 아쉽죠.. ㅜ",
          "speaker_id": "2",
          "time": "20210831 22:26"
        },
        {
          "id": "MDRW2100023221.1.15",
          "form": "흠 전 내일 감자탕을 먹겠어요.. 비슷하겠죠...? 양념은요 ㅋㅋㅋ",
          "original_form": "흠 전 내일 감자탕을 먹겠어요.. 비슷하겠죠...? 양념은요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:27"
        },
        {
          "id": "MDRW2100023221.1.16",
          "form": "ㅋㅋ 아? 전혀다른데요???",
          "original_form": "ㅋㅋ 아? 전혀다른데요???",
          "speaker_id": "1",
          "time": "20210831 22:28"
        },
        {
          "id": "MDRW2100023221.1.17",
          "form": "직접 집에서 하신건가요?",
          "original_form": "직접 집에서 하신건가요?",
          "speaker_id": "2",
          "time": "20210831 22:40"
        },
        {
          "id": "MDRW2100023221.1.18",
          "form": "사다했어요 엄마가ㅋㅋ",
          "original_form": "사다했어요 엄마가ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:41"
        },
        {
          "id": "MDRW2100023221.1.19",
          "form": "아항 ㅋㅋㅋ 밀키트같은거면 비슷할거라 생각했어용",
          "original_form": "아항 ㅋㅋㅋ 밀키트같은거면 비슷할거라 생각했어용",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100023221.1.20",
          "form": "ㅋㅋ 감자탕 직접해드실건가요?",
          "original_form": "ㅋㅋ 감자탕 직접해드실건가요?",
