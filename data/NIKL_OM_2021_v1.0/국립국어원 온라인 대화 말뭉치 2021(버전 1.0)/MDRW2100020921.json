      "utterance": [
        {
          "id": "MDRW2100020921.1.1",
          "form": "어디감",
          "original_form": "어디감",
          "speaker_id": "2",
          "time": "20210828 14:11"
        },
        {
          "id": "MDRW2100020921.1.2",
          "form": "나지금 제부도 가는중이야",
          "original_form": "나지금 제부도 가는중이야",
          "speaker_id": "1",
          "time": "20210828 14:11"
        },
        {
          "id": "MDRW2100020921.1.3",
          "form": "떠남?",
          "original_form": "떠남?",
          "speaker_id": "2",
          "time": "20210828 14:11"
        },
        {
          "id": "MDRW2100020921.1.4",
          "form": "조개구워먹으러 가는중",
          "original_form": "조개구워먹으러 가는중",
          "speaker_id": "1",
          "time": "20210828 14:11"
        },
        {
          "id": "MDRW2100020921.1.5",
          "form": "오ㅋㅋ맛집탐강",
          "original_form": "오ㅋㅋ맛집탐강",
          "speaker_id": "2",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100020921.1.6",
          "form": "제주도 대신 제부도",
          "original_form": "제주도 대신 제부도",
          "speaker_id": "2",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100020921.1.7",
          "form": "근데 물길 막혀있음 못들어감 ㅋㅋ",
          "original_form": "근데 물길 막혀있음 못들어감 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100020921.1.8",
          "form": "전 엄마집으로 옴",
          "original_form": "전 엄마집으로 옴",
          "speaker_id": "2",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100020921.1.9",
          "form": "와우 친정여행",
          "original_form": "와우 친정여행",
          "speaker_id": "1",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100020921.1.10",
          "form": "ㄱㄲㅋ조개 어케하죠",
          "original_form": "ㄱㄲㅋ조개 어케하죠",
          "speaker_id": "2",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100020921.1.11",
          "form": "들어가지도 못하고 입구에서 발동동",
          "original_form": "들어가지도 못하고 입구에서 발동동",
          "speaker_id": "1",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100020921.1.12",
          "form": "친정집이 최고의 여행지",
          "original_form": "친정집이 최고의 여행지",
          "speaker_id": "2",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100020921.1.13",
          "form": "기도하면서 가는중",
          "original_form": "기도하면서 가는중",
          "speaker_id": "1",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100020921.1.14",
          "form": "ㅋㅋㅋ물길 열리긴함?",
          "original_form": "ㅋㅋㅋ물길 열리긴함?",
          "speaker_id": "2",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100020921.1.15",
          "form": "친정은 특급호텔",
          "original_form": "친정은 특급호텔",
          "speaker_id": "1",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100020921.1.16",
          "form": "한밤에 열리는거 아닌가몰라",
          "original_form": "한밤에 열리는거 아닌가몰라",
          "speaker_id": "1",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100020921.1.17",
          "form": "ㅋㅋㅋ지금 낮잠타임이라",
          "original_form": "ㅋㅋㅋ지금 낮잠타임이라",
          "speaker_id": "2",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100020921.1.18",
          "form": "ㅋㅋ그럼 밤까지 제발어",
          "original_form": "ㅋㅋ그럼 밤까지 제발어",
          "speaker_id": "2",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100020921.1.19",
          "form": "엄마가 옥상에서 고기 구워준댔는데",
          "original_form": "엄마가 옥상에서 고기 구워준댔는데",
          "speaker_id": "1",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100020921.1.20",
          "form": "오늘은 뭐든 구울거라고",
          "original_form": "오늘은 뭐든 구울거라고",
          "speaker_id": "1",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100020921.1.21",
          "form": "굽여행",
          "original_form": "굽여행",
          "speaker_id": "1",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100020921.1.22",
          "form": "ㅋㅋ오 그게 더 땡긴다",
          "original_form": "ㅋㅋ오 그게 더 땡긴다",
          "speaker_id": "2",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100020921.1.23",
          "form": "고기파티",
          "original_form": "고기파티",
          "speaker_id": "2",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100020921.1.24",
          "form": "옥상에 그릴 있음 ㅋㅋ",
          "original_form": "옥상에 그릴 있음 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:15"
        },
        {
          "id": "MDRW2100020921.1.25",
          "form": "ㅋㅋㅋ젤부러워",
          "original_form": "ㅋㅋㅋ젤부러워",
          "speaker_id": "2",
          "time": "20210828 14:15"
        },
        {
          "id": "MDRW2100020921.1.26",
          "form": "엄마집 옥상에 그릴앤평상 세트있음",
          "original_form": "엄마집 옥상에 그릴앤평상 세트있음",
          "speaker_id": "1",
          "time": "20210828 14:15"
        },
        {
          "id": "MDRW2100020921.1.27",
          "form": "여행 왜가여",
          "original_form": "여행 왜가여",
          "speaker_id": "2",
          "time": "20210828 14:15"
        },
        {
          "id": "MDRW2100020921.1.28",
          "form": "거기가 펜션이라며",
          "original_form": "거기가 펜션이라며",
          "speaker_id": "1",
          "time": "20210828 14:15"
        },
        {
          "id": "MDRW2100020921.1.29",
          "form": "옥상이 바로그곳",
          "original_form": "옥상이 바로그곳",
          "speaker_id": "2",
          "time": "20210828 14:15"
        }
      ]
    }
  ]
