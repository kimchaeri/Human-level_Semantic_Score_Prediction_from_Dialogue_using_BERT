      "utterance": [
        {
          "id": "MDRW2100038605.1.1",
          "form": "해외여행 조아하시나요",
          "original_form": "해외여행 조아하시나요",
          "speaker_id": "1",
          "time": "20210902 16:48"
        },
        {
          "id": "MDRW2100038605.1.2",
          "form": "넹 좋아합니다",
          "original_form": "넹 좋아합니다",
          "speaker_id": "2",
          "time": "20210902 16:51"
        },
        {
          "id": "MDRW2100038605.1.3",
          "form": "해외여행 가보셨나요",
          "original_form": "해외여행 가보셨나요",
          "speaker_id": "2",
          "time": "20210902 16:52"
        },
        {
          "id": "MDRW2100038605.1.4",
          "form": "넹 어디가셧어용??",
          "original_form": "넹 어디가셧어용??",
          "speaker_id": "1",
          "time": "20210902 16:53"
        },
        {
          "id": "MDRW2100038605.1.5",
          "form": "저 많은곳 여기저기 가봤어용",
          "original_form": "저 많은곳 여기저기 가봤어용",
          "speaker_id": "2",
          "time": "20210902 16:54"
        },
        {
          "id": "MDRW2100038605.1.6",
          "form": "지금 가장 가고싶은건 미국이네요",
          "original_form": "지금 가장 가고싶은건 미국이네요",
          "speaker_id": "2",
          "time": "20210902 16:54"
        },
        {
          "id": "MDRW2100038605.1.7",
          "form": "우왕..부럽당",
          "original_form": "우왕..부럽당",
          "speaker_id": "1",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100038605.1.8",
          "form": "미국어디용",
          "original_form": "미국어디용",
          "speaker_id": "1",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100038605.1.9",
          "form": "LA요 ㅠㅠ",
          "original_form": "LA요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 17:09"
        },
        {
          "id": "MDRW2100038605.1.10",
          "form": "우옹 ㅠㅜ 엘에이 특별히 가고싶음이유가?",
          "original_form": "우옹 ㅠㅜ 엘에이 특별히 가고싶음이유가?",
          "speaker_id": "1",
          "time": "20210902 17:09"
        },
        {
          "id": "MDRW2100038605.1.11",
          "form": "움.. 뭔가 바닷가도 좋구 ㅋㅋ 늘 날씨도 좋구 하니까 그런 긍정적인 에너지.받고싶어요",
          "original_form": "움.. 뭔가 바닷가도 좋구 ㅋㅋ 늘 날씨도 좋구 하니까 그런 긍정적인 에너지.받고싶어요",
          "speaker_id": "2",
          "time": "20210902 17:10"
        },
        {
          "id": "MDRW2100038605.1.12",
          "form": "그랜드캐넌처럼",
          "original_form": "그랜드캐넌처럼",
          "speaker_id": "2",
          "time": "20210902 17:10"
        },
        {
          "id": "MDRW2100038605.1.13",
          "form": "자연듀 멋있구용 ㅠㅋㅋㅋㅋ",
          "original_form": "자연듀 멋있구용 ㅠㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:10"
        },
        {
          "id": "MDRW2100038605.1.14",
          "form": "긍정에너지!근데 엘에이 겨울에도 날 좋나요?",
          "original_form": "긍정에너지!근데 엘에이 겨울에도 날 좋나요?",
          "speaker_id": "1",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100038605.1.15",
          "form": "모루겠어용 ㅋㅋㅋ 왠지 좋을 것 같아요 ㅋㅋㅋㅋ 그치만.전 여름에.가고싶네요 ㅋㅋㅋㅋ",
          "original_form": "모루겠어용 ㅋㅋㅋ 왠지 좋을 것 같아요 ㅋㅋㅋㅋ 그치만.전 여름에.가고싶네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:12"
        },
        {
          "id": "MDRW2100038605.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 여름이최고죠 하 ..나도가고싶다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 여름이최고죠 하 ..나도가고싶다",
          "speaker_id": "1",
          "time": "20210902 17:13"
        },
        {
          "id": "MDRW2100038605.1.17",
          "form": "유럽은 어디가조아요",
          "original_form": "유럽은 어디가조아요",
          "speaker_id": "1",
          "time": "20210902 17:13"
        },
        {
          "id": "MDRW2100038605.1.18",
          "form": "흐음 유럽... 유럽 이제 간지 10년도 넘어서 옛날 얘기지만ㅋㅋㅋ",
          "original_form": "흐음 유럽... 유럽 이제 간지 10년도 넘어서 옛날 얘기지만ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:15"
        },
        {
          "id": "MDRW2100038605.1.19",
          "form": "저는 포르투갈좋았어용",
          "original_form": "저는 포르투갈좋았어용",
          "speaker_id": "2",
          "time": "20210902 17:15"
        },
        {
          "id": "MDRW2100038605.1.20",
          "form": "크... 날씨? 풍경 ?",
          "original_form": "크... 날씨? 풍경 ?",
          "speaker_id": "1",
          "time": "20210902 17:17"
        },
        {
          "id": "MDRW2100038605.1.21",
          "form": "네 날씨듀좋았거 풍경도 좋고 ㅋㅋ 나라분위기도 좋았어요",
          "original_form": "네 날씨듀좋았거 풍경도 좋고 ㅋㅋ 나라분위기도 좋았어요",
          "speaker_id": "2",
          "time": "20210902 17:19"
        },
        {
          "id": "MDRW2100038605.1.22",
          "form": "음식도 맛있고ㅋ",
          "original_form": "음식도 맛있고ㅋ",
