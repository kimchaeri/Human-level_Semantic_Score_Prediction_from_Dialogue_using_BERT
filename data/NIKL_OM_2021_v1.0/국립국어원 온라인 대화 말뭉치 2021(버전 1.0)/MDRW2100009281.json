      "utterance": [
        {
          "id": "MDRW2100009281.1.1",
          "form": "혹시 반려동물 키우세요?",
          "original_form": "혹시 반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210822 22:32"
        },
        {
          "id": "MDRW2100009281.1.2",
          "form": "아 아니요",
          "original_form": "아 아니요",
          "speaker_id": "2",
          "time": "20210822 22:32"
        },
        {
          "id": "MDRW2100009281.1.3",
          "form": "키우세요?",
          "original_form": "키우세요?",
          "speaker_id": "2",
          "time": "20210822 22:32"
        },
        {
          "id": "MDRW2100009281.1.4",
          "form": "저는 고양이 키우고 있어요",
          "original_form": "저는 고양이 키우고 있어요",
          "speaker_id": "1",
          "time": "20210822 22:32"
        },
        {
          "id": "MDRW2100009281.1.5",
          "form": "어느종이에요?",
          "original_form": "어느종이에요?",
          "speaker_id": "2",
          "time": "20210822 22:32"
        },
        {
          "id": "MDRW2100009281.1.6",
          "form": "아메리칸숏이요!",
          "original_form": "아메리칸숏이요!",
          "speaker_id": "1",
          "time": "20210822 22:33"
        },
        {
          "id": "MDRW2100009281.1.7",
          "form": "약간 코리안 숏 비슷해요",
          "original_form": "약간 코리안 숏 비슷해요",
          "speaker_id": "1",
          "time": "20210822 22:33"
        },
        {
          "id": "MDRW2100009281.1.8",
          "form": "코리안숏??",
          "original_form": "코리안숏??",
          "speaker_id": "2",
          "time": "20210822 22:33"
        },
        {
          "id": "MDRW2100009281.1.9",
          "form": "그 악간 길고양이들이 거의 코리안숏이라고 생각하심 편해요 ㅋㅋㅋ",
          "original_form": "그 악간 길고양이들이 거의 코리안숏이라고 생각하심 편해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009281.1.10",
          "form": "고등어들",
          "original_form": "고등어들",
          "speaker_id": "1",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009281.1.11",
          "form": "아 그렇구나",
          "original_form": "아 그렇구나",
          "speaker_id": "2",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009281.1.12",
          "form": "입양하신건가요?",
          "original_form": "입양하신건가요?",
          "speaker_id": "2",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009281.1.13",
          "form": "지인이 못키운다해서",
          "original_form": "지인이 못키운다해서",
          "speaker_id": "1",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009281.1.14",
          "form": "제가 받아 키우게 됐어요",
          "original_form": "제가 받아 키우게 됐어요",
          "speaker_id": "1",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009281.1.15",
          "form": "몇살이에요?",
          "original_form": "몇살이에요?",
          "speaker_id": "2",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009281.1.16",
          "form": "7살이요!",
          "original_form": "7살이요!",
          "speaker_id": "1",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100009281.1.17",
          "form": "ㅜㅜ 나이 많이 먹었어요",
          "original_form": "ㅜㅜ 나이 많이 먹었어요",
          "speaker_id": "1",
          "time": "20210822 22:35"
        },
        {
          "id": "MDRW2100009281.1.18",
          "form": "고양이는 평균수명이 어떻게 돼요?",
          "original_form": "고양이는 평균수명이 어떻게 돼요?",
          "speaker_id": "2",
          "time": "20210822 22:35"
        },
        {
          "id": "MDRW2100009281.1.19",
          "form": "보통 15년 전후인 것 같아요 ㅜㅜ",
          "original_form": "보통 15년 전후인 것 같아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 22:35"
        },
        {
          "id": "MDRW2100009281.1.20",
          "form": "아쉬워요 ㅠㅠ",
          "original_form": "아쉬워요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 22:35"
        },
        {
          "id": "MDRW2100009281.1.21",
          "form": "아 정말 생각보다 짧네요",
          "original_form": "아 정말 생각보다 짧네요",
          "speaker_id": "2",
          "time": "20210822 22:35"
        },
        {
          "id": "MDRW2100009281.1.22",
          "form": "네 ㅜㅜ",
          "original_form": "네 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 22:35"
        }
      ]
