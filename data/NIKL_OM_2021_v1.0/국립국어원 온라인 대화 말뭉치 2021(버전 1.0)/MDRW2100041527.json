      "utterance": [
        {
          "id": "MDRW2100041527.1.1",
          "form": "안녕하세요 \n동아리 드셨나요?",
          "original_form": "안녕하세요 \n동아리 드셨나요?",
          "speaker_id": "1",
          "time": "20210906 09:06"
        },
        {
          "id": "MDRW2100041527.1.2",
          "form": "학부생시절 들었던 동아리가 있죠",
          "original_form": "학부생시절 들었던 동아리가 있죠",
          "speaker_id": "2",
          "time": "20210906 09:47"
        },
        {
          "id": "MDRW2100041527.1.3",
          "form": "무슨 동아리 드셨낭ㅅ?",
          "original_form": "무슨 동아리 드셨낭ㅅ?",
          "speaker_id": "1",
          "time": "20210906 09:50"
        },
        {
          "id": "MDRW2100041527.1.4",
          "form": "나요",
          "original_form": "나요",
          "speaker_id": "1",
          "time": "20210906 09:50"
        },
        {
          "id": "MDRW2100041527.1.5",
          "form": "독서토론동아리랑 뮤지컬 동아리 했어요",
          "original_form": "독서토론동아리랑 뮤지컬 동아리 했어요",
          "speaker_id": "2",
          "time": "20210906 09:51"
        },
        {
          "id": "MDRW2100041527.1.6",
          "form": "오 뮤지컬 동아리는 직접 극을 올리는 동아리인가요?",
          "original_form": "오 뮤지컬 동아리는 직접 극을 올리는 동아리인가요?",
          "speaker_id": "1",
          "time": "20210906 09:53"
        },
        {
          "id": "MDRW2100041527.1.7",
          "form": "네. 맞아요.",
          "original_form": "네. 맞아요.",
          "speaker_id": "2",
          "time": "20210906 10:09"
        },
        {
          "id": "MDRW2100041527.1.8",
          "form": "전 작가ㅎㅎ 였답니다",
          "original_form": "전 작가ㅎㅎ 였답니다",
          "speaker_id": "2",
          "time": "20210906 10:09"
        },
        {
          "id": "MDRW2100041527.1.9",
          "form": "오오 작가님이시군요 대단하세요",
          "original_form": "오오 작가님이시군요 대단하세요",
          "speaker_id": "1",
          "time": "20210906 10:14"
        },
        {
          "id": "MDRW2100041527.1.10",
          "form": "하하하하 멋모르고 쓴거죠",
          "original_form": "하하하하 멋모르고 쓴거죠",
          "speaker_id": "2",
          "time": "20210906 10:17"
        },
        {
          "id": "MDRW2100041527.1.11",
          "form": "그때 쓴 대본 책장 깊숙히 봉인해뒀어요",
          "original_form": "그때 쓴 대본 책장 깊숙히 봉인해뒀어요",
          "speaker_id": "2",
          "time": "20210906 10:18"
        },
        {
          "id": "MDRW2100041527.1.12",
          "form": "다시 볼 용기가 안나더라구요 ㅋㅋㅋ",
          "original_form": "다시 볼 용기가 안나더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 10:18"
        },
        {
          "id": "MDRW2100041527.1.13",
          "form": "ㅋㅋㅋㅋㅋ완성했다는 것이 중요하죠!",
          "original_form": "ㅋㅋㅋㅋㅋ완성했다는 것이 중요하죠!",
          "speaker_id": "1",
          "time": "20210906 10:19"
        },
        {
          "id": "MDRW2100041527.1.14",
          "form": "그건 그렇죠.",
          "original_form": "그건 그렇죠.",
          "speaker_id": "2",
          "time": "20210906 10:21"
        },
        {
          "id": "MDRW2100041527.1.15",
          "form": "것도 남배우 하나가 갑자기 그만두는바람에 3일만에 갈아엎은거라...",
          "original_form": "것도 남배우 하나가 갑자기 그만두는바람에 3일만에 갈아엎은거라...",
          "speaker_id": "2",
          "time": "20210906 10:22"
        },
        {
          "id": "MDRW2100041527.1.16",
          "form": "끝내고나니 하... 사람이 하려면 뭐든 할 수 있구나 싶어졌달까... ㅎㅎㅎ",
          "original_form": "끝내고나니 하... 사람이 하려면 뭐든 할 수 있구나 싶어졌달까... ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 10:22"
        },
        {
          "id": "MDRW2100041527.1.17",
          "form": "3일만에.... 엄청 고생하셨네요ㅠㅠㅠ",
          "original_form": "3일만에.... 엄청 고생하셨네요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 10:24"
        },
        {
          "id": "MDRW2100041527.1.18",
          "form": "ㅋㅋㅋㅋ큐ㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 10:48"
        },
        {
          "id": "MDRW2100041527.1.19",
          "form": "아직 자소서를 써본적은 없지만",
          "original_form": "아직 자소서를 써본적은 없지만",
          "speaker_id": "2",
          "time": "20210906 10:48"
        },
        {
          "id": "MDRW2100041527.1.20",
          "form": "추후 쓰게된다면 역경란에 쓸거에요ㅋㅋㅋㅠㅠㅠ",
          "original_form": "추후 쓰게된다면 역경란에 쓸거에요ㅋㅋㅋㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 10:48"
        },
        {
          "id": "MDRW2100041527.1.21",
          "form": "쓰세요ㅠㅠ정말 어마어마한 역경ㅠㅠㅠ",
          "original_form": "쓰세요ㅠㅠ정말 어마어마한 역경ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 11:04"
        },
        {
          "id": "MDRW2100041527.1.22",
          "form": "진심. 정말 어마무시했슴다ㅋㅋ",
          "original_form": "진심. 정말 어마무시했슴다ㅋㅋ",
          "speaker_id": "2",
