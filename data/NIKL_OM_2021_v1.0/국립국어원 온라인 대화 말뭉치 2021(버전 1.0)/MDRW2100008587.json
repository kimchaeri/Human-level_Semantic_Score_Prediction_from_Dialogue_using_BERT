      "utterance": [
        {
          "id": "MDRW2100008587.1.1",
          "form": "게임 즐겨 하세요?",
          "original_form": "게임 즐겨 하세요?",
          "speaker_id": "1",
          "time": "20210822 16:58"
        },
        {
          "id": "MDRW2100008587.1.2",
          "form": "네 게임 보는것도 좋아하고",
          "original_form": "네 게임 보는것도 좋아하고",
          "speaker_id": "2",
          "time": "20210822 16:59"
        },
        {
          "id": "MDRW2100008587.1.3",
          "form": "하는것도 좋아해요!",
          "original_form": "하는것도 좋아해요!",
          "speaker_id": "2",
          "time": "20210822 16:59"
        },
        {
          "id": "MDRW2100008587.1.4",
          "form": "보는 거 좋아하시면 혹시 롤 보세요?",
          "original_form": "보는 거 좋아하시면 혹시 롤 보세요?",
          "speaker_id": "1",
          "time": "20210822 16:59"
        },
        {
          "id": "MDRW2100008587.1.5",
          "form": "지금 플레이오프 시작 일보직전인데",
          "original_form": "지금 플레이오프 시작 일보직전인데",
          "speaker_id": "1",
          "time": "20210822 16:59"
        },
        {
          "id": "MDRW2100008587.1.6",
          "form": "롤도 보죠!",
          "original_form": "롤도 보죠!",
          "speaker_id": "2",
          "time": "20210822 16:59"
        },
        {
          "id": "MDRW2100008587.1.7",
          "form": "다른 게임도 보세요?",
          "original_form": "다른 게임도 보세요?",
          "speaker_id": "1",
          "time": "20210822 17:19"
        },
        {
          "id": "MDRW2100008587.1.8",
          "form": "저는 배그도 봐요",
          "original_form": "저는 배그도 봐요",
          "speaker_id": "1",
          "time": "20210822 17:19"
        },
        {
          "id": "MDRW2100008587.1.9",
          "form": "배그! 저도 배그보는데",
          "original_form": "배그! 저도 배그보는데",
          "speaker_id": "2",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008587.1.10",
          "form": "Pws 말하시는건가용",
          "original_form": "Pws 말하시는건가용",
          "speaker_id": "2",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008587.1.11",
          "form": "PWS는 당연히 보죠 저는 PKL 있을 때부터 봤어요",
          "original_form": "PWS는 당연히 보죠 저는 PKL 있을 때부터 봤어요",
          "speaker_id": "1",
          "time": "20210822 17:21"
        },
        {
          "id": "MDRW2100008587.1.12",
          "form": "저는 그 전?",
          "original_form": "저는 그 전?",
          "speaker_id": "2",
          "time": "20210822 17:23"
        },
        {
          "id": "MDRW2100008587.1.13",
          "form": "에스카 프로 시절때부터 봤어요",
          "original_form": "에스카 프로 시절때부터 봤어요",
          "speaker_id": "2",
          "time": "20210822 17:23"
        },
        {
          "id": "MDRW2100008587.1.14",
          "form": "KSV NTT",
          "original_form": "KSV NTT",
          "speaker_id": "2",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008587.1.15",
          "form": "와 완전 오래전부터 보셨네요 NTT 너무 추억이고...",
          "original_form": "와 완전 오래전부터 보셨네요 NTT 너무 추억이고...",
          "speaker_id": "1",
          "time": "20210822 17:24"
        },
        {
          "id": "MDRW2100008587.1.16",
          "form": "네 엄청 오래전부터 본 유저라",
          "original_form": "네 엄청 오래전부터 본 유저라",
          "speaker_id": "2",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100008587.1.17",
          "form": "현재 치킨룰땜에 너무 노잼되긴했어도",
          "original_form": "현재 치킨룰땜에 너무 노잼되긴했어도",
          "speaker_id": "2",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100008587.1.18",
          "form": "꾸준하게 보기는해요",
          "original_form": "꾸준하게 보기는해요",
          "speaker_id": "2",
          "time": "20210822 17:26"
        },
        {
          "id": "MDRW2100008587.1.19",
          "form": "저는 치킨룰 되고 나서 가끔 보고 있어요",
          "original_form": "저는 치킨룰 되고 나서 가끔 보고 있어요",
          "speaker_id": "1",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100008587.1.20",
          "form": "혹시 응원팀 여쭤봐도 될까요?",
          "original_form": "혹시 응원팀 여쭤봐도 될까요?",
          "speaker_id": "1",
          "time": "20210822 17:27"
        },
        {
          "id": "MDRW2100008587.1.21",
          "form": "전 상대적 약팀? 좀 응원하는 타입이라",
          "original_form": "전 상대적 약팀? 좀 응원하는 타입이라",
          "speaker_id": "2",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008587.1.22",
          "form": "현재는 고앤고랑 OPGG 응원하고 있네요",
          "original_form": "현재는 고앤고랑 OPGG 응원하고 있네요",
          "speaker_id": "2",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008587.1.23",
          "form": "베로니카7하고요",
          "original_form": "베로니카7하고요",
          "speaker_id": "2",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008587.1.24",
          "form": "으악 그러시니까 갑자기 저 된거 같아요...ㅋㅋ",
          "original_form": "으악 그러시니까 갑자기 저 {censored}된거 같아요...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:29"
        },
        {
          "id": "MDRW2100008587.1.25",
          "form": "느낌이 기블리나 담원 응원하실거같은",
          "original_form": "느낌이 기블리나 담원 응원하실거같은",
          "speaker_id": "2",
          "time": "20210822 17:32"
        },
        {
          "id": "MDRW2100008587.1.26",
          "form": "느낌이 드네요 ㅋㅋㅋ",
          "original_form": "느낌이 드네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 17:32"
        },
        {
          "id": "MDRW2100008587.1.27",
          "form": "담원이랑 젠지 응원하고 있어요",
          "original_form": "담원이랑 젠지 응원하고 있어요",
          "speaker_id": "1",
          "time": "20210822 17:32"
        },
        {
          "id": "MDRW2100008587.1.28",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210822 17:33"
        },
        {
          "id": "MDRW2100008587.1.29",
          "form": "젠지 담원이 보통 팬층이 많으니",
          "original_form": "젠지 담원이 보통 팬층이 많으니",
          "speaker_id": "2",
          "time": "20210822 17:33"
        },
        {
          "id": "MDRW2100008587.1.30",
          "form": "이미 담원은 그랜드 파이널 진출이라 안심이긴 하시겠네요",
          "original_form": "이미 담원은 그랜드 파이널 진출이라 안심이긴 하시겠네요",
          "speaker_id": "2",
          "time": "20210822 17:34"
        },
        {
          "id": "MDRW2100008587.1.31",
          "form": "아 담원이래 젠지",
          "original_form": "아 담원이래 젠지",
          "speaker_id": "2",
          "time": "20210822 17:34"
        },
        {
          "id": "MDRW2100008587.1.32",
          "form": "ㅎㅎ 편안하게 보고 있습니다",
          "original_form": "ㅎㅎ 편안하게 보고 있습니다",
          "speaker_id": "1",
          "time": "20210822 17:34"
        },
        {
          "id": "MDRW2100008587.1.33",
          "form": "고앤고도 진출은 확정이긴한데",
          "original_form": "고앤고도 진출은 확정이긴한데",
          "speaker_id": "2",
          "time": "20210822 17:35"
        },
        {
          "id": "MDRW2100008587.1.34",
          "form": "불안한건 베로니카네요",
          "original_form": "불안한건 베로니카네요",
          "speaker_id": "2",
          "time": "20210822 17:36"
        },
        {
          "id": "MDRW2100008587.1.35",
          "form": "올라갈 수 있을거예요 저도 응원하겠습니다",
          "original_form": "올라갈 수 있을거예요 저도 응원하겠습니다",
          "speaker_id": "1",
          "time": "20210822 17:36"
        },
        {
          "id": "MDRW2100008587.1.36",
          "form": "감사합니다!",
          "original_form": "감사합니다!",
