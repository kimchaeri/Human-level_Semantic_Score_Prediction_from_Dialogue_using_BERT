      "utterance": [
        {
          "id": "MDRW2100006775.1.1",
          "form": "기념품 사오시나요?",
          "original_form": "기념품 사오시나요?",
          "speaker_id": "2",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100006775.1.2",
          "form": "저는 냉장고자석 사와요",
          "original_form": "저는 냉장고자석 사와요",
          "speaker_id": "1",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100006775.1.3",
          "form": "오 마그넷이요?어디서 사오셨나요",
          "original_form": "오 마그넷이요?어디서 사오셨나요",
          "speaker_id": "2",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100006775.1.4",
          "form": "저는 그 해당국가가면 기프트샵에서 주로사요",
          "original_form": "저는 그 해당국가가면 기프트샵에서 주로사요",
          "speaker_id": "1",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100006775.1.5",
          "form": "그럼 현재 몇개나 모으셨나요",
          "original_form": "그럼 현재 몇개나 모으셨나요",
          "speaker_id": "2",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100006775.1.6",
          "form": "좀 많아요 ㅎ",
          "original_form": "좀 많아요 ㅎ",
          "speaker_id": "1",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100006775.1.7",
          "form": "그래서 세계지도 타공판에 붙여놨어요",
          "original_form": "그래서 세계지도 타공판에 붙여놨어요",
          "speaker_id": "1",
          "time": "20210908 09:08"
        },
        {
          "id": "MDRW2100006775.1.8",
          "form": "근데 유럽이나 미국쪽은 별로 없고 ㅋ",
          "original_form": "근데 유럽이나 미국쪽은 별로 없고 ㅋ",
          "speaker_id": "1",
          "time": "20210908 09:08"
        },
        {
          "id": "MDRW2100006775.1.9",
          "form": "오 타공판이 화려하겠어요",
          "original_form": "오 타공판이 화려하겠어요",
          "speaker_id": "2",
          "time": "20210908 09:09"
        },
        {
          "id": "MDRW2100006775.1.10",
          "form": "비어있는곳 채워야하는데ㅎ",
          "original_form": "비어있는곳 채워야하는데ㅎ",
          "speaker_id": "1",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100006775.1.11",
          "form": "코시국이라",
          "original_form": "코시국이라",
          "speaker_id": "1",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100006775.1.12",
          "form": "갈수도 없고 ㅎ",
          "original_form": "갈수도 없고 ㅎ",
          "speaker_id": "1",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100006775.1.13",
          "form": "몇십개정도 되시는거죠??",
          "original_form": "몇십개정도 되시는거죠??",
          "speaker_id": "2",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100006775.1.14",
          "form": "음 세아려보질않아서 ㅎ",
          "original_form": "음 세아려보질않아서 ㅎ",
          "speaker_id": "1",
          "time": "20210908 09:20"
        },
        {
          "id": "MDRW2100006775.1.15",
          "form": "10개는 넘는듯요",
          "original_form": "10개는 넘는듯요",
          "speaker_id": "1",
          "time": "20210908 09:20"
        },
        {
          "id": "MDRW2100006775.1.16",
          "form": "여행 기억이 새록새록 나겠어요",
          "original_form": "여행 기억이 새록새록 나겠어요",
          "speaker_id": "2",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100006775.1.17",
          "form": "네 한번씩보면 추억 더듬죠 ㅎ",
          "original_form": "네 한번씩보면 추억 더듬죠 ㅎ",
          "speaker_id": "1",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100006775.1.18",
          "form": "머 기념품 있으세요?",
          "original_form": "머 기념품 있으세요?",
          "speaker_id": "1",
          "time": "20210908 09:25"
        },
        {
          "id": "MDRW2100006775.1.19",
          "form": "저는 물건말고 주로 먹을거요 ㅎㅎ",
          "original_form": "저는 물건말고 주로 먹을거요 ㅎㅎ",
