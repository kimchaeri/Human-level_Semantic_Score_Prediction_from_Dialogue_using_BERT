      "utterance": [
        {
          "id": "MDRW2100014165.1.1",
          "form": "오늘 날씨 너무 더워요ㅜㅜ",
          "original_form": "오늘 날씨 너무 더워요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 16:05"
        },
        {
          "id": "MDRW2100014165.1.2",
          "form": "어제는 날씨가 좋았는데 오늘은 또 흐리네요.",
          "original_form": "어제는 날씨가 좋았는데 오늘은 또 흐리네요.",
          "speaker_id": "2",
          "time": "20210827 09:10"
        },
        {
          "id": "MDRW2100014165.1.3",
          "form": "오늘은 날씨도 덥고 곧 비가 올 예정이래요ㅜㅜ",
          "original_form": "오늘은 날씨도 덥고 곧 비가 올 예정이래요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 10:21"
        },
        {
          "id": "MDRW2100014165.1.4",
          "form": "제가 사는 지역은 벌써 비가 와요ㅠㅠ 맑은 날이 그립네요",
          "original_form": "제가 사는 지역은 벌써 비가 와요ㅠㅠ 맑은 날이 그립네요",
          "speaker_id": "2",
          "time": "20210827 10:34"
        },
        {
          "id": "MDRW2100014165.1.5",
          "form": "비오는 날씨는 너무 싫어요ㅠㅠ",
          "original_form": "비오는 날씨는 너무 싫어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 12:12"
        },
        {
          "id": "MDRW2100014165.1.6",
          "form": "맞아요! 얼른 장마가 끝났으면 좋겠네요 ㅠ",
          "original_form": "맞아요! 얼른 장마가 끝났으면 좋겠네요 ㅠ",
          "speaker_id": "2",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100014165.1.7",
          "form": "이번주는 날씨도 덥고 장마라서 비도 막 오고ㅠㅠ",
          "original_form": "이번주는 날씨도 덥고 장마라서 비도 막 오고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 12:33"
        },
        {
          "id": "MDRW2100014165.1.8",
          "form": "작년에는 태풍때문에 고생했는데ㅜㅜ",
          "original_form": "작년에는 태풍때문에 고생했는데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100014165.1.9",
          "form": "얼른 가을지나고",
          "original_form": "얼른 가을지나고",
          "speaker_id": "1",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100014165.1.10",
          "form": "겨울이 왔으면 좋겠어요ㅜㅜ",
          "original_form": "겨울이 왔으면 좋겠어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100014165.1.11",
          "form": "맞아요! 겨울이 옷 입기도 편하고 좋아요!!",
          "original_form": "맞아요! 겨울이 옷 입기도 편하고 좋아요!!",
          "speaker_id": "2",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100014165.1.12",
          "form": "롱패딩 하나면 완성ㅋㅋㅋ",
          "original_form": "롱패딩 하나면 완성ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100014165.1.13",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100014165.1.14",
          "form": "패딩만있으면 겨울 보낼수 있어욬ㅋㅋㅋ",
          "original_form": "패딩만있으면 겨울 보낼수 있어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:41"
        },
        {
          "id": "MDRW2100014165.1.15",
          "form": "추우면 껴입으면 되는데 더운건 진짜 답이 없는 것 같아요",
          "original_form": "추우면 껴입으면 되는데 더운건 진짜 답이 없는 것 같아요",
          "speaker_id": "2",
          "time": "20210827 12:43"
        },
        {
          "id": "MDRW2100014165.1.16",
          "form": "그니깐요!!",
          "original_form": "그니깐요!!",
          "speaker_id": "1",
          "time": "20210827 13:17"
        },
        {
          "id": "MDRW2100014165.1.17",
          "form": "너무 더워서 까페에 놀러왔는데",
          "original_form": "너무 더워서 까페에 놀러왔는데",
          "speaker_id": "1",
          "time": "20210827 13:18"
        },
        {
          "id": "MDRW2100014165.1.18",
          "form": "많이 안시원하네요ㅜㅜ",
          "original_form": "많이 안시원하네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 13:18"
        },
        {
          "id": "MDRW2100014165.1.19",
          "form": "아 정말요? 전 오늘 되게 쌀쌀한데! 지역 차이가 큰가봐요",
          "original_form": "아 정말요? 전 오늘 되게 쌀쌀한데! 지역 차이가 큰가봐요",
          "speaker_id": "2",
          "time": "20210827 13:21"
        },
        {
          "id": "MDRW2100014165.1.20",
          "form": "어떤손님이 덥다고 얘기 하니깐 직원이",
          "original_form": "어떤손님이 덥다고 얘기 하니깐 직원이",
          "speaker_id": "1",
          "time": "20210827 14:17"
        },
        {
          "id": "MDRW2100014165.1.21",
          "form": "시원하게 해주네요",
          "original_form": "시원하게 해주네요",
          "speaker_id": "1",
          "time": "20210827 14:18"
        },
        {
          "id": "MDRW2100014165.1.22",
          "form": "잉?",
          "original_form": "잉?",
          "speaker_id": "1",
          "time": "20210827 14:18"
        }
      ]
    }
