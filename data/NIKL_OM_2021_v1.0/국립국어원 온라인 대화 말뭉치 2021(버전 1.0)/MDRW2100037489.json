      "utterance": [
        {
          "id": "MDRW2100037489.1.1",
          "form": "ㅎㅎ맞아요 탕비실이 제 입맛 많이 버려놨죠",
          "original_form": "ㅎㅎ맞아요 탕비실이 제 입맛 많이 버려놨죠",
          "speaker_id": "2",
          "time": "20210901 01:08"
        },
        {
          "id": "MDRW2100037489.1.2",
          "form": "요새는 쌀로별이 쌓여 있네요",
          "original_form": "요새는 쌀로별이 쌓여 있네요",
          "speaker_id": "2",
          "time": "20210901 01:08"
        },
        {
          "id": "MDRW2100037489.1.3",
          "form": "ㅋㅋㅋㅋㅋ아니... 무슨..... 과자선택 누가하는거죸ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ아니... 무슨..... 과자선택 누가하는거죸ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 01:10"
        },
        {
          "id": "MDRW2100037489.1.4",
          "form": "(근데 쌀로별로 맛있다는...)",
          "original_form": "(근데 쌀로별로 맛있다는...)",
          "speaker_id": "1",
          "time": "20210901 01:10"
        },
        {
          "id": "MDRW2100037489.1.5",
          "form": "ㅎㅎ 그게 비극이에요",
          "original_form": "ㅎㅎ 그게 비극이에요",
          "speaker_id": "2",
          "time": "20210901 01:10"
        },
        {
          "id": "MDRW2100037489.1.6",
          "form": "마가레트도 자주 보여요ㅎ",
          "original_form": "마가레트도 자주 보여요ㅎ",
          "speaker_id": "2",
          "time": "20210901 01:10"
        },
        {
          "id": "MDRW2100037489.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ으악ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ으악ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 01:13"
        },
        {
          "id": "MDRW2100037489.1.8",
          "form": "아진짜 탕비실 누가채워요? 너무궁금ㅋㅋㅋㅋㅋ",
          "original_form": "아진짜 탕비실 누가채워요? 너무궁금ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 01:13"
        },
        {
          "id": "MDRW2100037489.1.9",
          "form": "과자들이 다 주옥같네요...",
          "original_form": "과자들이 다 주옥같네요...",
          "speaker_id": "1",
          "time": "20210901 01:13"
        },
        {
          "id": "MDRW2100037489.1.10",
          "form": "회사 탕비실과 노조 탕비실이 있는데",
          "original_form": "회사 탕비실과 노조 탕비실이 있는데",
          "speaker_id": "2",
          "time": "20210901 01:14"
        },
        {
          "id": "MDRW2100037489.1.11",
          "form": "둘이 번갈아가며 그래요ㅜ",
          "original_form": "둘이 번갈아가며 그래요ㅜ",
          "speaker_id": "2",
          "time": "20210901 01:14"
        },
        {
          "id": "MDRW2100037489.1.12",
          "form": "그냥 이회사 터가 안좋은걸로",
          "original_form": "그냥 이회사 터가 안좋은걸로",
          "speaker_id": "2",
          "time": "20210901 01:14"
        },
        {
          "id": "MDRW2100037489.1.13",
          "form": "노조 탕비실은 노조위원장 입맛에 좌우됩니다ㅎ",
          "original_form": "노조 탕비실은 노조위원장 입맛에 좌우됩니다ㅎ",
          "speaker_id": "2",
          "time": "20210901 01:14"
        },
        {
          "id": "MDRW2100037489.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 01:14"
        },
        {
          "id": "MDRW2100037489.1.15",
          "form": "총무팀... 아!",
          "original_form": "총무팀... 아!",
          "speaker_id": "1",
          "time": "20210901 01:14"
        },
        {
          "id": "MDRW2100037489.1.16",
          "form": "노조위원장.....",
          "original_form": "노조위원장.....",
          "speaker_id": "1",
          "time": "20210901 01:14"
        },
        {
          "id": "MDRW2100037489.1.17",
          "form": "나이가 많으신 분인가보군여....",
          "original_form": "나이가 많으신 분인가보군여....",
          "speaker_id": "1",
          "time": "20210901 01:15"
        },
        {
          "id": "MDRW2100037489.1.18",
          "form": "과자를 보면 나이를 안다고ㅠㅠ",
          "original_form": "과자를 보면 나이를 안다고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 01:15"
        },
        {
          "id": "MDRW2100037489.1.19",
          "form": "비극적이게도 아직 30대예요ㅜ",
          "original_form": "비극적이게도 아직 30대예요ㅜ",
          "speaker_id": "2",
          "time": "20210901 01:15"
        },
        {
          "id": "MDRW2100037489.1.20",
          "form": "끝자락이지만",
          "original_form": "끝자락이지만",
          "speaker_id": "2",
          "time": "20210901 01:15"
        },
        {
          "id": "MDRW2100037489.1.21",
          "form": "차라리 전 노조위원장은 총무에게 전담해서",
          "original_form": "차라리 전 노조위원장은 총무에게 전담해서",
          "speaker_id": "2",
          "time": "20210901 01:15"
        },
        {
          "id": "MDRW2100037489.1.22",
          "form": "해외직구 과자도 많았는데",
          "original_form": "해외직구 과자도 많았는데",
          "speaker_id": "2",
          "time": "20210901 01:16"
        },
        {
          "id": "MDRW2100037489.1.23",
          "form": "이번 위원장은 절레절레",
          "original_form": "이번 위원장은 절레절레",
          "speaker_id": "2",
          "time": "20210901 01:16"
        },
        {
          "id": "MDRW2100037489.1.24",
          "form": "ㄴㅇㄱ",
          "original_form": "ㄴㅇㄱ",
          "speaker_id": "1",
          "time": "20210901 01:17"
        },
        {
          "id": "MDRW2100037489.1.25",
          "form": "어머나 세상엨ㅋㅋㅋㅋㅋㅋ",
          "original_form": "어머나 세상엨ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 01:17"
        },
        {
          "id": "MDRW2100037489.1.26",
          "form": "아니.... 할머니 할아버지랑 과자먹으면서 크셨나요",
          "original_form": "아니.... 할머니 할아버지랑 과자먹으면서 크셨나요",
          "speaker_id": "1",
          "time": "20210901 01:18"
        },
        {
          "id": "MDRW2100037489.1.27",
          "form": "아니... 상상도못한정쳌ㅋㅋㅋ",
          "original_form": "아니... 상상도못한정쳌ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 01:18"
        },
        {
          "id": "MDRW2100037489.1.28",
          "form": "문제는 그 입맛이 전염되고 있다는 거죠ㅜ",
          "original_form": "문제는 그 입맛이 전염되고 있다는 거죠ㅜ",
          "speaker_id": "2",
          "time": "20210901 01:20"
        },
        {
          "id": "MDRW2100037489.1.29",
          "form": "아 근데 맛동산 맛있어요",
          "original_form": "아 근데 맛동산 맛있어요",
          "speaker_id": "2",
          "time": "20210901 01:20"
        },
        {
          "id": "MDRW2100037489.1.30",
          "form": "심지어 비ㅋ쌈ㅋ",
          "original_form": "심지어 비ㅋ쌈ㅋ",
          "speaker_id": "2",
          "time": "20210901 01:20"
        },
        {
          "id": "MDRW2100037489.1.31",
          "form": "맛동산이 매년 과자 매출 톱텐에 들더라고요 의외로",
          "original_form": "맛동산이 매년 과자 매출 톱텐에 들더라고요 의외로",
          "speaker_id": "2",
          "time": "20210901 01:22"
        },
        {
          "id": "MDRW2100037489.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 01:22"
        },
        {
          "id": "MDRW2100037489.1.33",
          "form": "맛동산 먹고 즐거운 파티!",
          "original_form": "맛동산 먹고 즐거운 파티!",
          "speaker_id": "1",
          "time": "20210901 01:22"
        },
        {
          "id": "MDRW2100037489.1.34",
          "form": "한입 먹으면 뭔가 코팅되어있고 땅콩같은것도 씹히는게 상당히 만드는과정이 단순하지는 않을것 같은 느낌이에요 ㅋㅋ",
          "original_form": "한입 먹으면 뭔가 코팅되어있고 땅콩같은것도 씹히는게 상당히 만드는과정이 단순하지는 않을것 같은 느낌이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 01:23"
        },
        {
          "id": "MDRW2100037489.1.35",
          "form": "그거...국악 들려줍니다ㅎㅎ",
          "original_form": "그거...국악 들려줍니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 01:23"
        },
        {
          "id": "MDRW2100037489.1.36",
          "form": "아시나요 국악발효ㅎㅎㅎㅎ",
          "original_form": "아시나요 국악발효ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 01:24"
        },
        {
          "id": "MDRW2100037489.1.37",
          "form": "어디서 본적있는거 같긴한데 찐이었나욬ㅋㅋㅋㅋㅋ",
          "original_form": "어디서 본적있는거 같긴한데 찐이었나욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 01:26"
        },
        {
          "id": "MDRW2100037489.1.38",
          "form": "효과가 있는게 수치로 나온거에요? ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "효과가 있는게 수치로 나온거에요? ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 01:26"
        },
        {
          "id": "MDRW2100037489.1.39",
          "form": "일단 회사측 공식입장은 그게 맞아요ㅎㅎ",
          "original_form": "일단 회사측 공식입장은 그게 맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 01:27"
        },
        {
          "id": "MDRW2100037489.1.40",
          "form": "인터뷰했었음ㅋㅋㅋ",
          "original_form": "인터뷰했었음ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 01:27"
        }
      ]
    }
