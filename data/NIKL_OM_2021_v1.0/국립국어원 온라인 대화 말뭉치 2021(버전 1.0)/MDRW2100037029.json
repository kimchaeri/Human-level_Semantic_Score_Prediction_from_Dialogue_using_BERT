      "utterance": [
        {
          "id": "MDRW2100037029.1.1",
          "form": "반려동물 키우세여?",
          "original_form": "반려동물 키우세여?",
          "speaker_id": "2",
          "time": "20210831 17:37"
        },
        {
          "id": "MDRW2100037029.1.2",
          "form": "네 고양이키워여",
          "original_form": "네 고양이키워여",
          "speaker_id": "1",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100037029.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210831 17:38"
        },
        {
          "id": "MDRW2100037029.1.4",
          "form": "저두요!",
          "original_form": "저두요!",
          "speaker_id": "2",
          "time": "20210831 17:39"
        },
        {
          "id": "MDRW2100037029.1.5",
          "form": "몇마리용ㅈ?",
          "original_form": "몇마리용ㅈ?",
          "speaker_id": "2",
          "time": "20210831 17:39"
        },
        {
          "id": "MDRW2100037029.1.6",
          "form": "같은집사셨군여",
          "original_form": "같은집사셨군여",
          "speaker_id": "1",
          "time": "20210831 17:39"
        },
        {
          "id": "MDRW2100037029.1.7",
          "form": "전 코숏치즈태비 수컷1마리 키우고있어요",
          "original_form": "전 코숏치즈태비 수컷1마리 키우고있어요",
          "speaker_id": "1",
          "time": "20210831 17:39"
        },
        {
          "id": "MDRW2100037029.1.8",
          "form": "저는 아메숏 남여 2마리요!",
          "original_form": "저는 아메숏 남여 2마리요!",
          "speaker_id": "2",
          "time": "20210831 17:40"
        },
        {
          "id": "MDRW2100037029.1.9",
          "form": "사료 어떤거 먹이고 계세요?",
          "original_form": "사료 어떤거 먹이고 계세요?",
          "speaker_id": "2",
          "time": "20210831 17:40"
        },
        {
          "id": "MDRW2100037029.1.10",
          "form": "애기들 몇살이에요 넘귀여울거같아요",
          "original_form": "애기들 몇살이에요 넘귀여울거같아요",
          "speaker_id": "1",
          "time": "20210831 17:40"
        },
        {
          "id": "MDRW2100037029.1.11",
          "form": "오리젠에서 뉴트리언스로 갈아탔는데",
          "original_form": "오리젠에서 뉴트리언스로 갈아탔는데",
          "speaker_id": "1",
          "time": "20210831 17:40"
        },
        {
          "id": "MDRW2100037029.1.12",
          "form": "3살이에요!",
          "original_form": "3살이에요!",
          "speaker_id": "2",
          "time": "20210831 17:40"
        },
        {
          "id": "MDRW2100037029.1.13",
          "form": "고민이에요..",
          "original_form": "고민이에요..",
          "speaker_id": "1",
          "time": "20210831 17:40"
        },
        {
          "id": "MDRW2100037029.1.14",
          "form": "왜요??",
          "original_form": "왜요??",
          "speaker_id": "2",
          "time": "20210831 17:40"
        },
        {
          "id": "MDRW2100037029.1.15",
          "form": "뉴트리언스 서브제로에 트릿을넘좋아하고 잘먹긴하는데",
          "original_form": "뉴트리언스 서브제로에 트릿을넘좋아하고 잘먹긴하는데",
          "speaker_id": "1",
          "time": "20210831 17:41"
        },
        {
          "id": "MDRW2100037029.1.16",
          "form": "오리젠 고단백에 못미치더라구여",
          "original_form": "오리젠 고단백에 못미치더라구여",
          "speaker_id": "1",
          "time": "20210831 17:41"
        },
        {
          "id": "MDRW2100037029.1.17",
          "form": "모질차이가 좀나고 흠ㅜ",
          "original_form": "모질차이가 좀나고 흠ㅜ",
          "speaker_id": "1",
          "time": "20210831 17:41"
        },
        {
          "id": "MDRW2100037029.1.18",
          "form": "확실히 고단백이 다르긴하죠ㅠ",
          "original_form": "확실히 고단백이 다르긴하죠ㅠ",
          "speaker_id": "2",
          "time": "20210831 17:44"
        },
        {
          "id": "MDRW2100037029.1.19",
          "form": "저는 건조동결로 된거 좋은거 먹이고 싶은데 가격이 너무 살벌해서 지금 헤일로 먹이고 있어요ㅜ",
          "original_form": "저는 건조동결로 된거 좋은거 먹이고 싶은데 가격이 너무 살벌해서 지금 헤일로 먹이고 있어요ㅜ",
          "speaker_id": "2",
          "time": "20210831 17:45"
        },
        {
          "id": "MDRW2100037029.1.20",
          "form": "네ㅜ근데 가격대가 차이가많이나서",
          "original_form": "네ㅜ근데 가격대가 차이가많이나서",
          "speaker_id": "1",
          "time": "20210831 17:45"
        },
        {
          "id": "MDRW2100037029.1.21",
          "form": "헤일로제품도 들어봤어요",
          "original_form": "헤일로제품도 들어봤어요",
          "speaker_id": "1",
          "time": "20210831 17:45"
        },
        {
          "id": "MDRW2100037029.1.22",
          "form": "고민이네요 얘가 그래도 사료는안가리고잘먹어서",
          "original_form": "고민이네요 얘가 그래도 사료는안가리고잘먹어서",
          "speaker_id": "1",
          "time": "20210831 17:45"
        },
        {
          "id": "MDRW2100037029.1.23",
          "form": "아우 그래도 다행이네요",
          "original_form": "아우 그래도 다행이네요",
          "speaker_id": "2",
          "time": "20210831 17:46"
        },
        {
          "id": "MDRW2100037029.1.24",
          "form": "저도 이참에 샘플들 살까해요",
          "original_form": "저도 이참에 샘플들 살까해요",
          "speaker_id": "2",
          "time": "20210831 17:46"
        }
      ]
    }
  ]
