        },
        {
          "id": "MDRW2100024683.1.2",
          "form": "곧 name3씨 오신대요!!!",
          "original_form": "곧 &name3&씨 오신대요!!!",
          "speaker_id": "1",
          "time": "20210831 17:10"
        },
        {
          "id": "MDRW2100024683.1.3",
          "form": "학교 생활 쉽지 않을텐데",
          "original_form": "학교 생활 쉽지 않을텐데",
          "speaker_id": "2",
          "time": "20210831 17:23"
        },
        {
          "id": "MDRW2100024683.1.4",
          "form": "곧 개강하고",
          "original_form": "곧 개강하고",
          "speaker_id": "2",
          "time": "20210831 17:23"
        },
        {
          "id": "MDRW2100024683.1.5",
          "form": "맛있는거라도 먹기로 했나요?",
          "original_form": "맛있는거라도 먹기로 했나요?",
          "speaker_id": "2",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100024683.1.6",
          "form": "오늘 name3씨 생일이어서",
          "original_form": "오늘 &name3&씨 생일이어서",
          "speaker_id": "1",
          "time": "20210831 18:27"
        },
        {
          "id": "MDRW2100024683.1.7",
          "form": "선물도 줄 겸",
          "original_form": "선물도 줄 겸",
          "speaker_id": "1",
          "time": "20210831 18:27"
        },
        {
          "id": "MDRW2100024683.1.8",
          "form": "만났답니다!!!",
          "original_form": "만났답니다!!!",
          "speaker_id": "1",
          "time": "20210831 18:27"
        },
        {
          "id": "MDRW2100024683.1.9",
          "form": "맞아요 내일 개강이라고 놀라더라구요~~~",
          "original_form": "맞아요 내일 개강이라고 놀라더라구요~~~",
          "speaker_id": "1",
          "time": "20210831 18:28"
        },
        {
          "id": "MDRW2100024683.1.10",
          "form": "개강을 하다니",
          "original_form": "개강을 하다니",
          "speaker_id": "2",
          "time": "20210831 18:30"
        },
        {
          "id": "MDRW2100024683.1.11",
          "form": "오늘까지 인턴을 하고",
          "original_form": "오늘까지 인턴을 하고",
          "speaker_id": "2",
          "time": "20210831 18:31"
        },
        {
          "id": "MDRW2100024683.1.12",
          "form": "그러니까요 ㅠㅠ",
          "original_form": "그러니까요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 18:40"
        },
        {
          "id": "MDRW2100024683.1.13",
          "form": "그 이상한 사수가 연구실에 없으니까",
          "original_form": "그 이상한 사수가 연구실에 없으니까",
          "speaker_id": "1",
          "time": "20210831 18:40"
        },
        {
          "id": "MDRW2100024683.1.14",
          "form": "분위기가 그렇게 좋더래요",
          "original_form": "분위기가 그렇게 좋더래요",
          "speaker_id": "1",
          "time": "20210831 18:40"
        },
        {
          "id": "MDRW2100024683.1.15",
          "form": "어머 없어졌어요?",
          "original_form": "어머 없어졌어요?",
          "speaker_id": "2",
          "time": "20210831 18:41"
        },
        {
          "id": "MDRW2100024683.1.16",
          "form": "아 휴가라서 3일동안 안나온거래요 ㅎㅎㅎㅎ",
          "original_form": "아 휴가라서 3일동안 안나온거래요 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100024683.1.17",
          "form": "그러니까 연구실 분위기가 좋았다고 하더라구요",
          "original_form": "그러니까 연구실 분위기가 좋았다고 하더라구요",
          "speaker_id": "1",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100024683.1.18",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100024683.1.19",
          "form": "name3씨한테만 별로였던게 아니었군요",
          "original_form": "&name3&씨한테만 별로였던게 아니었군요",
          "speaker_id": "2",
          "time": "20210831 18:53"
        },
        {
          "id": "MDRW2100024683.1.20",
          "form": "그런가봐요!",
          "original_form": "그런가봐요!",
          "speaker_id": "1",
          "time": "20210831 18:55"
        },
        {
          "id": "MDRW2100024683.1.21",
          "form": "역시,,,",
          "original_form": "역시,,,",
          "speaker_id": "1",
          "time": "20210831 18:55"
        },
        {
          "id": "MDRW2100024683.1.22",
          "form": "그래도 개강하기 전에 스트레스 요인이 잠깐 사라져서 다행이네요",
          "original_form": "그래도 개강하기 전에 스트레스 요인이 잠깐 사라져서 다행이네요",
          "speaker_id": "2",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100024683.1.23",
          "form": "맞아요!!!!",
          "original_form": "맞아요!!!!",
          "speaker_id": "1",
          "time": "20210901 00:02"
        },
        {
          "id": "MDRW2100024683.1.24",
