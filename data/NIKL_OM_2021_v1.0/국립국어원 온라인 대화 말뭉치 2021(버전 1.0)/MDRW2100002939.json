        {
          "id": "MDRW2100002939.1.2",
          "form": "요즘 뭐해?",
          "original_form": "요즘 뭐해?",
          "speaker_id": "1",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002939.1.3",
          "form": "나는 콜라를 좋아해",
          "original_form": "나는 콜라를 좋아해",
          "speaker_id": "2",
          "time": "20210820 16:50"
        },
        {
          "id": "MDRW2100002939.1.4",
          "form": "콜라 좋아한다고",
          "original_form": "콜라 좋아한다고",
          "speaker_id": "2",
          "time": "20210820 16:52"
        },
        {
          "id": "MDRW2100002939.1.5",
          "form": "대답안하냐",
          "original_form": "대답안하냐",
          "speaker_id": "2",
          "time": "20210820 16:52"
        },
        {
          "id": "MDRW2100002939.1.6",
          "form": "나는 아무거나 안가리고 잘먹어",
          "original_form": "나는 아무거나 안가리고 잘먹어",
          "speaker_id": "1",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002939.1.7",
          "form": "천안이야?",
          "original_form": "천안이야?",
          "speaker_id": "1",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002939.1.8",
          "form": "천안이야",
          "original_form": "천안이야",
          "speaker_id": "2",
          "time": "20210820 16:53"
        },
        {
          "id": "MDRW2100002939.1.9",
          "form": "근처에 예쁜카페있어?",
          "original_form": "근처에 예쁜카페있어?",
          "speaker_id": "1",
          "time": "20210820 16:55"
        },
        {
          "id": "MDRW2100002939.1.10",
          "form": "청년다방으로 와",
          "original_form": "청년다방으로 와",
          "speaker_id": "2",
          "time": "20210820 16:55"
        },
        {
          "id": "MDRW2100002939.1.11",
          "form": "거기는 뭐팔아?",
          "original_form": "거기는 뭐팔아?",
          "speaker_id": "1",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002939.1.12",
          "form": "커피랑 에이드랑 떡볶이랑 튀김",
          "original_form": "커피랑 에이드랑 떡볶이랑 튀김",
          "speaker_id": "2",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002939.1.13",
          "form": "아 이거 600개 가능하냐???",
          "original_form": "아 이거 600개 가능하냐???",
          "speaker_id": "2",
          "time": "20210820 16:56"
        },
        {
          "id": "MDRW2100002939.1.14",
          "form": "2달이니까",
          "original_form": "2달이니까",
          "speaker_id": "1",
          "time": "20210820 16:58"
        },
        {
          "id": "MDRW2100002939.1.15",
          "form": "하루에 5개씩하면되지 않을까",
          "original_form": "하루에 5개씩하면되지 않을까",
          "speaker_id": "1",
          "time": "20210820 16:58"
        },
        {
          "id": "MDRW2100002939.1.16",
          "form": "나 3번 대화했는데 통과 대화가 없는걸???",
          "original_form": "나 3번 대화했는데 통과 대화가 없는걸???",
          "speaker_id": "2",
          "time": "20210820 17:00"
        },
        {
          "id": "MDRW2100002939.1.17",
          "form": "시간 지나면 통과 돼",
          "original_form": "시간 지나면 통과 돼",
          "speaker_id": "1",
          "time": "20210820 17:00"
        },
        {
          "id": "MDRW2100002939.1.18",
          "form": "응 나는 콜라를 좋아하고 사이다도 좋아해",
          "original_form": "응 나는 콜라를 좋아하고 사이다도 좋아해",
          "speaker_id": "2",
          "time": "20210820 17:00"
