      "utterance": [
        {
          "id": "MDRW2100031503.1.1",
          "form": "고양이 유튜브 좋아하세요",
          "original_form": "고양이 유튜브 좋아하세요",
          "speaker_id": "1",
          "time": "20210904 22:12"
        },
        {
          "id": "MDRW2100031503.1.2",
          "form": "딱히 즐기지는 않습니다",
          "original_form": "딱히 즐기지는 않습니다",
          "speaker_id": "2",
          "time": "20210904 22:13"
        },
        {
          "id": "MDRW2100031503.1.3",
          "form": "제가 자주 보는 고양이 유튜버가 있는데 고양이들이 완전 강아지 예요",
          "original_form": "제가 자주 보는 고양이 유튜버가 있는데 고양이들이 완전 강아지 예요",
          "speaker_id": "1",
          "time": "20210904 22:13"
        },
        {
          "id": "MDRW2100031503.1.4",
          "form": "종류가 뭔가요",
          "original_form": "종류가 뭔가요",
          "speaker_id": "2",
          "time": "20210904 22:13"
        },
        {
          "id": "MDRW2100031503.1.5",
          "form": "그냥 길고양이 치즈 냥이에요",
          "original_form": "그냥 길고양이 치즈 냥이에요",
          "speaker_id": "1",
          "time": "20210904 22:14"
        },
        {
          "id": "MDRW2100031503.1.6",
          "form": "치즈가 순하다고",
          "original_form": "치즈가 순하다고",
          "speaker_id": "2",
          "time": "20210904 22:14"
        },
        {
          "id": "MDRW2100031503.1.7",
          "form": "맞아요 맞아요 완전 쓰세요",
          "original_form": "맞아요 맞아요 완전 쓰세요",
          "speaker_id": "1",
          "time": "20210904 22:14"
        },
        {
          "id": "MDRW2100031503.1.8",
          "form": "안전수 네요",
          "original_form": "안전수 네요",
          "speaker_id": "1",
          "time": "20210904 22:14"
        },
        {
          "id": "MDRW2100031503.1.9",
          "form": "순해요",
          "original_form": "순해요",
          "speaker_id": "1",
          "time": "20210904 22:14"
        },
        {
          "id": "MDRW2100031503.1.10",
          "form": "기억 있네요",
          "original_form": "기억 있네요",
          "speaker_id": "2",
          "time": "20210904 22:14"
        },
        {
          "id": "MDRW2100031503.1.11",
          "form": "본 기억이",
          "original_form": "본 기억이",
          "speaker_id": "2",
          "time": "20210904 22:14"
        },
        {
          "id": "MDRW2100031503.1.12",
          "form": "맞아요 거기서 꼬리가 나오는데 너무 이뻐요",
          "original_form": "맞아요 거기서 꼬리가 나오는데 너무 이뻐요",
          "speaker_id": "1",
          "time": "20210904 22:14"
        },
        {
          "id": "MDRW2100031503.1.13",
          "form": "그 꼬리짧은 애 말하는",
          "original_form": "그 꼬리짧은 애 말하는",
          "speaker_id": "2",
          "time": "20210904 22:14"
        },
        {
          "id": "MDRW2100031503.1.14",
          "form": "내꼬리 양치하는 거 보고 있는데 너무 귀여워요",
          "original_form": "내꼬리 양치하는 거 보고 있는데 너무 귀여워요",
          "speaker_id": "1",
          "time": "20210904 22:14"
        },
        {
          "id": "MDRW2100031503.1.15",
          "form": "고양이도 양치해요",
          "original_form": "고양이도 양치해요",
          "speaker_id": "2",
          "time": "20210904 22:15"
        },
        {
          "id": "MDRW2100031503.1.16",
          "form": "내 그래야지 치아가 건강해진 데요",
          "original_form": "내 그래야지 치아가 건강해진 데요",
          "speaker_id": "1",
