      "utterance": [
        {
          "id": "MDRW2100040670.1.1",
          "form": "근데 다른애들이랑은 안 그런데",
          "original_form": "근데 다른애들이랑은 안 그런데",
          "speaker_id": "2",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100040670.1.2",
          "form": "응응ㅋ",
          "original_form": "응응ㅋ",
          "speaker_id": "1",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100040670.1.3",
          "form": "꼭 name3랑은 제일 친해서 그런지",
          "original_form": "꼭 &name3&랑은 제일 친해서 그런지",
          "speaker_id": "2",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100040670.1.4",
          "form": "너무 친해서",
          "original_form": "너무 친해서",
          "speaker_id": "1",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100040670.1.5",
          "form": "엄청 싸우더라고",
          "original_form": "엄청 싸우더라고",
          "speaker_id": "2",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100040670.1.6",
          "form": "그런듯ㅋ",
          "original_form": "그런듯ㅋ",
          "speaker_id": "1",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100040670.1.7",
          "form": "다른친구랑은 안그러니",
          "original_form": "다른친구랑은 안그러니",
          "speaker_id": "2",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100040670.1.8",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100040670.1.9",
          "form": "안싸우면서 좀 놀았으면 좋겠구만",
          "original_form": "안싸우면서 좀 놀았으면 좋겠구만",
          "speaker_id": "2",
          "time": "20210904 23:53"
        },
        {
          "id": "MDRW2100040670.1.10",
          "form": "애들은 또 애들이니",
          "original_form": "애들은 또 애들이니",
          "speaker_id": "2",
          "time": "20210904 23:53"
        },
        {
          "id": "MDRW2100040670.1.11",
          "form": "아직 어려도 자존심이 잇으니ㅋ",
          "original_form": "아직 어려도 자존심이 잇으니ㅋ",
          "speaker_id": "1",
          "time": "20210904 23:53"
        },
        {
          "id": "MDRW2100040670.1.12",
          "form": "그러니",
          "original_form": "그러니",
          "speaker_id": "2",
          "time": "20210904 23:53"
        },
        {
          "id": "MDRW2100040670.1.13",
          "form": "쪼메난 녀석들이",
          "original_form": "쪼메난 녀석들이",
          "speaker_id": "2",
          "time": "20210904 23:53"
        },
        {
          "id": "MDRW2100040670.1.14",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:53"
        },
        {
          "id": "MDRW2100040670.1.15",
          "form": "그래 맞어",
          "original_form": "그래 맞어",
          "speaker_id": "1",
          "time": "20210904 23:53"
        },
        {
          "id": "MDRW2100040670.1.16",
          "form": "애들 일찍 잠들었나??",
          "original_form": "애들 일찍 잠들었나??",
          "speaker_id": "2",
          "time": "20210904 23:53"
        },
        {
