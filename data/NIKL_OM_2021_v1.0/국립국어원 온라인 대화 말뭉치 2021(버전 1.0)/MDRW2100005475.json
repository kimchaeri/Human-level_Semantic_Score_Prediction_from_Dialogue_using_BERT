      "utterance": [
        {
          "id": "MDRW2100005475.1.1",
          "form": "또 다른 거 추천해줄래?",
          "original_form": "또 다른 거 추천해줄래?",
          "speaker_id": "1",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005475.1.2",
          "form": "응 영국드라마 추천해줄게",
          "original_form": "응 영국드라마 추천해줄게",
          "speaker_id": "2",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005475.1.3",
          "form": "영국 드라마?",
          "original_form": "영국 드라마?",
          "speaker_id": "1",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005475.1.4",
          "form": "시트콤이라고 해야 더 맞나",
          "original_form": "시트콤이라고 해야 더 맞나",
          "speaker_id": "2",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005475.1.5",
          "form": "시트콤?",
          "original_form": "시트콤?",
          "speaker_id": "1",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005475.1.6",
          "form": "미란다라는 영드인데 완전 웃겨 ㅋㅋ",
          "original_form": "미란다라는 영드인데 완전 웃겨 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005475.1.7",
          "form": "미란다 메모한다",
          "original_form": "미란다 메모한다",
          "speaker_id": "1",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005475.1.8",
          "form": "진짜 웃겨 ㅋㅋㅋ 꼭 봐야해",
          "original_form": "진짜 웃겨 ㅋㅋㅋ 꼭 봐야해",
          "speaker_id": "2",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005475.1.9",
          "form": "코믹이야?",
          "original_form": "코믹이야?",
          "speaker_id": "1",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005475.1.10",
          "form": "응 ㅋㅋ 빵빵 터져",
          "original_form": "응 ㅋㅋ 빵빵 터져",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005475.1.11",
          "form": "미란다라는 사람 이야기인데",
          "original_form": "미란다라는 사람 이야기인데",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005475.1.12",
          "form": "미란다가 사람 이름이구나",
          "original_form": "미란다가 사람 이름이구나",
          "speaker_id": "1",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005475.1.13",
          "form": "미란다 일상 생활이 개그야",
          "original_form": "미란다 일상 생활이 개그야",
          "speaker_id": "2",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005475.1.14",
          "form": "재밌다니까 보고 싶네",
          "original_form": "재밌다니까 보고 싶네",
