      "utterance": [
        {
          "id": "MDRW2100019729.1.1",
          "form": "넷플릭스를 이용하시나요?",
          "original_form": "넷플릭스를 이용하시나요?",
          "speaker_id": "1",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100019729.1.2",
          "form": "아뇽 예전에 이용하고 지금은 안해요ㅎ",
          "original_form": "아뇽 예전에 이용하고 지금은 안해요ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:21"
        },
        {
          "id": "MDRW2100019729.1.3",
          "form": "넷플보세요?",
          "original_form": "넷플보세요?",
          "speaker_id": "2",
          "time": "20210827 22:21"
        },
        {
          "id": "MDRW2100019729.1.4",
          "form": "네 그런데 자주 보고있진않네요",
          "original_form": "네 그런데 자주 보고있진않네요",
          "speaker_id": "1",
          "time": "20210827 22:22"
        },
        {
          "id": "MDRW2100019729.1.5",
          "form": "취향이 좀 까다로워서 그런가봐요",
          "original_form": "취향이 좀 까다로워서 그런가봐요",
          "speaker_id": "1",
          "time": "20210827 22:22"
        },
        {
          "id": "MDRW2100019729.1.6",
          "form": "주로 어떤거 보세요?",
          "original_form": "주로 어떤거 보세요?",
          "speaker_id": "2",
          "time": "20210827 22:22"
        },
        {
          "id": "MDRW2100019729.1.7",
          "form": "드라마나 영화 애니요",
          "original_form": "드라마나 영화 애니요",
          "speaker_id": "1",
          "time": "20210827 22:22"
        },
        {
          "id": "MDRW2100019729.1.8",
          "form": "어라 다 보는 것 같네",
          "original_form": "어라 다 보는 것 같네",
          "speaker_id": "1",
          "time": "20210827 22:22"
        },
        {
          "id": "MDRW2100019729.1.9",
          "form": "ㅋㅋㅋㅋㅋ다 보시는 거같은데...?",
          "original_form": "ㅋㅋㅋㅋㅋ다 보시는 거같은데...?",
          "speaker_id": "2",
          "time": "20210827 22:23"
        },
        {
          "id": "MDRW2100019729.1.10",
          "form": "드라마는 뭐보세요?",
          "original_form": "드라마는 뭐보세요?",
          "speaker_id": "2",
          "time": "20210827 22:23"
        },
        {
          "id": "MDRW2100019729.1.11",
          "form": "재밌었던게",
          "original_form": "재밌었던게",
          "speaker_id": "1",
          "time": "20210827 22:24"
        },
        {
          "id": "MDRW2100019729.1.12",
          "form": "브루클린 나인나인",
          "original_form": "브루클린 나인나인",
          "speaker_id": "1",
          "time": "20210827 22:24"
        },
        {
          "id": "MDRW2100019729.1.13",
          "form": "엄브렐라 아카데미",
          "original_form": "엄브렐라 아카데미",
          "speaker_id": "1",
          "time": "20210827 22:24"
        },
        {
          "id": "MDRW2100019729.1.14",
          "form": "우리나라꺼는 괴물 추천드려요",
          "original_form": "우리나라꺼는 괴물 추천드려요",
          "speaker_id": "1",
          "time": "20210827 22:24"
        },
        {
          "id": "MDRW2100019729.1.15",
          "form": "오...저 두가지는 미드인가요?",
          "original_form": "오...저 두가지는 미드인가요?",
          "speaker_id": "2",
          "time": "20210827 22:25"
        },
        {
          "id": "MDRW2100019729.1.16",
          "form": "그렇습니다 위에 두가지는 미국드라마예요",
          "original_form": "그렇습니다 위에 두가지는 미국드라마예요",
          "speaker_id": "1",
          "time": "20210827 22:25"
        },
        {
          "id": "MDRW2100019729.1.17",
          "form": "장르가 어떻게 되나요??",
          "original_form": "장르가 어떻게 되나요??",
          "speaker_id": "2",
          "time": "20210827 22:25"
        },
        {
          "id": "MDRW2100019729.1.18",
          "form": "브룩은 코믹이고",
          "original_form": "브룩은 코믹이고",
          "speaker_id": "1",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100019729.1.19",
          "form": "엄브렐라는... 장르를 뭐로 정의해야할지",
          "original_form": "엄브렐라는... 장르를 뭐로 정의해야할지",
          "speaker_id": "1",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100019729.1.20",
          "form": "괴물은 형사물인데 정말 재밌습니다",
          "original_form": "괴물은 형사물인데 정말 재밌습니다",
          "speaker_id": "1",
          "time": "20210827 22:26"
        },
        {
          "id": "MDRW2100019729.1.21",
          "form": "오...저는 펜하랑 결혼작사 이혼작곡하고 악마판사 봤어요",
          "original_form": "오...저는 펜하랑 결혼작사 이혼작곡하고 악마판사 봤어요",
          "speaker_id": "2",
          "time": "20210827 22:27"
        },
        {
          "id": "MDRW2100019729.1.22",
          "form": "펜하 보시는군요",
          "original_form": "펜하 보시는군요",
          "speaker_id": "1",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019729.1.23",
          "form": "괴물 한번 봐야겠네요",
          "original_form": "괴물 한번 봐야겠네요",
          "speaker_id": "2",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019729.1.24",
          "form": "시즌2까지는 정말 재밌었는데 지금은...하하",
          "original_form": "시즌2까지는 정말 재밌었는데 지금은...하하",
          "speaker_id": "1",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019729.1.25",
          "form": "ㅋㅋㅋㅋ저도 중간에 이상해져서",
          "original_form": "ㅋㅋㅋㅋ저도 중간에 이상해져서",
          "speaker_id": "2",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019729.1.26",
          "form": "안보다가 보다가 중...ㅜ",
          "original_form": "안보다가 보다가 중...ㅜ",
          "speaker_id": "2",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019729.1.27",
          "form": "저도 그래요 오늘 하는데 엄마가 그만보자고 하면서 지금 다큐보세요",
          "original_form": "저도 그래요 오늘 하는데 엄마가 그만보자고 하면서 지금 다큐보세요",
          "speaker_id": "1",
          "time": "20210827 22:29"
        },
        {
          "id": "MDRW2100019729.1.28",
          "form": "아 오늘 초반부분을 놓쳤네요...",
          "original_form": "아 오늘 초반부분을 놓쳤네요...",
          "speaker_id": "2",
          "time": "20210827 22:29"
        },
        {
          "id": "MDRW2100019729.1.29",
          "form": "저번 11화 때 마지막장면이 너무 충격이었는데",
          "original_form": "저번 11화 때 마지막장면이 너무 충격이었는데",
          "speaker_id": "2",
          "time": "20210827 22:29"
        }
      ]
    }
