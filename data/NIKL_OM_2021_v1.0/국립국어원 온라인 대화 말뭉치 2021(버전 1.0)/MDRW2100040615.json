      "utterance": [
        {
          "id": "MDRW2100040615.1.1",
          "form": "최근 잘 산거 있어?",
          "original_form": "최근 잘 산거 있어?",
          "speaker_id": "1",
          "time": "20210904 23:11"
        },
        {
          "id": "MDRW2100040615.1.2",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100040615.1.3",
          "form": "브리타??",
          "original_form": "브리타??",
          "speaker_id": "2",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100040615.1.4",
          "form": "알아?",
          "original_form": "알아?",
          "speaker_id": "2",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100040615.1.5",
          "form": "브리타 뭐지?",
          "original_form": "브리타 뭐지?",
          "speaker_id": "1",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100040615.1.6",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100040615.1.7",
          "form": "정수기같은거?",
          "original_form": "정수기같은거?",
          "speaker_id": "2",
          "time": "20210904 23:19"
        },
        {
          "id": "MDRW2100040615.1.8",
          "form": "아 뭔지 알아 독일거 아냐?",
          "original_form": "아 뭔지 알아 독일거 아냐?",
          "speaker_id": "1",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100040615.1.9",
          "form": "물 걸러주는 물병",
          "original_form": "물 걸러주는 물병",
          "speaker_id": "1",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100040615.1.10",
          "form": "응 그거!",
          "original_form": "응 그거!",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100040615.1.11",
          "form": "사먹으니까",
          "original_form": "사먹으니까",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100040615.1.12",
          "form": "플라스틱이 너무많이나와서",
          "original_form": "플라스틱이 너무많이나와서",
          "speaker_id": "2",
          "time": "20210904 23:21"
        },
        {
          "id": "MDRW2100040615.1.13",
          "form": "헐 생수에서?",
          "original_form": "헐 생수에서?",
          "speaker_id": "1",
          "time": "20210904 23:24"
        },
        {
          "id": "MDRW2100040615.1.14",
          "form": "아니 병이ㅋㅋㅋ",
          "original_form": "아니 병이ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:24"
        },
        {
          "id": "MDRW2100040615.1.15",
          "form": "매번 버리기 귀찮았어",
          "original_form": "매번 버리기 귀찮았어",
          "speaker_id": "2",
          "time": "20210904 23:24"
        },
        {
          "id": "MDRW2100040615.1.16",
          "form": "아아 플라스팅 재활용",
          "original_form": "아아 플라스팅 재활용",
          "speaker_id": "1",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100040615.1.17",
          "form": "그것도 그래",
          "original_form": "그것도 그래",
          "speaker_id": "1",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100040615.1.18",
          "form": "그래서 우리집도 다시 정수기로 돌아가",
          "original_form": "그래서 우리집도 다시 정수기로 돌아가",
          "speaker_id": "1",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100040615.1.19",
          "form": "그럼 브리타는 수돗물 걸러먹는거지?",
          "original_form": "그럼 브리타는 수돗물 걸러먹는거지?",
          "speaker_id": "1",
          "time": "20210904 23:25"
        },
        {
          "id": "MDRW2100040615.1.20",
          "form": "응! 수돗물이야",
          "original_form": "응! 수돗물이야",
          "speaker_id": "2",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100040615.1.21",
          "form": "그래도 거르니까",
          "original_form": "그래도 거르니까",
          "speaker_id": "2",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100040615.1.22",
          "form": "수돗물맛안나드라거ㅋㅋㅋ",
          "original_form": "수돗물맛안나드라거ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100040615.1.23",
          "form": "와 정말",
          "original_form": "와 정말",
          "speaker_id": "1",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100040615.1.24",
          "form": "효과좋네",
          "original_form": "효과좋네",
          "speaker_id": "1",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100040615.1.25",
          "form": "오늘도 브리타물 먹었다",
          "original_form": "오늘도 브리타물 먹었다",
          "speaker_id": "2",
          "time": "20210905 00:17"
        },
        {
          "id": "MDRW2100040615.1.26",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 00:17"
        },
        {
          "id": "MDRW2100040615.1.27",
          "form": "내 주변엔 진짜 많이 쓰더라",
          "original_form": "내 주변엔 진짜 많이 쓰더라",
          "speaker_id": "2",
          "time": "20210905 00:17"
        },
        {
          "id": "MDRW2100040615.1.28",
          "form": "특히자취생들!",
          "original_form": "특히자취생들!",
          "speaker_id": "2",
          "time": "20210905 00:17"
        },
        {
          "id": "MDRW2100040615.1.29",
          "form": "아정말 난 남친이 유럽유학중이라",
          "original_form": "아정말 난 남친이 유럽유학중이라",
          "speaker_id": "1",
          "time": "20210905 08:53"
        },
        {
          "id": "MDRW2100040615.1.30",
          "form": "그거 써보라했는데 말 안들어",
          "original_form": "그거 써보라했는데 말 안들어",
          "speaker_id": "1",
          "time": "20210905 08:53"
        }
      ]
    }
