      "utterance": [
        {
          "id": "MDRW2100022033.1.1",
          "form": "제가 알기로는 펜트하우스랑 고민하다가",
          "original_form": "제가 알기로는 펜트하우스랑 고민하다가",
          "speaker_id": "1",
          "time": "20210829 01:05"
        },
        {
          "id": "MDRW2100022033.1.2",
          "form": "카이로스 선택했다고 들었어요",
          "original_form": "카이로스 선택했다고 들었어요",
          "speaker_id": "1",
          "time": "20210829 01:05"
        },
        {
          "id": "MDRW2100022033.1.3",
          "form": "나중에 이용하는 ott에 있으면 봐야겠네요ㅠㅠ",
          "original_form": "나중에 이용하는 ott에 있으면 봐야겠네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 01:05"
        },
        {
          "id": "MDRW2100022033.1.4",
          "form": "name1 혹시 신의 퀴즈라는 드라마 아시나요??",
          "original_form": "&name1& 혹시 신의 퀴즈라는 드라마 아시나요??",
          "speaker_id": "2",
          "time": "20210829 01:06"
        },
        {
          "id": "MDRW2100022033.1.5",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210829 01:06"
        },
        {
          "id": "MDRW2100022033.1.6",
          "form": "그 누구였더라",
          "original_form": "그 누구였더라",
          "speaker_id": "1",
          "time": "20210829 01:06"
        },
        {
          "id": "MDRW2100022033.1.7",
          "form": "전원일기 아역으로 나온 배우였는데",
          "original_form": "전원일기 아역으로 나온 배우였는데",
          "speaker_id": "1",
          "time": "20210829 01:06"
        },
        {
          "id": "MDRW2100022033.1.8",
          "form": "류덕환 배우님이요!",
          "original_form": "류덕환 배우님이요!",
          "speaker_id": "2",
          "time": "20210829 01:06"
        },
        {
          "id": "MDRW2100022033.1.9",
          "form": "아아 맞아요!ㅋㅋㅋ",
          "original_form": "아아 맞아요!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 01:06"
        },
        {
          "id": "MDRW2100022033.1.10",
          "form": "저는 처음에 봤던 역할이 천하장사 마돈나였어서 같은 배운지 못알아봤었어욬ㅋㅋ",
          "original_form": "저는 처음에 봤던 역할이 천하장사 마돈나였어서 같은 배운지 못알아봤었어욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 01:07"
        },
        {
          "id": "MDRW2100022033.1.11",
          "form": "이것도 재밌다고 얘기 진짜 많이 들었어요",
          "original_form": "이것도 재밌다고 얘기 진짜 많이 들었어요",
          "speaker_id": "1",
          "time": "20210829 01:07"
        },
        {
          "id": "MDRW2100022033.1.12",
          "form": "시즌 1,2가 찐이에요ㅜㅠ",
          "original_form": "시즌 1,2가 찐이에요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210829 01:07"
        },
        {
          "id": "MDRW2100022033.1.13",
          "form": "와ㅋㅋㅋㅋ 갭이 엄청나네요",
          "original_form": "와ㅋㅋㅋㅋ 갭이 엄청나네요",
          "speaker_id": "1",
          "time": "20210829 01:07"
        },
        {
          "id": "MDRW2100022033.1.14",
          "form": "저 이런거 좋아하는데 꼭 볼게요",
          "original_form": "저 이런거 좋아하는데 꼭 볼게요",
          "speaker_id": "1",
          "time": "20210829 01:07"
        },
        {
          "id": "MDRW2100022033.1.15",
          "form": "마돈나 찍을때 일부러 살찌웠다고 하시더라구요ㅠㅠ",
          "original_form": "마돈나 찍을때 일부러 살찌웠다고 하시더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 01:07"
        },
        {
          "id": "MDRW2100022033.1.16",
          "form": "이런거보면 진짜 대단해요",
          "original_form": "이런거보면 진짜 대단해요",
          "speaker_id": "1",
          "time": "20210829 01:07"
        },
        {
          "id": "MDRW2100022033.1.17",
          "form": "시즌 1, 2가 주연 조연 할거 없이 제일 케미좋고 재밌어요 ㅠㅠ",
          "original_form": "시즌 1, 2가 주연 조연 할거 없이 제일 케미좋고 재밌어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 01:08"
        },
        {
          "id": "MDRW2100022033.1.18",
          "form": "드라마나 영화 작품때문에 체중 늘렸다 조였다하는게ㅋㅋㅋ",
          "original_form": "드라마나 영화 작품때문에 체중 늘렸다 조였다하는게ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 01:08"
        },
        {
          "id": "MDRW2100022033.1.19",
          "form": "고구마같은 전개는 없나용?",
          "original_form": "고구마같은 전개는 없나용?",
          "speaker_id": "1",
          "time": "20210829 01:08"
        },
        {
          "id": "MDRW2100022033.1.20",
          "form": "맞아요ㅠㅠㅠㅠ",
          "original_form": "맞아요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 01:08"
        },
        {
          "id": "MDRW2100022033.1.21",
          "form": "그 외국배우들 입금전후 그런거 보면ㅋㅋㅋㅋ",
          "original_form": "그 외국배우들 입금전후 그런거 보면ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 01:08"
        },
        {
          "id": "MDRW2100022033.1.22",
          "form": "갑자기 소지섭 배우 입금전후가 생각나네욬ㅋㅋㅋㅋㅋ",
          "original_form": "갑자기 소지섭 배우 입금전후가 생각나네욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 01:08"
        },
        {
          "id": "MDRW2100022033.1.23",
          "form": "보다보면 고구마도 살짝 있기는 한데 큰스토리는 그렇고 옴니버스식이라 막혀서 답답한곤 없어요",
          "original_form": "보다보면 고구마도 살짝 있기는 한데 큰스토리는 그렇고 옴니버스식이라 막혀서 답답한곤 없어요",
          "speaker_id": "2",
          "time": "20210829 01:09"
        },
        {
          "id": "MDRW2100022033.1.24",
          "form": "옴니버스식 좋아해용",
          "original_form": "옴니버스식 좋아해용",
          "speaker_id": "1",
          "time": "20210829 01:09"
        },
        {
          "id": "MDRW2100022033.1.25",
          "form": "한편씩 보는거ㅋㅋㅋㅋ",
          "original_form": "한편씩 보는거ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 01:09"
        }
      ]
