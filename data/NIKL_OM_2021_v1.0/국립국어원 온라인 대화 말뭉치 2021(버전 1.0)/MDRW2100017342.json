      "utterance": [
        {
          "id": "MDRW2100017342.1.1",
          "form": "오늘 부업했네요",
          "original_form": "오늘 부업했네요",
          "speaker_id": "2",
          "time": "20210826 18:49"
        },
        {
          "id": "MDRW2100017342.1.2",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017342.1.3",
          "form": "어플을 이용한 앱테크라고",
          "original_form": "어플을 이용한 앱테크라고",
          "speaker_id": "2",
          "time": "20210826 18:57"
        },
        {
          "id": "MDRW2100017342.1.4",
          "form": "이제 캐시슬라이드같은걸로 해서 돈벌어용",
          "original_form": "이제 캐시슬라이드같은걸로 해서 돈벌어용",
          "speaker_id": "2",
          "time": "20210826 18:57"
        },
        {
          "id": "MDRW2100017342.1.5",
          "form": "돈 좀되나요?",
          "original_form": "돈 좀되나요?",
          "speaker_id": "1",
          "time": "20210826 18:58"
        },
        {
          "id": "MDRW2100017342.1.6",
          "form": "많이되는 편은 아니지만",
          "original_form": "많이되는 편은 아니지만",
          "speaker_id": "2",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017342.1.7",
          "form": "안하는거보단 좋으니 하고있어요 ㅎㅎ",
          "original_form": "안하는거보단 좋으니 하고있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017342.1.8",
          "form": "그렇됴",
          "original_form": "그렇됴",
          "speaker_id": "1",
          "time": "20210826 19:13"
        },
        {
          "id": "MDRW2100017342.1.9",
          "form": "돈을 버냐 안버냐에 따라",
          "original_form": "돈을 버냐 안버냐에 따라",
          "speaker_id": "2",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017342.1.10",
          "form": "생활이 달라지죵",
          "original_form": "생활이 달라지죵",
          "speaker_id": "2",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017342.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 19:26"
        },
        {
          "id": "MDRW2100017342.1.12",
          "form": "돈벌어서 간간히",
          "original_form": "돈벌어서 간간히",
          "speaker_id": "2",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100017342.1.13",
          "form": "야식도 먹고 그래요 ㅎㅎ",
          "original_form": "야식도 먹고 그래요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100017342.1.14",
          "form": "부업이 참 좋아요!",
          "original_form": "부업이 참 좋아요!",
          "speaker_id": "2",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100017342.1.15",
          "form": "가계에 도움이 되네요",
          "original_form": "가계에 도움이 되네요",
          "speaker_id": "1",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017342.1.16",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100017342.1.17",
          "form": "저는 보업에 충실",
          "original_form": "저는 보업에 충실",
          "speaker_id": "1",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100017342.1.18",
          "form": "본업에 충실하는것도 좋죠!",
          "original_form": "본업에 충실하는것도 좋죠!",
          "speaker_id": "2",
          "time": "20210826 20:52"
        },
        {
          "id": "MDRW2100017342.1.19",
          "form": "저도 얼른 군문제 해결하고 본업가지고싶네요~",
          "original_form": "저도 얼른 군문제 해결하고 본업가지고싶네요~",
          "speaker_id": "2",
          "time": "20210826 20:53"
        }
      ]
