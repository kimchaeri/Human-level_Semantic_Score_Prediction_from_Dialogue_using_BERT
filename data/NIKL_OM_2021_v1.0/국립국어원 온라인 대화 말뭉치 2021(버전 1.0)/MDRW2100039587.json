      "utterance": [
        {
          "id": "MDRW2100039587.1.1",
          "form": "지금 계절 진짜 너무 좋아",
          "original_form": "지금 계절 진짜 너무 좋아",
          "speaker_id": "2",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100039587.1.2",
          "form": "완전 내 최애야",
          "original_form": "완전 내 최애야",
          "speaker_id": "2",
          "time": "20210903 19:19"
        },
        {
          "id": "MDRW2100039587.1.3",
          "form": "가을 좋아해요?",
          "original_form": "가을 좋아해요?",
          "speaker_id": "1",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100039587.1.4",
          "form": "오왕",
          "original_form": "오왕",
          "speaker_id": "1",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100039587.1.5",
          "form": "저도 가을 좋아요",
          "original_form": "저도 가을 좋아요",
          "speaker_id": "1",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100039587.1.6",
          "form": "적당히 쌀쌀하고",
          "original_form": "적당히 쌀쌀하고",
          "speaker_id": "1",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100039587.1.7",
          "form": "코디하기도 좋고 ㅎㅎ",
          "original_form": "코디하기도 좋고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100039587.1.8",
          "form": "맞아 ㅠㅠ 특히 난 여름에서 이렇게 가을",
          "original_form": "맞아 ㅠㅠ 특히 난 여름에서 이렇게 가을",
          "speaker_id": "2",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100039587.1.9",
          "form": "넘어가는 때에 바뀌는",
          "original_form": "넘어가는 때에 바뀌는",
          "speaker_id": "2",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100039587.1.10",
          "form": "우왕 그때 딱 좋죠~",
          "original_form": "우왕 그때 딱 좋죠~",
          "speaker_id": "1",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100039587.1.11",
          "form": "쌀쌀한 공기가 너무 좋아",
          "original_form": "쌀쌀한 공기가 너무 좋아",
          "speaker_id": "2",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100039587.1.12",
          "form": "크 좋다.. 지금이네요 딱",
          "original_form": "크 좋다.. 지금이네요 딱",
          "speaker_id": "1",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100039587.1.13",
          "form": "요즘 마스크 때문에 밖에서 공기 마시기도 어려운데",
          "original_form": "요즘 마스크 때문에 밖에서 공기 마시기도 어려운데",
          "speaker_id": "2",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100039587.1.14",
          "form": "베란다 열고 숨 쉬면 진짜 최고~ㅠㅠ",
          "original_form": "베란다 열고 숨 쉬면 진짜 최고~ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100039587.1.15",
          "form": "은주는 무슨 계절 제일 좋아해?",
          "original_form": "은주는 무슨 계절 제일 좋아해?",
          "speaker_id": "2",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100039587.1.16",
          "form": "전 겨울이용",
          "original_form": "전 겨울이용",
          "speaker_id": "1",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100039587.1.17",
          "form": "겨울!!! 겨울도 너무 좋아ㅠㅠ",
          "original_form": "겨울!!! 겨울도 너무 좋아ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100039587.1.18",
          "form": "겨울 어떤 점이 제일 좋아?",
          "original_form": "겨울 어떤 점이 제일 좋아?",
          "speaker_id": "2",
          "time": "20210903 19:21"
        },
        {
          "id": "MDRW2100039587.1.19",
          "form": "그쵸~ 제 생일 있어서 좋아요 ㅋㅋㅋㅋ",
          "original_form": "그쵸~ 제 생일 있어서 좋아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100039587.1.20",
          "form": "ㅎㅎㅎ귀여워!!",
          "original_form": "ㅎㅎㅎ귀여워!!",
          "speaker_id": "2",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100039587.1.21",
          "form": "그치~~ 생일 있어서 제일 좋은 계절~~ㅎㅎㅎ",
          "original_form": "그치~~ 생일 있어서 제일 좋은 계절~~ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100039587.1.22",
          "form": "ㅋㅋㅋㅋ겨울느낌도 좋아요",
          "original_form": "ㅋㅋㅋㅋ겨울느낌도 좋아요",
          "speaker_id": "1",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100039587.1.23",
          "form": "추운건 싫지만 ㅠㅠ",
          "original_form": "추운건 싫지만 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 19:22"
        },
        {
          "id": "MDRW2100039587.1.24",
          "form": "약간 그런 느낌 좋아",
          "original_form": "약간 그런 느낌 좋아",
