      "utterance": [
        {
          "id": "MDRW2100028571.1.1",
          "form": "9월 되면서 시원해졌어",
          "original_form": "9월 되면서 시원해졌어",
          "speaker_id": "2",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100028571.1.2",
          "form": "금방 가을이야",
          "original_form": "금방 가을이야",
          "speaker_id": "1",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100028571.1.3",
          "form": "이제 안더워서 좋아",
          "original_form": "이제 안더워서 좋아",
          "speaker_id": "2",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100028571.1.4",
          "form": "덥지 않아서 다행이네",
          "original_form": "덥지 않아서 다행이네",
          "speaker_id": "1",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100028571.1.5",
          "form": "여름에 너무 더워서 힘들었어",
          "original_form": "여름에 너무 더워서 힘들었어",
          "speaker_id": "2",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100028571.1.6",
          "form": "너는 괜찮았어?",
          "original_form": "너는 괜찮았어?",
          "speaker_id": "2",
          "time": "20210902 20:53"
        },
        {
          "id": "MDRW2100028571.1.7",
          "form": "조금 더웠어",
          "original_form": "조금 더웠어",
          "speaker_id": "1",
          "time": "20210902 20:53"
        },
        {
          "id": "MDRW2100028571.1.8",
          "form": "여름 안왔으면 좋겠다",
          "original_form": "여름 안왔으면 좋겠다",
          "speaker_id": "1",
          "time": "20210902 20:53"
        },
        {
          "id": "MDRW2100028571.1.9",
          "form": "더운데 맨날 걸어오고",
          "original_form": "더운데 맨날 걸어오고",
          "speaker_id": "2",
          "time": "20210902 20:53"
        },
        {
          "id": "MDRW2100028571.1.10",
          "form": "나는 못한다",
          "original_form": "나는 못한다",
          "speaker_id": "2",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028571.1.11",
          "form": "뭘 못해?",
          "original_form": "뭘 못해?",
          "speaker_id": "1",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028571.1.12",
          "form": "여름에 수영하는건 좋잖아",
          "original_form": "여름에 수영하는건 좋잖아",
          "speaker_id": "2",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028571.1.13",
          "form": "나는 여름에 못걸어다닌다고",
          "original_form": "나는 여름에 못걸어다닌다고",
          "speaker_id": "2",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028571.1.14",
          "form": "수영하는거 좋다고?",
          "original_form": "수영하는거 좋다고?",
          "speaker_id": "1",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028571.1.15",
          "form": "수영 안좋아해?",
          "original_form": "수영 안좋아해?",
          "speaker_id": "2",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100028571.1.16",
          "form": "바닷가에서 수영하는건 여름밖에 못하잖아",
          "original_form": "바닷가에서 수영하는건 여름밖에 못하잖아",
          "speaker_id": "2",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100028571.1.17",
          "form": "수영 못해서",
          "original_form": "수영 못해서",
          "speaker_id": "1",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100028571.1.18",
          "form": "수영 배우고싶어?",
          "original_form": "수영 배우고싶어?",
          "speaker_id": "2",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100028571.1.19",
          "form": "배우면 할수있다고",
          "original_form": "배우면 할수있다고",
          "speaker_id": "1",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100028571.1.20",
          "form": "내년에 바닷가나 워터파크 가서 수영하자 그럼",
          "original_form": "내년에 바닷가나 워터파크 가서 수영하자 그럼",
          "speaker_id": "2",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100028571.1.21",
          "form": "어느 바닷가? 워터파크?",
          "original_form": "어느 바닷가? 워터파크?",
          "speaker_id": "1",
          "time": "20210902 20:57"
        },
        {
          "id": "MDRW2100028571.1.22",
          "form": "동해",
          "original_form": "동해",
          "speaker_id": "2",
          "time": "20210902 20:57"
        }
      ]
