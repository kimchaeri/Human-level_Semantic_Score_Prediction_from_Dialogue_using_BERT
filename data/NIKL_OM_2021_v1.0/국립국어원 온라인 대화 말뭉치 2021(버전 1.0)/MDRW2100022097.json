          "time": "20210829 03:06"
        },
        {
          "id": "MDRW2100022097.1.4",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210829 03:06"
        },
        {
          "id": "MDRW2100022097.1.5",
          "form": "오ㅑ죠",
          "original_form": "오ㅑ죠",
          "speaker_id": "1",
          "time": "20210829 03:08"
        },
        {
          "id": "MDRW2100022097.1.6",
          "form": "힘든거 싫어요",
