      "utterance": [
        {
          "id": "MDRW2100015923.1.1",
          "form": "싸이월드 혹시 아세요?",
          "original_form": "싸이월드 혹시 아세요?",
          "speaker_id": "2",
          "time": "20210826 03:00"
        },
        {
          "id": "MDRW2100015923.1.2",
          "form": "들어봤어요",
          "original_form": "들어봤어요",
          "speaker_id": "1",
          "time": "20210826 03:00"
        },
        {
          "id": "MDRW2100015923.1.3",
          "form": "혹시 해보신적 있으세요?",
          "original_form": "혹시 해보신적 있으세요?",
          "speaker_id": "2",
          "time": "20210826 03:00"
        },
        {
          "id": "MDRW2100015923.1.4",
          "form": "아니오.",
          "original_form": "아니오.",
          "speaker_id": "1",
          "time": "20210826 03:00"
        },
        {
          "id": "MDRW2100015923.1.5",
          "form": "저는 어렸을때 가끔 했는데",
          "original_form": "저는 어렸을때 가끔 했는데",
          "speaker_id": "2",
          "time": "20210826 03:00"
        },
        {
          "id": "MDRW2100015923.1.6",
          "form": "싸이월드가 부활한다니 신기하네요.",
          "original_form": "싸이월드가 부활한다니 신기하네요.",
          "speaker_id": "2",
          "time": "20210826 03:01"
        },
        {
          "id": "MDRW2100015923.1.7",
          "form": "그러거요",
          "original_form": "그러거요",
          "speaker_id": "1",
          "time": "20210826 03:01"
        },
        {
          "id": "MDRW2100015923.1.8",
          "form": "부활 소식 들었어요",
          "original_form": "부활 소식 들었어요",
          "speaker_id": "1",
          "time": "20210826 03:01"
        },
        {
          "id": "MDRW2100015923.1.9",
          "form": "싸이월드가 진짜 처음에는 획기적이었잖아요",
          "original_form": "싸이월드가 진짜 처음에는 획기적이었잖아요",
          "speaker_id": "2",
          "time": "20210826 03:01"
        },
        {
          "id": "MDRW2100015923.1.10",
          "form": "싸이월드 부활하면 다시 하실건가요?",
          "original_form": "싸이월드 부활하면 다시 하실건가요?",
          "speaker_id": "1",
          "time": "20210826 03:01"
        },
        {
          "id": "MDRW2100015923.1.11",
          "form": "솔직히 아니오...",
          "original_form": "솔직히 아니오...",
          "speaker_id": "2",
          "time": "20210826 03:01"
        },
        {
          "id": "MDRW2100015923.1.12",
          "form": "이제는 재미가 없어서",
          "original_form": "이제는 재미가 없어서",
          "speaker_id": "2",
          "time": "20210826 03:02"
        },
        {
          "id": "MDRW2100015923.1.13",
          "form": "요즈음엔 대체재가 많이 있네요",
          "original_form": "요즈음엔 대체재가 많이 있네요",
          "speaker_id": "1",
          "time": "20210826 03:02"
        },
        {
          "id": "MDRW2100015923.1.14",
          "form": "그런것같아요",
          "original_form": "그런것같아요",
          "speaker_id": "2",
          "time": "20210826 03:02"
        },
        {
          "id": "MDRW2100015923.1.15",
          "form": "싸이월드가 신선하기는 했는데",
          "original_form": "싸이월드가 신선하기는 했는데",
          "speaker_id": "2",
          "time": "20210826 03:02"
        },
        {
          "id": "MDRW2100015923.1.16",
          "form": "지금은 경쟁력이 적을 것 같아요",
          "original_form": "지금은 경쟁력이 적을 것 같아요",
          "speaker_id": "2",
          "time": "20210826 03:02"
        },
        {
          "id": "MDRW2100015923.1.17",
          "form": "그래도 부활했으니 잘되었으면좋겠네요",
          "original_form": "그래도 부활했으니 잘되었으면좋겠네요",
          "speaker_id": "1",
          "time": "20210826 03:03"
        },
        {
          "id": "MDRW2100015923.1.18",
          "form": "저도 그렇게 생각해요!!",
          "original_form": "저도 그렇게 생각해요!!",
          "speaker_id": "2",
          "time": "20210826 03:03"
        }
