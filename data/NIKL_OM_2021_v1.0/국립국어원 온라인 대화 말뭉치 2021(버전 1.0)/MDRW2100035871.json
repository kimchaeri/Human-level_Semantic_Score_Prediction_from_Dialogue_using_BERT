        {
          "id": "MDRW2100035871.1.2",
          "form": "흑",
          "original_form": "흑",
          "speaker_id": "1",
          "time": "20210830 10:01"
        },
        {
          "id": "MDRW2100035871.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 10:07"
        },
        {
          "id": "MDRW2100035871.1.4",
          "form": "저도 조카가 어린이가 되면,,, 그렇겠죠?ㅠ",
          "original_form": "저도 조카가 어린이가 되면,,, 그렇겠죠?ㅠ",
          "speaker_id": "2",
          "time": "20210830 10:07"
        },
        {
          "id": "MDRW2100035871.1.5",
          "form": "흡ㅠㅠ 성장은 기쁘지만 아쉬워요",
          "original_form": "흡ㅠㅠ 성장은 기쁘지만 아쉬워요",
          "speaker_id": "1",
          "time": "20210830 10:19"
        },
        {
          "id": "MDRW2100035871.1.6",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "2",
          "time": "20210830 10:19"
        },
        {
          "id": "MDRW2100035871.1.7",
          "form": "근데 요즘 유아콘텐츠도",
          "original_form": "근데 요즘 유아콘텐츠도",
          "speaker_id": "2",
          "time": "20210830 10:20"
        },
        {
          "id": "MDRW2100035871.1.8",
          "form": "정말 다양하더라구요",
          "original_form": "정말 다양하더라구요",
          "speaker_id": "2",
          "time": "20210830 10:20"
        },
        {
          "id": "MDRW2100035871.1.9",
          "form": "그래요? 저 헤이지니? 그 분밖에 몰라서욥",
          "original_form": "그래요? 저 헤이지니? 그 분밖에 몰라서욥",
          "speaker_id": "1",
          "time": "20210830 10:20"
        },
        {
          "id": "MDRW2100035871.1.10",
          "form": "아! 펭수도 알아욥!!",
          "original_form": "아! 펭수도 알아욥!!",
          "speaker_id": "1",
          "time": "20210830 10:21"
        },
        {
          "id": "MDRW2100035871.1.11",
          "form": "앗 저도 유투브는 잘 모르는데",
          "original_form": "앗 저도 유투브는 잘 모르는데",
          "speaker_id": "2",
          "time": "20210830 10:23"
        },
        {
          "id": "MDRW2100035871.1.12",
          "form": "도티? 인가",
          "original_form": "도티? 인가",
          "speaker_id": "2",
          "time": "20210830 10:23"
        },
        {
          "id": "MDRW2100035871.1.13",
          "form": "그사람도 초등학생들이 되게 좋아한다고 들은고같아요",
          "original_form": "그사람도 초등학생들이 되게 좋아한다고 들은고같아요",
          "speaker_id": "2",
          "time": "20210830 10:23"
        },
        {
          "id": "MDRW2100035871.1.14",
          "form": "핑크퐁도 저희가 아는 아빠상어엄마상어 이런거 말고도 많더라구요 ㅎㅎ",
          "original_form": "핑크퐁도 저희가 아는 아빠상어엄마상어 이런거 말고도 많더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 10:24"
        },
        {
          "id": "MDRW2100035871.1.15",
          "form": "아!  티비서 본 것 같아요ㅋㅋㅋㅋ",
          "original_form": "아!  티비서 본 것 같아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:48"
        },
        {
          "id": "MDRW2100035871.1.16",
          "form": "어.. 다른게 더 있었나요? 새로운 세계네요ㅋㅋ",
          "original_form": "어.. 다른게 더 있었나요? 새로운 세계네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 11:48"
        },
        {
          "id": "MDRW2100035871.1.17",
          "form": "저도 조카볼때 곁눈질하는 수준이라",
          "original_form": "저도 조카볼때 곁눈질하는 수준이라",
          "speaker_id": "2",
          "time": "20210830 12:57"
        },
        {
          "id": "MDRW2100035871.1.18",
          "form": "잘은 몰라요 ㅎㅎㅎㅎ",
          "original_form": "잘은 몰라요 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 12:57"
        },
        {
          "id": "MDRW2100035871.1.19",
          "form": "아! 그렇겠네요ㅋㅋ 따로 보진 않긴하죠ㅋㅋ",
          "original_form": "아! 그렇겠네요ㅋㅋ 따로 보진 않긴하죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:01"
        },
        {
          "id": "MDRW2100035871.1.20",
          "form": "ㅎㅎ맞아요",
          "original_form": "ㅎㅎ맞아요",
          "speaker_id": "2",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100035871.1.21",
          "form": "가끔 회사에서 일하다보면 노래듣고싶어져요",
          "original_form": "가끔 회사에서 일하다보면 노래듣고싶어져요",
          "speaker_id": "2",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100035871.1.22",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210830 13:56"
        },
        {
          "id": "MDRW2100035871.1.23",
          "form": "특히 애들게 중독성이 엄청나요ㅋㅋ",
          "original_form": "특히 애들게 중독성이 엄청나요ㅋㅋ",
