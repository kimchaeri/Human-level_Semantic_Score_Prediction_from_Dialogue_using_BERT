      "utterance": [
        {
          "id": "MDRW2100018386.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210827 00:50"
        },
        {
          "id": "MDRW2100018386.1.2",
          "form": "아니요ㅠㅠ name1은요?",
          "original_form": "아니요ㅠㅠ &name1&은요?",
          "speaker_id": "2",
          "time": "20210827 00:50"
        },
        {
          "id": "MDRW2100018386.1.3",
          "form": "저는 고양이 한 마리 키워요ㅎㅎ",
          "original_form": "저는 고양이 한 마리 키워요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 00:50"
        },
        {
          "id": "MDRW2100018386.1.4",
          "form": "담달이면 한 살 되네요",
          "original_form": "담달이면 한 살 되네요",
          "speaker_id": "1",
          "time": "20210827 00:51"
        },
        {
          "id": "MDRW2100018386.1.5",
          "form": "우와 너무 사랑스럽겠네요",
          "original_form": "우와 너무 사랑스럽겠네요",
          "speaker_id": "2",
          "time": "20210827 00:51"
        },
        {
          "id": "MDRW2100018386.1.6",
          "form": "이름이 뭐에요??",
          "original_form": "이름이 뭐에요??",
          "speaker_id": "2",
          "time": "20210827 00:51"
        },
        {
          "id": "MDRW2100018386.1.7",
          "form": "이름 name3에요 ㅎㅎㅎ",
          "original_form": "이름 &name3&에요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 00:51"
        },
        {
          "id": "MDRW2100018386.1.8",
          "form": "색 유추 가능하신가요?",
          "original_form": "색 유추 가능하신가요?",
          "speaker_id": "1",
          "time": "20210827 00:52"
        },
        {
          "id": "MDRW2100018386.1.9",
          "form": "검은색인가요??",
          "original_form": "검은색인가요??",
          "speaker_id": "2",
          "time": "20210827 00:52"
        },
        {
          "id": "MDRW2100018386.1.10",
          "form": "네넵 맞아요ㅎㅎㅎ!",
          "original_form": "네넵 맞아요ㅎㅎㅎ!",
          "speaker_id": "1",
          "time": "20210827 00:52"
        },
        {
          "id": "MDRW2100018386.1.11",
          "form": "반려동물 키울 생각은 없으세요?",
          "original_form": "반려동물 키울 생각은 없으세요?",
          "speaker_id": "1",
          "time": "20210827 00:52"
        },
        {
          "id": "MDRW2100018386.1.12",
          "form": "저는 사실 책임질 자신이 없어서 키우기 두려워요ㅠㅠ",
          "original_form": "저는 사실 책임질 자신이 없어서 키우기 두려워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 00:53"
        },
        {
          "id": "MDRW2100018386.1.13",
          "form": "아ㅠㅠ 저도 그랬어요",
          "original_form": "아ㅠㅠ 저도 그랬어요",
          "speaker_id": "1",
          "time": "20210827 00:53"
        },
        {
          "id": "MDRW2100018386.1.14",
          "form": "노곤하개랑 개를 낳았다라는 웹툰아세요?",
          "original_form": "노곤하개랑 개를 낳았다라는 웹툰아세요?",
          "speaker_id": "2",
          "time": "20210827 00:53"
        },
        {
          "id": "MDRW2100018386.1.15",
          "form": "헉 완적 좋아해요 특히 개를 낳았다!",
          "original_form": "헉 완적 좋아해요 특히 개를 낳았다!",
          "speaker_id": "1",
          "time": "20210827 00:53"
        },
        {
          "id": "MDRW2100018386.1.16",
          "form": "동동이? 너무 귀엽지 않나요",
          "original_form": "동동이? 너무 귀엽지 않나요",
          "speaker_id": "1",
          "time": "20210827 00:54"
        },
        {
          "id": "MDRW2100018386.1.17",
          "form": "저도 너무 좋아해요!! 웹툰 보면서 제가 반려동물을 키울 수 있는 사람인지 많이 생각해보게됐어요",
          "original_form": "저도 너무 좋아해요!! 웹툰 보면서 제가 반려동물을 키울 수 있는 사람인지 많이 생각해보게됐어요",
          "speaker_id": "2",
          "time": "20210827 00:54"
        },
        {
          "id": "MDRW2100018386.1.18",
          "form": "맞아요 사실 다 너무 귀여워요ㅠㅠ",
          "original_form": "맞아요 사실 다 너무 귀여워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 00:54"
        },
        {
          "id": "MDRW2100018386.1.19",
          "form": "진짜 그거 보려고 쿠키 다써요 ㅠㅠ",
          "original_form": "진짜 그거 보려고 쿠키 다써요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 00:55"
        },
        {
          "id": "MDRW2100018386.1.20",
          "form": "읽다보면 생각 많아짐...",
          "original_form": "읽다보면 생각 많아짐...",
          "speaker_id": "1",
          "time": "20210827 00:55"
        },
        {
          "id": "MDRW2100018386.1.21",
          "form": "이번 유기견 입양 에피도 좋더라구요 ㅎㅎㅎ",
          "original_form": "이번 유기견 입양 에피도 좋더라구요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 00:55"
        },
        {
          "id": "MDRW2100018386.1.22",
          "form": "네 진짜 유기견들 다 행복해졌으면 좋겠어요..ㅠㅠ",
          "original_form": "네 진짜 유기견들 다 행복해졌으면 좋겠어요..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 00:56"
        },
        {
          "id": "MDRW2100018386.1.23",
          "form": "맞아요 어떻게 다 그렇게 사연이 많은지 ㅠㅠ",
          "original_form": "맞아요 어떻게 다 그렇게 사연이 많은지 ㅠㅠ",
