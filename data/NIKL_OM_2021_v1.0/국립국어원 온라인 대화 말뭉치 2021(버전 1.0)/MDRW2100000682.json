      "utterance": [
        {
          "id": "MDRW2100000682.1.1",
          "form": "요리하시는거 좋아하세요?",
          "original_form": "요리하시는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000682.1.2",
          "form": "네 고기 유튜브 주로 봐요",
          "original_form": "네 고기 유튜브 주로 봐요",
          "speaker_id": "2",
          "time": "20210819 17:40"
        },
        {
          "id": "MDRW2100000682.1.3",
          "form": "이번 주제로 길게 해봐요",
          "original_form": "이번 주제로 길게 해봐요",
          "speaker_id": "2",
          "time": "20210819 17:40"
        },
        {
          "id": "MDRW2100000682.1.4",
          "form": "고기! 혹시 스테이크 같은거 주로 해드시나요?",
          "original_form": "고기! 혹시 스테이크 같은거 주로 해드시나요?",
          "speaker_id": "1",
          "time": "20210819 17:41"
        },
        {
          "id": "MDRW2100000682.1.5",
          "form": "네네 로즈마리넣고 아웃백스타일로",
          "original_form": "네네 로즈마리넣고 아웃백스타일로",
          "speaker_id": "2",
          "time": "20210819 17:41"
        },
        {
          "id": "MDRW2100000682.1.6",
          "form": "우와 대단하네요 스테이크 굽는거 생각보다 어렵던데ㅠㅠ",
          "original_form": "우와 대단하네요 스테이크 굽는거 생각보다 어렵던데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 17:42"
        },
        {
          "id": "MDRW2100000682.1.7",
          "form": "처음에는 덜 익히는거 포함해서 굽기 힘들었어요",
          "original_form": "처음에는 덜 익히는거 포함해서 굽기 힘들었어요",
          "speaker_id": "2",
          "time": "20210819 17:43"
        },
        {
          "id": "MDRW2100000682.1.8",
          "form": "그쵸 전 아직 굽기 단계들이 너무 어렵더라구요ㅠㅠ",
          "original_form": "그쵸 전 아직 굽기 단계들이 너무 어렵더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 17:44"
        },
        {
          "id": "MDRW2100000682.1.9",
          "form": "그래서 전 그냥 타이머써요",
          "original_form": "그래서 전 그냥 타이머써요",
          "speaker_id": "2",
          "time": "20210819 17:44"
        },
        {
          "id": "MDRW2100000682.1.10",
          "form": "오오 타이머로도 그 굽기가 정확하던가요? 다들 온도계 사라고 그러던데ㅠㅠ",
          "original_form": "오오 타이머로도 그 굽기가 정확하던가요? 다들 온도계 사라고 그러던데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 17:45"
        },
        {
          "id": "MDRW2100000682.1.11",
          "form": "온도계도 필요해요",
          "original_form": "온도계도 필요해요",
          "speaker_id": "2",
          "time": "20210819 17:47"
        },
        {
          "id": "MDRW2100000682.1.12",
          "form": "타이머랑 온도계만 있으면 됩니다",
          "original_form": "타이머랑 온도계만 있으면 됩니다",
          "speaker_id": "2",
          "time": "20210819 17:47"
        },
        {
          "id": "MDRW2100000682.1.13",
          "form": "그렇군요ㅎㅎ 다음에 스테이크 구울 때 잘 해봐야겠어요",
          "original_form": "그렇군요ㅎㅎ 다음에 스테이크 구울 때 잘 해봐야겠어요",
          "speaker_id": "1",
          "time": "20210819 17:49"
        }
