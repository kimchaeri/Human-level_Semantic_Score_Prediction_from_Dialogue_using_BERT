      "utterance": [
        {
          "id": "MDRW2100039374.1.1",
          "form": "오늘 점심엔 뭐 드셨나요?",
          "original_form": "오늘 점심엔 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210903 16:49"
        },
        {
          "id": "MDRW2100039374.1.2",
          "form": "어제 먹다남은 떡볶이랑 치킨이요",
          "original_form": "어제 먹다남은 떡볶이랑 치킨이요",
          "speaker_id": "2",
          "time": "20210903 16:50"
        },
        {
          "id": "MDRW2100039374.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210903 16:50"
        },
        {
          "id": "MDRW2100039374.1.4",
          "form": "저는 김치찌개를 먹었어요",
          "original_form": "저는 김치찌개를 먹었어요",
          "speaker_id": "1",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100039374.1.5",
          "form": "저녁엔 뭘 드실 예정인가요?",
          "original_form": "저녁엔 뭘 드실 예정인가요?",
          "speaker_id": "1",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100039374.1.6",
          "form": "삼겹살김치볶음밥이요~ name1님은요?",
          "original_form": "삼겹살김치볶음밥이요~ &name1&님은요?",
          "speaker_id": "2",
          "time": "20210903 17:16"
        },
        {
          "id": "MDRW2100039374.1.7",
          "form": "세상에 이름만 들어도 맛있겠네요",
          "original_form": "세상에 이름만 들어도 맛있겠네요",
          "speaker_id": "1",
          "time": "20210903 17:51"
        },
        {
          "id": "MDRW2100039374.1.8",
          "form": "저는 버거킹 와퍼를 먹었어요",
          "original_form": "저는 버거킹 와퍼를 먹었어요",
          "speaker_id": "1",
          "time": "20210903 17:52"
        },
        {
          "id": "MDRW2100039374.1.9",
          "form": "와 맛있겠어요 저도 햄버거 좋아해요 혜화에 있는 버거파크 추천해요!",
          "original_form": "와 맛있겠어요 저도 햄버거 좋아해요 혜화에 있는 버거파크 추천해요!",
          "speaker_id": "2",
          "time": "20210903 19:37"
        },
        {
          "id": "MDRW2100039374.1.10",
          "form": "오! 거기가 맛있군요 꼭 먹어봐야겠어요ㅎㅎ",
          "original_form": "오! 거기가 맛있군요 꼭 먹어봐야겠어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 19:38"
        },
        {
          "id": "MDRW2100039374.1.11",
          "form": "또 좋아하는 음식 있으신가요?",
          "original_form": "또 좋아하는 음식 있으신가요?",
          "speaker_id": "1",
          "time": "20210903 19:38"
        },
        {
          "id": "MDRW2100039374.1.12",
          "form": "한식이랑 치킨이요~ name1님은여",
          "original_form": "한식이랑 치킨이요~ &name1&님은여",
          "speaker_id": "2",
          "time": "20210903 19:38"
        },
        {
          "id": "MDRW2100039374.1.13",
          "form": "저는 일식이랑 피자요ㅋㅋ",
          "original_form": "저는 일식이랑 피자요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:39"
        },
        {
          "id": "MDRW2100039374.1.14",
          "form": "치킨파시군요!",
          "original_form": "치킨파시군요!",
          "speaker_id": "1",
          "time": "20210903 19:39"
        },
        {
          "id": "MDRW2100039374.1.15",
          "form": "네!ㅋㅋ 어떤 피자가 제일 맛있나요?",
          "original_form": "네!ㅋㅋ 어떤 피자가 제일 맛있나요?",
          "speaker_id": "2",
          "time": "20210904 12:55"
        },
        {
          "id": "MDRW2100039374.1.16",
          "form": "저는......그날 그날 다른 것 같긴한데\r\n하와이안 피자처럼 파인애플 들어간거랑",
          "original_form": "저는......그날 그날 다른 것 같긴한데\r\n하와이안 피자처럼 파인애플 들어간거랑",
          "speaker_id": "1",
          "time": "20210904 13:08"
        },
        {
          "id": "MDRW2100039374.1.17",
          "form": "페퍼로니 피자처럼 페퍼로니 듬뿍 들어간 피자",
          "original_form": "페퍼로니 피자처럼 페퍼로니 듬뿍 들어간 피자",
          "speaker_id": "1",
          "time": "20210904 13:08"
        },
        {
          "id": "MDRW2100039374.1.18",
          "form": "그리고 올리브 잔뜩....그리고 치즈 잔뜩 좋아해요",
          "original_form": "그리고 올리브 잔뜩....그리고 치즈 잔뜩 좋아해요",
          "speaker_id": "1",
          "time": "20210904 13:08"
        },
        {
          "id": "MDRW2100039374.1.19",
          "form": "name2님은 어떤 치킨 좋아하세요?!",
          "original_form": "&name2&님은 어떤 치킨 좋아하세요?!",
