      "utterance": [
        {
          "id": "MDRW2100032010.1.1",
          "form": "어떤 과목을 제일 좋아하셨나요? ㅎㅎ",
          "original_form": "어떤 과목을 제일 좋아하셨나요? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 10:46"
        },
        {
          "id": "MDRW2100032010.1.2",
          "form": "ㅋㅋㄱㅋㄱ신박한질문이에요",
          "original_form": "ㅋㅋㄱㅋㄱ신박한질문이에요",
          "speaker_id": "2",
          "time": "20210905 10:47"
        },
        {
          "id": "MDRW2100032010.1.3",
          "form": "늙었나 이런질문 넘 오랜만이네욬ㅋㅋㅋㅋ",
          "original_form": "늙었나 이런질문 넘 오랜만이네욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:47"
        },
        {
          "id": "MDRW2100032010.1.4",
          "form": "너무 반가운 질문이죠 ㅋㅋ",
          "original_form": "너무 반가운 질문이죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:48"
        },
        {
          "id": "MDRW2100032010.1.5",
          "form": "우선 급식일때 어떤 과목 좋아하셨어요?",
          "original_form": "우선 급식일때 어떤 과목 좋아하셨어요?",
          "speaker_id": "1",
          "time": "20210905 10:48"
        },
        {
          "id": "MDRW2100032010.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210905 10:49"
        },
        {
          "id": "MDRW2100032010.1.7",
          "form": "영어요",
          "original_form": "영어요",
          "speaker_id": "2",
          "time": "20210905 10:49"
        },
        {
          "id": "MDRW2100032010.1.8",
          "form": "이.. 이.. 글로벌한 분...",
          "original_form": "이.. 이.. 글로벌한 분...",
          "speaker_id": "1",
          "time": "20210905 10:56"
        },
        {
          "id": "MDRW2100032010.1.9",
          "form": "왜 영어를 좋아하셨어요??",
          "original_form": "왜 영어를 좋아하셨어요??",
          "speaker_id": "1",
          "time": "20210905 10:56"
        },
        {
          "id": "MDRW2100032010.1.10",
          "form": "ㅎㅎ그냥 뭔가 글자생김새도 발음도 좋았어요",
          "original_form": "ㅎㅎ그냥 뭔가 글자생김새도 발음도 좋았어요",
          "speaker_id": "2",
          "time": "20210905 11:02"
        },
        {
          "id": "MDRW2100032010.1.11",
          "form": "어쩌다보니 영어관련일하고 있네요 ㅋㅋㅋㅋ",
          "original_form": "어쩌다보니 영어관련일하고 있네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:03"
        },
        {
          "id": "MDRW2100032010.1.12",
          "form": "우와! 좋아하는 과목으로 일을 하고 계시는군요!",
          "original_form": "우와! 좋아하는 과목으로 일을 하고 계시는군요!",
          "speaker_id": "1",
          "time": "20210905 11:10"
        },
        {
          "id": "MDRW2100032010.1.13",
          "form": "얼마전에 다른분은 번역일을 하신다고 이야기를 나눴었는데",
          "original_form": "얼마전에 다른분은 번역일을 하신다고 이야기를 나눴었는데",
          "speaker_id": "1",
          "time": "20210905 11:11"
        },
        {
          "id": "MDRW2100032010.1.14",
          "form": "그분은 알바로 하신다고 하셨었거든요",
          "original_form": "그분은 알바로 하신다고 하셨었거든요",
          "speaker_id": "1",
          "time": "20210905 11:11"
        },
        {
          "id": "MDRW2100032010.1.15",
          "form": "직업적으로 좋아하는 과목이 계속 가다니 정말 신기해욤",
          "original_form": "직업적으로 좋아하는 과목이 계속 가다니 정말 신기해욤",
          "speaker_id": "1",
          "time": "20210905 11:11"
        },
        {
          "id": "MDRW2100032010.1.16",
          "form": "ㅎㅎ번역알바도 재밌겠어요",
          "original_form": "ㅎㅎ번역알바도 재밌겠어요",
          "speaker_id": "2",
          "time": "20210905 11:38"
        },
        {
          "id": "MDRW2100032010.1.17",
          "form": "ㅎㅎname1님은 무슨 과목 좋아하세요",
          "original_form": "ㅎㅎ&name1&님은 무슨 과목 좋아하세요",
          "speaker_id": "2",
          "time": "20210905 11:38"
        },
        {
          "id": "MDRW2100032010.1.18",
          "form": "저는... 음악 좋아했어요!",
          "original_form": "저는... 음악 좋아했어요!",
          "speaker_id": "1",
          "time": "20210905 11:54"
        },
        {
          "id": "MDRW2100032010.1.19",
          "form": "ㅎㅎ음악은 선생님따라",
          "original_form": "ㅎㅎ음악은 선생님따라",
          "speaker_id": "2",
          "time": "20210905 16:45"
        },
        {
          "id": "MDRW2100032010.1.20",
          "form": "차이가 컸던것 같아요",
          "original_form": "차이가 컸던것 같아요",
          "speaker_id": "2",
          "time": "20210905 16:45"
        },
        {
          "id": "MDRW2100032010.1.21",
          "form": "수업에대한 느낌이",
          "original_form": "수업에대한 느낌이",
          "speaker_id": "2",
          "time": "20210905 16:45"
        },
        {
          "id": "MDRW2100032010.1.22",
          "form": "맞아요 ㅋㅋ 음악 수업때",
          "original_form": "맞아요 ㅋㅋ 음악 수업때",
          "speaker_id": "1",
          "time": "20210905 18:13"
        },
        {
          "id": "MDRW2100032010.1.23",
          "form": "한사람씩 가창시험본다고 노래 부르는건 별로 안좋아했지만요 ㅋㅋㅋ",
          "original_form": "한사람씩 가창시험본다고 노래 부르는건 별로 안좋아했지만요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:14"
        },
        {
          "id": "MDRW2100032010.1.24",
          "form": "맞아요 가창이 젤 싫었지만",
          "original_form": "맞아요 가창이 젤 싫었지만",
          "speaker_id": "2",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100032010.1.25",
          "form": "음악자체는 좋은것같아요",
          "original_form": "음악자체는 좋은것같아요",
          "speaker_id": "2",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100032010.1.26",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100032010.1.27",
          "form": "예체능 과목중에 혹시 좋아하신 과목이 있으신가요??",
          "original_form": "예체능 과목중에 혹시 좋아하신 과목이 있으신가요??",
          "speaker_id": "1",
          "time": "20210905 20:58"
        },
        {
          "id": "MDRW2100032010.1.28",
          "form": "미술요 ㅎㅎ",
          "original_form": "미술요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 21:03"
        },
        {
          "id": "MDRW2100032010.1.29",
          "form": "미술 잘하는 사람들 짱 부러웠어요!",
          "original_form": "미술 잘하는 사람들 짱 부러웠어요!",
          "speaker_id": "1",
          "time": "20210905 21:06"
        }
      ]
    }
