      "utterance": [
        {
          "id": "MDRW2100018743.1.1",
          "form": "나 가인학생 방송 보는중",
          "original_form": "나 가인학생 방송 보는중",
          "speaker_id": "2",
          "time": "20210827 11:41"
        },
        {
          "id": "MDRW2100018743.1.2",
          "form": "항상 보네",
          "original_form": "항상 보네",
          "speaker_id": "1",
          "time": "20210827 11:41"
        },
        {
          "id": "MDRW2100018743.1.3",
          "form": "재밋으니깐",
          "original_form": "재밋으니깐",
          "speaker_id": "2",
          "time": "20210827 11:41"
        },
        {
          "id": "MDRW2100018743.1.4",
          "form": "넌 왜 아직도 학교야",
          "original_form": "넌 왜 아직도 학교야",
          "speaker_id": "2",
          "time": "20210827 11:41"
        },
        {
          "id": "MDRW2100018743.1.5",
          "form": "오늘 시험이라구~",
          "original_form": "오늘 시험이라구~",
          "speaker_id": "1",
          "time": "20210827 11:41"
        },
        {
          "id": "MDRW2100018743.1.6",
          "form": "언제끝난다남",
          "original_form": "언제끝난다남",
          "speaker_id": "2",
          "time": "20210827 11:41"
        },
        {
          "id": "MDRW2100018743.1.7",
          "form": "곧 이제 집가",
          "original_form": "곧 이제 집가",
          "speaker_id": "1",
          "time": "20210827 11:41"
        },
        {
          "id": "MDRW2100018743.1.8",
          "form": "얼른 오슈",
          "original_form": "얼른 오슈",
          "speaker_id": "2",
          "time": "20210827 11:41"
        },
        {
          "id": "MDRW2100018743.1.9",
          "form": "우리 같이 양띵봐야돼",
          "original_form": "우리 같이 양띵봐야돼",
          "speaker_id": "2",
          "time": "20210827 11:41"
        },
        {
          "id": "MDRW2100018743.1.10",
          "form": "알겟어!",
          "original_form": "알겟어!",
          "speaker_id": "1",
          "time": "20210827 11:41"
        },
        {
          "id": "MDRW2100018743.1.11",
          "form": "나도 빨리 집가고싶다",
          "original_form": "나도 빨리 집가고싶다",
          "speaker_id": "1",
          "time": "20210827 11:41"
        },
        {
          "id": "MDRW2100018743.1.12",
          "form": "집에 근데 빨래걷어야돼",
          "original_form": "집에 근데 빨래걷어야돼",
          "speaker_id": "2",
          "time": "20210827 11:42"
        },
        {
          "id": "MDRW2100018743.1.13",
          "form": "엄마가 어제 시켰음",
          "original_form": "엄마가 어제 시켰음",
          "speaker_id": "2",
          "time": "20210827 11:42"
        },
        {
          "id": "MDRW2100018743.1.14",
          "form": "아나",
          "original_form": "아나",
          "speaker_id": "1",
          "time": "20210827 11:42"
        },
        {
          "id": "MDRW2100018743.1.15",
          "form": "귀찮구만",
          "original_form": "귀찮구만",
          "speaker_id": "1",
          "time": "20210827 11:42"
        },
        {
          "id": "MDRW2100018743.1.16",
          "form": "그래도 같이혀",
          "original_form": "그래도 같이혀",
          "speaker_id": "2",
          "time": "20210827 11:42"
        },
        {
          "id": "MDRW2100018743.1.17",
          "form": "ㅜㅡ 알게써",
          "original_form": "ㅜㅡ 알게써",
          "speaker_id": "1",
          "time": "20210827 11:42"
        },
        {
          "id": "MDRW2100018743.1.18",
          "form": "맨날 해도 하기싫어",
          "original_form": "맨날 해도 하기싫어",
          "speaker_id": "2",
          "time": "20210827 11:42"
        },
        {
          "id": "MDRW2100018743.1.19",
          "form": "인정...",
          "original_form": "인정...",
          "speaker_id": "1",
          "time": "20210827 11:42"
        },
        {
          "id": "MDRW2100018743.1.20",
          "form": "맞다 청소기는?",
          "original_form": "맞다 청소기는?",
          "speaker_id": "1",
