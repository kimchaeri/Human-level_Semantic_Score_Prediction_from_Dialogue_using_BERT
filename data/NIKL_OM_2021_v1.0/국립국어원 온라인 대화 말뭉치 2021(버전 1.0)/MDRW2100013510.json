      "utterance": [
        {
          "id": "MDRW2100013510.1.1",
          "form": "또 다른 드라마 보는 거 있으세요?",
          "original_form": "또 다른 드라마 보는 거 있으세요?",
          "speaker_id": "1",
          "time": "20210825 14:56"
        },
        {
          "id": "MDRW2100013510.1.2",
          "form": "음 요즘하는거 중에 보는 건 없어요ㅜㅜ!",
          "original_form": "음 요즘하는거 중에 보는 건 없어요ㅜㅜ!",
          "speaker_id": "2",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013510.1.3",
          "form": "혹시 라켓소년단 작가님이 쓰신 슬기로운감빵생활 보셨나요?",
          "original_form": "혹시 라켓소년단 작가님이 쓰신 슬기로운감빵생활 보셨나요?",
          "speaker_id": "2",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013510.1.4",
          "form": "앗 그거 보려다 못봤어요",
          "original_form": "앗 그거 보려다 못봤어요",
          "speaker_id": "1",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013510.1.5",
          "form": "그거 어떤가요?",
          "original_form": "그거 어떤가요?",
          "speaker_id": "1",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013510.1.6",
          "form": "진짜 재밌어요!!! 그것도 감동, 재미 둘다 있어요!",
          "original_form": "진짜 재밌어요!!! 그것도 감동, 재미 둘다 있어요!",
          "speaker_id": "2",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013510.1.7",
          "form": "강추예요!!",
          "original_form": "강추예요!!",
          "speaker_id": "2",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013510.1.8",
          "form": "감동 재미를 갖췄다니",
          "original_form": "감동 재미를 갖췄다니",
          "speaker_id": "1",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013510.1.9",
          "form": "궁금해지는걸요",
          "original_form": "궁금해지는걸요",
          "speaker_id": "1",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013510.1.10",
          "form": "정해인씨 나온 작품 맞나요?",
          "original_form": "정해인씨 나온 작품 맞나요?",
          "speaker_id": "1",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013510.1.11",
          "form": "엇 네네!!",
          "original_form": "엇 네네!!",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013510.1.12",
          "form": "들어본거같아요ㅋㅋㅋ",
          "original_form": "들어본거같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013510.1.13",
          "form": "그 다른건 모르고 헤롱이인가 인물이 있다고 본거같아서",
          "original_form": "그 다른건 모르고 헤롱이인가 인물이 있다고 본거같아서",
          "speaker_id": "1",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013510.1.14",
          "form": "오오 맞아요 라켓에도 해롱이 잠깐 나왔어요!",
          "original_form": "오오 맞아요 라켓에도 해롱이 잠깐 나왔어요!",
          "speaker_id": "2",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013510.1.15",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "1",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013510.1.16",
          "form": "극중 누구셨나요!",
          "original_form": "극중 누구셨나요!",
          "speaker_id": "1",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013510.1.17",
          "form": "카메오로 나오신건가요 혹시",
          "original_form": "카메오로 나오신건가요 혹시",
          "speaker_id": "1",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100013510.1.18",
          "form": "그 우정출연하셨는데 이규형배우님이요! 마지막화쯤에",
          "original_form": "그 우정출연하셨는데 이규형배우님이요! 마지막화쯤에",
          "speaker_id": "2",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100013510.1.19",
          "form": "아 그분이시구나..!",
          "original_form": "아 그분이시구나..!",
          "speaker_id": "1",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100013510.1.20",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100013510.1.21",
          "form": "슬빵우정출연이 많았더라고요!",
          "original_form": "슬빵우정출연이 많았더라고요!",
          "speaker_id": "2",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100013510.1.22",
          "form": "서로다른 드라마의 우정출연이라니 보는 재미가있겠어요",
          "original_form": "서로다른 드라마의 우정출연이라니 보는 재미가있겠어요",
          "speaker_id": "1",
          "time": "20210825 15:05"
        }
      ]
