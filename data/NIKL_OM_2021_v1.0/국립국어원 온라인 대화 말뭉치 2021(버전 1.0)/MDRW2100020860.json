        },
        {
          "id": "MDRW2100020860.1.2",
          "form": "안녕하세요!!",
          "original_form": "안녕하세요!!",
          "speaker_id": "2",
          "time": "20210828 13:33"
        },
        {
          "id": "MDRW2100020860.1.3",
          "form": "요새 날씨가 선선하더라구요",
          "original_form": "요새 날씨가 선선하더라구요",
          "speaker_id": "1",
          "time": "20210828 13:33"
        },
        {
          "id": "MDRW2100020860.1.4",
          "form": "구러니까요",
          "original_form": "구러니까요",
          "speaker_id": "2",
          "time": "20210828 13:33"
        },
        {
          "id": "MDRW2100020860.1.5",
          "form": "가을인가봐요!",
          "original_form": "가을인가봐요!",
          "speaker_id": "1",
          "time": "20210828 13:34"
        },
        {
          "id": "MDRW2100020860.1.6",
          "form": "많이 선선햐졌어요",
          "original_form": "많이 선선햐졌어요",
          "speaker_id": "2",
          "time": "20210828 13:34"
        },
        {
          "id": "MDRW2100020860.1.7",
          "form": "가을 좋아하세요??",
          "original_form": "가을 좋아하세요??",
          "speaker_id": "2",
          "time": "20210828 13:34"
        },
        {
          "id": "MDRW2100020860.1.8",
          "form": "네 제일 좋아해요!",
          "original_form": "네 제일 좋아해요!",
          "speaker_id": "1",
          "time": "20210828 13:34"
        },
        {
          "id": "MDRW2100020860.1.9",
          "form": "어느계절 좋아하세요??",
          "original_form": "어느계절 좋아하세요??",
          "speaker_id": "1",
          "time": "20210828 13:34"
        },
        {
          "id": "MDRW2100020860.1.10",
          "form": "오 저도 가을이 젤 좋아요",
          "original_form": "오 저도 가을이 젤 좋아요",
          "speaker_id": "2",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100020860.1.11",
          "form": "같네요 ㅋㅋㅋ",
          "original_form": "같네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100020860.1.12",
          "form": "미세먼지도 적고",
          "original_form": "미세먼지도 적고",
          "speaker_id": "2",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100020860.1.13",
          "form": "오저두요",
          "original_form": "오저두요",
          "speaker_id": "1",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100020860.1.14",
          "form": "산책하기딱이라 좋아요",
          "original_form": "산책하기딱이라 좋아요",
          "speaker_id": "2",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100020860.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100020860.1.16",
          "form": "근데요샌 가을장마라더라구여?",
          "original_form": "근데요샌 가을장마라더라구여?",
          "speaker_id": "1",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100020860.1.17",
          "form": "산책 좋아하세요??",
          "original_form": "산책 좋아하세요??",
          "speaker_id": "2",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100020860.1.18",
          "form": "전엔 싫어했는데",
          "original_form": "전엔 싫어했는데",
          "speaker_id": "1",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100020860.1.19",
          "form": "아 요새 비 많이 오더라구요",
          "original_form": "아 요새 비 많이 오더라구요",
          "speaker_id": "2",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100020860.1.20",
          "form": "요샌 살려고 하려고 해욬ㅋㅋㅋ",
