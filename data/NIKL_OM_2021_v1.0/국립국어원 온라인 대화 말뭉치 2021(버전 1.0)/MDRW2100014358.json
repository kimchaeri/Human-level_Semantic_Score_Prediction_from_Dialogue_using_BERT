      "utterance": [
        {
          "id": "MDRW2100014358.1.1",
          "form": "어릴적 기억인데도",
          "original_form": "어릴적 기억인데도",
          "speaker_id": "2",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100014358.1.2",
          "form": "이층집 아주머니 아들 보내고",
          "original_form": "이층집 아주머니 아들 보내고",
          "speaker_id": "1",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100014358.1.3",
          "form": "정신이 가버림",
          "original_form": "정신이 가버림",
          "speaker_id": "1",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100014358.1.4",
          "form": "그 오빠 되게 공부잘하고 똑똑한 교회오빠 스타일이었음",
          "original_form": "그 오빠 되게 공부잘하고 똑똑한 교회오빠 스타일이었음",
          "speaker_id": "2",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100014358.1.5",
          "form": "나는 그거까지 기억남",
          "original_form": "나는 그거까지 기억남",
          "speaker_id": "2",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100014358.1.6",
          "form": "울 윗집오빠도",
          "original_form": "울 윗집오빠도",
          "speaker_id": "1",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100014358.1.7",
          "form": "똑똑한 유망주였음",
          "original_form": "똑똑한 유망주였음",
          "speaker_id": "1",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100014358.1.8",
          "form": "군대에서는 감나무였나 밤나무였나",
          "original_form": "군대에서는 감나무였나 밤나무였나",
          "speaker_id": "2",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100014358.1.9",
          "form": "나무에서 떨어졌다고했댔음",
          "original_form": "나무에서 떨어졌다고했댔음",
          "speaker_id": "2",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100014358.1.10",
          "form": "헐.. 소상히 알려주진않고?",
          "original_form": "헐.. 소상히 알려주진않고?",
          "speaker_id": "1",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100014358.1.11",
          "form": "ㅇㅇ그냥 작업하다 나무에서 떨어져서 죽었다고",
          "original_form": "ㅇㅇ그냥 작업하다 나무에서 떨어져서 죽었다고",
          "speaker_id": "2",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100014358.1.12",
          "form": "무섭당",
          "original_form": "무섭당",
          "speaker_id": "1",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100014358.1.13",
          "form": "울 윗층 오빠는 원인 모름",
          "original_form": "울 윗층 오빠는 원인 모름",
          "speaker_id": "1",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100014358.1.14",
          "form": "아마  이라던데",
          "original_form": "아마 {censored} 이라던데",
          "speaker_id": "1",
          "time": "20210825 18:39"
        },
        {
          "id": "MDRW2100014358.1.15",
          "form": "근데 도저히 부모가 납득할수 없었다고했음",
          "original_form": "근데 도저히 부모가 납득할수 없었다고했음",
          "speaker_id": "2",
          "time": "20210825 18:39"
        },
        {
          "id": "MDRW2100014358.1.16",
          "form": "안타까웠오",
          "original_form": "안타까웠오",
          "speaker_id": "1",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014358.1.17",
          "form": "울아주머니도 그럼",
          "original_form": "울아주머니도 그럼",
          "speaker_id": "1",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014358.1.18",
          "form": "시신의 상태랑 전혀 안맞았다고",
          "original_form": "시신의 상태랑 전혀 안맞았다고",
          "speaker_id": "2",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014358.1.19",
          "form": "어두운추억임 어린시절의 ㅜ",
          "original_form": "어두운추억임 어린시절의 ㅜ",
          "speaker_id": "1",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014358.1.20",
          "form": "그런거도 갈켜주드나",
          "original_form": "그런거도 갈켜주드나",
          "speaker_id": "1",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014358.1.21",
          "form": "애한테",
          "original_form": "애한테",
          "speaker_id": "1",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014358.1.22",
          "form": "아 듣기는 크고나서들었음",
          "original_form": "아 듣기는 크고나서들었음",
          "speaker_id": "2",
          "time": "20210825 18:41"
        },
        {
          "id": "MDRW2100014358.1.23",
          "form": "왜냐면 그래서 그분들이 계속 시신을 안가져왔었었음",
          "original_form": "왜냐면 그래서 그분들이 계속 시신을 안가져왔었었음",
          "speaker_id": "2",
