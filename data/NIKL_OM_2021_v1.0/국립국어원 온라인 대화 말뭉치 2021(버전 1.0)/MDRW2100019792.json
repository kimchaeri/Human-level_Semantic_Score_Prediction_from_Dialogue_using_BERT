      "utterance": [
        {
          "id": "MDRW2100019792.1.1",
          "form": "다이어트하는데 야식 먹고 있어요",
          "original_form": "다이어트하는데 야식 먹고 있어요",
          "speaker_id": "1",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100019792.1.2",
          "form": "저녁은 든든하게 드셧구여?",
          "original_form": "저녁은 든든하게 드셧구여?",
          "speaker_id": "2",
          "time": "20210827 22:04"
        },
        {
          "id": "MDRW2100019792.1.3",
          "form": "넵! 닭갈비 든든히 먹고",
          "original_form": "넵! 닭갈비 든든히 먹고",
          "speaker_id": "1",
          "time": "20210827 22:04"
        },
        {
          "id": "MDRW2100019792.1.4",
          "form": "빙수 먹고 있어요",
          "original_form": "빙수 먹고 있어요",
          "speaker_id": "1",
          "time": "20210827 22:04"
        },
        {
          "id": "MDRW2100019792.1.5",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210827 22:04"
        },
        {
          "id": "MDRW2100019792.1.6",
          "form": "아.. 그리고 야식이여? ㅋㅋㅋㅋ",
          "original_form": "아.. 그리고 야식이여? ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100019792.1.7",
          "form": "전 샐러드랑.. 과자요.. ㅋㅋㅋ",
          "original_form": "전 샐러드랑.. 과자요.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100019792.1.8",
          "form": "네ㅋㅋ",
          "original_form": "네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100019792.1.9",
          "form": "다이어트와 멀어지는 하루에요..ㅋㅋ",
          "original_form": "다이어트와 멀어지는 하루에요..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100019792.1.10",
          "form": "앗 샐러드",
          "original_form": "앗 샐러드",
          "speaker_id": "1",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100019792.1.11",
          "form": "요즘은 샐러드도 잘나오더라구요",
          "original_form": "요즘은 샐러드도 잘나오더라구요",
          "speaker_id": "1",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100019792.1.12",
          "form": "딱 한끼씩 나오는거 편해보이더라구요..",
          "original_form": "딱 한끼씩 나오는거 편해보이더라구요..",
          "speaker_id": "2",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019792.1.13",
          "form": "근데 가격이.. 그 돈이면 고기사먹고 말지.. 이런..ㅋㅋㅋ",
          "original_form": "근데 가격이.. 그 돈이면 고기사먹고 말지.. 이런..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019792.1.14",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:30"
        },
        {
          "id": "MDRW2100019792.1.15",
          "form": "생각보다 꽤 나가더라구요",
          "original_form": "생각보다 꽤 나가더라구요",
          "speaker_id": "1",
          "time": "20210827 22:30"
        },
        {
          "id": "MDRW2100019792.1.16",
          "form": "전 1주잉에.한번씩 준비해놓고  저녁때 스까 먹어요.. ㅋㅋ",
          "original_form": "전 1주잉에.한번씩 준비해놓고  저녁때 스까 먹어요.. ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019792.1.17",
          "form": "우왕 대단해요",
          "original_form": "우왕 대단해요",
          "speaker_id": "1",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100019792.1.18",
          "form": "쉽지않우데",
          "original_form": "쉽지않우데",
          "speaker_id": "1",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100019792.1.19",
          "form": "자취하니까... 밥해먹는거보가 간편해서요..",
          "original_form": "자취하니까... 밥해먹는거보가 간편해서요..",
          "speaker_id": "2",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019792.1.20",
          "form": "오히려 시간절약되요..",
          "original_form": "오히려 시간절약되요..",
          "speaker_id": "2",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019792.1.21",
          "form": "아ㅠ 그건 그래요",
          "original_form": "아ㅠ 그건 그래요",
          "speaker_id": "1",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100019792.1.22",
          "form": "저는 밥 일주일치 얼려놔요ㅋㅋ",
          "original_form": "저는 밥 일주일치 얼려놔요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100019792.1.23",
          "form": "저도 예전에 글케했는데.. \r\n반찬 준비가..",
          "original_form": "저도 예전에 글케했는데.. \r\n반찬 준비가..",
          "speaker_id": "2",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100019792.1.24",
          "form": "반찬은 어케하세요?",
          "original_form": "반찬은 어케하세요?",
          "speaker_id": "2",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100019792.1.25",
          "form": "계란후라이나 볶음밥을 자주 먹어욥",
          "original_form": "계란후라이나 볶음밥을 자주 먹어욥",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100019792.1.26",
          "form": "냉동야채로요!",
          "original_form": "냉동야채로요!",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100019792.1.27",
          "form": "냉동 야채란게 있어요?",
          "original_form": "냉동 야채란게 있어요?",
          "speaker_id": "2",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100019792.1.28",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210827 23:38"
        }
      ]
    }
