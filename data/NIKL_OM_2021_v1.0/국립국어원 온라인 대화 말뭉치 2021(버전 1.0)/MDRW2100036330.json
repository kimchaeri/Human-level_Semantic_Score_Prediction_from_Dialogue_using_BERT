      "utterance": [
        {
          "id": "MDRW2100036330.1.1",
          "form": "한순간 감정 조절을 못해서리ㅜ.ㅜ",
          "original_form": "한순간 감정 조절을 못해서리ㅜ.ㅜ",
          "speaker_id": "2",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100036330.1.2",
          "form": "승강기 공사 중 추락해서 사망ㅠ",
          "original_form": "승강기 공사 중 추락해서 사망ㅠ",
          "speaker_id": "1",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100036330.1.3",
          "form": "에궁 맨날 사고로 사망자 발생",
          "original_form": "에궁 맨날 사고로 사망자 발생",
          "speaker_id": "2",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100036330.1.4",
          "form": "좋은 뉴스가 하나도 안나옴",
          "original_form": "좋은 뉴스가 하나도 안나옴",
          "speaker_id": "1",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100036330.1.5",
          "form": "그래 좋은 소식이 나온지 언젠지 모르겠다",
          "original_form": "그래 좋은 소식이 나온지 언젠지 모르겠다",
          "speaker_id": "2",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100036330.1.6",
          "form": "코로나가 빨리 없어져야 되는데",
          "original_form": "코로나가 빨리 없어져야 되는데",
          "speaker_id": "1",
          "time": "20210830 21:33"
        },
        {
          "id": "MDRW2100036330.1.7",
          "form": "치료제 없인 안 없어질듯",
          "original_form": "치료제 없인 안 없어질듯",
          "speaker_id": "2",
          "time": "20210830 21:33"
        },
        {
          "id": "MDRW2100036330.1.8",
          "form": "먹는 치료제 빨리 나와야 함",
          "original_form": "먹는 치료제 빨리 나와야 함",
          "speaker_id": "1",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100036330.1.9",
          "form": "연구는 하고 있는줄 모르겠다",
          "original_form": "연구는 하고 있는줄 모르겠다",
          "speaker_id": "2",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100036330.1.10",
          "form": "다른 전염병 보다 더 오래가고 있다",
          "original_form": "다른 전염병 보다 더 오래가고 있다",
          "speaker_id": "1",
          "time": "20210830 21:34"
        },
        {
          "id": "MDRW2100036330.1.11",
          "form": "음모론 있는거 아님?",
          "original_form": "음모론 있는거 아님?",
          "speaker_id": "1",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100036330.1.12",
          "form": "그렇다고 하는데 모르지",
          "original_form": "그렇다고 하는데 모르지",
          "speaker_id": "2",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100036330.1.13",
          "form": "연가시처럼 제약회사에 퍼트린거 아닌가 싶기도",
          "original_form": "연가시처럼 제약회사에 퍼트린거 아닌가 싶기도",
          "speaker_id": "1",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100036330.1.14",
          "form": "일단 중국이 퍼트린거지",
          "original_form": "일단 중국이 퍼트린거지",
