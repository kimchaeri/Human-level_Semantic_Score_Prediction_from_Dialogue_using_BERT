      "utterance": [
        {
          "id": "MDRW2100023775.1.1",
          "form": "탄산음료는 못끊어욤",
          "original_form": "탄산음료는 못끊어욤",
          "speaker_id": "2",
          "time": "20210830 17:10"
        },
        {
          "id": "MDRW2100023775.1.2",
          "form": "저는 탄산음료는 잘 안마셔요",
          "original_form": "저는 탄산음료는 잘 안마셔요",
          "speaker_id": "1",
          "time": "20210830 17:12"
        },
        {
          "id": "MDRW2100023775.1.3",
          "form": "직장 점심시간때 배달 음식 매일 시켜먹어요",
          "original_form": "직장 점심시간때 배달 음식 매일 시켜먹어요",
          "speaker_id": "2",
          "time": "20210830 18:01"
        },
        {
          "id": "MDRW2100023775.1.4",
          "form": "많이드시면 살쪄요",
          "original_form": "많이드시면 살쪄요",
          "speaker_id": "1",
          "time": "20210830 18:02"
        },
        {
          "id": "MDRW2100023775.1.5",
          "form": "이빨도다썪음",
          "original_form": "이빨도다썪음",
          "speaker_id": "1",
          "time": "20210830 18:02"
        },
        {
          "id": "MDRW2100023775.1.6",
          "form": "ㅠㅠ그래서 살이 안빠지나봐요",
          "original_form": "ㅠㅠ그래서 살이 안빠지나봐요",
          "speaker_id": "2",
          "time": "20210830 21:10"
        },
        {
          "id": "MDRW2100023775.1.7",
          "form": "유지만혀",
          "original_form": "유지만혀",
          "speaker_id": "1",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100023775.1.8",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100023775.1.9",
          "form": "코로나 끝나면 애들데리고 맛집가야겠어요ㅋㅋ",
          "original_form": "코로나 끝나면 애들데리고 맛집가야겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:34"
        },
        {
          "id": "MDRW2100023775.1.10",
          "form": "뭐 드시고싶으세요?",
          "original_form": "뭐 드시고싶으세요?",
          "speaker_id": "1",
          "time": "20210831 20:35"
        },
        {
          "id": "MDRW2100023775.1.11",
          "form": "여기저기 유명 맛집탐방이여ㅋㅋ",
          "original_form": "여기저기 유명 맛집탐방이여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:50"
        },
        {
          "id": "MDRW2100023775.1.12",
          "form": "그건 언제라도 말씀하세요",
          "original_form": "그건 언제라도 말씀하세요",
          "speaker_id": "1",
          "time": "20210831 20:54"
        },
        {
          "id": "MDRW2100023775.1.13",
          "form": "잠잠해지면 가욤ㅎㅎ",
          "original_form": "잠잠해지면 가욤ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:06"
        },
        {
          "id": "MDRW2100023775.1.14",
          "form": "뭐드시고싶은거잇나용",
          "original_form": "뭐드시고싶은거잇나용",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100023775.1.15",
          "form": "제주도 연돈 먹어보고싶어요ㅋ 백종원 극찬 돈까스집",
          "original_form": "제주도 연돈 먹어보고싶어요ㅋ 백종원 극찬 돈까스집",
          "speaker_id": "2",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100023775.1.16",
          "form": "그거 가성비 별로래여",
          "original_form": "그거 가성비 별로래여",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100023775.1.17",
          "form": "기다릴만하지않다고",
          "original_form": "기다릴만하지않다고",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
