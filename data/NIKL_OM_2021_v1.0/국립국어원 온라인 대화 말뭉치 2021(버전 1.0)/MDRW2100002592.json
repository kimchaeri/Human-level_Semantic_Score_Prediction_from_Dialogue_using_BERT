      "utterance": [
        {
          "id": "MDRW2100002592.1.1",
          "form": "누나 동물 키워?",
          "original_form": "누나 동물 키워?",
          "speaker_id": "1",
          "time": "20210820 13:49"
        },
        {
          "id": "MDRW2100002592.1.2",
          "form": "누나 반려동물키워?",
          "original_form": "누나 반려동물키워?",
          "speaker_id": "1",
          "time": "20210820 13:50"
        },
        {
          "id": "MDRW2100002592.1.3",
          "form": "안키워 한달뒤에 고양이가 올예정",
          "original_form": "안키워 한달뒤에 고양이가 올예정",
          "speaker_id": "2",
          "time": "20210820 13:50"
        },
        {
          "id": "MDRW2100002592.1.4",
          "form": "오! 종류 뭐야?",
          "original_form": "오! 종류 뭐야?",
          "speaker_id": "1",
          "time": "20210820 13:50"
        },
        {
          "id": "MDRW2100002592.1.5",
          "form": "종류는몰르겠어 아는동생이 고양이 키워서 몇마리 아기생겨서 한마리주는거라서",
          "original_form": "종류는몰르겠어 아는동생이 고양이 키워서 몇마리 아기생겨서 한마리주는거라서",
          "speaker_id": "2",
          "time": "20210820 15:17"
        },
        {
          "id": "MDRW2100002592.1.6",
          "form": "오..분양받는구나",
          "original_form": "오..분양받는구나",
          "speaker_id": "1",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002592.1.7",
          "form": "귀엽겠다 ㅠㅠ",
          "original_form": "귀엽겠다 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002592.1.8",
          "form": "한달뒤에 오묜",
          "original_form": "한달뒤에 오묜",
          "speaker_id": "1",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002592.1.9",
          "form": "사진 하나 보내주는거 알지?",
          "original_form": "사진 하나 보내주는거 알지?",
          "speaker_id": "1",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002592.1.10",
          "form": "기대할께~~",
          "original_form": "기대할께~~",
          "speaker_id": "1",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002592.1.11",
          "form": "당연히 사진으로 고양이보여주지~~",
          "original_form": "당연히 사진으로 고양이보여주지~~",
          "speaker_id": "2",
          "time": "20210820 21:35"
        },
        {
          "id": "MDRW2100002592.1.12",
          "form": "크으 역시",
          "original_form": "크으 역시",
          "speaker_id": "1",
          "time": "20210820 21:36"
        },
        {
          "id": "MDRW2100002592.1.13",
          "form": "고양이 사진 보는거 힐링이야",
          "original_form": "고양이 사진 보는거 힐링이야",
          "speaker_id": "1",
          "time": "20210820 21:36"
        },
        {
          "id": "MDRW2100002592.1.14",
          "form": "보는것도 좋은데",
          "original_form": "보는것도 좋은데",
          "speaker_id": "1",
          "time": "20210820 21:36"
        },
        {
          "id": "MDRW2100002592.1.15",
          "form": "마자.. 너무귀여워",
          "original_form": "마자.. 너무귀여워",
          "speaker_id": "2",
          "time": "20210820 21:36"
        },
        {
          "id": "MDRW2100002592.1.16",
          "form": "그치그치 ㅠㅠ",
          "original_form": "그치그치 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 21:36"
        },
        {
          "id": "MDRW2100002592.1.17",
          "form": "음청 귀여워",
          "original_form": "음청 귀여워",
          "speaker_id": "1",
          "time": "20210820 21:36"
        },
        {
          "id": "MDRW2100002592.1.18",
          "form": "고양이 나중에 키우면",
          "original_form": "고양이 나중에 키우면",
          "speaker_id": "1",
          "time": "20210820 21:36"
        },
        {
          "id": "MDRW2100002592.1.19",
          "form": "음청귀엽다",
          "original_form": "음청귀엽다",
          "speaker_id": "2",
          "time": "20210820 21:37"
        },
        {
          "id": "MDRW2100002592.1.20",
          "form": "꼭 그건 준비해줘",
          "original_form": "꼭 그건 준비해줘",
          "speaker_id": "1",
          "time": "20210820 21:37"
        },
        {
          "id": "MDRW2100002592.1.21",
          "form": "브러쉬?같은거",
          "original_form": "브러쉬?같은거",
          "speaker_id": "1",
          "time": "20210820 21:37"
        },
        {
          "id": "MDRW2100002592.1.22",
          "form": "브러쉬?",
          "original_form": "브러쉬?",
          "speaker_id": "2",
          "time": "20210820 21:37"
        },
        {
          "id": "MDRW2100002592.1.23",
          "form": "그리고 중성화도 빠르게 해주고",
          "original_form": "그리고 중성화도 빠르게 해주고",
          "speaker_id": "1",
          "time": "20210820 21:37"
        },
        {
          "id": "MDRW2100002592.1.24",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210820 21:37"
        },
        {
          "id": "MDRW2100002592.1.25",
          "form": "털갈이할때",
          "original_form": "털갈이할때",
          "speaker_id": "1",
          "time": "20210820 21:37"
        },
        {
          "id": "MDRW2100002592.1.26",
          "form": "고양이전용 빗?",
          "original_form": "고양이전용 빗?",
          "speaker_id": "2",
          "time": "20210820 21:37"
        },
        {
          "id": "MDRW2100002592.1.27",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210820 21:37"
        },
        {
          "id": "MDRW2100002592.1.28",
          "form": "그거 잘보묜 시중에 가성비 좋은거 많어",
          "original_form": "그거 잘보묜 시중에 가성비 좋은거 많어",
          "speaker_id": "1",
          "time": "20210820 21:37"
        },
        {
          "id": "MDRW2100002592.1.29",
          "form": "와집가고있는대",
          "original_form": "와집가고있는대",
          "speaker_id": "2",
          "time": "20210820 21:37"
        },
        {
          "id": "MDRW2100002592.1.30",
          "form": "아기강아지분양하는곳있어",
          "original_form": "아기강아지분양하는곳있어",
          "speaker_id": "2",
          "time": "20210820 21:38"
        },
        {
          "id": "MDRW2100002592.1.31",
          "form": "댜박귀여워",
          "original_form": "댜박귀여워",
          "speaker_id": "2",
          "time": "20210820 21:38"
        },
        {
          "id": "MDRW2100002592.1.32",
          "form": "아기강아지들 참 귀엽긴하지 ㅠㅠ",
          "original_form": "아기강아지들 참 귀엽긴하지 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 21:38"
        },
        {
          "id": "MDRW2100002592.1.33",
          "form": "아기강아지가 지금 뒹굴거려",
          "original_form": "아기강아지가 지금 뒹굴거려",
          "speaker_id": "2",
          "time": "20210820 21:38"
        },
        {
          "id": "MDRW2100002592.1.34",
          "form": "애교부리나보네 ㅠㅠㅠ귀여워라",
          "original_form": "애교부리나보네 ㅠㅠㅠ귀여워라",
          "speaker_id": "1",
          "time": "20210820 21:38"
        },
        {
          "id": "MDRW2100002592.1.35",
          "form": "바닥에",
          "original_form": "바닥에",
          "speaker_id": "2",
          "time": "20210820 21:39"
        },
        {
          "id": "MDRW2100002592.1.36",
          "form": "깔려있는",
          "original_form": "깔려있는",
          "speaker_id": "2",
          "time": "20210820 21:39"
        },
        {
          "id": "MDRW2100002592.1.37",
          "form": "이불을막 물어뜯고",
          "original_form": "이불을막 물어뜯고",
          "speaker_id": "2",
          "time": "20210820 21:39"
        },
        {
          "id": "MDRW2100002592.1.38",
          "form": "애기네 증말",
          "original_form": "애기네 증말",
          "speaker_id": "1",
          "time": "20210820 21:39"
        },
        {
          "id": "MDRW2100002592.1.39",
          "form": "마자애기야..",
          "original_form": "마자애기야..",
          "speaker_id": "2",
          "time": "20210820 21:40"
        }
      ]
