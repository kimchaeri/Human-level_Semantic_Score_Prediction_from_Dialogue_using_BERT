      "utterance": [
        {
          "id": "MDRW2100010363.1.1",
          "form": "니가 싸이월드를 알아?",
          "original_form": "니가 싸이월드를 알아?",
          "speaker_id": "2",
          "time": "20210823 17:12"
        },
        {
          "id": "MDRW2100010363.1.2",
          "form": "그 세댄가?",
          "original_form": "그 세댄가?",
          "speaker_id": "2",
          "time": "20210823 17:12"
        },
        {
          "id": "MDRW2100010363.1.3",
          "form": "참나 당연하지",
          "original_form": "참나 당연하지",
          "speaker_id": "1",
          "time": "20210823 17:12"
        },
        {
          "id": "MDRW2100010363.1.4",
          "form": "아구래?",
          "original_form": "아구래?",
          "speaker_id": "2",
          "time": "20210823 17:12"
        },
        {
          "id": "MDRW2100010363.1.5",
          "form": "애기라서 모를줄",
          "original_form": "애기라서 모를줄",
          "speaker_id": "2",
          "time": "20210823 17:12"
        },
        {
          "id": "MDRW2100010363.1.6",
          "form": "소싯적 다이어리 좀 꾸며봤다고",
          "original_form": "소싯적 다이어리 좀 꾸며봤다고",
          "speaker_id": "1",
          "time": "20210823 17:13"
        },
        {
          "id": "MDRW2100010363.1.7",
          "form": "제발 내 싸이는 조용히 사라졌으면 좋겠다",
          "original_form": "제발 내 싸이는 조용히 사라졌으면 좋겠다",
          "speaker_id": "2",
          "time": "20210823 17:13"
        },
        {
          "id": "MDRW2100010363.1.8",
          "form": "들어가봤니",
          "original_form": "들어가봤니",
          "speaker_id": "1",
          "time": "20210823 17:13"
        },
        {
          "id": "MDRW2100010363.1.9",
          "form": "들어갈수있어?",
          "original_form": "들어갈수있어?",
          "speaker_id": "2",
          "time": "20210823 17:13"
        },
        {
          "id": "MDRW2100010363.1.10",
          "form": "아이디이런거하나도생각안나",
          "original_form": "아이디이런거하나도생각안나",
          "speaker_id": "2",
          "time": "20210823 17:13"
        },
        {
          "id": "MDRW2100010363.1.11",
          "form": "들어갈 수 있을걸",
          "original_form": "들어갈 수 있을걸",
          "speaker_id": "1",
          "time": "20210823 17:34"
        },
        {
          "id": "MDRW2100010363.1.12",
          "form": "그래서 난 다 삭제하고 왔어",
          "original_form": "그래서 난 다 삭제하고 왔어",
          "speaker_id": "1",
          "time": "20210823 17:34"
        },
        {
          "id": "MDRW2100010363.1.13",
          "form": "부활해서 도토리 환불도 된대",
          "original_form": "부활해서 도토리 환불도 된대",
          "speaker_id": "1",
          "time": "20210823 17:34"
        },
        {
          "id": "MDRW2100010363.1.14",
          "form": "들어가봐야지",
          "original_form": "들어가봐야지",
          "speaker_id": "2",
          "time": "20210823 17:34"
        },
        {
          "id": "MDRW2100010363.1.15",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210823 17:35"
        },
        {
          "id": "MDRW2100010363.1.16",
          "form": "배경음악열심히샀었는데",
          "original_form": "배경음악열심히샀었는데",
          "speaker_id": "2",
          "time": "20210823 17:35"
        },
        {
          "id": "MDRW2100010363.1.17",
          "form": "싸이월드아이디를넣으라니..모르는데!",
          "original_form": "싸이월드아이디를넣으라니..모르는데!",
          "speaker_id": "2",
          "time": "20210823 17:36"
        },
        {
          "id": "MDRW2100010363.1.18",
          "form": "왠지찜찜해서안하기로",
          "original_form": "왠지찜찜해서안하기로",
          "speaker_id": "2",
          "time": "20210823 17:37"
        },
        {
          "id": "MDRW2100010363.1.19",
          "form": "비밀번호찾기는 별로니",
          "original_form": "비밀번호찾기는 별로니",
          "speaker_id": "1",
          "time": "20210824 15:53"
        },
        {
          "id": "MDRW2100010363.1.20",
          "form": "그냥 안하려구",
          "original_form": "그냥 안하려구",
          "speaker_id": "2",
          "time": "20210824 15:55"
        },
        {
          "id": "MDRW2100010363.1.21",
          "form": "부활왜하는지 모르겠다!",
          "original_form": "부활왜하는지 모르겠다!",
          "speaker_id": "2",
          "time": "20210824 15:55"
        },
        {
          "id": "MDRW2100010363.1.22",
          "form": "싸이에 너무 잊고싶은 사진뿐이야ㅋㅋ",
          "original_form": "싸이에 너무 잊고싶은 사진뿐이야ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:55"
        },
        {
          "id": "MDRW2100010363.1.23",
          "form": "없애려고",
          "original_form": "없애려고",
          "speaker_id": "1",
          "time": "20210824 15:55"
        },
        {
          "id": "MDRW2100010363.1.24",
          "form": "깔깔..",
          "original_form": "깔깔..",
          "speaker_id": "2",
          "time": "20210824 15:55"
        },
        {
          "id": "MDRW2100010363.1.25",
          "form": "너 뭔가 도토리 많이 남아있을것같아",
          "original_form": "너 뭔가 도토리 많이 남아있을것같아",
          "speaker_id": "1",
          "time": "20210824 15:55"
        },
        {
          "id": "MDRW2100010363.1.26",
          "form": "아마 살뜰하게 써서 없을걸",
          "original_form": "아마 살뜰하게 써서 없을걸",
          "speaker_id": "2",
          "time": "20210824 15:56"
        },
        {
          "id": "MDRW2100010363.1.27",
          "form": "배경음악밖에 안샀었음",
          "original_form": "배경음악밖에 안샀었음",
          "speaker_id": "2",
          "time": "20210824 15:56"
        },
        {
          "id": "MDRW2100010363.1.28",
          "form": "그렇군",
          "original_form": "그렇군",
          "speaker_id": "1",
          "time": "20210824 15:57"
        },
        {
          "id": "MDRW2100010363.1.29",
          "form": "난 맨날 구걸했어",
          "original_form": "난 맨날 구걸했어",
          "speaker_id": "1",
          "time": "20210824 15:57"
        }
      ]
    }
  ]
