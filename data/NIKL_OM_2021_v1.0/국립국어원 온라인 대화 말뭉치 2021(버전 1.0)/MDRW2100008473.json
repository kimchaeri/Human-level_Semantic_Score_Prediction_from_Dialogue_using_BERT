      "utterance": [
        {
          "id": "MDRW2100008473.1.1",
          "form": "배달음식 자주 시키시나요?",
          "original_form": "배달음식 자주 시키시나요?",
          "speaker_id": "2",
          "time": "20210822 16:03"
        },
        {
          "id": "MDRW2100008473.1.2",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100008473.1.3",
          "form": "치킨 피자 햄버거",
          "original_form": "치킨 피자 햄버거",
          "speaker_id": "1",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100008473.1.4",
          "form": "요새 넘 많이 시켰어요",
          "original_form": "요새 넘 많이 시켰어요",
          "speaker_id": "1",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100008473.1.5",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "2",
          "time": "20210822 16:05"
        },
        {
          "id": "MDRW2100008473.1.6",
          "form": "저는 요즘은 잘  안시켜요 ㅎ",
          "original_form": "저는 요즘은 잘  안시켜요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100008473.1.7",
          "form": "오 바르세요",
          "original_form": "오 바르세요",
          "speaker_id": "1",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100008473.1.8",
          "form": "그래야 하는데",
          "original_form": "그래야 하는데",
          "speaker_id": "1",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100008473.1.9",
          "form": "자꾸 귀찮아서 시켰네요",
          "original_form": "자꾸 귀찮아서 시켰네요",
          "speaker_id": "1",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100008473.1.10",
          "form": "치킨 족발 메뉴면 배달부르는데",
          "original_form": "치킨 족발 메뉴면 배달부르는데",
          "speaker_id": "2",
          "time": "20210822 16:06"
        },
        {
          "id": "MDRW2100008473.1.11",
          "form": "자주먹는음식이 아니니 안하게되더라구요",
          "original_form": "자주먹는음식이 아니니 안하게되더라구요",
          "speaker_id": "2",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100008473.1.12",
          "form": "어 그렇군요",
          "original_form": "어 그렇군요",
          "speaker_id": "1",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100008473.1.13",
          "form": "어제는 친정에서 호박잎에 된장끓여주셔서 그걸로 저녁먹었거든요",
          "original_form": "어제는 친정에서 호박잎에 된장끓여주셔서 그걸로 저녁먹었거든요",
          "speaker_id": "2",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100008473.1.14",
          "form": "호박잎이 왜이렇게 단지.., ㅎ\r\n정말 맛있더라구요",
          "original_form": "호박잎이 왜이렇게 단지.., ㅎ\r\n정말 맛있더라구요",
          "speaker_id": "2",
          "time": "20210822 16:07"
        },
        {
          "id": "MDRW2100008473.1.15",
          "form": "아 어릴때 진짜ㅜ많이 먹었어요",
          "original_form": "아 어릴때 진짜ㅜ많이 먹었어요",
          "speaker_id": "1",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100008473.1.16",
          "form": "넘맛나요",
          "original_form": "넘맛나요",
          "speaker_id": "1",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100008473.1.17",
          "form": "아 부럽당",
          "original_form": "아 부럽당",
          "speaker_id": "1",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100008473.1.18",
          "form": "먹고싶네요",
          "original_form": "먹고싶네요",
          "speaker_id": "1",
          "time": "20210822 16:08"
        },
        {
          "id": "MDRW2100008473.1.19",
          "form": "어릴때는 안먹는 메뉴아닌가요?ㅎ",
          "original_form": "어릴때는 안먹는 메뉴아닌가요?ㅎ",
          "speaker_id": "2",
          "time": "20210822 16:09"
        },
        {
          "id": "MDRW2100008473.1.20",
          "form": "저는 어릴땐 안먹었던거같아요 ㅎ",
          "original_form": "저는 어릴땐 안먹었던거같아요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 16:09"
        },
        {
          "id": "MDRW2100008473.1.21",
          "form": "이제 나이가 들었는지 이런게 입에 맞기시작하네요",
          "original_form": "이제 나이가 들었는지 이런게 입에 맞기시작하네요",
          "speaker_id": "2",
          "time": "20210822 16:09"
        },
        {
          "id": "MDRW2100008473.1.22",
          "form": "저도 좋아하는데 게을러서 안했네요 ㅜㅜ",
          "original_form": "저도 좋아하는데 게을러서 안했네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008473.1.23",
          "form": "엄마밥 막고싶어요",
          "original_form": "엄마밥 막고싶어요",
          "speaker_id": "1",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008473.1.24",
          "form": "은근 하기 귀찮은 메뉴죠",
          "original_form": "은근 하기 귀찮은 메뉴죠",
          "speaker_id": "2",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008473.1.25",
          "form": "아 게장 먹고싶네요",
          "original_form": "아 게장 먹고싶네요",
          "speaker_id": "1",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008473.1.26",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008473.1.27",
          "form": "엄마가 게장을 잘만드세요 ㅎㅎ",
          "original_form": "엄마가 게장을 잘만드세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008473.1.28",
          "form": "게장도 맛있죠 ㅎㅎ",
          "original_form": "게장도 맛있죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008473.1.29",
          "form": "김치도 맛나고요",
          "original_form": "김치도 맛나고요",
          "speaker_id": "1",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008473.1.30",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008473.1.31",
          "form": "간장게장? 양념게장?",
          "original_form": "간장게장? 양념게장?",
          "speaker_id": "2",
          "time": "20210822 16:11"
        },
        {
          "id": "MDRW2100008473.1.32",
          "form": "저도 요샌 김치는 담가요",
          "original_form": "저도 요샌 김치는 담가요",
          "speaker_id": "1",
          "time": "20210822 16:12"
        },
        {
          "id": "MDRW2100008473.1.33",
          "form": "어느쪽이세요?",
          "original_form": "어느쪽이세요?",
          "speaker_id": "2",
          "time": "20210822 16:12"
        },
        {
          "id": "MDRW2100008473.1.34",
          "form": "간장요",
          "original_form": "간장요",
          "speaker_id": "1",
          "time": "20210822 16:12"
        },
        {
          "id": "MDRW2100008473.1.35",
          "form": "저도 간장이요 ㅎ",
          "original_form": "저도 간장이요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 16:12"
        },
        {
          "id": "MDRW2100008473.1.36",
          "form": "밥ㄱ쓱 싹 비벼요",
          "original_form": "밥ㄱ쓱 싹 비벼요",
          "speaker_id": "1",
          "time": "20210822 16:12"
        }
      ]
    }
