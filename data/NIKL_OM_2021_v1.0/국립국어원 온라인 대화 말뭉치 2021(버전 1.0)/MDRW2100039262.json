      "utterance": [
        {
          "id": "MDRW2100039262.1.1",
          "form": "우와 카페모카",
          "original_form": "우와 카페모카",
          "speaker_id": "2",
          "time": "20210903 15:34"
        },
        {
          "id": "MDRW2100039262.1.2",
          "form": "저는 귀찮아서 누워있어요",
          "original_form": "저는 귀찮아서 누워있어요",
          "speaker_id": "2",
          "time": "20210903 15:34"
        },
        {
          "id": "MDRW2100039262.1.3",
          "form": "아아마시고싶은뎅",
          "original_form": "아아마시고싶은뎅",
          "speaker_id": "2",
          "time": "20210903 15:34"
        },
        {
          "id": "MDRW2100039262.1.4",
          "form": "어디꺼가 맛있을까요?",
          "original_form": "어디꺼가 맛있을까요?",
          "speaker_id": "1",
          "time": "20210903 15:44"
        },
        {
          "id": "MDRW2100039262.1.5",
          "form": "카누?",
          "original_form": "카누?",
          "speaker_id": "1",
          "time": "20210903 15:44"
        },
        {
          "id": "MDRW2100039262.1.6",
          "form": "카페가서 마시고파용 ㅋㅋㅋ",
          "original_form": "카페가서 마시고파용 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:04"
        },
        {
          "id": "MDRW2100039262.1.7",
          "form": "아하 ㅋㅋㅋ 스벅 쿠폰하나 있는데 혹시 맛난거 아시나요",
          "original_form": "아하 ㅋㅋㅋ 스벅 쿠폰하나 있는데 혹시 맛난거 아시나요",
          "speaker_id": "1",
          "time": "20210903 16:07"
        },
        {
          "id": "MDRW2100039262.1.8",
          "form": "와와 저도있는데",
          "original_form": "와와 저도있는데",
          "speaker_id": "2",
          "time": "20210903 16:12"
        },
        {
          "id": "MDRW2100039262.1.9",
          "form": "저는 망고바나나좋아해요",
          "original_form": "저는 망고바나나좋아해요",
          "speaker_id": "2",
          "time": "20210903 16:12"
        },
        {
          "id": "MDRW2100039262.1.10",
          "form": "ㅠㅠㅠ 그거는 안된데요",
          "original_form": "ㅠㅠㅠ 그거는 안된데요",
          "speaker_id": "1",
          "time": "20210903 16:16"
        },
        {
          "id": "MDRW2100039262.1.11",
          "form": "바나나가 들어가서...",
          "original_form": "바나나가 들어가서...",
          "speaker_id": "1",
          "time": "20210903 16:17"
        },
        {
          "id": "MDRW2100039262.1.12",
          "form": "바나나 들어가면 안된대요?",
          "original_form": "바나나 들어가면 안된대요?",
          "speaker_id": "2",
          "time": "20210903 21:49"
        },
        {
          "id": "MDRW2100039262.1.13",
          "form": "마자요 그래서 피치어쩌고 먹었어요",
          "original_form": "마자요 그래서 피치어쩌고 먹었어요",
          "speaker_id": "1",
          "time": "20210903 21:51"
        },
        {
          "id": "MDRW2100039262.1.14",
          "form": "오 그거도 베스트 음료 아닌가여",
          "original_form": "오 그거도 베스트 음료 아닌가여",
          "speaker_id": "2",
          "time": "20210903 22:02"
        },
        {
          "id": "MDRW2100039262.1.15",
          "form": "맛이괜찮더라구요 ㅎ 젤리가 있었어요",
          "original_form": "맛이괜찮더라구요 ㅎ 젤리가 있었어요",
          "speaker_id": "1",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100039262.1.16",
          "form": "오 저도 그거 먹어본거같아여",
          "original_form": "오 저도 그거 먹어본거같아여",
          "speaker_id": "2",
          "time": "20210903 22:05"
        },
        {
          "id": "MDRW2100039262.1.17",
          "form": "저녁은 뭐드셨나요",
          "original_form": "저녁은 뭐드셨나요",
          "speaker_id": "2",
          "time": "20210903 22:05"
        },
        {
          "id": "MDRW2100039262.1.18",
          "form": "땅땅치킨 먹었어요 님은요?",
          "original_form": "땅땅치킨 먹었어요 님은요?",
          "speaker_id": "1",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039262.1.19",
          "form": "전 낮에먹은 감자탕과 복슝아요",
          "original_form": "전 낮에먹은 감자탕과 복슝아요",
          "speaker_id": "2",
          "time": "20210903 22:11"
        }
      ]
