      "utterance": [
        {
          "id": "MDRW2100008488.1.1",
          "form": "헐 저 미술전시 너무 보고싶습니다...",
          "original_form": "헐 저 미술전시 너무 보고싶습니다...",
          "speaker_id": "2",
          "time": "20210822 17:00"
        },
        {
          "id": "MDRW2100008488.1.2",
          "form": "원래 달에 너댓번은 보러 다녔는데 이젠 다니지도 못하고 ㅜㅜㅜ",
          "original_form": "원래 달에 너댓번은 보러 다녔는데 이젠 다니지도 못하고 ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 17:01"
        },
        {
          "id": "MDRW2100008488.1.3",
          "form": "그러게요 가까워도 갈 수가 없네요 사람들도 많고 막혀있어서",
          "original_form": "그러게요 가까워도 갈 수가 없네요 사람들도 많고 막혀있어서",
          "speaker_id": "1",
          "time": "20210822 17:28"
        },
        {
          "id": "MDRW2100008488.1.4",
          "form": "진짜 속상해요 멋진 그림들이 많은데",
          "original_form": "진짜 속상해요 멋진 그림들이 많은데",
          "speaker_id": "2",
          "time": "20210822 17:35"
        },
        {
          "id": "MDRW2100008488.1.5",
          "form": "name2님 지역은 전시가 많은편이예요?",
          "original_form": "&name2&님 지역은 전시가 많은편이예요?",
          "speaker_id": "1",
          "time": "20210824 18:11"
        },
        {
          "id": "MDRW2100008488.1.6",
          "form": "대전은 많은편은 아니고 기획전처럼 하거든요",
          "original_form": "대전은 많은편은 아니고 기획전처럼 하거든요",
          "speaker_id": "1",
          "time": "20210824 18:11"
        },
        {
          "id": "MDRW2100008488.1.7",
          "form": "아하 저는 서울이라서 많은 편이에요!",
          "original_form": "아하 저는 서울이라서 많은 편이에요!",
          "speaker_id": "2",
          "time": "20210824 19:42"
        },
        {
          "id": "MDRW2100008488.1.8",
          "form": "사실 그저 제가 요즘 안가는 것 뿐이죠ㅜㅜ",
          "original_form": "사실 그저 제가 요즘 안가는 것 뿐이죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 19:42"
        },
        {
          "id": "MDRW2100008488.1.9",
          "form": "서울에는 이것저것 볼 기회가 많아서 부러워요",
          "original_form": "서울에는 이것저것 볼 기회가 많아서 부러워요",
          "speaker_id": "1",
          "time": "20210824 22:57"
        },
        {
          "id": "MDRW2100008488.1.10",
          "form": "대전에는 언제쯤ㅋㅋ",
          "original_form": "대전에는 언제쯤ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:57"
        },
        {
          "id": "MDRW2100008488.1.11",
          "form": "광역시지만 시골스타일 도시예요",
          "original_form": "광역시지만 시골스타일 도시예요",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100008488.1.12",
          "form": "도시신생아 느낌?",
          "original_form": "도시신생아 느낌?",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100008488.1.13",
          "form": "저 원래 본가가 천안이라 대전 자주 다녔는데!",
          "original_form": "저 원래 본가가 천안이라 대전 자주 다녔는데!",
          "speaker_id": "2",
          "time": "20210825 00:18"
        },
        {
          "id": "MDRW2100008488.1.14",
          "form": "은근 항상 같은 느낌이었던 것 같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "은근 항상 같은 느낌이었던 것 같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:18"
        },
        {
          "id": "MDRW2100008488.1.15",
          "form": "하지만 천안보다 더좋음.",
          "original_form": "하지만 천안보다 더좋음.",
          "speaker_id": "2",
          "time": "20210825 00:18"
        },
        {
          "id": "MDRW2100008488.1.16",
          "form": "천안이 더 자유롭고 다양하지않아요?",
          "original_form": "천안이 더 자유롭고 다양하지않아요?",
          "speaker_id": "1",
          "time": "20210825 00:30"
        },
        {
          "id": "MDRW2100008488.1.17",
          "form": "가끔가면 진짜 크다 느낌인데",
          "original_form": "가끔가면 진짜 크다 느낌인데",
          "speaker_id": "1",
          "time": "20210825 00:30"
        },
        {
          "id": "MDRW2100008488.1.18",
          "form": "전 뭔가 천안에 진짜 아무것도 없다고 생각했어서..!",
          "original_form": "전 뭔가 천안에 진짜 아무것도 없다고 생각했어서..!",
          "speaker_id": "2",
          "time": "20210825 09:40"
        },
        {
          "id": "MDRW2100008488.1.19",
          "form": "이게 주민효과인가유...",
          "original_form": "이게 주민효과인가유...",
          "speaker_id": "2",
          "time": "20210825 09:40"
        },
        {
          "id": "MDRW2100008488.1.20",
          "form": "ㅋㅋㅋ맞아요ㅋㅋ 근데 대전 진짜 재미없는동네맞아요",
          "original_form": "ㅋㅋㅋ맞아요ㅋㅋ 근데 대전 진짜 재미없는동네맞아요",
          "speaker_id": "1",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100008488.1.21",
          "form": "늘상조용",
          "original_form": "늘상조용",
          "speaker_id": "1",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100008488.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 조용한 도시였군요..!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 조용한 도시였군요..!",
          "speaker_id": "2",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100008488.1.23",
          "form": "근데 대전도 은근 공연 많이 가지 않나요?",
          "original_form": "근데 대전도 은근 공연 많이 가지 않나요?",
          "speaker_id": "2",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100008488.1.24",
          "form": "서울다음으로 코로나제일많은도시된지 오래예요",
          "original_form": "서울다음으로 코로나제일많은도시된지 오래예요",
          "speaker_id": "1",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100008488.1.25",
          "form": "이렇게 이기심많은 동네인지 몰랐어요ㅠㅠ",
          "original_form": "이렇게 이기심많은 동네인지 몰랐어요ㅠㅠ",
