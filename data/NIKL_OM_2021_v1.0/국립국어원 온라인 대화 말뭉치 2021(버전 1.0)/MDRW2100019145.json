      "utterance": [
        {
          "id": "MDRW2100019145.1.1",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "1",
          "time": "20210827 15:38"
        },
        {
          "id": "MDRW2100019145.1.2",
          "form": "삼첩이 제일 실망이었어요 ㅠㅠ 너무 묽어서..",
          "original_form": "삼첩이 제일 실망이었어요 ㅠㅠ 너무 묽어서..",
          "speaker_id": "1",
          "time": "20210827 15:38"
        },
        {
          "id": "MDRW2100019145.1.3",
          "form": "아아 저두요 !!",
          "original_form": "아아 저두요 !!",
          "speaker_id": "2",
          "time": "20210827 15:38"
        },
        {
          "id": "MDRW2100019145.1.4",
          "form": "맛있대서 먹었는데",
          "original_form": "맛있대서 먹었는데",
          "speaker_id": "2",
          "time": "20210827 15:38"
        },
        {
          "id": "MDRW2100019145.1.5",
          "form": "사이드 감튀가 더 맛있었어요 ㅋㅋ",
          "original_form": "사이드 감튀가 더 맛있었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:38"
        },
        {
          "id": "MDRW2100019145.1.6",
          "form": "오맞아요 ㅋㅋㅋㅋ",
          "original_form": "오맞아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:39"
        },
        {
          "id": "MDRW2100019145.1.7",
          "form": "사길 거긴 감튀때메 먹는거같아요",
          "original_form": "사길 거긴 감튀때메 먹는거같아요",
          "speaker_id": "1",
          "time": "20210827 15:39"
        },
        {
          "id": "MDRW2100019145.1.8",
          "form": "전 한번먹고 안먹었어요 ㅜㅜ",
          "original_form": "전 한번먹고 안먹었어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 15:39"
        },
        {
          "id": "MDRW2100019145.1.9",
          "form": "태리로제는 어떠셨어요?",
          "original_form": "태리로제는 어떠셨어요?",
          "speaker_id": "2",
          "time": "20210827 15:40"
        },
        {
          "id": "MDRW2100019145.1.10",
          "form": "아거기는 안 먹어봤어요! 맛있나요??",
          "original_form": "아거기는 안 먹어봤어요! 맛있나요??",
          "speaker_id": "1",
          "time": "20210827 15:40"
        },
        {
          "id": "MDRW2100019145.1.11",
          "form": "맛있어요 ! 소시지 추가해서 드셔야해요 ㅋㅋ",
          "original_form": "맛있어요 ! 소시지 추가해서 드셔야해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:40"
        },
        {
          "id": "MDRW2100019145.1.12",
          "form": "아마 좋아하실거같아요 ㅋㅋㅋ",
          "original_form": "아마 좋아하실거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:40"
        },
        {
          "id": "MDRW2100019145.1.13",
          "form": "오 꾸덕한가여? 넘 궁금해요 ㅋㅋㅋ",
          "original_form": "오 꾸덕한가여? 넘 궁금해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:41"
        },
        {
          "id": "MDRW2100019145.1.14",
          "form": "네 꾸덕해요 !",
          "original_form": "네 꾸덕해요 !",
          "speaker_id": "2",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100019145.1.15",
          "form": "오 먹어봐야겠어요 ㅋㅋㅋ",
          "original_form": "오 먹어봐야겠어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100019145.1.16",
          "form": "동네에 있는데 실패할까봐 도전 못 했거든요",
          "original_form": "동네에 있는데 실패할까봐 도전 못 했거든요",
          "speaker_id": "1",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100019145.1.17",
          "form": "점바점이 있을수도 있으니 ㅋㅋ 리뷰 보고 시켜보세요 ㅎㅎ",
          "original_form": "점바점이 있을수도 있으니 ㅋㅋ 리뷰 보고 시켜보세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100019145.1.18",
          "form": "리뷰 필수죠 ㅋㅋㅋ  후후",
          "original_form": "리뷰 필수죠 ㅋㅋㅋ  후후",
          "speaker_id": "1",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100019145.1.19",
          "form": "신전이랑 어떻게 다른가요??",
          "original_form": "신전이랑 어떻게 다른가요??",
          "speaker_id": "1",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100019145.1.20",
          "form": "음 신전은 약간 그 특유의 향이랑 잘어울려서 맛있구요",
          "original_form": "음 신전은 약간 그 특유의 향이랑 잘어울려서 맛있구요",
          "speaker_id": "2",
          "time": "20210827 15:44"
        },
        {
          "id": "MDRW2100019145.1.21",
          "form": "태리로제는 진짜 찐 로제스러운 맛이요 !",
          "original_form": "태리로제는 진짜 찐 로제스러운 맛이요 !",
          "speaker_id": "2",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100019145.1.22",
          "form": "젤 꾸덕해요 ㅋㅋ",
          "original_form": "젤 꾸덕해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100019145.1.23",
          "form": "헉!! 꾸덕한거 환장하는데 꼭 먹어야겠어요 결심",
          "original_form": "헉!! 꾸덕한거 환장하는데 꼭 먹어야겠어요 결심",
          "speaker_id": "1",
          "time": "20210827 15:46"
        }
      ]
