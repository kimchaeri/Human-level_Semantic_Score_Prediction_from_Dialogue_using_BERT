      "utterance": [
        {
          "id": "MDRW2100034598.1.1",
          "form": "혹시 백신 맞으셨나요?",
          "original_form": "혹시 백신 맞으셨나요?",
          "speaker_id": "1",
          "time": "20210828 16:00"
        },
        {
          "id": "MDRW2100034598.1.2",
          "form": "전 9월초에 맞아요!",
          "original_form": "전 9월초에 맞아요!",
          "speaker_id": "2",
          "time": "20210828 16:01"
        },
        {
          "id": "MDRW2100034598.1.3",
          "form": "전 9월 중순에 맞아요",
          "original_form": "전 9월 중순에 맞아요",
          "speaker_id": "1",
          "time": "20210828 16:02"
        },
        {
          "id": "MDRW2100034598.1.4",
          "form": "부작용없이 잘 지나가면 좋겠어요",
          "original_form": "부작용없이 잘 지나가면 좋겠어요",
          "speaker_id": "1",
          "time": "20210828 16:02"
        },
        {
          "id": "MDRW2100034598.1.5",
          "form": "추석 후에 맞으세요?",
          "original_form": "추석 후에 맞으세요?",
          "speaker_id": "2",
          "time": "20210828 16:02"
        },
        {
          "id": "MDRW2100034598.1.6",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210828 16:02"
        },
        {
          "id": "MDRW2100034598.1.7",
          "form": "전 전 주인데 추석때까진 안아프지 않을까요 하하",
          "original_form": "전 전 주인데 추석때까진 안아프지 않을까요 하하",
          "speaker_id": "1",
          "time": "20210828 16:02"
        },
        {
          "id": "MDRW2100034598.1.8",
          "form": "추석때 고향대신 응급실에 안가길바래요오",
          "original_form": "추석때 고향대신 응급실에 안가길바래요오",
          "speaker_id": "1",
          "time": "20210828 16:03"
        },
        {
          "id": "MDRW2100034598.1.9",
          "form": "앜ㅋㅋㅋㅋㅋ 괜찮으실거에요",
          "original_form": "앜ㅋㅋㅋㅋㅋ 괜찮으실거에요",
          "speaker_id": "2",
          "time": "20210828 16:03"
        },
        {
          "id": "MDRW2100034598.1.10",
          "form": "저는 후였는데 바꿨어요",
          "original_form": "저는 후였는데 바꿨어요",
          "speaker_id": "2",
          "time": "20210828 16:03"
        },
        {
          "id": "MDRW2100034598.1.11",
          "form": "전에 맞는게 좋죠 추석때 가서 옮기거나 옮을까봐 걱정이 되어서",
          "original_form": "전에 맞는게 좋죠 추석때 가서 옮기거나 옮을까봐 걱정이 되어서",
          "speaker_id": "1",
          "time": "20210828 16:04"
        },
        {
          "id": "MDRW2100034598.1.12",
          "form": "일부러 전으로 했어요",
          "original_form": "일부러 전으로 했어요",
          "speaker_id": "1",
          "time": "20210828 16:04"
        },
        {
          "id": "MDRW2100034598.1.13",
          "form": "잘하셨어요",
          "original_form": "잘하셨어요",
          "speaker_id": "2",
          "time": "20210828 16:04"
        },
        {
          "id": "MDRW2100034598.1.14",
          "form": "추석때 위험할수도 있죠 ㅜㅜ",
          "original_form": "추석때 위험할수도 있죠 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 16:04"
        },
        {
          "id": "MDRW2100034598.1.15",
          "form": "저는 바꾸란 권유 문잘 받았어요 물량 풀렸다구 ㅋㅋ",
          "original_form": "저는 바꾸란 권유 문잘 받았어요 물량 풀렸다구 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:04"
        },
        {
          "id": "MDRW2100034598.1.16",
          "form": "헐 그런 문자도 오나요? 전 첨부터 전 으로 예약해서 그런다 문자릉 못받았어요",
          "original_form": "헐 그런 문자도 오나요? 전 첨부터 전 으로 예약해서 그런다 문자릉 못받았어요",
          "speaker_id": "1",
          "time": "20210828 16:05"
        },
        {
          "id": "MDRW2100034598.1.17",
          "form": "아 그러고보니 뉴스서 봤네요",
          "original_form": "아 그러고보니 뉴스서 봤네요",
          "speaker_id": "1",
          "time": "20210828 16:05"
        },
        {
          "id": "MDRW2100034598.1.18",
          "form": "네 추석 후라고 문자왔어요",
          "original_form": "네 추석 후라고 문자왔어요",
          "speaker_id": "2",
          "time": "20210828 16:05"
        },
        {
          "id": "MDRW2100034598.1.19",
          "form": "그래서 얼른 바꿨죠 ㅋㅋㅋ",
          "original_form": "그래서 얼른 바꿨죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:05"
        },
        {
          "id": "MDRW2100034598.1.20",
          "form": "문자아니면 몰랐을거에요 풀렸는지두",
          "original_form": "문자아니면 몰랐을거에요 풀렸는지두",
          "speaker_id": "2",
          "time": "20210828 16:05"
        },
        {
          "id": "MDRW2100034598.1.21",
          "form": "뉴스서 보니깐 추석전으로 시기를 바꾸는건 지금 된다고 하길래",
          "original_form": "뉴스서 보니깐 추석전으로 시기를 바꾸는건 지금 된다고 하길래",
          "speaker_id": "1",
          "time": "20210828 16:06"
        },
        {
          "id": "MDRW2100034598.1.22",
          "form": "오 나왔군요",
          "original_form": "오 나왔군요",
          "speaker_id": "2",
          "time": "20210828 16:06"
        },
        {
          "id": "MDRW2100034598.1.23",
          "form": "뒤로 미루는건 안되나보다 했죠",
          "original_form": "뒤로 미루는건 안되나보다 했죠",
          "speaker_id": "1",
          "time": "20210828 16:06"
        }
      ]
    }
  ]
