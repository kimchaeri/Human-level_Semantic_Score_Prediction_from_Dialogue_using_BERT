      "utterance": [
        {
          "id": "MDRW2100024830.1.1",
          "form": "요리잘하세요???",
          "original_form": "요리잘하세요???",
          "speaker_id": "1",
          "time": "20210831 19:24"
        },
        {
          "id": "MDRW2100024830.1.2",
          "form": "아뇽.. 요리그냥",
          "original_form": "아뇽.. 요리그냥",
          "speaker_id": "2",
          "time": "20210831 19:25"
        },
        {
          "id": "MDRW2100024830.1.3",
          "form": "밀키트나 사서 해먹는정도 ㅋㅋ",
          "original_form": "밀키트나 사서 해먹는정도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:25"
        },
        {
          "id": "MDRW2100024830.1.4",
          "form": "그래도 만들어드시다니",
          "original_form": "그래도 만들어드시다니",
          "speaker_id": "1",
          "time": "20210831 19:26"
        },
        {
          "id": "MDRW2100024830.1.5",
          "form": "성실하시네요ㅋㅋ",
          "original_form": "성실하시네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:26"
        },
        {
          "id": "MDRW2100024830.1.6",
          "form": "ㅋㅋㅋㅋ다 사먹자니 돈이 넘 많이들어요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ다 사먹자니 돈이 넘 많이들어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:26"
        },
        {
          "id": "MDRW2100024830.1.7",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "1",
          "time": "20210831 19:28"
        },
        {
          "id": "MDRW2100024830.1.8",
          "form": "몸도 안좋아지는것같고ㅠ",
          "original_form": "몸도 안좋아지는것같고ㅠ",
          "speaker_id": "1",
          "time": "20210831 19:28"
        },
        {
          "id": "MDRW2100024830.1.9",
          "form": "글쵸 ㅠㅠ 배달 ..시키면 항상후회",
          "original_form": "글쵸 ㅠㅠ 배달 ..시키면 항상후회",
          "speaker_id": "2",
          "time": "20210831 19:28"
        },
        {
          "id": "MDRW2100024830.1.10",
          "form": "그리고 저는 혼자먹는편이라 항상 남더라고요",
          "original_form": "그리고 저는 혼자먹는편이라 항상 남더라고요",
          "speaker_id": "1",
          "time": "20210831 19:29"
        },
        {
          "id": "MDRW2100024830.1.11",
          "form": "맞아요 아까워요ㅠㅠ",
          "original_form": "맞아요 아까워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 19:30"
        },
        {
          "id": "MDRW2100024830.1.12",
          "form": "쓰레기도 많이나오고",
          "original_form": "쓰레기도 많이나오고",
          "speaker_id": "2",
          "time": "20210831 19:30"
        },
        {
          "id": "MDRW2100024830.1.13",
          "form": "네 진짜요ㅠㅠ 일회용 용기",
          "original_form": "네 진짜요ㅠㅠ 일회용 용기",
          "speaker_id": "1",
          "time": "20210831 19:30"
        },
        {
          "id": "MDRW2100024830.1.14",
          "form": "환경도 망가지고",
          "original_form": "환경도 망가지고",
          "speaker_id": "1",
          "time": "20210831 19:30"
        },
        {
          "id": "MDRW2100024830.1.15",
          "form": "그래서 저도 최대한 안시키려합니다ㅠㅠㅋㅋㅋ",
          "original_form": "그래서 저도 최대한 안시키려합니다ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:31"
        },
        {
          "id": "MDRW2100024830.1.16",
          "form": "ㅋㅋㅋㅋㅋ쿠ㅠㅠ오늘 전 시켜먹네요....",
          "original_form": "ㅋㅋㅋㅋㅋ쿠ㅠㅠ오늘 전 시켜먹네요....",
          "speaker_id": "1",
          "time": "20210831 19:32"
        },
        {
          "id": "MDRW2100024830.1.17",
          "form": "ㅋㅋㅋ 뭐시켜드세용",
          "original_form": "ㅋㅋㅋ 뭐시켜드세용",
          "speaker_id": "2",
          "time": "20210831 19:32"
        },
        {
          "id": "MDRW2100024830.1.18",
          "form": "엽떡이요!!",
          "original_form": "엽떡이요!!",
          "speaker_id": "1",
          "time": "20210831 21:21"
        },
        {
          "id": "MDRW2100024830.1.19",
