      "utterance": [
        {
          "id": "MDRW2100007239.1.1",
          "form": "그지역은 비많이 오나요?",
          "original_form": "그지역은 비많이 오나요?",
          "speaker_id": "2",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100007239.1.2",
          "form": "아뇨~! 별로안오는편인거같아요",
          "original_form": "아뇨~! 별로안오는편인거같아요",
          "speaker_id": "1",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007239.1.3",
          "form": "거긴 어디에요?",
          "original_form": "거긴 어디에요?",
          "speaker_id": "1",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100007239.1.4",
          "form": "여긴 서울인데 빗소리는 안들리네요",
          "original_form": "여긴 서울인데 빗소리는 안들리네요",
          "speaker_id": "2",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007239.1.5",
          "form": "태풍오고있대요",
          "original_form": "태풍오고있대요",
          "speaker_id": "2",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007239.1.6",
          "form": "헐 전 인천인데 서울이랑 비슷해요 ㅋㅋ",
          "original_form": "헐 전 인천인데 서울이랑 비슷해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007239.1.7",
          "form": "태풍 서울까지오는경우 별로없는더같애요",
          "original_form": "태풍 서울까지오는경우 별로없는더같애요",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007239.1.8",
          "form": "맨날 제주도쪽만 난리남",
          "original_form": "맨날 제주도쪽만 난리남",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007239.1.9",
          "form": "오 인천 저본가있는데 반갑네요ㅎ",
          "original_form": "오 인천 저본가있는데 반갑네요ㅎ",
          "speaker_id": "2",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007239.1.10",
          "form": "남부지방만 많이오나봐요",
          "original_form": "남부지방만 많이오나봐요",
          "speaker_id": "2",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007239.1.11",
          "form": "오 서울에자취하시는건가요",
          "original_form": "오 서울에자취하시는건가요",
          "speaker_id": "1",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007239.1.12",
          "form": "맞아요 태풍경로가 항상그렇더라",
          "original_form": "맞아요 태풍경로가 항상그렇더라",
          "speaker_id": "1",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007239.1.13",
          "form": "네 직장근처로 ㅎㅎ",
          "original_form": "네 직장근처로 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007239.1.14",
          "form": "비피해 없어야될텐데",
          "original_form": "비피해 없어야될텐데",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007239.1.15",
          "form": "서울은괜찮을거에요!",
          "original_form": "서울은괜찮을거에요!",
          "speaker_id": "1",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007239.1.16",
          "form": "날씨 요즘 선선해졌나요",
          "original_form": "날씨 요즘 선선해졌나요",
          "speaker_id": "1",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007239.1.17",
          "form": "가을 오는거같죠?",
          "original_form": "가을 오는거같죠?",
          "speaker_id": "2",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007239.1.18",
          "form": "언제 더웠냐는듯 갑자기 선선해졌어요",
          "original_form": "언제 더웠냐는듯 갑자기 선선해졌어요",
          "speaker_id": "2",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007239.1.19",
          "form": "오 전 추운거보다 더운게 힘들거든요 ㅜㅜ",
          "original_form": "오 전 추운거보다 더운게 힘들거든요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007239.1.20",
          "form": "거기다 매년 더 더워진다며요?",
          "original_form": "거기다 매년 더 더워진다며요?",
          "speaker_id": "1",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007239.1.21",
          "form": "저도 여름 넘 싫어요",
          "original_form": "저도 여름 넘 싫어요",
          "speaker_id": "2",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007239.1.22",
          "form": "벌레도많고 숨이차서",
          "original_form": "벌레도많고 숨이차서",
          "speaker_id": "1",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007239.1.23",
          "form": "빨리 가을오면좋겠어요",
          "original_form": "빨리 가을오면좋겠어요",
          "speaker_id": "2",
          "time": "20210822 01:27"
        }
      ]
    }
