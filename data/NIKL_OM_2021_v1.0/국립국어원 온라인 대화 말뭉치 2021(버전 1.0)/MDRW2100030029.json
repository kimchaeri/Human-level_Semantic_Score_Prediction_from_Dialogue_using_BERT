      "utterance": [
        {
          "id": "MDRW2100030029.1.1",
          "form": "address에 비가 오나요",
          "original_form": "&address&에 비가 오나요",
          "speaker_id": "2",
          "time": "20210903 19:03"
        },
        {
          "id": "MDRW2100030029.1.2",
          "form": "가을에 원래 이렇게 비가 많이 왔던가요",
          "original_form": "가을에 원래 이렇게 비가 많이 왔던가요",
          "speaker_id": "2",
          "time": "20210903 19:04"
        },
        {
          "id": "MDRW2100030029.1.3",
          "form": "비가오는가 같네요~",
          "original_form": "비가오는가 같네요~",
          "speaker_id": "1",
          "time": "20210903 19:05"
        },
        {
          "id": "MDRW2100030029.1.4",
          "form": "가을 장마..정말 지겹구만요~",
          "original_form": "가을 장마..정말 지겹구만요~",
          "speaker_id": "1",
          "time": "20210903 19:05"
        },
        {
          "id": "MDRW2100030029.1.5",
          "form": "아이고 내일 당근  하러 address 이차 갑니다",
          "original_form": "아이고 내일 당근  하러 &address& 이차 갑니다",
          "speaker_id": "2",
          "time": "20210903 19:06"
        },
        {
          "id": "MDRW2100030029.1.6",
          "form": "아 비 오는 계절에 운전해도 될까요",
          "original_form": "아 비 오는 계절에 운전해도 될까요",
          "speaker_id": "2",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100030029.1.7",
          "form": "바쁘네요 ㅋㅋㅋㅋ",
          "original_form": "바쁘네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:08"
        },
        {
          "id": "MDRW2100030029.1.8",
          "form": "저는 낼 일갑니다요~",
          "original_form": "저는 낼 일갑니다요~",
          "speaker_id": "1",
          "time": "20210903 19:08"
        },
        {
          "id": "MDRW2100030029.1.9",
          "form": "날씨가 이러니",
          "original_form": "날씨가 이러니",
          "speaker_id": "1",
          "time": "20210903 19:08"
        },
        {
          "id": "MDRW2100030029.1.10",
          "form": "잠이 오네요;",
          "original_form": "잠이 오네요;",
          "speaker_id": "1",
          "time": "20210903 19:08"
        },
        {
          "id": "MDRW2100030029.1.11",
          "form": "가을은 원래 낮잠이 잘 옵니다",
          "original_form": "가을은 원래 낮잠이 잘 옵니다",
          "speaker_id": "2",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100030029.1.12",
          "form": "말이 살찌는 계절이죠",
          "original_form": "말이 살찌는 계절이죠",
          "speaker_id": "2",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100030029.1.13",
          "form": "말 말띠 신가요",
          "original_form": "말 말띠 신가요",
          "speaker_id": "2",
          "time": "20210903 19:11"
        },
        {
          "id": "MDRW2100030029.1.14",
          "form": "토끼띱니다~",
          "original_form": "토끼띱니다~",
          "speaker_id": "1",
          "time": "20210903 19:13"
        },
        {
          "id": "MDRW2100030029.1.15",
          "form": "다 살찌는계절이네여 ㅋㅋㅋ",
          "original_form": "다 살찌는계절이네여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:13"
        },
        {
          "id": "MDRW2100030029.1.16",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210903 19:34"
        },
        {
          "id": "MDRW2100030029.1.17",
          "form": "내일 비가 온다 나요",
          "original_form": "내일 비가 온다 나요",
          "speaker_id": "2",
          "time": "20210903 19:34"
        },
        {
          "id": "MDRW2100030029.1.18",
          "form": "내일 address 비 안오면 좋겠습니다",
          "original_form": "내일 &address& 비 안오면 좋겠습니다",
          "speaker_id": "2",
          "time": "20210903 19:34"
        },
        {
          "id": "MDRW2100030029.1.19",
          "form": "날씨가 흐리멍텅 하요",
          "original_form": "날씨가 흐리멍텅 하요",
          "speaker_id": "2",
          "time": "20210904 10:32"
        },
        {
          "id": "MDRW2100030029.1.20",
          "form": "저희는 좀있다 구미갑니다 ㅋㅋㅋㅋㅋ",
          "original_form": "저희는 좀있다 구미갑니다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:59"
        },
        {
          "id": "MDRW2100030029.1.21",
          "form": "너무 힘들어서 좀 쉬러 가야겠습니다~",
          "original_form": "너무 힘들어서 좀 쉬러 가야겠습니다~",
          "speaker_id": "1",
          "time": "20210904 10:59"
        },
        {
          "id": "MDRW2100030029.1.22",
          "form": "이런 날씨에 구미에 가다니요",
          "original_form": "이런 날씨에 구미에 가다니요",
          "speaker_id": "2",
          "time": "20210904 12:28"
        },
        {
          "id": "MDRW2100030029.1.23",
          "form": "날씨 좋은 데 구미 가지 마세요",
          "original_form": "날씨 좋은 데 구미 가지 마세요",
          "speaker_id": "2",
          "time": "20210904 12:28"
        },
        {
          "id": "MDRW2100030029.1.24",
          "form": "놀러가야 됩니다 좋은 날씨",
          "original_form": "놀러가야 됩니다 좋은 날씨",
          "speaker_id": "2",
          "time": "20210904 12:28"
        },
        {
          "id": "MDRW2100030029.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ어디로 가십니꺼??",
          "original_form": "ㅋㅋㅋㅋㅋㅋ어디로 가십니꺼??",
          "speaker_id": "1",
          "time": "20210904 13:49"
        },
        {
          "id": "MDRW2100030029.1.26",
          "form": "구미 가셨습니까",
          "original_form": "구미 가셨습니까",
          "speaker_id": "2",
          "time": "20210904 13:56"
        },
        {
          "id": "MDRW2100030029.1.27",
          "form": "지금 엄마 집 왔어요",
          "original_form": "지금 엄마 집 왔어요",
          "speaker_id": "2",
          "time": "20210904 13:56"
        },
        {
          "id": "MDRW2100030029.1.28",
          "form": "밖에 비 옵니까",
          "original_form": "밖에 비 옵니까",
          "speaker_id": "2",
          "time": "20210904 13:57"
        },
        {
          "id": "MDRW2100030029.1.29",
          "form": "햇볕이 쨍쨍한가요",
          "original_form": "햇볕이 쨍쨍한가요",
          "speaker_id": "2",
          "time": "20210904 13:57"
        },
        {
          "id": "MDRW2100030029.1.30",
          "form": "와따 여긴 진짜 추워요~감기걸릴꺼같네요~",
          "original_form": "와따 여긴 진짜 추워요~감기걸릴꺼같네요~",
