      "utterance": [
        {
          "id": "MDRW2100046939.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20211108 14:00"
        },
        {
          "id": "MDRW2100046939.1.2",
          "form": "안녕하세욤",
          "original_form": "안녕하세욤",
          "speaker_id": "2",
          "time": "20211108 14:07"
        },
        {
          "id": "MDRW2100046939.1.3",
          "form": "혹시 싸이월드 세대이신가요?",
          "original_form": "혹시 싸이월드 세대이신가요?",
          "speaker_id": "2",
          "time": "20211108 14:08"
        },
        {
          "id": "MDRW2100046939.1.4",
          "form": "네 싸이월드 해봤어요",
          "original_form": "네 싸이월드 해봤어요",
          "speaker_id": "1",
          "time": "20211108 14:20"
        },
        {
          "id": "MDRW2100046939.1.5",
          "form": "이젠 추억속으로 사라지는줄 알았는데",
          "original_form": "이젠 추억속으로 사라지는줄 알았는데",
          "speaker_id": "1",
          "time": "20211108 14:20"
        },
        {
          "id": "MDRW2100046939.1.6",
          "form": "호오 반갑네용",
          "original_form": "호오 반갑네용",
          "speaker_id": "2",
          "time": "20211108 14:20"
        },
        {
          "id": "MDRW2100046939.1.7",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20211108 14:20"
        },
        {
          "id": "MDRW2100046939.1.8",
          "form": "재오픈 한다는말이",
          "original_form": "재오픈 한다는말이",
          "speaker_id": "1",
          "time": "20211108 14:20"
        },
        {
          "id": "MDRW2100046939.1.9",
          "form": "있더라구요",
          "original_form": "있더라구요",
          "speaker_id": "1",
          "time": "20211108 14:20"
        },
        {
          "id": "MDRW2100046939.1.10",
          "form": "이미 했더라구요!",
          "original_form": "이미 했더라구요!",
          "speaker_id": "2",
          "time": "20211108 14:20"
        },
        {
          "id": "MDRW2100046939.1.11",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20211108 14:20"
        },
        {
          "id": "MDRW2100046939.1.12",
          "form": "생각보다 조용할뿐",
          "original_form": "생각보다 조용할뿐",
          "speaker_id": "2",
          "time": "20211108 14:20"
        },
        {
          "id": "MDRW2100046939.1.13",
          "form": "정말요!",
          "original_form": "정말요!",
          "speaker_id": "1",
          "time": "20211108 14:21"
        },
        {
          "id": "MDRW2100046939.1.14",
          "form": "하긴 요즘 이미 페이스북 인스타가",
          "original_form": "하긴 요즘 이미 페이스북 인스타가",
          "speaker_id": "1",
          "time": "20211108 14:21"
        },
        {
          "id": "MDRW2100046939.1.15",
          "form": "대세이니",
          "original_form": "대세이니",
          "speaker_id": "1",
          "time": "20211108 14:21"
        },
        {
          "id": "MDRW2100046939.1.16",
          "form": "넹 10월달에 재오픈했더라구요!",
          "original_form": "넹 10월달에 재오픈했더라구요!",
          "speaker_id": "2",
          "time": "20211108 14:21"
        },
        {
          "id": "MDRW2100046939.1.17",
          "form": "ㅎㅎ 그쵸",
          "original_form": "ㅎㅎ 그쵸",
          "speaker_id": "2",
          "time": "20211108 14:21"
        },
        {
          "id": "MDRW2100046939.1.18",
          "form": "다른 SNS가 이미 너무 일상화되서",
          "original_form": "다른 SNS가 이미 너무 일상화되서",
          "speaker_id": "2",
          "time": "20211108 14:21"
        },
        {
          "id": "MDRW2100046939.1.19",
          "form": "싸이월드로 넘어가기 쉽지 않을 거 같긴했는데",
          "original_form": "싸이월드로 넘어가기 쉽지 않을 거 같긴했는데",
          "speaker_id": "2",
          "time": "20211108 14:21"
        },
        {
          "id": "MDRW2100046939.1.20",
          "form": "생각보다 더 조용한 느낌",
          "original_form": "생각보다 더 조용한 느낌",
          "speaker_id": "2",
          "time": "20211108 14:21"
        },
        {
          "id": "MDRW2100046939.1.21",
          "form": "싸이월드가 계속 유지될려면",
          "original_form": "싸이월드가 계속 유지될려면",
          "speaker_id": "1",
          "time": "20211108 14:22"
        },
        {
          "id": "MDRW2100046939.1.22",
          "form": "사용자수가 늘어야할텐데",
          "original_form": "사용자수가 늘어야할텐데",
          "speaker_id": "1",
          "time": "20211108 14:22"
        },
        {
          "id": "MDRW2100046939.1.23",
          "form": "그쵸 ㅎ 근데",
          "original_form": "그쵸 ㅎ 근데",
          "speaker_id": "2",
          "time": "20211108 14:22"
        },
        {
          "id": "MDRW2100046939.1.24",
          "form": "단순하게 추억 되살리는거 때문에",
          "original_form": "단순하게 추억 되살리는거 때문에",
          "speaker_id": "2",
          "time": "20211108 14:22"
        },
        {
          "id": "MDRW2100046939.1.25",
          "form": "재출시한건",
          "original_form": "재출시한건",
          "speaker_id": "2",
          "time": "20211108 14:22"
        },
        {
          "id": "MDRW2100046939.1.26",
          "form": "아닌거같던데용",
          "original_form": "아닌거같던데용",
          "speaker_id": "2",
          "time": "20211108 14:22"
        },
        {
          "id": "MDRW2100046939.1.27",
          "form": "네 사업성을 보고",
          "original_form": "네 사업성을 보고",
          "speaker_id": "1",
          "time": "20211108 14:22"
        },
        {
          "id": "MDRW2100046939.1.28",
          "form": "다시재오픈한거같은데",
          "original_form": "다시재오픈한거같은데",
          "speaker_id": "1",
          "time": "20211108 14:23"
        },
        {
          "id": "MDRW2100046939.1.29",
          "form": "코인 관련해서 무슨 큰 그림을 그리는 거 같더라구요",
          "original_form": "코인 관련해서 무슨 큰 그림을 그리는 거 같더라구요",
          "speaker_id": "2",
          "time": "20211108 14:23"
        },
        {
          "id": "MDRW2100046939.1.30",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211108 14:23"
        },
        {
          "id": "MDRW2100046939.1.31",
          "form": "아 코인!",
          "original_form": "아 코인!",
          "speaker_id": "1",
          "time": "20211108 14:23"
        },
        {
          "id": "MDRW2100046939.1.32",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "2",
          "time": "20211108 14:23"
        },
        {
          "id": "MDRW2100046939.1.33",
          "form": "주식이나",
          "original_form": "주식이나",
          "speaker_id": "2",
          "time": "20211108 14:23"
        },
        {
          "id": "MDRW2100046939.1.34",
          "form": "코인 잘 몰라서 걍 그렇구나 하는데",
          "original_form": "코인 잘 몰라서 걍 그렇구나 하는데",
          "speaker_id": "2",
          "time": "20211108 14:23"
        },
        {
          "id": "MDRW2100046939.1.35",
          "form": "도토리 대신 코인으로 하는건지...ㅋㅋㅋㅋㅋ",
          "original_form": "도토리 대신 코인으로 하는건지...ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211108 14:25"
        },
        {
          "id": "MDRW2100046939.1.36",
          "form": "아 근데 검색해보니까",
          "original_form": "아 근데 검색해보니까",
          "speaker_id": "2",
          "time": "20211108 14:25"
        },
        {
          "id": "MDRW2100046939.1.37",
          "form": "싸이월드가 지금은 베타테스트 느낌으로 맛보기 오픈을 해있는거고",
          "original_form": "싸이월드가 지금은 베타테스트 느낌으로 맛보기 오픈을 해있는거고",
          "speaker_id": "2",
          "time": "20211108 14:25"
        },
        {
          "id": "MDRW2100046939.1.38",
          "form": "정식 오픈은 계속 미뤄지고 있다고 하네요 ㅠㅠㅋㅋㅋ",
          "original_form": "정식 오픈은 계속 미뤄지고 있다고 하네요 ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211108 14:25"
        },
        {
          "id": "MDRW2100046939.1.39",
          "form": "다날과 관련되어있네요",
          "original_form": "다날과 관련되어있네요",
          "speaker_id": "1",
          "time": "20211108 14:25"
        },
        {
          "id": "MDRW2100046939.1.40",
          "form": "다날은 페이코인쪽이였나요",
          "original_form": "다날은 페이코인쪽이였나요",
          "speaker_id": "1",
          "time": "20211108 14:26"
        },
        {
          "id": "MDRW2100046939.1.41",
          "form": "그러게요!",
          "original_form": "그러게요!",
          "speaker_id": "2",
          "time": "20211108 14:26"
        },
        {
          "id": "MDRW2100046939.1.42",
          "form": "페이코인 도 엄청올랐더라구요",
          "original_form": "페이코인 도 엄청올랐더라구요",
          "speaker_id": "1",
          "time": "20211108 14:26"
        },
        {
          "id": "MDRW2100046939.1.43",
          "form": "제 월급 빼고 다 잘오르는군요",
          "original_form": "제 월급 빼고 다 잘오르는군요",
          "speaker_id": "2",
          "time": "20211108 14:26"
        },
        {
          "id": "MDRW2100046939.1.44",
          "form": "눈물",
          "original_form": "눈물",
          "speaker_id": "2",
          "time": "20211108 14:26"
        },
        {
          "id": "MDRW2100046939.1.45",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20211108 14:26"
        },
        {
          "id": "MDRW2100046939.1.46",
          "form": "월급빼고 다오르네요",
          "original_form": "월급빼고 다오르네요",
          "speaker_id": "1",
          "time": "20211108 14:26"
        },
        {
          "id": "MDRW2100046939.1.47",
          "form": "벼락거지된느낌이에요",
          "original_form": "벼락거지된느낌이에요",
          "speaker_id": "1",
          "time": "20211108 14:26"
        },
        {
          "id": "MDRW2100046939.1.48",
          "form": "그저 눈물만 날뿐",
          "original_form": "그저 눈물만 날뿐",
          "speaker_id": "2",
          "time": "20211108 14:26"
        },
        {
          "id": "MDRW2100046939.1.49",
          "form": "벼락거지 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "벼락거지 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211108 14:26"
        },
        {
          "id": "MDRW2100046939.1.50",
          "form": "신박하게 슬픈 표현인데요...ㅠ",
          "original_form": "신박하게 슬픈 표현인데요...ㅠ",
          "speaker_id": "2",
          "time": "20211108 14:26"
        },
        {
          "id": "MDRW2100046939.1.51",
          "form": "우리 존재 화이팅....\r\n",
          "original_form": "우리 존재 화이팅....\r\n",
          "speaker_id": "2",
