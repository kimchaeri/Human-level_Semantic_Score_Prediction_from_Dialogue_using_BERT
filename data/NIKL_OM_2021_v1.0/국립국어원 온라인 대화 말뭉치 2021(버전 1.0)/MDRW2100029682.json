      "utterance": [
        {
          "id": "MDRW2100029682.1.1",
          "form": "개강했잖아요,",
          "original_form": "개강했잖아요,",
          "speaker_id": "1",
          "time": "20210903 11:39"
        },
        {
          "id": "MDRW2100029682.1.2",
          "form": "네 맞아요!!!",
          "original_form": "네 맞아요!!!",
          "speaker_id": "2",
          "time": "20210903 11:40"
        },
        {
          "id": "MDRW2100029682.1.3",
          "form": "다 비대면이면 학교는 안나와요?",
          "original_form": "다 비대면이면 학교는 안나와요?",
          "speaker_id": "1",
          "time": "20210903 11:40"
        },
        {
          "id": "MDRW2100029682.1.4",
          "form": "화실기랑 화학연구는 대면이던데",
          "original_form": "화실기랑 화학연구는 대면이던데",
          "speaker_id": "2",
          "time": "20210903 11:42"
        },
        {
          "id": "MDRW2100029682.1.5",
          "form": "지금 화실기랑 화학연구 들을지말지 고민중이에요",
          "original_form": "지금 화실기랑 화학연구 들을지말지 고민중이에요",
          "speaker_id": "2",
          "time": "20210903 11:42"
        },
        {
          "id": "MDRW2100029682.1.6",
          "form": "나머지는 다 비대면이에요!",
          "original_form": "나머지는 다 비대면이에요!",
          "speaker_id": "2",
          "time": "20210903 11:42"
        },
        {
          "id": "MDRW2100029682.1.7",
          "form": "화학연구는 오티랑 막주 발표만 빼면 나가는 일 없을걸요?",
          "original_form": "화학연구는 오티랑 막주 발표만 빼면 나가는 일 없을걸요?",
          "speaker_id": "1",
          "time": "20210903 11:45"
        },
        {
          "id": "MDRW2100029682.1.8",
          "form": "코시국 전에도 그랬으니까요",
          "original_form": "코시국 전에도 그랬으니까요",
          "speaker_id": "1",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100029682.1.9",
          "form": "세미나 참석을 체크하긴 하는데",
          "original_form": "세미나 참석을 체크하긴 하는데",
          "speaker_id": "1",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100029682.1.10",
          "form": "요즘은 세미나도 안하거나 줌으로 해서...",
          "original_form": "요즘은 세미나도 안하거나 줌으로 해서...",
          "speaker_id": "1",
          "time": "20210903 11:46"
        },
        {
          "id": "MDRW2100029682.1.11",
          "form": "네 지난학기때는 세미나 온라인으로 10번 참석하는것으로 했다고 하더라고요",
          "original_form": "네 지난학기때는 세미나 온라인으로 10번 참석하는것으로 했다고 하더라고요",
          "speaker_id": "2",
          "time": "20210903 11:58"
        },
        {
          "id": "MDRW2100029682.1.12",
          "form": "기기분석을들을지",
          "original_form": "기기분석을들을지",
          "speaker_id": "2",
          "time": "20210903 11:59"
        },
        {
          "id": "MDRW2100029682.1.13",
          "form": "화실기와 화학연구를 들을지 고민중이에요",
          "original_form": "화실기와 화학연구를 들을지 고민중이에요",
          "speaker_id": "2",
          "time": "20210903 11:59"
        },
        {
          "id": "MDRW2100029682.1.14",
          "form": "오... 기기분석.....",
          "original_form": "오... 기기분석.....",
          "speaker_id": "1",
          "time": "20210903 12:01"
        },
        {
          "id": "MDRW2100029682.1.15",
          "form": "만약 분석기사 자격증같은거 따려면 좋죠",
          "original_form": "만약 분석기사 자격증같은거 따려면 좋죠",
          "speaker_id": "1",
          "time": "20210903 12:01"
        },
        {
          "id": "MDRW2100029682.1.16",
          "form": "만약에 언니라면 둘중에 어떤걸 들으실건가요???",
          "original_form": "만약에 언니라면 둘중에 어떤걸 들으실건가요???",
          "speaker_id": "2",
          "time": "20210903 12:01"
        },
        {
          "id": "MDRW2100029682.1.17",
          "form": "전 분석이 영 안맞았지만",
          "original_form": "전 분석이 영 안맞았지만",
          "speaker_id": "1",
          "time": "20210903 12:01"
        },
        {
          "id": "MDRW2100029682.1.18",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210903 12:02"
        },
        {
          "id": "MDRW2100029682.1.19",
          "form": "뭔가 실용적?이다 싶긴 했어요",
          "original_form": "뭔가 실용적?이다 싶긴 했어요",
          "speaker_id": "1",
          "time": "20210903 12:02"
        },
        {
          "id": "MDRW2100029682.1.20",
          "form": "기기분석이",
          "original_form": "기기분석이",
          "speaker_id": "1",
          "time": "20210903 12:02"
        },
        {
          "id": "MDRW2100029682.1.21",
          "form": "화실기는 기계를 깔짝대긴 하는데",
          "original_form": "화실기는 기계를 깔짝대긴 하는데",
          "speaker_id": "1",
          "time": "20210903 12:02"
        },
        {
          "id": "MDRW2100029682.1.22",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210903 12:03"
        },
        {
          "id": "MDRW2100029682.1.23",
          "form": "솔직히 레포트쓰는 기술은 는거 같은데",
          "original_form": "솔직히 레포트쓰는 기술은 는거 같은데",
          "speaker_id": "1",
          "time": "20210903 12:03"
        },
        {
          "id": "MDRW2100029682.1.24",
          "form": "분석하고 기기다루는 법은 딱히 는 거 같지 않고...",
          "original_form": "분석하고 기기다루는 법은 딱히 는 거 같지 않고...",
          "speaker_id": "1",
          "time": "20210903 12:03"
        },
        {
          "id": "MDRW2100029682.1.25",
          "form": "제 주관적 생각입니다...",
          "original_form": "제 주관적 생각입니다...",
          "speaker_id": "1",
          "time": "20210903 12:04"
        },
        {
          "id": "MDRW2100029682.1.26",
          "form": "아 화실기가 기기다루는법을 딱히 배우는 느낌은 아니었나요ㅜㅠ",
          "original_form": "아 화실기가 기기다루는법을 딱히 배우는 느낌은 아니었나요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210903 12:04"
        },
        {
          "id": "MDRW2100029682.1.27",
          "form": "전 그걸 기대했는데..",
          "original_form": "전 그걸 기대했는데..",
          "speaker_id": "2",
          "time": "20210903 12:04"
        },
        {
          "id": "MDRW2100029682.1.28",
          "form": "가르쳐주기는 하죠",
          "original_form": "가르쳐주기는 하죠",
          "speaker_id": "1",
          "time": "20210903 12:06"
        },
        {
          "id": "MDRW2100029682.1.29",
          "form": "기기 실제로 쓰기는 하고",
          "original_form": "기기 실제로 쓰기는 하고",
          "speaker_id": "1",
          "time": "20210903 12:07"
        },
        {
          "id": "MDRW2100029682.1.30",
          "form": "근데 약간 매뉴얼보고 차근히 따라하면 다 할수있는 수준의 기본만 했던거같고",
          "original_form": "근데 약간 매뉴얼보고 차근히 따라하면 다 할수있는 수준의 기본만 했던거같고",
          "speaker_id": "1",
          "time": "20210903 12:07"
        },
        {
          "id": "MDRW2100029682.1.31",
          "form": "기기 수준?이 아주 좋지는 않으니까",
          "original_form": "기기 수준?이 아주 좋지는 않으니까",
          "speaker_id": "1",
          "time": "20210903 12:08"
        },
        {
          "id": "MDRW2100029682.1.32",
          "form": "가설이나 예상이랑 다르게 나오는 경우가 많아요",
          "original_form": "가설이나 예상이랑 다르게 나오는 경우가 많아요",
          "speaker_id": "1",
          "time": "20210903 12:08"
        },
        {
          "id": "MDRW2100029682.1.33",
          "form": "솔직히 학부생 입장에선 많이 혼란스러웠던거 같아요",
          "original_form": "솔직히 학부생 입장에선 많이 혼란스러웠던거 같아요",
          "speaker_id": "1",
          "time": "20210903 12:09"
        },
        {
          "id": "MDRW2100029682.1.34",
          "form": "근데 1,2 니까 한번쯤은 들으세요",
          "original_form": "근데 1,2 니까 한번쯤은 들으세요",
          "speaker_id": "1",
          "time": "20210903 12:10"
        },
        {
          "id": "MDRW2100029682.1.35",
          "form": "아직 좀더 고민해봐야겠네요ㅜㅠ",
          "original_form": "아직 좀더 고민해봐야겠네요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210903 20:58"
        },
        {
          "id": "MDRW2100029682.1.36",
          "form": "아직 정정기간이니까 많이 생각해보고 결정하셔요",
          "original_form": "아직 정정기간이니까 많이 생각해보고 결정하셔요",
