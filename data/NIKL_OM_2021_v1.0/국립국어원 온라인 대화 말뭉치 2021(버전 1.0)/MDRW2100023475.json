      "utterance": [
        {
          "id": "MDRW2100023475.1.1",
          "form": "올림픽 보셨나요?",
          "original_form": "올림픽 보셨나요?",
          "speaker_id": "1",
          "time": "20210830 08:03"
        },
        {
          "id": "MDRW2100023475.1.2",
          "form": "아니요 ㅎㅎ..",
          "original_form": "아니요 ㅎㅎ..",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100023475.1.3",
          "form": "이스포츠는 좋아하는데",
          "original_form": "이스포츠는 좋아하는데",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100023475.1.4",
          "form": "올림픽 이번엔 못보ㅓㅆ어요ㅠ",
          "original_form": "올림픽 이번엔 못보ㅓㅆ어요ㅠ",
          "speaker_id": "2",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100023475.1.5",
          "form": "악!! 왜요?? ㅠ",
          "original_form": "악!! 왜요?? ㅠ",
          "speaker_id": "1",
          "time": "20210902 07:48"
        },
        {
          "id": "MDRW2100023475.1.6",
          "form": "집에 티비가 없으니까 잘 안보게되더라구요ㅠㅠ",
          "original_form": "집에 티비가 없으니까 잘 안보게되더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 14:19"
        },
        {
          "id": "MDRW2100023475.1.7",
          "form": "이런.. ㅠ 티비 없으면 아무래도 보기 어렵죠. 그럼 어떤 스포츠 좋아하세요?",
          "original_form": "이런.. ㅠ 티비 없으면 아무래도 보기 어렵죠. 그럼 어떤 스포츠 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100023475.1.8",
          "form": "맞아요ㅜㅠㅠ",
          "original_form": "맞아요ㅜㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100023475.1.9",
          "form": "저는 이제 축구에 관심을 가져볼까해요!",
          "original_form": "저는 이제 축구에 관심을 가져볼까해요!",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100023475.1.10",
          "form": "국내 축구요??",
          "original_form": "국내 축구요??",
          "speaker_id": "1",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100023475.1.11",
          "form": "아뇨 해외축구요!",
          "original_form": "아뇨 해외축구요!",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100023475.1.12",
          "form": "혹시 잘 아시나요?",
          "original_form": "혹시 잘 아시나요?",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100023475.1.13",
          "form": "아뇨~ 신랑이 결혼전에 좋아했대요",
          "original_form": "아뇨~ 신랑이 결혼전에 좋아했대요",
          "speaker_id": "1",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100023475.1.14",
          "form": "응원하는 팀 있나요?",
          "original_form": "응원하는 팀 있나요?",
          "speaker_id": "1",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100023475.1.15",
          "form": "오오 저 잘 몰라요 ㅎㅎ",
          "original_form": "오오 저 잘 몰라요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100023475.1.16",
          "form": "손흥민뿐",
          "original_form": "손흥민뿐",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100023475.1.17",
          "form": "저도 손흥민은 알아요 ㅎ",
          "original_form": "저도 손흥민은 알아요 ㅎ",
          "speaker_id": "1",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100023475.1.18",
          "form": "추천하는 팀이 있을까요?",
          "original_form": "추천하는 팀이 있을까요?",
          "speaker_id": "2",
          "time": "20210907 19:50"
        },
        {
          "id": "MDRW2100023475.1.19",
