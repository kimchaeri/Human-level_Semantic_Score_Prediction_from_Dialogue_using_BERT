      "utterance": [
        {
          "id": "MDRW2100001766.1.1",
          "form": "안녕하십니까",
          "original_form": "안녕하십니까",
          "speaker_id": "1",
          "time": "20210819 19:01"
        },
        {
          "id": "MDRW2100001766.1.2",
          "form": "예아 안녕하십니까",
          "original_form": "예아 안녕하십니까",
          "speaker_id": "2",
          "time": "20210819 19:02"
        },
        {
          "id": "MDRW2100001766.1.3",
          "form": "전 자취생인데 자취하십니까?",
          "original_form": "전 자취생인데 자취하십니까?",
          "speaker_id": "1",
          "time": "20210819 19:03"
        },
        {
          "id": "MDRW2100001766.1.4",
          "form": "아뇨 대학때 자취하다가 졸업후엔  가족들과 지내네요",
          "original_form": "아뇨 대학때 자취하다가 졸업후엔  가족들과 지내네요",
          "speaker_id": "2",
          "time": "20210819 19:04"
        },
        {
          "id": "MDRW2100001766.1.5",
          "form": "자취 몇년차세요?",
          "original_form": "자취 몇년차세요?",
          "speaker_id": "2",
          "time": "20210819 19:04"
        },
        {
          "id": "MDRW2100001766.1.6",
          "form": "아직 6개월차입니다.",
          "original_form": "아직 6개월차입니다.",
          "speaker_id": "1",
          "time": "20210819 19:04"
        },
        {
          "id": "MDRW2100001766.1.7",
          "form": "한창 귀차니즘이 심해질때죠",
          "original_form": "한창 귀차니즘이 심해질때죠",
          "speaker_id": "1",
          "time": "20210819 19:05"
        },
        {
          "id": "MDRW2100001766.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 19:07"
        },
        {
          "id": "MDRW2100001766.1.9",
          "form": "지금쯤이면 이제 음식은 배달음식 아닌가요?ㅋㅋㅋ",
          "original_form": "지금쯤이면 이제 음식은 배달음식 아닌가요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 19:07"
        },
        {
          "id": "MDRW2100001766.1.10",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "1",
          "time": "20210819 19:07"
        },
        {
          "id": "MDRW2100001766.1.11",
          "form": "세상에 배달음식 말고 다른 음식이 있나요?",
          "original_form": "세상에 배달음식 말고 다른 음식이 있나요?",
          "speaker_id": "1",
          "time": "20210819 19:08"
        },
        {
          "id": "MDRW2100001766.1.12",
          "form": "맙소사",
          "original_form": "맙소사",
          "speaker_id": "1",
          "time": "20210819 19:08"
        },
        {
          "id": "MDRW2100001766.1.13",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210819 19:08"
        },
        {
          "id": "MDRW2100001766.1.14",
          "form": "배우신 분이셨네요",
          "original_form": "배우신 분이셨네요",
          "speaker_id": "2",
          "time": "20210819 19:08"
        },
        {
          "id": "MDRW2100001766.1.15",
          "form": "배달 음식 최고..",
          "original_form": "배달 음식 최고..",
          "speaker_id": "2",
          "time": "20210819 19:09"
        },
        {
          "id": "MDRW2100001766.1.16",
          "form": "자취하면서 느낀거지만",
          "original_form": "자취하면서 느낀거지만",
          "speaker_id": "1",
          "time": "20210819 19:10"
        },
        {
          "id": "MDRW2100001766.1.17",
          "form": "배달 없을땐 어떻게 살았나 싶네요",
          "original_form": "배달 없을땐 어떻게 살았나 싶네요",
          "speaker_id": "1",
          "time": "20210819 19:10"
        },
        {
          "id": "MDRW2100001766.1.18",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210819 19:10"
        },
        {
          "id": "MDRW2100001766.1.19",
          "form": "코로나 사태로 유일하게 좋은점이 배달이에요 ㅋㅋㅋ",
          "original_form": "코로나 사태로 유일하게 좋은점이 배달이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 19:11"
        },
        {
          "id": "MDRW2100001766.1.20",
          "form": "1인가족아 많아지면서 1인세트도 많아지고",
          "original_form": "1인가족아 많아지면서 1인세트도 많아지고",
          "speaker_id": "2",
          "time": "20210819 19:11"
        },
        {
          "id": "MDRW2100001766.1.21",
          "form": "배민 만든 사람은 진짜 상 줘야됩니다",
          "original_form": "배민 만든 사람은 진짜 상 줘야됩니다",
          "speaker_id": "1",
          "time": "20210819 19:13"
        },
        {
          "id": "MDRW2100001766.1.22",
          "form": "요리 좋아하시나요 혹시?",
          "original_form": "요리 좋아하시나요 혹시?",
          "speaker_id": "1",
          "time": "20210819 19:13"
        },
        {
          "id": "MDRW2100001766.1.23",
          "form": "저는 집안일 자체를 안좋아해서 요리를 잘 안하는 편입니다",
          "original_form": "저는 집안일 자체를 안좋아해서 요리를 잘 안하는 편입니다",
          "speaker_id": "1",
          "time": "20210819 19:29"
        },
        {
          "id": "MDRW2100001766.1.24",
          "form": "요리 할땐 재밌게하는데 시작하기가 힘드네요",
          "original_form": "요리 할땐 재밌게하는데 시작하기가 힘드네요",
          "speaker_id": "2",
          "time": "20210819 20:55"
        },
        {
          "id": "MDRW2100001766.1.25",
          "form": "손이 느려서 너무 오래 걸려서 ㅋㅋㅋ",
          "original_form": "손이 느려서 너무 오래 걸려서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:55"
        },
        {
          "id": "MDRW2100001766.1.26",
          "form": "대신 라면은 잘 끓여요! 자취 5년차에 라면 고수 반열에 올랐습니다",
          "original_form": "대신 라면은 잘 끓여요! 자취 5년차에 라면 고수 반열에 올랐습니다",
          "speaker_id": "2",
          "time": "20210819 20:55"
        },
        {
          "id": "MDRW2100001766.1.27",
          "form": "내공이 여기까지 느껴집니다.",
          "original_form": "내공이 여기까지 느껴집니다.",
          "speaker_id": "1",
          "time": "20210819 20:56"
        },
        {
          "id": "MDRW2100001766.1.28",
          "form": "라면이 거의 5성 호텔급 맛일것같네요",
          "original_form": "라면이 거의 5성 호텔급 맛일것같네요",
          "speaker_id": "1",
          "time": "20210819 20:56"
        },
        {
          "id": "MDRW2100001766.1.29",
          "form": "제가 없어서",
          "original_form": "제가 없어서",
          "speaker_id": "2",
          "time": "20210819 20:59"
        },
        {
          "id": "MDRW2100001766.1.30",
          "form": "5성 호텔에서도 못 드시는 맛이에요",
          "original_form": "5성 호텔에서도 못 드시는 맛이에요",
          "speaker_id": "2",
          "time": "20210819 20:59"
        },
        {
          "id": "MDRW2100001766.1.31",
          "form": "평생 못먹어볼 맛이겠네요",
          "original_form": "평생 못먹어볼 맛이겠네요",
          "speaker_id": "1",
          "time": "20210819 21:01"
        },
        {
          "id": "MDRW2100001766.1.32",
          "form": "죽는순간까지 궁금할 것 같습니다",
          "original_form": "죽는순간까지 궁금할 것 같습니다",
          "speaker_id": "1",
          "time": "20210819 21:01"
        },
        {
          "id": "MDRW2100001766.1.33",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "1",
          "time": "20210819 21:01"
        },
        {
          "id": "MDRW2100001766.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ인터넷이나 어디엔가 라면 맛있다고 소문이 나면 저니까 찾아와서 드세오 ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ인터넷이나 어디엔가 라면 맛있다고 소문이 나면 저니까 찾아와서 드세오 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 21:02"
        },
        {
          "id": "MDRW2100001766.1.35",
          "form": "예의주시하고있겠습니다",
          "original_form": "예의주시하고있겠습니다",
          "speaker_id": "1",
          "time": "20210819 21:11"
        }
      ]
