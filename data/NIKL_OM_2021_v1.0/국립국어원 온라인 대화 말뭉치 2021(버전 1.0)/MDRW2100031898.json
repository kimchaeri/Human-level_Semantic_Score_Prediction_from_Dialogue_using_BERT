      "utterance": [
        {
          "id": "MDRW2100031898.1.1",
          "form": "학창시절 추억 있나요",
          "original_form": "학창시절 추억 있나요",
          "speaker_id": "1",
          "time": "20210905 07:52"
        },
        {
          "id": "MDRW2100031898.1.2",
          "form": "많아요. 넘재미있는",
          "original_form": "많아요. 넘재미있는",
          "speaker_id": "2",
          "time": "20210905 07:54"
        },
        {
          "id": "MDRW2100031898.1.3",
          "form": "그러시군요 어떤게 기억나시나요",
          "original_form": "그러시군요 어떤게 기억나시나요",
          "speaker_id": "1",
          "time": "20210905 08:44"
        },
        {
          "id": "MDRW2100031898.1.4",
          "form": "중고대모두 재미나게다녔어요",
          "original_form": "중고대모두 재미나게다녔어요",
          "speaker_id": "2",
          "time": "20210905 09:22"
        },
        {
          "id": "MDRW2100031898.1.5",
          "form": "중학교때는 별로인듯..생각해보니",
          "original_form": "중학교때는 별로인듯..생각해보니",
          "speaker_id": "2",
          "time": "20210905 09:22"
        },
        {
          "id": "MDRW2100031898.1.6",
          "form": "고등학교가 더 재밌으셨나요?",
          "original_form": "고등학교가 더 재밌으셨나요?",
          "speaker_id": "1",
          "time": "20210905 13:31"
        },
        {
          "id": "MDRW2100031898.1.7",
          "form": "대학이제일잼나고",
          "original_form": "대학이제일잼나고",
          "speaker_id": "2",
          "time": "20210905 13:49"
        },
        {
          "id": "MDRW2100031898.1.8",
          "form": "그다음고등학교요",
          "original_form": "그다음고등학교요",
          "speaker_id": "2",
          "time": "20210905 13:49"
        },
        {
          "id": "MDRW2100031898.1.9",
          "form": "그렇죠 저도 대학시절이 재밌었어요",
          "original_form": "그렇죠 저도 대학시절이 재밌었어요",
          "speaker_id": "1",
          "time": "20210905 14:02"
        },
        {
          "id": "MDRW2100031898.1.10",
          "form": "자유롭고 시간도많고",
          "original_form": "자유롭고 시간도많고",
          "speaker_id": "2",
          "time": "20210905 14:03"
        },
        {
          "id": "MDRW2100031898.1.11",
          "form": "맞아요 자유가 많아진게 참 좋았죠",
          "original_form": "맞아요 자유가 많아진게 참 좋았죠",
          "speaker_id": "1",
          "time": "20210905 14:04"
        },
        {
          "id": "MDRW2100031898.1.12",
          "form": "전공은뭐하셨어요?",
          "original_form": "전공은뭐하셨어요?",
          "speaker_id": "2",
          "time": "20210905 14:05"
        },
        {
          "id": "MDRW2100031898.1.13",
          "form": "전자쪽이요",
          "original_form": "전자쪽이요",
          "speaker_id": "1",
          "time": "20210905 14:07"
        },
        {
          "id": "MDRW2100031898.1.14",
          "form": "이과군요",
          "original_form": "이과군요",
          "speaker_id": "2",
          "time": "20210905 14:53"
        },
        {
          "id": "MDRW2100031898.1.15",
          "form": "전문과요",
          "original_form": "전문과요",
          "speaker_id": "2",
          "time": "20210905 14:53"
        },
        {
          "id": "MDRW2100031898.1.16",
          "form": "무슨 전공이요ㅡ",
          "original_form": "무슨 전공이요ㅡ",
