      "utterance": [
        {
          "id": "MDRW2100002526.1.1",
          "form": "요새 즐겨보는 프로그램 있으세요?",
          "original_form": "요새 즐겨보는 프로그램 있으세요?",
          "speaker_id": "1",
          "time": "20210820 13:13"
        },
        {
          "id": "MDRW2100002526.1.2",
          "form": "저는 티비를 잘 안봐서",
          "original_form": "저는 티비를 잘 안봐서",
          "speaker_id": "2",
          "time": "20210820 13:16"
        },
        {
          "id": "MDRW2100002526.1.3",
          "form": "생각나는게없네요",
          "original_form": "생각나는게없네요",
          "speaker_id": "2",
          "time": "20210820 13:16"
        },
        {
          "id": "MDRW2100002526.1.4",
          "form": "순간님은요?",
          "original_form": "순간님은요?",
          "speaker_id": "2",
          "time": "20210820 13:16"
        },
        {
          "id": "MDRW2100002526.1.5",
          "form": "저는 슬의생이요ㅋㅋ",
          "original_form": "저는 슬의생이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 13:17"
        },
        {
          "id": "MDRW2100002526.1.6",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210820 13:17"
        },
        {
          "id": "MDRW2100002526.1.7",
          "form": "그드라마",
          "original_form": "그드라마",
          "speaker_id": "2",
          "time": "20210820 13:17"
        },
        {
          "id": "MDRW2100002526.1.8",
          "form": "인기정말많더라구요",
          "original_form": "인기정말많더라구요",
          "speaker_id": "2",
          "time": "20210820 13:17"
        },
        {
          "id": "MDRW2100002526.1.9",
          "form": "재밌나요",
          "original_form": "재밌나요",
          "speaker_id": "2",
          "time": "20210820 13:17"
        },
        {
          "id": "MDRW2100002526.1.10",
          "form": "매회가 너무 갬동이에요 ㅠㅠ",
          "original_form": "매회가 너무 갬동이에요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 13:17"
        },
        {
          "id": "MDRW2100002526.1.11",
          "form": "저도 나중에 기회되면",
          "original_form": "저도 나중에 기회되면",
          "speaker_id": "2",
          "time": "20210820 13:18"
        },
        {
          "id": "MDRW2100002526.1.12",
          "form": "봐야겠어요",
          "original_form": "봐야겠어요",
          "speaker_id": "2",
          "time": "20210820 13:18"
        },
        {
          "id": "MDRW2100002526.1.13",
          "form": "저는 마지막으로 본 드라마가 뭔지",
          "original_form": "저는 마지막으로 본 드라마가 뭔지",
          "speaker_id": "2",
          "time": "20210820 13:18"
        },
        {
          "id": "MDRW2100002526.1.14",
          "form": "네 진짜 꼭 보세요ㅋㅋ\r\n두번보세용",
          "original_form": "네 진짜 꼭 보세요ㅋㅋ\r\n두번보세용",
          "speaker_id": "1",
          "time": "20210820 13:18"
        },
        {
          "id": "MDRW2100002526.1.15",
          "form": "기억도 안나네요",
          "original_form": "기억도 안나네요",
          "speaker_id": "2",
          "time": "20210820 13:18"
        },
        {
          "id": "MDRW2100002526.1.16",
          "form": "뭐였더라...알함브라 어쩌고였나...",
          "original_form": "뭐였더라...알함브라 어쩌고였나...",
          "speaker_id": "2",
          "time": "20210820 13:18"
        },
        {
          "id": "MDRW2100002526.1.17",
          "form": "영화는요?",
          "original_form": "영화는요?",
          "speaker_id": "1",
          "time": "20210820 13:18"
        },
        {
          "id": "MDRW2100002526.1.18",
          "form": "영화는 자주 봐요",
          "original_form": "영화는 자주 봐요",
          "speaker_id": "2",
          "time": "20210820 13:18"
        },
        {
          "id": "MDRW2100002526.1.19",
          "form": "며칠전엔 프리가이 봤고",
          "original_form": "며칠전엔 프리가이 봤고",
          "speaker_id": "2",
          "time": "20210820 13:19"
        },
        {
          "id": "MDRW2100002526.1.20",
          "form": "최근에 본 영화중엔 모가디슈",
          "original_form": "최근에 본 영화중엔 모가디슈",
          "speaker_id": "2",
          "time": "20210820 13:19"
        },
        {
          "id": "MDRW2100002526.1.21",
          "form": "넘좋았어요",
          "original_form": "넘좋았어요",
          "speaker_id": "2",
          "time": "20210820 13:19"
        },
        {
          "id": "MDRW2100002526.1.22",
          "form": "보셨나요혹시",
          "original_form": "보셨나요혹시",
          "speaker_id": "2",
          "time": "20210820 13:19"
        },
        {
          "id": "MDRW2100002526.1.23",
          "form": "아니요ㅠㅠ 둘다 못봤어요",
          "original_form": "아니요ㅠㅠ 둘다 못봤어요",
          "speaker_id": "1",
          "time": "20210820 13:19"
        },
        {
          "id": "MDRW2100002526.1.24",
          "form": "둘다 좋으셨나여?",
          "original_form": "둘다 좋으셨나여?",
          "speaker_id": "1",
          "time": "20210820 13:19"
        },
        {
          "id": "MDRW2100002526.1.25",
          "form": "프리가이는 그냥..가볍게 보기 좋았고",
          "original_form": "프리가이는 그냥..가볍게 보기 좋았고",
          "speaker_id": "2",
          "time": "20210820 13:20"
        },
        {
          "id": "MDRW2100002526.1.26",
          "form": "모가디슈는 코로나아녔으면 천만 찍었겠다 싶을 정도로",
          "original_form": "모가디슈는 코로나아녔으면 천만 찍었겠다 싶을 정도로",
          "speaker_id": "2",
          "time": "20210820 13:20"
        },
        {
          "id": "MDRW2100002526.1.27",
          "form": "영화가 좋더라구요",
          "original_form": "영화가 좋더라구요",
          "speaker_id": "2",
          "time": "20210820 13:20"
        },
        {
          "id": "MDRW2100002526.1.28",
          "form": "와 그 정도인가요?ㅋㅋ\r\n저 메모메모 할게요\r\n꼭 보겠어요!!",
          "original_form": "와 그 정도인가요?ㅋㅋ\r\n저 메모메모 할게요\r\n꼭 보겠어요!!",
          "speaker_id": "1",
          "time": "20210820 13:20"
        },
        {
          "id": "MDRW2100002526.1.29",
          "form": "근데..사람마다...취향이 다른거라..!!!!",
          "original_form": "근데..사람마다...취향이 다른거라..!!!!",
          "speaker_id": "2",
          "time": "20210820 13:21"
        },
        {
          "id": "MDRW2100002526.1.30",
          "form": "기대하셨다가 실망하실까봐",
          "original_form": "기대하셨다가 실망하실까봐",
          "speaker_id": "2",
          "time": "20210820 13:21"
        },
        {
          "id": "MDRW2100002526.1.31",
          "form": "걱정되네요ㅋㅋㅋ",
          "original_form": "걱정되네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 13:21"
        },
        {
          "id": "MDRW2100002526.1.32",
          "form": "에이ㅋㅋ 괜찮아요ㅋㅋ",
          "original_form": "에이ㅋㅋ 괜찮아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 13:21"
        },
        {
          "id": "MDRW2100002526.1.33",
          "form": "암튼 못 만든 영화는 아녜여ㅋㅋㅋ",
          "original_form": "암튼 못 만든 영화는 아녜여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 13:21"
        }
      ]
    }
