      "utterance": [
        {
          "id": "MDRW2100037456.1.1",
          "form": "전 일본 편의점 푸딩을 좋아하는데 그건 빈은 아닐거에요",
          "original_form": "전 일본 편의점 푸딩을 좋아하는데 그건 빈은 아닐거에요",
          "speaker_id": "2",
          "time": "20210901 00:17"
        },
        {
          "id": "MDRW2100037456.1.2",
          "form": "아 그렇군요ㅎ",
          "original_form": "아 그렇군요ㅎ",
          "speaker_id": "1",
          "time": "20210901 00:17"
        },
        {
          "id": "MDRW2100037456.1.3",
          "form": "일본 편의점 푸딩 맛있겠어요ㅠ",
          "original_form": "일본 편의점 푸딩 맛있겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210901 00:17"
        },
        {
          "id": "MDRW2100037456.1.4",
          "form": "맛도 다양하고 그렇겠죠?",
          "original_form": "맛도 다양하고 그렇겠죠?",
          "speaker_id": "1",
          "time": "20210901 00:18"
        },
        {
          "id": "MDRW2100037456.1.5",
          "form": "그랬지만 코로나 이후엔 다 환상의 ㅋㅋ",
          "original_form": "그랬지만 코로나 이후엔 다 환상의 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 00:18"
        },
        {
          "id": "MDRW2100037456.1.6",
          "form": "먼 옛날 같네요",
          "original_form": "먼 옛날 같네요",
          "speaker_id": "2",
          "time": "20210901 00:18"
        },
        {
          "id": "MDRW2100037456.1.7",
          "form": "예전엔 여수나 이런데 가서 자고 차타고 갈 비용이면 일본 도쿄가서 싼 숙소가고 맛집 두세개 찍구 왔는데",
          "original_form": "예전엔 여수나 이런데 가서 자고 차타고 갈 비용이면 일본 도쿄가서 싼 숙소가고 맛집 두세개 찍구 왔는데",
          "speaker_id": "2",
          "time": "20210901 00:18"
        },
        {
          "id": "MDRW2100037456.1.8",
          "form": "코로나 진짜 넘해요ㅠ 일본 자주 가셨나봐요??",
          "original_form": "코로나 진짜 넘해요ㅠ 일본 자주 가셨나봐요??",
          "speaker_id": "1",
          "time": "20210901 00:25"
        },
        {
          "id": "MDRW2100037456.1.9",
          "form": "자주는 못가고 몇번 갔어요 ㅋㅋ",
          "original_form": "자주는 못가고 몇번 갔어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 00:26"
        },
        {
          "id": "MDRW2100037456.1.10",
          "form": "하지만 코로나 이후엔 어떻게 될지",
          "original_form": "하지만 코로나 이후엔 어떻게 될지",
          "speaker_id": "2",
          "time": "20210901 00:26"
        },
        {
          "id": "MDRW2100037456.1.11",
          "form": "일본 맛집이 협찬 바이럴 광고가 없어서",
          "original_form": "일본 맛집이 협찬 바이럴 광고가 없어서",
          "speaker_id": "2",
          "time": "20210901 00:27"
        },
        {
          "id": "MDRW2100037456.1.12",
          "form": "네이버에서 검색시 솔직한 후기가 뜹니다",
          "original_form": "네이버에서 검색시 솔직한 후기가 뜹니다",
          "speaker_id": "2",
          "time": "20210901 00:27"
        },
        {
          "id": "MDRW2100037456.1.13",
          "form": "전 일본 어릴때 한번 간 이후로 또 안간게 후회되네요ㅠ",
          "original_form": "전 일본 어릴때 한번 간 이후로 또 안간게 후회되네요ㅠ",
          "speaker_id": "1",
          "time": "20210901 00:29"
        },
        {
          "id": "MDRW2100037456.1.14",
          "form": "일본 편의점 정말 궁금하거든요",
          "original_form": "일본 편의점 정말 궁금하거든요",
          "speaker_id": "1",
          "time": "20210901 00:29"
        },
        {
          "id": "MDRW2100037456.1.15",
          "form": "좋아요 그리고 엔화약세일땐 한국보다 저렴핬는데",
          "original_form": "좋아요 그리고 엔화약세일땐 한국보다 저렴핬는데",
          "speaker_id": "2",
          "time": "20210901 11:30"
        },
        {
          "id": "MDRW2100037456.1.16",
          "form": "지금은 어떤지 모르겠네요 어차피 못가니깐ㅋㅋ",
          "original_form": "지금은 어떤지 모르겠네요 어차피 못가니깐ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 11:30"
        },
        {
          "id": "MDRW2100037456.1.17",
          "form": "그렇군요 근데 일본 맛집이 네이버에 협찬바이럴 광고가 없는진 몰랐네요",
          "original_form": "그렇군요 근데 일본 맛집이 네이버에 협찬바이럴 광고가 없는진 몰랐네요",
          "speaker_id": "1",
          "time": "20210901 13:15"
        },
        {
          "id": "MDRW2100037456.1.18",
          "form": "한국인들 자주가는 대만만해도 장난 아니던데요ㅠ",
          "original_form": "한국인들 자주가는 대만만해도 장난 아니던데요ㅠ",
          "speaker_id": "1",
          "time": "20210901 13:16"
        },
        {
          "id": "MDRW2100037456.1.19",
          "form": "와 대만도 바이럴 광고가 있나요?",
          "original_form": "와 대만도 바이럴 광고가 있나요?",
          "speaker_id": "2",
          "time": "20210901 13:21"
        },
        {
          "id": "MDRW2100037456.1.20",
          "form": "그래도 외국은 레뷰 협찬 이런건 없었는디,,",
          "original_form": "그래도 외국은 레뷰 협찬 이런건 없었는디,,",
          "speaker_id": "2",
          "time": "20210901 13:22"
        },
        {
          "id": "MDRW2100037456.1.21",
          "form": "아 제가 괜한 의심을 한걸지도ㅎ",
          "original_form": "아 제가 괜한 의심을 한걸지도ㅎ",
          "speaker_id": "1",
          "time": "20210901 13:30"
        },
        {
          "id": "MDRW2100037456.1.22",
          "form": "그래도 울나라만 할까요ㅎ",
          "original_form": "그래도 울나라만 할까요ㅎ",
          "speaker_id": "1",
          "time": "20210901 13:31"
        },
        {
          "id": "MDRW2100037456.1.23",
          "form": "한국 음식점은 네이버 블로그 포스팅의 반이상이 혐찬이라서",
          "original_form": "한국 음식점은 네이버 블로그 포스팅의 반이상이 혐찬이라서",
          "speaker_id": "2",
          "time": "20210901 13:38"
        },
        {
          "id": "MDRW2100037456.1.24",
          "form": "진짜 리뷰찾기가 더 힘들지경이에욬",
          "original_form": "진짜 리뷰찾기가 더 힘들지경이에욬",
          "speaker_id": "2",
          "time": "20210901 13:39"
        },
        {
          "id": "MDRW2100037456.1.25",
          "form": "저도 맛집 찾을때 개고생이더라고요ㅋㅋㅋ",
          "original_form": "저도 맛집 찾을때 개고생이더라고요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:44"
        },
        {
          "id": "MDRW2100037456.1.26",
          "form": "여기 괜찮으려나 보다가 젤아래 내리면 협찬문구 뜰때마다 허탈해요",
          "original_form": "여기 괜찮으려나 보다가 젤아래 내리면 협찬문구 뜰때마다 허탈해요",
          "speaker_id": "1",
          "time": "20210901 13:44"
        },
        {
          "id": "MDRW2100037456.1.27",
          "form": "협찬을 글자로 치면 검색되니깐",
          "original_form": "협찬을 글자로 치면 검색되니깐",
          "speaker_id": "2",
          "time": "20210901 16:34"
        },
        {
          "id": "MDRW2100037456.1.28",
          "form": "이미지로 꼭 넣잖아요",
          "original_form": "이미지로 꼭 넣잖아요",
          "speaker_id": "2",
          "time": "20210901 16:34"
        }
      ]
