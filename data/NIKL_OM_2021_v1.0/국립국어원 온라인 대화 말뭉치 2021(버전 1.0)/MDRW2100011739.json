      "utterance": [
        {
          "id": "MDRW2100011739.1.1",
          "form": "봉사해보신적 있으신가요?",
          "original_form": "봉사해보신적 있으신가요?",
          "speaker_id": "1",
          "time": "20210824 15:37"
        },
        {
          "id": "MDRW2100011739.1.2",
          "form": "네 아주조금요",
          "original_form": "네 아주조금요",
          "speaker_id": "2",
          "time": "20210824 15:38"
        },
        {
          "id": "MDRW2100011739.1.3",
          "form": "대학간다고 한거랑 급식봉사요",
          "original_form": "대학간다고 한거랑 급식봉사요",
          "speaker_id": "2",
          "time": "20210824 15:38"
        },
        {
          "id": "MDRW2100011739.1.4",
          "form": "개인으로 하신건가요?",
          "original_form": "개인으로 하신건가요?",
          "speaker_id": "1",
          "time": "20210824 15:40"
        },
        {
          "id": "MDRW2100011739.1.5",
          "form": "급식봉사는 로스쿨다닐때 해봤어요",
          "original_form": "급식봉사는 로스쿨다닐때 해봤어요",
          "speaker_id": "2",
          "time": "20210824 15:41"
        },
        {
          "id": "MDRW2100011739.1.6",
          "form": "수미님 엘리트 이신가요..?",
          "original_form": "수미님 엘리트 이신가요..?",
          "speaker_id": "1",
          "time": "20210824 15:42"
        },
        {
          "id": "MDRW2100011739.1.7",
          "form": "엘리트까진 아니고요 ㅋㅋ",
          "original_form": "엘리트까진 아니고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:42"
        },
        {
          "id": "MDRW2100011739.1.8",
          "form": "저는 청소년센터에서 봉사해봤어요~",
          "original_form": "저는 청소년센터에서 봉사해봤어요~",
          "speaker_id": "1",
          "time": "20210824 15:42"
        },
        {
          "id": "MDRW2100011739.1.9",
          "form": "개인으로 하신거에요?",
          "original_form": "개인으로 하신거에요?",
          "speaker_id": "2",
          "time": "20210824 15:42"
        },
        {
          "id": "MDRW2100011739.1.10",
          "form": "네 시청에 공지뜬거보고 지원했어요~",
          "original_form": "네 시청에 공지뜬거보고 지원했어요~",
          "speaker_id": "1",
          "time": "20210824 15:43"
        },
        {
          "id": "MDRW2100011739.1.11",
          "form": "해보신 소감 궁금해요",
          "original_form": "해보신 소감 궁금해요",
          "speaker_id": "2",
          "time": "20210824 15:44"
        },
        {
          "id": "MDRW2100011739.1.12",
          "form": "저는 이제서야 보육원 봉사해보고 싶던데 코로나가 안끝나네요",
          "original_form": "저는 이제서야 보육원 봉사해보고 싶던데 코로나가 안끝나네요",
          "speaker_id": "2",
          "time": "20210824 15:44"
        },
        {
          "id": "MDRW2100011739.1.13",
          "form": "저는 코로나 전에 했는데 검정고시 가르치는 봉사였어요",
          "original_form": "저는 코로나 전에 했는데 검정고시 가르치는 봉사였어요",
          "speaker_id": "1",
          "time": "20210824 15:45"
        },
        {
          "id": "MDRW2100011739.1.14",
          "form": "애들이 다 합격해서 뿌듯했네요",
          "original_form": "애들이 다 합격해서 뿌듯했네요",
          "speaker_id": "1",
          "time": "20210824 15:45"
        },
        {
          "id": "MDRW2100011739.1.15",
          "form": "오 좋네요",
          "original_form": "오 좋네요",
          "speaker_id": "2",
          "time": "20210824 15:45"
        },
        {
          "id": "MDRW2100011739.1.16",
          "form": "그것도 시청에서 아신거에요?",
          "original_form": "그것도 시청에서 아신거에요?",
          "speaker_id": "2",
          "time": "20210824 15:46"
        },
        {
          "id": "MDRW2100011739.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210824 15:51"
        },
        {
          "id": "MDRW2100011739.1.18",
          "form": "청소년센터에서 검정고시 가르친거예요ㅋㅋ",
