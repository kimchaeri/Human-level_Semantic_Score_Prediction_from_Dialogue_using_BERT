      "utterance": [
        {
          "id": "MDRW2100000513.1.1",
          "form": "게임좋아하세요? 저는 롤 좋아해요",
          "original_form": "게임좋아하세요? 저는 롤 좋아해요",
          "speaker_id": "1",
          "time": "20210819 16:31"
        },
        {
          "id": "MDRW2100000513.1.2",
          "form": "아 저는 게임을 안해요 마지막 게임이 카트라이더...",
          "original_form": "아 저는 게임을 안해요 마지막 게임이 카트라이더...",
          "speaker_id": "2",
          "time": "20210819 16:32"
        },
        {
          "id": "MDRW2100000513.1.3",
          "form": "아 저도 카트라이더 해봤어요 대학생때 ㅋㅋ 거의 아이템전만 했던",
          "original_form": "아 저도 카트라이더 해봤어요 대학생때 ㅋㅋ 거의 아이템전만 했던",
          "speaker_id": "1",
          "time": "20210819 16:33"
        },
        {
          "id": "MDRW2100000513.1.4",
          "form": "저도 아주 오래전에 ㅋㅋ",
          "original_form": "저도 아주 오래전에 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:34"
        },
        {
          "id": "MDRW2100000513.1.5",
          "form": "롤은 어떤 게임이에요? 이름만 들어봐서",
          "original_form": "롤은 어떤 게임이에요? 이름만 들어봐서",
          "speaker_id": "2",
          "time": "20210819 16:34"
        },
        {
          "id": "MDRW2100000513.1.6",
          "form": "5:5로 팀짜서 대결하는 게임인데",
          "original_form": "5:5로 팀짜서 대결하는 게임인데",
          "speaker_id": "1",
          "time": "20210819 16:35"
        },
        {
          "id": "MDRW2100000513.1.7",
          "form": "엄청유명해요 리그도 있어요!",
          "original_form": "엄청유명해요 리그도 있어요!",
          "speaker_id": "1",
          "time": "20210819 16:35"
        },
        {
          "id": "MDRW2100000513.1.8",
          "form": "어떻게 하는 게임인줄 모르지만 페이커는 압니다 ㅋㅋ",
          "original_form": "어떻게 하는 게임인줄 모르지만 페이커는 압니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:36"
        },
        {
          "id": "MDRW2100000513.1.9",
          "form": "맞아요 페이커 ㅋㅋㅋㅋ진짜 유명하죠",
          "original_form": "맞아요 페이커 ㅋㅋㅋㅋ진짜 유명하죠",
          "speaker_id": "1",
          "time": "20210819 16:37"
        },
        {
          "id": "MDRW2100000513.1.10",
          "form": "저는 주변에도 게임하는 사람이 아무도 없어요",
          "original_form": "저는 주변에도 게임하는 사람이 아무도 없어요",
          "speaker_id": "2",
          "time": "20210819 16:38"
        },
        {
          "id": "MDRW2100000513.1.11",
          "form": "페이커는 게임 관심없어도 다들 알더라구요",
          "original_form": "페이커는 게임 관심없어도 다들 알더라구요",
          "speaker_id": "1",
          "time": "20210819 16:38"
        },
        {
          "id": "MDRW2100000513.1.12",
          "form": "앗 그럼 휴대폰 게임도 안하시나요?",
          "original_form": "앗 그럼 휴대폰 게임도 안하시나요?",
          "speaker_id": "1",
          "time": "20210819 16:38"
        },
        {
          "id": "MDRW2100000513.1.13",
          "form": "네 휴대폰 게임도 안 해요 ㅋㅋ 배그는 좀 재미있어보이긴 하던데",
          "original_form": "네 휴대폰 게임도 안 해요 ㅋㅋ 배그는 좀 재미있어보이긴 하던데",
          "speaker_id": "2",
          "time": "20210819 16:39"
        },
        {
          "id": "MDRW2100000513.1.14",
          "form": "저도 사실 제가 게임 직접 플레이하는거보다 게임 방송을 많이 보게 되더라구요 ㅎㅎ",
          "original_form": "저도 사실 제가 게임 직접 플레이하는거보다 게임 방송을 많이 보게 되더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 16:41"
        },
        {
          "id": "MDRW2100000513.1.15",
          "form": "배그도 재밋어보이던데 좀 멀미나던데요ㅠㅠ",
          "original_form": "배그도 재밋어보이던데 좀 멀미나던데요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 16:41"
        },
        {
          "id": "MDRW2100000513.1.16",
          "form": "아 그쵸 요즘은 게임 관련 방송 많이 보는거 같더라구요 직접 하는게 아닌데도 재미있나요 ?",
          "original_form": "아 그쵸 요즘은 게임 관련 방송 많이 보는거 같더라구요 직접 하는게 아닌데도 재미있나요 ?",
          "speaker_id": "2",
          "time": "20210819 16:44"
        },
        {
          "id": "MDRW2100000513.1.17",
          "form": "네 오히려 게임직접 하는거보다 더 재밌는 거 같아요",
          "original_form": "네 오히려 게임직접 하는거보다 더 재밌는 거 같아요",
          "speaker_id": "1",
          "time": "20210819 16:45"
        },
        {
          "id": "MDRW2100000513.1.18",
          "form": "오 그정도에요? 신기하네요 ㅋㅋ",
          "original_form": "오 그정도에요? 신기하네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:52"
        }
      ]
