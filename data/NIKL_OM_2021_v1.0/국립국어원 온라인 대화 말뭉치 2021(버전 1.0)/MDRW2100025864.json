      "utterance": [
        {
          "id": "MDRW2100025864.1.1",
          "form": "무슨 음료 좋아하나요",
          "original_form": "무슨 음료 좋아하나요",
          "speaker_id": "1",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100025864.1.2",
          "form": "딸기 좋아하시나요",
          "original_form": "딸기 좋아하시나요",
          "speaker_id": "2",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100025864.1.3",
          "form": "저는 깔라만시 좋아하죠",
          "original_form": "저는 깔라만시 좋아하죠",
          "speaker_id": "2",
          "time": "20210901 17:32"
        },
        {
          "id": "MDRW2100025864.1.4",
          "form": "딸기 좋아해요! 깔라만시 에이드 생각나네요",
          "original_form": "딸기 좋아해요! 깔라만시 에이드 생각나네요",
          "speaker_id": "1",
          "time": "20210901 17:32"
        },
        {
          "id": "MDRW2100025864.1.5",
          "form": "네 깔라만시 음료는 이제는 단종돼서 구하기 힘들어요",
          "original_form": "네 깔라만시 음료는 이제는 단종돼서 구하기 힘들어요",
          "speaker_id": "2",
          "time": "20210901 17:32"
        },
        {
          "id": "MDRW2100025864.1.6",
          "form": "딸기청 만들어 보셨나요?",
          "original_form": "딸기청 만들어 보셨나요?",
          "speaker_id": "2",
          "time": "20210901 17:32"
        },
        {
          "id": "MDRW2100025864.1.7",
          "form": "이제 필리핀도 못가니 먹기 힘들겠어요",
          "original_form": "이제 필리핀도 못가니 먹기 힘들겠어요",
          "speaker_id": "1",
          "time": "20210901 17:32"
        },
        {
          "id": "MDRW2100025864.1.8",
          "form": "떨기잼도요",
          "original_form": "떨기잼도요",
          "speaker_id": "2",
          "time": "20210901 17:32"
        },
        {
          "id": "MDRW2100025864.1.9",
          "form": "딸기청 말고 귤청 만들어봤어요 한라봉청",
          "original_form": "딸기청 말고 귤청 만들어봤어요 한라봉청",
          "speaker_id": "1",
          "time": "20210901 17:32"
        },
        {
          "id": "MDRW2100025864.1.10",
          "form": "네 코시국 끝나면 깔라만시 먹으러 가요",
          "original_form": "네 코시국 끝나면 깔라만시 먹으러 가요",
          "speaker_id": "2",
          "time": "20210901 17:32"
        },
        {
          "id": "MDRW2100025864.1.11",
          "form": "한라봉청 못지네요 이름",
          "original_form": "한라봉청 못지네요 이름",
          "speaker_id": "2",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025864.1.12",
          "form": "멋지네요",
          "original_form": "멋지네요",
          "speaker_id": "2",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025864.1.13",
          "form": "가리봉동청도 만드셨어요?",
          "original_form": "가리봉동청도 만드셨어요?",
          "speaker_id": "2",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025864.1.14",
          "form": "그건뭔가요 저는 요새 청귤청머겅요",
          "original_form": "그건뭔가요 저는 요새 청귤청머겅요",
          "speaker_id": "1",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025864.1.15",
          "form": "청귤청먹고있어요",
          "original_form": "청귤청먹고있어요",
          "speaker_id": "1",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025864.1.16",
          "form": "청귤은 파란색 귤인가요?",
          "original_form": "청귤은 파란색 귤인가요?",
          "speaker_id": "2",
          "time": "20210901 17:33"
        },
        {
          "id": "MDRW2100025864.1.17",
          "form": "넵 이 시즌에만 나오는 귤이라 귀합니당",
          "original_form": "넵 이 시즌에만 나오는 귤이라 귀합니당",
          "speaker_id": "1",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100025864.1.18",
          "form": "저녁뭐 드실건가요",
          "original_form": "저녁뭐 드실건가요",
          "speaker_id": "1",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100025864.1.19",
          "form": "저도 한잔만 주세요",
          "original_form": "저도 한잔만 주세요",
          "speaker_id": "2",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100025864.1.20",
          "form": "놀러오시면 드리겠습니당",
          "original_form": "놀러오시면 드리겠습니당",
          "speaker_id": "1",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100025864.1.21",
          "form": "피자 어제 시켜먹고 남은거 데펴 먹으렵니다",
          "original_form": "피자 어제 시켜먹고 남은거 데펴 먹으렵니다",
          "speaker_id": "2",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100025864.1.22",
          "form": "뭐드실건가요",
          "original_form": "뭐드실건가요",
          "speaker_id": "2",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100025864.1.23",
          "form": "오 무슨 피자 드셨나요",
          "original_form": "오 무슨 피자 드셨나요",
          "speaker_id": "1",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100025864.1.24",
          "form": "저 집에가서 애호박전이나 먹으려구요",
          "original_form": "저 집에가서 애호박전이나 먹으려구요",
          "speaker_id": "1",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100025864.1.25",
          "form": "불고기할라피뇨피자 너무 맛있었습니다",
          "original_form": "불고기할라피뇨피자 너무 맛있었습니다",
          "speaker_id": "2",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100025864.1.26",
          "form": "추천드립니다",
          "original_form": "추천드립니다",
          "speaker_id": "2",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100025864.1.27",
          "form": "오늘 저녁 뭐 먹었나요",
          "original_form": "오늘 저녁 뭐 먹었나요",
