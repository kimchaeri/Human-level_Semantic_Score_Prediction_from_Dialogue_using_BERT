      "utterance": [
        {
          "id": "MDRW2100030459.1.1",
          "form": "진상승객들 만나시면 어떻게 하시나요 ㅋㅋㅋ",
          "original_form": "진상승객들 만나시면 어떻게 하시나요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:24"
        },
        {
          "id": "MDRW2100030459.1.2",
          "form": "이것도 매뉴얼이 있겠죠 ㅋㅋ",
          "original_form": "이것도 매뉴얼이 있겠죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:24"
        },
        {
          "id": "MDRW2100030459.1.3",
          "form": "각보고 정해요 ㅋㅋ",
          "original_form": "각보고 정해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:24"
        },
        {
          "id": "MDRW2100030459.1.4",
          "form": "걍 보낼지",
          "original_form": "걍 보낼지",
          "speaker_id": "1",
          "time": "20210904 01:24"
        },
        {
          "id": "MDRW2100030459.1.5",
          "form": "경찰 부를지",
          "original_form": "경찰 부를지",
          "speaker_id": "1",
          "time": "20210904 01:24"
        },
        {
          "id": "MDRW2100030459.1.6",
          "form": "같이 싸울지ㅋㅋ",
          "original_form": "같이 싸울지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:24"
        },
        {
          "id": "MDRW2100030459.1.7",
          "form": "ㅋㅋㅋ 오래 일하다보면 감이 오겠어요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 오래 일하다보면 감이 오겠어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:24"
        },
        {
          "id": "MDRW2100030459.1.8",
          "form": "사실 쪼렙이라",
          "original_form": "사실 쪼렙이라",
          "speaker_id": "1",
          "time": "20210904 01:25"
        },
        {
          "id": "MDRW2100030459.1.9",
          "form": "4년차에요 ㅋㅋ",
          "original_form": "4년차에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:25"
        },
        {
          "id": "MDRW2100030459.1.10",
          "form": "약간 그시기가 그런시기 아닌가요 ㅋㅋㅋ",
          "original_form": "약간 그시기가 그런시기 아닌가요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:25"
        },
        {
          "id": "MDRW2100030459.1.11",
          "form": "직업에 한번씩 회의감을 가지는 ㅋㅋㅋㅋ",
          "original_form": "직업에 한번씩 회의감을 가지는 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:25"
        },
        {
          "id": "MDRW2100030459.1.12",
          "form": "결혼해서 ㅋㅋ 이직하기도 했고해서",
          "original_form": "결혼해서 ㅋㅋ 이직하기도 했고해서",
          "speaker_id": "1",
          "time": "20210904 01:34"
        },
        {
          "id": "MDRW2100030459.1.13",
          "form": "만족하는 편입니다 ㅋㅋ",
          "original_form": "만족하는 편입니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:34"
        },
        {
          "id": "MDRW2100030459.1.14",
          "form": "다행이네요! ㅋㅋ 직업만족도가 좋아야 삶의 만족도도 높아지는거 같습니다 ㅋㅋㅋㅋ",
          "original_form": "다행이네요! ㅋㅋ 직업만족도가 좋아야 삶의 만족도도 높아지는거 같습니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:35"
        },
        {
          "id": "MDRW2100030459.1.15",
          "form": "그쵸 ㅋㅋ 눈 좀 붙였다가 나왔너요",
          "original_form": "그쵸 ㅋㅋ 눈 좀 붙였다가 나왔너요",
          "speaker_id": "1",
          "time": "20210904 05:16"
        },
        {
          "id": "MDRW2100030459.1.16",
          "form": "와 엄청 일찍가시는거 아닌가요",
          "original_form": "와 엄청 일찍가시는거 아닌가요",
          "speaker_id": "2",
          "time": "20210904 11:22"
        },
        {
          "id": "MDRW2100030459.1.17",
          "form": "아 일하는중이었어요ㅋㅋ",
          "original_form": "아 일하는중이었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:00"
        },
        {
          "id": "MDRW2100030459.1.18",
          "form": "근무중 휴게시간이오 ㅋㅋ",
          "original_form": "근무중 휴게시간이오 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:00"
        },
        {
          "id": "MDRW2100030459.1.19",
          "form": "아아아 그러시구나 ㅋㅋㅋㅋ 저는 수업하러 출근했습니다...ㅎㅎ",
          "original_form": "아아아 그러시구나 ㅋㅋㅋㅋ 저는 수업하러 출근했습니다...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100030459.1.20",
          "form": "ㅋㅋ돈벌어서 고래 사셔야죠 ㅋㅋ",
