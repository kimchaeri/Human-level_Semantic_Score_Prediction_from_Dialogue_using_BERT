      "utterance": [
        {
          "id": "MDRW2100034835.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 20:34"
        },
        {
          "id": "MDRW2100034835.1.2",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210828 20:34"
        },
        {
          "id": "MDRW2100034835.1.3",
          "form": "최근에 산거있으신가요",
          "original_form": "최근에 산거있으신가요",
          "speaker_id": "2",
          "time": "20210828 20:34"
        },
        {
          "id": "MDRW2100034835.1.4",
          "form": "마우스를 바꿨어요",
          "original_form": "마우스를 바꿨어요",
          "speaker_id": "1",
          "time": "20210828 20:35"
        },
        {
          "id": "MDRW2100034835.1.5",
          "form": "최근에 산거 있으세요?",
          "original_form": "최근에 산거 있으세요?",
          "speaker_id": "1",
          "time": "20210828 20:35"
        },
        {
          "id": "MDRW2100034835.1.6",
          "form": "겨울옷 샀어요",
          "original_form": "겨울옷 샀어요",
          "speaker_id": "2",
          "time": "20210828 20:36"
        },
        {
          "id": "MDRW2100034835.1.7",
          "form": "추위대비",
          "original_form": "추위대비",
          "speaker_id": "2",
          "time": "20210828 20:36"
        },
        {
          "id": "MDRW2100034835.1.8",
          "form": "일찍 준비하셨네요!",
          "original_form": "일찍 준비하셨네요!",
          "speaker_id": "1",
          "time": "20210828 20:38"
        },
        {
          "id": "MDRW2100034835.1.9",
          "form": "가을옷은 벌써 다 준비하셨나요?",
          "original_form": "가을옷은 벌써 다 준비하셨나요?",
          "speaker_id": "1",
          "time": "20210828 20:38"
        },
        {
          "id": "MDRW2100034835.1.10",
          "form": "가을은 휘리릭 지나가서 그냥 있는거 입으려구요ㅋㅋ",
          "original_form": "가을은 휘리릭 지나가서 그냥 있는거 입으려구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:44"
        },
        {
          "id": "MDRW2100034835.1.11",
          "form": "신상보면 사고싶긴한데",
          "original_form": "신상보면 사고싶긴한데",
          "speaker_id": "2",
          "time": "20210828 20:44"
        },
        {
          "id": "MDRW2100034835.1.12",
          "form": "가을 진짜 너무 후딱 지나가죠 ㅋㅋ",
          "original_form": "가을 진짜 너무 후딱 지나가죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:46"
        },
        {
          "id": "MDRW2100034835.1.13",
          "form": "저는 작년에 그랬다가 올해는 놀러는 못가도 옷으로 기분이라도 내야겠다싶어서 좀 샀어요 ㅋㅋ",
          "original_form": "저는 작년에 그랬다가 올해는 놀러는 못가도 옷으로 기분이라도 내야겠다싶어서 좀 샀어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:46"
        },
        {
          "id": "MDRW2100034835.1.14",
          "form": "겨울옷은 아우터 사셨나요?",
          "original_form": "겨울옷은 아우터 사셨나요?",
          "speaker_id": "1",
          "time": "20210828 20:46"
        },
        {
          "id": "MDRW2100034835.1.15",
          "form": "아니요 안에 입을 폴라티같은거요",
          "original_form": "아니요 안에 입을 폴라티같은거요",
          "speaker_id": "2",
          "time": "20210828 21:02"
        },
        {
          "id": "MDRW2100034835.1.16",
          "form": "아우터는 그냥 롱패딩입을것같아요ㅋㅋ",
          "original_form": "아우터는 그냥 롱패딩입을것같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:02"
        },
        {
          "id": "MDRW2100034835.1.17",
          "form": "저는 겨울용 신발 하나 사고싶네요",
          "original_form": "저는 겨울용 신발 하나 사고싶네요",
          "speaker_id": "1",
          "time": "20210828 21:05"
        },
        {
          "id": "MDRW2100034835.1.18",
          "form": "발이 너무 시려요 ㅎ",
          "original_form": "발이 너무 시려요 ㅎ",
          "speaker_id": "1",
          "time": "20210828 21:05"
        },
        {
          "id": "MDRW2100034835.1.19",
          "form": "미리 사는것도 괜찮죠!",
          "original_form": "미리 사는것도 괜찮죠!",
          "speaker_id": "2",
          "time": "20210828 21:09"
        },
        {
          "id": "MDRW2100034835.1.20",
          "form": "겨울엔 따듯한 부츠?",
          "original_form": "겨울엔 따듯한 부츠?",
          "speaker_id": "2",
          "time": "20210828 21:09"
        },
        {
          "id": "MDRW2100034835.1.21",
          "form": "털 복실복실한걸로 사볼까요 ㅎ",
          "original_form": "털 복실복실한걸로 사볼까요 ㅎ",
          "speaker_id": "1",
          "time": "20210828 21:13"
        },
        {
          "id": "MDRW2100034835.1.22",
          "form": "아이고.. 패딩부츠 이건 진짜 신는 걸까요..",
          "original_form": "아이고.. 패딩부츠 이건 진짜 신는 걸까요..",
          "speaker_id": "1",
          "time": "20210828 21:13"
        },
        {
          "id": "MDRW2100034835.1.23",
          "form": "그건..잘모르겠어요ㅋㅋㅋ",
          "original_form": "그건..잘모르겠어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:16"
        },
        {
          "id": "MDRW2100034835.1.24",
          "form": "일단 따수우면 장땡이라고 봅니다",
          "original_form": "일단 따수우면 장땡이라고 봅니다",
          "speaker_id": "2",
          "time": "20210828 21:16"
        },
        {
          "id": "MDRW2100034835.1.25",
          "form": "롱패딩에 패딩부츠까지 신으면 진짜 따뜻할 것 같긴해요 ㅋㅋㅋㅋ",
          "original_form": "롱패딩에 패딩부츠까지 신으면 진짜 따뜻할 것 같긴해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:18"
        }
      ]
    }
