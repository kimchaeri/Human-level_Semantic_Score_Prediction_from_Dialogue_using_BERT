      "utterance": [
        {
          "id": "MDRW2100038093.1.1",
          "form": "백신맞으셨어요?!",
          "original_form": "백신맞으셨어요?!",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100038093.1.2",
          "form": "아직요",
          "original_form": "아직요",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100038093.1.3",
          "form": "추석전에맞아요",
          "original_form": "추석전에맞아요",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100038093.1.4",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100038093.1.5",
          "form": "저는 저저번주에",
          "original_form": "저는 저저번주에",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100038093.1.6",
          "form": "맞았어요",
          "original_form": "맞았어요",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100038093.1.7",
          "form": "어떠셧어요",
          "original_form": "어떠셧어요",
          "speaker_id": "2",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100038093.1.8",
          "form": "저 근육통이 한3.4일정도",
          "original_form": "저 근육통이 한3.4일정도",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100038093.1.9",
          "form": "갔어요",
          "original_form": "갔어요",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100038093.1.10",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100038093.1.11",
          "form": "화이자?",
          "original_form": "화이자?",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100038093.1.12",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100038093.1.13",
          "form": "다른부작용은없었어요 그래도",
          "original_form": "다른부작용은없었어요 그래도",
          "speaker_id": "1",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100038093.1.14",
          "form": "화이자요?",
          "original_form": "화이자요?",
          "speaker_id": "2",
          "time": "20210901 23:10"
        },
        {
          "id": "MDRW2100038093.1.15",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100038093.1.16",
          "form": "ㄱ걱정대네요",
          "original_form": "ㄱ걱정대네요",
          "speaker_id": "2",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100038093.1.17",
          "form": "근육통 원래아프시던덴가요",
          "original_form": "근육통 원래아프시던덴가요",
          "speaker_id": "2",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100038093.1.18",
          "form": "타이레놀 사놓으세요!",
          "original_form": "타이레놀 사놓으세요!",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100038093.1.19",
          "form": "아뇨ㅠ",
          "original_form": "아뇨ㅠ",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100038093.1.20",
          "form": "무릎아픈사람은 뮤릎이글케 아프다던지 한다든데",
          "original_form": "무릎아픈사람은 뮤릎이글케 아프다던지 한다든데",
          "speaker_id": "2",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100038093.1.21",
          "form": "헉 저는 딴덴다멀쩡했어요!",
          "original_form": "헉 저는 딴덴다멀쩡했어요!",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100038093.1.22",
          "form": "근데타이레놀이 그렇게잘들지않아서",
          "original_form": "근데타이레놀이 그렇게잘들지않아서",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100038093.1.23",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100038093.1.24",
          "form": "근육통이 좀 짜증났죠ㅠ",
          "original_form": "근육통이 좀 짜증났죠ㅠ",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100038093.1.25",
          "form": "일부러 쉬게 추석바로전으로햇어요",
          "original_form": "일부러 쉬게 추석바로전으로햇어요",
          "speaker_id": "2",
          "time": "20210901 23:12"
        }
