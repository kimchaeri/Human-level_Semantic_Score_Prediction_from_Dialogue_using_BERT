      "utterance": [
        {
          "id": "MDRW2100019649.1.1",
          "form": "넷플이나 유튜브보세요??",
          "original_form": "넷플이나 유튜브보세요??",
          "speaker_id": "1",
          "time": "20210827 20:57"
        },
        {
          "id": "MDRW2100019649.1.2",
          "form": "덕질하시는 그룹있나요!",
          "original_form": "덕질하시는 그룹있나요!",
          "speaker_id": "2",
          "time": "20210827 20:57"
        },
        {
          "id": "MDRW2100019649.1.3",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210827 20:57"
        },
        {
          "id": "MDRW2100019649.1.4",
          "form": "유툽자주봐여!",
          "original_form": "유툽자주봐여!",
          "speaker_id": "2",
          "time": "20210827 20:57"
        },
        {
          "id": "MDRW2100019649.1.5",
          "form": "넷플보단 티빙!",
          "original_form": "넷플보단 티빙!",
          "speaker_id": "2",
          "time": "20210827 20:58"
        },
        {
          "id": "MDRW2100019649.1.6",
          "form": "저 엑소 오랜 팬이에요 ㅋㅋㅋ",
          "original_form": "저 엑소 오랜 팬이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:58"
        },
        {
          "id": "MDRW2100019649.1.7",
          "form": "오 전 티빙은 본적 없는데 궁금해요",
          "original_form": "오 전 티빙은 본적 없는데 궁금해요",
          "speaker_id": "1",
          "time": "20210827 20:58"
        },
        {
          "id": "MDRW2100019649.1.8",
          "form": "티빙은",
          "original_form": "티빙은",
          "speaker_id": "2",
          "time": "20210827 20:58"
        },
        {
          "id": "MDRW2100019649.1.9",
          "form": "대틸출때문에...",
          "original_form": "대틸출때문에...",
          "speaker_id": "2",
          "time": "20210827 20:58"
        },
        {
          "id": "MDRW2100019649.1.10",
          "form": "대탈출 들어본거같은데",
          "original_form": "대탈출 들어본거같은데",
          "speaker_id": "1",
          "time": "20210827 20:59"
        },
        {
          "id": "MDRW2100019649.1.11",
          "form": "뭔가요??",
          "original_form": "뭔가요??",
          "speaker_id": "1",
          "time": "20210827 20:59"
        },
        {
          "id": "MDRW2100019649.1.12",
          "form": "음 방탈출을",
          "original_form": "음 방탈출을",
          "speaker_id": "2",
          "time": "20210827 20:59"
        },
        {
          "id": "MDRW2100019649.1.13",
          "form": "프로그램으로만든거요!",
          "original_form": "프로그램으로만든거요!",
          "speaker_id": "2",
          "time": "20210827 20:59"
        },
        {
          "id": "MDRW2100019649.1.14",
          "form": "헉 신기하네요",
          "original_form": "헉 신기하네요",
          "speaker_id": "1",
          "time": "20210827 21:00"
        },
        {
          "id": "MDRW2100019649.1.15",
          "form": "누가 만들었나 머리좋네요 ㅋㅋㅋ",
          "original_form": "누가 만들었나 머리좋네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:00"
        },
        {
          "id": "MDRW2100019649.1.16",
          "form": "하 진짜 너무재밌어여",
          "original_form": "하 진짜 너무재밌어여",
          "speaker_id": "2",
          "time": "20210827 21:02"
        },
        {
          "id": "MDRW2100019649.1.17",
          "form": "시간 완전 잘가여 ㅠㅠㅠ",
          "original_form": "시간 완전 잘가여 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 21:02"
        },
        {
          "id": "MDRW2100019649.1.18",
          "form": "저도 나중에 찾아볼게요 ㅋㅋㅋ",
          "original_form": "저도 나중에 찾아볼게요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:02"
        },
        {
          "id": "MDRW2100019649.1.19",
          "form": "드라마는 안보세요?",
          "original_form": "드라마는 안보세요?",
          "speaker_id": "1",
          "time": "20210827 21:03"
        },
        {
          "id": "MDRW2100019649.1.20",
          "form": "음...",
          "original_form": "음...",
          "speaker_id": "2",
          "time": "20210827 21:04"
        },
        {
          "id": "MDRW2100019649.1.21",
          "form": "요즘은 슬의생!!!봐여",
          "original_form": "요즘은 슬의생!!!봐여",
          "speaker_id": "2",
          "time": "20210827 21:04"
        },
        {
          "id": "MDRW2100019649.1.22",
          "form": "오 재밌나여?",
          "original_form": "오 재밌나여?",
          "speaker_id": "1",
          "time": "20210827 21:04"
        },
        {
          "id": "MDRW2100019649.1.23",
          "form": "네네넨 뭔가",
          "original_form": "네네넨 뭔가",
          "speaker_id": "2",
          "time": "20210827 21:06"
        },
        {
          "id": "MDRW2100019649.1.24",
          "form": "로맨스 이벤트 수술 등등완벽한조화랄까",
          "original_form": "로맨스 이벤트 수술 등등완벽한조화랄까",
          "speaker_id": "2",
          "time": "20210827 21:06"
        },
        {
          "id": "MDRW2100019649.1.25",
          "form": "누나는 뭐보세여??!?",
          "original_form": "누나는 뭐보세여??!?",
          "speaker_id": "2",
          "time": "20210827 21:06"
        },
        {
          "id": "MDRW2100019649.1.26",
          "form": "음 저는 펜트하우스랑 광자매요!",
          "original_form": "음 저는 펜트하우스랑 광자매요!",
          "speaker_id": "1",
          "time": "20210827 21:07"
        }
      ]
