      "utterance": [
        {
          "id": "MDRW2100013940.1.1",
          "form": "여행은 좀 다니시나요?",
          "original_form": "여행은 좀 다니시나요?",
          "speaker_id": "1",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013940.1.2",
          "form": "아녀 코로나 때문에",
          "original_form": "아녀 코로나 때문에",
          "speaker_id": "2",
          "time": "20210825 16:29"
        },
        {
          "id": "MDRW2100013940.1.3",
          "form": "어딜 가기가 좀..",
          "original_form": "어딜 가기가 좀..",
          "speaker_id": "2",
          "time": "20210825 16:29"
        },
        {
          "id": "MDRW2100013940.1.4",
          "form": "하긴 그렇죠. 코로나 끝나면 어디 가고 싶으세요",
          "original_form": "하긴 그렇죠. 코로나 끝나면 어디 가고 싶으세요",
          "speaker_id": "1",
          "time": "20210825 16:29"
        },
        {
          "id": "MDRW2100013940.1.5",
          "form": "전 제주도요",
          "original_form": "전 제주도요",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013940.1.6",
          "form": "원ㄴ래 코로나 전에 가려고 햇는ㄷ0",
          "original_form": "원ㄴ래 코로나 전에 가려고 햇는ㄷ0",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013940.1.7",
          "form": "코로나 터져서 못 갔거든요 ㅠㅠ",
          "original_form": "코로나 터져서 못 갔거든요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013940.1.8",
          "form": "아아 전 이번에 백신맞고 가려고요. 함덕 주변으로",
          "original_form": "아아 전 이번에 백신맞고 가려고요. 함덕 주변으로",
          "speaker_id": "1",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013940.1.9",
          "form": "와.. 진짜 부러워요",
          "original_form": "와.. 진짜 부러워요",
          "speaker_id": "2",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013940.1.10",
          "form": "2박 3일로ㅎㅎ 원래 제주도 어디 다녀오려고 하셧나요",
          "original_form": "2박 3일로ㅎㅎ 원래 제주도 어디 다녀오려고 하셧나요",
          "speaker_id": "1",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013940.1.11",
          "form": "저는 아는 분이 계셔서",
          "original_form": "저는 아는 분이 계셔서",
          "speaker_id": "2",
          "time": "20210825 16:32"
        },
        {
          "id": "MDRW2100013940.1.12",
          "form": "그쪽으로 그냥 놀러 갔다오려고 햇거든요 ㅠ",
          "original_form": "그쪽으로 그냥 놀러 갔다오려고 햇거든요 ㅠ",
          "speaker_id": "2",
          "time": "20210825 16:32"
        },
        {
          "id": "MDRW2100013940.1.13",
          "form": "오 그렇군요 외국은 어디 가뷰셨나요",
          "original_form": "오 그렇군요 외국은 어디 가뷰셨나요",
          "speaker_id": "1",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013940.1.14",
          "form": "일본 이 정도 밨에 못 가봣어요",
          "original_form": "일본 이 정도 밨에 못 가봣어요",
          "speaker_id": "2",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013940.1.15",
          "form": "원래 여행가도 거의",
          "original_form": "원래 여행가도 거의",
          "speaker_id": "2",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013940.1.16",
          "form": "호캉스 같은걸 좋아해서",
          "original_form": "호캉스 같은걸 좋아해서",
          "speaker_id": "2",
          "time": "20210825 16:34"
        },
        {
          "id": "MDRW2100013940.1.17",
          "form": "전 호캉스 안해뵜는데",
          "original_form": "전 호캉스 안해뵜는데",
          "speaker_id": "1",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013940.1.18",
          "form": "혹시 추천해주실만한 호텔 있나요",
          "original_form": "혹시 추천해주실만한 호텔 있나요",
          "speaker_id": "1",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013940.1.19",
          "form": "제가 추천해드릴 정도로 호텔잘알은 아니어서 ㅠㅠㅠㅋㅋㅋ",
          "original_form": "제가 추천해드릴 정도로 호텔잘알은 아니어서 ㅠㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:41"
        },
        {
          "id": "MDRW2100013940.1.20",
          "form": "일본에 갔을 때 뭐가 젤 맛있었나요",
          "original_form": "일본에 갔을 때 뭐가 젤 맛있었나요",
          "speaker_id": "1",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013940.1.21",
          "form": "음식이 그냥 거의 제 입맛에 안 맞았어요 ㅠㅠ 라멘도 생각보다 느끼하구..",
          "original_form": "음식이 그냥 거의 제 입맛에 안 맞았어요 ㅠㅠ 라멘도 생각보다 느끼하구..",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013940.1.22",
          "form": "그냥 한국 돌아다니는게 낫겟더라고요 ㅋㅋㅋㅋ",
          "original_form": "그냥 한국 돌아다니는게 낫겟더라고요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013940.1.23",
          "form": "오 그러셨구나 일본 어디가셧나요",
          "original_form": "오 그러셨구나 일본 어디가셧나요",
          "speaker_id": "1",
          "time": "20210825 17:07"
        },
        {
          "id": "MDRW2100013940.1.24",
          "form": "도쿄요 처음 가는거라 그냥 메이저한 곳 가는게 낫겠더라구요 ㅋㅋㅋ",
          "original_form": "도쿄요 처음 가는거라 그냥 메이저한 곳 가는게 낫겠더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:09"
        }
      ]
    }
