      "utterance": [
        {
          "id": "MDRW2100035139.1.1",
          "form": "그쵸 누구는 아이스크림이라던데 전 못먹을듯ㅠ",
          "original_form": "그쵸 누구는 아이스크림이라던데 전 못먹을듯ㅠ",
          "speaker_id": "2",
          "time": "20210829 06:45"
        },
        {
          "id": "MDRW2100035139.1.2",
          "form": "날비린내날거같아요",
          "original_form": "날비린내날거같아요",
          "speaker_id": "2",
          "time": "20210829 06:45"
        },
        {
          "id": "MDRW2100035139.1.3",
          "form": "맞아요ㅋㅋㅋ 신기한거 유행 많이하더라구요",
          "original_form": "맞아요ㅋㅋㅋ 신기한거 유행 많이하더라구요",
          "speaker_id": "1",
          "time": "20210829 06:46"
        },
        {
          "id": "MDRW2100035139.1.4",
          "form": "미국은ㅋㅋㅋㅋ",
          "original_form": "미국은ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 06:46"
        },
        {
          "id": "MDRW2100035139.1.5",
          "form": "좋아하는 사람은 엄청 좋아하더라구요",
          "original_form": "좋아하는 사람은 엄청 좋아하더라구요",
          "speaker_id": "2",
          "time": "20210829 06:47"
        },
        {
          "id": "MDRW2100035139.1.6",
          "form": "ㅋㅋㅋㅋ신기신기",
          "original_form": "ㅋㅋㅋㅋ신기신기",
          "speaker_id": "2",
          "time": "20210829 06:47"
        },
        {
          "id": "MDRW2100035139.1.7",
          "form": "날이 이래서 그런지 김치부침개가 갑자기 땡기네요",
          "original_form": "날이 이래서 그런지 김치부침개가 갑자기 땡기네요",
          "speaker_id": "1",
          "time": "20210829 06:48"
        },
        {
          "id": "MDRW2100035139.1.8",
          "form": "비오는 날은 부침개가 진리죠",
          "original_form": "비오는 날은 부침개가 진리죠",
          "speaker_id": "2",
          "time": "20210829 06:48"
        },
        {
          "id": "MDRW2100035139.1.9",
          "form": "먹고싶네요",
          "original_form": "먹고싶네요",
          "speaker_id": "2",
          "time": "20210829 06:48"
        },
        {
          "id": "MDRW2100035139.1.10",
          "form": "네네ㅋㅋㅋ 부침개 아아...",
          "original_form": "네네ㅋㅋㅋ 부침개 아아...",
          "speaker_id": "1",
          "time": "20210829 06:49"
        },
        {
          "id": "MDRW2100035139.1.11",
          "form": "저는 완전 바삭바삭",
          "original_form": "저는 완전 바삭바삭",
          "speaker_id": "2",
          "time": "20210829 06:49"
        },
        {
          "id": "MDRW2100035139.1.12",
          "form": "저두요!",
          "original_form": "저두요!",
          "speaker_id": "1",
          "time": "20210829 06:49"
        },
        {
          "id": "MDRW2100035139.1.13",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 06:50"
        },
        {
          "id": "MDRW2100035139.1.14",
          "form": "부침개 부칠때",
          "original_form": "부침개 부칠때",
          "speaker_id": "1",
          "time": "20210829 06:50"
        },
        {
          "id": "MDRW2100035139.1.15",
          "form": "가장자리가 맛나요",
          "original_form": "가장자리가 맛나요",
          "speaker_id": "2",
          "time": "20210829 06:50"
        },
        {
          "id": "MDRW2100035139.1.16",
          "form": "가운데 도넛모양으로 뚫으면",
          "original_form": "가운데 도넛모양으로 뚫으면",
          "speaker_id": "1",
          "time": "20210829 06:50"
        },
        {
          "id": "MDRW2100035139.1.17",
          "form": "바삭바삭하다고해서 그렇게 만드는데",
          "original_form": "바삭바삭하다고해서 그렇게 만드는데",
          "speaker_id": "1",
          "time": "20210829 06:50"
        },
        {
          "id": "MDRW2100035139.1.18",
          "form": "진짜 바삭해요",
          "original_form": "진짜 바삭해요",
          "speaker_id": "1",
          "time": "20210829 06:50"
        },
        {
          "id": "MDRW2100035139.1.19",
          "form": "오 저도 글케 만들어요",
          "original_form": "오 저도 글케 만들어요",
          "speaker_id": "2",
          "time": "20210829 06:50"
        },
        {
          "id": "MDRW2100035139.1.20",
          "form": "꾹꾹 눌러주고",
          "original_form": "꾹꾹 눌러주고",
          "speaker_id": "2",
          "time": "20210829 06:50"
        }
      ]
