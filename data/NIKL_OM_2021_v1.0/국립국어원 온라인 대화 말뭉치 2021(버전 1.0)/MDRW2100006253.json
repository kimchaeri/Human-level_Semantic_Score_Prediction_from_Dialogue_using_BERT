      "utterance": [
        {
          "id": "MDRW2100006253.1.1",
          "form": "자기 나랑은 해외여행 간적 없자나",
          "original_form": "자기 나랑은 해외여행 간적 없자나",
          "speaker_id": "2",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100006253.1.2",
          "form": "응 그전에 나혼자 다녔었지",
          "original_form": "응 그전에 나혼자 다녔었지",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100006253.1.3",
          "form": "어디갔었엉?",
          "original_form": "어디갔었엉?",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100006253.1.4",
          "form": "영국",
          "original_form": "영국",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100006253.1.5",
          "form": "프랑스",
          "original_form": "프랑스",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100006253.1.6",
          "form": "일본",
          "original_form": "일본",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100006253.1.7",
          "form": "홍콩",
          "original_form": "홍콩",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100006253.1.8",
          "form": "이게 다인듯",
          "original_form": "이게 다인듯",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100006253.1.9",
          "form": "자긴 스페인만 갔다왔지?",
          "original_form": "자긴 스페인만 갔다왔지?",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100006253.1.10",
          "form": "아니 내가 스페인만 갔다왔지 참 ㅋㅋㅋ",
          "original_form": "아니 내가 스페인만 갔다왔지 참 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100006253.1.11",
          "form": "그래도 코로나전에 한군데라도 다녀와서 다행이야 ㅠ",
          "original_form": "그래도 코로나전에 한군데라도 다녀와서 다행이야 ㅠ",
          "speaker_id": "2",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100006253.1.12",
          "form": "또 가고싶다",
          "original_form": "또 가고싶다",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100006253.1.13",
          "form": "이탈리아도 못갔는데ㅜㅜ",
          "original_form": "이탈리아도 못갔는데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100006253.1.14",
          "form": "요즘 자기 유튜브로 대리여행 하자나 ㅋㅋㅋㅋ",
          "original_form": "요즘 자기 유튜브로 대리여행 하자나 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100006253.1.15",
          "form": "요즘 그러고 있지",
          "original_form": "요즘 그러고 있지",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100006253.1.16",
          "form": "방법이 없어",
          "original_form": "방법이 없어",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100006253.1.17",
          "form": "여행을 유튜브로 하다니",
          "original_form": "여행을 유튜브로 하다니",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100006253.1.18",
          "form": "너무 슬프다",
          "original_form": "너무 슬프다",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100006253.1.19",
          "form": "여행가고싶다아",
          "original_form": "여행가고싶다아",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100006253.1.20",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100006253.1.21",
          "form": "뭔가 새로운 리프레시를 위해 여행을 가야하는데",
          "original_form": "뭔가 새로운 리프레시를 위해 여행을 가야하는데",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100006253.1.22",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100006253.1.23",
          "form": "이거참 문제야",
          "original_form": "이거참 문제야",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100006253.1.24",
          "form": "사람들 진짜 갑갑해서 어떡하나",
          "original_form": "사람들 진짜 갑갑해서 어떡하나",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100006253.1.25",
          "form": "나야 뭐 안가도 그만인데 ㅋㅋ",
          "original_form": "나야 뭐 안가도 그만인데 ㅋㅋ",
          "speaker_id": "2",
