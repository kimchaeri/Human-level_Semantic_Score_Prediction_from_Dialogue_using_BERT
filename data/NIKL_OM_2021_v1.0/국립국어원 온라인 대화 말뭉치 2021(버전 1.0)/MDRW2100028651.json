        },
        {
          "id": "MDRW2100028651.1.2",
          "form": "안녕하세요 이젠 밤되면 쌀쌀하네요",
          "original_form": "안녕하세요 이젠 밤되면 쌀쌀하네요",
          "speaker_id": "2",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100028651.1.3",
          "form": "네 곧 두꺼운 이불 꺼낼거 같아요",
          "original_form": "네 곧 두꺼운 이불 꺼낼거 같아요",
          "speaker_id": "1",
          "time": "20210902 21:39"
        },
        {
          "id": "MDRW2100028651.1.4",
          "form": "계신곳 비왔었나요?",
          "original_form": "계신곳 비왔었나요?",
          "speaker_id": "1",
          "time": "20210902 21:39"
        },
        {
          "id": "MDRW2100028651.1.5",
          "form": "오늘은 비는 안 왔어요",
          "original_form": "오늘은 비는 안 왔어요",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100028651.1.6",
          "form": "여기도 안왔어요",
          "original_form": "여기도 안왔어요",
          "speaker_id": "1",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028651.1.7",
          "form": "담주 화요일 전국에 온다네요ㅠㅠ",
          "original_form": "담주 화요일 전국에 온다네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028651.1.8",
          "form": "많이는 안 왔음 좋겠네요ㅠ",
          "original_form": "많이는 안 왔음 좋겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100028651.1.9",
          "form": "네 이제는 비오면 추워서ㅠㅠ",
          "original_form": "네 이제는 비오면 추워서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100028651.1.10",
          "form": "좋아하는 계절 있으세요?",
          "original_form": "좋아하는 계절 있으세요?",
          "speaker_id": "1",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100028651.1.11",
          "form": "저는 봄이 좋아요",
          "original_form": "저는 봄이 좋아요",
          "speaker_id": "2",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100028651.1.12",
          "form": "저는 겨울 좋아합니다ㅎ",
          "original_form": "저는 겨울 좋아합니다ㅎ",
          "speaker_id": "1",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100028651.1.13",
          "form": "추운 거 싫어해서 따뜻해지기 시작하는 시기라서요ㅎㅎ",
          "original_form": "추운 거 싫어해서 따뜻해지기 시작하는 시기라서요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100028651.1.14",
          "form": "저는 겨울의 긴 밤이 좋더라구요ㅠㅎ",
          "original_form": "저는 겨울의 긴 밤이 좋더라구요ㅠㅎ",
          "speaker_id": "1",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100028651.1.15",
          "form": "ㅋㅋ 감성파이시군요",
          "original_form": "ㅋㅋ 감성파이시군요",
          "speaker_id": "2",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100028651.1.16",
          "form": "올빼미라 밤을 좋아합니다ㅎㅎ",
          "original_form": "올빼미라 밤을 좋아합니다ㅎㅎ",
