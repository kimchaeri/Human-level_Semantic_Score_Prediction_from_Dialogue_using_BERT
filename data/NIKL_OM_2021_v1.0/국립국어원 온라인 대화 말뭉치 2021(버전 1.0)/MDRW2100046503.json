          "speaker_id": "2",
          "time": "20211026 11:50"
        },
        {
          "id": "MDRW2100046503.1.7",
          "form": "이번주 금요일날 입니다",
          "original_form": "이번주 금요일날 입니다",
          "speaker_id": "1",
          "time": "20211026 11:51"
        },
        {
          "id": "MDRW2100046503.1.8",
          "form": "저도 조금 두렵습니다",
          "original_form": "저도 조금 두렵습니다",
          "speaker_id": "1",
          "time": "20211026 11:51"
        },
        {
          "id": "MDRW2100046503.1.9",
          "form": "그니까요",
