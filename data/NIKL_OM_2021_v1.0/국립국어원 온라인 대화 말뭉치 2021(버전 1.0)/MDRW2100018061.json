      "utterance": [
        {
          "id": "MDRW2100018061.1.1",
          "form": "어릴때 주로뭐읽으셨어요?",
          "original_form": "어릴때 주로뭐읽으셨어요?",
          "speaker_id": "2",
          "time": "20210826 22:22"
        },
        {
          "id": "MDRW2100018061.1.2",
          "form": "그리스로마신화요",
          "original_form": "그리스로마신화요",
          "speaker_id": "1",
          "time": "20210826 22:23"
        },
        {
          "id": "MDRW2100018061.1.3",
          "form": "전  마법천자문",
          "original_form": "전  마법천자문",
          "speaker_id": "2",
          "time": "20210826 22:23"
        },
        {
          "id": "MDRW2100018061.1.4",
          "form": "재있었나요?",
          "original_form": "재있었나요?",
          "speaker_id": "1",
          "time": "20210826 22:23"
        },
        {
          "id": "MDRW2100018061.1.5",
          "form": "네ㅋㅋ 덕분에 한자울렁증은 없어요",
          "original_form": "네ㅋㅋ 덕분에 한자울렁증은 없어요",
          "speaker_id": "2",
          "time": "20210826 22:23"
        },
        {
          "id": "MDRW2100018061.1.6",
          "form": "커서는 뭘 읽으시나요",
          "original_form": "커서는 뭘 읽으시나요",
          "speaker_id": "1",
          "time": "20210826 22:23"
        },
        {
          "id": "MDRW2100018061.1.7",
          "form": "잡지?",
          "original_form": "잡지?",
          "speaker_id": "2",
          "time": "20210826 22:23"
        },
        {
          "id": "MDRW2100018061.1.8",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "1",
          "time": "20210826 22:23"
        },
        {
          "id": "MDRW2100018061.1.9",
          "form": "보그나 패션잡지위주요",
          "original_form": "보그나 패션잡지위주요",
          "speaker_id": "2",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100018061.1.10",
          "form": "전 어릴때 부록많이주는 잡지 사서 읽었었어요",
          "original_form": "전 어릴때 부록많이주는 잡지 사서 읽었었어요",
          "speaker_id": "1",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100018061.1.11",
          "form": "저도 그랬어요",
          "original_form": "저도 그랬어요",
          "speaker_id": "2",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100018061.1.12",
          "form": "일본잡지부록이 진짜 빵빵했죠",
          "original_form": "일본잡지부록이 진짜 빵빵했죠",
          "speaker_id": "1",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100018061.1.13",
          "form": "요즘은 어떨지 모르겠네요",
          "original_form": "요즘은 어떨지 모르겠네요",
          "speaker_id": "2",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100018061.1.14",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100018061.1.15",
          "form": "애초에 요즘도 일본잡지 쉬이 구매할 수 있나요",
          "original_form": "애초에 요즘도 일본잡지 쉬이 구매할 수 있나요",
          "speaker_id": "2",
          "time": "20210826 22:25"
        },
        {
          "id": "MDRW2100018061.1.16",
          "form": "글쎄요...",
          "original_form": "글쎄요...",
          "speaker_id": "1",
