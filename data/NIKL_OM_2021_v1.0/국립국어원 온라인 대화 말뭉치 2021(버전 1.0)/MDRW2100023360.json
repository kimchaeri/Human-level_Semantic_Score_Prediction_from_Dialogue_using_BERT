      "utterance": [
        {
          "id": "MDRW2100023360.1.1",
          "form": "아침 드셨나요",
          "original_form": "아침 드셨나요",
          "speaker_id": "2",
          "time": "20210830 08:23"
        },
        {
          "id": "MDRW2100023360.1.2",
          "form": "ㅋㅋㅋ 아침은 간단하게... 쪼꼬 두유 바나나 빵 이런거요..",
          "original_form": "ㅋㅋㅋ 아침은 간단하게... 쪼꼬 두유 바나나 빵 이런거요..",
          "speaker_id": "1",
          "time": "20210830 11:06"
        },
        {
          "id": "MDRW2100023360.1.3",
          "form": "헉 맛있겠네요",
          "original_form": "헉 맛있겠네요",
          "speaker_id": "2",
          "time": "20210830 12:02"
        },
        {
          "id": "MDRW2100023360.1.4",
          "form": "나름 배가 차죠",
          "original_form": "나름 배가 차죠",
          "speaker_id": "2",
          "time": "20210830 12:03"
        },
        {
          "id": "MDRW2100023360.1.5",
          "form": "아침은 안드시나요?",
          "original_form": "아침은 안드시나요?",
          "speaker_id": "1",
          "time": "20210830 12:05"
        },
        {
          "id": "MDRW2100023360.1.6",
          "form": "네 먹기보다",
          "original_form": "네 먹기보다",
          "speaker_id": "2",
          "time": "20210830 12:12"
        },
        {
          "id": "MDRW2100023360.1.7",
          "form": "그냥 잠을 선택해요....ㅎ",
          "original_form": "그냥 잠을 선택해요....ㅎ",
          "speaker_id": "2",
          "time": "20210830 12:12"
        },
        {
          "id": "MDRW2100023360.1.8",
          "form": "공복에 괜찮으세요? 막 어지럽다거나..",
          "original_form": "공복에 괜찮으세요? 막 어지럽다거나..",
          "speaker_id": "1",
          "time": "20210830 12:15"
        },
        {
          "id": "MDRW2100023360.1.9",
          "form": "ㅜ 배고플 때도 많은데",
          "original_form": "ㅜ 배고플 때도 많은데",
          "speaker_id": "2",
          "time": "20210830 12:20"
        },
        {
          "id": "MDRW2100023360.1.10",
          "form": "그래도 아침에 밥먹기가",
          "original_form": "그래도 아침에 밥먹기가",
          "speaker_id": "2",
          "time": "20210830 12:20"
        },
        {
          "id": "MDRW2100023360.1.11",
          "form": "애매해요 ㅠㅠ",
          "original_form": "애매해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 12:20"
        },
        {
          "id": "MDRW2100023360.1.12",
          "form": "전 그래서 역까지 걸어가면서 먹어요.. ㅋㅋ",
          "original_form": "전 그래서 역까지 걸어가면서 먹어요.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 12:23"
        },
        {
          "id": "MDRW2100023360.1.13",
          "form": "그래서 두유나.. 바나나.. 빵같은거.. ㅋㅋ",
          "original_form": "그래서 두유나.. 바나나.. 빵같은거.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 12:24"
        },
        {
          "id": "MDRW2100023360.1.14",
          "form": "ㅋㅋㅋㅋ아ㅋㅋㅋ 좋죠",
          "original_form": "ㅋㅋㅋㅋ아ㅋㅋㅋ 좋죠",
          "speaker_id": "2",
          "time": "20210830 12:28"
        },
        {
          "id": "MDRW2100023360.1.15",
          "form": "그렇게라도 먹는 거 !",
          "original_form": "그렇게라도 먹는 거 !",
          "speaker_id": "2",
          "time": "20210830 12:28"
        },
        {
          "id": "MDRW2100023360.1.16",
          "form": "전 공복에 현기증 땜에...",
          "original_form": "전 공복에 현기증 땜에...",
          "speaker_id": "1",
          "time": "20210830 12:34"
        },
        {
          "id": "MDRW2100023360.1.17",
          "form": "배고프면 손발이 막 떨려요",
          "original_form": "배고프면 손발이 막 떨려요",
          "speaker_id": "1",
          "time": "20210830 12:34"
        },
        {
          "id": "MDRW2100023360.1.18",
          "form": "아ㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:55"
        },
        {
          "id": "MDRW2100023360.1.19",
          "form": "인정이요 ㅠ",
          "original_form": "인정이요 ㅠ",
          "speaker_id": "2",
          "time": "20210830 14:55"
        },
        {
          "id": "MDRW2100023360.1.20",
          "form": "이시간 간식은 드시나요?",
          "original_form": "이시간 간식은 드시나요?",
          "speaker_id": "1",
          "time": "20210830 14:56"
        },
        {
          "id": "MDRW2100023360.1.21",
          "form": "아니요 ㅠ 안먹어요!",
          "original_form": "아니요 ㅠ 안먹어요!",
          "speaker_id": "2",
          "time": "20210830 15:25"
        },
        {
          "id": "MDRW2100023360.1.22",
          "form": "뭐 드세요,?",
          "original_form": "뭐 드세요,?",
          "speaker_id": "2",
          "time": "20210830 15:25"
        },
        {
          "id": "MDRW2100023360.1.23",
          "form": "저는 이제 점심먹엇어요.. ㅋㅋㅋ",
          "original_form": "저는 이제 점심먹엇어요.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:28"
        },
        {
          "id": "MDRW2100023360.1.24",
          "form": "저녁",
          "original_form": "저녁",
          "speaker_id": "2",
          "time": "20210830 16:25"
        },
        {
          "id": "MDRW2100023360.1.25",
          "form": "뭐 드실거에요?",
          "original_form": "뭐 드실거에요?",
          "speaker_id": "2",
