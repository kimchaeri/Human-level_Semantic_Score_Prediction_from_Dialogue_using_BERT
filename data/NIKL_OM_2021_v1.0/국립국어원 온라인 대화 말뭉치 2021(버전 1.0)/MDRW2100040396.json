      "utterance": [
        {
          "id": "MDRW2100040396.1.1",
          "form": "저도요 바스락거리는걸로 사고싶어여",
          "original_form": "저도요 바스락거리는걸로 사고싶어여",
          "speaker_id": "2",
          "time": "20210904 18:04"
        },
        {
          "id": "MDRW2100040396.1.2",
          "form": "호텔침구처럼?",
          "original_form": "호텔침구처럼?",
          "speaker_id": "1",
          "time": "20210904 18:04"
        },
        {
          "id": "MDRW2100040396.1.3",
          "form": "네 이제 날추워지니까 좀 포근하면서~~",
          "original_form": "네 이제 날추워지니까 좀 포근하면서~~",
          "speaker_id": "2",
          "time": "20210904 18:05"
        },
        {
          "id": "MDRW2100040396.1.4",
          "form": "오호  이불사면 잇던거 어케요?",
          "original_form": "오호  이불사면 잇던거 어케요?",
          "speaker_id": "1",
          "time": "20210904 18:09"
        },
        {
          "id": "MDRW2100040396.1.5",
          "form": "장농속에 넣어둬야죠",
          "original_form": "장농속에 넣어둬야죠",
          "speaker_id": "2",
          "time": "20210904 18:09"
        },
        {
          "id": "MDRW2100040396.1.6",
          "form": "자리 많아요??",
          "original_form": "자리 많아요??",
          "speaker_id": "1",
          "time": "20210904 18:10"
        },
        {
          "id": "MDRW2100040396.1.7",
          "form": "장농없으면",
          "original_form": "장농없으면",
          "speaker_id": "1",
          "time": "20210904 18:10"
        },
        {
          "id": "MDRW2100040396.1.8",
          "form": "그리고 안쓰는 이불은 유기견센터에 보내주면 좋대요 이불이 많이 팔요하다네요",
          "original_form": "그리고 안쓰는 이불은 유기견센터에 보내주면 좋대요 이불이 많이 팔요하다네요",
          "speaker_id": "2",
          "time": "20210904 18:10"
        },
        {
          "id": "MDRW2100040396.1.9",
          "form": "자리없으면 보내세요",
          "original_form": "자리없으면 보내세요",
          "speaker_id": "2",
          "time": "20210904 18:10"
        },
        {
          "id": "MDRW2100040396.1.10",
          "form": "아그래요? 택배로??",
          "original_form": "아그래요? 택배로??",
          "speaker_id": "1",
          "time": "20210904 18:11"
        },
        {
          "id": "MDRW2100040396.1.11",
          "form": "네네 택배는 선불로~",
          "original_form": "네네 택배는 선불로~",
          "speaker_id": "2",
          "time": "20210904 18:11"
        },
        {
          "id": "MDRW2100040396.1.12",
          "form": "당근 해밧어요?",
          "original_form": "당근 해밧어요?",
          "speaker_id": "1",
          "time": "20210904 18:14"
        },
        {
          "id": "MDRW2100040396.1.13",
          "form": "아뇨 당근은 안해보도 번장은 해봤어요",
          "original_form": "아뇨 당근은 안해보도 번장은 해봤어요",
          "speaker_id": "2",
          "time": "20210904 18:16"
        },
        {
          "id": "MDRW2100040396.1.14",
          "form": "삿어요?",
          "original_form": "삿어요?",
          "speaker_id": "1",
          "time": "20210904 18:17"
        },
        {
          "id": "MDRW2100040396.1.15",
          "form": "네네 사기만 해봤어요!! 팔기는 안해봄요",
          "original_form": "네네 사기만 해봤어요!! 팔기는 안해봄요",
          "speaker_id": "2",
          "time": "20210904 18:19"
        },
        {
          "id": "MDRW2100040396.1.16",
          "form": "머삿어요",
          "original_form": "머삿어요",
          "speaker_id": "1",
          "time": "20210904 18:20"
        },
