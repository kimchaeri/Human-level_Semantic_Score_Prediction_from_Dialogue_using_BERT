      "utterance": [
        {
          "id": "MDRW2100005393.1.1",
          "form": "결혼했어요?",
          "original_form": "결혼했어요?",
          "speaker_id": "1",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005393.1.2",
          "form": "아니요 ㅎㅎㅎ",
          "original_form": "아니요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:11"
        },
        {
          "id": "MDRW2100005393.1.3",
          "form": "언제쯤 생각요?",
          "original_form": "언제쯤 생각요?",
          "speaker_id": "1",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005393.1.4",
          "form": "생각음 있는데 인연이 나타나질 않네요 ㅎㅎ",
          "original_form": "생각음 있는데 인연이 나타나질 않네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005393.1.5",
          "form": "어렵죠",
          "original_form": "어렵죠",
          "speaker_id": "1",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005393.1.6",
          "form": "네 ㅎㄹ 나이먹을수록 더 어려워지네요 ㅜ",
          "original_form": "네 ㅎㄹ 나이먹을수록 더 어려워지네요 ㅜ",
          "speaker_id": "2",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005393.1.7",
          "form": "인연은 있죠",
          "original_form": "인연은 있죠",
          "speaker_id": "1",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005393.1.8",
          "form": "결혼하셨어요?",
          "original_form": "결혼하셨어요?",
          "speaker_id": "2",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005393.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210821 22:26"
        },
        {
          "id": "MDRW2100005393.1.10",
          "form": "아이있으면 매일 전쟁이야요",
          "original_form": "아이있으면 매일 전쟁이야요",
          "speaker_id": "1",
          "time": "20210823 08:39"
        },
        {
          "id": "MDRW2100005393.1.11",
          "form": "그렇겠네요",
          "original_form": "그렇겠네요",
          "speaker_id": "2",
          "time": "20210823 09:19"
        },
        {
          "id": "MDRW2100005393.1.12",
          "form": "저는 아기 좋아하는데 막상 육아하려고 하면 쉽지않을거같아요",
          "original_form": "저는 아기 좋아하는데 막상 육아하려고 하면 쉽지않을거같아요",
          "speaker_id": "2",
          "time": "20210823 09:20"
        },
        {
          "id": "MDRW2100005393.1.13",
          "form": "네 천사랑 악마요",
          "original_form": "네 천사랑 악마요",
          "speaker_id": "1",
          "time": "20210823 09:53"
        },
        {
          "id": "MDRW2100005393.1.14",
          "form": "그래도 좋아요",
          "original_form": "그래도 좋아요",
          "speaker_id": "1",
          "time": "20210823 10:46"
        },
        {
          "id": "MDRW2100005393.1.15",
          "form": "저는 나이가 있어서 결혼해도 아이 낳기가 망설여지더라고요",
          "original_form": "저는 나이가 있어서 결혼해도 아이 낳기가 망설여지더라고요",
          "speaker_id": "2",
          "time": "20210823 13:37"
        },
        {
          "id": "MDRW2100005393.1.16",
          "form": "그래도 있으면 웃어요",
          "original_form": "그래도 있으면 웃어요",
          "speaker_id": "1",
