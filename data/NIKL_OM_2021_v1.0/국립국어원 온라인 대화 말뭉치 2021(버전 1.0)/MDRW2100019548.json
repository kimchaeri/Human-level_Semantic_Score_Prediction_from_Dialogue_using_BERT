      "utterance": [
        {
          "id": "MDRW2100019548.1.1",
          "form": "저는 방금 너무 과식해서 후회중이요ㅜ",
          "original_form": "저는 방금 너무 과식해서 후회중이요ㅜ",
          "speaker_id": "2",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100019548.1.2",
          "form": "뭐 드셨어요ㅋㅋ",
          "original_form": "뭐 드셨어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100019548.1.3",
          "form": "베트남 쌀국수요",
          "original_form": "베트남 쌀국수요",
          "speaker_id": "2",
          "time": "20210827 20:14"
        },
        {
          "id": "MDRW2100019548.1.4",
          "form": "정확히는 볶음면",
          "original_form": "정확히는 볶음면",
          "speaker_id": "2",
          "time": "20210827 20:15"
        },
        {
          "id": "MDRW2100019548.1.5",
          "form": "아~ 쌀국수면 배 금방꺼지죠 괜찮아질 거예요",
          "original_form": "아~ 쌀국수면 배 금방꺼지죠 괜찮아질 거예요",
          "speaker_id": "1",
          "time": "20210827 20:15"
        },
        {
          "id": "MDRW2100019548.1.6",
          "form": "볶음면ㅠㅠ 와",
          "original_form": "볶음면ㅠㅠ 와",
          "speaker_id": "1",
          "time": "20210827 20:15"
        },
        {
          "id": "MDRW2100019548.1.7",
          "form": "저 내일 점심 쌀국수 볶음면...",
          "original_form": "저 내일 점심 쌀국수 볶음면...",
          "speaker_id": "1",
          "time": "20210827 20:15"
        },
        {
          "id": "MDRW2100019548.1.8",
          "form": "미스 사이공이라는 프랜차이즈였는데",
          "original_form": "미스 사이공이라는 프랜차이즈였는데",
          "speaker_id": "2",
          "time": "20210827 20:16"
        },
        {
          "id": "MDRW2100019548.1.9",
          "form": "반미라는게 있더라고요",
          "original_form": "반미라는게 있더라고요",
          "speaker_id": "2",
          "time": "20210827 20:16"
        },
        {
          "id": "MDRW2100019548.1.10",
          "form": "아세요?",
          "original_form": "아세요?",
          "speaker_id": "2",
          "time": "20210827 20:16"
        },
        {
          "id": "MDRW2100019548.1.11",
          "form": "네네 미스사이공 반비 둘다 알아요",
          "original_form": "네네 미스사이공 반비 둘다 알아요",
          "speaker_id": "1",
          "time": "20210827 20:16"
        },
        {
          "id": "MDRW2100019548.1.12",
          "form": "샌드위치!",
          "original_form": "샌드위치!",
          "speaker_id": "1",
          "time": "20210827 20:16"
        },
        {
          "id": "MDRW2100019548.1.13",
          "form": "그게 또 궁금해서요ㅜ",
          "original_form": "그게 또 궁금해서요ㅜ",
          "speaker_id": "2",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100019548.1.14",
          "form": "둘다먹음ㅜㅜ",
          "original_form": "둘다먹음ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100019548.1.15",
          "form": "ㅋㅋㅋ 반미는 참으면 안 되죠",
          "original_form": "ㅋㅋㅋ 반미는 참으면 안 되죠",
          "speaker_id": "1",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100019548.1.16",
          "form": "그게 베트남 분들의 자존심 같은 음식이라면서요?",
          "original_form": "그게 베트남 분들의 자존심 같은 음식이라면서요?",
          "speaker_id": "1",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100019548.1.17",
          "form": "볶음면이 이미 라지였는데ㅜㅜ",
          "original_form": "볶음면이 이미 라지였는데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100019548.1.18",
          "form": "ㅋㅋㅋㅋㅋ 야무지게 먹었음 후회하지마여",
          "original_form": "ㅋㅋㅋㅋㅋ 야무지게 먹었음 후회하지마여",
          "speaker_id": "1",
          "time": "20210827 20:18"
        },
        {
          "id": "MDRW2100019548.1.19",
          "form": "에제리너스 반미가 그렇게 맛있다고 들었어요",
          "original_form": "에제리너스 반미가 그렇게 맛있다고 들었어요",
          "speaker_id": "1",
          "time": "20210827 20:18"
        },
        {
          "id": "MDRW2100019548.1.20",
          "form": "근데 복음면 먹기전에 먹었어야했어요ㅜ",
          "original_form": "근데 복음면 먹기전에 먹었어야했어요ㅜ",
          "speaker_id": "2",
          "time": "20210827 20:18"
        },
        {
          "id": "MDRW2100019548.1.21",
          "form": "아 반미는 에피타이저다?",
          "original_form": "아 반미는 에피타이저다?",
          "speaker_id": "1",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019548.1.22",
          "form": "맛이 잘 안느껴졌어요ㅜ",
          "original_form": "맛이 잘 안느껴졌어요ㅜ",
          "speaker_id": "2",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019548.1.23",
          "form": "복음면 맛 때문에 ㅎ",
          "original_form": "복음면 맛 때문에 ㅎ",
          "speaker_id": "2",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019548.1.24",
          "form": "아이구ㅠㅠ 어쩔 수 없이 다음에 또 맛봐줘야겠네요ㅋㅋ",
          "original_form": "아이구ㅠㅠ 어쩔 수 없이 다음에 또 맛봐줘야겠네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:19"
        }
      ]
    }
  ]
