      "utterance": [
        {
          "id": "MDRW2100035774.1.1",
          "form": "백신 예약하셨나요?",
          "original_form": "백신 예약하셨나요?",
          "speaker_id": "1",
          "time": "20210830 01:22"
        },
        {
          "id": "MDRW2100035774.1.2",
          "form": "아직이요",
          "original_form": "아직이요",
          "speaker_id": "2",
          "time": "20210830 08:23"
        },
        {
          "id": "MDRW2100035774.1.3",
          "form": "하셨어요?",
          "original_form": "하셨어요?",
          "speaker_id": "2",
          "time": "20210830 08:23"
        },
        {
          "id": "MDRW2100035774.1.4",
          "form": "저는 9월에 예약만 해뒀어요",
          "original_form": "저는 9월에 예약만 해뒀어요",
          "speaker_id": "1",
          "time": "20210830 11:04"
        },
        {
          "id": "MDRW2100035774.1.5",
          "form": "저도 해여하는데",
          "original_form": "저도 해여하는데",
          "speaker_id": "2",
          "time": "20210830 12:02"
        },
        {
          "id": "MDRW2100035774.1.6",
          "form": "ㅠㅠ ㅠㅠㅠ 언제 맞으세요",
          "original_form": "ㅠㅠ ㅠㅠㅠ 언제 맞으세요",
          "speaker_id": "2",
          "time": "20210830 12:02"
        },
        {
          "id": "MDRW2100035774.1.7",
          "form": "9월 말이여ㅠㅜ 더 당겨보려구요!",
          "original_form": "9월 말이여ㅠㅜ 더 당겨보려구요!",
          "speaker_id": "1",
          "time": "20210830 12:03"
        },
        {
          "id": "MDRW2100035774.1.8",
          "form": "저도 맞아야 하는ㄷ0",
          "original_form": "저도 맞아야 하는ㄷ0",
          "speaker_id": "2",
          "time": "20210830 12:11"
        },
        {
          "id": "MDRW2100035774.1.9",
          "form": "걱정돼요ㅋㅋㅋㅋㅋ",
          "original_form": "걱정돼요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 12:11"
        },
        {
          "id": "MDRW2100035774.1.10",
          "form": "주변에 큰일 난 사람은 없긴 한데 또 모르니까요ㅠ",
          "original_form": "주변에 큰일 난 사람은 없긴 한데 또 모르니까요ㅠ",
          "speaker_id": "1",
          "time": "20210830 12:24"
        },
        {
          "id": "MDRW2100035774.1.11",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 12:29"
        },
        {
          "id": "MDRW2100035774.1.12",
          "form": "올해 연말 전에 항체가 생기는 게 목표예요...",
          "original_form": "올해 연말 전에 항체가 생기는 게 목표예요...",
          "speaker_id": "1",
          "time": "20210830 12:36"
        },
        {
          "id": "MDRW2100035774.1.13",
          "form": "아하.. 안밀리면 그렇게 되겠죠?",
          "original_form": "아하.. 안밀리면 그렇게 되겠죠?",
          "speaker_id": "2",
          "time": "20210830 14:55"
        },
        {
          "id": "MDRW2100035774.1.14",
          "form": "6주에서 5주로 당길 수도 잇다고 해서 1차 맞고 여쭤보려구요",
          "original_form": "6주에서 5주로 당길 수도 잇다고 해서 1차 맞고 여쭤보려구요",
          "speaker_id": "1",
          "time": "20210830 16:14"
        },
        {
          "id": "MDRW2100035774.1.15",
          "form": "아 진짜요? 그럼 무조겅 당기세료!",
          "original_form": "아 진짜요? 그럼 무조겅 당기세료!",
          "speaker_id": "2",
          "time": "20210830 16:18"
        },
        {
          "id": "MDRW2100035774.1.16",
          "form": "추석 전에 많이 접종시키려고도 한다 해서 곧 문의넣어보려구요",
          "original_form": "추석 전에 많이 접종시키려고도 한다 해서 곧 문의넣어보려구요",
          "speaker_id": "1",
          "time": "20210830 16:18"
        },
        {
          "id": "MDRW2100035774.1.17",
          "form": "저도 그때 맞아야 하나 고밍이네유..",
          "original_form": "저도 그때 맞아야 하나 고밍이네유..",
          "speaker_id": "2",
          "time": "20210830 16:18"
        }
      ]
