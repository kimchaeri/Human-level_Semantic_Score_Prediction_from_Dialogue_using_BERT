      "utterance": [
        {
          "id": "MDRW2100019762.1.1",
          "form": "여행많이 가보셧나요?",
          "original_form": "여행많이 가보셧나요?",
          "speaker_id": "1",
          "time": "20210827 21:48"
        },
        {
          "id": "MDRW2100019762.1.2",
          "form": "요즘은 갈수가 없어서 아쉽습니다",
          "original_form": "요즘은 갈수가 없어서 아쉽습니다",
          "speaker_id": "2",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019762.1.3",
          "form": "예전엔 오디 가조셧어요?",
          "original_form": "예전엔 오디 가조셧어요?",
          "speaker_id": "1",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019762.1.4",
          "form": "해외여행가고싶어요 캄보디아 사원이 좋았어요",
          "original_form": "해외여행가고싶어요 캄보디아 사원이 좋았어요",
          "speaker_id": "2",
          "time": "20210827 22:22"
        },
        {
          "id": "MDRW2100019762.1.5",
          "form": "캄보디아여? 엄청 덥지 않아요?",
          "original_form": "캄보디아여? 엄청 덥지 않아요?",
          "speaker_id": "1",
          "time": "20210827 22:25"
        },
        {
          "id": "MDRW2100019762.1.6",
          "form": "더워도볼거리가 풍부해요",
          "original_form": "더워도볼거리가 풍부해요",
          "speaker_id": "2",
          "time": "20210827 22:32"
        },
        {
          "id": "MDRW2100019762.1.7",
          "form": "배낭 같은거로 가셧나요?",
          "original_form": "배낭 같은거로 가셧나요?",
          "speaker_id": "1",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100019762.1.8",
          "form": "배낭은 아니고 여행사로갔어요",
          "original_form": "배낭은 아니고 여행사로갔어요",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100019762.1.9",
          "form": "좋은여행지 추천해주세요",
          "original_form": "좋은여행지 추천해주세요",
          "speaker_id": "2",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100019762.1.10",
          "form": "국내요?",
          "original_form": "국내요?",
          "speaker_id": "1",
          "time": "20210827 23:31"
        },
        {
          "id": "MDRW2100019762.1.11",
          "form": "어디든요",
          "original_form": "어디든요",
          "speaker_id": "2",
          "time": "20210828 13:58"
        },
        {
          "id": "MDRW2100019762.1.12",
          "form": "제주도는 자주가시나여?",
          "original_form": "제주도는 자주가시나여?",
          "speaker_id": "1",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100019762.1.13",
          "form": "네 제주도는 가도가도 또 가고싶은곳같아요",
          "original_form": "네 제주도는 가도가도 또 가고싶은곳같아요",
          "speaker_id": "2",
          "time": "20210828 14:01"
        },
        {
          "id": "MDRW2100019762.1.14",
          "form": "특정 지역이 좋은거에요? 아님 골고루 댕기시나여?",
          "original_form": "특정 지역이 좋은거에요? 아님 골고루 댕기시나여?",
          "speaker_id": "1",
          "time": "20210828 14:03"
        },
        {
          "id": "MDRW2100019762.1.15",
          "form": "골고루다니는게좋은것같아요 제주도는넓으니까",
          "original_form": "골고루다니는게좋은것같아요 제주도는넓으니까",
          "speaker_id": "2",
