      "utterance": [
        {
          "id": "MDRW2100032373.1.1",
          "form": "토이트론꺼에요?ㅎ",
          "original_form": "토이트론꺼에요?ㅎ",
          "speaker_id": "2",
          "time": "20210905 16:35"
        },
        {
          "id": "MDRW2100032373.1.2",
          "form": "방금보고왔는데 만오천원짜리랑 3만원짜리있네요ㅎㅎ",
          "original_form": "방금보고왔는데 만오천원짜리랑 3만원짜리있네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 16:36"
        },
        {
          "id": "MDRW2100032373.1.3",
          "form": "네네 ㅎㅎ",
          "original_form": "네네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 16:36"
        },
        {
          "id": "MDRW2100032373.1.4",
          "form": "어짜피 살꺼 만오천원짜리사야겟어요ㅎㅎㅎ",
          "original_form": "어짜피 살꺼 만오천원짜리사야겟어요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 16:36"
        },
        {
          "id": "MDRW2100032373.1.5",
          "form": "집중력 좋아요 ㅎㅎ",
          "original_form": "집중력 좋아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 16:37"
        },
        {
          "id": "MDRW2100032373.1.6",
          "form": "싫어할수도 있으니 나중에 또 사주죠뭐ㅎ",
          "original_form": "싫어할수도 있으니 나중에 또 사주죠뭐ㅎ",
          "speaker_id": "2",
          "time": "20210905 16:37"
        },
        {
          "id": "MDRW2100032373.1.7",
          "form": "ㅎㅎ 결제하고 넘어오겟습니당ㅎㅎ",
          "original_form": "ㅎㅎ 결제하고 넘어오겟습니당ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 16:37"
        },
        {
          "id": "MDRW2100032373.1.8",
          "form": "아 그럼 혹시",
          "original_form": "아 그럼 혹시",
          "speaker_id": "1",
          "time": "20210905 16:37"
        },
        {
          "id": "MDRW2100032373.1.9",
          "form": "sk스토아 출첵 하셧어요?",
          "original_form": "sk스토아 출첵 하셧어요?",
          "speaker_id": "1",
          "time": "20210905 16:37"
        },
        {
          "id": "MDRW2100032373.1.10",
          "form": "아니면  교원 포인트 잇으시면 \r\n단품 저렴하게 살수잇어요 ㅎㅎ\r\n저도 첨에 그거 주문해서 맛보기로 ㅎㅎ",
          "original_form": "아니면  교원 포인트 잇으시면 \r\n단품 저렴하게 살수잇어요 ㅎㅎ\r\n저도 첨에 그거 주문해서 맛보기로 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 16:38"
        },
        {
          "id": "MDRW2100032373.1.11",
          "form": "k멤버스가 저렴하네요 ㅎㅎ",
          "original_form": "k멤버스가 저렴하네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 16:42"
        },
        {
          "id": "MDRW2100032373.1.12",
          "form": "아아ㅎ 취소가있으니ㅎㅎ",
          "original_form": "아아ㅎ 취소가있으니ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 16:47"
        },
        {
          "id": "MDRW2100032373.1.13",
          "form": "k멤버스가 스타터 세트가  14300원이요\r\n포인트있음 더저렴 ㅎㅎ",
          "original_form": "k멤버스가 스타터 세트가  14300원이요\r\n포인트있음 더저렴 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 16:48"
        },
        {
          "id": "MDRW2100032373.1.14",
          "form": "ㅎㅎ 11300원에 결제완료요ㅎ",
          "original_form": "ㅎㅎ 11300원에 결제완료요ㅎ",
          "speaker_id": "2",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100032373.1.15",
          "form": "ㅎㅎ 앱테크 아직 초보네요ㅎ 최저가로 배송비까지 붙여서 18400원 결제 완료했었는데ㅎㅎ",
          "original_form": "ㅎㅎ 앱테크 아직 초보네요ㅎ 최저가로 배송비까지 붙여서 18400원 결제 완료했었는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 17:03"
        },
        {
          "id": "MDRW2100032373.1.16",
          "form": "아아님때문에 7천원아꼈어요ㅎ 감사해요ㅎㅎ",
          "original_form": "아아님때문에 7천원아꼈어요ㅎ 감사해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 17:03"
        },
        {
          "id": "MDRW2100032373.1.17",
          "form": "손까락 하나로 스벅한잔을 아끼다니ㅎ 아직은 배울게많나봅니다ㅎ",
          "original_form": "손까락 하나로 스벅한잔을 아끼다니ㅎ 아직은 배울게많나봅니다ㅎ",
          "speaker_id": "2",
          "time": "20210905 17:03"
        },
        {
          "id": "MDRW2100032373.1.18",
          "form": "ㅎㅎㅎ 다행이네요 ㅎㅎ",
          "original_form": "ㅎㅎㅎ 다행이네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 17:20"
        },
        {
          "id": "MDRW2100032373.1.19",
          "form": "애기 반찬 만드는중이에요",
          "original_form": "애기 반찬 만드는중이에요",
          "speaker_id": "1",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100032373.1.20",
          "form": "새우구이랑  달걀장조림",
          "original_form": "새우구이랑  달걀장조림",
          "speaker_id": "1",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100032373.1.21",
          "form": "삼겹살더 드신다해서  ㅋㅋ",
          "original_form": "삼겹살더 드신다해서  ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:22"
        },
        {
          "id": "MDRW2100032373.1.22",
          "form": "요즘 새로 생길 마트  삼겹살 100g에 천원정도 해서 아주 꿀이네여 ㅎㅎ",
          "original_form": "요즘 새로 생길 마트  삼겹살 100g에 천원정도 해서 아주 꿀이네여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 17:23"
        },
        {
          "id": "MDRW2100032373.1.23",
          "form": "이야ㅎ 대단하시네요ㅎ",
          "original_form": "이야ㅎ 대단하시네요ㅎ",
          "speaker_id": "2",
          "time": "20210905 17:29"
        },
        {
          "id": "MDRW2100032373.1.24",
          "form": "전 전에 케이쇼핑에서 할인되서 산 삼치주려구요ㅎㅎ",
          "original_form": "전 전에 케이쇼핑에서 할인되서 산 삼치주려구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 17:30"
        },
        {
          "id": "MDRW2100032373.1.25",
          "form": "안먹어도 최대한 먹게꿈 해야 하니께유 ㅜㅁ ㅜ",
          "original_form": "안먹어도 최대한 먹게꿈 해야 하니께유 ㅜㅁ ㅜ",
          "speaker_id": "1",
          "time": "20210905 17:30"
        },
        {
          "id": "MDRW2100032373.1.26",
          "form": "에고 고생이많으시네요 ㅠ",
          "original_form": "에고 고생이많으시네요 ㅠ",
