      "utterance": [
        {
          "id": "MDRW2100002607.1.1",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "1",
          "time": "20210820 13:53"
        },
        {
          "id": "MDRW2100002607.1.2",
          "form": "이번주말에는 비가 온다네요ㅜㅜ",
          "original_form": "이번주말에는 비가 온다네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 13:53"
        },
        {
          "id": "MDRW2100002607.1.3",
          "form": "아고ㅠㅠ비좀 그만왔으면 좋겠네요ㅠ",
          "original_form": "아고ㅠㅠ비좀 그만왔으면 좋겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210820 13:56"
        },
        {
          "id": "MDRW2100002607.1.4",
          "form": "이번 년도에 유독 비가 많이 오는거 같아요",
          "original_form": "이번 년도에 유독 비가 많이 오는거 같아요",
          "speaker_id": "1",
          "time": "20210820 13:58"
        },
        {
          "id": "MDRW2100002607.1.5",
          "form": "맞아요ㅠ이쪽지역은 오전에도 비가 왔어요ㅠ",
          "original_form": "맞아요ㅠ이쪽지역은 오전에도 비가 왔어요ㅠ",
          "speaker_id": "2",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002607.1.6",
          "form": "엇 정말요? 제가 있는 지역은 비는 안왔고 어둡기만 하네요",
          "original_form": "엇 정말요? 제가 있는 지역은 비는 안왔고 어둡기만 하네요",
          "speaker_id": "1",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100002607.1.7",
          "form": "어둡다니ㅠ곧오려나봐요 아직낮인데",
          "original_form": "어둡다니ㅠ곧오려나봐요 아직낮인데",
          "speaker_id": "2",
          "time": "20210820 14:01"
        },
        {
          "id": "MDRW2100002607.1.8",
          "form": "네 ㅠㅠ 그래도 요즘은 미세먼지가 덜해서 좋지만, 비오는걸 즐기지는 않아서 ㅠㅠ",
          "original_form": "네 ㅠㅠ 그래도 요즘은 미세먼지가 덜해서 좋지만, 비오는걸 즐기지는 않아서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 14:02"
        },
        {
          "id": "MDRW2100002607.1.9",
          "form": "그초ㅡㅠ다니기가 힘드니ㅠ",
          "original_form": "그초ㅡㅠ다니기가 힘드니ㅠ",
          "speaker_id": "2",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100002607.1.10",
          "form": "비 젖는것도 싫고, 햇빛 쨍한 날을 좋아해서요 ㅋㅋ",
          "original_form": "비 젖는것도 싫고, 햇빛 쨍한 날을 좋아해서요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002607.1.11",
          "form": "어떤 날씨 좋아하세요?",
          "original_form": "어떤 날씨 좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 14:05"
        },
        {
          "id": "MDRW2100002607.1.12",
          "form": "전ㅠㅜ쨍한것보다는 그냥 선선한가을 날씨요",
          "original_form": "전ㅠㅜ쨍한것보다는 그냥 선선한가을 날씨요",
          "speaker_id": "2",
          "time": "20210820 14:09"
        },
        {
          "id": "MDRW2100002607.1.13",
          "form": "그것도 좋지요! 봄 가을같은 적당히 선선한 날씨",
          "original_form": "그것도 좋지요! 봄 가을같은 적당히 선선한 날씨",
          "speaker_id": "1",
          "time": "20210820 14:14"
        },
        {
          "id": "MDRW2100002607.1.14",
          "form": "맞아요 덥지도않고 춥지도 않은ㅋ",
          "original_form": "맞아요 덥지도않고 춥지도 않은ㅋ",
          "speaker_id": "2",
          "time": "20210820 14:19"
        },
        {
          "id": "MDRW2100002607.1.15",
          "form": "아마 옷도 좀 두껍게 입어야하지 않을지.. 벌써 반팔은 좀 추운느낌들어요",
          "original_form": "아마 옷도 좀 두껍게 입어야하지 않을지.. 벌써 반팔은 좀 추운느낌들어요",
          "speaker_id": "1",
          "time": "20210820 14:21"
        }
      ]
