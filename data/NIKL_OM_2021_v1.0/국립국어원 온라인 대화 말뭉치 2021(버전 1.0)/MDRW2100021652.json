      "utterance": [
        {
          "id": "MDRW2100021652.1.1",
          "form": "커뮤 많이 하세요?",
          "original_form": "커뮤 많이 하세요?",
          "speaker_id": "1",
          "time": "20210828 21:16"
        },
        {
          "id": "MDRW2100021652.1.2",
          "form": "인터넷은 자주해요!",
          "original_form": "인터넷은 자주해요!",
          "speaker_id": "2",
          "time": "20210828 21:17"
        },
        {
          "id": "MDRW2100021652.1.3",
          "form": "커뮤니티는 아예 안하세요?",
          "original_form": "커뮤니티는 아예 안하세요?",
          "speaker_id": "1",
          "time": "20210828 21:18"
        },
        {
          "id": "MDRW2100021652.1.4",
          "form": "음 딱히 없는 것 같아요 여기저기 돌아다니는 타입이라",
          "original_form": "음 딱히 없는 것 같아요 여기저기 돌아다니는 타입이라",
          "speaker_id": "2",
          "time": "20210828 21:19"
        },
        {
          "id": "MDRW2100021652.1.5",
          "form": "커뮤니티하시나요?",
          "original_form": "커뮤니티하시나요?",
          "speaker_id": "2",
          "time": "20210828 21:19"
        },
        {
          "id": "MDRW2100021652.1.6",
          "form": "대형 커뮤니티 눈팅이요..",
          "original_form": "대형 커뮤니티 눈팅이요..",
          "speaker_id": "1",
          "time": "20210828 21:19"
        },
        {
          "id": "MDRW2100021652.1.7",
          "form": "유모아 사이트나.. 아이티나..",
          "original_form": "유모아 사이트나.. 아이티나..",
          "speaker_id": "1",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100021652.1.8",
          "form": "유머사이트 말씀하시는건가요?",
          "original_form": "유머사이트 말씀하시는건가요?",
          "speaker_id": "2",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100021652.1.9",
          "form": "네... 혹시 웃대라고 들어 보셧나여?",
          "original_form": "네... 혹시 웃대라고 들어 보셧나여?",
          "speaker_id": "1",
          "time": "20210828 21:23"
        },
        {
          "id": "MDRW2100021652.1.10",
          "form": "아 네네 들어봤어요",
          "original_form": "아 네네 들어봤어요",
          "speaker_id": "2",
          "time": "20210828 21:23"
        },
        {
          "id": "MDRW2100021652.1.11",
          "form": "거기 주로 눈팅해요... \r\n커뮤 안하면 인터넷 주로 어디 보세요?",
          "original_form": "거기 주로 눈팅해요... \r\n커뮤 안하면 인터넷 주로 어디 보세요?",
          "speaker_id": "1",
          "time": "20210828 21:23"
        },
        {
          "id": "MDRW2100021652.1.12",
          "form": "그냥 다음인기글 보거나 웹툰 보고~ 기사들 읽고 그래요",
          "original_form": "그냥 다음인기글 보거나 웹툰 보고~ 기사들 읽고 그래요",
          "speaker_id": "2",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100021652.1.13",
          "form": "인기글이 모에요? 블로그 인기글 이런거에요?",
          "original_form": "인기글이 모에요? 블로그 인기글 이런거에요?",
          "speaker_id": "1",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100021652.1.14",
          "form": "다음카페들에서 올라온 게시글들중에 인기있는 글들이 전체공개로 올라와요",
          "original_form": "다음카페들에서 올라온 게시글들중에 인기있는 글들이 전체공개로 올라와요",
          "speaker_id": "2",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100021652.1.15",
          "form": "가입안해도 전부 볼수있게끔",
          "original_form": "가입안해도 전부 볼수있게끔",
          "speaker_id": "2",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100021652.1.16",
          "form": "인스타그램 인기글 같이 생각하시면 될거같아요",
          "original_form": "인스타그램 인기글 같이 생각하시면 될거같아요",
          "speaker_id": "2",
          "time": "20210828 21:26"
        }
      ]
