        {
          "id": "MDRW2100036681.1.1",
          "form": "네네 거기 루지도재밌구 바다도 좋아요",
          "original_form": "네네 거기 루지도재밌구 바다도 좋아요",
          "speaker_id": "1",
          "time": "20210831 08:42"
        },
        {
          "id": "MDRW2100036681.1.2",
          "form": "오오.. 좋네용",
          "original_form": "오오.. 좋네용",
          "speaker_id": "2",
          "time": "20210831 08:42"
        },
        {
          "id": "MDRW2100036681.1.3",
          "form": "가고싶은 공연 있으세요",
          "original_form": "가고싶은 공연 있으세요",
          "speaker_id": "1",
          "time": "20210831 08:42"
        },
        {
          "id": "MDRW2100036681.1.4",
          "form": "콘서트요!",
          "original_form": "콘서트요!",
          "speaker_id": "2",
          "time": "20210831 08:52"
        },
        {
          "id": "MDRW2100036681.1.5",
          "form": "딱 정해진건 아닌데 느껴보고싶어요!",
          "original_form": "딱 정해진건 아닌데 느껴보고싶어요!",
          "speaker_id": "2",
          "time": "20210831 08:52"
        },
        {
          "id": "MDRW2100036681.1.6",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 08:52"
        },
        {
          "id": "MDRW2100036681.1.7",
          "form": "저는 아이돌 콘서트 가고파요 ㅎ",
          "original_form": "저는 아이돌 콘서트 가고파요 ㅎ",
          "speaker_id": "1",
          "time": "20210831 11:01"
        },
        {
          "id": "MDRW2100036681.1.8",
          "form": "오 좋아하는 아이돌 있으세요?",
          "original_form": "오 좋아하는 아이돌 있으세요?",
          "speaker_id": "2",
          "time": "20210831 11:18"
        },
        {
          "id": "MDRW2100036681.1.9",
          "form": "저는 아스트로 좋아행6",
          "original_form": "저는 아스트로 좋아행6",
          "speaker_id": "1",
          "time": "20210831 14:45"
        },
        {
          "id": "MDRW2100036681.1.10",
          "form": "해요",
          "original_form": "해요",
          "speaker_id": "1",
          "time": "20210831 14:45"
        },
        {
          "id": "MDRW2100036681.1.11",
          "form": "오우 아스트로",
          "original_form": "오우 아스트로",
          "speaker_id": "2",
          "time": "20210831 16:39"
        },
        {
          "id": "MDRW2100036681.1.12",
          "form": "저 문빈 좋아해여",
          "original_form": "저 문빈 좋아해여",
          "speaker_id": "2",
          "time": "20210831 16:40"
        },
        {
          "id": "MDRW2100036681.1.13",
          "form": "헐 아시는구나",
          "original_form": "헐 아시는구나",
          "speaker_id": "1",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100036681.1.14",
          "form": "저도 차은우랑 문빈이요",
          "original_form": "저도 차은우랑 문빈이요",
          "speaker_id": "1",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100036681.1.15",
          "form": "사실 다 좋아해요",
          "original_form": "사실 다 좋아해요",
          "speaker_id": "1",
          "time": "20210831 17:34"
        },
        {
          "id": "MDRW2100036681.1.16",
          "form": "사실 외모가 뛰어나니 눈길 가고 그러다보면 또 다른게 보이고 그르잖아요",
          "original_form": "사실 외모가 뛰어나니 눈길 가고 그러다보면 또 다른게 보이고 그르잖아요",
          "speaker_id": "2",
          "time": "20210831 17:37"
        },
        {
          "id": "MDRW2100036681.1.17",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 18:23"
        },
        {
          "id": "MDRW2100036681.1.18",
          "form": "외모도 실력도 최고라서 동경합니다 ㅋㅋ",
          "original_form": "외모도 실력도 최고라서 동경합니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:23"
        },
        {
          "id": "MDRW2100036681.1.19",
          "form": "ㅋㅋ진짜요 노래도 좋더라구요",
          "original_form": "ㅋㅋ진짜요 노래도 좋더라구요",
          "speaker_id": "2",
          "time": "20210831 18:24"
        },
        {
