      "utterance": [
        {
          "id": "MDRW2100043203.1.1",
          "form": "처방은아니고",
          "original_form": "처방은아니고",
          "speaker_id": "2",
          "time": "20210907 15:41"
        },
        {
          "id": "MDRW2100043203.1.2",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210907 15:41"
        },
        {
          "id": "MDRW2100043203.1.3",
          "form": "그 머더라 바이탈뷰티에서 나온",
          "original_form": "그 머더라 바이탈뷰티에서 나온",
          "speaker_id": "2",
          "time": "20210907 15:41"
        },
        {
          "id": "MDRW2100043203.1.4",
          "form": "그린메타",
          "original_form": "그린메타",
          "speaker_id": "2",
          "time": "20210907 15:41"
        },
        {
          "id": "MDRW2100043203.1.5",
          "form": "엄마랑 5마넌씩 해서 샀는데 한달도안됐는데 다먹은듯...?...ㅋㅋㅋㅋ 또사야돼",
          "original_form": "엄마랑 5마넌씩 해서 샀는데 한달도안됐는데 다먹은듯...?...ㅋㅋㅋㅋ 또사야돼",
          "speaker_id": "2",
          "time": "20210907 15:42"
        },
        {
          "id": "MDRW2100043203.1.6",
          "form": "뭐든 먹으면좋겠지",
          "original_form": "뭐든 먹으면좋겠지",
          "speaker_id": "1",
          "time": "20210907 15:42"
        },
        {
          "id": "MDRW2100043203.1.7",
          "form": "그래야지..",
          "original_form": "그래야지..",
          "speaker_id": "2",
          "time": "20210907 15:42"
        },
        {
          "id": "MDRW2100043203.1.8",
          "form": "10만원이야?",
          "original_form": "10만원이야?",
          "speaker_id": "1",
          "time": "20210907 15:42"
        },
        {
          "id": "MDRW2100043203.1.9",
          "form": "비싼데.....가격에바야",
          "original_form": "비싼데.....가격에바야",
          "speaker_id": "2",
          "time": "20210907 15:42"
        },
        {
          "id": "MDRW2100043203.1.10",
          "form": "응근데 3개월치..",
          "original_form": "응근데 3개월치..",
          "speaker_id": "2",
          "time": "20210907 15:42"
        },
        {
          "id": "MDRW2100043203.1.11",
          "form": "근데비싸다..3개월치해도",
          "original_form": "근데비싸다..3개월치해도",
          "speaker_id": "1",
          "time": "20210907 15:42"
        },
        {
          "id": "MDRW2100043203.1.12",
          "form": "같이먹어서 너무빨리먹어",
          "original_form": "같이먹어서 너무빨리먹어",
          "speaker_id": "2",
          "time": "20210907 15:42"
        },
        {
          "id": "MDRW2100043203.1.13",
          "form": "그렇긴해 가끔 2포먹어도 되거든",
          "original_form": "그렇긴해 가끔 2포먹어도 되거든",
          "speaker_id": "2",
          "time": "20210907 15:42"
        },
        {
          "id": "MDRW2100043203.1.14",
          "form": "그래서 빨리먹게돼",
          "original_form": "그래서 빨리먹게돼",
          "speaker_id": "2",
          "time": "20210907 15:42"
        },
        {
          "id": "MDRW2100043203.1.15",
          "form": "1포만드ㅔ요",
          "original_form": "1포만드ㅔ요",
          "speaker_id": "1",
          "time": "20210907 15:42"
        },
        {
          "id": "MDRW2100043203.1.16",
          "form": "나는 그러는편..",
          "original_form": "나는 그러는편..",
          "speaker_id": "2",
          "time": "20210907 15:43"
        },
        {
          "id": "MDRW2100043203.1.17",
          "form": "기름진거 먹으면 먹긴하고",
          "original_form": "기름진거 먹으면 먹긴하고",
          "speaker_id": "2",
          "time": "20210907 15:43"
        },
        {
          "id": "MDRW2100043203.1.18",
          "form": "튀긴거...",
          "original_form": "튀긴거...",
          "speaker_id": "2",
          "time": "20210907 15:43"
        },
        {
          "id": "MDRW2100043203.1.19",
          "form": "너는진짜 기름지고튀긴거 피해야해",
          "original_form": "너는진짜 기름지고튀긴거 피해야해",
          "speaker_id": "1",
          "time": "20210907 15:43"
        },
        {
          "id": "MDRW2100043203.1.20",
          "form": "머먹어요 그러면..",
          "original_form": "머먹어요 그러면..",
          "speaker_id": "2",
          "time": "20210907 15:43"
        },
        {
          "id": "MDRW2100043203.1.21",
          "form": "그건또그래..그게맛있는데",
          "original_form": "그건또그래..그게맛있는데",
          "speaker_id": "1",
          "time": "20210907 15:43"
        }
      ]
