      "utterance": [
        {
          "id": "MDRW2100018963.1.1",
          "form": "좋은 카페 많이 알려주셔서 감사해요",
          "original_form": "좋은 카페 많이 알려주셔서 감사해요",
          "speaker_id": "1",
          "time": "20210827 14:07"
        },
        {
          "id": "MDRW2100018963.1.2",
          "form": "담에 또 괜찮으실때 대화 주세요!!",
          "original_form": "담에 또 괜찮으실때 대화 주세요!!",
          "speaker_id": "1",
          "time": "20210827 14:07"
        },
        {
          "id": "MDRW2100018963.1.3",
          "form": "점심드셨나요?",
          "original_form": "점심드셨나요?",
          "speaker_id": "2",
          "time": "20210828 13:28"
        },
        {
          "id": "MDRW2100018963.1.4",
          "form": "조금 전에 먹었습니다!",
          "original_form": "조금 전에 먹었습니다!",
          "speaker_id": "1",
          "time": "20210828 13:58"
        },
        {
          "id": "MDRW2100018963.1.5",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "1",
          "time": "20210828 13:58"
        },
        {
          "id": "MDRW2100018963.1.6",
          "form": "네! 오므라이스 먹었어요",
          "original_form": "네! 오므라이스 먹었어요",
          "speaker_id": "2",
          "time": "20210828 14:07"
        },
        {
          "id": "MDRW2100018963.1.7",
          "form": "오 맛있겠다",
          "original_form": "오 맛있겠다",
          "speaker_id": "1",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100018963.1.8",
          "form": "저는 고기먹었어요",
          "original_form": "저는 고기먹었어요",
          "speaker_id": "1",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100018963.1.9",
          "form": "직접 만드셨어요?",
          "original_form": "직접 만드셨어요?",
          "speaker_id": "1",
          "time": "20210828 14:41"
        },
        {
          "id": "MDRW2100018963.1.10",
          "form": "우와 구워드셨나요?",
          "original_form": "우와 구워드셨나요?",
          "speaker_id": "2",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100018963.1.11",
          "form": "네 만들었어요",
          "original_form": "네 만들었어요",
          "speaker_id": "2",
          "time": "20210828 14:42"
        },
        {
          "id": "MDRW2100018963.1.12",
          "form": "금손이시네요",
          "original_form": "금손이시네요",
          "speaker_id": "1",
          "time": "20210828 14:43"
        },
        {
          "id": "MDRW2100018963.1.13",
          "form": "저는 알리오올리오랑 라면만 할 줄 알아요 ",
          "original_form": "저는 알리오올리오랑 라면만 할 줄 알아요 ",
          "speaker_id": "1",
          "time": "20210828 14:43"
        },
        {
          "id": "MDRW2100018963.1.14",
          "form": "그럼 룸메분이 주로 요리하시나요?",
          "original_form": "그럼 룸메분이 주로 요리하시나요?",
          "speaker_id": "2",
          "time": "20210828 14:45"
        },
        {
          "id": "MDRW2100018963.1.15",
          "form": "그냥 재료가있어서 볶아먹었어요!",
          "original_form": "그냥 재료가있어서 볶아먹었어요!",
          "speaker_id": "2",
          "time": "20210828 14:46"
        },
        {
          "id": "MDRW2100018963.1.16",
          "form": "저는 가족들과 같이살아요!",
          "original_form": "저는 가족들과 같이살아요!",
          "speaker_id": "1",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100018963.1.17",
          "form": "자취하세용?",
          "original_form": "자취하세용?",
          "speaker_id": "1",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100018963.1.18",
          "form": "앗 그러시구나 네 저는 자취해요",
          "original_form": "앗 그러시구나 네 저는 자취해요",
          "speaker_id": "2",
          "time": "20210828 15:28"
        },
        {
          "id": "MDRW2100018963.1.19",
          "form": "점심 무슨고기 드셨나요?",
          "original_form": "점심 무슨고기 드셨나요?",
          "speaker_id": "2",
          "time": "20210828 15:28"
        },
        {
          "id": "MDRW2100018963.1.20",
          "form": "돼지고기 한판 시켜서 구워먹었습니다",
          "original_form": "돼지고기 한판 시켜서 구워먹었습니다",
          "speaker_id": "1",
          "time": "20210828 16:11"
        },
        {
          "id": "MDRW2100018963.1.21",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 16:11"
        },
        {
          "id": "MDRW2100018963.1.22",
          "form": "자취하면 음식고민 많으시겠어요",
          "original_form": "자취하면 음식고민 많으시겠어요",
          "speaker_id": "1",
          "time": "20210828 16:11"
        },
        {
          "id": "MDRW2100018963.1.23",
          "form": "채소같은건 사두기도 애매하고 ㅠ",
          "original_form": "채소같은건 사두기도 애매하고 ㅠ",
          "speaker_id": "1",
          "time": "20210828 16:11"
        },
        {
          "id": "MDRW2100018963.1.24",
          "form": "그죠...식재료관리가 제일 고민이죠ㅠㅠ",
          "original_form": "그죠...식재료관리가 제일 고민이죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 16:12"
        },
        {
          "id": "MDRW2100018963.1.25",
          "form": "배달음식도 최소주문금액이 있으니까..",
          "original_form": "배달음식도 최소주문금액이 있으니까..",
          "speaker_id": "1",
          "time": "20210828 16:12"
        },
        {
          "id": "MDRW2100018963.1.26",
          "form": "특히 여름이라 더힘들었어요",
          "original_form": "특히 여름이라 더힘들었어요",
          "speaker_id": "2",
          "time": "20210828 16:12"
        },
        {
          "id": "MDRW2100018963.1.27",
          "form": "그죠...",
          "original_form": "그죠...",
          "speaker_id": "2",
          "time": "20210828 16:12"
        },
        {
          "id": "MDRW2100018963.1.28",
          "form": "배달료도 문제구요",
          "original_form": "배달료도 문제구요",
          "speaker_id": "2",
          "time": "20210828 16:12"
        },
        {
          "id": "MDRW2100018963.1.29",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 16:13"
        },
        {
          "id": "MDRW2100018963.1.30",
          "form": "저는 조금 거리있는곳에",
          "original_form": "저는 조금 거리있는곳에",
          "speaker_id": "1",
          "time": "20210828 16:13"
        },
        {
          "id": "MDRW2100018963.1.31",
          "form": "카페 맛있길래 시키려했더니 배달료를 7~8천원 이렇게 요구해서... 놀랐어요",
          "original_form": "카페 맛있길래 시키려했더니 배달료를 7~8천원 이렇게 요구해서... 놀랐어요",
          "speaker_id": "1",
          "time": "20210828 16:13"
        }
      ]
