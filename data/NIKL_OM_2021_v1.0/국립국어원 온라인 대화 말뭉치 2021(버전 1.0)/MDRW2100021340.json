      "utterance": [
        {
          "id": "MDRW2100021340.1.1",
          "form": "피부관리 많이 하세요?",
          "original_form": "피부관리 많이 하세요?",
          "speaker_id": "1",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100021340.1.2",
          "form": "해야 하는데 세안하고 피지오겔 하나 바르고 끝... ㅎ",
          "original_form": "해야 하는데 세안하고 피지오겔 하나 바르고 끝... ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:00"
        },
        {
          "id": "MDRW2100021340.1.3",
          "form": "피부관리 하세요?",
          "original_form": "피부관리 하세요?",
          "speaker_id": "2",
          "time": "20210829 00:09"
        },
        {
          "id": "MDRW2100021340.1.4",
          "form": "저도 게을러서 잘 못해요 아니 안해요 ㅜ",
          "original_form": "저도 게을러서 잘 못해요 아니 안해요 ㅜ",
          "speaker_id": "1",
          "time": "20210829 01:36"
        },
        {
          "id": "MDRW2100021340.1.5",
          "form": "마스크팩도 있는데 안 하게 되네요..",
          "original_form": "마스크팩도 있는데 안 하게 되네요..",
          "speaker_id": "2",
          "time": "20210829 14:31"
        },
        {
          "id": "MDRW2100021340.1.6",
          "form": "그러게 말이에요 강제성이 없으니 우선순위에 밀려서  ㅜㅜ",
          "original_form": "그러게 말이에요 강제성이 없으니 우선순위에 밀려서  ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 20:06"
        },
        {
          "id": "MDRW2100021340.1.7",
          "form": "피부 노화 오고 나면 관리 안한거 후회 할 거 같아요ㅡㅡ",
          "original_form": "피부 노화 오고 나면 관리 안한거 후회 할 거 같아요ㅡㅡ",
          "speaker_id": "2",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100021340.1.8",
          "form": "맞아요 ㅠㅠ 그럴것같아요 ㅜㅜ",
          "original_form": "맞아요 ㅠㅠ 그럴것같아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 20:20"
        },
        {
          "id": "MDRW2100021340.1.9",
          "form": "썬크림이라도 발라야 하는데 썬크림도 안바르고.. 큰일이에요",
          "original_form": "썬크림이라도 발라야 하는데 썬크림도 안바르고.. 큰일이에요",
          "speaker_id": "2",
          "time": "20210829 20:21"
        },
        {
          "id": "MDRW2100021340.1.10",
          "form": "그르게요 후회할 짓만 ㅋ 썬크림 집에서도 바르라는데",
          "original_form": "그르게요 후회할 짓만 ㅋ 썬크림 집에서도 바르라는데",
          "speaker_id": "1",
          "time": "20210829 20:24"
        },
        {
          "id": "MDRW2100021340.1.11",
          "form": "요즘은 남자들도 피부관리 엄청 열심이던데요",
          "original_form": "요즘은 남자들도 피부관리 엄청 열심이던데요",
          "speaker_id": "2",
          "time": "20210829 20:26"
        },
        {
          "id": "MDRW2100021340.1.12",
          "form": "자극받아야 하는데 ㅋㅋ",
          "original_form": "자극받아야 하는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:54"
        },
        {
          "id": "MDRW2100021340.1.13",
          "form": "저도 안 바르는 비비가 기본이라고 하네요",
          "original_form": "저도 안 바르는 비비가 기본이라고 하네요",
          "speaker_id": "2",
          "time": "20210829 21:12"
        },
        {
          "id": "MDRW2100021340.1.14",
          "form": "직장동료남자 눈썹도 그려요 ㅎㅎ전 안그리고 ㅋㅋ",
          "original_form": "직장동료남자 눈썹도 그려요 ㅎㅎ전 안그리고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100021340.1.15",
          "form": "아이라인 하는 남자도 있어요.\r\n저도 눈썹 안그려요 ㅋ",
          "original_form": "아이라인 하는 남자도 있어요.\r\n저도 눈썹 안그려요 ㅋ",
          "speaker_id": "2",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100021340.1.16",
          "form": "ㅎㅎㅎ 부지런한 관리하는 남자들 진짜 대단",
          "original_form": "ㅎㅎㅎ 부지런한 관리하는 남자들 진짜 대단",
          "speaker_id": "1",
          "time": "20210829 23:00"
        },
        {
          "id": "MDRW2100021340.1.17",
          "form": "그러니까요. 요즘 남자 호ㅏ장품도 다양해진거  같더라구요",
          "original_form": "그러니까요. 요즘 남자 호ㅏ장품도 다양해진거  같더라구요",
          "speaker_id": "2",
