      "utterance": [
        {
          "id": "MDRW2100017944.1.1",
          "form": "오늘 뭐드실 예정?",
          "original_form": "오늘 뭐드실 예정?",
          "speaker_id": "1",
          "time": "20210903 08:28"
        },
        {
          "id": "MDRW2100017944.1.2",
          "form": "아직 모르겠네",
          "original_form": "아직 모르겠네",
          "speaker_id": "2",
          "time": "20210903 08:29"
        },
        {
          "id": "MDRW2100017944.1.3",
          "form": "고깃국 먹지않을까?",
          "original_form": "고깃국 먹지않을까?",
          "speaker_id": "2",
          "time": "20210903 08:29"
        },
        {
          "id": "MDRW2100017944.1.4",
          "form": "ㅋㅋ 가지에 오리넣어 볶아야겠네",
          "original_form": "ㅋㅋ 가지에 오리넣어 볶아야겠네",
          "speaker_id": "1",
          "time": "20210903 08:30"
        },
        {
          "id": "MDRW2100017944.1.5",
          "form": "그래도 좋아 ㅋㅋ",
          "original_form": "그래도 좋아 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 08:34"
        },
        {
          "id": "MDRW2100017944.1.6",
          "form": "점심메뉴 머삼?",
          "original_form": "점심메뉴 머삼?",
          "speaker_id": "1",
          "time": "20210903 08:53"
        },
        {
          "id": "MDRW2100017944.1.7",
          "form": "글쎄 칼국수?",
          "original_form": "글쎄 칼국수?",
          "speaker_id": "2",
          "time": "20210903 09:38"
        },
        {
          "id": "MDRW2100017944.1.8",
          "form": "반계탕먹을까?",
          "original_form": "반계탕먹을까?",
          "speaker_id": "2",
          "time": "20210903 09:39"
        },
        {
          "id": "MDRW2100017944.1.9",
          "form": "ㅋㅋ언니가 선택권이 있어?",
          "original_form": "ㅋㅋ언니가 선택권이 있어?",
          "speaker_id": "1",
          "time": "20210903 09:40"
        },
        {
          "id": "MDRW2100017944.1.10",
          "form": "회사근처에 식당은 정해져있으니까.. 돈까스 칼국소 아딸 삼계탕.. 돌아가며 먹는 ㅡㅡㅋㅋㅋㅋ",
          "original_form": "회사근처에 식당은 정해져있으니까.. 돈까스 칼국소 아딸 삼계탕.. 돌아가며 먹는 ㅡㅡㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:42"
        },
        {
          "id": "MDRW2100017944.1.11",
          "form": "ㅋㅋ 당신의 선택!",
          "original_form": "ㅋㅋ 당신의 선택!",
          "speaker_id": "1",
          "time": "20210903 09:44"
        },
        {
          "id": "MDRW2100017944.1.12",
          "form": "돈까스 ㅋㅋㅋ",
          "original_form": "돈까스 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 13:35"
        },
        {
          "id": "MDRW2100017944.1.13",
          "form": "ㅋㅋ 아침과 전혀 다른 선택ㅋㅋ",
          "original_form": "ㅋㅋ 아침과 전혀 다른 선택ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 13:42"
        },
        {
          "id": "MDRW2100017944.1.14",
          "form": "그런거요",
          "original_form": "그런거요",
          "speaker_id": "2",
          "time": "20210903 13:50"
        },
        {
          "id": "MDRW2100017944.1.15",
          "form": "퇴근함??? 저녁머먹지??.",
          "original_form": "퇴근함??? 저녁머먹지??.",
          "speaker_id": "1",
          "time": "20210903 13:59"
        },
        {
          "id": "MDRW2100017944.1.16",
          "form": "응 가구있다",
          "original_form": "응 가구있다",
          "speaker_id": "2",
          "time": "20210903 14:16"
        },
        {
          "id": "MDRW2100017944.1.17",
          "form": "난 국에 걍 밥먹음ㅋㅋ",
          "original_form": "난 국에 걍 밥먹음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:29"
        },
        {
          "id": "MDRW2100017944.1.18",
          "form": "난 밥먹고 바로 가면 된다고 생각한게 이상해서 보니",
          "original_form": "난 밥먹고 바로 가면 된다고 생각한게 이상해서 보니",
          "speaker_id": "2",
          "time": "20210903 14:35"
        },
        {
          "id": "MDRW2100017944.1.19",
          "form": "예약이 3시네",
          "original_form": "예약이 3시네",
          "speaker_id": "2",
          "time": "20210903 14:35"
        },
        {
          "id": "MDRW2100017944.1.20",
          "form": "4시는 어제 문자 온 시간이였어 ㅡㅡ",
          "original_form": "4시는 어제 문자 온 시간이였어 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210903 14:35"
        },
        {
          "id": "MDRW2100017944.1.21",
          "form": "똑똑하다ㅋㅋ",
          "original_form": "똑똑하다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:42"
        },
        {
          "id": "MDRW2100017944.1.22",
