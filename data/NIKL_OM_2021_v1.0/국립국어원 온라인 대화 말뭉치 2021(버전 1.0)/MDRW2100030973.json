      "utterance": [
        {
          "id": "MDRW2100030973.1.1",
          "form": "저녁 식사는 하셨나요",
          "original_form": "저녁 식사는 하셨나요",
          "speaker_id": "1",
          "time": "20210904 22:41"
        },
        {
          "id": "MDRW2100030973.1.2",
          "form": "했습니다 드셨나요",
          "original_form": "했습니다 드셨나요",
          "speaker_id": "2",
          "time": "20210904 22:41"
        },
        {
          "id": "MDRW2100030973.1.3",
          "form": "저는 전어회를 먹었습니다",
          "original_form": "저는 전어회를 먹었습니다",
          "speaker_id": "2",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100030973.1.4",
          "form": "세꼬시입니까",
          "original_form": "세꼬시입니까",
          "speaker_id": "1",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100030973.1.5",
          "form": "ㅣ새 것이랑 뼈 없는 거랑 반반으로 먹었습니다",
          "original_form": "ㅣ새 것이랑 뼈 없는 거랑 반반으로 먹었습니다",
          "speaker_id": "2",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100030973.1.6",
          "form": "전어 회 먹어도 배가 부른 가요",
          "original_form": "전어 회 먹어도 배가 부른 가요",
          "speaker_id": "1",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100030973.1.7",
          "form": "네 배가 부르네요 맛있어요",
          "original_form": "네 배가 부르네요 맛있어요",
          "speaker_id": "2",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100030973.1.8",
          "form": "그것만 드시지 않으셨겠죠",
          "original_form": "그것만 드시지 않으셨겠죠",
          "speaker_id": "1",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100030973.1.9",
          "form": "전어 철이라서 그런지 굉장히 고소했습니다",
          "original_form": "전어 철이라서 그런지 굉장히 고소했습니다",
          "speaker_id": "2",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100030973.1.10",
          "form": "완전 완전 맛있었겠 씁니다 부럽습니다",
          "original_form": "완전 완전 맛있었겠 씁니다 부럽습니다",
          "speaker_id": "1",
          "time": "20210904 22:43"
        },
        {
          "id": "MDRW2100030973.1.11",
          "form": "당연하지요 술 한잔 해야죠",
          "original_form": "당연하지요 술 한잔 해야죠",
          "speaker_id": "2",
          "time": "20210904 22:43"
        },
        {
          "id": "MDRW2100030973.1.12",
          "form": "기름 지지요 고소하게",
          "original_form": "기름 지지요 고소하게",
          "speaker_id": "1",
          "time": "20210904 22:43"
        },
        {
          "id": "MDRW2100030973.1.13",
          "form": "기름진 음식을 먹을 때에는 입안을 깔끔하게 행궈 줄 수 있는 소주를 한 잔 해야 해요",
          "original_form": "기름진 음식을 먹을 때에는 입안을 깔끔하게 행궈 줄 수 있는 소주를 한 잔 해야 해요",
          "speaker_id": "2",
          "time": "20210904 22:43"
        },
        {
          "id": "MDRW2100030973.1.14",
          "form": "오늘은 CU에 가서 독도라는 소주를 한 병 사 왔습니다",
          "original_form": "오늘은 CU에 가서 독도라는 소주를 한 병 사 왔습니다",
          "speaker_id": "2",
          "time": "20210904 22:43"
        },
        {
          "id": "MDRW2100030973.1.15",
          "form": "제가 먹은 것이 초라해 지는군요",
          "original_form": "제가 먹은 것이 초라해 지는군요",
          "speaker_id": "1",
          "time": "20210904 22:43"
        },
        {
          "id": "MDRW2100030973.1.16",
          "form": "신상 이네요",
          "original_form": "신상 이네요",
          "speaker_id": "2",
          "time": "20210904 22:43"
        }
      ]
