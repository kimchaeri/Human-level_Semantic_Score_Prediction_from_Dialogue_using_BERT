      "utterance": [
        {
          "id": "MDRW2100001882.1.1",
          "form": "강아지가 산책 좋아하는데",
          "original_form": "강아지가 산책 좋아하는데",
          "speaker_id": "1",
          "time": "20210819 21:56"
        },
        {
          "id": "MDRW2100001882.1.2",
          "form": "산책이라는 직접적인 단어를 말하면",
          "original_form": "산책이라는 직접적인 단어를 말하면",
          "speaker_id": "1",
          "time": "20210819 21:57"
        },
        {
          "id": "MDRW2100001882.1.3",
          "form": "안되더라구요",
          "original_form": "안되더라구요",
          "speaker_id": "1",
          "time": "20210819 21:57"
        },
        {
          "id": "MDRW2100001882.1.4",
          "form": "너무 좋아서 나가고 싶어하거든요",
          "original_form": "너무 좋아서 나가고 싶어하거든요",
          "speaker_id": "1",
          "time": "20210819 21:57"
        },
        {
          "id": "MDRW2100001882.1.5",
          "form": "생각만 해도 너무 귀여워요..",
          "original_form": "생각만 해도 너무 귀여워요..",
          "speaker_id": "2",
          "time": "20210819 21:58"
        },
        {
          "id": "MDRW2100001882.1.6",
          "form": "고양이 키우던 친구 집 간 적이 있었는데 맘마라는 소리 들으면 진짜 난리가 나더라구요ㅋㅋㅋ",
          "original_form": "고양이 키우던 친구 집 간 적이 있었는데 맘마라는 소리 들으면 진짜 난리가 나더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:58"
        },
        {
          "id": "MDRW2100001882.1.7",
          "form": "고양이도 알아들어요?",
          "original_form": "고양이도 알아들어요?",
          "speaker_id": "1",
          "time": "20210819 22:00"
        },
        {
          "id": "MDRW2100001882.1.8",
          "form": "신기하다",
          "original_form": "신기하다",
          "speaker_id": "1",
          "time": "20210819 22:00"
        },
        {
          "id": "MDRW2100001882.1.9",
          "form": "우리집 개는 까까! 이러면",
          "original_form": "우리집 개는 까까! 이러면",
          "speaker_id": "1",
          "time": "20210819 22:01"
        },
        {
          "id": "MDRW2100001882.1.10",
          "form": "귀를 쫑긋쫑긋 움직이던데",
          "original_form": "귀를 쫑긋쫑긋 움직이던데",
          "speaker_id": "1",
          "time": "20210819 22:01"
        },
        {
          "id": "MDRW2100001882.1.11",
          "form": "네 그런가봐요 밥 줄 때 맘마먹을까? 이렇게 물어보는게 습관인데",
          "original_form": "네 그런가봐요 밥 줄 때 맘마먹을까? 이렇게 물어보는게 습관인데",
          "speaker_id": "2",
          "time": "20210819 22:37"
        },
        {
          "id": "MDRW2100001882.1.12",
          "form": "그러면 막 뛰어와요ㅋㅋㅋ",
          "original_form": "그러면 막 뛰어와요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:37"
        },
        {
          "id": "MDRW2100001882.1.13",
          "form": "아 너무 귀여울 거 같아요ㅠㅠㅠ",
          "original_form": "아 너무 귀여울 거 같아요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 22:37"
        },
        {
          "id": "MDRW2100001882.1.14",
          "form": "맘마라니",
          "original_form": "맘마라니",
          "speaker_id": "1",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001882.1.15",
          "form": "어린아이 느낌이네요",
          "original_form": "어린아이 느낌이네요",
          "speaker_id": "1",
          "time": "20210819 22:40"
        },
        {
          "id": "MDRW2100001882.1.16",
          "form": "까까! 하면 자기가 먼저 부엌으로 우다다 뛰어가요",
          "original_form": "까까! 하면 자기가 먼저 부엌으로 우다다 뛰어가요",
          "speaker_id": "1",
          "time": "20210819 22:40"
        },
        {
          "id": "MDRW2100001882.1.17",
          "form": "어떡해 너무 똑똑해요ㅋㅋㅋ",
          "original_form": "어떡해 너무 똑똑해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:06"
        },
        {
          "id": "MDRW2100001882.1.18",
          "form": "그쵸? 그리고 맘마 할 때 그 리듬감이 있는데",
          "original_form": "그쵸? 그리고 맘마 할 때 그 리듬감이 있는데",
          "speaker_id": "2",
          "time": "20210819 23:06"
        },
        {
          "id": "MDRW2100001882.1.19",
          "form": "그렇게 다른 단어 말해도 좋아해요",
          "original_form": "그렇게 다른 단어 말해도 좋아해요",
          "speaker_id": "2",
          "time": "20210819 23:06"
        },
        {
          "id": "MDRW2100001882.1.20",
          "form": "근데 그 순간만 똑똑해요",
          "original_form": "근데 그 순간만 똑똑해요",
          "speaker_id": "1",
          "time": "20210819 23:06"
        },
        {
          "id": "MDRW2100001882.1.21",
          "form": "다른때는 불러도 모른척해요",
          "original_form": "다른때는 불러도 모른척해요",
          "speaker_id": "1",
          "time": "20210819 23:07"
        },
        {
          "id": "MDRW2100001882.1.22",
          "form": "맞아요 소리로 인지하는것 같아요",
          "original_form": "맞아요 소리로 인지하는것 같아요",
          "speaker_id": "1",
          "time": "20210819 23:07"
        },
        {
          "id": "MDRW2100001882.1.23",
          "form": "모른척 하는게 진짜 똑똑한거 같기도 하고 그러네요..ㅋㅋㅋ",
          "original_form": "모른척 하는게 진짜 똑똑한거 같기도 하고 그러네요..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:07"
        },
        {
          "id": "MDRW2100001882.1.24",
          "form": "막 사고치곤 이름 못알아 듣는 척 하고 이러는거 너무 귀여워요",
          "original_form": "막 사고치곤 이름 못알아 듣는 척 하고 이러는거 너무 귀여워요",
          "speaker_id": "2",
          "time": "20210819 23:07"
        },
        {
          "id": "MDRW2100001882.1.25",
          "form": "사고 쳐서 눈치 슬쩍 본다니깐요",
          "original_form": "사고 쳐서 눈치 슬쩍 본다니깐요",
          "speaker_id": "1",
          "time": "20210819 23:11"
        },
        {
          "id": "MDRW2100001882.1.26",
          "form": "전 귀엽던데 견주인 동생은 불같이 화내더라구요",
          "original_form": "전 귀엽던데 견주인 동생은 불같이 화내더라구요",
          "speaker_id": "1",
          "time": "20210819 23:11"
        },
        {
          "id": "MDRW2100001882.1.27",
          "form": "당연한거지만 ㅋㅋㅋㅋㅋ",
          "original_form": "당연한거지만 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:11"
        },
        {
          "id": "MDRW2100001882.1.28",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210819 23:11"
        },
        {
          "id": "MDRW2100001882.1.29",
          "form": "사고치면 그 눈치보는게 너무 웃기고 귀여워요",
          "original_form": "사고치면 그 눈치보는게 너무 웃기고 귀여워요",
          "speaker_id": "2",
          "time": "20210819 23:12"
        },
        {
          "id": "MDRW2100001882.1.30",
          "form": "와 완전 공감",
          "original_form": "와 완전 공감",
          "speaker_id": "1",
          "time": "20210819 23:12"
        },
        {
          "id": "MDRW2100001882.1.31",
          "form": "혼나도 쪼르르 오는거 보면 짠하고 귀여워요",
          "original_form": "혼나도 쪼르르 오는거 보면 짠하고 귀여워요",
          "speaker_id": "1",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100001882.1.32",
          "form": "애교도 있구요",
          "original_form": "애교도 있구요",
          "speaker_id": "1",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100001882.1.33",
          "form": "그 눈빛이 너무 웃기지 않아요?",
          "original_form": "그 눈빛이 너무 웃기지 않아요?",
          "speaker_id": "2",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100001882.1.34",
          "form": "내가 잘못했는데... 그걸 너무 잘 아는 눈빛",
          "original_form": "내가 잘못했는데... 그걸 너무 잘 아는 눈빛",
          "speaker_id": "2",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100001882.1.35",
          "form": "우리집 개는 잘못 아니깐 자기 방석으로 도망가서 딴데 쳐다봐요",
          "original_form": "우리집 개는 잘못 아니깐 자기 방석으로 도망가서 딴데 쳐다봐요",
          "speaker_id": "1",
          "time": "20210819 23:14"
        },
        {
          "id": "MDRW2100001882.1.36",
          "form": "속에 사람이 있나싶었어요",
          "original_form": "속에 사람이 있나싶었어요",
          "speaker_id": "1",
          "time": "20210819 23:14"
        }
      ]
    }
