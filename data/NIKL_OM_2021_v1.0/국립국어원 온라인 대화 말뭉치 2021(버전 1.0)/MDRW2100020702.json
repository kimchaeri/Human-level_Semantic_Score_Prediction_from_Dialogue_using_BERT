      "utterance": [
        {
          "id": "MDRW2100020702.1.1",
          "form": "이번엔 무슨 음식으로 가볼까요",
          "original_form": "이번엔 무슨 음식으로 가볼까요",
          "speaker_id": "2",
          "time": "20210828 11:21"
        },
        {
          "id": "MDRW2100020702.1.2",
          "form": "군것질거리 약간 새콤달콤 이런거 좋아하세요",
          "original_form": "군것질거리 약간 새콤달콤 이런거 좋아하세요",
          "speaker_id": "1",
          "time": "20210828 11:22"
        },
        {
          "id": "MDRW2100020702.1.3",
          "form": "젤리 이런거",
          "original_form": "젤리 이런거",
          "speaker_id": "1",
          "time": "20210828 11:22"
        },
        {
          "id": "MDRW2100020702.1.4",
          "form": "저는 젤리를 좋아합니다",
          "original_form": "저는 젤리를 좋아합니다",
          "speaker_id": "2",
          "time": "20210828 11:22"
        },
        {
          "id": "MDRW2100020702.1.5",
          "form": "젤리 그.. 가루 묻은 젤리 아시나요",
          "original_form": "젤리 그.. 가루 묻은 젤리 아시나요",
          "speaker_id": "2",
          "time": "20210828 11:22"
        },
        {
          "id": "MDRW2100020702.1.6",
          "form": "복숭아맛",
          "original_form": "복숭아맛",
          "speaker_id": "2",
          "time": "20210828 11:22"
        },
        {
          "id": "MDRW2100020702.1.7",
          "form": "사우어",
          "original_form": "사우어",
          "speaker_id": "1",
          "time": "20210828 11:22"
        },
        {
          "id": "MDRW2100020702.1.8",
          "form": "혹시 레옹젤ㅈ리",
          "original_form": "혹시 레옹젤ㅈ리",
          "speaker_id": "1",
          "time": "20210828 11:22"
        },
        {
          "id": "MDRW2100020702.1.9",
          "form": "아닌가",
          "original_form": "아닌가",
          "speaker_id": "1",
          "time": "20210828 11:22"
        },
        {
          "id": "MDRW2100020702.1.10",
          "form": "아  그 천원짜리 젤리말하시는건가",
          "original_form": "아  그 천원짜리 젤리말하시는건가",
          "speaker_id": "1",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100020702.1.11",
          "form": "레몬맛도 있는거",
          "original_form": "레몬맛도 있는거",
          "speaker_id": "1",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100020702.1.12",
          "form": "영어써진 분홍색 포장지",
          "original_form": "영어써진 분홍색 포장지",
          "speaker_id": "1",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100020702.1.13",
          "form": "예 후자인거 같아요",
          "original_form": "예 후자인거 같아요",
          "speaker_id": "2",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100020702.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100020702.1.15",
          "form": "그거랑 박카스 젤리..",
          "original_form": "그거랑 박카스 젤리..",
          "speaker_id": "2",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100020702.1.16",
          "form": "저 그거 좋아해요ㅋㅋㅋ",
          "original_form": "저 그거 좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100020702.1.17",
          "form": "비타오백 젤리..",
          "original_form": "비타오백 젤리..",
          "speaker_id": "2",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100020702.1.18",
          "form": "박카스젤리는 어때요",
          "original_form": "박카스젤리는 어때요",
          "speaker_id": "1",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100020702.1.19",
          "form": "청포도 젤리 좋아해요",
          "original_form": "청포도 젤리 좋아해요",
          "speaker_id": "2",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100020702.1.20",
          "form": "박카스 향이나서 뭔가 피로회복되는 느낌이에요",
          "original_form": "박카스 향이나서 뭔가 피로회복되는 느낌이에요",
          "speaker_id": "2",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100020702.1.21",
          "form": "저 그 스크류바젤리는 먹어봤는데",
          "original_form": "저 그 스크류바젤리는 먹어봤는데",
          "speaker_id": "1",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100020702.1.22",
          "form": "오 식감은요",
          "original_form": "오 식감은요",
          "speaker_id": "1",
          "time": "20210828 11:24"
        },
        {
          "id": "MDRW2100020702.1.23",
          "form": "말캉말캉 평범합니다",
          "original_form": "말캉말캉 평범합니다",
          "speaker_id": "2",
          "time": "20210828 11:24"
        },
        {
          "id": "MDRW2100020702.1.24",
          "form": "스크류바젤리는 어떤가요",
          "original_form": "스크류바젤리는 어떤가요",
          "speaker_id": "2",
          "time": "20210828 11:24"
        },
        {
          "id": "MDRW2100020702.1.25",
          "form": "제타입이 아니었어요",
          "original_form": "제타입이 아니었어요",
          "speaker_id": "1",
          "time": "20210828 11:24"
        },
        {
          "id": "MDRW2100020702.1.26",
          "form": "저런..",
          "original_form": "저런..",
          "speaker_id": "2",
          "time": "20210828 11:24"
        },
        {
          "id": "MDRW2100020702.1.27",
          "form": "왜죠?",
          "original_form": "왜죠?",
          "speaker_id": "2",
          "time": "20210828 11:24"
        },
        {
          "id": "MDRW2100020702.1.28",
          "form": "약간 그",
          "original_form": "약간 그",
          "speaker_id": "1",
          "time": "20210828 11:24"
        },
        {
          "id": "MDRW2100020702.1.29",
          "form": "그거알아요",
          "original_form": "그거알아요",
          "speaker_id": "1",
          "time": "20210828 11:24"
        },
        {
          "id": "MDRW2100020702.1.30",
          "form": "그 외국젠리인데",
          "original_form": "그 외국젠리인데",
          "speaker_id": "1",
          "time": "20210828 11:25"
        },
        {
          "id": "MDRW2100020702.1.31",
          "form": "길다래서 겁나맛없는거",
          "original_form": "길다래서 겁나맛없는거",
          "speaker_id": "1",
          "time": "20210828 11:25"
        },
        {
          "id": "MDRW2100020702.1.32",
          "form": "미드같은거 보면 외국애들이 많이 먹는데",
          "original_form": "미드같은거 보면 외국애들이 많이 먹는데",
          "speaker_id": "1",
          "time": "20210828 11:25"
        },
        {
          "id": "MDRW2100020702.1.33",
          "form": "흠.. 설탕가루 묻어있는?",
          "original_form": "흠.. 설탕가루 묻어있는?",
          "speaker_id": "2",
          "time": "20210828 11:25"
        },
        {
          "id": "MDRW2100020702.1.34",
          "form": "묻었던가",
          "original_form": "묻었던가",
          "speaker_id": "1",
          "time": "20210828 11:25"
        },
        {
          "id": "MDRW2100020702.1.35",
          "form": "약간 딸기맛있는데",
          "original_form": "약간 딸기맛있는데",
          "speaker_id": "1",
          "time": "20210828 11:25"
        },
        {
          "id": "MDRW2100020702.1.36",
          "form": "먹어보면 별맛안나는",
          "original_form": "먹어보면 별맛안나는",
          "speaker_id": "1",
          "time": "20210828 11:25"
        },
        {
          "id": "MDRW2100020702.1.37",
          "form": "뭔지 알사요",
          "original_form": "뭔지 알사요",
          "speaker_id": "2",
          "time": "20210828 11:25"
        },
        {
          "id": "MDRW2100020702.1.38",
          "form": "완전 노맛",
          "original_form": "완전 노맛",
          "speaker_id": "2",
          "time": "20210828 11:25"
        },
        {
          "id": "MDRW2100020702.1.39",
          "form": "약간 타이어\n십\n늣\n맛이라 표현되는",
          "original_form": "약간 타이어\n십\n늣\n맛이라 표현되는",
          "speaker_id": "1",
          "time": "20210828 11:26"
        }
      ]
