      "utterance": [
        {
          "id": "MDRW2100023595.1.1",
          "form": "어렸을때 태권도를 좀",
          "original_form": "어렸을때 태권도를 좀",
          "speaker_id": "2",
          "time": "20210830 12:18"
        },
        {
          "id": "MDRW2100023595.1.2",
          "form": "그거가지고 이나이까지 써먹고 있습니다",
          "original_form": "그거가지고 이나이까지 써먹고 있습니다",
          "speaker_id": "2",
          "time": "20210830 12:18"
        },
        {
          "id": "MDRW2100023595.1.3",
          "form": "아녔으면 지금쯤 기어다니고 있을지도요 ㅋㅋ",
          "original_form": "아녔으면 지금쯤 기어다니고 있을지도요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 12:18"
        },
        {
          "id": "MDRW2100023595.1.4",
          "form": "와 진짜요?\n그래도 태권도 꽤 오래하셨나봐요?",
          "original_form": "와 진짜요?\n그래도 태권도 꽤 오래하셨나봐요?",
          "speaker_id": "1",
          "time": "20210830 12:18"
        },
        {
          "id": "MDRW2100023595.1.5",
          "form": "저는 하얀띠까지 밖에 안해봐서..ㅋㅋ",
          "original_form": "저는 하얀띠까지 밖에 안해봐서..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 12:19"
        },
        {
          "id": "MDRW2100023595.1.6",
          "form": "걍 초등학교 5학년까지 했어요",
          "original_form": "걍 초등학교 5학년까지 했어요",
          "speaker_id": "2",
          "time": "20210830 12:19"
        },
        {
          "id": "MDRW2100023595.1.7",
          "form": "와~ 오래하셨네요",
          "original_form": "와~ 오래하셨네요",
          "speaker_id": "1",
          "time": "20210830 12:19"
        },
        {
          "id": "MDRW2100023595.1.8",
          "form": "검은띠 따자마자 엄마가 보습학원보내심 ㅎ",
          "original_form": "검은띠 따자마자 엄마가 보습학원보내심 ㅎ",
          "speaker_id": "2",
          "time": "20210830 12:19"
        },
        {
          "id": "MDRW2100023595.1.9",
          "form": "와 검은띠까지 따셨구나",
          "original_form": "와 검은띠까지 따셨구나",
          "speaker_id": "1",
          "time": "20210830 12:20"
        },
        {
          "id": "MDRW2100023595.1.10",
          "form": "겨루기도 많이 하셨나요?",
          "original_form": "겨루기도 많이 하셨나요?",
          "speaker_id": "1",
          "time": "20210830 12:20"
        },
        {
          "id": "MDRW2100023595.1.11",
          "form": "그쵸, 기왕하는거 검은띠는 따야죠",
          "original_form": "그쵸, 기왕하는거 검은띠는 따야죠",
          "speaker_id": "2",
          "time": "20210830 12:20"
        },
        {
          "id": "MDRW2100023595.1.12",
          "form": "조금요.",
          "original_form": "조금요.",
          "speaker_id": "2",
          "time": "20210830 12:20"
        },
        {
          "id": "MDRW2100023595.1.13",
          "form": "사실 학교다닐때 남자애들이랑 맞짱...을 더 많이 한듯요ㅋㅋ",
          "original_form": "사실 학교다닐때 남자애들이랑 맞짱...을 더 많이 한듯요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 12:21"
        },
        {
          "id": "MDRW2100023595.1.14",
          "form": "마자요 저는 흰띠 위에는 다 멋있어 보이더라구요",
          "original_form": "마자요 저는 흰띠 위에는 다 멋있어 보이더라구요",
          "speaker_id": "1",
          "time": "20210830 12:21"
        },
        {
          "id": "MDRW2100023595.1.15",
          "form": "아ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 12:21"
        },
        {
          "id": "MDRW2100023595.1.16",
          "form": "많이 싸우셨군요",
          "original_form": "많이 싸우셨군요",
          "speaker_id": "1",
          "time": "20210830 12:21"
        },
        {
          "id": "MDRW2100023595.1.17",
          "form": "오해하실까봐 말씀드리는데 저 굉장히 얌전한 성격이었습니다",
          "original_form": "오해하실까봐 말씀드리는데 저 굉장히 얌전한 성격이었습니다",
          "speaker_id": "2",
          "time": "20210830 12:22"
        },
        {
          "id": "MDRW2100023595.1.18",
          "form": "건드리지만 않으면 되는데",
          "original_form": "건드리지만 않으면 되는데",
          "speaker_id": "2",
          "time": "20210830 12:22"
        },
        {
          "id": "MDRW2100023595.1.19",
          "form": "꼭 개구쟁이들이 얌전한 여자애들을 건드리잖아요",
          "original_form": "꼭 개구쟁이들이 얌전한 여자애들을 건드리잖아요",
          "speaker_id": "2",
          "time": "20210830 12:22"
        },
        {
          "id": "MDRW2100023595.1.20",
          "form": "전 단지 참지않았을 뿐입니다 ㅎㅎ",
          "original_form": "전 단지 참지않았을 뿐입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 12:23"
        },
        {
          "id": "MDRW2100023595.1.21",
          "form": "아ㅋㅋㅋㄱ마자요\n가만히 있는데 건드리 니까요ㅋㅋㄱ",
          "original_form": "아ㅋㅋㅋㄱ마자요\n가만히 있는데 건드리 니까요ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210830 12:23"
        },
        {
          "id": "MDRW2100023595.1.22",
          "form": "참을순없죠!",
          "original_form": "참을순없죠!",
          "speaker_id": "1",
          "time": "20210830 12:23"
        },
        {
          "id": "MDRW2100023595.1.23",
          "form": "자꾸 머리카락 잡아당기고 의자 발로차고 뭐 던직",
          "original_form": "자꾸 머리카락 잡아당기고 의자 발로차고 뭐 던직{emoji:‥}",
          "speaker_id": "2",
          "time": "20210830 12:23"
        },
        {
          "id": "MDRW2100023595.1.24",
          "form": "어릴때부터 멋있으셨네요!",
          "original_form": "어릴때부터 멋있으셨네요!",
          "speaker_id": "1",
          "time": "20210830 12:23"
        },
        {
          "id": "MDRW2100023595.1.25",
          "form": "던지고",
          "original_form": "던지고",
          "speaker_id": "2",
          "time": "20210830 12:23"
        },
        {
          "id": "MDRW2100023595.1.26",
          "form": "마자요 왜그러는지 모르겠어요",
          "original_form": "마자요 왜그러는지 모르겠어요",
          "speaker_id": "1",
          "time": "20210830 12:24"
        },
        {
          "id": "MDRW2100023595.1.27",
          "form": "두번정도 경고하지만 안듣죠 걔들은 ㅋㅋ",
          "original_form": "두번정도 경고하지만 안듣죠 걔들은 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 12:24"
        },
        {
          "id": "MDRW2100023595.1.28",
          "form": "마자요ㅋㅋㅋ\n꼭 맞아야 정신차리죠ㅋㅋ",
          "original_form": "마자요ㅋㅋㅋ\n꼭 맞아야 정신차리죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 12:24"
        },
        {
          "id": "MDRW2100023595.1.29",
          "form": "그러니까 어쩌겠어요, 싸워야지",
          "original_form": "그러니까 어쩌겠어요, 싸워야지",
          "speaker_id": "2",
          "time": "20210830 12:24"
        },
        {
          "id": "MDRW2100023595.1.30",
          "form": "마자요!",
          "original_form": "마자요!",
          "speaker_id": "1",
          "time": "20210830 12:25"
        },
        {
          "id": "MDRW2100023595.1.31",
          "form": "그래도 한 중학교까지는 피지컬이 크게 안밀렸는데",
          "original_form": "그래도 한 중학교까지는 피지컬이 크게 안밀렸는데",
          "speaker_id": "2",
          "time": "20210830 12:25"
        },
        {
          "id": "MDRW2100023595.1.32",
          "form": "저두요ㅋㅋㅋ\n진짜 남자애들은 확 크더라구요",
          "original_form": "저두요ㅋㅋㅋ\n진짜 남자애들은 확 크더라구요",
          "speaker_id": "1",
          "time": "20210830 12:25"
        },
        {
          "id": "MDRW2100023595.1.33",
          "form": "저는 중딩때 키가 지금 키일뿐이고ㅠㅠ",
          "original_form": "저는 중딩때 키가 지금 키일뿐이고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 12:26"
        },
        {
          "id": "MDRW2100023595.1.34",
          "form": "에구ㅠㅠ",
          "original_form": "에구ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 12:26"
        },
        {
          "id": "MDRW2100023595.1.35",
          "form": "이제 그나마 있던 근육도 사라지기전에 암벽을 시작한거죠",
          "original_form": "이제 그나마 있던 근육도 사라지기전에 암벽을 시작한거죠",
          "speaker_id": "2",
          "time": "20210830 12:27"
        },
        {
          "id": "MDRW2100023595.1.36",
          "form": "암벽등반 선수들 보니까 딱 제가 이상적으로 보는 몸이저라구요",
          "original_form": "암벽등반 선수들 보니까 딱 제가 이상적으로 보는 몸이저라구요",
          "speaker_id": "2",
          "time": "20210830 12:27"
        },
        {
          "id": "MDRW2100023595.1.37",
          "form": "마자요 지금이라도 시작해야죠ㅠ",
          "original_form": "마자요 지금이라도 시작해야죠ㅠ",
          "speaker_id": "1",
          "time": "20210830 12:27"
        },
        {
          "id": "MDRW2100023595.1.38",
          "form": "마자요 선수들 몸이 딱 균형잡힌 느낌이죠",
          "original_form": "마자요 선수들 몸이 딱 균형잡힌 느낌이죠",
          "speaker_id": "1",
          "time": "20210830 12:28"
        }
      ]
    }
