      "utterance": [
        {
          "id": "MDRW2100005526.1.1",
          "form": "ㅇㅎ...ㅠㅠ 가족들 다 매운 거 못 드셔?",
          "original_form": "ㅇㅎ...ㅠㅠ 가족들 다 매운 거 못 드셔?",
          "speaker_id": "2",
          "time": "20210821 22:52"
        },
        {
          "id": "MDRW2100005526.1.2",
          "form": "그것도 있고",
          "original_form": "그것도 있고",
          "speaker_id": "1",
          "time": "20210821 22:52"
        },
        {
          "id": "MDRW2100005526.1.3",
          "form": "몸에 안좋대서",
          "original_form": "몸에 안좋대서",
          "speaker_id": "1",
          "time": "20210821 22:52"
        },
        {
          "id": "MDRW2100005526.1.4",
          "form": "너넨 뭐 배달해먹어",
          "original_form": "너넨 뭐 배달해먹어",
          "speaker_id": "1",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005526.1.5",
          "form": "아 하긴",
          "original_form": "아 하긴",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005526.1.6",
          "form": "우리는 보통 치킨이나 회 시켜 먹는데",
          "original_form": "우리는 보통 치킨이나 회 시켜 먹는데",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005526.1.7",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005526.1.8",
          "form": "동생이 마라탕 진짜 좋아하거든",
          "original_form": "동생이 마라탕 진짜 좋아하거든",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005526.1.9",
          "form": "맛있더라 마라탕",
          "original_form": "맛있더라 마라탕",
          "speaker_id": "1",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005526.1.10",
          "form": "그래서 아빠 오시면 아빠가 동생 먹이자고 마라탕 시켜",
          "original_form": "그래서 아빠 오시면 아빠가 동생 먹이자고 마라탕 시켜",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005526.1.11",
          "form": "ㅇㅈㅇㅈ",
          "original_form": "ㅇㅈㅇㅈ",
          "speaker_id": "2",
          "time": "20210821 22:53"
        },
        {
          "id": "MDRW2100005526.1.12",
          "form": "우린 감자탕 자주먹어",
          "original_form": "우린 감자탕 자주먹어",
          "speaker_id": "1",
          "time": "20210821 22:54"
        },
        {
          "id": "MDRW2100005526.1.13",
          "form": "요리를 잘 안함",
          "original_form": "요리를 잘 안함",
          "speaker_id": "1",
          "time": "20210821 22:54"
        },
        {
          "id": "MDRW2100005526.1.14",
          "form": "잌ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "잌ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:54"
        },
        {
          "id": "MDRW2100005526.1.15",
          "form": "요즘 밥 챙겨먹는게 귀찮아서 좀 걸렀더니",
          "original_form": "요즘 밥 챙겨먹는게 귀찮아서 좀 걸렀더니",
          "speaker_id": "2",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005526.1.16",
          "form": "웅웅ㅇ",
          "original_form": "웅웅ㅇ",
          "speaker_id": "1",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005526.1.17",
          "form": "엊그제 엄마한테 혼남 ㅜㅠ",
          "original_form": "엊그제 엄마한테 혼남 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005526.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005526.1.19",
          "form": "아예 안먹었어?",
          "original_form": "아예 안먹었어?",
          "speaker_id": "1",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005526.1.20",
          "form": "어어....ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 하루에 한 끼 먹고 그랬긴 함",
          "original_form": "어어....ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 하루에 한 끼 먹고 그랬긴 함",
          "speaker_id": "2",
          "time": "20210821 22:56"
        },
        {
          "id": "MDRW2100005526.1.21",
          "form": "위 상해..",
          "original_form": "위 상해..",
          "speaker_id": "1",
          "time": "20210821 22:56"
        },
        {
          "id": "MDRW2100005526.1.22",
          "form": "잉... 요즘 다시 챙겨먹는 중...ㅋㅋㅋㅋㅋㅋ",
          "original_form": "잉... 요즘 다시 챙겨먹는 중...ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:56"
        },
        {
          "id": "MDRW2100005526.1.23",
          "form": "나 영어 과외 해주는 선생님은",
          "original_form": "나 영어 과외 해주는 선생님은",
          "speaker_id": "2",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005526.1.24",
          "form": "자기가 요리해서 먹고 살려고 돈 번대",
          "original_form": "자기가 요리해서 먹고 살려고 돈 번대",
          "speaker_id": "2",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005526.1.25",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005526.1.26",
          "form": "혼자 사는데 요리하는거 엄청 좋아한다대",
          "original_form": "혼자 사는데 요리하는거 엄청 좋아한다대",
          "speaker_id": "2",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005526.1.27",
          "form": "나도 그렇게 살고 싶다니까 나보고 돈 많이 벌래",
          "original_form": "나도 그렇게 살고 싶다니까 나보고 돈 많이 벌래",
          "speaker_id": "2",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005526.1.28",
          "form": "요리하는거 좋아하시나봐",
          "original_form": "요리하는거 좋아하시나봐",
          "speaker_id": "1",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005526.1.29",
          "form": "...",
          "original_form": "...",
          "speaker_id": "2",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005526.1.30",
          "form": "잉잉",
          "original_form": "잉잉",
          "speaker_id": "2",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005526.1.31",
          "form": "막",
          "original_form": "막",
          "speaker_id": "2",
          "time": "20210821 22:58"
        },
        {
          "id": "MDRW2100005526.1.32",
          "form": "샌드위치 빵에 대해서만 쓴 책이 있는데 그걸 샀대",
          "original_form": "샌드위치 빵에 대해서만 쓴 책이 있는데 그걸 샀대",
