      "utterance": [
        {
          "id": "MDRW2100032534.1.1",
          "form": "이 세계로 가서",
          "original_form": "이 세계로 가서",
          "speaker_id": "2",
          "time": "20210905 20:22"
        },
        {
          "id": "MDRW2100032534.1.2",
          "form": "여러 여자가 좋아하는 그런것들이",
          "original_form": "여러 여자가 좋아하는 그런것들이",
          "speaker_id": "2",
          "time": "20210905 20:22"
        },
        {
          "id": "MDRW2100032534.1.3",
          "form": "사람 다 망쳐둠",
          "original_form": "사람 다 망쳐둠",
          "speaker_id": "2",
          "time": "20210905 20:22"
        },
        {
          "id": "MDRW2100032534.1.4",
          "form": "ㄹㅇㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㄹㅇㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:22"
        },
        {
          "id": "MDRW2100032534.1.5",
          "form": "하렘물..",
          "original_form": "하렘물..",
          "speaker_id": "1",
          "time": "20210905 20:22"
        },
        {
          "id": "MDRW2100032534.1.6",
          "form": "으",
          "original_form": "으",
          "speaker_id": "2",
          "time": "20210905 20:22"
        },
        {
          "id": "MDRW2100032534.1.7",
          "form": "위에 무슨 썰??",
          "original_form": "위에 무슨 썰??",
          "speaker_id": "2",
          "time": "20210905 20:22"
        },
        {
          "id": "MDRW2100032534.1.8",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210905 20:22"
        },
        {
          "id": "MDRW2100032534.1.9",
          "form": "그 조인성 데뷔초반에",
          "original_form": "그 조인성 데뷔초반에",
          "speaker_id": "1",
          "time": "20210905 20:22"
        },
        {
          "id": "MDRW2100032534.1.10",
          "form": "여자배우들이랑 자주 어울려다녔는데",
          "original_form": "여자배우들이랑 자주 어울려다녔는데",
          "speaker_id": "1",
          "time": "20210905 20:23"
        },
        {
          "id": "MDRW2100032534.1.11",
          "form": "그러니까 막 조인성한테",
          "original_form": "그러니까 막 조인성한테",
          "speaker_id": "1",
          "time": "20210905 20:23"
        },
        {
          "id": "MDRW2100032534.1.12",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210905 20:23"
        },
        {
          "id": "MDRW2100032534.1.13",
          "form": "카사노바다 바람둥이다 이런소문들이 진짜많이난거야",
          "original_form": "카사노바다 바람둥이다 이런소문들이 진짜많이난거야",
          "speaker_id": "1",
          "time": "20210905 20:23"
        },
        {
          "id": "MDRW2100032534.1.14",
          "form": "그래서 조인성이",
          "original_form": "그래서 조인성이",
          "speaker_id": "1",
          "time": "20210905 20:23"
        },
        {
          "id": "MDRW2100032534.1.15",
          "form": "그거 신경썼는지 일부러 남자배우들이랑만 다녔는데",
          "original_form": "그거 신경썼는지 일부러 남자배우들이랑만 다녔는데",
          "speaker_id": "1",
          "time": "20210905 20:23"
        },
        {
          "id": "MDRW2100032534.1.16",
          "form": "그러니까 또 ㄱ ㅔ이라는 소문나고 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그러니까 또 ㄱ ㅔ이라는 소문나고 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:23"
        },
        {
          "id": "MDRW2100032534.1.17",
          "form": "아 들어본적 있는것 같음ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 들어본적 있는것 같음ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:23"
        },
        {
          "id": "MDRW2100032534.1.18",
          "form": "인터뷰에서도 대놓고",
          "original_form": "인터뷰에서도 대놓고",
          "speaker_id": "1",
          "time": "20210905 20:23"
        },
        {
          "id": "MDRW2100032534.1.19",
          "form": "진짜 게.이세요?이러고",
          "original_form": "진짜 게.이세요?이러고",
          "speaker_id": "1",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100032534.1.20",
          "form": "조인성은 아니라고 제가대체 어떡해야하냐고 허허웃고",
          "original_form": "조인성은 아니라고 제가대체 어떡해야하냐고 허허웃고",
          "speaker_id": "1",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100032534.1.21",
          "form": "엥 누가?",
          "original_form": "엥 누가?",
          "speaker_id": "2",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100032534.1.22",
          "form": "인터뷰하는 사람이????",
          "original_form": "인터뷰하는 사람이????",
          "speaker_id": "2",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100032534.1.23",
          "form": "몰라 인터뷰 짤로만 봄",
          "original_form": "몰라 인터뷰 짤로만 봄",
          "speaker_id": "1",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100032534.1.24",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100032534.1.25",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100032534.1.26",
          "form": "진짜 너무 무례하다",
          "original_form": "진짜 너무 무례하다",
          "speaker_id": "2",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100032534.1.27",
          "form": "쨋든 그뒤로는",
          "original_form": "쨋든 그뒤로는",
          "speaker_id": "1",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100032534.1.28",
          "form": "다른 배우들이랑 잘 안다니고 집에서만 있었는데",
          "original_form": "다른 배우들이랑 잘 안다니고 집에서만 있었는데",
          "speaker_id": "1",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100032534.1.29",
          "form": "그러니까 또",
          "original_form": "그러니까 또",
          "speaker_id": "1",
          "time": "20210905 20:24"
        },
        {
          "id": "MDRW2100032534.1.30",
          "form": "조인성이 암내가 심하다",
          "original_form": "조인성이 암내가 심하다",
          "speaker_id": "1",
          "time": "20210905 20:25"
        },
        {
          "id": "MDRW2100032534.1.31",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "2",
          "time": "20210905 20:25"
        },
        {
          "id": "MDRW2100032534.1.32",
          "form": "같이 연기한 송혜교랑 전도연이 암내로 쓰러졌댄다",
          "original_form": "같이 연기한 송혜교랑 전도연이 암내로 쓰러졌댄다",
          "speaker_id": "1",
          "time": "20210905 20:25"
        },
        {
          "id": "MDRW2100032534.1.33",
          "form": "와 진짜 연예인등 정신병 안걸리는게 진짜",
          "original_form": "와 진짜 연예인등 정신병 안걸리는게 진짜",
          "speaker_id": "2",
          "time": "20210905 20:25"
        },
        {
          "id": "MDRW2100032534.1.34",
          "form": "이.런.소.뮨.나.고 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "이.런.소.뮨.나.고 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:25"
        },
        {
          "id": "MDRW2100032534.1.35",
          "form": "할말이 없다...",
          "original_form": "할말이 없다...",
          "speaker_id": "2",
          "time": "20210905 20:25"
        },
        {
          "id": "MDRW2100032534.1.36",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210905 20:25"
        }
      ]
    }
