      "utterance": [
        {
          "id": "MDRW2100011253.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 10:08"
        },
        {
          "id": "MDRW2100011253.1.2",
          "form": "이상형이 어떻게 되시나요",
          "original_form": "이상형이 어떻게 되시나요",
          "speaker_id": "1",
          "time": "20210824 10:08"
        },
        {
          "id": "MDRW2100011253.1.3",
          "form": "대화가 잘 되는 사람이요. 재밌으면 더 좋구요",
          "original_form": "대화가 잘 되는 사람이요. 재밌으면 더 좋구요",
          "speaker_id": "2",
          "time": "20210824 10:10"
        },
        {
          "id": "MDRW2100011253.1.4",
          "form": "저도 그래요",
          "original_form": "저도 그래요",
          "speaker_id": "1",
          "time": "20210824 10:10"
        },
        {
          "id": "MDRW2100011253.1.5",
          "form": "혹시 외적인거는요?",
          "original_form": "혹시 외적인거는요?",
          "speaker_id": "1",
          "time": "20210824 10:10"
        },
        {
          "id": "MDRW2100011253.1.6",
          "form": "특별하게 생각해보진 않았어요. 잘생기면 좋긴 하겠지만요ㅋㅋ",
          "original_form": "특별하게 생각해보진 않았어요. 잘생기면 좋긴 하겠지만요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:12"
        },
        {
          "id": "MDRW2100011253.1.7",
          "form": "ㅋㅋ ㅋㅋㅋ 그렇죠",
          "original_form": "ㅋㅋ ㅋㅋㅋ 그렇죠",
          "speaker_id": "1",
          "time": "20210824 10:13"
        },
        {
          "id": "MDRW2100011253.1.8",
          "form": "연예인 중 이상형이 있나요?",
          "original_form": "연예인 중 이상형이 있나요?",
          "speaker_id": "1",
          "time": "20210824 10:13"
        },
        {
          "id": "MDRW2100011253.1.9",
          "form": "글쎄요. 워낙 다 잘 생겼으니깐요.",
          "original_form": "글쎄요. 워낙 다 잘 생겼으니깐요.",
          "speaker_id": "2",
          "time": "20210824 10:15"
        },
        {
          "id": "MDRW2100011253.1.10",
          "form": "팬심이긴 하지만 요샌 김선호님이 멋있더라고요",
          "original_form": "팬심이긴 하지만 요샌 김선호님이 멋있더라고요",
          "speaker_id": "2",
          "time": "20210824 10:15"
        },
        {
          "id": "MDRW2100011253.1.11",
          "form": "생각하시는 스타일이 있으신가요?",
          "original_form": "생각하시는 스타일이 있으신가요?",
          "speaker_id": "2",
          "time": "20210824 10:16"
        },
        {
          "id": "MDRW2100011253.1.12",
          "form": "오 멋있죠ㅋㅋ ㅋㅋㅋ",
          "original_form": "오 멋있죠ㅋㅋ ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:17"
        },
        {
          "id": "MDRW2100011253.1.13",
          "form": "전 갑자기 펜싱선수 오상욱 이분이",
          "original_form": "전 갑자기 펜싱선수 오상욱 이분이",
          "speaker_id": "1",
          "time": "20210824 11:17"
        },
        {
          "id": "MDRW2100011253.1.14",
          "form": "이상형으로 바꼈어요",
          "original_form": "이상형으로 바꼈어요",
          "speaker_id": "1",
          "time": "20210824 11:17"
        },
        {
          "id": "MDRW2100011253.1.15",
          "form": "멋있죠ㅋㅋ 피지컬도 엄청 좋으시잖아요",
          "original_form": "멋있죠ㅋㅋ 피지컬도 엄청 좋으시잖아요",
          "speaker_id": "2",
          "time": "20210824 12:08"
        },
        {
          "id": "MDRW2100011253.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ 네 어제 알아버렸어요,,,",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 네 어제 알아버렸어요,,,",
          "speaker_id": "1",
          "time": "20210824 12:41"
        },
        {
          "id": "MDRW2100011253.1.17",
          "form": "그 피지컬에 외모도 좋으니까요ㅋㅋ",
          "original_form": "그 피지컬에 외모도 좋으니까요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 12:47"
        },
        {
          "id": "MDRW2100011253.1.18",
          "form": "ㅎ ㅎㅎㅋ 정말요",
          "original_form": "ㅎ ㅎㅎㅋ 정말요",
          "speaker_id": "1",
          "time": "20210824 13:10"
        },
        {
          "id": "MDRW2100011253.1.19",
          "form": "아주 아주 좋아요",
          "original_form": "아주 아주 좋아요",
          "speaker_id": "1",
          "time": "20210824 13:10"
        },
        {
          "id": "MDRW2100011253.1.20",
          "form": "그래서 그런가 다들 결혼도 잘 하잖아요ㅋㅋ",
          "original_form": "그래서 그런가 다들 결혼도 잘 하잖아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 13:16"
        },
        {
          "id": "MDRW2100011253.1.21",
          "form": "ㅋㅋㅋㅋㅋ운동선수라 그런가봐요",
          "original_form": "ㅋㅋㅋㅋㅋ운동선수라 그런가봐요",
