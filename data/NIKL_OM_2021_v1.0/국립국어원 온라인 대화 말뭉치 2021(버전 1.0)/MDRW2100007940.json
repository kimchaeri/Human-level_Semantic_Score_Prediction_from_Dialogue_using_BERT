      "utterance": [
        {
          "id": "MDRW2100007940.1.1",
          "form": "백신 신청하셨어요?",
          "original_form": "백신 신청하셨어요?",
          "speaker_id": "1",
          "time": "20210822 12:05"
        },
        {
          "id": "MDRW2100007940.1.2",
          "form": "백신 아직 안맞으셨죠?",
          "original_form": "백신 아직 안맞으셨죠?",
          "speaker_id": "1",
          "time": "20210822 12:06"
        },
        {
          "id": "MDRW2100007940.1.3",
          "form": "주사도 무서워하고 백신의 부작용이 걱정되서 못맞을거같아요",
          "original_form": "주사도 무서워하고 백신의 부작용이 걱정되서 못맞을거같아요",
          "speaker_id": "2",
          "time": "20210822 12:06"
        },
        {
          "id": "MDRW2100007940.1.4",
          "form": "저도 무서워서 고민중",
          "original_form": "저도 무서워서 고민중",
          "speaker_id": "1",
          "time": "20210822 12:07"
        },
        {
          "id": "MDRW2100007940.1.5",
          "form": "혹시 몰라 예약은 했는데",
          "original_form": "혹시 몰라 예약은 했는데",
          "speaker_id": "1",
          "time": "20210822 12:07"
        },
        {
          "id": "MDRW2100007940.1.6",
          "form": "백신 맞고 걱정되는거 없어요?",
          "original_form": "백신 맞고 걱정되는거 없어요?",
          "speaker_id": "2",
          "time": "20210822 12:08"
        },
        {
          "id": "MDRW2100007940.1.7",
          "form": "뉴스에도 안좋은 소식들이 많잖아요",
          "original_form": "뉴스에도 안좋은 소식들이 많잖아요",
          "speaker_id": "2",
          "time": "20210822 12:08"
        },
        {
          "id": "MDRW2100007940.1.8",
          "form": "죽을까봐 무섭더라구요",
          "original_form": "죽을까봐 무섭더라구요",
          "speaker_id": "1",
          "time": "20210822 12:09"
        },
        {
          "id": "MDRW2100007940.1.9",
          "form": "저도 그때가서 맞을지 결정할생각이예요",
          "original_form": "저도 그때가서 맞을지 결정할생각이예요",
          "speaker_id": "1",
          "time": "20210822 12:10"
        },
        {
          "id": "MDRW2100007940.1.10",
          "form": "몸이 건강하면 작은 문제도 안생기겠죠!",
          "original_form": "몸이 건강하면 작은 문제도 안생기겠죠!",
          "speaker_id": "2",
          "time": "20210822 12:13"
        },
        {
          "id": "MDRW2100007940.1.11",
          "form": "주변에 지인들 백신 맞은 사람없나요?",
          "original_form": "주변에 지인들 백신 맞은 사람없나요?",
          "speaker_id": "2",
          "time": "20210822 12:13"
        },
        {
          "id": "MDRW2100007940.1.12",
          "form": "최측근들은 그래도 맞고 큰이상은 없더라구요",
          "original_form": "최측근들은 그래도 맞고 큰이상은 없더라구요",
          "speaker_id": "1",
          "time": "20210822 12:13"
        },
        {
          "id": "MDRW2100007940.1.13",
          "form": "친구는 이미 2차맞았는데 아무렇지 않다네요",
          "original_form": "친구는 이미 2차맞았는데 아무렇지 않다네요",
          "speaker_id": "1",
          "time": "20210822 12:14"
        },
        {
          "id": "MDRW2100007940.1.14",
          "form": "1차아제 2화이자 맞고",
          "original_form": "1차아제 2화이자 맞고",
          "speaker_id": "1",
          "time": "20210822 12:14"
        },
        {
          "id": "MDRW2100007940.1.15",
          "form": "백신 맞고 몇일 쉬다보면 정상생활로 돌아갈거같아요",
          "original_form": "백신 맞고 몇일 쉬다보면 정상생활로 돌아갈거같아요",
          "speaker_id": "2",
          "time": "20210822 12:14"
        },
        {
          "id": "MDRW2100007940.1.16",
          "form": "근데 저는 지금도 맞을 생각이 없네요 ㅜ",
          "original_form": "근데 저는 지금도 맞을 생각이 없네요 ㅜ",
          "speaker_id": "2",
          "time": "20210822 12:15"
        },
        {
          "id": "MDRW2100007940.1.17",
          "form": "아예신청도 안하신거죠?",
          "original_form": "아예신청도 안하신거죠?",
          "speaker_id": "1",
          "time": "20210822 12:15"
        },
        {
          "id": "MDRW2100007940.1.18",
          "form": "네 맞아요 신청부터 두렵죠 ㅎ..",
          "original_form": "네 맞아요 신청부터 두렵죠 ㅎ..",
          "speaker_id": "2",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007940.1.19",
          "form": "백신도 중요하지만 평소에 건강관리는 하시나요?",
          "original_form": "백신도 중요하지만 평소에 건강관리는 하시나요?",
          "speaker_id": "2",
          "time": "20210822 12:18"
        },
        {
          "id": "MDRW2100007940.1.20",
          "form": "예전엔 운동도했는데 요즘은 못하고있어요",
          "original_form": "예전엔 운동도했는데 요즘은 못하고있어요",
          "speaker_id": "1",
          "time": "20210822 12:19"
        },
        {
          "id": "MDRW2100007940.1.21",
          "form": "운동 하시는거 있나요?",
          "original_form": "운동 하시는거 있나요?",
          "speaker_id": "1",
          "time": "20210822 12:21"
        },
        {
          "id": "MDRW2100007940.1.22",
          "form": "어떤 이유가 있는지 모르겠지만 운동을 안하니까 몸이 피곤한 날이 많아요",
          "original_form": "어떤 이유가 있는지 모르겠지만 운동을 안하니까 몸이 피곤한 날이 많아요",
          "speaker_id": "2",
          "time": "20210822 12:21"
        },
        {
          "id": "MDRW2100007940.1.23",
          "form": "저는 집에서 하는 운동을 좋아해서 푸쉬업이나 풀업만 하는거같아요",
          "original_form": "저는 집에서 하는 운동을 좋아해서 푸쉬업이나 풀업만 하는거같아요",
          "speaker_id": "2",
          "time": "20210822 12:22"
        },
        {
          "id": "MDRW2100007940.1.24",
          "form": "저도 만성피로예요",
          "original_form": "저도 만성피로예요",
          "speaker_id": "1",
          "time": "20210822 12:22"
        },
        {
          "id": "MDRW2100007940.1.25",
          "form": "풀업은 어케하는거예요?",
          "original_form": "풀업은 어케하는거예요?",
          "speaker_id": "1",
          "time": "20210822 12:22"
        },
        {
          "id": "MDRW2100007940.1.26",
          "form": "기구들고 할정도는 채력이 안되고 헬스장을 가야하니까..",
          "original_form": "기구들고 할정도는 채력이 안되고 헬스장을 가야하니까..",
          "speaker_id": "2",
          "time": "20210822 12:22"
        },
        {
          "id": "MDRW2100007940.1.27",
          "form": "풀업은 따로 설치하면되요",
          "original_form": "풀업은 따로 설치하면되요",
          "speaker_id": "2",
          "time": "20210822 12:23"
        }
      ]
    }
