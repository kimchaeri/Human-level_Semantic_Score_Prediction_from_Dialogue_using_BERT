      "utterance": [
        {
          "id": "MDRW2100034061.1.1",
          "form": "기념품 모으시는거 있아요?",
          "original_form": "기념품 모으시는거 있아요?",
          "speaker_id": "2",
          "time": "20210827 16:58"
        },
        {
          "id": "MDRW2100034061.1.2",
          "form": "저는 엽서요 가지고다니기도 좋고 보면 기억도 새록새록나요",
          "original_form": "저는 엽서요 가지고다니기도 좋고 보면 기억도 새록새록나요",
          "speaker_id": "1",
          "time": "20210827 16:59"
        },
        {
          "id": "MDRW2100034061.1.3",
          "form": "모으시는거 있나요?",
          "original_form": "모으시는거 있나요?",
          "speaker_id": "1",
          "time": "20210827 16:59"
        },
        {
          "id": "MDRW2100034061.1.4",
          "form": "저는 여행을 많이 안가봐서요 ㅎㅎ 한수 배우려고햇슴다",
          "original_form": "저는 여행을 많이 안가봐서요 ㅎㅎ 한수 배우려고햇슴다",
          "speaker_id": "2",
          "time": "20210827 17:09"
        },
        {
          "id": "MDRW2100034061.1.5",
          "form": "엽서 너무 좋은 것같네요 비슷하게 종이 책갈피도 예쁠것같고",
          "original_form": "엽서 너무 좋은 것같네요 비슷하게 종이 책갈피도 예쁠것같고",
          "speaker_id": "2",
          "time": "20210827 17:10"
        },
        {
          "id": "MDRW2100034061.1.6",
          "form": "제 친구는 마그넷을 모아서 집에 자석판에 붙여놓는데",
          "original_form": "제 친구는 마그넷을 모아서 집에 자석판에 붙여놓는데",
          "speaker_id": "1",
          "time": "20210827 17:11"
        },
        {
          "id": "MDRW2100034061.1.7",
          "form": "그것도 좋아보여요",
          "original_form": "그것도 좋아보여요",
          "speaker_id": "1",
          "time": "20210827 17:11"
        },
        {
          "id": "MDRW2100034061.1.8",
          "form": "그것도 너무 예쁘갰내요 ㅠㅠㅠ",
          "original_form": "그것도 너무 예쁘갰내요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 19:05"
        },
        {
          "id": "MDRW2100034061.1.9",
          "form": "생각하는 것만으로 신나는 기분..",
          "original_form": "생각하는 것만으로 신나는 기분..",
          "speaker_id": "2",
          "time": "20210827 19:06"
        },
        {
          "id": "MDRW2100034061.1.10",
          "form": "사진은 많이 찍고 다니시나요?",
          "original_form": "사진은 많이 찍고 다니시나요?",
          "speaker_id": "2",
          "time": "20210827 19:06"
        },
        {
          "id": "MDRW2100034061.1.11",
          "form": "풍겨사진 음식사진 많이 찍어요!",
          "original_form": "풍겨사진 음식사진 많이 찍어요!",
          "speaker_id": "1",
          "time": "20210827 19:09"
        },
        {
          "id": "MDRW2100034061.1.12",
          "form": "사진이 남는거라는게 이제 좀 와닿아서요",
          "original_form": "사진이 남는거라는게 이제 좀 와닿아서요",
          "speaker_id": "1",
          "time": "20210827 19:09"
        },
        {
          "id": "MDRW2100034061.1.13",
          "form": "그쵸~ 나중에 사진 보면 기분이 너무좋아요",
          "original_form": "그쵸~ 나중에 사진 보면 기분이 너무좋아요",
          "speaker_id": "2",
          "time": "20210827 19:13"
        },
        {
          "id": "MDRW2100034061.1.14",
          "form": "카메라 하나 장만하고싶은데 너무 비싸고 ㅠ",
          "original_form": "카메라 하나 장만하고싶은데 너무 비싸고 ㅠ",
          "speaker_id": "2",
          "time": "20210827 19:13"
        },
        {
          "id": "MDRW2100034061.1.15",
          "form": "즉석카메라는 어떠신가요? 추억 느낌도 살고 다른카메라보다는 좀 저렴한거 같던데...",
          "original_form": "즉석카메라는 어떠신가요? 추억 느낌도 살고 다른카메라보다는 좀 저렴한거 같던데...",
          "speaker_id": "1",
          "time": "20210827 19:34"
        },
        {
          "id": "MDRW2100034061.1.16",
          "form": "그렇더라고요 안그래도 고민중이애요 ㅎㅎ",
          "original_form": "그렇더라고요 안그래도 고민중이애요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:46"
        },
        {
          "id": "MDRW2100034061.1.17",
          "form": "근데 요즘 좀 여유가 없어서 ㅜㅜ",
          "original_form": "근데 요즘 좀 여유가 없어서 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 20:47"
        },
        {
          "id": "MDRW2100034061.1.18",
          "form": "아앗.....핸드폰카메라도 너무 잘나오니까요!",
          "original_form": "아앗.....핸드폰카메라도 너무 잘나오니까요!",
          "speaker_id": "1",
          "time": "20210827 20:49"
        },
        {
          "id": "MDRW2100034061.1.19",
          "form": "당장 안 급하지 않을까요...",
          "original_form": "당장 안 급하지 않을까요...",
          "speaker_id": "1",
          "time": "20210827 20:49"
        },
        {
          "id": "MDRW2100034061.1.20",
          "form": "ㅠㅠ 열심히 모아서 사는 수밖에 없죠 뭐",
          "original_form": "ㅠㅠ 열심히 모아서 사는 수밖에 없죠 뭐",
          "speaker_id": "2",
          "time": "20210827 20:58"
        },
        {
          "id": "MDRW2100034061.1.21",
          "form": "여행자체로 의미가있으니까요",
          "original_form": "여행자체로 의미가있으니까요",
          "speaker_id": "2",
          "time": "20210827 20:59"
        },
        {
          "id": "MDRW2100034061.1.22",
          "form": "다행이도? 코로나라서 어차피 못가니까요...찬찬히 모아봅시다",
          "original_form": "다행이도? 코로나라서 어차피 못가니까요...찬찬히 모아봅시다",
          "speaker_id": "1",
          "time": "20210827 21:02"
        },
        {
          "id": "MDRW2100034061.1.23",
          "form": "그래야겠어요 ㅋㅋㅋㅋ사실 여행자금은 엄청 모여서 두번도 갈것같거든요",
          "original_form": "그래야겠어요 ㅋㅋㅋㅋ사실 여행자금은 엄청 모여서 두번도 갈것같거든요",
          "speaker_id": "2",
          "time": "20210827 21:17"
        },
        {
          "id": "MDRW2100034061.1.24",
          "form": "요트타보고싶었눈데 ..ㅜㅜ",
          "original_form": "요트타보고싶었눈데 ..ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 21:18"
        }
      ]
