      "utterance": [
        {
          "id": "MDRW2100015412.1.1",
          "form": "name2님 저녁 식사는 하셨어요?",
          "original_form": "&name2&님 저녁 식사는 하셨어요?",
          "speaker_id": "1",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015412.1.2",
          "form": "넵 전 아까 6시 다되서 먹었어요!",
          "original_form": "넵 전 아까 6시 다되서 먹었어요!",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015412.1.3",
          "form": "다돼서",
          "original_form": "다돼서",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015412.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015412.1.5",
          "form": "저도 식사했어요:)",
          "original_form": "저도 식사했어요:)",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015412.1.6",
          "form": "치킨 좋아하세요?",
          "original_form": "치킨 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015412.1.7",
          "form": "네 치킨 맛있죵ㅎㅎ",
          "original_form": "네 치킨 맛있죵ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015412.1.8",
          "form": "혹시 치킨 드셨나요!",
          "original_form": "혹시 치킨 드셨나요!",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015412.1.9",
          "form": "ㅋㅋ오늘 먹은건 아닌데\r\n이 시간되니 생각나서요ㅋ",
          "original_form": "ㅋㅋ오늘 먹은건 아닌데\r\n이 시간되니 생각나서요ㅋ",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015412.1.10",
          "form": "name2님의 최애치킨은 어딘가용",
          "original_form": "&name2&님의 최애치킨은 어딘가용",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015412.1.11",
          "form": "아ㅋㅋㅋ 이시간이면 생각날때죠ㅜㅋㅋ",
          "original_form": "아ㅋㅋㅋ 이시간이면 생각날때죠ㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015412.1.12",
          "form": "전 뿌링클 좋아하구 교촌 라이스치킨 좋아해요",
          "original_form": "전 뿌링클 좋아하구 교촌 라이스치킨 좋아해요",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015412.1.13",
          "form": "오 뿌링클과 라이스 치킨ㅋㅋ",
          "original_form": "오 뿌링클과 라이스 치킨ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015412.1.14",
          "form": "라이스치킨은 한번도 안먹어봤네용",
          "original_form": "라이스치킨은 한번도 안먹어봤네용",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015412.1.15",
          "form": "교촌은 허니콤보만ㅋ",
          "original_form": "교촌은 허니콤보만ㅋ",
          "speaker_id": "1",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015412.1.16",
          "form": "허니콤보가 유명한 대표메뉴맞죠??",
          "original_form": "허니콤보가 유명한 대표메뉴맞죠??",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015412.1.17",
          "form": "라이스 치킨은 소스 맛있는거 줘서 좋아용 같이오는 감자튀김도 맛있고",
          "original_form": "라이스 치킨은 소스 맛있는거 줘서 좋아용 같이오는 감자튀김도 맛있고",
          "speaker_id": "2",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015412.1.18",
          "form": "오 이번에 라이스 치킨도 먹어봐야겠어요ㅋ",
          "original_form": "오 이번에 라이스 치킨도 먹어봐야겠어요ㅋ",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015412.1.19",
          "form": "맛있는 소스 궁금하네요 ㅋㅋ",
          "original_form": "맛있는 소스 궁금하네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015412.1.20",
          "form": "ㅋㅋㅋㅋ 하얀 소스? 하나랑 달짝지근한 초록색 소스, 소금 이렇게주는데",
          "original_form": "ㅋㅋㅋㅋ 하얀 소스? 하나랑 달짝지근한 초록색 소스, 소금 이렇게주는데",
          "speaker_id": "2",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015412.1.21",
          "form": "다른메뉴에 주는지는 모르겠지만 진짜 맛있어요 ㅋㅋ",
          "original_form": "다른메뉴에 주는지는 모르겠지만 진짜 맛있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015412.1.22",
          "form": "오호 궁금증이 밀려오네요 ㅋㅋ",
          "original_form": "오호 궁금증이 밀려오네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015412.1.23",
          "form": "ㅋㅋㅋ name1님 최애치킨 교촌 허니콤보말고 있나요??",
          "original_form": "ㅋㅋㅋ &name1&님 최애치킨 교촌 허니콤보말고 있나요??",
          "speaker_id": "2",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015412.1.24",
          "form": "저의 최애는 비비큐에요",
          "original_form": "저의 최애는 비비큐에요",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015412.1.25",
          "form": "황금올리브",
          "original_form": "황금올리브{emoji:♡}",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015412.1.26",
          "form": "오 저도 황올 좋아하는데ㅠㅠ",
          "original_form": "오 저도 황올 좋아하는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015412.1.27",
          "form": "근데 가족들이 후라이드 먹기싫다그래서",
          "original_form": "근데 가족들이 후라이드 먹기싫다그래서",
          "speaker_id": "2",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015412.1.28",
          "form": "먹어본지 까마득하네여ㅠㅠㅠ",
          "original_form": "먹어본지 까마득하네여ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015412.1.29",
          "form": "앗 정말요?\r\n완전 맛있죠? ㅋㅋㅋ",
          "original_form": "앗 정말요?\r\n완전 맛있죠? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015412.1.30",
          "form": "저는 꼭 닭다리 추가해서 시켜요 ㅋㅋ",
          "original_form": "저는 꼭 닭다리 추가해서 시켜요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:44"
        }
