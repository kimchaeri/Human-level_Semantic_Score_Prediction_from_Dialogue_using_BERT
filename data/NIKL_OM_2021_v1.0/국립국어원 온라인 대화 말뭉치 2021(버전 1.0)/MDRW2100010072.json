      "utterance": [
        {
          "id": "MDRW2100010072.1.1",
          "form": "점심은 드셨어요?",
          "original_form": "점심은 드셨어요?",
          "speaker_id": "1",
          "time": "20210823 13:55"
        },
        {
          "id": "MDRW2100010072.1.2",
          "form": "오늘 잘 먹었어요 ㅎㅎ",
          "original_form": "오늘 잘 먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 13:59"
        },
        {
          "id": "MDRW2100010072.1.3",
          "form": "홈쇼핑에서 산 장어를 먹었네요",
          "original_form": "홈쇼핑에서 산 장어를 먹었네요",
          "speaker_id": "2",
          "time": "20210823 13:59"
        },
        {
          "id": "MDRW2100010072.1.4",
          "form": "오 장어를 점심으로",
          "original_form": "오 장어를 점심으로",
          "speaker_id": "1",
          "time": "20210823 14:05"
        },
        {
          "id": "MDRW2100010072.1.5",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "1",
          "time": "20210823 14:05"
        },
        {
          "id": "MDRW2100010072.1.6",
          "form": "홈쇼핑에서 사는것도 맛나나요?",
          "original_form": "홈쇼핑에서 사는것도 맛나나요?",
          "speaker_id": "1",
          "time": "20210823 14:06"
        },
        {
          "id": "MDRW2100010072.1.7",
          "form": "음 맛은 그냥 그렇고",
          "original_form": "음 맛은 그냥 그렇고",
          "speaker_id": "2",
          "time": "20210823 16:28"
        },
        {
          "id": "MDRW2100010072.1.8",
          "form": "가격은 착해요 그래도 ㅋㅋ",
          "original_form": "가격은 착해요 그래도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:28"
        },
        {
          "id": "MDRW2100010072.1.9",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210823 16:30"
        },
        {
          "id": "MDRW2100010072.1.10",
          "form": "저는 구내식당에서 먹어서인지",
          "original_form": "저는 구내식당에서 먹어서인지",
          "speaker_id": "1",
          "time": "20210823 16:30"
        },
        {
          "id": "MDRW2100010072.1.11",
          "form": "벌써 출출하네요 ㅎ",
          "original_form": "벌써 출출하네요 ㅎ",
          "speaker_id": "1",
          "time": "20210823 16:30"
        },
        {
          "id": "MDRW2100010072.1.12",
          "form": "저도요ㅜ",
          "original_form": "저도요ㅜ",
          "speaker_id": "2",
          "time": "20210823 16:33"
        },
        {
          "id": "MDRW2100010072.1.13",
          "form": "근데 오늘부터 진짜 다이어트 식단 들어가게요",
          "original_form": "근데 오늘부터 진짜 다이어트 식단 들어가게요",
          "speaker_id": "2",
          "time": "20210823 16:34"
        },
        {
          "id": "MDRW2100010072.1.14",
          "form": "저도 다이어트식단으로 먹겠다고 밥을 적게 담았더니",
          "original_form": "저도 다이어트식단으로 먹겠다고 밥을 적게 담았더니",
          "speaker_id": "1",
          "time": "20210823 18:41"
        },
        {
          "id": "MDRW2100010072.1.15",
          "form": "배고프네요",
          "original_form": "배고프네요",
          "speaker_id": "1",
          "time": "20210823 18:41"
        },
        {
          "id": "MDRW2100010072.1.16",
          "form": "저녁먹을시간이군요 ㅎ",
          "original_form": "저녁먹을시간이군요 ㅎ",
          "speaker_id": "1",
          "time": "20210823 18:41"
        },
        {
          "id": "MDRW2100010072.1.17",
          "form": "저는 다이어트 쉐이크를 사놨어요",
          "original_form": "저는 다이어트 쉐이크를 사놨어요",
          "speaker_id": "2",
          "time": "20210823 18:41"
        },
        {
          "id": "MDRW2100010072.1.18",
          "form": "먹기만하면되요 ㅋㅋ",
          "original_form": "먹기만하면되요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 18:41"
        },
        {
          "id": "MDRW2100010072.1.19",
          "form": "어디꺼예요?",
          "original_form": "어디꺼예요?",
          "speaker_id": "1",
          "time": "20210823 18:42"
        },
        {
          "id": "MDRW2100010072.1.20",
          "form": "맛나나요,,",
          "original_form": "맛나나요,,",
          "speaker_id": "1",
          "time": "20210823 18:42"
        },
        {
          "id": "MDRW2100010072.1.21",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210823 18:42"
        },
        {
          "id": "MDRW2100010072.1.22",
          "form": "노니 곡물맛이에요",
          "original_form": "노니 곡물맛이에요",
          "speaker_id": "2",
          "time": "20210823 18:42"
        },
        {
          "id": "MDRW2100010072.1.23",
          "form": "생각만해도 망한거같아요 ㅋㅋ",
          "original_form": "생각만해도 망한거같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 18:42"
        },
        {
          "id": "MDRW2100010072.1.24",
          "form": "앗...안땡기는 맛이긴...하네요 ㅎ",
          "original_form": "앗...안땡기는 맛이긴...하네요 ㅎ",
          "speaker_id": "1",
          "time": "20210823 18:43"
        },
        {
          "id": "MDRW2100010072.1.25",
          "form": "어쩌다 ㅋㅋㅋ",
          "original_form": "어쩌다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:44"
        },
        {
          "id": "MDRW2100010072.1.26",
          "form": "동생이 ㅋㅋ 주고가써요",
          "original_form": "동생이 ㅋㅋ 주고가써요",
          "speaker_id": "2",
          "time": "20210823 18:46"
        },
        {
          "id": "MDRW2100010072.1.27",
          "form": "미묘하게 고마움 ㅋㅋㅋ",
          "original_form": "미묘하게 고마움 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 18:46"
        },
        {
          "id": "MDRW2100010072.1.28",
          "form": "노니 들어간거니 몸에 좋을꺼예요",
          "original_form": "노니 들어간거니 몸에 좋을꺼예요",
          "speaker_id": "1",
          "time": "20210823 18:48"
        },
        {
          "id": "MDRW2100010072.1.29",
          "form": "언니생각하는 따뜻한맘으로 맛나게 드세요",
          "original_form": "언니생각하는 따뜻한맘으로 맛나게 드세요",
          "speaker_id": "1",
          "time": "20210823 18:49"
        },
        {
          "id": "MDRW2100010072.1.30",
          "form": "뭔가 저것만 먹자니 서러워요 ㅋ",
          "original_form": "뭔가 저것만 먹자니 서러워요 ㅋ",
          "speaker_id": "2",
          "time": "20210823 20:07"
        },
        {
          "id": "MDRW2100010072.1.31",
          "form": "집을 뒤지고있어요 ㅋㅋ",
          "original_form": "집을 뒤지고있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:07"
        },
        {
          "id": "MDRW2100010072.1.32",
          "form": "스탑!!!ㅎㅎ",
          "original_form": "스탑!!!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 22:52"
        }
      ]
    }
