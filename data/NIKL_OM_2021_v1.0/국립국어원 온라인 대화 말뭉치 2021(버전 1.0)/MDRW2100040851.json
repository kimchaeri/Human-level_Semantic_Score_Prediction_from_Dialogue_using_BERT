      "utterance": [
        {
          "id": "MDRW2100040851.1.1",
          "form": "네 당근마켓은 눈팅만 하다가 지웠어요",
          "original_form": "네 당근마켓은 눈팅만 하다가 지웠어요",
          "speaker_id": "2",
          "time": "20210905 09:55"
        },
        {
          "id": "MDRW2100040851.1.2",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210905 09:57"
        },
        {
          "id": "MDRW2100040851.1.3",
          "form": "자기에게 맞는 를랫폼이 있나봅니다",
          "original_form": "자기에게 맞는 를랫폼이 있나봅니다",
          "speaker_id": "1",
          "time": "20210905 09:57"
        },
        {
          "id": "MDRW2100040851.1.4",
          "form": "네 그런 거 같아요",
          "original_form": "네 그런 거 같아요",
          "speaker_id": "2",
          "time": "20210905 09:57"
        },
        {
          "id": "MDRW2100040851.1.5",
          "form": "저는 올해 가입해서 아주 잘 이용하고 있거든요",
          "original_form": "저는 올해 가입해서 아주 잘 이용하고 있거든요",
          "speaker_id": "1",
          "time": "20210905 09:57"
        },
        {
          "id": "MDRW2100040851.1.6",
          "form": "저는 네이버페이 포인트 많이 줘서 좋아요",
          "original_form": "저는 네이버페이 포인트 많이 줘서 좋아요",
          "speaker_id": "2",
          "time": "20210905 09:57"
        },
        {
          "id": "MDRW2100040851.1.7",
          "form": "사기보다는 주로 팔죠",
          "original_form": "사기보다는 주로 팔죠",
          "speaker_id": "1",
          "time": "20210905 09:57"
        },
        {
          "id": "MDRW2100040851.1.8",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210905 09:57"
        },
        {
          "id": "MDRW2100040851.1.9",
          "form": "많이 파셔서 좋으시겠어요",
          "original_form": "많이 파셔서 좋으시겠어요",
          "speaker_id": "2",
          "time": "20210905 09:57"
        },
        {
          "id": "MDRW2100040851.1.10",
          "form": "당근 온도가 꽤 올랐죠",
          "original_form": "당근 온도가 꽤 올랐죠",
          "speaker_id": "1",
          "time": "20210905 09:59"
        },
        {
          "id": "MDRW2100040851.1.11",
          "form": "기프티스타는 네이버포인트도 줘요?",
          "original_form": "기프티스타는 네이버포인트도 줘요?",
          "speaker_id": "1",
          "time": "20210905 09:59"
        },
        {
          "id": "MDRW2100040851.1.12",
          "form": "아녀 돈으로 줘요",
          "original_form": "아녀 돈으로 줘요",
          "speaker_id": "2",
          "time": "20210905 09:59"
        },
        {
          "id": "MDRW2100040851.1.13",
          "form": "아 그러면 네이버쇼핑으로 물건 사면 포인트 많이 준다는 말인가요?",
          "original_form": "아 그러면 네이버쇼핑으로 물건 사면 포인트 많이 준다는 말인가요?",
          "speaker_id": "1",
          "time": "20210905 10:00"
        },
        {
          "id": "MDRW2100040851.1.14",
          "form": "아니요 기프티스타 기프티콘 파는 곳인데요",
          "original_form": "아니요 기프티스타 기프티콘 파는 곳인데요",
          "speaker_id": "2",
          "time": "20210905 10:00"
        },
        {
          "id": "MDRW2100040851.1.15",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210905 10:00"
        },
        {
          "id": "MDRW2100040851.1.16",
          "form": "최근 구매한 물건 있으세요?",
          "original_form": "최근 구매한 물건 있으세요?",
