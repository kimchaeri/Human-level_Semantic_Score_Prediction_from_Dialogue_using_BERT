        {
          "id": "MDRW2100006157.1.1",
          "form": "여행 좋아하시나요",
          "original_form": "여행 좋아하시나요",
          "speaker_id": "1",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100006157.1.2",
          "form": "여행 좋아하죠",
          "original_form": "여행 좋아하죠",
          "speaker_id": "2",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100006157.1.3",
          "form": "해외vs국내",
          "original_form": "해외vs국내",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100006157.1.4",
          "form": "국내 좋아해요!",
          "original_form": "국내 좋아해요!",
          "speaker_id": "2",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100006157.1.5",
          "form": "추천하는 여행지 잇으세요??",
          "original_form": "추천하는 여행지 잇으세요??",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100006157.1.6",
          "form": "전 부산이 재밌더라구요",
          "original_form": "전 부산이 재밌더라구요",
          "speaker_id": "2",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100006157.1.7",
          "form": "ㅋㅋㅋ맞아요 뷰산 좋져 마지막으로 간지 오래되긴햇네요",
          "original_form": "ㅋㅋㅋ맞아요 뷰산 좋져 마지막으로 간지 오래되긴햇네요",
          "speaker_id": "1",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100006157.1.8",
          "form": "7년 전이던가..",
          "original_form": "7년 전이던가..",
          "speaker_id": "1",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100006157.1.9",
          "form": "저는 최근에 전주다녀왓어요",
          "original_form": "저는 최근에 전주다녀왓어요",
          "speaker_id": "1",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100006157.1.10",
          "form": "헐 전주좋죠...",
          "original_form": "헐 전주좋죠...",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100006157.1.11",
          "form": "전주 가보고싶었어요..",
          "original_form": "전주 가보고싶었어요..",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100006157.1.12",
          "form": "역시 ...전주...",
          "original_form": "역시 ...전주...",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100006157.1.13",
          "form": "음식이 미쳣어요",
          "original_form": "음식이 미쳣어요",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100006157.1.14",
          "form": "대존맛",
          "original_form": "대존맛",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100006157.1.15",
          "form": "한옥마을 주변에 먹을거 많잖아요ㅠㅜ",
          "original_form": "한옥마을 주변에 먹을거 많잖아요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100006157.1.16",
          "form": "맞아요! 바로앞에 남부시장",
          "original_form": "맞아요! 바로앞에 남부시장",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100006157.1.17",
          "form": "그안에 맛집들이 ㅋㅋㅋㅋ",
          "original_form": "그안에 맛집들이 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100006157.1.18",
          "form": "여자친구랑 같이 가보고싶었어요ㅠㅜ",
          "original_form": "여자친구랑 같이 가보고싶었어요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100006157.1.19",
          "form": "알바를 하지 않을 지금이 기회일거에요 ㅋㅋㅋㅋ",
          "original_form": "알바를 하지 않을 지금이 기회일거에요 ㅋㅋㅋㅋ",
