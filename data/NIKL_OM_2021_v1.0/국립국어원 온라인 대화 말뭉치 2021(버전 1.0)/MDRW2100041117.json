        },
        {
          "id": "MDRW2100041117.1.2",
          "form": "네네 좋아하죠",
          "original_form": "네네 좋아하죠",
          "speaker_id": "2",
          "time": "20210905 17:24"
        },
        {
          "id": "MDRW2100041117.1.3",
          "form": "뭐 자주 사세요?",
          "original_form": "뭐 자주 사세요?",
          "speaker_id": "1",
          "time": "20210905 17:24"
        },
        {
          "id": "MDRW2100041117.1.4",
          "form": "옷이나 먹을거요 ㅋㅋㄱㅋ",
          "original_form": "옷이나 먹을거요 ㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210905 17:25"
        },
        {
          "id": "MDRW2100041117.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210905 17:25"
        },
        {
          "id": "MDRW2100041117.1.6",
          "form": "저는 문구류요",
          "original_form": "저는 문구류요",
          "speaker_id": "1",
          "time": "20210905 17:25"
        },
        {
          "id": "MDRW2100041117.1.7",
          "form": "어디서 자주 사세요?",
          "original_form": "어디서 자주 사세요?",
          "speaker_id": "1",
          "time": "20210905 17:25"
        },
        {
          "id": "MDRW2100041117.1.8",
          "form": "인터넷에서 주로사는것 같아요",
          "original_form": "인터넷에서 주로사는것 같아요",
          "speaker_id": "2",
          "time": "20210905 17:26"
        },
        {
          "id": "MDRW2100041117.1.9",
          "form": "항상 최저가로 ㅎㅎ",
          "original_form": "항상 최저가로 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 17:26"
        },
        {
          "id": "MDRW2100041117.1.10",
          "form": "문구류는 직접보고 사세요?",
          "original_form": "문구류는 직접보고 사세요?",
          "speaker_id": "2",
          "time": "20210905 17:26"
        },
        {
          "id": "MDRW2100041117.1.11",
          "form": "저도 인터넷에서 사는 편이에요",
          "original_form": "저도 인터넷에서 사는 편이에요",
          "speaker_id": "1",
          "time": "20210905 17:26"
        },
        {
          "id": "MDRW2100041117.1.12",
          "form": "텐바이텐에서",
          "original_form": "텐바이텐에서",
          "speaker_id": "1",
          "time": "20210905 17:26"
        },
        {
          "id": "MDRW2100041117.1.13",
          "form": "중고거래도 자주 하시나요?",
          "original_form": "중고거래도 자주 하시나요?",
          "speaker_id": "1",
          "time": "20210905 17:26"
        },
        {
          "id": "MDRW2100041117.1.14",
          "form": "당근 가끔해요",
          "original_form": "당근 가끔해요",
          "speaker_id": "2",
          "time": "20210905 17:26"
        },
        {
          "id": "MDRW2100041117.1.15",
          "form": "당근하세요?",
          "original_form": "당근하세요?",
          "speaker_id": "2",
          "time": "20210905 17:27"
        },
        {
          "id": "MDRW2100041117.1.16",
          "form": "네 은근 재밌더라고요",
          "original_form": "네 은근 재밌더라고요",
          "speaker_id": "1",
          "time": "20210905 17:28"
        },
        {
          "id": "MDRW2100041117.1.17",
          "form": "주로 파시는쪽?",
          "original_form": "주로 파시는쪽?",
          "speaker_id": "2",
          "time": "20210905 17:28"
        },
        {
          "id": "MDRW2100041117.1.18",
          "form": "당근네서 사신 물건 잇으신가요",
          "original_form": "당근네서 사신 물건 잇으신가요",
          "speaker_id": "1",
          "time": "20210905 17:28"
        },
        {
          "id": "MDRW2100041117.1.19",
          "form": "저는 사는쪽이에요",
          "original_form": "저는 사는쪽이에요",
          "speaker_id": "1",
          "time": "20210905 17:28"
        },
        {
          "id": "MDRW2100041117.1.20",
          "form": "저 필링기샀어요",
          "original_form": "저 필링기샀어요",
          "speaker_id": "2",
          "time": "20210905 17:28"
        },
        {
          "id": "MDRW2100041117.1.21",
