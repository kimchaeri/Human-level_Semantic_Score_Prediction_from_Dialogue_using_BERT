      "utterance": [
        {
          "id": "MDRW2100022626.1.1",
          "form": "많이 본편인거같아요",
          "original_form": "많이 본편인거같아요",
          "speaker_id": "2",
          "time": "20210829 16:28"
        },
        {
          "id": "MDRW2100022626.1.2",
          "form": "밥먹을때나 짬날때 자주 본거같아요",
          "original_form": "밥먹을때나 짬날때 자주 본거같아요",
          "speaker_id": "2",
          "time": "20210829 16:29"
        },
        {
          "id": "MDRW2100022626.1.3",
          "form": "요즘 괜찮은 시트콤이 너무 없어요 ㅜㅜ",
          "original_form": "요즘 괜찮은 시트콤이 너무 없어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 16:32"
        },
        {
          "id": "MDRW2100022626.1.4",
          "form": "맞아요ㅜ케이블에서 뭐 하는거같긴하던데..",
          "original_form": "맞아요ㅜ케이블에서 뭐 하는거같긴하던데..",
          "speaker_id": "2",
          "time": "20210829 16:35"
        },
        {
          "id": "MDRW2100022626.1.5",
          "form": "성동일 나오고?",
          "original_form": "성동일 나오고?",
          "speaker_id": "2",
          "time": "20210829 16:36"
        },
        {
          "id": "MDRW2100022626.1.6",
          "form": "근데 재미없는거같아서 안 봤어요ㅋㅋ",
          "original_form": "근데 재미없는거같아서 안 봤어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:36"
        },
        {
          "id": "MDRW2100022626.1.7",
          "form": "그거보셨어요? 김선호나오는거 ㅋㅋㅋ 이이경이랑 ㅋㅋㅋ",
          "original_form": "그거보셨어요? 김선호나오는거 ㅋㅋㅋ 이이경이랑 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022626.1.8",
          "form": "와이키키! ㅋㅋ",
          "original_form": "와이키키! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022626.1.9",
          "form": "완전재밌죠 그겈ㅋㅋㅋㅋ",
          "original_form": "완전재밌죠 그겈ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:38"
        },
        {
          "id": "MDRW2100022626.1.10",
          "form": "저는 그걸로 김선호알았어요",
          "original_form": "저는 그걸로 김선호알았어요",
          "speaker_id": "2",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100022626.1.11",
          "form": "저도요저도됴 ㅋㅋㅋ 아 너무웃겨요 ㅋㅋㅋ 이이경도 진짜 좋아요 ㅋㅋ",
          "original_form": "저도요저도됴 ㅋㅋㅋ 아 너무웃겨요 ㅋㅋㅋ 이이경도 진짜 좋아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:41"
        },
        {
          "id": "MDRW2100022626.1.12",
          "form": "시트콤이 이윤이 많이 안나서 안한대요ㅠㅠㅠㅠㅠ",
          "original_form": "시트콤이 이윤이 많이 안나서 안한대요ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 16:41"
        },
        {
          "id": "MDRW2100022626.1.13",
          "form": "이이경 노래 들어보셨어요?ㅋㅋㅋㅋ칼퇴근?",
          "original_form": "이이경 노래 들어보셨어요?ㅋㅋㅋㅋ칼퇴근?",
          "speaker_id": "2",
          "time": "20210829 16:41"
        },
        {
          "id": "MDRW2100022626.1.14",
          "form": "앗 그건또뭐에요?ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앗 그건또뭐에요?ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:42"
        },
        {
          "id": "MDRW2100022626.1.15",
          "form": "제목이 너무 멋진단어네요 ㅋㅋㅋ",
          "original_form": "제목이 너무 멋진단어네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:42"
        },
        {
          "id": "MDRW2100022626.1.16",
          "form": "들어보세요ㅋㅋㅋㅋ전세계적으로 퇴근시간에 틀어줘야하는 곡입니다",
          "original_form": "들어보세요ㅋㅋㅋㅋ전세계적으로 퇴근시간에 틀어줘야하는 곡입니다",
          "speaker_id": "2",
          "time": "20210829 16:43"
        },
        {
          "id": "MDRW2100022626.1.17",
          "form": "춤도 웃겨요ㅋㅋㅋㅋㅋ",
          "original_form": "춤도 웃겨요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:43"
        },
        {
          "id": "MDRW2100022626.1.18",
          "form": "음원낸거에요? ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "음원낸거에요? ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:43"
        },
        {
          "id": "MDRW2100022626.1.19",
          "form": "갑자기?ㅋㅋㅋ",
          "original_form": "갑자기?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:43"
        },
        {
          "id": "MDRW2100022626.1.20",
          "form": "ㅋㅋㅋㅋ플레이어에서 각자 노래하나씩 내다가 만든거같ㅇㅇ았어요",
          "original_form": "ㅋㅋㅋㅋ플레이어에서 각자 노래하나씩 내다가 만든거같ㅇㅇ았어요",
          "speaker_id": "2",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100022626.1.21",
          "form": "약간 트롯재질인데 컬러링으로 하고 싶을정도",
          "original_form": "약간 트롯재질인데 컬러링으로 하고 싶을정도",
          "speaker_id": "2",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100022626.1.22",
          "form": "아 플레이어 뭐더라 예능인가요?",
          "original_form": "아 플레이어 뭐더라 예능인가요?",
          "speaker_id": "1",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022626.1.23",
          "form": "네네 이수근나오고",
          "original_form": "네네 이수근나오고",
          "speaker_id": "2",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022626.1.24",
          "form": "웃을때마다 물맞고 출연료 빠지는ㅋㅋㅋㅋ가끔봤어요",
          "original_form": "웃을때마다 물맞고 출연료 빠지는ㅋㅋㅋㅋ가끔봤어요",
          "speaker_id": "2",
          "time": "20210829 16:47"
        }
      ]
