      "utterance": [
        {
          "id": "MDRW2100003672.1.1",
          "form": "오늘 아침식사하셨나요",
          "original_form": "오늘 아침식사하셨나요",
          "speaker_id": "1",
          "time": "20210821 10:23"
        },
        {
          "id": "MDRW2100003672.1.2",
          "form": "네 제육볶음요 ㅋㅋ",
          "original_form": "네 제육볶음요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 10:23"
        },
        {
          "id": "MDRW2100003672.1.3",
          "form": "헉 저도 아침에 제육먹었어요",
          "original_form": "헉 저도 아침에 제육먹었어요",
          "speaker_id": "1",
          "time": "20210821 10:23"
        },
        {
          "id": "MDRW2100003672.1.4",
          "form": "신기하네요 ㅋㅋㅋㅋ",
          "original_form": "신기하네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 10:23"
        },
        {
          "id": "MDRW2100003672.1.5",
          "form": "왠일 넘신기해요",
          "original_form": "왠일 넘신기해요",
          "speaker_id": "2",
          "time": "20210821 10:24"
        },
        {
          "id": "MDRW2100003672.1.6",
          "form": "아침에 제육 사실 안먹잖아요 다들 ㅋㅋ",
          "original_form": "아침에 제육 사실 안먹잖아요 다들 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 10:24"
        },
        {
          "id": "MDRW2100003672.1.7",
          "form": "매콤하니 맛났어요 버섯 넣어 했어요",
          "original_form": "매콤하니 맛났어요 버섯 넣어 했어요",
          "speaker_id": "2",
          "time": "20210821 10:24"
        },
        {
          "id": "MDRW2100003672.1.8",
          "form": "전 커피숍 디저트도 엄청 좋아해요",
          "original_form": "전 커피숍 디저트도 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210821 10:25"
        },
        {
          "id": "MDRW2100003672.1.9",
          "form": "케이크 같은거 머핀도 막즐겨요",
          "original_form": "케이크 같은거 머핀도 막즐겨요",
          "speaker_id": "2",
          "time": "20210821 10:26"
        },
        {
          "id": "MDRW2100003672.1.10",
          "form": "디저트 뭐 좋아 하세요?",
          "original_form": "디저트 뭐 좋아 하세요?",
          "speaker_id": "2",
          "time": "20210821 10:28"
        },
        {
          "id": "MDRW2100003672.1.11",
          "form": "ㅋㅋㅋㅋ오늘 갑자기땡기더라구여",
          "original_form": "ㅋㅋㅋㅋ오늘 갑자기땡기더라구여",
          "speaker_id": "1",
          "time": "20210821 10:42"
        },
        {
          "id": "MDRW2100003672.1.12",
          "form": "디저트 거의 다좋아해요 케이크 마카롱 과자에 환장합니다 ㅋㅋ",
          "original_form": "디저트 거의 다좋아해요 케이크 마카롱 과자에 환장합니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 10:42"
        },
        {
          "id": "MDRW2100003672.1.13",
          "form": "오 저도요",
          "original_form": "오 저도요",
          "speaker_id": "2",
          "time": "20210821 10:43"
        },
        {
          "id": "MDRW2100003672.1.14",
          "form": "얼마전까진 스콘에 빠졌러요",
          "original_form": "얼마전까진 스콘에 빠졌러요",
          "speaker_id": "2",
          "time": "20210821 10:43"
        },
        {
          "id": "MDRW2100003672.1.15",
          "form": "오 스콘 넘 좋죠 직접만드시기도 하시나요?",
          "original_form": "오 스콘 넘 좋죠 직접만드시기도 하시나요?",
          "speaker_id": "1",
          "time": "20210821 10:44"
        },
        {
          "id": "MDRW2100003672.1.16",
          "form": "아뇨 ㅋㅋ",
          "original_form": "아뇨 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 10:45"
        },
        {
          "id": "MDRW2100003672.1.17",
          "form": "전 한번 해봤는데 영아니였어요",
          "original_form": "전 한번 해봤는데 영아니였어요",
          "speaker_id": "2",
          "time": "20210821 10:45"
        },
        {
          "id": "MDRW2100003672.1.18",
          "form": "백화점에서 토핑도 올려주고 맛나서 이것저것먹었어요",
          "original_form": "백화점에서 토핑도 올려주고 맛나서 이것저것먹었어요",
          "speaker_id": "2",
          "time": "20210821 10:45"
        },
        {
          "id": "MDRW2100003672.1.19",
          "form": "ㅋㅋㅋㅋ사먹는게 젤맛있긴해요 저도 한번만들어봤다가",
          "original_form": "ㅋㅋㅋㅋ사먹는게 젤맛있긴해요 저도 한번만들어봤다가",
          "speaker_id": "1",
          "time": "20210821 10:46"
        },
        {
          "id": "MDRW2100003672.1.20",
          "form": "파는 그맛안나더라구여",
          "original_form": "파는 그맛안나더라구여",
          "speaker_id": "1",
          "time": "20210821 10:46"
        },
        {
          "id": "MDRW2100003672.1.21",
          "form": "그죠 파는맛 ㅋㅋ",
          "original_form": "그죠 파는맛 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 10:49"
        },
        {
          "id": "MDRW2100003672.1.22",
          "form": "전 크린베리 들어간거좋아해요",
          "original_form": "전 크린베리 들어간거좋아해요",
          "speaker_id": "2",
          "time": "20210821 10:49"
        },
        {
          "id": "MDRW2100003672.1.23",
          "form": "전 초코들어간거요 ㅋㅋㅋ애입맛이라",
          "original_form": "전 초코들어간거요 ㅋㅋㅋ애입맛이라",
          "speaker_id": "1",
          "time": "20210821 10:51"
        },
        {
          "id": "MDRW2100003672.1.24",
          "form": "그래서 스콘같은건 우유랑같이먹어요 ㅋㅋㅋㅋ",
          "original_form": "그래서 스콘같은건 우유랑같이먹어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 10:51"
        },
        {
          "id": "MDRW2100003672.1.25",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 10:52"
        },
        {
          "id": "MDRW2100003672.1.26",
          "form": "저도 초고 광팬입니다",
          "original_form": "저도 초고 광팬입니다",
          "speaker_id": "2",
          "time": "20210821 10:52"
        },
        {
          "id": "MDRW2100003672.1.27",
          "form": "생일에 초코 케이크 입니다",
          "original_form": "생일에 초코 케이크 입니다",
          "speaker_id": "2",
          "time": "20210821 10:52"
        },
        {
          "id": "MDRW2100003672.1.28",
          "form": "우유는 초코죠",
          "original_form": "우유는 초코죠",
          "speaker_id": "2",
          "time": "20210821 10:52"
        },
        {
          "id": "MDRW2100003672.1.29",
          "form": "ㅎㅎ저도 초코케이크 무지좋아해요",
          "original_form": "ㅎㅎ저도 초코케이크 무지좋아해요",
          "speaker_id": "1",
          "time": "20210821 10:53"
        }
      ]
