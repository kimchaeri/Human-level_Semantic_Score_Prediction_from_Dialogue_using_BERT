        {
          "id": "MDRW2100029581.1.1",
          "form": "아니 딴사람도 아니구 님이 제게 그런말을 하시다니ㅂㄷㅂㄷ",
          "original_form": "아니 딴사람도 아니구 님이 제게 그런말을 하시다니ㅂㄷㅂㄷ",
          "speaker_id": "1",
          "time": "20210903 10:38"
        },
        {
          "id": "MDRW2100029581.1.2",
          "form": "낮술쟁이name1",
          "original_form": "낮술쟁이&name1&",
          "speaker_id": "2",
          "time": "20210903 10:39"
        },
        {
          "id": "MDRW2100029581.1.3",
          "form": "저는 낮술 안먹었는데요 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 낮술 안먹었는데요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 10:39"
        },
        {
          "id": "MDRW2100029581.1.4",
          "form": "평생 안 마셨나욧.!,",
          "original_form": "평생 안 마셨나욧.!,",
          "speaker_id": "1",
          "time": "20210903 10:40"
        },
        {
          "id": "MDRW2100029581.1.5",
          "form": "그건 아니지만",
          "original_form": "그건 아니지만",
          "speaker_id": "2",
          "time": "20210903 10:42"
        },
        {
          "id": "MDRW2100029581.1.6",
          "form": "뻔뻔하시군욧.!",
          "original_form": "뻔뻔하시군욧.!",
          "speaker_id": "1",
          "time": "20210903 10:50"
        },
        {
          "id": "MDRW2100029581.1.7",
          "form": "정치인이셨나요?",
          "original_form": "정치인이셨나요?",
          "speaker_id": "1",
          "time": "20210903 10:51"
        },
        {
          "id": "MDRW2100029581.1.8",
          "form": "제 꿈이 에헴",
          "original_form": "제 꿈이 에헴",
          "speaker_id": "2",
          "time": "20210903 11:37"
        },
        {
          "id": "MDRW2100029581.1.9",
          "form": "아 본가 가기 귀찮다요",
          "original_form": "아 본가 가기 귀찮다요",
          "speaker_id": "2",
          "time": "20210903 11:37"
        },
        {
          "id": "MDRW2100029581.1.10",
          "form": "본가 갈 일이 있남요",
          "original_form": "본가 갈 일이 있남요",
          "speaker_id": "1",
          "time": "20210903 11:38"
        },
        {
          "id": "MDRW2100029581.1.11",
          "form": "엄마가 돈 받으러 오래서요",
          "original_form": "엄마가 돈 받으러 오래서요",
          "speaker_id": "2",
          "time": "20210903 11:41"
        },
        {
          "id": "MDRW2100029581.1.12",
          "form": "점심도 먹고",
          "original_form": "점심도 먹고",
          "speaker_id": "2",
          "time": "20210903 11:41"
        },
        {
          "id": "MDRW2100029581.1.13",
          "form": "은혜롭고 자애로우시네요",
          "original_form": "은혜롭고 자애로우시네요",
          "speaker_id": "1",
          "time": "20210903 11:45"
        },
        {
          "id": "MDRW2100029581.1.14",
          "form": "집밥인가요!!",
          "original_form": "집밥인가요!!",
          "speaker_id": "1",
          "time": "20210903 11:45"
        },
        {
          "id": "MDRW2100029581.1.15",
          "form": "네 근데 엄마 요리 못함",
          "original_form": "네 근데 엄마 요리 못함",
          "speaker_id": "2",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100029581.1.16",
          "form": "불효자시군요",
          "original_form": "불효자시군요",
          "speaker_id": "1",
          "time": "20210903 12:43"
        },
        {
          "id": "MDRW2100029581.1.17",
          "form": "팥으로 메주를 쒀도 꿀처럼 드셔야 자식된 도리쥬ㅎㅎ",
          "original_form": "팥으로 메주를 쒀도 꿀처럼 드셔야 자식된 도리쥬ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 12:44"
        },
        {
          "id": "MDRW2100029581.1.18",
          "form": "밥 세공기 드셔요!!ㅎㅎ",
          "original_form": "밥 세공기 드셔요!!ㅎㅎ",
