      "utterance": [
        {
          "id": "MDRW2100047408.1.1",
          "form": "미용실 요새는 잘 안가게 되네요",
          "original_form": "미용실 요새는 잘 안가게 되네요",
          "speaker_id": "2",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100047408.1.2",
          "form": "귀찮기도 하구요ㅋㅋ",
          "original_form": "귀찮기도 하구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100047408.1.3",
          "form": "ㅋㅋㅋ단발이신가요",
          "original_form": "ㅋㅋㅋ단발이신가요",
          "speaker_id": "1",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100047408.1.4",
          "form": "아님 긴머리?",
          "original_form": "아님 긴머리?",
          "speaker_id": "1",
          "time": "20210826 15:40"
        },
        {
          "id": "MDRW2100047408.1.5",
          "form": "중단발이요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "중단발이요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:13"
        },
        {
          "id": "MDRW2100047408.1.6",
          "form": "근데 자르고싶네요",
          "original_form": "근데 자르고싶네요",
          "speaker_id": "2",
          "time": "20210826 16:13"
        },
        {
          "id": "MDRW2100047408.1.7",
          "form": "ㅋㅋㅋㅋ저는 등 중반까지 길렀어요",
          "original_form": "ㅋㅋㅋㅋ저는 등 중반까지 길렀어요",
          "speaker_id": "1",
          "time": "20210826 16:18"
        },
        {
          "id": "MDRW2100047408.1.8",
          "form": "원래 짧은 단발이었는데ㅜ",
          "original_form": "원래 짧은 단발이었는데ㅜ",
          "speaker_id": "1",
          "time": "20210826 16:19"
        },
        {
          "id": "MDRW2100047408.1.9",
          "form": "덥지 않나요..",
          "original_form": "덥지 않나요..",
          "speaker_id": "2",
          "time": "20210826 16:43"
        },
        {
          "id": "MDRW2100047408.1.10",
          "form": "엄청 더워요... 그래서 맨날 묶고있어요ㅜ",
          "original_form": "엄청 더워요... 그래서 맨날 묶고있어요ㅜ",
          "speaker_id": "1",
          "time": "20210826 16:46"
        },
        {
          "id": "MDRW2100047408.1.11",
          "form": "ㅋㅋㅋ진짜 목뒤가",
          "original_form": "ㅋㅋㅋ진짜 목뒤가",
          "speaker_id": "2",
          "time": "20210826 16:49"
        },
        {
          "id": "MDRW2100047408.1.12",
          "form": "넘나 더웡6",
          "original_form": "넘나 더웡6",
          "speaker_id": "2",
          "time": "20210826 16:49"
        },
        {
          "id": "MDRW2100047408.1.13",
          "form": "머리카락 들러붙을 때 진짜 짲응...",
          "original_form": "머리카락 들러붙을 때 진짜 짲응...",
          "speaker_id": "1",
          "time": "20210826 16:51"
        },
        {
          "id": "MDRW2100047408.1.14",
          "form": "네.. 저도 알죠......",
          "original_form": "네.. 저도 알죠......",
          "speaker_id": "2",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100047408.1.15",
          "form": "그 심정 정말 ...",
          "original_form": "그 심정 정말 ...",
          "speaker_id": "2",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100047408.1.16",
          "form": "그럴 때는 짧게 자르고싶은데\r\n제가 이번에 처음 길러봐서ㅠ 조금만 다듬고 1년만 더 유지하고 싶어요",
          "original_form": "그럴 때는 짧게 자르고싶은데\r\n제가 이번에 처음 길러봐서ㅠ 조금만 다듬고 1년만 더 유지하고 싶어요",
          "speaker_id": "1",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100047408.1.17",
          "form": "어쩌다가 기르셨어요?",
          "original_form": "어쩌다가 기르셨어요?",
          "speaker_id": "2",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100047408.1.18",
          "form": "코로나가 심해서 안가다보니 기르게 됐어요ㅋㅋ",
          "original_form": "코로나가 심해서 안가다보니 기르게 됐어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100047408.1.19",
          "form": "아하 일부러 기르신게 아니군요ㅋㅋㅋㅋ",
          "original_form": "아하 일부러 기르신게 아니군요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100047408.1.20",
          "form": "네ㅋㅋㅋ일부러 기른건 아니에요ㅋㅋ 어쩌다보니...ㅜ",
          "original_form": "네ㅋㅋㅋ일부러 기른건 아니에요ㅋㅋ 어쩌다보니...ㅜ",
          "speaker_id": "1",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100047408.1.21",
          "form": "ㅎ 그래도 좋은 경험이죠",
          "original_form": "ㅎ 그래도 좋은 경험이죠",
          "speaker_id": "2",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100047408.1.22",
          "form": "맞아요ㅋㅋ그래서 사진이라도 찍어두고 자르려구요",
          "original_form": "맞아요ㅋㅋ그래서 사진이라도 찍어두고 자르려구요",
          "speaker_id": "1",
          "time": "20210826 19:09"
        }
      ]
