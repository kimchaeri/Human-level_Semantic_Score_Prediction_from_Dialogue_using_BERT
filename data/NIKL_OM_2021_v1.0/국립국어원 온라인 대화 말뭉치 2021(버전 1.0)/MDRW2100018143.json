      "utterance": [
        {
          "id": "MDRW2100018143.1.1",
          "form": "아니요 가족과 함께 살고 있어요",
          "original_form": "아니요 가족과 함께 살고 있어요",
          "speaker_id": "2",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100018143.1.2",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100018143.1.3",
          "form": "저두요 ㅋㅋㅋ",
          "original_form": "저두요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100018143.1.4",
          "form": "자취하고 싶진 않으세요?",
          "original_form": "자취하고 싶진 않으세요?",
          "speaker_id": "1",
          "time": "20210826 22:51"
        },
        {
          "id": "MDRW2100018143.1.5",
          "form": "하고싶어요!",
          "original_form": "하고싶어요!",
          "speaker_id": "2",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018143.1.6",
          "form": "근데 쉽지않네요ㅠ",
          "original_form": "근데 쉽지않네요ㅠ",
          "speaker_id": "2",
          "time": "20210826 22:55"
        },
        {
          "id": "MDRW2100018143.1.7",
          "form": "맞아요 현실적으로 돈도 많이들고 힘들죠 ㅠ",
          "original_form": "맞아요 현실적으로 돈도 많이들고 힘들죠 ㅠ",
          "speaker_id": "1",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018143.1.8",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210826 22:56"
        },
        {
          "id": "MDRW2100018143.1.9",
          "form": "언제 가능할런지 싶어요ㅜ",
          "original_form": "언제 가능할런지 싶어요ㅜ",
          "speaker_id": "2",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100018143.1.10",
          "form": "저는 같이 사는게 편한거같아요 ㅋㅋㅋ",
          "original_form": "저는 같이 사는게 편한거같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100018143.1.11",
          "form": "사이가 좋으신가봐요!",
          "original_form": "사이가 좋으신가봐요!",
          "speaker_id": "2",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100018143.1.12",
          "form": "그럭저럭 좋은 편이에요 ㅋㅋㅋ",
          "original_form": "그럭저럭 좋은 편이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018143.1.13",
          "form": "좋네요ㅋㅋ 그게 은근 어렵잖아요",
          "original_form": "좋네요ㅋㅋ 그게 은근 어렵잖아요",
          "speaker_id": "2",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018143.1.14",
          "form": "맞아요 가까울수록 더 어려운거같아요",
          "original_form": "맞아요 가까울수록 더 어려운거같아요",
          "speaker_id": "1",
          "time": "20210826 22:59"
        },
        {
          "id": "MDRW2100018143.1.15",
          "form": "맞아요 어긋나기도 쉽고 반대도 쉽고요ㅋㅋ",
          "original_form": "맞아요 어긋나기도 쉽고 반대도 쉽고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100018143.1.16",
          "form": "맞아요 너무 붙어있어서 더 그런거같아요",
          "original_form": "맞아요 너무 붙어있어서 더 그런거같아요",
          "speaker_id": "1",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100018143.1.17",
