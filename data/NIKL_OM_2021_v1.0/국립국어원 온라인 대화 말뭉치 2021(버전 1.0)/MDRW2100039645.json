        },
        {
          "id": "MDRW2100039645.1.2",
          "form": "민덩아~~~~~",
          "original_form": "민덩아~~~~~",
          "speaker_id": "1",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100039645.1.3",
          "form": "내가 이제야 집에 와서 핸드폰을 본다네~~~~",
          "original_form": "내가 이제야 집에 와서 핸드폰을 본다네~~~~",
          "speaker_id": "1",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100039645.1.4",
          "form": "응응 나 9시 수업이었는데",
          "original_form": "응응 나 9시 수업이었는데",
          "speaker_id": "1",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100039645.1.5",
          "form": "지각했지만 그래도 오늘은 괜찮았어!!!!",
          "original_form": "지각했지만 그래도 오늘은 괜찮았어!!!!",
          "speaker_id": "1",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100039645.1.6",
          "form": "응응 잘했어~!!",
          "original_form": "응응 잘했어~!!",
          "speaker_id": "2",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100039645.1.7",
          "form": "민덩이는",
          "original_form": "민덩이는",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100039645.1.8",
          "form": "컨디션 좀 어때?!",
          "original_form": "컨디션 좀 어때?!",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100039645.1.9",
          "form": "오늘 화이자 2차 맞은지",
          "original_form": "오늘 화이자 2차 맞은지",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100039645.1.10",
          "form": "만으로 2일차인가 ?!",
          "original_form": "만으로 2일차인가 ?!",
          "speaker_id": "1",
          "time": "20210903 23:22"
        },
        {
          "id": "MDRW2100039645.1.11",
          "form": "웅웅",
          "original_form": "웅웅",
          "speaker_id": "2",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100039645.1.12",
          "form": "생각보다 괜찮아요",
          "original_form": "생각보다 괜찮아요",
          "speaker_id": "2",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100039645.1.13",
          "form": "다행이다!!!!",
          "original_form": "다행이다!!!!",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100039645.1.14",
          "form": "우리 엄마도 어제",
          "original_form": "우리 엄마도 어제",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100039645.1.15",
          "form": "화이자 2차 맞았어!",
          "original_form": "화이자 2차 맞았어!",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100039645.1.16",
          "form": ":)",
          "original_form": ":)",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100039645.1.17",
          "form": "괜찮으셔?",
          "original_form": "괜찮으셔?",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100039645.1.18",
          "form": "응응 아직까지는 괜찮아!! 헤헤",
          "original_form": "응응 아직까지는 괜찮아!! 헤헤",
          "speaker_id": "1",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039645.1.19",
          "form": "1차가 AZ였는데",
          "original_form": "1차가 AZ였는데",
          "speaker_id": "1",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039645.1.20",
          "form": "혈전 반응 보였었거든 ㅠ",
          "original_form": "혈전 반응 보였었거든 ㅠ",
          "speaker_id": "1",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039645.1.21",
          "form": "응 다행이다 정마",
          "original_form": "응 다행이다 정마",
