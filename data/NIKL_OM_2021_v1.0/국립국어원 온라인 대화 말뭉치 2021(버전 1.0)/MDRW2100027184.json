      "utterance": [
        {
          "id": "MDRW2100027184.1.1",
          "form": "오늘도 비가 오네요",
          "original_form": "오늘도 비가 오네요",
          "speaker_id": "1",
          "time": "20210902 10:18"
        },
        {
          "id": "MDRW2100027184.1.2",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210902 10:21"
        },
        {
          "id": "MDRW2100027184.1.3",
          "form": "이놈의 장마 언제 끝날지...",
          "original_form": "이놈의 장마 언제 끝날지...",
          "speaker_id": "2",
          "time": "20210902 10:21"
        },
        {
          "id": "MDRW2100027184.1.4",
          "form": "비가 와리가리하네욪ㅋ",
          "original_form": "비가 와리가리하네욪ㅋ",
          "speaker_id": "1",
          "time": "20210902 12:08"
        },
        {
          "id": "MDRW2100027184.1.5",
          "form": "내렸다가 안내렸다가...",
          "original_form": "내렸다가 안내렸다가...",
          "speaker_id": "2",
          "time": "20210902 12:13"
        },
        {
          "id": "MDRW2100027184.1.6",
          "form": "그렇다고 흐린게 풀리지는 않네ㅛ",
          "original_form": "그렇다고 흐린게 풀리지는 않네ㅛ",
          "speaker_id": "2",
          "time": "20210902 12:13"
        },
        {
          "id": "MDRW2100027184.1.7",
          "form": "요",
          "original_form": "요",
          "speaker_id": "2",
          "time": "20210902 12:13"
        },
        {
          "id": "MDRW2100027184.1.8",
          "form": "또 내려요ㅜ 백신맞으러가는뎅...",
          "original_form": "또 내려요ㅜ 백신맞으러가는뎅...",
          "speaker_id": "1",
          "time": "20210902 12:14"
        },
        {
          "id": "MDRW2100027184.1.9",
          "form": "백신 맞으시는군요",
          "original_form": "백신 맞으시는군요",
          "speaker_id": "2",
          "time": "20210902 12:16"
        },
        {
          "id": "MDRW2100027184.1.10",
          "form": "저는 시기 못맞춰서 엄청 미뤘어요",
          "original_form": "저는 시기 못맞춰서 엄청 미뤘어요",
          "speaker_id": "2",
          "time": "20210902 12:16"
        },
        {
          "id": "MDRW2100027184.1.11",
          "form": "축축한 날씨에 주사라니ㅜ",
          "original_form": "축축한 날씨에 주사라니ㅜ",
          "speaker_id": "1",
          "time": "20210902 12:17"
        },
        {
          "id": "MDRW2100027184.1.12",
          "form": "맞고 나서 푹 쉬세요",
          "original_form": "맞고 나서 푹 쉬세요",
          "speaker_id": "2",
          "time": "20210902 12:19"
        },
        {
          "id": "MDRW2100027184.1.13",
          "form": "날씨가 이래서야 원...",
          "original_form": "날씨가 이래서야 원...",
          "speaker_id": "2",
          "time": "20210902 12:19"
        },
        {
          "id": "MDRW2100027184.1.14",
          "form": "장마 싫어요ㅜ",
          "original_form": "장마 싫어요ㅜ",
          "speaker_id": "1",
          "time": "20210902 12:20"
        },
        {
          "id": "MDRW2100027184.1.15",
          "form": "저두요ㅠ 빨리 해 떴으면 좋겠어요",
          "original_form": "저두요ㅠ 빨리 해 떴으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210902 12:21"
        },
        {
          "id": "MDRW2100027184.1.16",
          "form": "음 비가 언제까지 내릴지",
          "original_form": "음 비가 언제까지 내릴지",
          "speaker_id": "1",
          "time": "20210902 12:24"
        },
        {
          "id": "MDRW2100027184.1.17",
          "form": "이번주 안에는 끝나겠죠?",
          "original_form": "이번주 안에는 끝나겠죠?",
          "speaker_id": "2",
          "time": "20210902 12:25"
        },
        {
          "id": "MDRW2100027184.1.18",
          "form": "그럼 가을 올듯요",
          "original_form": "그럼 가을 올듯요",
          "speaker_id": "1",
          "time": "20210902 12:26"
        }
      ]
