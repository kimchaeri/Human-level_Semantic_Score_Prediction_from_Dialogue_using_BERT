      "utterance": [
        {
          "id": "MDRW2100035942.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210830 12:28"
        },
        {
          "id": "MDRW2100035942.1.2",
          "form": "식사는 하셨어요?",
          "original_form": "식사는 하셨어요?",
          "speaker_id": "1",
          "time": "20210830 12:28"
        },
        {
          "id": "MDRW2100035942.1.3",
          "form": "안녕하세요 아직 안했ㅅ습니다",
          "original_form": "안녕하세요 아직 안했ㅅ습니다",
          "speaker_id": "2",
          "time": "20210830 12:28"
        },
        {
          "id": "MDRW2100035942.1.4",
          "form": "오늘 임연수 먹으려구요!",
          "original_form": "오늘 임연수 먹으려구요!",
          "speaker_id": "2",
          "time": "20210830 12:28"
        },
        {
          "id": "MDRW2100035942.1.5",
          "form": "아하ㅠㅠ 임연수구이! 맛있죵ㅎㅎㅎ",
          "original_form": "아하ㅠㅠ 임연수구이! 맛있죵ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 12:28"
        },
        {
          "id": "MDRW2100035942.1.6",
          "form": "저도 아직 안먹었어요ㅠ",
          "original_form": "저도 아직 안먹었어요ㅠ",
          "speaker_id": "1",
          "time": "20210830 12:29"
        },
        {
          "id": "MDRW2100035942.1.7",
          "form": "뭐 드실거에요?",
          "original_form": "뭐 드실거에요?",
          "speaker_id": "2",
          "time": "20210830 12:52"
        },
        {
          "id": "MDRW2100035942.1.8",
          "form": "저는 수제버거 먹으려구용",
          "original_form": "저는 수제버거 먹으려구용",
          "speaker_id": "1",
          "time": "20210830 12:53"
        },
        {
          "id": "MDRW2100035942.1.9",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 12:53"
        },
        {
          "id": "MDRW2100035942.1.10",
          "form": "수제버거 진짜좋아해요",
          "original_form": "수제버거 진짜좋아해요",
          "speaker_id": "2",
          "time": "20210830 13:13"
        },
        {
          "id": "MDRW2100035942.1.11",
          "form": "우왕ㅎㅎ 감자튀김이랑 같이 먹으면",
          "original_form": "우왕ㅎㅎ 감자튀김이랑 같이 먹으면",
          "speaker_id": "1",
          "time": "20210830 13:13"
        },
        {
          "id": "MDRW2100035942.1.12",
          "form": "전 양파와 치즈를 넣고 다른 소스를 안 넣어서 고기 맛이 잘 느껴지는 버거가 좋아요",
          "original_form": "전 양파와 치즈를 넣고 다른 소스를 안 넣어서 고기 맛이 잘 느껴지는 버거가 좋아요",
          "speaker_id": "2",
          "time": "20210830 13:13"
        },
        {
          "id": "MDRW2100035942.1.13",
          "form": "환상이죠",
          "original_form": "환상이죠",
          "speaker_id": "1",
          "time": "20210830 13:13"
        },
        {
          "id": "MDRW2100035942.1.14",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210830 13:13"
        },
        {
          "id": "MDRW2100035942.1.15",
          "form": "저도 그래용",
          "original_form": "저도 그래용",
          "speaker_id": "1",
          "time": "20210830 13:13"
        },
        {
          "id": "MDRW2100035942.1.16",
          "form": "감튀에 마요네즈랑 후추 먹어도 맛잇더라구요",
          "original_form": "감튀에 마요네즈랑 후추 먹어도 맛잇더라구요",
          "speaker_id": "2",
          "time": "20210830 13:14"
        },
        {
          "id": "MDRW2100035942.1.17",
          "form": "후추요???",
          "original_form": "후추요???",
          "speaker_id": "1",
          "time": "20210830 13:14"
        },
        {
          "id": "MDRW2100035942.1.18",
          "form": "롯데리아 클래식치즈버거 맛있는데 양이 적어요",
          "original_form": "롯데리아 클래식치즈버거 맛있는데 양이 적어요",
          "speaker_id": "2",
          "time": "20210830 13:14"
        },
        {
          "id": "MDRW2100035942.1.19",
          "form": "후추는 처음 들어봤는데 그렇게 먹으면 맛있나용??",
          "original_form": "후추는 처음 들어봤는데 그렇게 먹으면 맛있나용??",
          "speaker_id": "1",
          "time": "20210830 13:14"
        },
        {
          "id": "MDRW2100035942.1.20",
          "form": "마요네즈!",
          "original_form": "마요네즈!",
          "speaker_id": "1",
          "time": "20210830 13:14"
        },
        {
          "id": "MDRW2100035942.1.21",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210830 13:14"
        },
        {
          "id": "MDRW2100035942.1.22",
          "form": "저는 롯데리아 아재버거라고 그거 좋아했는데 단종됐더라구요...ㅠ",
          "original_form": "저는 롯데리아 아재버거라고 그거 좋아했는데 단종됐더라구요...ㅠ",
          "speaker_id": "1",
          "time": "20210830 13:14"
        },
        {
          "id": "MDRW2100035942.1.23",
          "form": "넼ㅋㅋㅋ 어느 버거 체인ㅈㅓㅁ에서 후추랑 마요네즈랑 해서 팔더라구요",
          "original_form": "넼ㅋㅋㅋ 어느 버거 체인ㅈㅓㅁ에서 후추랑 마요네즈랑 해서 팔더라구요",
          "speaker_id": "2",
          "time": "20210830 13:16"
        }
      ]
    }
