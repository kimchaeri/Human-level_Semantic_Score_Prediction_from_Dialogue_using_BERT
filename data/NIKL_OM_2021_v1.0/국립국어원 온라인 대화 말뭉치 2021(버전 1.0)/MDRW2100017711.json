      "utterance": [
        {
          "id": "MDRW2100017711.1.1",
          "form": "우옹",
          "original_form": "우옹",
          "speaker_id": "1",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017711.1.2",
          "form": "이렇게 하면 돼!",
          "original_form": "이렇게 하면 돼!",
          "speaker_id": "2",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017711.1.3",
          "form": "여행 가고 싶다!",
          "original_form": "여행 가고 싶다!",
          "speaker_id": "2",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017711.1.4",
          "form": "해외로~",
          "original_form": "해외로~",
          "speaker_id": "2",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017711.1.5",
          "form": "나도!",
          "original_form": "나도!",
          "speaker_id": "1",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017711.1.6",
          "form": "시베리아 열차타고",
          "original_form": "시베리아 열차타고",
          "speaker_id": "1",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017711.1.7",
          "form": "유럽 !",
          "original_form": "유럽 !",
          "speaker_id": "1",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017711.1.8",
          "form": "오 대박",
          "original_form": "오 대박",
          "speaker_id": "2",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017711.1.9",
          "form": "좋다",
          "original_form": "좋다",
          "speaker_id": "2",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017711.1.10",
          "form": "꼬리칸 타서",
          "original_form": "꼬리칸 타서",
          "speaker_id": "1",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017711.1.11",
          "form": "외국인 만나서 친햐지는게",
          "original_form": "외국인 만나서 친햐지는게",
          "speaker_id": "1",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017711.1.12",
          "form": "내.목표",
          "original_form": "내.목표",
          "speaker_id": "1",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017711.1.13",
          "form": "두둥탁",
          "original_form": "두둥탁",
          "speaker_id": "1",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017711.1.14",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017711.1.15",
          "form": "나도 한 번 타보고 싶어",
          "original_form": "나도 한 번 타보고 싶어",
          "speaker_id": "2",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017711.1.16",
          "form": "거기서 컵라면 먹을래",
          "original_form": "거기서 컵라면 먹을래",
          "speaker_id": "2",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017711.1.17",
          "form": "같이 타자",
          "original_form": "같이 타자",
          "speaker_id": "1",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017711.1.18",
          "form": "재밌곗다",
          "original_form": "재밌곗다",
          "speaker_id": "1",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017711.1.19",
          "form": "제법 소박한 꿈",
          "original_form": "제법 소박한 꿈",
          "speaker_id": "2",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017711.1.20",
          "form": "앜ㅋㅋㅋㅋ 졸다",
          "original_form": "앜ㅋㅋㅋㅋ 졸다",
          "speaker_id": "1",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017711.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017711.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017711.1.23",
          "form": "매우 소박하다",
          "original_form": "매우 소박하다",
          "speaker_id": "1",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017711.1.24",
          "form": "나 벨기에 가서",
          "original_form": "나 벨기에 가서",
          "speaker_id": "1",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017711.1.25",
          "form": "초콜릿 먹고싶어",
          "original_form": "초콜릿 먹고싶어",
          "speaker_id": "1",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017711.1.26",
          "form": "사탕 같은 거 챙겨가서 나눠주고 외국 친구도 만들구",
          "original_form": "사탕 같은 거 챙겨가서 나눠주고 외국 친구도 만들구",
          "speaker_id": "2",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017711.1.27",
          "form": "마자마자",
          "original_form": "마자마자",
          "speaker_id": "1",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100017711.1.28",
          "form": "마이쮸",
          "original_form": "마이쮸",
          "speaker_id": "1",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100017711.1.29",
          "form": "대박 벨기에 초콜릿",
          "original_form": "대박 벨기에 초콜릿",
          "speaker_id": "2",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100017711.1.30",
          "form": "초코파이",
          "original_form": "초코파이",
          "speaker_id": "1",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100017711.1.31",
          "form": "들고 가자",
          "original_form": "들고 가자",
          "speaker_id": "1",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100017711.1.32",
          "form": "이름만 들어도 고급져",
          "original_form": "이름만 들어도 고급져",
          "speaker_id": "2",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100017711.1.33",
          "form": "왕창 들고가자!",
          "original_form": "왕창 들고가자!",
          "speaker_id": "1",
          "time": "20210826 20:49"
        },
        {
          "id": "MDRW2100017711.1.34",
