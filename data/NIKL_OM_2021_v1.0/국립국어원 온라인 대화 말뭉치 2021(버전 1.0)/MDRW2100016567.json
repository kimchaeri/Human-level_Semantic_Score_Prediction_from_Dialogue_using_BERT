      "utterance": [
        {
          "id": "MDRW2100016567.1.1",
          "form": "싫어하시는 음식 있으세요?",
          "original_form": "싫어하시는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210826 13:50"
        },
        {
          "id": "MDRW2100016567.1.2",
          "form": "저 초딩입맛에 편식 진짜 심해요ㅋㅋㅋㅋㅋ",
          "original_form": "저 초딩입맛에 편식 진짜 심해요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:51"
        },
        {
          "id": "MDRW2100016567.1.3",
          "form": "저도 초딩입맛인데 ㅋㅋㅋ",
          "original_form": "저도 초딩입맛인데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:51"
        },
        {
          "id": "MDRW2100016567.1.4",
          "form": "해산물이랑 오이 가지 싫어해요",
          "original_form": "해산물이랑 오이 가지 싫어해요",
          "speaker_id": "2",
          "time": "20210826 13:51"
        },
        {
          "id": "MDRW2100016567.1.5",
          "form": "아 전 해산물은 좋아하는데 가지 극혐해요 펭귄 시체같아요",
          "original_form": "아 전 해산물은 좋아하는데 가지 극혐해요 펭귄 시체같아요",
          "speaker_id": "1",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100016567.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ펭귄시체ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ펭귄시체ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100016567.1.7",
          "form": "ㅠㅠ 식감도 너무 싫어요",
          "original_form": "ㅠㅠ 식감도 너무 싫어요",
          "speaker_id": "1",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100016567.1.8",
          "form": "물컹물컹한 식감 자체가 너무 별로애요",
          "original_form": "물컹물컹한 식감 자체가 너무 별로애요",
          "speaker_id": "2",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100016567.1.9",
          "form": "에요",
          "original_form": "에요",
          "speaker_id": "2",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100016567.1.10",
          "form": "맞ㅇ요..",
          "original_form": "맞ㅇ요..",
          "speaker_id": "2",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100016567.1.11",
          "form": "맞아요 소름돋아요",
          "original_form": "맞아요 소름돋아요",
          "speaker_id": "1",
          "time": "20210826 13:52"
        },
        {
          "id": "MDRW2100016567.1.12",
          "form": "저 그래서 고기도 비계는 별로 안좋아해요",
          "original_form": "저 그래서 고기도 비계는 별로 안좋아해요",
          "speaker_id": "2",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100016567.1.13",
          "form": "헉 저도요 삼겹살도 잘 안 먹어요",
          "original_form": "헉 저도요 삼겹살도 잘 안 먹어요",
          "speaker_id": "1",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100016567.1.14",
          "form": "저두요ㅋㅋㅋㅋ살코기부분 좋아해서 퍽퍽살 완전 잘먹어요",
          "original_form": "저두요ㅋㅋㅋㅋ살코기부분 좋아해서 퍽퍽살 완전 잘먹어요",
          "speaker_id": "2",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016567.1.15",
          "form": "닭도 퍽살 좋아하세요??",
          "original_form": "닭도 퍽살 좋아하세요??",
          "speaker_id": "1",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100016567.1.16",
          "form": "네 닭가슴살 좋아해요ㅋㅋㅋ",
          "original_form": "네 닭가슴살 좋아해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100016567.1.17",
          "form": "치킨도 무조건 순살로..",
          "original_form": "치킨도 무조건 순살로..",
          "speaker_id": "2",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100016567.1.18",
          "form": "아 전 치킨은 다리살 아니면 안 먹어요 ㅋㅋㅋ",
          "original_form": "아 전 치킨은 다리살 아니면 안 먹어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100016567.1.19",
          "form": "저도 순살파인데 다리살로 주는 데만 먹어요",
          "original_form": "저도 순살파인데 다리살로 주는 데만 먹어요",
          "speaker_id": "1",
          "time": "20210826 13:57"
        }
      ]
