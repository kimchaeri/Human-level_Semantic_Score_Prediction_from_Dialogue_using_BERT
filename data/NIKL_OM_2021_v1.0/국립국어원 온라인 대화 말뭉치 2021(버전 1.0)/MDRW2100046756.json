      "utterance": [
        {
          "id": "MDRW2100046756.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211104 16:06"
        },
        {
          "id": "MDRW2100046756.1.2",
          "form": "안녕하세요~ 오늘 저녁에 약속이나 계획있으신가요?",
          "original_form": "안녕하세요~ 오늘 저녁에 약속이나 계획있으신가요?",
          "speaker_id": "2",
          "time": "20211104 16:06"
        },
        {
          "id": "MDRW2100046756.1.3",
          "form": "친구랑 약속 있어요",
          "original_form": "친구랑 약속 있어요",
          "speaker_id": "1",
          "time": "20211104 16:09"
        },
        {
          "id": "MDRW2100046756.1.4",
          "form": "친구랑 같이 저녁 먹기로 하셨나요?",
          "original_form": "친구랑 같이 저녁 먹기로 하셨나요?",
          "speaker_id": "2",
          "time": "20211104 16:10"
        },
        {
          "id": "MDRW2100046756.1.5",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20211104 16:10"
        },
        {
          "id": "MDRW2100046756.1.6",
          "form": "고기 먹으러 가요 ㅋ",
          "original_form": "고기 먹으러 가요 ㅋ",
          "speaker_id": "1",
          "time": "20211104 16:11"
        },
        {
          "id": "MDRW2100046756.1.7",
          "form": "오 삼겹살인가요?",
          "original_form": "오 삼겹살인가요?",
          "speaker_id": "2",
          "time": "20211104 16:11"
        },
        {
          "id": "MDRW2100046756.1.8",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20211104 16:11"
        },
        {
          "id": "MDRW2100046756.1.9",
          "form": "ㅋㅋ맛있겠네요!",
          "original_form": "ㅋㅋ맛있겠네요!",
          "speaker_id": "2",
          "time": "20211104 16:11"
        },
        {
          "id": "MDRW2100046756.1.10",
          "form": "오랜만에 고기를 ㅋ",
          "original_form": "오랜만에 고기를 ㅋ",
          "speaker_id": "1",
          "time": "20211104 16:11"
        },
        {
          "id": "MDRW2100046756.1.11",
          "form": "네 삼겹살에 쏘주 한잔",
          "original_form": "네 삼겹살에 쏘주 한잔",
          "speaker_id": "1",
          "time": "20211104 16:11"
        },
        {
          "id": "MDRW2100046756.1.12",
          "form": "삼쏘는 인정이죠 ㅎㅎ",
          "original_form": "삼쏘는 인정이죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 16:11"
        },
        {
          "id": "MDRW2100046756.1.13",
          "form": "맞아요 ㅋ",
          "original_form": "맞아요 ㅋ",
          "speaker_id": "1",
          "time": "20211104 16:11"
        },
        {
          "id": "MDRW2100046756.1.14",
          "form": "가장 좋아하는 주종이 소주이신가요?",
          "original_form": "가장 좋아하는 주종이 소주이신가요?",
          "speaker_id": "2",
          "time": "20211104 16:11"
        },
        {
          "id": "MDRW2100046756.1.15",
          "form": "근데 시간이 안가요 ㅋㅋ",
          "original_form": "근데 시간이 안가요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211104 16:12"
        },
        {
          "id": "MDRW2100046756.1.16",
          "form": "네 소주요",
          "original_form": "네 소주요",
          "speaker_id": "1",
          "time": "20211104 16:12"
        },
        {
          "id": "MDRW2100046756.1.17",
          "form": "저도 너무 시간이 안가네요 ㅋㅋ",
          "original_form": "저도 너무 시간이 안가네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 16:12"
        },
        {
          "id": "MDRW2100046756.1.18",
          "form": "저도 소주 제일 좋아합니다 ㅎㅎ",
          "original_form": "저도 소주 제일 좋아합니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 16:12"
        },
        {
          "id": "MDRW2100046756.1.19",
          "form": "오 ㅋ",
          "original_form": "오 ㅋ",
          "speaker_id": "1",
          "time": "20211104 16:12"
        },
        {
          "id": "MDRW2100046756.1.20",
          "form": "이 시간만 되면 시간이 안간다는",
          "original_form": "이 시간만 되면 시간이 안간다는",
          "speaker_id": "1",
          "time": "20211104 16:12"
        },
        {
          "id": "MDRW2100046756.1.21",
          "form": "마의 4시 구간이죠 ㅎㅎ 조금만 참으면 곧 퇴근인데 말이에요,,",
          "original_form": "마의 4시 구간이죠 ㅎㅎ 조금만 참으면 곧 퇴근인데 말이에요,,",
          "speaker_id": "2",
          "time": "20211104 16:12"
        },
        {
          "id": "MDRW2100046756.1.22",
          "form": "그러니깐요",
          "original_form": "그러니깐요",
          "speaker_id": "1",
          "time": "20211104 16:13"
        },
        {
          "id": "MDRW2100046756.1.23",
          "form": "오늘 하루도 수고 많으셨어요! 오늘 친구분과 맛있는 저녁드세요~!",
          "original_form": "오늘 하루도 수고 많으셨어요! 오늘 친구분과 맛있는 저녁드세요~!",
