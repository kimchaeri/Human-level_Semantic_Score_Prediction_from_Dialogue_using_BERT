        },
        {
          "id": "MDRW2100015152.1.2",
          "form": "안녕하세요~~",
          "original_form": "안녕하세요~~",
          "speaker_id": "2",
          "time": "20210826 03:38"
        },
        {
          "id": "MDRW2100015152.1.3",
          "form": "요즘 집에서 무슨 음식 많이 드세요m",
          "original_form": "요즘 집에서 무슨 음식 많이 드세요m",
          "speaker_id": "2",
          "time": "20210826 03:38"
        },
        {
          "id": "MDRW2100015152.1.4",
          "form": "어제는 쫄면 먹었어요",
          "original_form": "어제는 쫄면 먹었어요",
          "speaker_id": "1",
          "time": "20210826 07:26"
        },
        {
          "id": "MDRW2100015152.1.5",
          "form": "name2 님은요?",
          "original_form": "&name2& 님은요?",
          "speaker_id": "1",
          "time": "20210826 07:26"
        },
        {
          "id": "MDRW2100015152.1.6",
          "form": "저는 김치찌개 많이 먹어요 ㅋㅋㅋ",
          "original_form": "저는 김치찌개 많이 먹어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 09:05"
        },
        {
          "id": "MDRW2100015152.1.7",
          "form": "쫄면 맛있겠어요",
          "original_form": "쫄면 맛있겠어요",
          "speaker_id": "2",
          "time": "20210826 09:05"
        },
        {
          "id": "MDRW2100015152.1.8",
          "form": "저는 왠지 쫄면하면 족발이 생각나요",
          "original_form": "저는 왠지 쫄면하면 족발이 생각나요",
          "speaker_id": "2",
          "time": "20210826 09:05"
        },
        {
          "id": "MDRW2100015152.1.9",
          "form": "오 족발엔 막국수 아닌가요?",
          "original_form": "오 족발엔 막국수 아닌가요?",
          "speaker_id": "1",
          "time": "20210826 09:09"
        },
        {
          "id": "MDRW2100015152.1.10",
          "form": "저도 김치찌개 좋아해요",
          "original_form": "저도 김치찌개 좋아해요",
          "speaker_id": "1",
          "time": "20210826 09:09"
        },
        {
          "id": "MDRW2100015152.1.11",
          "form": "맞아요 족발엔 막국수 ㅎㅎ 근데 쫄면도 잘어울려서 그런가봐요!",
          "original_form": "맞아요 족발엔 막국수 ㅎㅎ 근데 쫄면도 잘어울려서 그런가봐요!",
          "speaker_id": "2",
          "time": "20210826 09:09"
        },
        {
          "id": "MDRW2100015152.1.12",
          "form": "김치찌개는 보통 돼지고기랑 참치중에 하나 넣잖아요",
          "original_form": "김치찌개는 보통 돼지고기랑 참치중에 하나 넣잖아요",
          "speaker_id": "2",
          "time": "20210826 09:10"
        },
        {
          "id": "MDRW2100015152.1.13",
          "form": "둘중에 어떤 것을 더 좋아하세요?",
          "original_form": "둘중에 어떤 것을 더 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 09:10"
        },
        {
          "id": "MDRW2100015152.1.14",
          "form": "전 참치요",
          "original_form": "전 참치요",
          "speaker_id": "1",
          "time": "20210826 09:12"
        },
        {
          "id": "MDRW2100015152.1.15",
          "form": "더 잘 어울리는 거 같아요",
          "original_form": "더 잘 어울리는 거 같아요",
          "speaker_id": "1",
          "time": "20210826 09:12"
        },
        {
          "id": "MDRW2100015152.1.16",
          "form": "오오 저도요!",
          "original_form": "오오 저도요!",
          "speaker_id": "2",
          "time": "20210826 09:12"
        },
        {
          "id": "MDRW2100015152.1.17",
          "form": "돼지고기도 맛있긴한데 더 구수한맛이 나는건 참치김치찌개같아요 ㅋㅋㅋ",
          "original_form": "돼지고기도 맛있긴한데 더 구수한맛이 나는건 참치김치찌개같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 09:12"
        },
        {
          "id": "MDRW2100015152.1.18",
          "form": "오 그쵸",
          "original_form": "오 그쵸",
          "speaker_id": "1",
          "time": "20210826 09:38"
        },
        {
          "id": "MDRW2100015152.1.19",
          "form": "먹고 싶어지네요",
          "original_form": "먹고 싶어지네요",
          "speaker_id": "1",
          "time": "20210826 09:38"
        },
        {
          "id": "MDRW2100015152.1.20",
          "form": "ㅋㅋㅋㅋ 요즘 파리바게트 세일하던데!!",
          "original_form": "ㅋㅋㅋㅋ 요즘 파리바게트 세일하던데!!",
          "speaker_id": "2",
          "time": "20210826 14:02"
        },
        {
          "id": "MDRW2100015152.1.21",
          "form": "파리바게트 드셨나요",
          "original_form": "파리바게트 드셨나요",
          "speaker_id": "2",
          "time": "20210826 14:02"
        },
        {
          "id": "MDRW2100015152.1.22",
          "form": "저는 오늘 사왔어요",
          "original_form": "저는 오늘 사왔어요",
          "speaker_id": "2",
          "time": "20210826 14:02"
        },
        {
          "id": "MDRW2100015152.1.23",
          "form": "아 세일 소식은 못 들었어요",
          "original_form": "아 세일 소식은 못 들었어요",
          "speaker_id": "1",
          "time": "20210826 14:22"
        },
        {
          "id": "MDRW2100015152.1.24",
          "form": "저희 동네 파바는 맛있는 빵이 너무 없어요",
          "original_form": "저희 동네 파바는 맛있는 빵이 너무 없어요",
          "speaker_id": "1",
          "time": "20210826 14:22"
        },
        {
          "id": "MDRW2100015152.1.25",
          "form": "앗 정말요? ㅠㅠ 너무 슬프네요",
          "original_form": "앗 정말요? ㅠㅠ 너무 슬프네요",
          "speaker_id": "2",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100015152.1.26",
          "form": "파바에서 어떤 빵이 맛있나요? 저는 평소에 프랜차이즈 빵집을 잘 안가서 그냥 아는 맛만 고르게 되더라고여",
          "original_form": "파바에서 어떤 빵이 맛있나요? 저는 평소에 프랜차이즈 빵집을 잘 안가서 그냥 아는 맛만 고르게 되더라고여",
          "speaker_id": "2",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100015152.1.27",
          "form": "전 모카크림빵이랑 소보로크림빵 제일 좋아해요",
          "original_form": "전 모카크림빵이랑 소보로크림빵 제일 좋아해요",
          "speaker_id": "1",
          "time": "20210826 23:02"
        },
        {
          "id": "MDRW2100015152.1.28",
          "form": "모카크림빵은 특히 너무 맛있어서 과식하게 돼요ㅋㅋㅋ",
          "original_form": "모카크림빵은 특히 너무 맛있어서 과식하게 돼요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100015152.1.29",
          "form": "모카크림빵 맛있죠!!!",
          "original_form": "모카크림빵 맛있죠!!!",
          "speaker_id": "2",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100015152.1.30",
          "form": "먹잘알이시네요 ㅋㅋㅋ",
          "original_form": "먹잘알이시네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100015152.1.31",
          "form": "달기만 하면 질릴수도 있는데 모카향때문에 하염없이 먹게돼요",
          "original_form": "달기만 하면 질릴수도 있는데 모카향때문에 하염없이 먹게돼요",
          "speaker_id": "2",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100015152.1.32",
          "form": "맞아요 달달한데 짭짤한 맛도 있잖아요",
          "original_form": "맞아요 달달한데 짭짤한 맛도 있잖아요",
          "speaker_id": "1",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100015152.1.33",
          "form": "스트레스 좀 받으면 그거 땡기더라고요",
          "original_form": "스트레스 좀 받으면 그거 땡기더라고요",
          "speaker_id": "1",
          "time": "20210826 23:24"
        }
      ]
