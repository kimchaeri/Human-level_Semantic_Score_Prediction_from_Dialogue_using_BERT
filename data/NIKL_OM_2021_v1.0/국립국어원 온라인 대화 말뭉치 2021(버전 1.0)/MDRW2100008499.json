      "utterance": [
        {
          "id": "MDRW2100008499.1.1",
          "form": "너네학교도대청교하냐?",
          "original_form": "너네학교도대청교하냐?",
          "speaker_id": "1",
          "time": "20210822 16:12"
        },
        {
          "id": "MDRW2100008499.1.2",
          "form": "아동센터가서근로하는거",
          "original_form": "아동센터가서근로하는거",
          "speaker_id": "1",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008499.1.3",
          "form": "너네학교도대청교함?",
          "original_form": "너네학교도대청교함?",
          "speaker_id": "1",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008499.1.4",
          "form": "근로하는거",
          "original_form": "근로하는거",
          "speaker_id": "1",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008499.1.5",
          "form": "그게 뭐야",
          "original_form": "그게 뭐야",
          "speaker_id": "2",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008499.1.6",
          "form": "대청교 처음 들었어",
          "original_form": "대청교 처음 들었어",
          "speaker_id": "2",
          "time": "20210822 16:14"
        },
        {
          "id": "MDRW2100008499.1.7",
          "form": "내가 관심 없어서 그런 거일 수도 있음",
          "original_form": "내가 관심 없어서 그런 거일 수도 있음",
          "speaker_id": "2",
          "time": "20210822 16:15"
        },
        {
          "id": "MDRW2100008499.1.8",
          "form": "근로는뽑음?",
          "original_form": "근로는뽑음?",
          "speaker_id": "1",
          "time": "20210822 16:15"
        },
        {
          "id": "MDRW2100008499.1.9",
          "form": "교내근로같은거",
          "original_form": "교내근로같은거",
          "speaker_id": "1",
          "time": "20210822 16:15"
        },
        {
          "id": "MDRW2100008499.1.10",
          "form": "근로는 뽑음",
          "original_form": "근로는 뽑음",
          "speaker_id": "2",
          "time": "20210822 16:54"
        },
        {
          "id": "MDRW2100008499.1.11",
          "form": "근로중에아동센터가서교육하는거있거든",
          "original_form": "근로중에아동센터가서교육하는거있거든",
          "speaker_id": "1",
          "time": "20210822 16:55"
        },
        {
          "id": "MDRW2100008499.1.12",
          "form": "11,150원",
          "original_form": "11,150원",
          "speaker_id": "1",
          "time": "20210822 16:55"
        },
        {
          "id": "MDRW2100008499.1.13",
          "form": "오오 시급??",
          "original_form": "오오 시급??",
          "speaker_id": "2",
          "time": "20210822 16:56"
        },
        {
          "id": "MDRW2100008499.1.14",
          "form": "오오 신기하네",
          "original_form": "오오 신기하네",
          "speaker_id": "2",
          "time": "20210822 16:56"
        },
        {
          "id": "MDRW2100008499.1.15",
          "form": "ㅇㅇㅇ근로도시급으로줄걸?",
          "original_form": "ㅇㅇㅇ근로도시급으로줄걸?",
          "speaker_id": "1",
          "time": "20210822 16:57"
        },
        {
          "id": "MDRW2100008499.1.16",
          "form": "근로해봤음?",
          "original_form": "근로해봤음?",
          "speaker_id": "1",
          "time": "20210822 16:57"
        },
        {
          "id": "MDRW2100008499.1.17",
          "form": "오오 ㄴㄴㄴ 안해봤어",
          "original_form": "오오 ㄴㄴㄴ 안해봤어",
          "speaker_id": "2",
          "time": "20210822 16:57"
        },
        {
          "id": "MDRW2100008499.1.18",
          "form": "해봤으??",
          "original_form": "해봤으??",
          "speaker_id": "2",
          "time": "20210822 16:57"
        },
        {
          "id": "MDRW2100008499.1.19",
          "form": "ㄴㄴㄴ아직안해봄",
          "original_form": "ㄴㄴㄴ아직안해봄",
          "speaker_id": "1",
          "time": "20210822 16:58"
        },
        {
          "id": "MDRW2100008499.1.20",
          "form": "근데교내근로하면꿀이긴하대",
          "original_form": "근데교내근로하면꿀이긴하대",
          "speaker_id": "1",
          "time": "20210822 16:58"
        },
        {
          "id": "MDRW2100008499.1.21",
          "form": "아 진심???",
          "original_form": "아 진심???",
          "speaker_id": "2",
          "time": "20210822 17:05"
        },
        {
          "id": "MDRW2100008499.1.22",
          "form": "그러긴 하겠네",
          "original_form": "그러긴 하겠네",
          "speaker_id": "2",
          "time": "20210822 17:06"
        },
        {
          "id": "MDRW2100008499.1.23",
          "form": "등록금 내고 거기서 월급받기...",
          "original_form": "등록금 내고 거기서 월급받기...",
          "speaker_id": "2",
          "time": "20210822 17:06"
        },
        {
          "id": "MDRW2100008499.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ아그렇네",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아그렇네",
          "speaker_id": "1",
          "time": "20210822 17:06"
        },
        {
          "id": "MDRW2100008499.1.25",
          "form": "다시돌려받는거네",
          "original_form": "다시돌려받는거네",
          "speaker_id": "1",
          "time": "20210822 17:06"
        },
        {
          "id": "MDRW2100008499.1.26",
          "form": "너나중에교내근로할거임?",
          "original_form": "너나중에교내근로할거임?",
          "speaker_id": "1",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008499.1.27",
          "form": "기회 있으면 하려고",
          "original_form": "기회 있으면 하려고",
          "speaker_id": "2",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008499.1.28",
          "form": "하면서 보조 배토리 충전해야지",
          "original_form": "하면서 보조 배토리 충전해야지",
          "speaker_id": "2",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008499.1.29",
          "form": "ㅋㅋㅋㅋㅋ내년부터최저시급도오르잖아",
          "original_form": "ㅋㅋㅋㅋㅋ내년부터최저시급도오르잖아",
          "speaker_id": "1",
          "time": "20210822 17:09"
        },
        {
          "id": "MDRW2100008499.1.30",
          "form": "최저시급오르면교내근로도",
          "original_form": "최저시급오르면교내근로도",
          "speaker_id": "1",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008499.1.31",
          "form": "오르나?",
          "original_form": "오르나?",
          "speaker_id": "1",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008499.1.32",
          "form": "내년부터 교내 근로 해야지",
          "original_form": "내년부터 교내 근로 해야지",
          "speaker_id": "2",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008499.1.33",
          "form": "오르지 않을까?",
          "original_form": "오르지 않을까?",
          "speaker_id": "2",
          "time": "20210822 17:10"
        }
      ]
    }
