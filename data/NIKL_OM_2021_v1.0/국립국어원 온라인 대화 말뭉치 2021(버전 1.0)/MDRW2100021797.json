      "utterance": [
        {
          "id": "MDRW2100021797.1.1",
          "form": "운동은 자주 하시나요?",
          "original_form": "운동은 자주 하시나요?",
          "speaker_id": "2",
          "time": "20210828 22:33"
        },
        {
          "id": "MDRW2100021797.1.2",
          "form": "아뇨 코로나이후로 살쪗어요 ㅎㅎ",
          "original_form": "아뇨 코로나이후로 살쪗어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:33"
        },
        {
          "id": "MDRW2100021797.1.3",
          "form": "ㅋㅋ 코로나 잘못이죠ㅋㅋ 그전에는 하던 운동 있으셨어요?",
          "original_form": "ㅋㅋ 코로나 잘못이죠ㅋㅋ 그전에는 하던 운동 있으셨어요?",
          "speaker_id": "2",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100021797.1.4",
          "form": "주로 걷기 많이 했는데",
          "original_form": "주로 걷기 많이 했는데",
          "speaker_id": "1",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100021797.1.5",
          "form": "지금은.더위 +마스크.핑계로 집콕 후아 ㅜㅜ",
          "original_form": "지금은.더위 +마스크.핑계로 집콕 후아 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 22:37"
        },
        {
          "id": "MDRW2100021797.1.6",
          "form": "아 저도 걸어서 퇴근하거나 주말엔 런닝했는데 요즘 못해요ㅜ",
          "original_form": "아 저도 걸어서 퇴근하거나 주말엔 런닝했는데 요즘 못해요ㅜ",
          "speaker_id": "2",
          "time": "20210828 22:37"
        },
        {
          "id": "MDRW2100021797.1.7",
          "form": "코로나가 너무 일상을 망쳐서 슬프죠",
          "original_form": "코로나가 너무 일상을 망쳐서 슬프죠",
          "speaker_id": "1",
          "time": "20210828 22:39"
        },
        {
          "id": "MDRW2100021797.1.8",
          "form": "그러니까요 피부도 저세상ㅜㅠ",
          "original_form": "그러니까요 피부도 저세상ㅜㅠ",
          "speaker_id": "2",
          "time": "20210828 22:40"
        },
        {
          "id": "MDRW2100021797.1.9",
          "form": "마스크 트러블 속상 ㅜㅜ",
          "original_form": "마스크 트러블 속상 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 22:40"
        },
        {
          "id": "MDRW2100021797.1.10",
          "form": "ㅜㅠ 생리전에도 난리나는데 코로나까지ㅜㅠ",
          "original_form": "ㅜㅠ 생리전에도 난리나는데 코로나까지ㅜㅠ",
          "speaker_id": "2",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021797.1.11",
          "form": "코로나도 힘든데 몸도 덤으로 고생 하네요",
          "original_form": "코로나도 힘든데 몸도 덤으로 고생 하네요",
          "speaker_id": "1",
          "time": "20210828 22:42"
        },
        {
          "id": "MDRW2100021797.1.12",
          "form": "그러니까요 마스크로 가리고 다니는건 편하긴하지만 너무 힘들어요",
          "original_form": "그러니까요 마스크로 가리고 다니는건 편하긴하지만 너무 힘들어요",
          "speaker_id": "2",
          "time": "20210828 22:43"
        },
        {
          "id": "MDRW2100021797.1.13",
          "form": "화장 일년에 세번정도 한거같아요",
          "original_form": "화장 일년에 세번정도 한거같아요",
          "speaker_id": "2",
          "time": "20210828 22:43"
        },
        {
          "id": "MDRW2100021797.1.14",
          "form": "저도 눈썹만 ㅋㅋㅋㅋ열심히 그리고",
          "original_form": "저도 눈썹만 ㅋㅋㅋㅋ열심히 그리고",
          "speaker_id": "1",
          "time": "20210828 22:44"
        },
        {
          "id": "MDRW2100021797.1.15",
          "form": "맞아요! 눈만 힘주고 다녀요ㅜㅜ 직전에 립 많이 샀는데ㅜ",
          "original_form": "맞아요! 눈만 힘주고 다녀요ㅜㅜ 직전에 립 많이 샀는데ㅜ",
          "speaker_id": "2",
          "time": "20210828 22:46"
        }
      ]
