      "utterance": [
        {
          "id": "MDRW2100028828.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028828.1.2",
          "form": "아니요..",
          "original_form": "아니요..",
          "speaker_id": "1",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028828.1.3",
          "form": "애가 셋잇라",
          "original_form": "애가 셋잇라",
          "speaker_id": "1",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100028828.1.4",
          "form": "저는 사실 리트리버 키워요",
          "original_form": "저는 사실 리트리버 키워요",
          "speaker_id": "2",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028828.1.5",
          "form": "대단하시네요",
          "original_form": "대단하시네요",
          "speaker_id": "1",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028828.1.6",
          "form": "엄청 커서 힘들 때도 있지만 너무 귀여워요",
          "original_form": "엄청 커서 힘들 때도 있지만 너무 귀여워요",
          "speaker_id": "2",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028828.1.7",
          "form": "엄청커서 힘드실텐데",
          "original_form": "엄청커서 힘드실텐데",
          "speaker_id": "1",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028828.1.8",
          "form": "주택사세요? 아파트사세요?",
          "original_form": "주택사세요? 아파트사세요?",
          "speaker_id": "1",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100028828.1.9",
          "form": "저는 주택에서 살아요",
          "original_form": "저는 주택에서 살아요",
          "speaker_id": "2",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028828.1.10",
          "form": "주택사시니 가능하겠네요..아파트에선 키우기 힘들것같아요",
          "original_form": "주택사시니 가능하겠네요..아파트에선 키우기 힘들것같아요",
          "speaker_id": "1",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100028828.1.11",
          "form": "맞아요. 그런데 정말정말 귀여워요",
          "original_form": "맞아요. 그런데 정말정말 귀여워요",
          "speaker_id": "2",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028828.1.12",
          "form": "사랑으로 키우시는군요",
          "original_form": "사랑으로 키우시는군요",
          "speaker_id": "1",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100028828.1.13",
          "form": "네 ㅎㅎㅎ",
          "original_form": "네 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100028828.1.14",
          "form": "그 말이 젤 맞는거 같아요",
          "original_form": "그 말이 젤 맞는거 같아요",
          "speaker_id": "2",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100028828.1.15",
          "form": "저도 나중에 키워볼께요",
          "original_form": "저도 나중에 키워볼께요",
          "speaker_id": "1",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100028828.1.16",
