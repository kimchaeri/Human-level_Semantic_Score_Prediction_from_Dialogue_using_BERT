      "utterance": [
        {
          "id": "MDRW2100022842.1.1",
          "form": "책읽어야 하는데...",
          "original_form": "책읽어야 하는데...",
          "speaker_id": "2",
          "time": "20210829 19:00"
        },
        {
          "id": "MDRW2100022842.1.2",
          "form": "나두..ㅎ",
          "original_form": "나두..ㅎ",
          "speaker_id": "1",
          "time": "20210830 15:05"
        },
        {
          "id": "MDRW2100022842.1.3",
          "form": "근데 이거 경제기사 책",
          "original_form": "근데 이거 경제기사 책",
          "speaker_id": "1",
          "time": "20210830 15:06"
        },
        {
          "id": "MDRW2100022842.1.4",
          "form": "재밌다",
          "original_form": "재밌다",
          "speaker_id": "1",
          "time": "20210830 15:06"
        },
        {
          "id": "MDRW2100022842.1.5",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "2",
          "time": "20210830 15:06"
        },
        {
          "id": "MDRW2100022842.1.6",
          "form": "다행이네",
          "original_form": "다행이네",
          "speaker_id": "2",
          "time": "20210830 15:06"
        },
        {
          "id": "MDRW2100022842.1.7",
          "form": "나 오늘 200페이지 읽을 수 있을까",
          "original_form": "나 오늘 200페이지 읽을 수 있을까",
          "speaker_id": "2",
          "time": "20210830 15:07"
        },
        {
          "id": "MDRW2100022842.1.8",
          "form": "ㅇㅇ 가능가능",
          "original_form": "ㅇㅇ 가능가능",
          "speaker_id": "1",
          "time": "20210830 15:07"
        },
        {
          "id": "MDRW2100022842.1.9",
          "form": "한 3시간정도 읽으면 되지않을까?",
          "original_form": "한 3시간정도 읽으면 되지않을까?",
          "speaker_id": "1",
          "time": "20210830 15:07"
        },
        {
          "id": "MDRW2100022842.1.10",
          "form": "저날에 200페이지 읽었어?",
          "original_form": "저날에 200페이지 읽었어?",
          "speaker_id": "1",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100022842.1.11",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100022842.1.12",
          "form": "걍 코난 봤는디...",
          "original_form": "걍 코난 봤는디...",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100022842.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100022842.1.14",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100022842.1.15",
          "form": "짱",
          "original_form": "짱{emoji:♡}",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100022842.1.16",
          "form": "나도 개강하기 전 이틀은",
          "original_form": "나도 개강하기 전 이틀은",
          "speaker_id": "1",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100022842.1.17",
          "form": "넌 경제기사 읽을만해?",
          "original_form": "넌 경제기사 읽을만해?",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100022842.1.18",
          "form": "그냥 놀았음 ㅎㅎ",
          "original_form": "그냥 놀았음 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100022842.1.19",
          "form": "넌 수업 내용 생각하니까",
          "original_form": "넌 수업 내용 생각하니까",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100022842.1.20",
          "form": "ㅇㅇㅇㅇㄹ완전재밌어",
          "original_form": "ㅇㅇㅇㅇㄹ완전재밌어",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100022842.1.21",
          "form": "더 헷갈리던데",
          "original_form": "더 헷갈리던데",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100022842.1.22",
          "form": "난",
          "original_form": "난",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100022842.1.23",
          "form": "그래??",
          "original_form": "그래??",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100022842.1.24",
          "form": "응..",
          "original_form": "응..",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100022842.1.25",
          "form": "어짜피 20권 못 읽어서",
          "original_form": "어짜피 20권 못 읽어서",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100022842.1.26",
          "form": "난 아는게많이없어서",
          "original_form": "난 아는게많이없어서",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100022842.1.27",
          "form": "학기중에 읽고",
          "original_form": "학기중에 읽고",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100022842.1.28",
          "form": "그냥 아글쿠나~ 하면서 읽는중",
          "original_form": "그냥 아글쿠나~ 하면서 읽는중",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100022842.1.29",
          "form": "지금은 메타버스나 읽어야지",
          "original_form": "지금은 메타버스나 읽어야지",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100022842.1.30",
          "form": "요즘은 뭔가",
          "original_form": "요즘은 뭔가",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100022842.1.31",
          "form": "문학보다 비문학이 잘읽혀",
          "original_form": "문학보다 비문학이 잘읽혀",
          "speaker_id": "1",
          "time": "20210901 23:21"
        }
      ]
