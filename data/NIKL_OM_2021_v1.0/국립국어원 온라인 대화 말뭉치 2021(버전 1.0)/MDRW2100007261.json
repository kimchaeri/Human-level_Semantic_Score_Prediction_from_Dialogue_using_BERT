      "utterance": [
        {
          "id": "MDRW2100007261.1.1",
          "form": "역시 학교생활얘기가",
          "original_form": "역시 학교생활얘기가",
          "speaker_id": "1",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007261.1.2",
          "form": "진도가 쫙 나가네",
          "original_form": "진도가 쫙 나가네",
          "speaker_id": "1",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007261.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007261.1.4",
          "form": "그럼 우리 조별과제",
          "original_form": "그럼 우리 조별과제",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007261.1.5",
          "form": "진상썰 풀어볼까",
          "original_form": "진상썰 풀어볼까",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007261.1.6",
          "form": "끝없는 학교이야기 주제중 하나지...",
          "original_form": "끝없는 학교이야기 주제중 하나지...",
          "speaker_id": "2",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007261.1.7",
          "form": "앜ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007261.1.8",
          "form": "이번주제는",
          "original_form": "이번주제는",
          "speaker_id": "1",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007261.1.9",
          "form": "조별과제인걸로?ㅋㅋㅋ",
          "original_form": "조별과제인걸로?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007261.1.10",
          "form": "넌 조별과제때",
          "original_form": "넌 조별과제때",
          "speaker_id": "2",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007261.1.11",
          "form": "인상깊은 진상없어?",
          "original_form": "인상깊은 진상없어?",
          "speaker_id": "2",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007261.1.12",
          "form": "나는 뭐 솔직히",
          "original_form": "나는 뭐 솔직히",
          "speaker_id": "1",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007261.1.13",
          "form": "그렇게까지 진상은 만난적 없고",
          "original_form": "그렇게까지 진상은 만난적 없고",
          "speaker_id": "1",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007261.1.14",
          "form": "걍 자료조사 시켰더니 나무위키 긁어오는?",
          "original_form": "걍 자료조사 시켰더니 나무위키 긁어오는?",
          "speaker_id": "1",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007261.1.15",
          "form": "평범한 빌런들만 만났던거같애 ㅋㅋㅋ",
          "original_form": "평범한 빌런들만 만났던거같애 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007261.1.16",
          "form": "앗...",
          "original_form": "앗...",
          "speaker_id": "2",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007261.1.17",
          "form": "평범치않아",
          "original_form": "평범치않아",
          "speaker_id": "2",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007261.1.18",
          "form": "근데 나 그거 뭔지 알아",
          "original_form": "근데 나 그거 뭔지 알아",
          "speaker_id": "2",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007261.1.19",
          "form": "나 그래서 ㅋㅋㅋㅋㅋ조별과제 할때",
          "original_form": "나 그래서 ㅋㅋㅋㅋㅋ조별과제 할때",
          "speaker_id": "2",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007261.1.20",
          "form": "전공 조별과제는 하다보면 누가 빌런인지",
          "original_form": "전공 조별과제는 하다보면 누가 빌런인지",
          "speaker_id": "2",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007261.1.21",
          "form": "딱 알잖아 안해봐도 소문으로 ㅋㅋㅋㅋ",
          "original_form": "딱 알잖아 안해봐도 소문으로 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007261.1.22",
          "form": "근데 자료 조사는 무조건!!! 무조건!!!! riss에서 가져오세요",
          "original_form": "근데 자료 조사는 무조건!!! 무조건!!!! riss에서 가져오세요",
          "speaker_id": "2",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007261.1.23",
          "form": "하고 강조했거든",
          "original_form": "하고 강조했거든",
          "speaker_id": "2",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007261.1.24",
          "form": "앜ㅋㅋㅋㅋㅋㅋ 평범치않은거야?",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ 평범치않은거야?",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007261.1.25",
          "form": "근데 riss자체를 모르더라",
          "original_form": "근데 riss자체를 모르더라",
          "speaker_id": "2",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007261.1.26",
          "form": "난 주로 빌런이라고 하면",
          "original_form": "난 주로 빌런이라고 하면",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007261.1.27",
          "form": "교양에서...?",
          "original_form": "교양에서...?",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007261.1.28",
          "form": "전공은 서로 아니까",
          "original_form": "전공은 서로 아니까",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007261.1.29",
          "form": "자긴 항상 해피캠퍼스 결제했대",
          "original_form": "자긴 항상 해피캠퍼스 결제했대",
          "speaker_id": "2",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007261.1.30",
          "form": "그렇게까지 빌런짓은 못한거 아닌가 싶어...",
          "original_form": "그렇게까지 빌런짓은 못한거 아닌가 싶어...",
          "speaker_id": "1",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007261.1.31",
          "form": "ㅇㅎㅇㅎ 교양...",
          "original_form": "ㅇㅎㅇㅎ 교양...",
          "speaker_id": "2",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007261.1.32",
          "form": "ㅇ0ㅇ... 동기??",
          "original_form": "ㅇ0ㅇ... 동기??",
          "speaker_id": "1",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007261.1.33",
          "form": "선배 ㅎ",
          "original_form": "선배 ㅎ",
          "speaker_id": "2",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007261.1.34",
          "form": "아니 riss를 모르다니 ㄷㄷㄷㄷ",
          "original_form": "아니 riss를 모르다니 ㄷㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007261.1.35",
          "form": "헐....",
          "original_form": "헐....",
