      "utterance": [
        {
          "id": "MDRW2100034901.1.1",
          "form": "안녕하세용 기초 어디꺼 쓰세용?",
          "original_form": "안녕하세용 기초 어디꺼 쓰세용?",
          "speaker_id": "1",
          "time": "20210828 21:47"
        },
        {
          "id": "MDRW2100034901.1.2",
          "form": "저는 일리윤인가? 거기꺼 써요",
          "original_form": "저는 일리윤인가? 거기꺼 써요",
          "speaker_id": "2",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100034901.1.3",
          "form": "오 일리윤이 기초도 만들어요? 바디류만 만드는줄알았는데",
          "original_form": "오 일리윤이 기초도 만들어요? 바디류만 만드는줄알았는데",
          "speaker_id": "1",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100034901.1.4",
          "form": "저도 한번 써봐야겠어용",
          "original_form": "저도 한번 써봐야겠어용",
          "speaker_id": "1",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100034901.1.5",
          "form": "전 바디로션을 그냥 얼굴에도 발라요ㅎ 잘맞더라고요",
          "original_form": "전 바디로션을 그냥 얼굴에도 발라요ㅎ 잘맞더라고요",
          "speaker_id": "2",
          "time": "20210828 22:30"
        },
        {
          "id": "MDRW2100034901.1.6",
          "form": "스킨은 스킨푸드꺼 쓰고요",
          "original_form": "스킨은 스킨푸드꺼 쓰고요",
          "speaker_id": "2",
          "time": "20210828 22:30"
        },
        {
          "id": "MDRW2100034901.1.7",
          "form": "아아 그 집중크림이라고 좀 꾸덕한 크림있는데",
          "original_form": "아아 그 집중크림이라고 좀 꾸덕한 크림있는데",
          "speaker_id": "1",
          "time": "20210828 22:38"
        },
        {
          "id": "MDRW2100034901.1.8",
          "form": "그거 팔자주름에 잘발랐어요! 겨울에ㅋㅋㅋㅋ",
          "original_form": "그거 팔자주름에 잘발랐어요! 겨울에ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:38"
        },
        {
          "id": "MDRW2100034901.1.9",
          "form": "저는 스킨 이니스프리꺼 써용!",
          "original_form": "저는 스킨 이니스프리꺼 써용!",
          "speaker_id": "1",
          "time": "20210828 22:39"
        },
        {
          "id": "MDRW2100034901.1.10",
          "form": "저도 기존에 이니스프리 스킨 가장 많이 썼어요 올리브인가 그걸로요ㅎ",
          "original_form": "저도 기존에 이니스프리 스킨 가장 많이 썼어요 올리브인가 그걸로요ㅎ",
          "speaker_id": "2",
          "time": "20210828 23:02"
        },
        {
          "id": "MDRW2100034901.1.11",
          "form": "이니스프리가 기초 화장품 좋은게 많은것같아용ㅋㅋㅋㅋ",
          "original_form": "이니스프리가 기초 화장품 좋은게 많은것같아용ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:03"
        },
        {
          "id": "MDRW2100034901.1.12",
          "form": "저번에 아이크림 사서 썼는데",
          "original_form": "저번에 아이크림 사서 썼는데",
          "speaker_id": "1",
          "time": "20210828 23:03"
        },
        {
          "id": "MDRW2100034901.1.13",
          "form": "눈가 주름이 옅어진것같더라구요",
          "original_form": "눈가 주름이 옅어진것같더라구요",
          "speaker_id": "1",
          "time": "20210828 23:03"
        },
        {
          "id": "MDRW2100034901.1.14",
          "form": "이니스프리 어느라인 좋아하시나요?",
          "original_form": "이니스프리 어느라인 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100034901.1.15",
          "form": "저 블루베리 라인 좋아해용!",
          "original_form": "저 블루베리 라인 좋아해용!",
          "speaker_id": "1",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100034901.1.16",
          "form": "이 라인이 순한것같더라구용",
          "original_form": "이 라인이 순한것같더라구용",
          "speaker_id": "1",
          "time": "20210828 23:39"
        },
        {
          "id": "MDRW2100034901.1.17",
          "form": "블루베리는 어떤 피부타입에 맞나요? 건성? 지성?",
          "original_form": "블루베리는 어떤 피부타입에 맞나요? 건성? 지성?",
          "speaker_id": "2",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100034901.1.18",
          "form": "저는 복합성인데",
          "original_form": "저는 복합성인데",
          "speaker_id": "1",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100034901.1.19",
          "form": "저한테 잘맞아용!",
          "original_form": "저한테 잘맞아용!",
          "speaker_id": "1",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100034901.1.20",
          "form": "이거 폼클렌징도 있는데",
          "original_form": "이거 폼클렌징도 있는데",
          "speaker_id": "1",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100034901.1.21",
          "form": "추천드려요",
          "original_form": "추천드려요",
          "speaker_id": "1",
          "time": "20210828 23:52"
        },
        {
          "id": "MDRW2100034901.1.22",
          "form": "저도 복합성인데 담에 한번 써봐야겠네요",
          "original_form": "저도 복합성인데 담에 한번 써봐야겠네요",
          "speaker_id": "2",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100034901.1.23",
          "form": "복합성이 까다롭죠ㅠ",
          "original_form": "복합성이 까다롭죠ㅠ",
          "speaker_id": "2",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100034901.1.24",
          "form": "네네ㅠㅠ 요즘 환절기가 피부도 푸석푸석하고",
          "original_form": "네네ㅠㅠ 요즘 환절기가 피부도 푸석푸석하고",
          "speaker_id": "1",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100034901.1.25",
          "form": "관리하기 어렵네요",
          "original_form": "관리하기 어렵네요",
          "speaker_id": "1",
          "time": "20210829 00:21"
        }
      ]
    }
