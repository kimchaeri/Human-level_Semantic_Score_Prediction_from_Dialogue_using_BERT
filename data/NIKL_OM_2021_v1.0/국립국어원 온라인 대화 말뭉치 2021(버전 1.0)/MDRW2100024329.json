      "utterance": [
        {
          "id": "MDRW2100024329.1.1",
          "form": "저 점심 진짜 기다리는 사람이거든요?ㅋㅋㅋㅋ",
          "original_form": "저 점심 진짜 기다리는 사람이거든요?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:56"
        },
        {
          "id": "MDRW2100024329.1.2",
          "form": "오늘 점심을 못먹었어요ㅠㅠㅠ",
          "original_form": "오늘 점심을 못먹었어요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 00:56"
        },
        {
          "id": "MDRW2100024329.1.3",
          "form": "앗 정말요",
          "original_form": "앗 정말요",
          "speaker_id": "2",
          "time": "20210831 00:56"
        },
        {
          "id": "MDRW2100024329.1.4",
          "form": "심지어 제육볶음이였는데...",
          "original_form": "심지어 제육볶음이였는데...",
          "speaker_id": "1",
          "time": "20210831 00:56"
        },
        {
          "id": "MDRW2100024329.1.5",
          "form": "어떻게 그런일이",
          "original_form": "어떻게 그런일이",
          "speaker_id": "2",
          "time": "20210831 00:56"
        },
        {
          "id": "MDRW2100024329.1.6",
          "form": "너무 속상하더라구요...",
          "original_form": "너무 속상하더라구요...",
          "speaker_id": "1",
          "time": "20210831 00:56"
        },
        {
          "id": "MDRW2100024329.1.7",
          "form": "제육볶음을못먹다니",
          "original_form": "제육볶음을못먹다니",
          "speaker_id": "2",
          "time": "20210831 00:56"
        },
        {
          "id": "MDRW2100024329.1.8",
          "form": "왜요??",
          "original_form": "왜요??",
          "speaker_id": "2",
          "time": "20210831 00:57"
        },
        {
          "id": "MDRW2100024329.1.9",
          "form": "같이 식사하는 분이 점심에 일이 있어서ㅋㅋ기다리다보니깐",
          "original_form": "같이 식사하는 분이 점심에 일이 있어서ㅋㅋ기다리다보니깐",
          "speaker_id": "1",
          "time": "20210831 00:57"
        },
        {
          "id": "MDRW2100024329.1.10",
          "form": "점심시갓이 지났어요ㅠㅠ",
          "original_form": "점심시갓이 지났어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 00:57"
        },
        {
          "id": "MDRW2100024329.1.11",
          "form": "먼저먹어버리지그랬어요",
          "original_form": "먼저먹어버리지그랬어요",
          "speaker_id": "2",
          "time": "20210831 00:57"
        },
        {
          "id": "MDRW2100024329.1.12",
          "form": "다행이도 아침에 호두과자를 5개먹어서.ㅠ급한허기는 껐어요",
          "original_form": "다행이도 아침에 호두과자를 5개먹어서.ㅠ급한허기는 껐어요",
          "speaker_id": "1",
          "time": "20210831 00:57"
        },
        {
          "id": "MDRW2100024329.1.13",
          "form": "완전배고프셨겠어요",
          "original_form": "완전배고프셨겠어요",
          "speaker_id": "2",
          "time": "20210831 00:57"
        },
        {
          "id": "MDRW2100024329.1.14",
          "form": "나머지 5개도 먹고ㅋㅋ점심메뉴요플레나왔거든요?",
          "original_form": "나머지 5개도 먹고ㅋㅋ점심메뉴요플레나왔거든요?",
          "speaker_id": "1",
          "time": "20210831 00:58"
        },
        {
          "id": "MDRW2100024329.1.15",
          "form": "먼저먹으라고안하셨나요?",
          "original_form": "먼저먹으라고안하셨나요?",
          "speaker_id": "2",
          "time": "20210831 00:58"
        },
        {
          "id": "MDRW2100024329.1.16",
          "form": "그거2개 받아서ㅋㄲ2개먹음요ㅋㅋ",
          "original_form": "그거2개 받아서ㅋㄲ2개먹음요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:58"
        },
        {
          "id": "MDRW2100024329.1.17",
          "form": "요플레 맛있죠",
          "original_form": "요플레 맛있죠",
          "speaker_id": "2",
          "time": "20210831 00:58"
        },
        {
          "id": "MDRW2100024329.1.18",
          "form": "복직한지얼마안되어서점심시간을 잊어버렸나봐요ㅋㅋㅋ",
          "original_form": "복직한지얼마안되어서점심시간을 잊어버렸나봐요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:58"
        },
        {
          "id": "MDRW2100024329.1.19",
          "form": "어머나 그럴수도있군요 밥시간을까먹다니",
          "original_form": "어머나 그럴수도있군요 밥시간을까먹다니",
          "speaker_id": "2",
          "time": "20210831 00:59"
        },
        {
          "id": "MDRW2100024329.1.20",
          "form": "점심 좋아하시는 분이신데ㅎㅎㅎ 아쉽아쉽ㅋㅋㅋㅋ",
          "original_form": "점심 좋아하시는 분이신데ㅎㅎㅎ 아쉽아쉽ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:59"
        },
        {
          "id": "MDRW2100024329.1.21",
          "form": "이번주복직하신건가요?",
          "original_form": "이번주복직하신건가요?",
          "speaker_id": "2",
          "time": "20210831 00:59"
        },
        {
          "id": "MDRW2100024329.1.22",
          "form": "ㅋㅋ저번주요ㅋㅋ",
          "original_form": "ㅋㅋ저번주요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:00"
        },
        {
          "id": "MDRW2100024329.1.23",
          "form": "오늘은 낚지콩나물덮밥ㅋㅋ",
          "original_form": "오늘은 낚지콩나물덮밥ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:00"
        },
        {
          "id": "MDRW2100024329.1.24",
          "form": "점심시간알람걸어두라고하세요ㅋㅋ",
          "original_form": "점심시간알람걸어두라고하세요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 01:00"
        },
        {
          "id": "MDRW2100024329.1.25",
          "form": "언양식불고기ㅋㅋㅋ",
          "original_form": "언양식불고기ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:00"
        },
        {
          "id": "MDRW2100024329.1.26",
          "form": "초코우유ㅋㅋㅋ",
          "original_form": "초코우유ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:00"
        },
        {
          "id": "MDRW2100024329.1.27",
          "form": "절대놓치지않을꺼예요ㅋㅋ",
          "original_form": "절대놓치지않을꺼예요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 01:00"
        },
        {
