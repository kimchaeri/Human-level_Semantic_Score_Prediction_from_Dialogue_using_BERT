      "utterance": [
        {
          "id": "MDRW2100038425.1.1",
          "form": "참외 그 씨앗부분을 아예 다 벗겨내고",
          "original_form": "참외 그 씨앗부분을 아예 다 벗겨내고",
          "speaker_id": "1",
          "time": "20210902 13:00"
        },
        {
          "id": "MDRW2100038425.1.2",
          "form": "이제 점심먹으러 갑니다요",
          "original_form": "이제 점심먹으러 갑니다요",
          "speaker_id": "2",
          "time": "20210902 13:00"
        },
        {
          "id": "MDRW2100038425.1.3",
          "form": "하얀 부분만 나와요",
          "original_form": "하얀 부분만 나와요",
          "speaker_id": "1",
          "time": "20210902 13:00"
        },
        {
          "id": "MDRW2100038425.1.4",
          "form": "오 뭐드세요?",
          "original_form": "오 뭐드세요?",
          "speaker_id": "1",
          "time": "20210902 13:00"
        },
        {
          "id": "MDRW2100038425.1.5",
          "form": "그냥 라면 먹고",
          "original_form": "그냥 라면 먹고",
          "speaker_id": "2",
          "time": "20210902 13:00"
        },
        {
          "id": "MDRW2100038425.1.6",
          "form": "나가서 참외 사오려구요ㅋㅋ",
          "original_form": "나가서 참외 사오려구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:01"
        },
        {
          "id": "MDRW2100038425.1.7",
          "form": "참욐ㅋㅋㅋㅋㅋ 아 맛있겠네요",
          "original_form": "참욐ㅋㅋㅋㅋㅋ 아 맛있겠네요",
          "speaker_id": "1",
          "time": "20210902 13:01"
        },
        {
          "id": "MDRW2100038425.1.8",
          "form": "이디야에 참외맛 음료 나왔는데 그거 참외맛 안나고 별로에요..",
          "original_form": "이디야에 참외맛 음료 나왔는데 그거 참외맛 안나고 별로에요..",
          "speaker_id": "1",
          "time": "20210902 13:01"
        },
        {
          "id": "MDRW2100038425.1.9",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "2",
          "time": "20210902 13:46"
        },
        {
          "id": "MDRW2100038425.1.10",
          "form": "안먹길 잘했네요",
          "original_form": "안먹길 잘했네요",
          "speaker_id": "2",
          "time": "20210902 13:46"
        },
        {
          "id": "MDRW2100038425.1.11",
          "form": "약간 메로나 녹은맛이고",
          "original_form": "약간 메로나 녹은맛이고",
          "speaker_id": "1",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100038425.1.12",
          "form": "너무 인공적이에요",
          "original_form": "너무 인공적이에요",
          "speaker_id": "1",
          "time": "20210902 17:31"
        },
        {
          "id": "MDRW2100038425.1.13",
          "form": "ㅋㅋ 메로나가 원래 참외맛이긴 했죠",
          "original_form": "ㅋㅋ 메로나가 원래 참외맛이긴 했죠",
          "speaker_id": "2",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100038425.1.14",
          "form": "참외음료는 거르겠습니다",
          "original_form": "참외음료는 거르겠습니다",
          "speaker_id": "2",
          "time": "20210902 17:34"
        },
        {
          "id": "MDRW2100038425.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ 맞아요 ㅜ 참외음료 노맛",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 맞아요 ㅜ 참외음료 노맛",
          "speaker_id": "1",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100038425.1.16",
          "form": "참외는 과일로 먹기로..",
          "original_form": "참외는 과일로 먹기로..",
          "speaker_id": "1",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100038425.1.17",
          "form": "다른 맛있는 음료 없나요",
          "original_form": "다른 맛있는 음료 없나요",
          "speaker_id": "2",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100038425.1.18",
          "form": "저 이디야 캔디라떼 좋아했었는데",
          "original_form": "저 이디야 캔디라떼 좋아했었는데",
          "speaker_id": "2",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100038425.1.19",
          "form": "전 이디야 홍시주스가 맛잇더라구요",
          "original_form": "전 이디야 홍시주스가 맛잇더라구요",
          "speaker_id": "1",
          "time": "20210903 08:37"
        },
        {
          "id": "MDRW2100038425.1.20",
          "form": "찐 홍시맛이에요",
          "original_form": "찐 홍시맛이에요",
          "speaker_id": "1",
          "time": "20210903 08:37"
        },
        {
          "id": "MDRW2100038425.1.21",
          "form": "헐 진짜요??",
          "original_form": "헐 진짜요??",
          "speaker_id": "2",
          "time": "20210903 08:37"
        },
        {
          "id": "MDRW2100038425.1.22",
          "form": "이디야까지 걸어서 10분거리",
          "original_form": "이디야까지 걸어서 10분거리",
          "speaker_id": "2",
          "time": "20210903 08:37"
        },
        {
          "id": "MDRW2100038425.1.23",
          "form": "곧 출발합니다",
          "original_form": "곧 출발합니다",
          "speaker_id": "2",
          "time": "20210903 08:38"
        },
        {
          "id": "MDRW2100038425.1.24",
          "form": "잌ㅋㅋㅋㅋ저 이디야 엄청자주가요",
          "original_form": "잌ㅋㅋㅋㅋ저 이디야 엄청자주가요",
          "speaker_id": "1",
          "time": "20210903 08:38"
        },
        {
          "id": "MDRW2100038425.1.25",
          "form": "저는 홍시주스랑 이곡라떼가 좋아요",
          "original_form": "저는 홍시주스랑 이곡라떼가 좋아요",
          "speaker_id": "1",
          "time": "20210903 08:38"
        },
        {
          "id": "MDRW2100038425.1.26",
          "form": "이곡라떼는 뭔가요??",
          "original_form": "이곡라떼는 뭔가요??",
          "speaker_id": "2",
          "time": "20210903 08:40"
        },
        {
          "id": "MDRW2100038425.1.27",
          "form": "미숫가루인데 존맛이에요..",
          "original_form": "미숫가루인데 존맛이에요..",
          "speaker_id": "1",
          "time": "20210903 08:40"
        }
      ]
    }
  ]
