          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020294.1.98",
          "form": "아글쿠나",
          "original_form": "아글쿠나",
          "speaker_id": "2",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020294.1.99",
          "form": "음식물은",
          "original_form": "음식물은",
          "speaker_id": "2",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020294.1.100",
          "form": "그릇 좀 닦고 물에",
          "original_form": "그릇 좀 닦고 물에",
