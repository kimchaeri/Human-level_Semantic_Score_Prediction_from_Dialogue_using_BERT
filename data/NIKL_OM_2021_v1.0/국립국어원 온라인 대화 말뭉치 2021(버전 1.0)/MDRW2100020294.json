        },
        {
          "id": "MDRW2100020294.1.2",
          "form": "하루에한번 돌리면돼",
          "original_form": "하루에한번 돌리면돼",
          "speaker_id": "1",
          "time": "20210828 01:03"
        },
        {
          "id": "MDRW2100020294.1.3",
          "form": "난 청소기",
          "original_form": "난 청소기",
          "speaker_id": "2",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020294.1.4",
          "form": "하루에한번 돌리는사람이",
          "original_form": "하루에한번 돌리는사람이",
          "speaker_id": "2",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020294.1.5",
          "form": "제일대단해",
          "original_form": "제일대단해",
          "speaker_id": "2",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020294.1.6",
          "form": "근데 나도 내가 안돌림",
          "original_form": "근데 나도 내가 안돌림",
          "speaker_id": "1",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020294.1.7",
          "form": "청서기 싫어해서",
          "original_form": "청서기 싫어해서",
          "speaker_id": "1",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020294.1.8",
          "form": "그냥",
          "original_form": "그냥",
          "speaker_id": "1",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020294.1.9",
          "form": "바닥이",
          "original_form": "바닥이",
          "speaker_id": "1",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020294.1.10",
          "form": "까슬하다?",
          "original_form": "까슬하다?",
          "speaker_id": "1",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020294.1.11",
          "form": "나는근데 하면 또 잘한다",
          "original_form": "나는근데 하면 또 잘한다",
          "speaker_id": "2",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020294.1.12",
          "form": "그럼 돌림..",
          "original_form": "그럼 돌림..",
          "speaker_id": "1",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020294.1.13",
          "form": "올~~~~",
          "original_form": "올~~~~",
          "speaker_id": "1",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020294.1.14",
          "form": "그건님도겟지요?",
          "original_form": "그건님도겟지요?",
          "speaker_id": "2",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020294.1.15",
          "form": "아마도..",
          "original_form": "아마도..",
          "speaker_id": "1",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020294.1.16",
          "form": "저 집이",
          "original_form": "저 집이",
          "speaker_id": "1",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020294.1.17",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "2",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020294.1.18",
          "form": "그렇다고",
          "original_form": "그렇다고",
          "speaker_id": "1",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020294.1.19",
          "form": "더럽다고 묫견디고 그러지않음",
          "original_form": "더럽다고 묫견디고 그러지않음",
          "speaker_id": "1",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020294.1.20",
          "form": "더러운거 너머 익숙해서 ㅋ",
          "original_form": "더러운거 너머 익숙해서 ㅋ",
          "speaker_id": "1",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020294.1.21",
          "form": "그리고 나 세탁기돌리는거 잘함ㅋ",
          "original_form": "그리고 나 세탁기돌리는거 잘함ㅋ",
          "speaker_id": "2",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020294.1.22",
          "form": "걍 막 드러눕고",
          "original_form": "걍 막 드러눕고",
          "speaker_id": "1",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020294.1.23",
          "form": "세탁물도 꺼내주세요",
          "original_form": "세탁물도 꺼내주세요",
          "speaker_id": "1",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020294.1.24",
          "form": "저는 세탁기",
          "original_form": "저는 세탁기",
          "speaker_id": "1",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020294.1.25",
          "form": "옷거낼대",
          "original_form": "옷거낼대",
          "speaker_id": "1",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020294.1.26",
          "form": "세튿물꺼내는거",
          "original_form": "세튿물꺼내는거",
          "speaker_id": "2",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020294.1.27",
          "form": "맨날 내가해",
          "original_form": "맨날 내가해",
          "speaker_id": "2",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020294.1.28",
          "form": "할머니 손도",
          "original_form": "할머니 손도",
          "speaker_id": "2",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020294.1.29",
          "form": "안 닿아서",
          "original_form": "안 닿아서",
          "speaker_id": "2",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020294.1.30",
          "form": "배가 너무아파",
          "original_form": "배가 너무아파",
          "speaker_id": "1",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020294.1.31",
          "form": "곡소리 내야함",
          "original_form": "곡소리 내야함",
          "speaker_id": "1",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020294.1.32",
          "form": "아개웃겨",
          "original_form": "아개웃겨",
          "speaker_id": "2",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020294.1.33",
          "form": "하필 또 우리집 세탁기",
          "original_form": "하필 또 우리집 세탁기",
          "speaker_id": "1",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020294.1.34",
          "form": "베란다 좀 먼곳에 ㅣ엣어기지고",
          "original_form": "베란다 좀 먼곳에 ㅣ엣어기지고",
          "speaker_id": "1",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020294.1.35",
          "form": "머무힘들다",
          "original_form": "머무힘들다",
          "speaker_id": "1",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020294.1.36",
          "form": "근데 발래널기 나 잘함",
          "original_form": "근데 발래널기 나 잘함",
          "speaker_id": "1",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020294.1.37",
          "form": "개기도 잘햄",
          "original_form": "개기도 잘햄",
          "speaker_id": "1",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020294.1.38",
          "form": "아빠가 나보고 군대갓다ㅏ놧냐함",
          "original_form": "아빠가 나보고 군대갓다ㅏ놧냐함",
          "speaker_id": "1",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020294.1.39",
          "form": "나개는거",
          "original_form": "나개는거",
          "speaker_id": "2",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020294.1.40",
          "form": "수건 잘접는다고",
          "original_form": "수건 잘접는다고",
          "speaker_id": "1",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020294.1.41",
          "form": "근데 나 티셔츠 거능거 싫어함",
          "original_form": "근데 나 티셔츠 거능거 싫어함",
          "speaker_id": "1",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020294.1.42",
          "form": "진짜 싫어함",
          "original_form": "진짜 싫어함",
          "speaker_id": "2",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020294.1.43",
          "form": "근데 걸으라면 걸음",
          "original_form": "근데 걸으라면 걸음",
          "speaker_id": "1",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020294.1.44",
          "form": "난 거는게 낫자",
          "original_form": "난 거는게 낫자",
          "speaker_id": "2",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020294.1.45",
          "form": "난 개기",
          "original_form": "난 개기",
          "speaker_id": "1",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020294.1.46",
          "form": "어케 개지",
          "original_form": "어케 개지",
          "speaker_id": "2",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020294.1.47",
          "form": "ㄱ개는거 너무 재밌음..수건이",
          "original_form": "ㄱ개는거 너무 재밌음..수건이",
          "speaker_id": "1",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020294.1.48",
          "form": "진짜 좋음",
          "original_form": "진짜 좋음",
          "speaker_id": "1",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020294.1.49",
          "form": "진짜 댄단하다",
          "original_form": "진짜 댄단하다",
          "speaker_id": "2",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020294.1.50",
          "form": "님은개세요",
          "original_form": "님은개세요",
          "speaker_id": "2",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020294.1.51",
          "form": "저는 걸게요",
          "original_form": "저는 걸게요",
          "speaker_id": "2",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020294.1.52",
          "form": "ㅇㅋ근데 내것도 걸어주세요",
          "original_form": "ㅇㅋ근데 내것도 걸어주세요",
          "speaker_id": "1",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020294.1.53",
          "form": "아니면 님이걍 널고개세요",
          "original_form": "아니면 님이걍 널고개세요",
          "speaker_id": "2",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020294.1.54",
          "form": "나 티 개는건 싫어",
          "original_form": "나 티 개는건 싫어",
          "speaker_id": "1",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020294.1.55",
          "form": "나는 돌릭ㅎ",
          "original_form": "나는 돌릭ㅎ",
          "speaker_id": "2",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020294.1.56",
          "form": "꺼낼게",
          "original_form": "꺼낼게",
          "speaker_id": "2",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020294.1.57",
          "form": "그럼 나는 수건ㅇㄹ 개고",
          "original_form": "그럼 나는 수건ㅇㄹ 개고",
          "speaker_id": "1",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020294.1.58",
          "form": "님운 나머지를 거셈",
          "original_form": "님운 나머지를 거셈",
          "speaker_id": "1",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020294.1.59",
          "form": "ㅇㅋ",
          "original_form": "ㅇㅋ",
          "speaker_id": "2",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020294.1.60",
          "form": "나 거는거 잘하지",
          "original_form": "나 거는거 잘하지",
          "speaker_id": "2",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020294.1.61",
          "form": "오~~~~~",
          "original_form": "오~~~~~",
          "speaker_id": "1",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020294.1.62",
          "form": "옷걸이놔두고왜",
          "original_form": "옷걸이놔두고왜",
          "speaker_id": "2",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020294.1.63",
          "form": "개야하는지",
          "original_form": "개야하는지",
          "speaker_id": "2",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020294.1.64",
          "form": "더대체왜",
          "original_form": "더대체왜",
          "speaker_id": "2",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020294.1.65",
          "form": "수건은 근데 옷걸이를 못써",
          "original_form": "수건은 근데 옷걸이를 못써",
          "speaker_id": "1",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020294.1.66",
          "form": "그건맞아",
          "original_form": "그건맞아",
          "speaker_id": "2",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020294.1.67",
          "form": "또뭐잇지",
          "original_form": "또뭐잇지",
          "speaker_id": "1",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020294.1.68",
          "form": "음..",
          "original_form": "음..",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020294.1.69",
          "form": "룸메가 됏을때 합의봐야할것..",
          "original_form": "룸메가 됏을때 합의봐야할것..",
          "speaker_id": "1",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020294.1.70",
          "form": "설거지",
          "original_form": "설거지",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020294.1.71",
          "form": "설거지 나",
          "original_form": "설거지 나",
          "speaker_id": "1",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020294.1.72",
          "form": "고무장갑 있으면 잘함",
          "original_form": "고무장갑 있으면 잘함",
          "speaker_id": "1",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020294.1.73",
          "form": "돌아가면서 하거나",
          "original_form": "돌아가면서 하거나",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020294.1.74",
          "form": "넚으면 싫어요",
          "original_form": "넚으면 싫어요",
          "speaker_id": "1",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020294.1.75",
          "form": "그럇으면좋겟음",
          "original_form": "그럇으면좋겟음",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020294.1.76",
          "form": "싫 다고 하능걱도 나쁜말이야?",
          "original_form": "싫 다고 하능걱도 나쁜말이야?",
          "speaker_id": "1",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020294.1.77",
          "form": "너무해",
          "original_form": "너무해",
          "speaker_id": "1",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020294.1.78",
          "form": "난 없이잘한다",
          "original_form": "난 없이잘한다",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020294.1.79",
          "form": "오 좋지",
          "original_form": "오 좋지",
          "speaker_id": "1",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020294.1.80",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020294.1.81",
          "form": "ㅣ님아 없이하면",
          "original_form": "ㅣ님아 없이하면",
          "speaker_id": "1",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020294.1.82",
          "form": "지금은 ㄱㅊ아서 다행인데",
          "original_form": "지금은 ㄱㅊ아서 다행인데",
          "speaker_id": "1",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020294.1.83",
          "form": "언제 습진생길지모름",
          "original_form": "언제 습진생길지모름",
          "speaker_id": "1",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020294.1.84",
          "form": "ㄹㅇ..맞는대",
          "original_form": "ㄹㅇ..맞는대",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020294.1.85",
          "form": "세제 맨손에 닿으면 습진 생기가도한대요",
          "original_form": "세제 맨손에 닿으면 습진 생기가도한대요",
          "speaker_id": "1",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020294.1.86",
          "form": "저 한번",
          "original_form": "저 한번",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020294.1.87",
          "form": "저난 근데 그게 좀 심해ㅓㅅ",
          "original_form": "저난 근데 그게 좀 심해ㅓㅅ",
          "speaker_id": "1",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020294.1.88",
          "form": "약하게 생긴적도잇는데",
          "original_form": "약하게 생긴적도잇는데",
          "speaker_id": "2",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020294.1.89",
          "form": "자주생겻어요..",
          "original_form": "자주생겻어요..",
          "speaker_id": "1",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020294.1.90",
