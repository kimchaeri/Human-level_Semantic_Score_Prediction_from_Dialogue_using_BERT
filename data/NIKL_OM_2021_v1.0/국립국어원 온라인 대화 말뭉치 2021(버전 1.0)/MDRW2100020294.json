          "time": "20210828 01:10"
        },
        {
          "id": "MDRW2100020294.1.106",
          "form": "ㄱ미글거리고",
          "original_form": "ㄱ미글거리고",
          "speaker_id": "1",
          "time": "20210828 01:10"
        },
        {
          "id": "MDRW2100020294.1.107",
          "form": "그거는 비누로 손씻으면돼",
