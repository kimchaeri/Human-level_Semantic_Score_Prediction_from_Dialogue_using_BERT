          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020294.1.102",
          "form": "ㄱㅊ은데",
          "original_form": "ㄱㅊ은데",
          "speaker_id": "2",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020294.1.103",
          "form": "근데 그 그릇이 있는 물도 더럽",
