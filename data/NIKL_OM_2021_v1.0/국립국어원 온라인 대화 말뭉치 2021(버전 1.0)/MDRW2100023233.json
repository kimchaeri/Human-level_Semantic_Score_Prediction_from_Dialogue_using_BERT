      "utterance": [
        {
          "id": "MDRW2100023233.1.1",
          "form": "으하하하",
          "original_form": "으하하하",
          "speaker_id": "1",
          "time": "20210829 23:04"
        },
        {
          "id": "MDRW2100023233.1.2",
          "form": "ㅎㅎㅎ 요즘 하시는 운동이 있나요??",
          "original_form": "ㅎㅎㅎ 요즘 하시는 운동이 있나요??",
          "speaker_id": "2",
          "time": "20210829 23:14"
        },
        {
          "id": "MDRW2100023233.1.3",
          "form": "네!!! 저 오늘은 산책 하고",
          "original_form": "네!!! 저 오늘은 산책 하고",
          "speaker_id": "1",
          "time": "20210829 23:19"
        },
        {
          "id": "MDRW2100023233.1.4",
          "form": "방금은 저스트댄스를 했어요 ^~^",
          "original_form": "방금은 저스트댄스를 했어요 ^~^",
          "speaker_id": "1",
          "time": "20210829 23:19"
        },
        {
          "id": "MDRW2100023233.1.5",
          "form": "name2은요?!",
          "original_form": "&name2&은요?!",
          "speaker_id": "1",
          "time": "20210829 23:20"
        },
        {
          "id": "MDRW2100023233.1.6",
          "form": "우와! 저는 운동을 막하면 안돼서 요즘은 조금 사리고 있어요ㅜㅜ",
          "original_form": "우와! 저는 운동을 막하면 안돼서 요즘은 조금 사리고 있어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 23:29"
        },
        {
          "id": "MDRW2100023233.1.7",
          "form": "사실 얼마전에 병원에서 검사받고 왔는데 결과가 나오면 의사가 뭐라고 말해주겠죠!!",
          "original_form": "사실 얼마전에 병원에서 검사받고 왔는데 결과가 나오면 의사가 뭐라고 말해주겠죠!!",
          "speaker_id": "2",
          "time": "20210829 23:29"
        },
        {
          "id": "MDRW2100023233.1.8",
          "form": "운동해도 된다고 하면 열심히 하려고요! 집에 자전거도 있고 저스트 댄스도 뽐뿌오네요~~",
          "original_form": "운동해도 된다고 하면 열심히 하려고요! 집에 자전거도 있고 저스트 댄스도 뽐뿌오네요~~",
          "speaker_id": "2",
          "time": "20210829 23:29"
        },
        {
          "id": "MDRW2100023233.1.9",
          "form": "헉 병원 다녀오셨군요!!!!!",
          "original_form": "헉 병원 다녀오셨군요!!!!!",
          "speaker_id": "1",
          "time": "20210830 00:01"
        },
        {
          "id": "MDRW2100023233.1.10",
          "form": "결과는 언제 나오나요....?",
          "original_form": "결과는 언제 나오나요....?",
          "speaker_id": "1",
          "time": "20210830 00:01"
        },
        {
          "id": "MDRW2100023233.1.11",
          "form": "부디 아픈 데가 없기를 바라요!!! ",
          "original_form": "부디 아픈 데가 없기를 바라요!!! ",
          "speaker_id": "1",
          "time": "20210830 00:01"
        },
        {
          "id": "MDRW2100023233.1.12",
          "form": "이번주 금요일에 다시 가야해요!",
          "original_form": "이번주 금요일에 다시 가야해요!",
          "speaker_id": "2",
          "time": "20210830 09:46"
        },
        {
          "id": "MDRW2100023233.1.13",
          "form": "감사해요 ㅎㅎ",
          "original_form": "감사해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 09:47"
        },
        {
          "id": "MDRW2100023233.1.14",
          "form": "요즘 건강이 제일 중요하다고 느껴요..!! 건강 잘챙기세요ㅜㅜ",
          "original_form": "요즘 건강이 제일 중요하다고 느껴요..!! 건강 잘챙기세요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 09:49"
        },
        {
          "id": "MDRW2100023233.1.15",
          "form": "혹시 어디가 아프신건지 물어봐도 되나요?!",
          "original_form": "혹시 어디가 아프신건지 물어봐도 되나요?!",
          "speaker_id": "1",
          "time": "20210830 09:49"
        },
        {
          "id": "MDRW2100023233.1.16",
          "form": "아앗 맞아요",
          "original_form": "아앗 맞아요",
          "speaker_id": "1",
          "time": "20210830 09:49"
        },
        {
          "id": "MDRW2100023233.1.17",
          "form": "오늘도 아침에 일어나는데",
          "original_form": "오늘도 아침에 일어나는데",
          "speaker_id": "1",
          "time": "20210830 09:49"
        },
        {
          "id": "MDRW2100023233.1.18",
          "form": "너무 피곤하더라구요 ㅠㅠ",
          "original_form": "너무 피곤하더라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 09:49"
        },
        {
          "id": "MDRW2100023233.1.19",
          "form": "저 원인을 몰라서 이번에 씨티 찍었어요ㅜㅜ",
          "original_form": "저 원인을 몰라서 이번에 씨티 찍었어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 09:49"
        },
        {
          "id": "MDRW2100023233.1.20",
          "form": "허걱 씨티까지.. !!!!!!",
          "original_form": "허걱 씨티까지.. !!!!!!",
          "speaker_id": "1",
          "time": "20210830 09:49"
        },
        {
          "id": "MDRW2100023233.1.21",
          "form": "비용도 많이 들었겠어요 ㅠㅠ",
          "original_form": "비용도 많이 들었겠어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 09:49"
        },
        {
          "id": "MDRW2100023233.1.22",
          "form": "네 씨티는 좀 비싸더라구요...!",
          "original_form": "네 씨티는 좀 비싸더라구요...!",
          "speaker_id": "2",
          "time": "20210830 09:50"
        },
        {
          "id": "MDRW2100023233.1.23",
          "form": "그래도 너무 오래 신경쓰이게해서",
          "original_form": "그래도 너무 오래 신경쓰이게해서",
          "speaker_id": "2",
          "time": "20210830 09:50"
        },
        {
          "id": "MDRW2100023233.1.24",
          "form": "이제그만 끝내자! 하고 큰맘먹고 찍었어요 ㅋㅋㅋㅋ",
          "original_form": "이제그만 끝내자! 하고 큰맘먹고 찍었어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 09:50"
        },
        {
          "id": "MDRW2100023233.1.25",
          "form": "잘 생각하셨어요!",
          "original_form": "잘 생각하셨어요!",
          "speaker_id": "1",
          "time": "20210830 09:51"
        },
        {
          "id": "MDRW2100023233.1.26",
          "form": "정말 건강이 최고예요",
          "original_form": "정말 건강이 최고예요",
          "speaker_id": "1",
          "time": "20210830 09:51"
        },
        {
          "id": "MDRW2100023233.1.27",
          "form": "벌거 아니어도 원인을 모르면 신경쓰이더라구요",
          "original_form": "벌거 아니어도 원인을 모르면 신경쓰이더라구요",
          "speaker_id": "1",
          "time": "20210830 09:52"
        },
        {
          "id": "MDRW2100023233.1.28",
          "form": "맞아요... 그리고 병원은 언제가도 긴장되고 무서워요",
          "original_form": "맞아요... 그리고 병원은 언제가도 긴장되고 무서워요",
          "speaker_id": "2",
          "time": "20210830 09:53"
        },
        {
          "id": "MDRW2100023233.1.29",
          "form": "될 수 있으면 안가고 싶은 곳이에요...",
          "original_form": "될 수 있으면 안가고 싶은 곳이에요...",
          "speaker_id": "2",
          "time": "20210830 09:53"
        },
        {
          "id": "MDRW2100023233.1.30",
          "form": "오늘은 저도 산책을 좀 해야겠어요!",
          "original_form": "오늘은 저도 산책을 좀 해야겠어요!",
          "speaker_id": "2",
          "time": "20210830 09:54"
        },
        {
          "id": "MDRW2100023233.1.31",
          "form": "정말 공감해요 ㅠㅠ",
          "original_form": "정말 공감해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 10:05"
        },
        {
          "id": "MDRW2100023233.1.32",
          "form": "병원은 살면서 최대한 안가고싶은 곳이에요!",
          "original_form": "병원은 살면서 최대한 안가고싶은 곳이에요!",
          "speaker_id": "1",
          "time": "20210830 10:05"
        }
      ]
