      "utterance": [
        {
          "id": "MDRW2100016636.1.1",
          "form": "차라리 그런 글은 막혔으면 좋겠다",
          "original_form": "차라리 그런 글은 막혔으면 좋겠다",
          "speaker_id": "2",
          "time": "20210826 14:18"
        },
        {
          "id": "MDRW2100016636.1.2",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210826 14:18"
        },
        {
          "id": "MDRW2100016636.1.3",
          "form": "그거봤는데",
          "original_form": "그거봤는데",
          "speaker_id": "1",
          "time": "20210826 14:18"
        },
        {
          "id": "MDRW2100016636.1.4",
          "form": "에타로",
          "original_form": "에타로",
          "speaker_id": "1",
          "time": "20210826 14:18"
        },
        {
          "id": "MDRW2100016636.1.5",
          "form": "시험기간에",
          "original_form": "시험기간에",
          "speaker_id": "1",
          "time": "20210826 14:18"
        },
        {
          "id": "MDRW2100016636.1.6",
          "form": "방해하는방법",
          "original_form": "방해하는방법",
          "speaker_id": "1",
          "time": "20210826 14:18"
        },
        {
          "id": "MDRW2100016636.1.7",
          "form": "뭔데??",
          "original_form": "뭔데??",
          "speaker_id": "2",
          "time": "20210826 14:18"
        },
        {
          "id": "MDRW2100016636.1.8",
          "form": "시험기간에 에타에 일부러 페미글 올리고",
          "original_form": "시험기간에 에타에 일부러 페미글 올리고",
          "speaker_id": "1",
          "time": "20210826 14:19"
        },
        {
          "id": "MDRW2100016636.1.9",
          "form": "알림꺼두고 공부하기",
          "original_form": "알림꺼두고 공부하기",
          "speaker_id": "1",
          "time": "20210826 14:19"
        },
        {
          "id": "MDRW2100016636.1.10",
          "form": "그러면남자애들몰려와서",
          "original_form": "그러면남자애들몰려와서",
          "speaker_id": "1",
          "time": "20210826 14:19"
        },
        {
          "id": "MDRW2100016636.1.11",
          "form": "난리치니까",
          "original_form": "난리치니까",
          "speaker_id": "1",
          "time": "20210826 14:19"
        },
        {
          "id": "MDRW2100016636.1.12",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:19"
        },
        {
          "id": "MDRW2100016636.1.13",
          "form": "아 뭐임ㅋㅌㅋㅌㅌㅋㅋㅋㅋㅋㅋㅋ 앜ㅌㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 뭐임ㅋㅌㅋㅌㅌㅋㅋㅋㅋㅋㅋㅋ 앜ㅌㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:19"
        },
        {
          "id": "MDRW2100016636.1.14",
          "form": "정말 놀라운 방법이네",
          "original_form": "정말 놀라운 방법이네",
          "speaker_id": "2",
          "time": "20210826 14:19"
        },
        {
          "id": "MDRW2100016636.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그니까",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그니까",
          "speaker_id": "1",
          "time": "20210826 14:19"
        },
        {
          "id": "MDRW2100016636.1.16",
          "form": "근데 요즘 여초 커뮤도",
          "original_form": "근데 요즘 여초 커뮤도",
          "speaker_id": "1",
          "time": "20210826 14:19"
        },
        {
          "id": "MDRW2100016636.1.17",
          "form": "재밌겠다 직관해보고 싶다",
          "original_form": "재밌겠다 직관해보고 싶다",
          "speaker_id": "2",
          "time": "20210826 14:20"
        },
        {
          "id": "MDRW2100016636.1.18",
          "form": "약간 연령대가 올라간듯",
          "original_form": "약간 연령대가 올라간듯",
          "speaker_id": "1",
          "time": "20210826 14:20"
        },
        {
          "id": "MDRW2100016636.1.19",
          "form": "아 그래??",
          "original_form": "아 그래??",
          "speaker_id": "2",
          "time": "20210826 14:20"
        },
        {
          "id": "MDRW2100016636.1.20",
          "form": "백래시도많이오고",
          "original_form": "백래시도많이오고",
          "speaker_id": "1",
          "time": "20210826 14:20"
        },
        {
          "id": "MDRW2100016636.1.21",
          "form": "어이고야 진짜??",
          "original_form": "어이고야 진짜??",
          "speaker_id": "2",
          "time": "20210826 14:20"
        },
        {
          "id": "MDRW2100016636.1.22",
          "form": "제일유명한커뮤에",
          "original_form": "제일유명한커뮤에",
          "speaker_id": "1",
          "time": "20210826 14:20"
        },
        {
          "id": "MDRW2100016636.1.23",
          "form": "ㅁ비혼방밑에바로마미방있대",
          "original_form": "ㅁ비혼방밑에바로마미방있대",
          "speaker_id": "1",
          "time": "20210826 14:21"
        },
        {
          "id": "MDRW2100016636.1.24",
          "form": "엄마의 마미??",
          "original_form": "엄마의 마미??",
          "speaker_id": "2",
          "time": "20210826 14:21"
        },
        {
          "id": "MDRW2100016636.1.25",
          "form": "그래서마미방에서 맨날 비혼방 글 캡쳐해가서 욕한대",
          "original_form": "그래서마미방에서 맨날 비혼방 글 캡쳐해가서 욕한대",
          "speaker_id": "1",
          "time": "20210826 14:21"
        },
        {
          "id": "MDRW2100016636.1.26",
