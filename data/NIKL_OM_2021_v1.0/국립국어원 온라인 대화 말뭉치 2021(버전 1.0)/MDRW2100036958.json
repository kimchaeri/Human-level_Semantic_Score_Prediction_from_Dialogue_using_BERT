      "utterance": [
        {
          "id": "MDRW2100036958.1.1",
          "form": "전 금요일날 1차맞아여ㅠ",
          "original_form": "전 금요일날 1차맞아여ㅠ",
          "speaker_id": "2",
          "time": "20210831 15:41"
        },
        {
          "id": "MDRW2100036958.1.2",
          "form": "맞으셨나요?",
          "original_form": "맞으셨나요?",
          "speaker_id": "2",
          "time": "20210831 15:41"
        },
        {
          "id": "MDRW2100036958.1.3",
          "form": "이번주요? 얼마 안남았네요..",
          "original_form": "이번주요? 얼마 안남았네요..",
          "speaker_id": "1",
          "time": "20210831 15:42"
        },
        {
          "id": "MDRW2100036958.1.4",
          "form": "전 다다음주에 1차에요",
          "original_form": "전 다다음주에 1차에요",
          "speaker_id": "1",
          "time": "20210831 15:42"
        },
        {
          "id": "MDRW2100036958.1.5",
          "form": "화이자 맞으시나요?",
          "original_form": "화이자 맞으시나요?",
          "speaker_id": "1",
          "time": "20210831 15:42"
        },
        {
          "id": "MDRW2100036958.1.6",
          "form": "네 그런거같아요ㅠㅠ",
          "original_form": "네 그런거같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 15:42"
        },
        {
          "id": "MDRW2100036958.1.7",
          "form": "저랑같이일하시는분이 저번주 금요일날 맞으셨는데ㅠㅠ",
          "original_form": "저랑같이일하시는분이 저번주 금요일날 맞으셨는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 15:43"
        },
        {
          "id": "MDRW2100036958.1.8",
          "form": "보통 많이 느끼는 부작용이있어서ㅠ저도 무서워여ㅠ",
          "original_form": "보통 많이 느끼는 부작용이있어서ㅠ저도 무서워여ㅠ",
          "speaker_id": "2",
          "time": "20210831 15:43"
        },
        {
          "id": "MDRW2100036958.1.9",
          "form": "오.. 그래도 며칠 이내게 괜찮아지면 그건 그냥 있는 증상이라 괜찮을텐데요",
          "original_form": "오.. 그래도 며칠 이내게 괜찮아지면 그건 그냥 있는 증상이라 괜찮을텐데요",
          "speaker_id": "1",
          "time": "20210831 15:45"
        },
        {
          "id": "MDRW2100036958.1.10",
          "form": "오래 가면 병원 가봐야하겟지만",
          "original_form": "오래 가면 병원 가봐야하겟지만",
          "speaker_id": "1",
          "time": "20210831 15:45"
        },
        {
          "id": "MDRW2100036958.1.11",
          "form": "금요일날 오후에 접종후 토요일은 괜찮네 싶었는데 일요일날 기운이 빠지더래요ㅠ 그러고 월요일날 출근하고 두통도 심하게 오고 속도 메스껍고 링겔맞앗네요ㅠ",
          "original_form": "금요일날 오후에 접종후 토요일은 괜찮네 싶었는데 일요일날 기운이 빠지더래요ㅠ 그러고 월요일날 출근하고 두통도 심하게 오고 속도 메스껍고 링겔맞앗네요ㅠ",
          "speaker_id": "2",
          "time": "20210831 15:55"
        },
        {
          "id": "MDRW2100036958.1.12",
          "form": "아이고.. 링겔까지.. 주변에 그런 예가 있으면 좀 무섭죠",
          "original_form": "아이고.. 링겔까지.. 주변에 그런 예가 있으면 좀 무섭죠",
          "speaker_id": "1",
          "time": "20210831 15:57"
        },
        {
          "id": "MDRW2100036958.1.13",
          "form": "저는 주변에 백신 맞고 심하게 아픈 분들이 없어서인지 그건 다행인데",
          "original_form": "저는 주변에 백신 맞고 심하게 아픈 분들이 없어서인지 그건 다행인데",
          "speaker_id": "1",
          "time": "20210831 15:57"
        },
        {
          "id": "MDRW2100036958.1.14",
          "form": "괜히 나만 안좋을까봐 걱정은 좀 돼요..ㅎ",
          "original_form": "괜히 나만 안좋을까봐 걱정은 좀 돼요..ㅎ",
          "speaker_id": "1",
          "time": "20210831 15:58"
        },
        {
          "id": "MDRW2100036958.1.15",
          "form": "그러니깐요ㅠㅠ 평소때도 일과육아에 피로감이 많이 쌓이기도한게 영향도있을꺼같아요ㅠㅠ 만성피로ㅠ",
          "original_form": "그러니깐요ㅠㅠ 평소때도 일과육아에 피로감이 많이 쌓이기도한게 영향도있을꺼같아요ㅠㅠ 만성피로ㅠ",
          "speaker_id": "2",
          "time": "20210831 15:58"
        },
        {
          "id": "MDRW2100036958.1.16",
          "form": "그래서 맞기 전에 체력관리 하려고 당분간 다이어트 빡세가 안하고 있어요",
          "original_form": "그래서 맞기 전에 체력관리 하려고 당분간 다이어트 빡세가 안하고 있어요",
          "speaker_id": "1",
          "time": "20210831 15:59"
        },
        {
          "id": "MDRW2100036958.1.17",
          "form": "혹시 몸이 허해지면 안되니까요",
          "original_form": "혹시 몸이 허해지면 안되니까요",
          "speaker_id": "1",
          "time": "20210831 15:59"
        },
        {
          "id": "MDRW2100036958.1.18",
          "form": "몸에서 어떤 반응을 할지는 모르니 ㅠㅠ",
          "original_form": "몸에서 어떤 반응을 할지는 모르니 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 16:00"
        },
        {
          "id": "MDRW2100036958.1.19",
          "form": "기초체력관리도 필요할듯해요!!!",
          "original_form": "기초체력관리도 필요할듯해요!!!",
          "speaker_id": "2",
          "time": "20210831 16:00"
        },
        {
          "id": "MDRW2100036958.1.20",
          "form": "맞아요.. 평소에 안먹던거 갑자기 먹지 않으려구요",
          "original_form": "맞아요.. 평소에 안먹던거 갑자기 먹지 않으려구요",
          "speaker_id": "1",
          "time": "20210831 16:04"
        },
        {
          "id": "MDRW2100036958.1.21",
          "form": "어느거에 반응해서 몸이 이상해졌는지 모르니까요",
          "original_form": "어느거에 반응해서 몸이 이상해졌는지 모르니까요",
          "speaker_id": "1",
          "time": "20210831 16:04"
        },
        {
          "id": "MDRW2100036958.1.22",
          "form": "그런부분도 조심해야겠네요!",
          "original_form": "그런부분도 조심해야겠네요!",
          "speaker_id": "2",
          "time": "20210831 16:16"
        },
        {
          "id": "MDRW2100036958.1.23",
          "form": "알러지가 날수도있고ㅜㅠ",
          "original_form": "알러지가 날수도있고ㅜㅠ",
          "speaker_id": "2",
          "time": "20210831 16:16"
        },
        {
          "id": "MDRW2100036958.1.24",
          "form": "네 백신때문인지 알려면 최대한 평소와 똑같아야 할 같아요",
          "original_form": "네 백신때문인지 알려면 최대한 평소와 똑같아야 할 같아요",
          "speaker_id": "1",
          "time": "20210831 17:03"
        },
        {
          "id": "MDRW2100036958.1.25",
          "form": "생각없었는데주의해야겠어요ㅠㅠ",
          "original_form": "생각없었는데주의해야겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 20:22"
        }
      ]
    }
