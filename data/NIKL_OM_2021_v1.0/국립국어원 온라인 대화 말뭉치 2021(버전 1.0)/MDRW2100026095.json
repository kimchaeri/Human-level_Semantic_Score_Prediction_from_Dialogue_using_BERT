      "utterance": [
        {
          "id": "MDRW2100026095.1.1",
          "form": "맞아 근데 제대로 키운 기억이 없다... 이것도",
          "original_form": "맞아 근데 제대로 키운 기억이 없다... 이것도",
          "speaker_id": "2",
          "time": "20210901 19:41"
        },
        {
          "id": "MDRW2100026095.1.2",
          "form": "곤충말고 동물은 키워봤어?",
          "original_form": "곤충말고 동물은 키워봤어?",
          "speaker_id": "2",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100026095.1.3",
          "form": "생각해보니 나도 정말 많이 키운 것 같아",
          "original_form": "생각해보니 나도 정말 많이 키운 것 같아",
          "speaker_id": "1",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100026095.1.4",
          "form": "나 토끼도 키워봤고 앵무새도 키워봤어",
          "original_form": "나 토끼도 키워봤고 앵무새도 키워봤어",
          "speaker_id": "1",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100026095.1.5",
          "form": "오 그래?",
          "original_form": "오 그래?",
          "speaker_id": "2",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100026095.1.6",
          "form": "와 대박인데",
          "original_form": "와 대박인데",
          "speaker_id": "2",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100026095.1.7",
          "form": "대부분 햄스터 키워보지 않나",
          "original_form": "대부분 햄스터 키워보지 않나",
          "speaker_id": "2",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100026095.1.8",
          "form": "토끼 앵무새는 신기하다",
          "original_form": "토끼 앵무새는 신기하다",
          "speaker_id": "2",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100026095.1.9",
          "form": "주변에 별로 없긴 하더라",
          "original_form": "주변에 별로 없긴 하더라",
          "speaker_id": "1",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100026095.1.10",
          "form": "난 오히랴 핸스터는 안 키워봤어",
          "original_form": "난 오히랴 핸스터는 안 키워봤어",
          "speaker_id": "1",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100026095.1.11",
          "form": "나도 안키워봤다... 햄스터",
          "original_form": "나도 안키워봤다... 햄스터",
          "speaker_id": "2",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100026095.1.12",
          "form": "친구네 햄스터는 구경해봤는데",
          "original_form": "친구네 햄스터는 구경해봤는데",
          "speaker_id": "2",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100026095.1.13",
          "form": "토끼, 앵무새, 고양이, 강아지, 물고기, 닭, 오리, 공작 등등 키워봤어",
          "original_form": "토끼, 앵무새, 고양이, 강아지, 물고기, 닭, 오리, 공작 등등 키워봤어",
          "speaker_id": "1",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100026095.1.14",
          "form": "공작은... 약간 임보느낌?",
          "original_form": "공작은... 약간 임보느낌?",
          "speaker_id": "1",
          "time": "20210901 19:43"
        },
        {
          "id": "MDRW2100026095.1.15",
          "form": "와 ㅋㅋㅋㅋ 뭐야",
          "original_form": "와 ㅋㅋㅋㅋ 뭐야",
          "speaker_id": "2",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026095.1.16",
          "form": "엄청 많이 키웠네",
          "original_form": "엄청 많이 키웠네",
          "speaker_id": "2",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026095.1.17",
          "form": "나 어릴 때 살짝 있다가 갔어",
          "original_form": "나 어릴 때 살짝 있다가 갔어",
          "speaker_id": "1",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026095.1.18",
          "form": "멋지다",
          "original_form": "멋지다",
          "speaker_id": "2",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026095.1.19",
          "form": "시골에 살아서 동물농장 형식으로 키웠지ㅋㅋㅋㅋㅋ",
          "original_form": "시골에 살아서 동물농장 형식으로 키웠지ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026095.1.20",
          "form": "뭔지 알 거 같아",
          "original_form": "뭔지 알 거 같아",
          "speaker_id": "2",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026095.1.21",
          "form": "개가 아기 낳는 것도 직접 받아보고",
          "original_form": "개가 아기 낳는 것도 직접 받아보고",
          "speaker_id": "1",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026095.1.22",
          "form": "앵무새 말할 줄 알았었어??",
          "original_form": "앵무새 말할 줄 알았었어??",
          "speaker_id": "2",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026095.1.23",
          "form": "말 열심히 가르쳤지만... 자기 할 말만 하더라",
          "original_form": "말 열심히 가르쳤지만... 자기 할 말만 하더라",
          "speaker_id": "1",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100026095.1.24",
          "form": "와... 되게 체험관 간 거 같아",
          "original_form": "와... 되게 체험관 간 거 같아",
          "speaker_id": "2",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100026095.1.25",
          "form": "ㅋㅋ 그래도 할 순 있단 거네",
          "original_form": "ㅋㅋ 그래도 할 순 있단 거네",
          "speaker_id": "2",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100026095.1.26",
          "form": "신기하다",
          "original_form": "신기하다",
          "speaker_id": "2",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100026095.1.27",
          "form": "아 할아버지 아시는 분이 그쪽에 일하셔서 더 많이 키워봤어?",
          "original_form": "아 할아버지 아시는 분이 그쪽에 일하셔서 더 많이 키워봤어?",
          "speaker_id": "1",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100026095.1.28",
          "form": "!느낌표야",
          "original_form": "!느낌표야",
          "speaker_id": "1",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100026095.1.29",
          "form": "ㅋㅋㅋㅋㅋ엉엉",
          "original_form": "ㅋㅋㅋㅋㅋ엉엉",
          "speaker_id": "2",
          "time": "20210901 19:45"
        }
      ]
    }
