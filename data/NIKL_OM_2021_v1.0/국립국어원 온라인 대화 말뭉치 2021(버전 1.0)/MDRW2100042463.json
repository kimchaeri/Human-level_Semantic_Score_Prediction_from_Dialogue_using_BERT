      "utterance": [
        {
          "id": "MDRW2100042463.1.1",
          "form": "우리 name3은 지금도 크록스 신는데 ㅋㅋㅋㅋㅋ",
          "original_form": "우리 &name3&은 지금도 크록스 신는데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042463.1.2",
          "form": "우리 엄마가",
          "original_form": "우리 엄마가",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042463.1.3",
          "form": "밖에 비 오는데",
          "original_form": "밖에 비 오는데",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042463.1.4",
          "form": "샌들 신겨 보내냐고",
          "original_form": "샌들 신겨 보내냐고",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042463.1.5",
          "form": "그래서 어차피 운동화씨 네 나 샌들씨 네 나",
          "original_form": "그래서 어차피 운동화씨 네 나 샌들씨 네 나",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042463.1.6",
          "form": "크록 수가 쟁 편하지 언니",
          "original_form": "크록 수가 쟁 편하지 언니",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042463.1.7",
          "form": "운동화 신으나 샌들 신으나",
          "original_form": "운동화 신으나 샌들 신으나",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042463.1.8",
          "form": "크록스 뭐 물하면 헹구면 되는데",
          "original_form": "크록스 뭐 물하면 헹구면 되는데",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042463.1.9",
          "form": "젖는 건 똑같다고",
          "original_form": "젖는 건 똑같다고",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042463.1.10",
          "form": "젖는 건 똑같으니",
          "original_form": "젖는 건 똑같으니",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042463.1.11",
          "form": "그냥 가서 젖으면",
          "original_form": "그냥 가서 젖으면",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042463.1.12",
          "form": "역시",
          "original_form": "역시",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042463.1.13",
          "form": "양말 벗고 있으라고",
          "original_form": "양말 벗고 있으라고",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042463.1.14",
          "form": "나도 그래서 비오면 크 럭 쑤신 게 보냈는데",
          "original_form": "나도 그래서 비오면 크 럭 쑤신 게 보냈는데",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042463.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042463.1.16",
          "form": "그니까 ㅋㅋㅋㅋㅋㅋㄱㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋㅋㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042463.1.17",
          "form": "몰래 딸래미 들은 다 장하 신고 온대요",
          "original_form": "몰래 딸래미 들은 다 장하 신고 온대요",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100042463.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210906 22:18"
        },
        {
