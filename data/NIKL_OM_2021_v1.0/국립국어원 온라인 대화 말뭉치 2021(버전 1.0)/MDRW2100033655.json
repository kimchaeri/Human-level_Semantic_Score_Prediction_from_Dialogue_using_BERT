      "utterance": [
        {
          "id": "MDRW2100033655.1.1",
          "form": "좋아하는 프로그램 있으세요?",
          "original_form": "좋아하는 프로그램 있으세요?",
          "speaker_id": "1",
          "time": "20210824 21:01"
        },
        {
          "id": "MDRW2100033655.1.2",
          "form": "슬의생 재밌게보고있어요~",
          "original_form": "슬의생 재밌게보고있어요~",
          "speaker_id": "2",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100033655.1.3",
          "form": "오 슬의생 재밌죠",
          "original_form": "오 슬의생 재밌죠",
          "speaker_id": "1",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100033655.1.4",
          "form": "예능은요?",
          "original_form": "예능은요?",
          "speaker_id": "1",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100033655.1.5",
          "form": "체인지 데이트",
          "original_form": "체인지 데이트",
          "speaker_id": "2",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100033655.1.6",
          "form": "그거 열심히보고있어요",
          "original_form": "그거 열심히보고있어요",
          "speaker_id": "2",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100033655.1.7",
          "form": "그건 어떤 건가요??!",
          "original_form": "그건 어떤 건가요??!",
          "speaker_id": "1",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100033655.1.8",
          "form": "처음 듣네요",
          "original_form": "처음 듣네요",
          "speaker_id": "1",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100033655.1.9",
          "form": "이거 진짜 화끈해요",
          "original_form": "이거 진짜 화끈해요",
          "speaker_id": "2",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100033655.1.10",
          "form": "이별직전 연인들 모아서.. 체인지 데이트..하는거예요",
          "original_form": "이별직전 연인들 모아서.. 체인지 데이트..하는거예요",
          "speaker_id": "2",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100033655.1.11",
          "form": "오.. 대받 뭔가 처음 듣는 예능이네요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "오.. 대받 뭔가 처음 듣는 예능이네요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100033655.1.12",
          "form": "서로 바꿔 사귀는건가요",
          "original_form": "서로 바꿔 사귀는건가요",
          "speaker_id": "1",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100033655.1.13",
          "form": "데이트를 바꿔서요!",
          "original_form": "데이트를 바꿔서요!",
          "speaker_id": "2",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100033655.1.14",
          "form": "근데 막 흔들린다그러고..",
          "original_form": "근데 막 흔들린다그러고..",
          "speaker_id": "2",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100033655.1.15",
          "form": "설렜다 그러고..",
          "original_form": "설렜다 그러고..",
          "speaker_id": "2",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100033655.1.16",
          "form": "서로 기싸움하고..",
          "original_form": "서로 기싸움하고..",
          "speaker_id": "2",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100033655.1.17",
          "form": "오 듣기만해도 마라맛이네요",
          "original_form": "오 듣기만해도 마라맛이네요",
          "speaker_id": "1",
          "time": "20210824 23:10"
        },
        {
          "id": "MDRW2100033655.1.18",
          "form": "근데 피곤 할 거 같은ㅋㅋㅋㅋㅋ",
          "original_form": "근데 피곤 할 거 같은ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:10"
        },
        {
          "id": "MDRW2100033655.1.19",
          "form": "일주일간하는대",
          "original_form": "일주일간하는대",
          "speaker_id": "2",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100033655.1.20",
          "form": "진짜 회차가 많아요 ㅋㅋㅋㅋㅋ",
          "original_form": "진짜 회차가 많아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100033655.1.21",
          "form": "뽕을 지대로뽑은",
          "original_form": "뽕을 지대로뽑은",
          "speaker_id": "2",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100033655.1.22",
          "form": "한번 봐야겠어요",
          "original_form": "한번 봐야겠어요",
          "speaker_id": "1",
          "time": "20210824 23:12"
        },
        {
          "id": "MDRW2100033655.1.23",
          "form": "저 그런거 좋아해요",
          "original_form": "저 그런거 좋아해요",
          "speaker_id": "1",
          "time": "20210824 23:12"
        },
        {
          "id": "MDRW2100033655.1.24",
          "form": "보고 같이 얘기해주세요 ㅋㅋㅋㅋ",
          "original_form": "보고 같이 얘기해주세요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 04:20"
        },
        {
          "id": "MDRW2100033655.1.25",
          "form": "진짜 재밌어요",
          "original_form": "진짜 재밌어요",
          "speaker_id": "2",
          "time": "20210825 04:20"
        },
        {
          "id": "MDRW2100033655.1.26",
          "form": "ㅋㅋㅋㅋㅋ 네 알려드릴게요!",
          "original_form": "ㅋㅋㅋㅋㅋ 네 알려드릴게요!",
          "speaker_id": "1",
          "time": "20210825 07:45"
        }
      ]
    }
