      "utterance": [
        {
          "id": "MDRW2100036355.1.1",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100036355.1.2",
          "form": "바텐더님이 조각하신것같은",
          "original_form": "바텐더님이 조각하신것같은",
          "speaker_id": "1",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100036355.1.3",
          "form": "그런 왕 동그란 얼음이 나와용",
          "original_form": "그런 왕 동그란 얼음이 나와용",
          "speaker_id": "1",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100036355.1.4",
          "form": "와!! 쩌네요 ㅋㅋ 전 매번 틀에 얼리기 바쁜데",
          "original_form": "와!! 쩌네요 ㅋㅋ 전 매번 틀에 얼리기 바쁜데",
          "speaker_id": "2",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100036355.1.5",
          "form": "저도 얼음 틀에 얼려먹어용ㅋㅋㅋㅋ",
          "original_form": "저도 얼음 틀에 얼려먹어용ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100036355.1.6",
          "form": "토마토쥬스하셧으요?ㅎㅎ",
          "original_form": "토마토쥬스하셧으요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100036355.1.7",
          "form": "갈아놓고",
          "original_form": "갈아놓고",
          "speaker_id": "1",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100036355.1.8",
          "form": "아직 못먹고있어욬ㅋㅋㅋㅋ",
          "original_form": "아직 못먹고있어욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100036355.1.9",
          "form": "토마토데쳐서 껍질베겨서 하면 더 좋다는데",
          "original_form": "토마토데쳐서 껍질베겨서 하면 더 좋다는데",
          "speaker_id": "2",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100036355.1.10",
          "form": "그 상태로 뽑아서",
          "original_form": "그 상태로 뽑아서",
          "speaker_id": "1",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100036355.1.11",
          "form": "전그렇게해본적 없어요 ㅋㅋㅋ",
          "original_form": "전그렇게해본적 없어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100036355.1.12",
          "form": "냉동실에 넣어놨어용",
          "original_form": "냉동실에 넣어놨어용",
          "speaker_id": "1",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100036355.1.13",
          "form": "아 저도 귀차니즘이 심해서ㅋㅋㅋ",
          "original_form": "아 저도 귀차니즘이 심해서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:52"
        },
        {
          "id": "MDRW2100036355.1.14",
          "form": "사시면서 드신 음식중에",
          "original_form": "사시면서 드신 음식중에",
          "speaker_id": "1",
          "time": "20210830 21:52"
        },
        {
          "id": "MDRW2100036355.1.15",
          "form": "가장 매니악한 음식이 뭐였나용?",
          "original_form": "가장 매니악한 음식이 뭐였나용?",
          "speaker_id": "1",
          "time": "20210830 21:52"
        },
        {
          "id": "MDRW2100036355.1.16",
          "form": "매니악한",
          "original_form": "매니악한",
          "speaker_id": "2",
          "time": "20210830 21:52"
        },
        {
          "id": "MDRW2100036355.1.17",
          "form": "네네 남들이 보기엔 으 저걸 먹냐 싶은ㅋㅋㅋㅋ",
          "original_form": "네네 남들이 보기엔 으 저걸 먹냐 싶은ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:52"
        },
        {
          "id": "MDRW2100036355.1.18",
          "form": "아 !",
          "original_form": "아 !",
          "speaker_id": "2",
          "time": "20210830 21:53"
        },
        {
          "id": "MDRW2100036355.1.19",
          "form": "궁금하네용ㅋㅋㅌ",
          "original_form": "궁금하네용ㅋㅋㅌ",
          "speaker_id": "1",
          "time": "20210830 21:53"
        },
        {
          "id": "MDRW2100036355.1.20",
          "form": "전 막상없는거같아여ㅠㅠ",
          "original_form": "전 막상없는거같아여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 21:53"
        },
        {
          "id": "MDRW2100036355.1.21",
