        },
        {
          "id": "MDRW2100033956.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 00:47"
        },
        {
          "id": "MDRW2100033956.1.3",
          "form": "최근에 구매한 물건 있으세요?",
          "original_form": "최근에 구매한 물건 있으세요?",
          "speaker_id": "2",
          "time": "20210826 03:29"
        },
        {
          "id": "MDRW2100033956.1.4",
          "form": "이제 하나 살려구요",
          "original_form": "이제 하나 살려구요",
          "speaker_id": "1",
          "time": "20210826 03:30"
        },
        {
          "id": "MDRW2100033956.1.5",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "2",
          "time": "20210826 03:30"
        },
        {
          "id": "MDRW2100033956.1.6",
          "form": "h패션몰에서 옷이요",
          "original_form": "h패션몰에서 옷이요",
          "speaker_id": "1",
          "time": "20210826 03:30"
        },
        {
          "id": "MDRW2100033956.1.7",
          "form": "뭐사신거 있으신가요?",
          "original_form": "뭐사신거 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 03:31"
        },
        {
          "id": "MDRW2100033956.1.8",
          "form": "가을 옷인가요",
          "original_form": "가을 옷인가요",
          "speaker_id": "2",
          "time": "20210826 03:31"
        },
        {
          "id": "MDRW2100033956.1.9",
          "form": "브리타 정수기 샀어요",
          "original_form": "브리타 정수기 샀어요",
          "speaker_id": "2",
          "time": "20210826 03:31"
        },
        {
          "id": "MDRW2100033956.1.10",
          "form": "아직 미정인데 적립금맞춰서 대충살려구요ㅎ",
          "original_form": "아직 미정인데 적립금맞춰서 대충살려구요ㅎ",
          "speaker_id": "1",
          "time": "20210826 03:31"
        },
        {
          "id": "MDRW2100033956.1.11",
          "form": "브리타는 첨들어봐요",
          "original_form": "브리타는 첨들어봐요",
          "speaker_id": "1",
          "time": "20210826 03:32"
        },
        {
          "id": "MDRW2100033956.1.12",
          "form": "옷좋아하세요?",
          "original_form": "옷좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 03:32"
        },
        {
          "id": "MDRW2100033956.1.13",
          "form": "아니요 요즘 먹을거사요",
          "original_form": "아니요 요즘 먹을거사요",
          "speaker_id": "1",
          "time": "20210826 03:32"
        },
        {
          "id": "MDRW2100033956.1.14",
          "form": "옷찾는거 귀찮아서",
          "original_form": "옷찾는거 귀찮아서",
          "speaker_id": "1",
          "time": "20210826 03:32"
        },
        {
          "id": "MDRW2100033956.1.15",
          "form": "밀키트 잘나오더라고요",
          "original_form": "밀키트 잘나오더라고요",
          "speaker_id": "2",
          "time": "20210826 03:33"
        },
        {
          "id": "MDRW2100033956.1.16",
          "form": "네 요즘 신기한거 많던데요",
          "original_form": "네 요즘 신기한거 많던데요",
          "speaker_id": "1",
          "time": "20210826 03:33"
        },
        {
          "id": "MDRW2100033956.1.17",
          "form": "사보셨나요?",
          "original_form": "사보셨나요?",
          "speaker_id": "2",
          "time": "20210826 03:34"
        },
        {
          "id": "MDRW2100033956.1.18",
          "form": "부대찌개 사봤어요",
          "original_form": "부대찌개 사봤어요",
          "speaker_id": "1",
          "time": "20210826 03:34"
        }
      ]
