      "utterance": [
        {
          "id": "MDRW2100029629.1.1",
          "form": "무슨겜",
          "original_form": "무슨겜",
          "speaker_id": "2",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029629.1.2",
          "form": "좋아하신댔죠",
          "original_form": "좋아하신댔죠",
          "speaker_id": "2",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029629.1.3",
          "form": "땅따먹기",
          "original_form": "땅따먹기",
          "speaker_id": "2",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029629.1.4",
          "form": "해보셨나요",
          "original_form": "해보셨나요",
          "speaker_id": "2",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029629.1.5",
          "form": "네 해봤죠 ㅋㅋㅋㅋ",
          "original_form": "네 해봤죠 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029629.1.6",
          "form": "꽃키우기랑",
          "original_form": "꽃키우기랑",
          "speaker_id": "2",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029629.1.7",
          "form": "땅따먹기 선긋는거 말씀하시는거죠?",
          "original_form": "땅따먹기 선긋는거 말씀하시는거죠?",
          "speaker_id": "1",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029629.1.8",
          "form": "경영겜은 해보셨나요",
          "original_form": "경영겜은 해보셨나요",
          "speaker_id": "2",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029629.1.9",
          "form": "네네 ㅋㅋㅋ 선긋는거 ",
          "original_form": "네네 ㅋㅋㅋ 선긋는거 ",
          "speaker_id": "2",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029629.1.10",
          "form": "오락실게임 좋죠",
          "original_form": "오락실게임 좋죠",
          "speaker_id": "1",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029629.1.11",
          "form": "제취향입니다",
          "original_form": "제취향입니다",
          "speaker_id": "2",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029629.1.12",
          "form": "어릴때 돈좀 썼었는데",
          "original_form": "어릴때 돈좀 썼었는데",
          "speaker_id": "2",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029629.1.13",
          "form": "배경이 부끄럽죠",
          "original_form": "배경이 부끄럽죠",
          "speaker_id": "1",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029629.1.14",
          "form": "건전한배경이 나오면좋겠네요",
          "original_form": "건전한배경이 나오면좋겠네요",
          "speaker_id": "2",
          "time": "20210903 11:04"
        },
        {
          "id": "MDRW2100029629.1.15",
          "form": "수영복여자가 우ㅡ합니다",
          "original_form": "수영복여자가 우ㅡ합니다",
          "speaker_id": "1",
          "time": "20210903 11:04"
        },
        {
          "id": "MDRW2100029629.1.16",
          "form": "요즘은 그런겜별로 없더라구요",
          "original_form": "요즘은 그런겜별로 없더라구요",
          "speaker_id": "2",
          "time": "20210903 11:04"
        },
        {
          "id": "MDRW2100029629.1.17",
          "form": "남자 타겟이군요",
          "original_form": "남자 타겟이군요",
          "speaker_id": "1",
          "time": "20210903 11:04"
        },
        {
          "id": "MDRW2100029629.1.18",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210903 11:04"
        },
        {
          "id": "MDRW2100029629.1.19",
          "form": "남자 타겟 게임 넘 싫어요",
          "original_form": "남자 타겟 게임 넘 싫어요",
          "speaker_id": "2",
          "time": "20210903 11:04"
        },
        {
          "id": "MDRW2100029629.1.20",
          "form": "건전한배경게임 하고싶군요",
          "original_form": "건전한배경게임 하고싶군요",
          "speaker_id": "1",
          "time": "20210903 11:04"
        },
        {
          "id": "MDRW2100029629.1.21",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210903 11:04"
        },
        {
          "id": "MDRW2100029629.1.22",
          "form": "근육남 배경이라거나",
          "original_form": "근육남 배경이라거나",
          "speaker_id": "1",
          "time": "20210903 11:04"
        },
        {
          "id": "MDRW2100029629.1.23",
          "form": "얼굴맛집배경",
          "original_form": "얼굴맛집배경",
          "speaker_id": "2",
          "time": "20210903 11:04"
        },
        {
          "id": "MDRW2100029629.1.24",
          "form": "나도 눈요기하자",
          "original_form": "나도 눈요기하자",
