      "utterance": [
        {
          "id": "MDRW2100003044.1.1",
          "form": "집안일은 힘드시지요?",
          "original_form": "집안일은 힘드시지요?",
          "speaker_id": "1",
          "time": "20210823 10:59"
        },
        {
          "id": "MDRW2100003044.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210823 10:59"
        },
        {
          "id": "MDRW2100003044.1.3",
          "form": "바쁠땐 힘들어요",
          "original_form": "바쁠땐 힘들어요",
          "speaker_id": "2",
          "time": "20210823 10:59"
        },
        {
          "id": "MDRW2100003044.1.4",
          "form": "여러가지 해야될일이 많을때요 ㅜㅜ",
          "original_form": "여러가지 해야될일이 많을때요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 10:59"
        },
        {
          "id": "MDRW2100003044.1.5",
          "form": "집안일 뭐가 젤 힘드세요?",
          "original_form": "집안일 뭐가 젤 힘드세요?",
          "speaker_id": "2",
          "time": "20210823 11:00"
        },
        {
          "id": "MDRW2100003044.1.6",
          "form": "전 음쓰버리기요ㅋㅋㅋ",
          "original_form": "전 음쓰버리기요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 11:00"
        },
        {
          "id": "MDRW2100003044.1.7",
          "form": "싫아하는거나 힘든일요?",
          "original_form": "싫아하는거나 힘든일요?",
          "speaker_id": "2",
          "time": "20210823 11:00"
        },
        {
          "id": "MDRW2100003044.1.8",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210823 11:00"
        },
        {
          "id": "MDRW2100003044.1.9",
          "form": "여름이라 오래도 못놔두고ㅠ",
          "original_form": "여름이라 오래도 못놔두고ㅠ",
          "speaker_id": "1",
          "time": "20210823 11:00"
        },
        {
          "id": "MDRW2100003044.1.10",
          "form": "이해돼요 ㅜㅜ윽 저도 싫어요",
          "original_form": "이해돼요 ㅜㅜ윽 저도 싫어요",
          "speaker_id": "2",
          "time": "20210823 11:00"
        },
        {
          "id": "MDRW2100003044.1.11",
          "form": "냄새나니까요  왜이리 버리러 가기 싫은가 몰라요",
          "original_form": "냄새나니까요  왜이리 버리러 가기 싫은가 몰라요",
          "speaker_id": "2",
          "time": "20210823 11:01"
        },
        {
          "id": "MDRW2100003044.1.12",
          "form": "직접 하세요?",
          "original_form": "직접 하세요?",
          "speaker_id": "1",
          "time": "20210823 11:01"
        },
        {
          "id": "MDRW2100003044.1.13",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210823 11:01"
        },
        {
          "id": "MDRW2100003044.1.14",
          "form": "아무도 안하네요 ㅡㅡ",
          "original_form": "아무도 안하네요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210823 11:01"
        },
        {
          "id": "MDRW2100003044.1.15",
          "form": "ㅜㅠ 버티는 사람이 결국 하게 되나요ㅠ",
          "original_form": "ㅜㅠ 버티는 사람이 결국 하게 되나요ㅠ",
          "speaker_id": "1",
          "time": "20210823 11:02"
        },
        {
          "id": "MDRW2100003044.1.16",
          "form": "혼자하게돼요..",
          "original_form": "혼자하게돼요..",
          "speaker_id": "2",
          "time": "20210823 11:02"
        },
        {
          "id": "MDRW2100003044.1.17",
          "form": "네 그렇죠 ㅡㅡ",
          "original_form": "네 그렇죠 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210823 11:02"
        },
        {
          "id": "MDRW2100003044.1.18",
          "form": "전 설거지 싫어요",
          "original_form": "전 설거지 싫어요",
          "speaker_id": "2",
          "time": "20210823 11:02"
        },
        {
          "id": "MDRW2100003044.1.19",
          "form": "다먹고 쉬고싶은데 일이 생긴거잖아요 ㅜㅜ",
          "original_form": "다먹고 쉬고싶은데 일이 생긴거잖아요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 11:02"
        },
        {
          "id": "MDRW2100003044.1.20",
          "form": "아 맞아요ㅠ",
          "original_form": "아 맞아요ㅠ",
          "speaker_id": "1",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100003044.1.21",
          "form": "식기세척기 고민되네요 ㅡㅡ",
          "original_form": "식기세척기 고민되네요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100003044.1.22",
          "form": "설거지도 다 하세요?",
          "original_form": "설거지도 다 하세요?",
          "speaker_id": "1",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100003044.1.23",
          "form": "식세기 진짜 좋대요",
          "original_form": "식세기 진짜 좋대요",
          "speaker_id": "1",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100003044.1.24",
          "form": "네 ㅜㅜ",
          "original_form": "네 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100003044.1.25",
          "form": "오 그래요? 좀 사볼까봐요",
          "original_form": "오 그래요? 좀 사볼까봐요",
          "speaker_id": "2",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100003044.1.26",
          "form": "혼자하는데",
          "original_form": "혼자하는데",
          "speaker_id": "2",
          "time": "20210823 11:03"
        },
        {
          "id": "MDRW2100003044.1.27",
          "form": "로봇청소기처럼",
          "original_form": "로봇청소기처럼",
          "speaker_id": "1",
          "time": "20210823 11:04"
        },
        {
          "id": "MDRW2100003044.1.28",
          "form": "가끔 아주 가끔 도와줘요",
          "original_form": "가끔 아주 가끔 도와줘요",
          "speaker_id": "2",
          "time": "20210823 11:04"
        },
        {
          "id": "MDRW2100003044.1.29",
          "form": "없었던 때가 상상이 안된대요ㅋ",
          "original_form": "없었던 때가 상상이 안된대요ㅋ",
          "speaker_id": "1",
          "time": "20210823 11:04"
        },
        {
          "id": "MDRW2100003044.1.30",
          "form": "잘도와주시나봐요?",
          "original_form": "잘도와주시나봐요?",
          "speaker_id": "2",
          "time": "20210823 11:04"
        },
        {
          "id": "MDRW2100003044.1.31",
          "form": "네 제가 합니다ㅋㅋㅋ",
          "original_form": "네 제가 합니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 11:04"
        }
      ]
    }
