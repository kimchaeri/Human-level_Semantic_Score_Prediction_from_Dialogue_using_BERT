      "utterance": [
        {
          "id": "MDRW2100042041.1.1",
          "form": "눈수술 무섭다구요 다른 수술은 괜찮은데",
          "original_form": "눈수술 무섭다구요 다른 수술은 괜찮은데",
          "speaker_id": "2",
          "time": "20210906 19:14"
        },
        {
          "id": "MDRW2100042041.1.2",
          "form": "차라리 지방흡입 했으면 좋겠어요 ㅋ",
          "original_form": "차라리 지방흡입 했으면 좋겠어요 ㅋ",
          "speaker_id": "2",
          "time": "20210906 19:14"
        },
        {
          "id": "MDRW2100042041.1.3",
          "form": "참나 알아서 잘 빼야지",
          "original_form": "참나 알아서 잘 빼야지",
          "speaker_id": "1",
          "time": "20210906 19:14"
        },
        {
          "id": "MDRW2100042041.1.4",
          "form": "다이어트 귀찮구 힘들단말이에요 힝",
          "original_form": "다이어트 귀찮구 힘들단말이에요 힝",
          "speaker_id": "2",
          "time": "20210906 19:15"
        },
        {
          "id": "MDRW2100042041.1.5",
          "form": "그래도 잘 운동해서 빼는게 몸에도 좋아 장기적으로",
          "original_form": "그래도 잘 운동해서 빼는게 몸에도 좋아 장기적으로",
          "speaker_id": "1",
          "time": "20210906 19:16"
        },
        {
          "id": "MDRW2100042041.1.6",
          "form": "그건 그렇긴 하지만 ㅠ",
          "original_form": "그건 그렇긴 하지만 ㅠ",
          "speaker_id": "2",
          "time": "20210906 19:16"
        },
        {
          "id": "MDRW2100042041.1.7",
          "form": "라식도 안하고",
          "original_form": "라식도 안하고",
          "speaker_id": "1",
          "time": "20210906 19:16"
        },
        {
          "id": "MDRW2100042041.1.8",
          "form": "라식도 눈수술인데요 무섭다구요",
          "original_form": "라식도 눈수술인데요 무섭다구요",
          "speaker_id": "2",
          "time": "20210906 19:16"
        },
        {
          "id": "MDRW2100042041.1.9",
          "form": "뭐가 무서워 잘되라고 하는건데",
          "original_form": "뭐가 무서워 잘되라고 하는건데",
          "speaker_id": "1",
          "time": "20210906 19:17"
        },
        {
          "id": "MDRW2100042041.1.10",
          "form": "엄마가 개어있을때 눈수술 안해서 그래요",
          "original_form": "엄마가 개어있을때 눈수술 안해서 그래요",
          "speaker_id": "2",
          "time": "20210906 19:17"
        },
        {
          "id": "MDRW2100042041.1.11",
          "form": "깨어있을때",
          "original_form": "깨어있을때",
          "speaker_id": "2",
          "time": "20210906 19:17"
        },
        {
          "id": "MDRW2100042041.1.12",
          "form": "십년전부터 그얘기만 하더라",
          "original_form": "십년전부터 그얘기만 하더라",
          "speaker_id": "1",
          "time": "20210906 19:17"
        },
        {
          "id": "MDRW2100042041.1.13",
          "form": "십년전부터 계속 무서웠어서 그래요 ㅇㅅ ㅠ",
          "original_form": "십년전부터 계속 무서웠어서 그래요 ㅇㅅ ㅠ",
          "speaker_id": "2",
          "time": "20210906 19:18"
        },
        {
          "id": "MDRW2100042041.1.14",
          "form": "그래도 이뻐지려면 쌍커풀도 만들고 살도 빼고 하는게 좋아",
          "original_form": "그래도 이뻐지려면 쌍커풀도 만들고 살도 빼고 하는게 좋아",
          "speaker_id": "1",
          "time": "20210906 19:18"
        },
        {
          "id": "MDRW2100042041.1.15",
          "form": "성형수술이 미용을 위해 변질된게 너무 짱나요",
          "original_form": "성형수술이 미용을 위해 변질된게 너무 짱나요",
          "speaker_id": "2",
          "time": "20210906 19:18"
        },
        {
          "id": "MDRW2100042041.1.16",
          "form": "원래 성형은 예뻐지려고 하는거잖아",
          "original_form": "원래 성형은 예뻐지려고 하는거잖아",
          "speaker_id": "1",
          "time": "20210906 19:19"
        },
        {
          "id": "MDRW2100042041.1.17",
          "form": "의료용 목적이면 괜찮은데 흑흑",
          "original_form": "의료용 목적이면 괜찮은데 흑흑",
          "speaker_id": "2",
          "time": "20210906 19:19"
        },
        {
          "id": "MDRW2100042041.1.18",
          "form": "저같은 사람들한테도 강요하고 그러잖아요 슬퍼",
          "original_form": "저같은 사람들한테도 강요하고 그러잖아요 슬퍼",
          "speaker_id": "2",
          "time": "20210906 19:20"
        },
        {
          "id": "MDRW2100042041.1.19",
          "form": "알아요",
          "original_form": "알아요",
          "speaker_id": "2",
          "time": "20210906 19:21"
        },
        {
          "id": "MDRW2100042041.1.20",
          "form": "너 위해서 얘기하는거 알지?",
          "original_form": "너 위해서 얘기하는거 알지?",
          "speaker_id": "1",
          "time": "20210906 19:21"
        },
        {
          "id": "MDRW2100042041.1.21",
          "form": "엄마가 사랑해서 그래 억지로 하라는건 아니구",
          "original_form": "엄마가 사랑해서 그래 억지로 하라는건 아니구",
          "speaker_id": "1",
          "time": "20210906 19:21"
        },
        {
          "id": "MDRW2100042041.1.22",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 19:21"
        },
        {
          "id": "MDRW2100042041.1.23",
          "form": "잘됐으면 좋겠으니까 하는 얘기야",
          "original_form": "잘됐으면 좋겠으니까 하는 얘기야",
          "speaker_id": "1",
          "time": "20210906 19:21"
        }
      ]
    }
