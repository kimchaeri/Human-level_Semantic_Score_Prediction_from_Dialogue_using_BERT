      "utterance": [
        {
          "id": "MDRW2100005218.1.1",
          "form": "뭐야 연애가주제야 왜",
          "original_form": "뭐야 연애가주제야 왜",
          "speaker_id": "2",
          "time": "20210822 11:24"
        },
        {
          "id": "MDRW2100005218.1.2",
          "form": "난 결혼 생각이 없다",
          "original_form": "난 결혼 생각이 없다",
          "speaker_id": "1",
          "time": "20210822 11:25"
        },
        {
          "id": "MDRW2100005218.1.3",
          "form": "왜 없어 ㅋㅋㅋ",
          "original_form": "왜 없어 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:26"
        },
        {
          "id": "MDRW2100005218.1.4",
          "form": "나는 결혼 빨리 하고 싶었는데",
          "original_form": "나는 결혼 빨리 하고 싶었는데",
          "speaker_id": "2",
          "time": "20210822 11:26"
        },
        {
          "id": "MDRW2100005218.1.5",
          "form": "최소 몇 살엔 결혼하고 싶어",
          "original_form": "최소 몇 살엔 결혼하고 싶어",
          "speaker_id": "1",
          "time": "20210822 11:26"
        },
        {
          "id": "MDRW2100005218.1.6",
          "form": "이미 지났어",
          "original_form": "이미 지났어",
          "speaker_id": "2",
          "time": "20210822 11:28"
        },
        {
          "id": "MDRW2100005218.1.7",
          "form": "가능한 빨리 하고싶지",
          "original_form": "가능한 빨리 하고싶지",
          "speaker_id": "2",
          "time": "20210822 11:28"
        },
        {
          "id": "MDRW2100005218.1.8",
          "form": "마자.. 하려면 35전엔 해야해..",
          "original_form": "마자.. 하려면 35전엔 해야해..",
          "speaker_id": "1",
          "time": "20210822 11:29"
        },
        {
          "id": "MDRW2100005218.1.9",
          "form": "학교나 학원에 괜찮은사람 없어?",
          "original_form": "학교나 학원에 괜찮은사람 없어?",
          "speaker_id": "2",
          "time": "20210822 11:31"
        },
        {
          "id": "MDRW2100005218.1.10",
          "form": "있을리 X",
          "original_form": "있을리 X",
          "speaker_id": "1",
          "time": "20210822 11:32"
        },
        {
          "id": "MDRW2100005218.1.11",
          "form": "넌 이제 소개팅 안하냐",
          "original_form": "넌 이제 소개팅 안하냐",
          "speaker_id": "1",
          "time": "20210822 11:32"
        },
        {
          "id": "MDRW2100005218.1.12",
          "form": "아무도 안시켜 주던데",
          "original_form": "아무도 안시켜 주던데",
          "speaker_id": "2",
          "time": "20210822 11:33"
        },
        {
          "id": "MDRW2100005218.1.13",
          "form": "하고싶지만 대학원생에게 연애는 사치지",
          "original_form": "하고싶지만 대학원생에게 연애는 사치지",
          "speaker_id": "2",
          "time": "20210822 11:33"
        },
        {
          "id": "MDRW2100005218.1.14",
          "form": "백수보다 낫지",
          "original_form": "백수보다 낫지",
          "speaker_id": "1",
          "time": "20210822 11:33"
        },
        {
          "id": "MDRW2100005218.1.15",
          "form": "대학원생 은근 시간 많다",
          "original_form": "대학원생 은근 시간 많다",
          "speaker_id": "1",
          "time": "20210822 11:34"
        },
        {
          "id": "MDRW2100005218.1.16",
          "form": "소개시켜달라해",
          "original_form": "소개시켜달라해",
          "speaker_id": "1",
          "time": "20210822 11:34"
        },
        {
          "id": "MDRW2100005218.1.17",
          "form": "누구한테",
          "original_form": "누구한테",
          "speaker_id": "2",
          "time": "20210822 11:34"
        },
        {
          "id": "MDRW2100005218.1.18",
          "form": "나좀시켜줘봐",
          "original_form": "나좀시켜줘봐",
          "speaker_id": "2",
          "time": "20210822 11:34"
        },
        {
          "id": "MDRW2100005218.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:34"
        },
        {
          "id": "MDRW2100005218.1.20",
          "form": "아니 나는 말고...",
          "original_form": "아니 나는 말고...",
          "speaker_id": "1",
          "time": "20210822 11:36"
        },
        {
          "id": "MDRW2100005218.1.21",
          "form": "거기에 있는 사람 소개시켜달라해",
          "original_form": "거기에 있는 사람 소개시켜달라해",
          "speaker_id": "1",
          "time": "20210822 11:36"
        },
        {
          "id": "MDRW2100005218.1.22",
          "form": "너 겜 친구 많자나",
          "original_form": "너 겜 친구 많자나",
          "speaker_id": "1",
          "time": "20210822 11:36"
        },
        {
          "id": "MDRW2100005218.1.23",
          "form": "거기는ㅋㅋㅋㅋ",
          "original_form": "거기는ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:36"
        },
        {
          "id": "MDRW2100005218.1.24",
