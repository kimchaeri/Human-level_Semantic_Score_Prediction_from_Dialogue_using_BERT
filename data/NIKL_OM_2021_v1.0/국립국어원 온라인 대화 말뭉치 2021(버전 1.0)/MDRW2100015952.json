        },
        {
          "id": "MDRW2100015952.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210826 03:38"
        },
        {
          "id": "MDRW2100015952.1.3",
          "form": "동호회회하시나요?",
          "original_form": "동호회회하시나요?",
          "speaker_id": "1",
          "time": "20210826 03:40"
        },
        {
          "id": "MDRW2100015952.1.4",
          "form": "동호회는 딱히 하는게 없네용..",
          "original_form": "동호회는 딱히 하는게 없네용..",
          "speaker_id": "2",
          "time": "20210826 03:40"
        },
        {
          "id": "MDRW2100015952.1.5",
          "form": "대학교 다닐때 동아리나 소모임같은건 좀 했어요",
          "original_form": "대학교 다닐때 동아리나 소모임같은건 좀 했어요",
          "speaker_id": "2",
          "time": "20210826 03:40"
        },
        {
          "id": "MDRW2100015952.1.6",
          "form": "봉사 해보신적 있으신가요",
          "original_form": "봉사 해보신적 있으신가요",
          "speaker_id": "1",
          "time": "20210826 03:40"
        },
        {
          "id": "MDRW2100015952.1.7",
          "form": "봉사는 학교 봉사시간 이수하려구",
          "original_form": "봉사는 학교 봉사시간 이수하려구",
          "speaker_id": "2",
          "time": "20210826 03:41"
        },
        {
          "id": "MDRW2100015952.1.8",
          "form": "한두번 해본 것 같아요 ㅋㅋㅋㅋ",
          "original_form": "한두번 해본 것 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 03:41"
        },
        {
          "id": "MDRW2100015952.1.9",
          "form": "봉사 해보셨나요?",
          "original_form": "봉사 해보셨나요?",
          "speaker_id": "2",
          "time": "20210826 03:41"
        },
        {
          "id": "MDRW2100015952.1.10",
          "form": "네 개인적으로 한번해봤어요",
          "original_form": "네 개인적으로 한번해봤어요",
          "speaker_id": "1",
          "time": "20210826 03:41"
        },
        {
          "id": "MDRW2100015952.1.11",
          "form": "오오 어떤 봉사였어요?",
          "original_form": "오오 어떤 봉사였어요?",
          "speaker_id": "2",
          "time": "20210826 03:42"
        },
        {
          "id": "MDRW2100015952.1.12",
          "form": "교육 봉사였어요!",
          "original_form": "교육 봉사였어요!",
          "speaker_id": "1",
          "time": "20210826 03:42"
        },
        {
          "id": "MDRW2100015952.1.13",
          "form": "오 교육봉사 하셨으면 아이들 만나는거 좋아하시나요?",
          "original_form": "오 교육봉사 하셨으면 아이들 만나는거 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 03:42"
        },
        {
          "id": "MDRW2100015952.1.14",
          "form": "검정고시 가르쳤어요ㅎㅎ",
          "original_form": "검정고시 가르쳤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 03:43"
        },
        {
          "id": "MDRW2100015952.1.15",
          "form": "아하 그렇군요!!!",
          "original_form": "아하 그렇군요!!!",
          "speaker_id": "2",
          "time": "20210826 03:43"
        },
        {
          "id": "MDRW2100015952.1.16",
          "form": "검정고시도 엄청 보람이 클 것 같아요",
          "original_form": "검정고시도 엄청 보람이 클 것 같아요",
          "speaker_id": "2",
          "time": "20210826 03:43"
        },
        {
          "id": "MDRW2100015952.1.17",
          "form": "저희 어머니도 늦게 검정고시 보셨었거든요",
          "original_form": "저희 어머니도 늦게 검정고시 보셨었거든요",
          "speaker_id": "2",
          "time": "20210826 03:43"
        },
        {
          "id": "MDRW2100015952.1.18",
          "form": "교회 공부방같은데서 공부하셨는데 되게 재밌어하시더라고요!",
          "original_form": "교회 공부방같은데서 공부하셨는데 되게 재밌어하시더라고요!",
          "speaker_id": "2",
          "time": "20210826 03:44"
        },
        {
          "id": "MDRW2100015952.1.19",
          "form": "애들이 다 합격해서 보람찼어용",
          "original_form": "애들이 다 합격해서 보람찼어용",
