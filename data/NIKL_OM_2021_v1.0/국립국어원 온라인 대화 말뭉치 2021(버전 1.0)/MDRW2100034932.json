      "utterance": [
        {
          "id": "MDRW2100034932.1.1",
          "form": "name2님은 훠궈를 좋아하시나요?",
          "original_form": "&name2&님은 훠궈를 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210828 22:45"
        },
        {
          "id": "MDRW2100034932.1.2",
          "form": "네 자주먹지는않는데 좋아해요",
          "original_form": "네 자주먹지는않는데 좋아해요",
          "speaker_id": "2",
          "time": "20210828 22:46"
        },
        {
          "id": "MDRW2100034932.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 22:46"
        },
        {
          "id": "MDRW2100034932.1.4",
          "form": "전 좋아해요 토마토탕도 좋아하구 백탕도 홍탕도",
          "original_form": "전 좋아해요 토마토탕도 좋아하구 백탕도 홍탕도",
          "speaker_id": "1",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100034932.1.5",
          "form": "다 좋아해요",
          "original_form": "다 좋아해요",
          "speaker_id": "1",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100034932.1.6",
          "form": "오 전 홍탕은 잘못먹겠더라구여 맵고 그 뭐지 팔각?그거 향이 넘강해서요",
          "original_form": "오 전 홍탕은 잘못먹겠더라구여 맵고 그 뭐지 팔각?그거 향이 넘강해서요",
          "speaker_id": "2",
          "time": "20210828 23:21"
        },
        {
          "id": "MDRW2100034932.1.7",
          "form": "대만에서 홍탕도전했다가 눈물쏟고 고기만 건져먹었던 기억이있어요",
          "original_form": "대만에서 홍탕도전했다가 눈물쏟고 고기만 건져먹었던 기억이있어요",
          "speaker_id": "2",
          "time": "20210828 23:21"
        },
        {
          "id": "MDRW2100034932.1.8",
          "form": "팔각도 있고 까맣고 동그란 화자오가",
          "original_form": "팔각도 있고 까맣고 동그란 화자오가",
          "speaker_id": "1",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100034932.1.9",
          "form": "씹으면 아린맛나는 향ㅇ신료가 있어요",
          "original_form": "씹으면 아린맛나는 향ㅇ신료가 있어요",
          "speaker_id": "1",
          "time": "20210828 23:22"
        },
        {
          "id": "MDRW2100034932.1.10",
          "form": "네네 그건거같아요 아린맛나는",
          "original_form": "네네 그건거같아요 아린맛나는",
          "speaker_id": "2",
          "time": "20210828 23:23"
        },
        {
          "id": "MDRW2100034932.1.11",
          "form": "마라탕은 잘먹겠는데..홍탕은 한국식아니면 못먹겠더라구여",
          "original_form": "마라탕은 잘먹겠는데..홍탕은 한국식아니면 못먹겠더라구여",
          "speaker_id": "2",
          "time": "20210828 23:23"
        },
        {
          "id": "MDRW2100034932.1.12",
          "form": "ㅠㅠ 아 그러시구나 근데 그런 사람이 많아요 한국음식서 스는 향신료가 아니에요",
          "original_form": "ㅠㅠ 아 그러시구나 근데 그런 사람이 많아요 한국음식서 스는 향신료가 아니에요",
          "speaker_id": "1",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100034932.1.13",
          "form": "네ㅜ 고수같은건 잘먹겠는데 그건 좀 튀더라구여",
          "original_form": "네ㅜ 고수같은건 잘먹겠는데 그건 좀 튀더라구여",
          "speaker_id": "2",
          "time": "20210828 23:29"
        },
        {
          "id": "MDRW2100034932.1.14",
          "form": "안그래도 매운것도 잘못먹는데 더그렇더라구여",
          "original_form": "안그래도 매운것도 잘못먹는데 더그렇더라구여",
          "speaker_id": "2",
          "time": "20210828 23:29"
        },
        {
          "id": "MDRW2100034932.1.15",
          "form": "그럼 그냥 샤브샤브가 더 좋겠어요 홍탕냄새가 훠궈집엔 가득하니까요",
          "original_form": "그럼 그냥 샤브샤브가 더 좋겠어요 홍탕냄새가 훠궈집엔 가득하니까요",
          "speaker_id": "1",
          "time": "20210828 23:30"
        },
        {
          "id": "MDRW2100034932.1.16",
          "form": "네 샤브샤브아니면 마라탕은잘먹어요",
          "original_form": "네 샤브샤브아니면 마라탕은잘먹어요",
          "speaker_id": "2",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100034932.1.17",
          "form": "아 한국에서 조선족이하는 홍탕은 잘먹어요",
          "original_form": "아 한국에서 조선족이하는 홍탕은 잘먹어요",
          "speaker_id": "2",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100034932.1.18",
          "form": "좀 순하더라구여",
          "original_form": "좀 순하더라구여",
          "speaker_id": "2",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100034932.1.19",
          "form": "아항 좀 한국식이죠",
          "original_form": "아항 좀 한국식이죠",
          "speaker_id": "1",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100034932.1.20",
          "form": "전 삭힌 홍어정도요,,",
          "original_form": "전 삭힌 홍어정도요,,",
          "speaker_id": "1",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100034932.1.21",
          "form": "너무 화학적인 냄새가 내요",
          "original_form": "너무 화학적인 냄새가 내요",
          "speaker_id": "1",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100034932.1.22",
          "form": "저도 홍어는 그냥은못먹고",
          "original_form": "저도 홍어는 그냥은못먹고",
          "speaker_id": "2",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100034932.1.23",
          "form": "홍어무침은잘먹어요 그 삭힌맛이 양념에 잘어울리더라구여",
          "original_form": "홍어무침은잘먹어요 그 삭힌맛이 양념에 잘어울리더라구여",
          "speaker_id": "2",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100034932.1.24",
          "form": "전 홍어만 먹ㄱ어보고 무침은 시도도 안했는데 한 번 먹어보야겠어요",
          "original_form": "전 홍어만 먹ㄱ어보고 무침은 시도도 안했는데 한 번 먹어보야겠어요",
          "speaker_id": "1",
          "time": "20210828 23:38"
        }
      ]
    }
