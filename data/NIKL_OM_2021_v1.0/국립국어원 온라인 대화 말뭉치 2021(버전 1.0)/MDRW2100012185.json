      "utterance": [
        {
          "id": "MDRW2100012185.1.1",
          "form": "저는 점심에 김밥천국 갔어요",
          "original_form": "저는 점심에 김밥천국 갔어요",
          "speaker_id": "2",
          "time": "20210824 20:07"
        },
        {
          "id": "MDRW2100012185.1.2",
          "form": "김밥은 안 먹었지만... 참치김치철판볶음밥 먹었네요",
          "original_form": "김밥은 안 먹었지만... 참치김치철판볶음밥 먹었네요",
          "speaker_id": "2",
          "time": "20210824 20:07"
        },
        {
          "id": "MDRW2100012185.1.3",
          "form": "그렇군요. 혹시 냉동볶음밥을 사드셔보셨나요?",
          "original_form": "그렇군요. 혹시 냉동볶음밥을 사드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210824 20:08"
        },
        {
          "id": "MDRW2100012185.1.4",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210824 20:08"
        },
        {
          "id": "MDRW2100012185.1.5",
          "form": "존쿡 베이컨 김치 필라프 추천합니다",
          "original_form": "존쿡 베이컨 김치 필라프 추천합니다",
          "speaker_id": "2",
          "time": "20210824 20:08"
        },
        {
          "id": "MDRW2100012185.1.6",
          "form": "김천보다 맛있지않냐고 말하려고 했는데 이미 드셔보셨군요",
          "original_form": "김천보다 맛있지않냐고 말하려고 했는데 이미 드셔보셨군요",
          "speaker_id": "1",
          "time": "20210824 20:09"
        },
        {
          "id": "MDRW2100012185.1.7",
          "form": "김천은 멀리 나가기 귀찮을 때 주로 가요",
          "original_form": "김천은 멀리 나가기 귀찮을 때 주로 가요",
          "speaker_id": "2",
          "time": "20210824 20:11"
        },
        {
          "id": "MDRW2100012185.1.8",
          "form": "맛에는 기대가 엄슴...",
          "original_form": "맛에는 기대가 엄슴...",
          "speaker_id": "2",
          "time": "20210824 20:11"
        },
        {
          "id": "MDRW2100012185.1.9",
          "form": "오늘 드신 김밥은 김천 거였나요?",
          "original_form": "오늘 드신 김밥은 김천 거였나요?",
          "speaker_id": "2",
          "time": "20210824 20:11"
        },
        {
          "id": "MDRW2100012185.1.10",
          "form": "아니오 홈메이드 김밥이요",
          "original_form": "아니오 홈메이드 김밥이요",
          "speaker_id": "1",
          "time": "20210824 20:11"
        },
        {
          "id": "MDRW2100012185.1.11",
          "form": "우엉조림이 남아서 단무지랑 햄이랑 돌돌 말았어요",
          "original_form": "우엉조림이 남아서 단무지랑 햄이랑 돌돌 말았어요",
          "speaker_id": "1",
          "time": "20210824 20:11"
        },
        {
          "id": "MDRW2100012185.1.12",
          "form": "헐...",
          "original_form": "헐...",
          "speaker_id": "2",
          "time": "20210824 20:12"
        },
        {
          "id": "MDRW2100012185.1.13",
          "form": "왜요 ㅋㅋㅋ",
          "original_form": "왜요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 20:12"
        },
        {
          "id": "MDRW2100012185.1.14",
          "form": "요리왕이셔요ㅋㅋㅋ",
          "original_form": "요리왕이셔요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 20:12"
        },
        {
          "id": "MDRW2100012185.1.15",
          "form": "부지런...",
          "original_form": "부지런...",
          "speaker_id": "2",
          "time": "20210824 20:12"
        },
        {
          "id": "MDRW2100012185.1.16",
          "form": "귀찮긴 한데 집에 다 재료가 있어서요",
          "original_form": "귀찮긴 한데 집에 다 재료가 있어서요",
          "speaker_id": "1",
          "time": "20210824 20:12"
        },
        {
          "id": "MDRW2100012185.1.17",
          "form": "라구소스를 4키로샀는데 자꾸 파스타에 그 소스만 얹어주니 가족들이 싫어해요.",
          "original_form": "라구소스를 4키로샀는데 자꾸 파스타에 그 소스만 얹어주니 가족들이 싫어해요.",
          "speaker_id": "1",
          "time": "20210824 20:13"
        },
        {
          "id": "MDRW2100012185.1.18",
          "form": "역시 요리는 부지런함 + 재료 상비군이 중요한 거 같아요",
          "original_form": "역시 요리는 부지런함 + 재료 상비군이 중요한 거 같아요",
          "speaker_id": "2",
          "time": "20210824 20:13"
        },
        {
          "id": "MDRW2100012185.1.19",
          "form": "맞아요 스킬이야 뭐..유튜브보고 하면 돼요",
          "original_form": "맞아요 스킬이야 뭐..유튜브보고 하면 돼요",
          "speaker_id": "1",
          "time": "20210824 20:14"
        },
        {
          "id": "MDRW2100012185.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 백종원 거 보시나요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 백종원 거 보시나요?",
          "speaker_id": "2",
          "time": "20210824 20:15"
        },
        {
          "id": "MDRW2100012185.1.21",
          "form": "백종원은 레시피만 찾아봐요 대체로 쉬워요",
          "original_form": "백종원은 레시피만 찾아봐요 대체로 쉬워요",
          "speaker_id": "1",
          "time": "20210824 20:15"
        }
      ]
