      "utterance": [
        {
          "id": "MDRW2100008760.1.1",
          "form": "와 누구 보셨어요??",
          "original_form": "와 누구 보셨어요??",
          "speaker_id": "2",
          "time": "20210822 18:11"
        },
        {
          "id": "MDRW2100008760.1.2",
          "form": "생각나는사람은 조세호욬ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "생각나는사람은 조세호욬ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:15"
        },
        {
          "id": "MDRW2100008760.1.3",
          "form": "딱 보는데 진짜 그냥 웃음이나서 ㅋㅋㅋㅋㅋ",
          "original_form": "딱 보는데 진짜 그냥 웃음이나서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:16"
        },
        {
          "id": "MDRW2100008760.1.4",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:16"
        },
        {
          "id": "MDRW2100008760.1.5",
          "form": "어때요??ㅋㅋ",
          "original_form": "어때요??ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:16"
        },
        {
          "id": "MDRW2100008760.1.6",
          "form": "되게 부어있었고 그때가 오후였는데 그때까지 자다온듯한.ㅋㅋㅋ",
          "original_form": "되게 부어있었고 그때가 오후였는데 그때까지 자다온듯한.ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:18"
        },
        {
          "id": "MDRW2100008760.1.7",
          "form": "ㅋㅋㅋㅋㅋ 어떤 느낌인지 알거같아여 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 어떤 느낌인지 알거같아여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:23"
        },
        {
          "id": "MDRW2100008760.1.8",
          "form": "ㅋㅋㅋㅋ뭔지알겠져 ....",
          "original_form": "ㅋㅋㅋㅋ뭔지알겠져 ....",
          "speaker_id": "1",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008760.1.9",
          "form": "같이 있던친구가 너무 못참고 대놓고 웃는거예요 ㅋㅋㅋㅋ",
          "original_form": "같이 있던친구가 너무 못참고 대놓고 웃는거예요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008760.1.10",
          "form": "저도 웃참하느라 힘들었어요",
          "original_form": "저도 웃참하느라 힘들었어요",
          "speaker_id": "1",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008760.1.11",
          "form": "근데 약간 의식하더라구요 ㅋㅋㅋㅋ 자기보고 웃는게 좋은지 돌아서 가는길에 거울보고 저있는쪽 쳐다보더라구욬ㅋㅋㅋ",
          "original_form": "근데 약간 의식하더라구요 ㅋㅋㅋㅋ 자기보고 웃는게 좋은지 돌아서 가는길에 거울보고 저있는쪽 쳐다보더라구욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008760.1.12",
          "form": "ㅋㅋㅋㅋㅋ왠",
          "original_form": "ㅋㅋㅋㅋㅋ왠",
          "speaker_id": "2",
          "time": "20210822 18:31"
        },
        {
          "id": "MDRW2100008760.1.13",
          "form": "왠지 관심받는거좋아하는거 같아요",
          "original_form": "왠지 관심받는거좋아하는거 같아요",
          "speaker_id": "2",
          "time": "20210822 18:31"
        },
        {
          "id": "MDRW2100008760.1.14",
          "form": "조세호님은",
          "original_form": "조세호님은",
          "speaker_id": "2",
          "time": "20210822 18:31"
        },
        {
          "id": "MDRW2100008760.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ약간 그런느낌이었어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ약간 그런느낌이었어요",
          "speaker_id": "1",
          "time": "20210822 18:34"
        },
        {
          "id": "MDRW2100008760.1.16",
          "form": "티비에 나오는거 보면ㅎㅎㅎㅎ",
          "original_form": "티비에 나오는거 보면ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 18:36"
        },
        {
          "id": "MDRW2100008760.1.17",
          "form": "전 유재석 한번보고싶어요ㅜ",
          "original_form": "전 유재석 한번보고싶어요ㅜ",
          "speaker_id": "2",
          "time": "20210822 18:36"
        },
        {
          "id": "MDRW2100008760.1.18",
          "form": "저도요ㅠㅠㅠㅠㅠㅠ",
          "original_form": "저도요ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 18:38"
        },
        {
          "id": "MDRW2100008760.1.19",
          "form": "진짜 무도한참할때 보는게 소원이었어요ㅠㅠ",
          "original_form": "진짜 무도한참할때 보는게 소원이었어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 18:38"
        },
        {
          "id": "MDRW2100008760.1.20",
          "form": "그죠ㅜㅜ",
          "original_form": "그죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 18:40"
        },
        {
          "id": "MDRW2100008760.1.21",
          "form": "진짜 유퀴즈할때 지나가면서라도 보고 싶네여ㅋㅋㅋㅋ",
          "original_form": "진짜 유퀴즈할때 지나가면서라도 보고 싶네여ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:40"
        },
        {
          "id": "MDRW2100008760.1.22",
          "form": "슈트입은모습 정말 멋있더라구요!!",
          "original_form": "슈트입은모습 정말 멋있더라구요!!",
          "speaker_id": "2",
          "time": "20210822 18:40"
        },
        {
          "id": "MDRW2100008760.1.23",
          "form": "진짜 퀴즈는 안풀어도 옆에서 좀 보고싶어요ㅠㅋㅋㅋ",
          "original_form": "진짜 퀴즈는 안풀어도 옆에서 좀 보고싶어요ㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:52"
        },
        {
          "id": "MDRW2100008760.1.24",
          "form": "좋아하는 연예인 있으세요?",
          "original_form": "좋아하는 연예인 있으세요?",
