      "utterance": [
        {
          "id": "MDRW2100025866.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210901 17:32"
        },
        {
          "id": "MDRW2100025866.1.2",
          "form": "안녕하세요 오늘 저녁 식사 메뉴는 뭔가요",
          "original_form": "안녕하세요 오늘 저녁 식사 메뉴는 뭔가요",
          "speaker_id": "2",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100025866.1.3",
          "form": "저는 떡볶이 먹으려구요 님은요??",
          "original_form": "저는 떡볶이 먹으려구요 님은요??",
          "speaker_id": "1",
          "time": "20210901 17:38"
        },
        {
          "id": "MDRW2100025866.1.4",
          "form": "저는 가을 제철이라고",
          "original_form": "저는 가을 제철이라고",
          "speaker_id": "2",
          "time": "20210901 17:40"
        },
        {
          "id": "MDRW2100025866.1.5",
          "form": "꽃게 팔길래 샀어요",
          "original_form": "꽃게 팔길래 샀어요",
          "speaker_id": "2",
          "time": "20210901 17:40"
        },
        {
          "id": "MDRW2100025866.1.6",
          "form": "우와 저도 어제 꽃게탕 먹었어요",
          "original_form": "우와 저도 어제 꽃게탕 먹었어요",
          "speaker_id": "1",
          "time": "20210901 17:43"
        },
        {
          "id": "MDRW2100025866.1.7",
          "form": "꽃게 살이 꽉 차서 진짜 맛있더라구요",
          "original_form": "꽃게 살이 꽉 차서 진짜 맛있더라구요",
          "speaker_id": "1",
          "time": "20210901 17:43"
        },
        {
          "id": "MDRW2100025866.1.8",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210901 17:44"
        },
        {
          "id": "MDRW2100025866.1.9",
          "form": "철이긴 한가봐요",
          "original_form": "철이긴 한가봐요",
          "speaker_id": "2",
          "time": "20210901 17:45"
        },
        {
          "id": "MDRW2100025866.1.10",
          "form": "저는 조리법 몰라서 그냥 쪄먹어야해요 ㅠㅠ",
          "original_form": "저는 조리법 몰라서 그냥 쪄먹어야해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 17:45"
        },
        {
          "id": "MDRW2100025866.1.11",
          "form": "아 ㅠㅠ 아쉽네요",
          "original_form": "아 ㅠㅠ 아쉽네요",
          "speaker_id": "1",
          "time": "20210901 17:46"
        },
        {
          "id": "MDRW2100025866.1.12",
          "form": "얼큰하게 국물이랑 같이 먹으면 진짜 맛있는데",
          "original_form": "얼큰하게 국물이랑 같이 먹으면 진짜 맛있는데",
          "speaker_id": "1",
          "time": "20210901 17:46"
        },
        {
          "id": "MDRW2100025866.1.13",
          "form": "쪄먹어도 충분히 맛있죠",
          "original_form": "쪄먹어도 충분히 맛있죠",
          "speaker_id": "1",
          "time": "20210901 17:46"
        },
        {
          "id": "MDRW2100025866.1.14",
          "form": "껍질 처리가 힘들긴한데 자주 먹어야겠어요 ㅎㅎ",
          "original_form": "껍질 처리가 힘들긴한데 자주 먹어야겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:49"
        },
        {
          "id": "MDRW2100025866.1.15",
          "form": "떡볶이랑 튀김도 드시나요",
          "original_form": "떡볶이랑 튀김도 드시나요",
          "speaker_id": "2",
          "time": "20210901 17:50"
        },
        {
          "id": "MDRW2100025866.1.16",
          "form": "어머니가 직접 만들어주시는 거라서",
          "original_form": "어머니가 직접 만들어주시는 거라서",
          "speaker_id": "1",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100025866.1.17",
          "form": "튀김은 아쉽게도 없고 대신 중국당면 넣어서 먹으려구요",
          "original_form": "튀김은 아쉽게도 없고 대신 중국당면 넣어서 먹으려구요",
          "speaker_id": "1",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100025866.1.18",
          "form": "중국당면 좋아하세요???",
          "original_form": "중국당면 좋아하세요???",
          "speaker_id": "1",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100025866.1.19",
          "form": "대박 그거 진짜 쫀득하니 맛있죠",
          "original_form": "대박 그거 진짜 쫀득하니 맛있죠",
          "speaker_id": "2",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100025866.1.20",
          "form": "그게 집에 있으시다니",
          "original_form": "그게 집에 있으시다니",
          "speaker_id": "2",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100025866.1.21",
          "form": "배우신분",
          "original_form": "배우신분",
          "speaker_id": "2",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100025866.1.22",
          "form": "ㅎㅎ 진짜 맛있었어요",
          "original_form": "ㅎㅎ 진짜 맛있었어요",
          "speaker_id": "1",
          "time": "20210901 19:11"
        },
        {
          "id": "MDRW2100025866.1.23",
          "form": "후식으로 포도도 먹으려구요",
          "original_form": "후식으로 포도도 먹으려구요",
          "speaker_id": "1",
          "time": "20210901 19:11"
        },
        {
          "id": "MDRW2100025866.1.24",
          "form": "꽃게 어떠셨나요??",
          "original_form": "꽃게 어떠셨나요??",
          "speaker_id": "1",
          "time": "20210901 19:12"
        },
        {
          "id": "MDRW2100025866.1.25",
          "form": "아직 살아 움직이고 있어서 보고만있어요 ㅋㅋㅋ",
          "original_form": "아직 살아 움직이고 있어서 보고만있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:13"
        },
        {
          "id": "MDRW2100025866.1.26",
          "form": "헐 저녁 아직 안 드셨어요???",
          "original_form": "헐 저녁 아직 안 드셨어요???",
          "speaker_id": "1",
          "time": "20210901 19:29"
        },
        {
          "id": "MDRW2100025866.1.27",
          "form": "이제 꽃게 손질법 찾아서 보고있어요",
          "original_form": "이제 꽃게 손질법 찾아서 보고있어요",
          "speaker_id": "2",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100025866.1.28",
          "form": "현기증 나네요 배고파서 ㅋㅋ",
          "original_form": "현기증 나네요 배고파서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:37"
        }
      ]
    }
