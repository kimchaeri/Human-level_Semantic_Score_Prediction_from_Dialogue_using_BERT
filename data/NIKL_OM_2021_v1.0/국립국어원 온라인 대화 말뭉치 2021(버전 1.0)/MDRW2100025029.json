      "utterance": [
        {
          "id": "MDRW2100025029.1.1",
          "form": "곰표맥주드셔보셨나요?",
          "original_form": "곰표맥주드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210831 22:29"
        },
        {
          "id": "MDRW2100025029.1.2",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100025029.1.3",
          "form": "궁금해요!",
          "original_form": "궁금해요!",
          "speaker_id": "2",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100025029.1.4",
          "form": "근데 불안해서 손이 안가더라구요",
          "original_form": "근데 불안해서 손이 안가더라구요",
          "speaker_id": "2",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100025029.1.5",
          "form": "밀가루맛은 안나고요ㅋㅋㄱ달달해요!",
          "original_form": "밀가루맛은 안나고요ㅋㅋㄱ달달해요!",
          "speaker_id": "1",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100025029.1.6",
          "form": "달달해요?",
          "original_form": "달달해요?",
          "speaker_id": "2",
          "time": "20210901 14:15"
        },
        {
          "id": "MDRW2100025029.1.7",
          "form": "우와 맛이 예상 안가요",
          "original_form": "우와 맛이 예상 안가요",
          "speaker_id": "2",
          "time": "20210901 14:15"
        },
        {
          "id": "MDRW2100025029.1.8",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:15"
        },
        {
          "id": "MDRW2100025029.1.9",
          "form": "쓴건 싫어하는데 그럼 먹어볼만하네요",
          "original_form": "쓴건 싫어하는데 그럼 먹어볼만하네요",
          "speaker_id": "2",
          "time": "20210901 14:15"
        },
        {
          "id": "MDRW2100025029.1.10",
          "form": "밀맥이라 순하고 달달해서",
          "original_form": "밀맥이라 순하고 달달해서",
          "speaker_id": "1",
          "time": "20210901 14:16"
        },
        {
          "id": "MDRW2100025029.1.11",
          "form": "드실만 할거에요~^^",
          "original_form": "드실만 할거에요~^^",
          "speaker_id": "1",
          "time": "20210901 14:16"
        },
        {
          "id": "MDRW2100025029.1.12",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "2",
          "time": "20210901 14:16"
        },
        {
          "id": "MDRW2100025029.1.13",
          "form": "그럼 진짜 먹어봐야겠어요",
          "original_form": "그럼 진짜 먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210901 14:17"
        },
        {
          "id": "MDRW2100025029.1.14",
          "form": "편의점에서 볼때마다",
          "original_form": "편의점에서 볼때마다",
          "speaker_id": "2",
          "time": "20210901 14:17"
        },
        {
          "id": "MDRW2100025029.1.15",
          "form": "확 눈이 사로잡히더라구요",
          "original_form": "확 눈이 사로잡히더라구요",
          "speaker_id": "2",
          "time": "20210901 14:17"
        },
        {
          "id": "MDRW2100025029.1.16",
          "form": "편의점에 재고가 잘 없는데",
          "original_form": "편의점에 재고가 잘 없는데",
          "speaker_id": "1",
          "time": "20210901 14:17"
        },
        {
          "id": "MDRW2100025029.1.17",
          "form": "우와 부럽네요",
          "original_form": "우와 부럽네요",
          "speaker_id": "1",
          "time": "20210901 14:17"
        },
        {
          "id": "MDRW2100025029.1.18",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "2",
          "time": "20210901 14:18"
        },
        {
          "id": "MDRW2100025029.1.19",
          "form": "인기 많군요?",
          "original_form": "인기 많군요?",
          "speaker_id": "2",
          "time": "20210901 14:18"
        },
        {
          "id": "MDRW2100025029.1.20",
          "form": "갑자기 좀 불안해요",
          "original_form": "갑자기 좀 불안해요",
          "speaker_id": "2",
          "time": "20210901 14:18"
        },
        {
          "id": "MDRW2100025029.1.21",
          "form": "오늘가서 사여겠어요",
          "original_form": "오늘가서 사여겠어요",
          "speaker_id": "2",
          "time": "20210901 14:18"
        },
        {
          "id": "MDRW2100025029.1.22",
          "form": "전 사러갈때마다 없더라구요ㅠㅠ",
          "original_form": "전 사러갈때마다 없더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 14:49"
        },
        {
          "id": "MDRW2100025029.1.23",
          "form": "성공하시기를!ㅋㄲ",
          "original_form": "성공하시기를!ㅋㄲ",
          "speaker_id": "1",
          "time": "20210901 14:49"
        },
        {
          "id": "MDRW2100025029.1.24",
          "form": "네! name1도 성공 하세요",
          "original_form": "네! &name1&도 성공 하세요",
          "speaker_id": "2",
          "time": "20210901 14:58"
        },
        {
          "id": "MDRW2100025029.1.25",
          "form": "네! 오늘은 일끝나고 편의점으로 달려가야겠네요ㅎㅎㅎㅎ",
          "original_form": "네! 오늘은 일끝나고 편의점으로 달려가야겠네요ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 15:18"
        },
        {
          "id": "MDRW2100025029.1.26",
          "form": "구할 수 있길 전 실패했아요ㅜ",
          "original_form": "구할 수 있길 전 실패했아요ㅜ",
