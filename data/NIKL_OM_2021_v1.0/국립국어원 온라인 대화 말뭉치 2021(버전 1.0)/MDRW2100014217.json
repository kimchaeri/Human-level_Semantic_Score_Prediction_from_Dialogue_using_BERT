      "utterance": [
        {
          "id": "MDRW2100014217.1.1",
          "form": "학교 생활어떠셨나요??",
          "original_form": "학교 생활어떠셨나요??",
          "speaker_id": "1",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014217.1.2",
          "form": "고등학교때가 제일 재밌었던 거 같아요",
          "original_form": "고등학교때가 제일 재밌었던 거 같아요",
          "speaker_id": "2",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014217.1.3",
          "form": "대학생은 뭔가 생각보다 별루...ㅠㅠㅠ",
          "original_form": "대학생은 뭔가 생각보다 별루...ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014217.1.4",
          "form": "아 혹시 지금 대학생이세요..???",
          "original_form": "아 혹시 지금 대학생이세요..???",
          "speaker_id": "1",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014217.1.5",
          "form": "저는 1학년인데 저도 생각보다 진짜 힘든것같아요ㅠ",
          "original_form": "저는 1학년인데 저도 생각보다 진짜 힘든것같아요ㅠ",
          "speaker_id": "1",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014217.1.6",
          "form": "저는 졸업했어요 ㅋㅋㅋㅋㅋ",
          "original_form": "저는 졸업했어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014217.1.7",
          "form": "원래 그때가 힘들어여 ㅋㅋㅋㅋㅋ",
          "original_form": "원래 그때가 힘들어여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014217.1.8",
          "form": "대학생활이 막막해서 힘든걸까요ㅠ?",
          "original_form": "대학생활이 막막해서 힘든걸까요ㅠ?",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014217.1.9",
          "form": "약간 고딩에서 대딩 딱 넘어가는 시기다보니까 힘들어여",
          "original_form": "약간 고딩에서 대딩 딱 넘어가는 시기다보니까 힘들어여",
          "speaker_id": "2",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014217.1.10",
          "form": "근데 적응되면 이때만큼 편한 생활이 없져",
          "original_form": "근데 적응되면 이때만큼 편한 생활이 없져",
          "speaker_id": "2",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014217.1.11",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014217.1.12",
          "form": "대학과제들이 생각을 끄집어내라 힘든것 같아요 ㅋㅋㅋ큐ㅠ",
          "original_form": "대학과제들이 생각을 끄집어내라 힘든것 같아요 ㅋㅋㅋ큐ㅠ",
          "speaker_id": "1",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014217.1.13",
          "form": "고딩때는 주어진대로 풀면됐는데",
          "original_form": "고딩때는 주어진대로 풀면됐는데",
          "speaker_id": "1",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014217.1.14",
          "form": "대충 무슨과 다니세여??",
          "original_form": "대충 무슨과 다니세여??",
          "speaker_id": "2",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014217.1.15",
          "form": "아 저는 교대요!",
          "original_form": "아 저는 교대요!",
          "speaker_id": "1",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100014217.1.16",
          "form": "오 그러시구나 그러면",
          "original_form": "오 그러시구나 그러면",
          "speaker_id": "2",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100014217.1.17",
          "form": "어려울만하져",
          "original_form": "어려울만하져",
          "speaker_id": "2",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100014217.1.18",
          "form": "대학생때 미팅은 해보셨나요????ㅎㅎ",
          "original_form": "대학생때 미팅은 해보셨나요????ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100014217.1.19",
          "form": "주변에서 해준다고 나가라 했는데 도저히 나갈 자신이 없어서... ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "주변에서 해준다고 나가라 했는데 도저히 나갈 자신이 없어서... ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100014217.1.20",
          "form": "앜 저두요",
          "original_form": "앜 저두요",
          "speaker_id": "1",
          "time": "20210825 18:39"
        },
        {
          "id": "MDRW2100014217.1.21",
          "form": "제 주량도 몰라서",
          "original_form": "제 주량도 몰라서",
          "speaker_id": "1",
          "time": "20210825 18:39"
        },
        {
          "id": "MDRW2100014217.1.22",
          "form": "주량 잘 아셔야되여",
          "original_form": "주량 잘 아셔야되여",
          "speaker_id": "2",
          "time": "20210825 18:39"
        },
        {
          "id": "MDRW2100014217.1.23",
          "form": "학교 생활에서 술 마실일이 많나요?",
          "original_form": "학교 생활에서 술 마실일이 많나요?",
          "speaker_id": "1",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014217.1.24",
          "form": "비대면이라 학교를 안가서요",
          "original_form": "비대면이라 학교를 안가서요",
          "speaker_id": "1",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014217.1.25",
          "form": "마실일이 좀 있져",
          "original_form": "마실일이 좀 있져",
          "speaker_id": "2",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100014217.1.26",
          "form": "동아리 하면 더 그렇구여",
          "original_form": "동아리 하면 더 그렇구여",
          "speaker_id": "2",
          "time": "20210825 18:45"
        },
        {
          "id": "MDRW2100014217.1.27",
          "form": "뭔가 대학사람 잘 못사귀겠어요....... name1은 동기들 잘 사귀셨나요",
          "original_form": "뭔가 대학사람 잘 못사귀겠어요....... &name1&은 동기들 잘 사귀셨나요",
          "speaker_id": "1",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100014217.1.28",
          "form": "넵 그냥 먼저 다가가면",
          "original_form": "넵 그냥 먼저 다가가면",
          "speaker_id": "2",
          "time": "20210825 18:57"
        },
        {
          "id": "MDRW2100014217.1.29",
          "form": "편해뇹",
          "original_form": "편해뇹",
          "speaker_id": "2",
          "time": "20210825 18:57"
        },
        {
          "id": "MDRW2100014217.1.30",
          "form": "저두 그래야 겠어요 !!.. 집에만 있으니까 별로 하는게 없네요ㅎ",
          "original_form": "저두 그래야 겠어요 !!.. 집에만 있으니까 별로 하는게 없네요ㅎ",
          "speaker_id": "1",
