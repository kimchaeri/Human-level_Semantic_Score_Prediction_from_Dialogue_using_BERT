      "utterance": [
        {
          "id": "MDRW2100005190.1.1",
          "form": "집안일 많이하시나요(?)",
          "original_form": "집안일 많이하시나요(?)",
          "speaker_id": "1",
          "time": "20210821 21:03"
        },
        {
          "id": "MDRW2100005190.1.2",
          "form": "많이하죠ㅠ 오늘도 대청소했습니다ㅡ",
          "original_form": "많이하죠ㅠ 오늘도 대청소했습니다ㅡ",
          "speaker_id": "2",
          "time": "20210821 21:04"
        },
        {
          "id": "MDRW2100005190.1.3",
          "form": "name1님도 많이하시죠?",
          "original_form": "&name1&님도 많이하시죠?",
          "speaker_id": "2",
          "time": "20210821 21:04"
        },
        {
          "id": "MDRW2100005190.1.4",
          "form": "와우~전 오늘 청소 못했네요",
          "original_form": "와우~전 오늘 청소 못했네요",
          "speaker_id": "1",
          "time": "20210821 21:04"
        },
        {
          "id": "MDRW2100005190.1.5",
          "form": "저녁 설겆이는 9시이후한답니다",
          "original_form": "저녁 설겆이는 9시이후한답니다",
          "speaker_id": "1",
          "time": "20210821 21:04"
        },
        {
          "id": "MDRW2100005190.1.6",
          "form": "바로바로 안하시는군요 ㅎ",
          "original_form": "바로바로 안하시는군요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100005190.1.7",
          "form": "전 바로바로 해야해서ㅎ",
          "original_form": "전 바로바로 해야해서ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100005190.1.8",
          "form": "쓰레기는요?",
          "original_form": "쓰레기는요?",
          "speaker_id": "2",
          "time": "20210821 21:06"
        },
        {
          "id": "MDRW2100005190.1.9",
          "form": "쓰레기도 분리수거도",
          "original_form": "쓰레기도 분리수거도",
          "speaker_id": "1",
          "time": "20210821 21:06"
        },
        {
          "id": "MDRW2100005190.1.10",
          "form": "저녁9시 이후",
          "original_form": "저녁9시 이후",
          "speaker_id": "1",
          "time": "20210821 21:06"
        },
        {
          "id": "MDRW2100005190.1.11",
          "form": "아이와 함께 놀고나서 집안일하시눈군요ㅎ",
          "original_form": "아이와 함께 놀고나서 집안일하시눈군요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100005190.1.12",
          "form": "어찌 오늘은 시간이 되셔요?",
          "original_form": "어찌 오늘은 시간이 되셔요?",
          "speaker_id": "2",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100005190.1.13",
          "form": "오늘은 지금 애들이 다 자고있어요",
          "original_form": "오늘은 지금 애들이 다 자고있어요",
          "speaker_id": "1",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100005190.1.14",
          "form": "육아하시는데 힘드시겟어요",
          "original_form": "육아하시는데 힘드시겟어요",
          "speaker_id": "2",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100005190.1.15",
          "form": "주말, 평일할꺼 없이ㅠㅠ",
          "original_form": "주말, 평일할꺼 없이ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100005190.1.16",
          "form": "유아 가사는 똑같죠 매일매일",
          "original_form": "유아 가사는 똑같죠 매일매일",
          "speaker_id": "1",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100005190.1.17",
          "form": "힘내세요ㅎ 아빠잖아요ㅎ",
          "original_form": "힘내세요ㅎ 아빠잖아요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:11"
        },
        {
          "id": "MDRW2100005190.1.18",
          "form": "저번에 사진보니깐 딸이 너무 귀엽더라구요ㅎ",
          "original_form": "저번에 사진보니깐 딸이 너무 귀엽더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:11"
        },
        {
          "id": "MDRW2100005190.1.19",
          "form": "없던힘도 쏟아날것같애요ㅎㅎ",
          "original_form": "없던힘도 쏟아날것같애요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:11"
        },
        {
          "id": "MDRW2100005190.1.20",
          "form": "아빠 힘내세요 우리가 있자나요",
          "original_form": "아빠 힘내세요 우리가 있자나요",
          "speaker_id": "1",
          "time": "20210821 21:11"
        },
        {
          "id": "MDRW2100005190.1.21",
          "form": "가족은 힘입니다",
          "original_form": "가족은 힘입니다",
          "speaker_id": "1",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100005190.1.22",
          "form": "맞아요ㅎ 가족은 힘이죠ㅎ 밖에나가서 일하는거는 가족때문이니깐요 ㅎ",
          "original_form": "맞아요ㅎ 가족은 힘이죠ㅎ 밖에나가서 일하는거는 가족때문이니깐요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005190.1.23",
          "form": "육아, 집안일, 업무 스트레스는 아이의 웃음 한번으로 끝나죠ㅎ",
          "original_form": "육아, 집안일, 업무 스트레스는 아이의 웃음 한번으로 끝나죠ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:14"
        },
        {
          "id": "MDRW2100005190.1.24",
          "form": "근데 또 싸울때는 휴 ㅋㅋ",
          "original_form": "근데 또 싸울때는 휴 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100005190.1.25",
          "form": "네 웃음 은 참 모든걸 ^^ 해결해줘요",
          "original_form": "네 웃음 은 참 모든걸 ^^ 해결해줘요",
          "speaker_id": "1",
          "time": "20210821 21:15"
        }
      ]
