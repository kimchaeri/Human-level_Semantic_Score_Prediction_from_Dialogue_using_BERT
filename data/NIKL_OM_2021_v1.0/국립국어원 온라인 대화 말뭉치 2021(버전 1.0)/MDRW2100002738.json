      "utterance": [
        {
          "id": "MDRW2100002738.1.1",
          "form": "난 아직도 더워",
          "original_form": "난 아직도 더워",
          "speaker_id": "1",
          "time": "20210820 15:11"
        },
        {
          "id": "MDRW2100002738.1.2",
          "form": "난 아직도 더워",
          "original_form": "난 아직도 더워",
          "speaker_id": "1",
          "time": "20210820 15:12"
        },
        {
          "id": "MDRW2100002738.1.3",
          "form": "난 요즘 밤에 창문열면 쌀쌀행",
          "original_form": "난 요즘 밤에 창문열면 쌀쌀행",
          "speaker_id": "2",
          "time": "20210820 15:12"
        },
        {
          "id": "MDRW2100002738.1.4",
          "form": "벌써 가을이 온 느낌이야",
          "original_form": "벌써 가을이 온 느낌이야",
          "speaker_id": "2",
          "time": "20210820 15:12"
        },
        {
          "id": "MDRW2100002738.1.5",
          "form": "새벽에만 쬐끔 시원한 느낌...",
          "original_form": "새벽에만 쬐끔 시원한 느낌...",
          "speaker_id": "1",
          "time": "20210820 15:13"
        },
        {
          "id": "MDRW2100002738.1.6",
          "form": "9월만 되두 밤에 쌀쌀해질껄?",
          "original_form": "9월만 되두 밤에 쌀쌀해질껄?",
          "speaker_id": "2",
          "time": "20210820 15:13"
        },
        {
          "id": "MDRW2100002738.1.7",
          "form": "자기 좋아하는 겨울이 다가오고있네ㅎ",
          "original_form": "자기 좋아하는 겨울이 다가오고있네ㅎ",
          "speaker_id": "2",
          "time": "20210820 15:14"
        },
        {
          "id": "MDRW2100002738.1.8",
          "form": "겨울 안좋아하는디ㅋ 추웤 ㅋ",
          "original_form": "겨울 안좋아하는디ㅋ 추웤 ㅋ",
          "speaker_id": "1",
          "time": "20210820 15:18"
        },
        {
          "id": "MDRW2100002738.1.9",
          "form": "모라구용\r\n춥다구용?",
          "original_form": "모라구용\r\n춥다구용?",
          "speaker_id": "2",
          "time": "20210820 15:19"
        },
        {
          "id": "MDRW2100002738.1.10",
          "form": "겨울에 반팔은 왜입는겨ㅋ",
          "original_form": "겨울에 반팔은 왜입는겨ㅋ",
          "speaker_id": "2",
          "time": "20210820 15:20"
        },
        {
          "id": "MDRW2100002738.1.11",
          "form": "더워서ㅋㅋ",
          "original_form": "더워서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:20"
        },
        {
          "id": "MDRW2100002738.1.12",
          "form": "적당히 시원한 늦가을이 좋아",
          "original_form": "적당히 시원한 늦가을이 좋아",
          "speaker_id": "1",
          "time": "20210820 15:20"
        },
        {
          "id": "MDRW2100002738.1.13",
          "form": "ㅋㅋ 앞뒤가 안 맞잖소",
          "original_form": "ㅋㅋ 앞뒤가 안 맞잖소",
          "speaker_id": "2",
          "time": "20210820 15:20"
        },
        {
          "id": "MDRW2100002738.1.14",
          "form": "나도 봄보단 가을이 좋아",
          "original_form": "나도 봄보단 가을이 좋아",
          "speaker_id": "2",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002738.1.15",
          "form": "근데 우리나라도 이젠 2계절인듯..",
          "original_form": "근데 우리나라도 이젠 2계절인듯..",
          "speaker_id": "1",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002738.1.16",
          "form": "사계절도 옛말",
          "original_form": "사계절도 옛말",
          "speaker_id": "1",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002738.1.17",
          "form": "그냥 여름겨울",
          "original_form": "그냥 여름겨울",
          "speaker_id": "1",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002738.1.18",
          "form": "중간이 없어",
          "original_form": "중간이 없어",
          "speaker_id": "2",
          "time": "20210820 15:22"
        },
        {
          "id": "MDRW2100002738.1.19",
          "form": "아주 짧게있지ㅋ",
          "original_form": "아주 짧게있지ㅋ",
          "speaker_id": "1",
          "time": "20210820 15:22"
        },
        {
          "id": "MDRW2100002738.1.20",
          "form": "ㅋㅋ 그렇지\r\n스치듯 안녕이지ㅋ",
          "original_form": "ㅋㅋ 그렇지\r\n스치듯 안녕이지ㅋ",
          "speaker_id": "2",
          "time": "20210820 15:22"
        },
        {
          "id": "MDRW2100002738.1.21",
          "form": "태풍올라온다는데...",
          "original_form": "태풍올라온다는데...",
          "speaker_id": "1",
          "time": "20210823 15:11"
        },
        {
          "id": "MDRW2100002738.1.22",
          "form": "여긴 아직 비가 많이 안오네...",
          "original_form": "여긴 아직 비가 많이 안오네...",
          "speaker_id": "1",
          "time": "20210823 15:11"
        },
        {
          "id": "MDRW2100002738.1.23",
          "form": "응 태풍이 시렁~~\r\n한창 곡식 익어바는 계절에 태풍이 올까",
          "original_form": "응 태풍이 시렁~~\r\n한창 곡식 익어바는 계절에 태풍이 올까",
          "speaker_id": "2",
          "time": "20210823 15:11"
        },
        {
          "id": "MDRW2100002738.1.24",
          "form": "제주도에 비 많이 온데",
          "original_form": "제주도에 비 많이 온데",
          "speaker_id": "2",
          "time": "20210823 15:11"
        },
        {
          "id": "MDRW2100002738.1.25",
          "form": "그러게~올해는 태풍이 좀 늦네~",
          "original_form": "그러게~올해는 태풍이 좀 늦네~",
          "speaker_id": "1",
          "time": "20210823 15:12"
        },
        {
          "id": "MDRW2100002738.1.26",
          "form": "태풍 늦은거 아니지 않오?",
          "original_form": "태풍 늦은거 아니지 않오?",
          "speaker_id": "2",
          "time": "20210823 15:13"
        },
        {
          "id": "MDRW2100002738.1.27",
          "form": "그런가??",
          "original_form": "그런가??",
          "speaker_id": "1",
          "time": "20210823 15:15"
        },
        {
          "id": "MDRW2100002738.1.28",
          "form": "보통 7월말에 많이 왔던거 같어...",
          "original_form": "보통 7월말에 많이 왔던거 같어...",
          "speaker_id": "1",
          "time": "20210823 15:16"
        },
        {
          "id": "MDRW2100002738.1.29",
          "form": "그런가?ㅋ 난 8월말에서 9월초로 알고있음",
          "original_form": "그런가?ㅋ 난 8월말에서 9월초로 알고있음",
          "speaker_id": "2",
          "time": "20210823 15:16"
        },
        {
          "id": "MDRW2100002738.1.30",
          "form": "언능 태풍 지나갔으면 좋겠다!",
          "original_form": "언능 태풍 지나갔으면 좋겠다!",
          "speaker_id": "2",
          "time": "20210823 15:16"
        },
        {
          "id": "MDRW2100002738.1.31",
          "form": "비만 적당히 내리고 지나갔음 좋겠다 ㅎㅎ",
          "original_form": "비만 적당히 내리고 지나갔음 좋겠다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 15:17"
        }
      ]
    }
