        {
          "id": "MDRW2100019357.1.1",
          "form": "ㅎㅎ 주말엔 푹 쉬면 되겠네",
          "original_form": "ㅎㅎ 주말엔 푹 쉬면 되겠네",
          "speaker_id": "2",
          "time": "20210827 18:16"
        },
        {
          "id": "MDRW2100019357.1.2",
          "form": "내일 비 안오면 산책하기로 했잖아",
          "original_form": "내일 비 안오면 산책하기로 했잖아",
          "speaker_id": "1",
          "time": "20210827 18:17"
        },
        {
          "id": "MDRW2100019357.1.3",
          "form": "ㅇㅇ 가면 되지",
          "original_form": "ㅇㅇ 가면 되지",
          "speaker_id": "2",
          "time": "20210827 18:17"
        },
        {
          "id": "MDRW2100019357.1.4",
          "form": "어디로 갈까?",
          "original_form": "어디로 갈까?",
          "speaker_id": "1",
          "time": "20210827 18:17"
        },
        {
          "id": "MDRW2100019357.1.5",
          "form": "푸른 수목원이나 한바퀴 돌까?",
          "original_form": "푸른 수목원이나 한바퀴 돌까?",
          "speaker_id": "2",
          "time": "20210827 18:17"
        },
        {
          "id": "MDRW2100019357.1.6",
          "form": "아 푸른 수목원? 요즘 시원해서 진짜 좋겠다 ㅎㅎ",
          "original_form": "아 푸른 수목원? 요즘 시원해서 진짜 좋겠다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 18:17"
        },
        {
          "id": "MDRW2100019357.1.7",
          "form": "ㅇㅇ 거기 경치 좋고 넓어서 애들도 좋아할듯",
          "original_form": "ㅇㅇ 거기 경치 좋고 넓어서 애들도 좋아할듯",
          "speaker_id": "2",
          "time": "20210827 18:18"
        },
        {
          "id": "MDRW2100019357.1.8",
          "form": "좀 시원하게 오전 일찍이나 오후 늦게 가야겠내",
          "original_form": "좀 시원하게 오전 일찍이나 오후 늦게 가야겠내",
          "speaker_id": "1",
          "time": "20210827 18:18"
        },
        {
          "id": "MDRW2100019357.1.9",
          "form": "내일 아마 흐려서 아무때나 가도 될 걸",
          "original_form": "내일 아마 흐려서 아무때나 가도 될 걸",
          "speaker_id": "2",
          "time": "20210827 18:18"
        },
        {
          "id": "MDRW2100019357.1.10",
          "form": "그럴려나 ㅎㅎ 하긴 오늘처럼 흐리면",
          "original_form": "그럴려나 ㅎㅎ 하긴 오늘처럼 흐리면",
          "speaker_id": "1",
          "time": "20210827 18:18"
        },
        {
          "id": "MDRW2100019357.1.11",
          "form": "내일은 거기만 한바퀴 돌아도 운동되겠네",
          "original_form": "내일은 거기만 한바퀴 돌아도 운동되겠네",
          "speaker_id": "2",
          "time": "20210827 18:19"
        },
        {
          "id": "MDRW2100019357.1.12",
          "form": "ㅎㅎ 좋으넹",
          "original_form": "ㅎㅎ 좋으넹",
