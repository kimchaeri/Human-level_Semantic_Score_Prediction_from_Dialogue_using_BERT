      "utterance": [
        {
          "id": "MDRW2100026212.1.1",
          "form": "자몽피지오 맛나요?",
          "original_form": "자몽피지오 맛나요?",
          "speaker_id": "1",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100026212.1.2",
          "form": "네 맛나요",
          "original_form": "네 맛나요",
          "speaker_id": "2",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100026212.1.3",
          "form": "다른 피지오는 안먹나요?",
          "original_form": "다른 피지오는 안먹나요?",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026212.1.4",
          "form": "오늘 라임피지오??먹었ㅇ요",
          "original_form": "오늘 라임피지오??먹었ㅇ요",
          "speaker_id": "2",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026212.1.5",
          "form": "그건 맛있나요?",
          "original_form": "그건 맛있나요?",
          "speaker_id": "1",
          "time": "20210901 20:54"
        },
        {
          "id": "MDRW2100026212.1.6",
          "form": "음 네 뭐 그럭저럭",
          "original_form": "음 네 뭐 그럭저럭",
          "speaker_id": "2",
          "time": "20210901 20:58"
        },
        {
          "id": "MDRW2100026212.1.7",
          "form": "패션탱고티레몬에이드 피지오 드세요",
          "original_form": "패션탱고티레몬에이드 피지오 드세요",
          "speaker_id": "1",
          "time": "20210901 20:59"
        },
        {
          "id": "MDRW2100026212.1.8",
          "form": "그게 젤존맛이에요",
          "original_form": "그게 젤존맛이에요",
          "speaker_id": "1",
          "time": "20210901 20:59"
        },
        {
          "id": "MDRW2100026212.1.9",
          "form": "아하 그런가요",
          "original_form": "아하 그런가요",
          "speaker_id": "2",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026212.1.10",
          "form": "맛없으면어쩌죠",
          "original_form": "맛없으면어쩌죠",
          "speaker_id": "2",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026212.1.11",
          "form": "돈 버린거죠",
          "original_form": "돈 버린거죠",
          "speaker_id": "1",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026212.1.12",
          "form": "어쩔수없어요",
          "original_form": "어쩔수없어요",
          "speaker_id": "1",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026212.1.13",
          "form": "name1님만믿었는데",
          "original_form": "&name1&님만믿었는데",
          "speaker_id": "2",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026212.1.14",
          "form": "너무하네요무책임하세요",
          "original_form": "너무하네요무책임하세요",
          "speaker_id": "2",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026212.1.15",
          "form": "절 믿으시면 어떻게요?",
          "original_form": "절 믿으시면 어떻게요?",
          "speaker_id": "1",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026212.1.16",
          "form": "그럼 저에게 버려주세요",
          "original_form": "그럼 저에게 버려주세요",
          "speaker_id": "1",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100026212.1.17",
          "form": "제 최애입니다",
          "original_form": "제 최애입니다",
          "speaker_id": "1",
          "time": "20210901 21:08"
        },
        {
          "id": "MDRW2100026212.1.18",
          "form": "아하..참고할게요",
          "original_form": "아하..참고할게요",
          "speaker_id": "2",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100026212.1.19",
          "form": "넴..",
          "original_form": "넴..",
          "speaker_id": "1",
          "time": "20210901 21:09"
        },
        {
          "id": "MDRW2100026212.1.20",
          "form": "다른 음료는 뭐 좋아하세요?",
          "original_form": "다른 음료는 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100026212.1.21",
          "form": "다른 브랜드..?",
          "original_form": "다른 브랜드..?",
          "speaker_id": "1",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100026212.1.22",
          "form": "밀크티를",
          "original_form": "밀크티를",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100026212.1.23",
          "form": "좋아하진않는데",
          "original_form": "좋아하진않는데",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100026212.1.24",
          "form": "뭔가 이상의밀크티를 찾아헤매는거같아요",
          "original_form": "뭔가 이상의밀크티를 찾아헤매는거같아요",
          "speaker_id": "2",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100026212.1.25",
          "form": "밀크티라.... 전 밀크티는 별로 안좋아하는거같아요",
          "original_form": "밀크티라.... 전 밀크티는 별로 안좋아하는거같아요",
          "speaker_id": "1",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100026212.1.26",
          "form": "우유가 별로랄까",
          "original_form": "우유가 별로랄까",
          "speaker_id": "1",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100026212.1.27",
