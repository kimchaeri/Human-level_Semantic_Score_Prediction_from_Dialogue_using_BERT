      "utterance": [
        {
          "id": "MDRW2100044780.1.1",
          "form": "자녀가 2명인가요?",
          "original_form": "자녀가 2명인가요?",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044780.1.2",
          "form": "네 둘이에용",
          "original_form": "네 둘이에용",
          "speaker_id": "2",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044780.1.3",
          "form": "성별은요?",
          "original_form": "성별은요?",
          "speaker_id": "1",
          "time": "20210907 23:44"
        },
        {
          "id": "MDRW2100044780.1.4",
          "form": "여아 둘 ㅎㅎ",
          "original_form": "여아 둘 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044780.1.5",
          "form": "앗!! 금메달이예요~ 전 목매달...",
          "original_form": "앗!! 금메달이예요~ 전 목매달...",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044780.1.6",
          "form": "아ㅠㅠㅠ 힘드시져.",
          "original_form": "아ㅠㅠㅠ 힘드시져.",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044780.1.7",
          "form": "네 ㅠ 아들 에너지는 어마어마 하거든요",
          "original_form": "네 ㅠ 아들 에너지는 어마어마 하거든요",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044780.1.8",
          "form": "와우 조카만 봐도 그래요ㅠㅠ",
          "original_form": "와우 조카만 봐도 그래요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044780.1.9",
          "form": "전 감당이 안될듯 해여",
          "original_form": "전 감당이 안될듯 해여",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044780.1.10",
          "form": "전 딸을 낳으려고 했는데 현실은 아들 둘이네요",
          "original_form": "전 딸을 낳으려고 했는데 현실은 아들 둘이네요",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044780.1.11",
          "form": "저 사실 아들 낳고팠는데 실패..그렇지만 딸둘은 괜찮으니ㅠ",
          "original_form": "저 사실 아들 낳고팠는데 실패..그렇지만 딸둘은 괜찮으니ㅠ",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044780.1.12",
          "form": "그건 실패하셔도 되요~ 아들 힘듭니다 ㅠ",
          "original_form": "그건 실패하셔도 되요~ 아들 힘듭니다 ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044780.1.13",
          "form": "ㅠㅠ 아들1딸1 엄마한테는 좋을것 같았는데",
          "original_form": "ㅠㅠ 아들1딸1 엄마한테는 좋을것 같았는데",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044780.1.14",
          "form": "저도 다 키워보고 싶었는데요",
          "original_form": "저도 다 키워보고 싶었는데요",
