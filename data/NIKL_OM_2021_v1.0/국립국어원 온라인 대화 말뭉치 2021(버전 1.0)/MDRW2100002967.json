      "utterance": [
        {
          "id": "MDRW2100002967.1.1",
          "form": "유튜브에서 주로 뭐 봐요?",
          "original_form": "유튜브에서 주로 뭐 봐요?",
          "speaker_id": "1",
          "time": "20210820 17:08"
        },
        {
          "id": "MDRW2100002967.1.2",
          "form": "저는 고양이 나오는 채널이나 브이로그나 슬라임 많이 봐용",
          "original_form": "저는 고양이 나오는 채널이나 브이로그나 슬라임 많이 봐용",
          "speaker_id": "2",
          "time": "20210820 17:09"
        },
        {
          "id": "MDRW2100002967.1.3",
          "form": "name1님은 어떤거 많이보세요?",
          "original_form": "&name1&님은 어떤거 많이보세요?",
          "speaker_id": "2",
          "time": "20210820 17:10"
        },
        {
          "id": "MDRW2100002967.1.4",
          "form": "저는 바이올린을 좋아해서",
          "original_form": "저는 바이올린을 좋아해서",
          "speaker_id": "1",
          "time": "20210820 17:11"
        },
        {
          "id": "MDRW2100002967.1.5",
          "form": "바이올린 관련 영상을 많이 봐요",
          "original_form": "바이올린 관련 영상을 많이 봐요",
          "speaker_id": "1",
          "time": "20210820 17:11"
        },
        {
          "id": "MDRW2100002967.1.6",
          "form": "우와 뭔가 고상?한 유튜브 취향이시네용!",
          "original_form": "우와 뭔가 고상?한 유튜브 취향이시네용!",
          "speaker_id": "2",
          "time": "20210820 17:12"
        },
        {
          "id": "MDRW2100002967.1.7",
          "form": "앗ㅋㅋ 아니에요",
          "original_form": "앗ㅋㅋ 아니에요",
          "speaker_id": "1",
          "time": "20210820 17:12"
        },
        {
          "id": "MDRW2100002967.1.8",
          "form": "바이올린 소리가 넘 좋아서 보거나 틀어두면 기분이 좋아져요",
          "original_form": "바이올린 소리가 넘 좋아서 보거나 틀어두면 기분이 좋아져요",
          "speaker_id": "1",
          "time": "20210820 17:12"
        },
        {
          "id": "MDRW2100002967.1.9",
          "form": "고양이랑 슬라임도 보고있으면 기분 좋을거 같아요",
          "original_form": "고양이랑 슬라임도 보고있으면 기분 좋을거 같아요",
          "speaker_id": "1",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002967.1.10",
          "form": "ㅋㅋ저는 음악은 아이돌밖에 안들어서 그런지 뭔가 대단해보이세용! 저는 멜론 탑백귀라...ㅠㅠ",
          "original_form": "ㅋㅋ저는 음악은 아이돌밖에 안들어서 그런지 뭔가 대단해보이세용! 저는 멜론 탑백귀라...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002967.1.11",
          "form": "우와 아이돌 누구 좋아하세요?",
          "original_form": "우와 아이돌 누구 좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002967.1.12",
          "form": "전 요즘 아이돌 노래는 에스파 넥스트 레벨밖에 몰라요",
          "original_form": "전 요즘 아이돌 노래는 에스파 넥스트 레벨밖에 몰라요",
          "speaker_id": "1",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002967.1.13",
          "form": "넧ㅌ~ 레블~",
          "original_form": "넧ㅌ~ 레블~",
          "speaker_id": "1",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002967.1.14",
          "form": "저는 막 좋아하는 그룹은 없고 멜론에있음 다들어용ㅋㅋㅋ저는 요즘엔 트와이스 알콜프리 많이들었어용",
          "original_form": "저는 막 좋아하는 그룹은 없고 멜론에있음 다들어용ㅋㅋㅋ저는 요즘엔 트와이스 알콜프리 많이들었어용",
          "speaker_id": "2",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002967.1.15",
          "form": "넥스트레벨도 중독성넘좋죵",
          "original_form": "넥스트레벨도 중독성넘좋죵",
          "speaker_id": "2",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002967.1.16",
          "form": "알콜프리도 알아요!",
          "original_form": "알콜프리도 알아요!",
          "speaker_id": "1",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002967.1.17",
          "form": "춤이 신기해서 커버영상 몇개 봤어요",
          "original_form": "춤이 신기해서 커버영상 몇개 봤어요",
          "speaker_id": "1",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002967.1.18",
          "form": "이지리스닝이라고하나용? 쉽게 들을수있어서좋더라구용 춤도예쁘구용ㅋㅋㅋ",
          "original_form": "이지리스닝이라고하나용? 쉽게 들을수있어서좋더라구용 춤도예쁘구용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002967.1.19",
          "form": "오 이지리스닝은 처음들어봐요",
          "original_form": "오 이지리스닝은 처음들어봐요",
          "speaker_id": "1",
          "time": "20210820 17:16"
        },
        {
          "id": "MDRW2100002967.1.20",
          "form": "ㅋㅋㅋㅋ사실 저는 여돌음악이면 다좋아해용 신나서요ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋ사실 저는 여돌음악이면 다좋아해용 신나서요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002967.1.21",
          "form": "하긴 그래요",
          "original_form": "하긴 그래요",
          "speaker_id": "1",
          "time": "20210820 17:18"
        },
        {
          "id": "MDRW2100002967.1.22",
          "form": "여돌음악은 보통 신나게 듣기 좋은 음악이어서 잘 유행하는거 같아요",
          "original_form": "여돌음악은 보통 신나게 듣기 좋은 음악이어서 잘 유행하는거 같아요",
          "speaker_id": "1",
          "time": "20210820 17:19"
        }
      ]
    }
