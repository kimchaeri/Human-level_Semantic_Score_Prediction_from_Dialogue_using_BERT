      "utterance": [
        {
          "id": "MDRW2100023297.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210829 23:48"
        },
        {
          "id": "MDRW2100023297.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210829 23:59"
        },
        {
          "id": "MDRW2100023297.1.3",
          "form": "이번 올림픽 보셨나요??",
          "original_form": "이번 올림픽 보셨나요??",
          "speaker_id": "2",
          "time": "20210829 23:59"
        },
        {
          "id": "MDRW2100023297.1.4",
          "form": "네 올림픽전엔 별관심없었다가 시작과동시에 과몰입해서 열심히봤어요ㅋㅋㅋㅋ",
          "original_form": "네 올림픽전엔 별관심없었다가 시작과동시에 과몰입해서 열심히봤어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:00"
        },
        {
          "id": "MDRW2100023297.1.5",
          "form": "어떤경기 가장재밌으셨어요",
          "original_form": "어떤경기 가장재밌으셨어요",
          "speaker_id": "1",
          "time": "20210830 00:00"
        },
        {
          "id": "MDRW2100023297.1.6",
          "form": "저도 그런 것 같아요 이번은 특히...!",
          "original_form": "저도 그런 것 같아요 이번은 특히...!",
          "speaker_id": "2",
          "time": "20210830 09:46"
        },
        {
          "id": "MDRW2100023297.1.7",
          "form": "저는 양궁하고 배구요ㅜㅜ",
          "original_form": "저는 양궁하고 배구요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 09:46"
        },
        {
          "id": "MDRW2100023297.1.8",
          "form": "너무 멋있고 드라마같았어요 둘 다ㅠㅠ",
          "original_form": "너무 멋있고 드라마같았어요 둘 다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 09:46"
        },
        {
          "id": "MDRW2100023297.1.9",
          "form": "저도 그 두개 진짜 재밌게봤어요ㅜㅜㅜ",
          "original_form": "저도 그 두개 진짜 재밌게봤어요ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 13:16"
        },
        {
          "id": "MDRW2100023297.1.10",
          "form": "배구 한일전에서는 무릎꿇고 보고ㅜㅜ",
          "original_form": "배구 한일전에서는 무릎꿇고 보고ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 13:16"
        },
        {
          "id": "MDRW2100023297.1.11",
          "form": "맞아요 배구랑 양궁 둘다 모든 경기가 다 극적이고",
          "original_form": "맞아요 배구랑 양궁 둘다 모든 경기가 다 극적이고",
          "speaker_id": "2",
          "time": "20210830 20:40"
        },
        {
          "id": "MDRW2100023297.1.12",
          "form": "손에 땀을 쥐게했죠!",
          "original_form": "손에 땀을 쥐게했죠!",
          "speaker_id": "2",
          "time": "20210830 20:40"
        },
        {
          "id": "MDRW2100023297.1.13",
          "form": "둘다 연장까지 가고ㅜㅜ",
          "original_form": "둘다 연장까지 가고ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 20:40"
        },
        {
          "id": "MDRW2100023297.1.14",
          "form": "맞아요 그래서 경기보러가고싶고그랬는데",
          "original_form": "맞아요 그래서 경기보러가고싶고그랬는데",
          "speaker_id": "1",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100023297.1.15",
          "form": "저희지역엔 없고...한참가야되도라구여",
          "original_form": "저희지역엔 없고...한참가야되도라구여",
          "speaker_id": "1",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100023297.1.16",
          "form": "배구 경기장이요?",
          "original_form": "배구 경기장이요?",
          "speaker_id": "2",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100023297.1.17",
          "form": "배구 경기장은 어디에 있나요??",
          "original_form": "배구 경기장은 어디에 있나요??",
          "speaker_id": "2",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100023297.1.18",
          "form": "코보컵은 경기도에서 하더라구여",
          "original_form": "코보컵은 경기도에서 하더라구여",
          "speaker_id": "1",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100023297.1.19",
          "form": "체육관에서ㅜ",
          "original_form": "체육관에서ㅜ",
          "speaker_id": "1",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100023297.1.20",
          "form": "지역별 구단도 있어요",
          "original_form": "지역별 구단도 있어요",
          "speaker_id": "1",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100023297.1.21",
          "form": "아하 경기도쪽이군요ㅜㅜ",
          "original_form": "아하 경기도쪽이군요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 22:11"
        },
        {
          "id": "MDRW2100023297.1.22",
          "form": "저는 양궁도 해보고싶어서 양궁카페 가보고싶은데",
          "original_form": "저는 양궁도 해보고싶어서 양궁카페 가보고싶은데",
          "speaker_id": "2",
          "time": "20210830 22:11"
        },
        {
          "id": "MDRW2100023297.1.23",
          "form": "활이 생각보다 엄청 쎄대요...! 선수들 거는 웬만한 사람들이 당기기도 힘들다고 하더라구요?!",
          "original_form": "활이 생각보다 엄청 쎄대요...! 선수들 거는 웬만한 사람들이 당기기도 힘들다고 하더라구요?!",
          "speaker_id": "2",
          "time": "20210830 22:12"
        },
        {
          "id": "MDRW2100023297.1.24",
          "form": "맞아요 양궁카페용은 초등학생용인가 그렇다고들었는데",
          "original_form": "맞아요 양궁카페용은 초등학생용인가 그렇다고들었는데",
          "speaker_id": "1",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100023297.1.25",
          "form": "그것도엄청무겁다면서요",
          "original_form": "그것도엄청무겁다면서요",
          "speaker_id": "1",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100023297.1.26",
          "form": "그렇군요ㅜㅜ 아직 못가봤어요",
          "original_form": "그렇군요ㅜㅜ 아직 못가봤어요",
          "speaker_id": "2",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100023297.1.27",
          "form": "다음에 꼭 가봐야겠어요!!",
          "original_form": "다음에 꼭 가봐야겠어요!!",
          "speaker_id": "2",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100023297.1.28",
          "form": "네 저도 기회가되면 꼭가봐서 활한번 쏘고싶어요",
          "original_form": "네 저도 기회가되면 꼭가봐서 활한번 쏘고싶어요",
          "speaker_id": "1",
          "time": "20210830 22:42"
        },
        {
          "id": "MDRW2100023297.1.29",
          "form": "코로나라 문닫은곳이많더라구여ㅜ",
          "original_form": "코로나라 문닫은곳이많더라구여ㅜ",
          "speaker_id": "1",
          "time": "20210830 22:43"
        }
      ]
    }
