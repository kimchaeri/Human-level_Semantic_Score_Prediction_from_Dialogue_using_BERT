      "utterance": [
        {
          "id": "MDRW2100035763.1.1",
          "form": "대충 먹었어요",
          "original_form": "대충 먹었어요",
          "speaker_id": "2",
          "time": "20210830 01:01"
        },
        {
          "id": "MDRW2100035763.1.2",
          "form": "아까 점심을 늦게 먹고 저녁은 과자대충먹고요",
          "original_form": "아까 점심을 늦게 먹고 저녁은 과자대충먹고요",
          "speaker_id": "2",
          "time": "20210830 01:01"
        },
        {
          "id": "MDRW2100035763.1.3",
          "form": "앗 저녁이 과자가 ㅎㅎ",
          "original_form": "앗 저녁이 과자가 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:01"
        },
        {
          "id": "MDRW2100035763.1.4",
          "form": "칙촉 티라미수맛 드셔보셨어요? 이거 맛있던데요",
          "original_form": "칙촉 티라미수맛 드셔보셨어요? 이거 맛있던데요",
          "speaker_id": "2",
          "time": "20210830 01:01"
        },
        {
          "id": "MDRW2100035763.1.5",
          "form": "저도 과자진짜 좋아하는데",
          "original_form": "저도 과자진짜 좋아하는데",
          "speaker_id": "1",
          "time": "20210830 01:01"
        },
        {
          "id": "MDRW2100035763.1.6",
          "form": "어 먹어본것 같아요 ㅎ",
          "original_form": "어 먹어본것 같아요 ㅎ",
          "speaker_id": "1",
          "time": "20210830 01:01"
        },
        {
          "id": "MDRW2100035763.1.7",
          "form": "칙촉은 우유랑 먹으면 ㅎㅎ 캬",
          "original_form": "칙촉은 우유랑 먹으면 ㅎㅎ 캬",
          "speaker_id": "1",
          "time": "20210830 01:01"
        },
        {
          "id": "MDRW2100035763.1.8",
          "form": "안에 치즈처럼 생긴거 맛있더라구요",
          "original_form": "안에 치즈처럼 생긴거 맛있더라구요",
          "speaker_id": "2",
          "time": "20210830 01:01"
        },
        {
          "id": "MDRW2100035763.1.9",
          "form": "히히 맛있죠",
          "original_form": "히히 맛있죠",
          "speaker_id": "2",
          "time": "20210830 01:02"
        },
        {
          "id": "MDRW2100035763.1.10",
          "form": "우왕, 과자 뜯으면 애들이 우르르와서ㅠㅠ",
          "original_form": "우왕, 과자 뜯으면 애들이 우르르와서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 01:02"
        },
        {
          "id": "MDRW2100035763.1.11",
          "form": "애들이 못먹는 과자먹을땐 몰래 먹어야해요 ㅎㅎ",
          "original_form": "애들이 못먹는 과자먹을땐 몰래 먹어야해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:02"
        },
        {
          "id": "MDRW2100035763.1.12",
          "form": "엄마엄마 이러면서 달라고해요?",
          "original_form": "엄마엄마 이러면서 달라고해요?",
          "speaker_id": "2",
          "time": "20210830 01:02"
        },
        {
          "id": "MDRW2100035763.1.13",
          "form": "안주면 울고 ㅋㅋㅋ",
          "original_form": "안주면 울고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:02"
        },
        {
          "id": "MDRW2100035763.1.14",
          "form": "ㅋㅋㅋㅋ네",
          "original_form": "ㅋㅋㅋㅋ네",
          "speaker_id": "2",
          "time": "20210830 01:02"
        },
        {
          "id": "MDRW2100035763.1.15",
          "form": "귀여워요",
          "original_form": "귀여워요",
          "speaker_id": "2",
          "time": "20210830 01:02"
        },
        {
          "id": "MDRW2100035763.1.16",
          "form": "첫째는 초딩이라 혼자꺼내먹는데",
          "original_form": "첫째는 초딩이라 혼자꺼내먹는데",
          "speaker_id": "1",
          "time": "20210830 01:03"
        },
        {
          "id": "MDRW2100035763.1.17",
          "form": "둘때가 밥먹을때 먹을려고 해서 자꾸 못먹게 해요ㅠㅠ",
          "original_form": "둘때가 밥먹을때 먹을려고 해서 자꾸 못먹게 해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 01:03"
        },
        {
          "id": "MDRW2100035763.1.18",
          "form": "아아 네 ㅜㅜ",
          "original_form": "아아 네 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 01:03"
        },
        {
          "id": "MDRW2100035763.1.19",
          "form": "밥 먹는데 언니가 과자먹으면 ㅠㅠ 밥 안먹을라 하니까ㅠㅠ",
          "original_form": "밥 먹는데 언니가 과자먹으면 ㅠㅠ 밥 안먹을라 하니까ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 01:03"
        },
        {
          "id": "MDRW2100035763.1.20",
          "form": "아 그죠...",
          "original_form": "아 그죠...",
          "speaker_id": "2",
          "time": "20210830 01:03"
        },
        {
          "id": "MDRW2100035763.1.21",
          "form": "요즘은 이상한 과자들도 되게 많이 나오더라구요ㅠㅠ",
          "original_form": "요즘은 이상한 과자들도 되게 많이 나오더라구요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 01:03"
        }
      ]
