      "utterance": [
        {
          "id": "MDRW2100036385.1.1",
          "form": "쿼카 아시나요?!",
          "original_form": "쿼카 아시나요?!",
          "speaker_id": "1",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100036385.1.2",
          "form": "너무 귀엽죠ㅠㅠ",
          "original_form": "너무 귀엽죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036385.1.3",
          "form": "설마 쿼카 키우시나요??",
          "original_form": "설마 쿼카 키우시나요??",
          "speaker_id": "2",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036385.1.4",
          "form": "아니욬ㅋㅋ ! 키우고 싶네요 ㅜㅜ",
          "original_form": "아니욬ㅋㅋ ! 키우고 싶네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036385.1.5",
          "form": "인형은 있어요 ㅎㅎ",
          "original_form": "인형은 있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036385.1.6",
          "form": "쿼카는 반려동물로 못 데려오겠죠? ㅎㅎ",
          "original_form": "쿼카는 반려동물로 못 데려오겠죠? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036385.1.7",
          "form": "저는 고양이 한 마리 키우고 있어요!",
          "original_form": "저는 고양이 한 마리 키우고 있어요!",
          "speaker_id": "2",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036385.1.8",
          "form": "헐 진짜요?",
          "original_form": "헐 진짜요?",
          "speaker_id": "1",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036385.1.9",
          "form": "무슨종이에요?!",
          "original_form": "무슨종이에요?!",
          "speaker_id": "1",
          "time": "20210830 23:44"
        },
        {
          "id": "MDRW2100036385.1.10",
          "form": "코숏이에요! 근데 어떤 종이 섞였는지 모르겠지만 강아지만해요 ㅎㅎ",
          "original_form": "코숏이에요! 근데 어떤 종이 섞였는지 모르겠지만 강아지만해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100036385.1.11",
          "form": "우와 ! 뭔가 고양이계의 한정판같은 느낌이군요 ㅋㅋㅋ !",
          "original_form": "우와 ! 뭔가 고양이계의 한정판같은 느낌이군요 ㅋㅋㅋ !",
          "speaker_id": "1",
          "time": "20210830 23:47"
        },
        {
          "id": "MDRW2100036385.1.12",
          "form": "몇살이에요?",
          "original_form": "몇살이에요?",
          "speaker_id": "1",
          "time": "20210830 23:47"
        },
        {
          "id": "MDRW2100036385.1.13",
          "form": "6살이에요!",
          "original_form": "6살이에요!",
          "speaker_id": "2",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100036385.1.14",
          "form": "어떤 종이 섞였는지 알아야 유전병 대비를 할텐데 조금 걱정이에요 ㅎㅎ",
          "original_form": "어떤 종이 섞였는지 알아야 유전병 대비를 할텐데 조금 걱정이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100036385.1.15",
          "form": "아 헐 !! 그런거는 어떻게 알아볼수가 없나요ㅜㅜ?",
          "original_form": "아 헐 !! 그런거는 어떻게 알아볼수가 없나요ㅜㅜ?",
          "speaker_id": "1",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100036385.1.16",
          "form": "지금 다니는 병원에서는 일단 방법이 없나봐요 ㅎㅎ",
          "original_form": "지금 다니는 병원에서는 일단 방법이 없나봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036385.1.17",
          "form": "근데 또 병원마다 진단이 다른 경우도 있어서 고양이 전문병원을 가야하는데 타지역이라 쉽지가않네요ㅠㅠ ㅎㅎ",
          "original_form": "근데 또 병원마다 진단이 다른 경우도 있어서 고양이 전문병원을 가야하는데 타지역이라 쉽지가않네요ㅠㅠ ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036385.1.18",
          "form": "아 그렇겠네요ㅜㅜ 또 케이지에 넣어서 가야되지않나요ㅜㅜ",
          "original_form": "아 그렇겠네요ㅜㅜ 또 케이지에 넣어서 가야되지않나요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100036385.1.19",
          "form": "유전병은 보통 어떤 종류가 있나요??",
          "original_form": "유전병은 보통 어떤 종류가 있나요??",
          "speaker_id": "1",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100036385.1.20",
          "form": "제일 유명한 유전병은.. 폴드 같은 종의 애들은 뼈나 연골이 녹아요ㅠㅠ",
          "original_form": "제일 유명한 유전병은.. 폴드 같은 종의 애들은 뼈나 연골이 녹아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100036385.1.21",
          "form": "심한 애들은 눈도 흘러내리거든요.. 그게 다 품종묘 얻으려고 근친교배하고, 같은 품종끼리만 교배해서 그래요ㅠㅠ",
          "original_form": "심한 애들은 눈도 흘러내리거든요.. 그게 다 품종묘 얻으려고 근친교배하고, 같은 품종끼리만 교배해서 그래요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100036385.1.22",
          "form": "헐헐 ㅜㅜㅜㅜ",
          "original_form": "헐헐 ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100036385.1.23",
          "form": "아 완전 인간의 욕심이",
          "original_form": "아 완전 인간의 욕심이",
          "speaker_id": "1",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100036385.1.24",
          "form": "인간의 욕심이",
          "original_form": "인간의 욕심이",
          "speaker_id": "1",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100036385.1.25",
          "form": "애기들을 ㅜㅜ 고통스럽게하네요 ㅜㅜ",
          "original_form": "애기들을 ㅜㅜ 고통스럽게하네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 23:57"
        }
      ]
    }
