      "utterance": [
        {
          "id": "MDRW2100038022.1.1",
          "form": "와 순간 욕 나왔네요",
          "original_form": "와 순간 욕 나왔네요",
          "speaker_id": "1",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100038022.1.2",
          "form": "진짜 제품 후기에 완전 악플 적는 마음 알수 있을듯",
          "original_form": "진짜 제품 후기에 완전 악플 적는 마음 알수 있을듯",
          "speaker_id": "1",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100038022.1.3",
          "form": "저 누가 이거 산다고하면",
          "original_form": "저 누가 이거 산다고하면",
          "speaker_id": "2",
          "time": "20210901 22:31"
        },
        {
          "id": "MDRW2100038022.1.4",
          "form": "도시락 싸들고다니면서 말리고싶어요",
          "original_form": "도시락 싸들고다니면서 말리고싶어요",
          "speaker_id": "2",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100038022.1.5",
          "form": "미투요.,",
          "original_form": "미투요.,",
          "speaker_id": "1",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100038022.1.6",
          "form": "저솔직히 비만인상태인데 그런사람일수록 더",
          "original_form": "저솔직히 비만인상태인데 그런사람일수록 더",
          "speaker_id": "2",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100038022.1.7",
          "form": "48시간까지 얼마 남았죠",
          "original_form": "48시간까지 얼마 남았죠",
          "speaker_id": "1",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100038022.1.8",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100038022.1.9",
          "form": "효과가 드라마틱해야하는거 아닌가 란생각마저들어요",
          "original_form": "효과가 드라마틱해야하는거 아닌가 란생각마저들어요",
          "speaker_id": "2",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100038022.1.10",
          "form": "40에서 1키로빼는거보다 80에서 2키로를빼줘야지!",
          "original_form": "40에서 1키로빼는거보다 80에서 2키로를빼줘야지!",
          "speaker_id": "2",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100038022.1.11",
          "form": "진짜 눈먼돈 노린 제품이네요",
          "original_form": "진짜 눈먼돈 노린 제품이네요",
          "speaker_id": "1",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100038022.1.12",
          "form": "아 뭔가 약간 이성을잃었군요ㅋㅋ",
          "original_form": "아 뭔가 약간 이성을잃었군요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100038022.1.13",
          "form": "독소빼는 효과도 모르겠고",
          "original_form": "독소빼는 효과도 모르겠고",
          "speaker_id": "1",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100038022.1.14",
          "form": "아녜요ㅎ",
          "original_form": "아녜요ㅎ",
          "speaker_id": "1",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100038022.1.15",
          "form": "저도 나쁜말 하지 말란 알림 계속 들었어요ㅎ",
          "original_form": "저도 나쁜말 하지 말란 알림 계속 들었어요ㅎ",
          "speaker_id": "1",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100038022.1.16",
          "form": "48시간까지 얼마 남았죠",
          "original_form": "48시간까지 얼마 남았죠",
          "speaker_id": "1",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100038022.1.17",
          "form": "음 이제거의 끝난거 아닌가요",
          "original_form": "음 이제거의 끝난거 아닌가요",
          "speaker_id": "2",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100038022.1.18",
          "form": "음 어제9쯤 첫 주스마셨으니",
          "original_form": "음 어제9쯤 첫 주스마셨으니",
          "speaker_id": "2",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100038022.1.19",
          "form": "낼 9시까지는 더있어보게요",
          "original_form": "낼 9시까지는 더있어보게요",
          "speaker_id": "2",
          "time": "20210901 22:40"
        },
        {
          "id": "MDRW2100038022.1.20",
          "form": "네네 아직 10시간은 남았으니",
          "original_form": "네네 아직 10시간은 남았으니",
          "speaker_id": "1",
          "time": "20210901 22:46"
        },
        {
          "id": "MDRW2100038022.1.21",
          "form": "그래도 속 비우고 다요트 시작하면 좋다? 이런말 들은 것도 같으니",
          "original_form": "그래도 속 비우고 다요트 시작하면 좋다? 이런말 들은 것도 같으니",
          "speaker_id": "1",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100038022.1.22",
          "form": "그지같은 자몽주스가 좀 그래도 맘 편히 드세요",
          "original_form": "그지같은 자몽주스가 좀 그래도 맘 편히 드세요",
          "speaker_id": "1",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100038022.1.23",
          "form": "자고일어나면 그래도 좀 더내려갈지도요",
          "original_form": "자고일어나면 그래도 좀 더내려갈지도요",
          "speaker_id": "2",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100038022.1.24",
          "form": "님 같은 의지면 뭐든 성공할듯요!",
          "original_form": "님 같은 의지면 뭐든 성공할듯요!",
          "speaker_id": "1",
          "time": "20210901 22:47"
        },
        {
          "id": "MDRW2100038022.1.25",
          "form": "아 근데 ㅋㅋ동생이 ㅋㅋ10개준게아니었어요",
          "original_form": "아 근데 ㅋㅋ동생이 ㅋㅋ10개준게아니었어요",
          "speaker_id": "2",
          "time": "20210901 22:48"
        }
      ]
