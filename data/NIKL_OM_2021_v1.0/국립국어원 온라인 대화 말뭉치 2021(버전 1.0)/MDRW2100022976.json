      "utterance": [
        {
          "id": "MDRW2100022976.1.1",
          "form": "일은 무슨일하세요?ㅎ",
          "original_form": "일은 무슨일하세요?ㅎ",
          "speaker_id": "1",
          "time": "20210829 19:56"
        },
        {
          "id": "MDRW2100022976.1.2",
          "form": "이직하고싶나요?",
          "original_form": "이직하고싶나요?",
          "speaker_id": "1",
          "time": "20210829 19:57"
        },
        {
          "id": "MDRW2100022976.1.3",
          "form": "아니요ㅎㅎit엔지니어요ㅎㅎㅎ",
          "original_form": "아니요ㅎㅎit엔지니어요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:06"
        },
        {
          "id": "MDRW2100022976.1.4",
          "form": "무슨 일하세요?",
          "original_form": "무슨 일하세요?",
          "speaker_id": "2",
          "time": "20210829 20:06"
        },
        {
          "id": "MDRW2100022976.1.5",
          "form": "우왐 ㅎ 아이티ㅎㅎ",
          "original_form": "우왐 ㅎ 아이티ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 20:09"
        },
        {
          "id": "MDRW2100022976.1.6",
          "form": "엄청 공부잘하셨겟네요ㅎ",
          "original_form": "엄청 공부잘하셨겟네요ㅎ",
          "speaker_id": "1",
          "time": "20210829 20:09"
        },
        {
          "id": "MDRW2100022976.1.7",
          "form": "전 선박관련 안전관리자에요ㅎ",
          "original_form": "전 선박관련 안전관리자에요ㅎ",
          "speaker_id": "1",
          "time": "20210829 20:09"
        },
        {
          "id": "MDRW2100022976.1.8",
          "form": "업무 스트레스는 없으세요?",
          "original_form": "업무 스트레스는 없으세요?",
          "speaker_id": "1",
          "time": "20210829 20:09"
        },
        {
          "id": "MDRW2100022976.1.9",
          "form": "아니요ㅋㅋㅋ개발하는건 아니라서ㅋㅋ오오오 선박 어려운데ㅋㅋ",
          "original_form": "아니요ㅋㅋㅋ개발하는건 아니라서ㅋㅋ오오오 선박 어려운데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:13"
        },
        {
          "id": "MDRW2100022976.1.10",
          "form": "있죠ㅋㅋ업무스트레스는 다 있을거같아요",
          "original_form": "있죠ㅋㅋ업무스트레스는 다 있을거같아요",
          "speaker_id": "2",
          "time": "20210829 20:13"
        },
        {
          "id": "MDRW2100022976.1.11",
          "form": "맞아요ㅎ it업계가 엄청 빡세다고 하는데 사실이에요?",
          "original_form": "맞아요ㅎ it업계가 엄청 빡세다고 하는데 사실이에요?",
          "speaker_id": "1",
          "time": "20210829 20:13"
        },
        {
          "id": "MDRW2100022976.1.12",
          "form": "저희도 많긴한데...ㅠ",
          "original_form": "저희도 많긴한데...ㅠ",
          "speaker_id": "1",
          "time": "20210829 20:13"
        },
        {
          "id": "MDRW2100022976.1.13",
          "form": "아아 프로젝트들어가면요ㅎㅎ선박쪽도 야근많던데ㅜ",
          "original_form": "아아 프로젝트들어가면요ㅎㅎ선박쪽도 야근많던데ㅜ",
          "speaker_id": "2",
          "time": "20210829 20:46"
        },
        {
          "id": "MDRW2100022976.1.14",
          "form": "글긴해요ㅠ 저희도 야근은 기본이고 주말에도 가끔나가요ㅎ",
          "original_form": "글긴해요ㅠ 저희도 야근은 기본이고 주말에도 가끔나가요ㅎ",
          "speaker_id": "1",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100022976.1.15",
          "form": "ㅜㅜ주말에 나가는게제일싫죠",
          "original_form": "ㅜㅜ주말에 나가는게제일싫죠",
          "speaker_id": "2",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100022976.1.16",
          "form": "맞아요ㅎㅎ 주말에는 안나가고싶은데",
          "original_form": "맞아요ㅎㅎ 주말에는 안나가고싶은데",
          "speaker_id": "1",
          "time": "20210829 21:46"
        },
        {
          "id": "MDRW2100022976.1.17",
          "form": "어쩔수없이 나갈때가 있아요ㅠ",
          "original_form": "어쩔수없이 나갈때가 있아요ㅠ",
          "speaker_id": "1",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100022976.1.18",
          "form": "그럼 따로 근무수당 주나요?",
          "original_form": "그럼 따로 근무수당 주나요?",
          "speaker_id": "2",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100022976.1.19",
          "form": "안주요ㅎㅎ",
          "original_form": "안주요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100022976.1.20",
          "form": "그거 주면 회사 망할껄요?ㅎㅎㅎ",
          "original_form": "그거 주면 회사 망할껄요?ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100022976.1.21",
          "form": "그죠 사실 저희도요ㅠㅠ",
          "original_form": "그죠 사실 저희도요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100022976.1.22",
          "form": "맞아요ㅎ",
          "original_form": "맞아요ㅎ",
          "speaker_id": "1",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100022976.1.23",
          "form": "회사는 돈을 안주죠ㅎㅎ",
          "original_form": "회사는 돈을 안주죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:56"
        },
        {
          "id": "MDRW2100022976.1.24",
          "form": "ㅠㅠ그나마 저흰 주말근무는 연차주긴하는데 못쓰면 그만..인",
          "original_form": "ㅠㅠ그나마 저흰 주말근무는 연차주긴하는데 못쓰면 그만..인",
          "speaker_id": "2",
          "time": "20210829 21:57"
        },
        {
          "id": "MDRW2100022976.1.25",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100022976.1.26",
          "form": "연차이야기가 나와서 말인데",
          "original_form": "연차이야기가 나와서 말인데",
          "speaker_id": "1",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100022976.1.27",
          "form": "우리회사는 여름휴가도 연차로 주고",
          "original_form": "우리회사는 여름휴가도 연차로 주고",
          "speaker_id": "1",
          "time": "20210829 21:58"
        },
        {
          "id": "MDRW2100022976.1.28",
          "form": "연차다 안쓰면 남잖아요?ㅎ 그럼 돈안줘요",
          "original_form": "연차다 안쓰면 남잖아요?ㅎ 그럼 돈안줘요",
          "speaker_id": "1",
          "time": "20210829 21:59"
        },
        {
          "id": "MDRW2100022976.1.29",
          "form": "여름휴가 연차는 너무하네요ㅜㅜ\n저희도 연차남아도 돈안줘요..에효",
          "original_form": "여름휴가 연차는 너무하네요ㅜㅜ\n저희도 연차남아도 돈안줘요..에효",
          "speaker_id": "2",
          "time": "20210829 22:04"
        },
        {
          "id": "MDRW2100022976.1.30",
          "form": "에휴...",
          "original_form": "에휴...",
          "speaker_id": "1",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100022976.1.31",
          "form": "다똑같네요ㅠ",
          "original_form": "다똑같네요ㅠ",
          "speaker_id": "1",
          "time": "20210829 22:08"
        },
        {
          "id": "MDRW2100022976.1.32",
          "form": "우리나라 회사들도 이제 좀 바껴야하는데 안바뀌네요",
          "original_form": "우리나라 회사들도 이제 좀 바껴야하는데 안바뀌네요",
          "speaker_id": "1",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100022976.1.33",
          "form": "ㅠㅠ 저희 회사는 주자고 말이 나왔는데 임윈진 한 분이 그렇게되면 다 주말근무한다고 나온다고 기준 세우기힘들다고 해서  다시 없던일된..",
          "original_form": "ㅠㅠ 저희 회사는 주자고 말이 나왔는데 임윈진 한 분이 그렇게되면 다 주말근무한다고 나온다고 기준 세우기힘들다고 해서  다시 없던일된..",
          "speaker_id": "2",
          "time": "20210829 22:14"
        },
        {
          "id": "MDRW2100022976.1.34",
          "form": "그냥 눈치보는거네요ㅎ",
          "original_form": "그냥 눈치보는거네요ㅎ",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100022976.1.35",
          "form": "임원진은 우리생각안해요ㅎ",
          "original_form": "임원진은 우리생각안해요ㅎ",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100022976.1.36",
          "form": "자기가 살길생각하죠ㅎ",
          "original_form": "자기가 살길생각하죠ㅎ",
          "speaker_id": "1",
          "time": "20210829 22:17"
        },
        {
          "id": "MDRW2100022976.1.37",
          "form": "그죵ㅜㅜ다들...",
          "original_form": "그죵ㅜㅜ다들...",
          "speaker_id": "2",
          "time": "20210829 22:37"
        },
        {
          "id": "MDRW2100022976.1.38",
          "form": "팀은 따로 있으신가요?",
          "original_form": "팀은 따로 있으신가요?",
          "speaker_id": "2",
          "time": "20210829 23:00"
        },
        {
          "id": "MDRW2100022976.1.39",
          "form": "팀이요?ㅎ 본부라고 거창하게 해놧어요ㅎㅎ",
          "original_form": "팀이요?ㅎ 본부라고 거창하게 해놧어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 23:42"
        },
        {
          "id": "MDRW2100022976.1.40",
          "form": "아 큰가보네요",
          "original_form": "아 큰가보네요",
          "speaker_id": "2",
          "time": "20210830 07:18"
        },
        {
          "id": "MDRW2100022976.1.41",
          "form": "아니에요ㅎㅎ 작은데 이름만거창하게",
          "original_form": "아니에요ㅎㅎ 작은데 이름만거창하게",
          "speaker_id": "1",
          "time": "20210830 22:18"
        },
        {
          "id": "MDRW2100022976.1.42",
          "form": "나참 직원이 많아야 본부인거지ㅎㅎㅎㅎ",
          "original_form": "나참 직원이 많아야 본부인거지ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 22:19"
        },
        {
          "id": "MDRW2100022976.1.43",
          "form": "구멍가게인데ㅎㅎ",
          "original_form": "구멍가게인데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 22:19"
        },
        {
          "id": "MDRW2100022976.1.44",
          "form": "저희 회사도 인원많지않은데 계열사 다빼고..",
          "original_form": "저희 회사도 인원많지않은데 계열사 다빼고..",
          "speaker_id": "2",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100022976.1.45",
          "form": "ㅠㅠ 다 내맘같지가 않아요ㅎㅎ",
          "original_form": "ㅠㅠ 다 내맘같지가 않아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100022976.1.46",
          "form": "오늘은 안힘들었어요?",
          "original_form": "오늘은 안힘들었어요?",
          "speaker_id": "1",
