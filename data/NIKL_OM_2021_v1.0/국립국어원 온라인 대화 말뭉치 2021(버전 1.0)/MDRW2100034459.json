      "utterance": [
        {
          "id": "MDRW2100034459.1.1",
          "form": "안녕하세요, 어떤 계절을 좋아하세요?",
          "original_form": "안녕하세요, 어떤 계절을 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 12:16"
        },
        {
          "id": "MDRW2100034459.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210828 12:17"
        },
        {
          "id": "MDRW2100034459.1.3",
          "form": "저는 가을을 제일 좋아해요",
          "original_form": "저는 가을을 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210828 12:17"
        },
        {
          "id": "MDRW2100034459.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 12:17"
        },
        {
          "id": "MDRW2100034459.1.5",
          "form": "저도 가을을 가장 좋아해요, 선선해서요ㅎㅎ",
          "original_form": "저도 가을을 가장 좋아해요, 선선해서요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 12:18"
        },
        {
          "id": "MDRW2100034459.1.6",
          "form": "그쵸 하늘도 푸르고 맑고",
          "original_form": "그쵸 하늘도 푸르고 맑고",
          "speaker_id": "2",
          "time": "20210828 12:18"
        },
        {
          "id": "MDRW2100034459.1.7",
          "form": "장마기간이 얼른 지나가면 좋겠어요",
          "original_form": "장마기간이 얼른 지나가면 좋겠어요",
          "speaker_id": "2",
          "time": "20210828 12:18"
        },
        {
          "id": "MDRW2100034459.1.8",
          "form": "맞아요ㅠ 오늘은 아직은 비가 안 오는데 또 모르겠네요ㅠ",
          "original_form": "맞아요ㅠ 오늘은 아직은 비가 안 오는데 또 모르겠네요ㅠ",
          "speaker_id": "1",
          "time": "20210828 12:19"
        },
        {
          "id": "MDRW2100034459.1.9",
          "form": "또 갑자기 올수도 있고ㅜㅜ",
          "original_form": "또 갑자기 올수도 있고ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 12:20"
        },
        {
          "id": "MDRW2100034459.1.10",
          "form": "비가안오니까 또 미세먼지가 올라가네요..",
          "original_form": "비가안오니까 또 미세먼지가 올라가네요..",
          "speaker_id": "2",
          "time": "20210828 12:20"
        },
        {
          "id": "MDRW2100034459.1.11",
          "form": "푸른 하늘을 본 지 오래됐어요ㅠㅜ",
          "original_form": "푸른 하늘을 본 지 오래됐어요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210828 12:22"
        },
        {
          "id": "MDRW2100034459.1.12",
          "form": "그러게요ㅜㅜ",
          "original_form": "그러게요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100034459.1.13",
          "form": "탁트인 하늘이 그립네요",
          "original_form": "탁트인 하늘이 그립네요",
          "speaker_id": "2",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100034459.1.14",
          "form": "매미 소리만 안 그립고 나머지가 모두 그리워요...",
          "original_form": "매미 소리만 안 그립고 나머지가 모두 그리워요...",
          "speaker_id": "1",
          "time": "20210828 12:39"
        },
        {
          "id": "MDRW2100034459.1.15",
          "form": "ㅋㅋㅋㅋ매미가 시끄럽긴해요",
          "original_form": "ㅋㅋㅋㅋ매미가 시끄럽긴해요",
          "speaker_id": "2",
          "time": "20210828 12:47"
        },
        {
          "id": "MDRW2100034459.1.16",
          "form": "아침에 매미 때문에 깼어요 ㅋㅋㅋ",
          "original_form": "아침에 매미 때문에 깼어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100034459.1.17",
          "form": "앜ㅋㅋㅋㅋ그래서",
          "original_form": "앜ㅋㅋㅋㅋ그래서",
          "speaker_id": "2",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100034459.1.18",
          "form": "모기보단 낫다고 생각해요..",
          "original_form": "모기보단 낫다고 생각해요..",
          "speaker_id": "2",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100034459.1.19",
          "form": "모기...아직 모기장 못 걷었어요ㅠ",
          "original_form": "모기...아직 모기장 못 걷었어요ㅠ",
