      "utterance": [
        {
          "id": "MDRW2100020181.1.1",
          "form": "에이스랑 에이스 사이에 크림? 든거 못봄?",
          "original_form": "에이스랑 에이스 사이에 크림? 든거 못봄?",
          "speaker_id": "1",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020181.1.2",
          "form": "내가 본게 맞나 모르겠어 치즈아니고 크림?",
          "original_form": "내가 본게 맞나 모르겠어 치즈아니고 크림?",
          "speaker_id": "2",
          "time": "20210828 02:40"
        },
        {
          "id": "MDRW2100020181.1.3",
          "form": "내가알기로는 크림인데 ㅋㅋㅋㅋ 에이스 샌드",
          "original_form": "내가알기로는 크림인데 ㅋㅋㅋㅋ 에이스 샌드",
          "speaker_id": "1",
          "time": "20210828 04:06"
        },
        {
          "id": "MDRW2100020181.1.4",
          "form": "암튼 그런거 초코버전이 신상으로 나옴ㅋㅋ",
          "original_form": "암튼 그런거 초코버전이 신상으로 나옴ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 04:06"
        },
        {
          "id": "MDRW2100020181.1.5",
          "form": "과자안왔디 ㅜㅜㅜㅜㅜ",
          "original_form": "과자안왔디 ㅜㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 21:44"
        },
        {
          "id": "MDRW2100020181.1.6",
          "form": "ㅋㅋㅋㅋㅋ그럼 다음주까지ㅜㅜ",
          "original_form": "ㅋㅋㅋㅋㅋ그럼 다음주까지ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 21:45"
        },
        {
          "id": "MDRW2100020181.1.7",
          "form": "그니까 ㅜㅜ",
          "original_form": "그니까 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 21:45"
        },
        {
          "id": "MDRW2100020181.1.8",
          "form": "월요일에나오는거아닌가싶음",
          "original_form": "월요일에나오는거아닌가싶음",
          "speaker_id": "1",
          "time": "20210828 21:45"
        },
        {
          "id": "MDRW2100020181.1.9",
          "form": "근데 나지금 미쯔먹음ㅋㅋ",
          "original_form": "근데 나지금 미쯔먹음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:46"
        },
        {
          "id": "MDRW2100020181.1.10",
          "form": "미쯔",
          "original_form": "미쯔",
          "speaker_id": "2",
          "time": "20210828 21:47"
        },
        {
          "id": "MDRW2100020181.1.11",
          "form": "샀어?",
          "original_form": "샀어?",
          "speaker_id": "2",
          "time": "20210828 21:47"
        },
        {
          "id": "MDRW2100020181.1.12",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:47"
        },
        {
          "id": "MDRW2100020181.1.13",
          "form": "어 고모네집가는 마트에 있더라 우리학원밑에는 없어서 화났는데",
          "original_form": "어 고모네집가는 마트에 있더라 우리학원밑에는 없어서 화났는데",
          "speaker_id": "1",
          "time": "20210828 21:47"
        },
        {
          "id": "MDRW2100020181.1.14",
          "form": "ㅋㅋ세계로마트에도 있는데",
          "original_form": "ㅋㅋ세계로마트에도 있는데",
          "speaker_id": "2",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100020181.1.15",
          "form": "오랜만에 먹고싶넹",
          "original_form": "오랜만에 먹고싶넹",
          "speaker_id": "2",
          "time": "20210828 21:49"
        },
        {
          "id": "MDRW2100020181.1.16",
          "form": "미쯔 상자에들어있는거아냐",
          "original_form": "미쯔 상자에들어있는거아냐",
          "speaker_id": "1",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100020181.1.17",
          "form": "ㅇㅇ맞움",
          "original_form": "ㅇㅇ맞움",
          "speaker_id": "2",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100020181.1.18",
          "form": "내기억속 미쯔는 봉지인데...",
          "original_form": "내기억속 미쯔는 봉지인데...",
          "speaker_id": "1",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100020181.1.19",
          "form": "칸초아직 에프기 안해봄ㅋㅋ",
          "original_form": "칸초아직 에프기 안해봄ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100020181.1.20",
          "form": "옛날사람...",
          "original_form": "옛날사람...",
          "speaker_id": "1",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100020181.1.21",
          "form": "왜안해봤어! ㅋㅋㅋ",
          "original_form": "왜안해봤어! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100020181.1.22",
          "form": "맞아....옛날엔 그랬지ㅜㅜ",
          "original_form": "맞아....옛날엔 그랬지ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100020181.1.23",
          "form": "나귀찮아서 그냥 까먹음ㅋㅋ",
          "original_form": "나귀찮아서 그냥 까먹음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:50"
        },
        {
          "id": "MDRW2100020181.1.24",
          "form": "ㅋㅋㅋㅋ뭐여 ㅋㅋㅋ 눅눅하다며",
          "original_form": "ㅋㅋㅋㅋ뭐여 ㅋㅋㅋ 눅눅하다며",
          "speaker_id": "1",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100020181.1.25",
          "form": "난과일맛종류가좋던데",
          "original_form": "난과일맛종류가좋던데",
          "speaker_id": "1",
          "time": "20210831 22:38"
        }
      ]
    }
