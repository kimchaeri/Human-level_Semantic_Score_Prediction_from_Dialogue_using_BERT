      "utterance": [
        {
          "id": "MDRW2100005704.1.1",
          "form": "여행 얘기 너무 재밌네요...",
          "original_form": "여행 얘기 너무 재밌네요...",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100005704.1.2",
          "form": "그쳐 코로나끝나면 가고싶은 여행지 있으신가요",
          "original_form": "그쳐 코로나끝나면 가고싶은 여행지 있으신가요",
          "speaker_id": "2",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100005704.1.3",
          "form": "몽골도 가고 싶어요",
          "original_form": "몽골도 가고 싶어요",
          "speaker_id": "1",
          "time": "20210822 15:24"
        },
        {
          "id": "MDRW2100005704.1.4",
          "form": "어머니랑 몽골 같이 가자 한 게",
          "original_form": "어머니랑 몽골 같이 가자 한 게",
          "speaker_id": "1",
          "time": "20210822 15:24"
        },
        {
          "id": "MDRW2100005704.1.5",
          "form": "몇년 됐는데",
          "original_form": "몇년 됐는데",
          "speaker_id": "1",
          "time": "20210822 15:24"
        },
        {
          "id": "MDRW2100005704.1.6",
          "form": "아직도 못 갔네요...",
          "original_form": "아직도 못 갔네요...",
          "speaker_id": "1",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100005704.1.7",
          "form": "헉 저두 몽골 넘가보고싶어요",
          "original_form": "헉 저두 몽골 넘가보고싶어요",
          "speaker_id": "2",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100005704.1.8",
          "form": "밤하늘별쏟아지는것도보고",
          "original_form": "밤하늘별쏟아지는것도보고",
          "speaker_id": "2",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100005704.1.9",
          "form": "친구들이갔다왔는데 넘좋다고하더라구여",
          "original_form": "친구들이갔다왔는데 넘좋다고하더라구여",
          "speaker_id": "2",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100005704.1.10",
          "form": "그러니가요ㅜ 게르 체험도 해보고 싶고",
          "original_form": "그러니가요ㅜ 게르 체험도 해보고 싶고",
          "speaker_id": "1",
          "time": "20210822 16:28"
        },
        {
          "id": "MDRW2100005704.1.11",
          "form": "네 밤에 모닥불피고 바베큐파티도하고싶고ㅜㅜ",
          "original_form": "네 밤에 모닥불피고 바베큐파티도하고싶고ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100005704.1.12",
          "form": "화장실이 불편하다고는 들었지만 ㅋㅋㅋㅋ가보고싶어요ㅜㅜ",
          "original_form": "화장실이 불편하다고는 들었지만 ㅋㅋㅋㅋ가보고싶어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 17:25"
        },
        {
          "id": "MDRW2100005704.1.13",
          "form": "맞아요ㅋㅋㅋㅋ ㅜ 불편해도 그런 것도 다 경험이라 생각해요",
          "original_form": "맞아요ㅋㅋㅋㅋ ㅜ 불편해도 그런 것도 다 경험이라 생각해요",
          "speaker_id": "1",
          "time": "20210822 18:49"
        },
        {
          "id": "MDRW2100005704.1.14",
          "form": "네ㅋㅋㅋㅋ지나고나면 그런것도 넘 추억이될듯여",
          "original_form": "네ㅋㅋㅋㅋ지나고나면 그런것도 넘 추억이될듯여",
          "speaker_id": "2",
          "time": "20210822 18:49"
        },
        {
          "id": "MDRW2100005704.1.15",
          "form": "중국은 어디다녀오셨어요",
          "original_form": "중국은 어디다녀오셨어요",
          "speaker_id": "2",
          "time": "20210822 18:49"
        },
        {
          "id": "MDRW2100005704.1.16",
          "form": "저 북경 다녀왓어요",
          "original_form": "저 북경 다녀왓어요",
          "speaker_id": "1",
          "time": "20210822 18:50"
        },
        {
          "id": "MDRW2100005704.1.17",
          "form": "첫 해외여행이었어요ㅋㅋㅋ",
          "original_form": "첫 해외여행이었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:50"
        },
        {
          "id": "MDRW2100005704.1.18",
          "form": "오 중국은 다크다고들어서 넘궁금해요 대만이랑또다를것같아서",
          "original_form": "오 중국은 다크다고들어서 넘궁금해요 대만이랑또다를것같아서",
          "speaker_id": "2",
          "time": "20210822 18:51"
        },
        {
          "id": "MDRW2100005704.1.19",
          "form": "음식은어떠셨어요",
          "original_form": "음식은어떠셨어요",
          "speaker_id": "2",
          "time": "20210822 18:52"
        },
        {
          "id": "MDRW2100005704.1.20",
          "form": "ㅋㅋㅋㅋ최악이었어요ㅜ",
          "original_form": "ㅋㅋㅋㅋ최악이었어요ㅜ",
          "speaker_id": "1",
          "time": "20210822 18:52"
        },
        {
          "id": "MDRW2100005704.1.21",
          "form": "향신료가 저랑 너무 안 맞아서",
          "original_form": "향신료가 저랑 너무 안 맞아서",
          "speaker_id": "1",
          "time": "20210822 18:52"
        },
        {
          "id": "MDRW2100005704.1.22",
          "form": "ㅋㅋㅋㅋㅋ왠지..그럴것같았어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ왠지..그럴것같았어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:52"
        },
        {
          "id": "MDRW2100005704.1.23",
          "form": "저 취두부도전했다가 호되게당했덤",
          "original_form": "저 취두부도전했다가 호되게당했덤",
          "speaker_id": "2",
          "time": "20210822 18:52"
        },
        {
          "id": "MDRW2100005704.1.24",
          "form": "기억이나네요",
          "original_form": "기억이나네요",
          "speaker_id": "2",
          "time": "20210822 18:53"
        },
        {
