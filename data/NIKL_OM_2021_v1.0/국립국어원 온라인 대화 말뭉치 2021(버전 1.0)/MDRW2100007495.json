      "utterance": [
        {
          "id": "MDRW2100007495.1.1",
          "form": "ㅎㅎ쇼핑 좋아하시나요",
          "original_form": "ㅎㅎ쇼핑 좋아하시나요",
          "speaker_id": "2",
          "time": "20210822 02:36"
        },
        {
          "id": "MDRW2100007495.1.2",
          "form": "아뇨근데 최근에 우울하고짜증나서",
          "original_form": "아뇨근데 최근에 우울하고짜증나서",
          "speaker_id": "1",
          "time": "20210822 02:37"
        },
        {
          "id": "MDRW2100007495.1.3",
          "form": "완전 과소비했어요",
          "original_form": "완전 과소비했어요",
          "speaker_id": "1",
          "time": "20210822 02:37"
        },
        {
          "id": "MDRW2100007495.1.4",
          "form": "후회할짓은아니라고생각하지만",
          "original_form": "후회할짓은아니라고생각하지만",
          "speaker_id": "1",
          "time": "20210822 02:38"
        },
        {
          "id": "MDRW2100007495.1.5",
          "form": "뭐 한번은",
          "original_form": "뭐 한번은",
          "speaker_id": "2",
          "time": "20210822 02:38"
        },
        {
          "id": "MDRW2100007495.1.6",
          "form": "플렉스해도 되죠",
          "original_form": "플렉스해도 되죠",
          "speaker_id": "2",
          "time": "20210822 02:38"
        },
        {
          "id": "MDRW2100007495.1.7",
          "form": "맞아요 인생에한번이라생각하고한거죠",
          "original_form": "맞아요 인생에한번이라생각하고한거죠",
          "speaker_id": "1",
          "time": "20210822 02:39"
        },
        {
          "id": "MDRW2100007495.1.8",
          "form": "버는돈 모으기만해서 뭐하지 내가이렇게슬픈데",
          "original_form": "버는돈 모으기만해서 뭐하지 내가이렇게슬픈데",
          "speaker_id": "1",
          "time": "20210822 02:40"
        },
        {
          "id": "MDRW2100007495.1.9",
          "form": "근데 또 쓰고나서 잔고 줄어든거보니까 그것도 불안요소가 되네요 이부분이 참",
          "original_form": "근데 또 쓰고나서 잔고 줄어든거보니까 그것도 불안요소가 되네요 이부분이 참",
          "speaker_id": "1",
          "time": "20210822 02:40"
        },
        {
          "id": "MDRW2100007495.1.10",
          "form": "한번 팍 쓰시면 다시 채우면 되죠ㅎㅎ",
          "original_form": "한번 팍 쓰시면 다시 채우면 되죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 02:42"
        },
        {
          "id": "MDRW2100007495.1.11",
          "form": "그렇죠 ㅎㅎ",
          "original_form": "그렇죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 02:43"
        },
        {
          "id": "MDRW2100007495.1.12",
          "form": "이번달 지출 팍 줄이세요ㅎㅎ",
          "original_form": "이번달 지출 팍 줄이세요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 02:46"
        },
        {
          "id": "MDRW2100007495.1.13",
          "form": "ㅎㅎ줄여야죠!",
          "original_form": "ㅎㅎ줄여야죠!",
          "speaker_id": "1",
          "time": "20210822 02:47"
        },
        {
          "id": "MDRW2100007495.1.14",
          "form": "가능할거에오",
          "original_form": "가능할거에오",
          "speaker_id": "2",
          "time": "20210822 02:48"
        },
        {
          "id": "MDRW2100007495.1.15",
          "form": "최근에 쇼핑하신거있으신가요",
          "original_form": "최근에 쇼핑하신거있으신가요",
          "speaker_id": "1",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007495.1.16",
          "form": "저는 음 패딩 샀어요",
          "original_form": "저는 음 패딩 샀어요",
          "speaker_id": "2",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007495.1.17",
          "form": "역시즌",
          "original_form": "역시즌",
          "speaker_id": "2",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007495.1.18",
          "form": "미리 겨울대비용?",
          "original_form": "미리 겨울대비용?",
          "speaker_id": "1",
          "time": "20210822 02:53"
        },
        {
          "id": "MDRW2100007495.1.19",
          "form": "네 지금 사면 싸더라고요",
          "original_form": "네 지금 사면 싸더라고요",
          "speaker_id": "2",
          "time": "20210822 02:54"
        }
      ]
