      "utterance": [
        {
          "id": "MDRW2100006630.1.1",
          "form": "암요암요",
          "original_form": "암요암요",
          "speaker_id": "1",
          "time": "20210908 04:00"
        },
        {
          "id": "MDRW2100006630.1.2",
          "form": "그럼요 언제나 옳죠",
          "original_form": "그럼요 언제나 옳죠",
          "speaker_id": "2",
          "time": "20210908 04:00"
        },
        {
          "id": "MDRW2100006630.1.3",
          "form": "하 이 밤에 정말 구할 수도 없고",
          "original_form": "하 이 밤에 정말 구할 수도 없고",
          "speaker_id": "2",
          "time": "20210908 04:00"
        },
        {
          "id": "MDRW2100006630.1.4",
          "form": "점심까지만 참아야겠어요",
          "original_form": "점심까지만 참아야겠어요",
          "speaker_id": "2",
          "time": "20210908 04:01"
        },
        {
          "id": "MDRW2100006630.1.5",
          "form": "ㅋㅋㅋㅋㅋ 저도 슬슬 배가 고파지네요....",
          "original_form": "ㅋㅋㅋㅋㅋ 저도 슬슬 배가 고파지네요....",
          "speaker_id": "1",
          "time": "20210908 04:01"
        },
        {
          "id": "MDRW2100006630.1.6",
          "form": "코로나 전에는 24시간 영업하는 곳이 꽤 됐는데",
          "original_form": "코로나 전에는 24시간 영업하는 곳이 꽤 됐는데",
          "speaker_id": "1",
          "time": "20210908 04:01"
        },
        {
          "id": "MDRW2100006630.1.7",
          "form": "이젠 새벽엔 진짜 편의점 밖에 없어요ㅜ",
          "original_form": "이젠 새벽엔 진짜 편의점 밖에 없어요ㅜ",
          "speaker_id": "1",
          "time": "20210908 04:01"
        },
        {
          "id": "MDRW2100006630.1.8",
          "form": "그러니까요 슬픈 일입니다ㅠ",
          "original_form": "그러니까요 슬픈 일입니다ㅠ",
          "speaker_id": "2",
          "time": "20210908 04:02"
        },
        {
          "id": "MDRW2100006630.1.9",
          "form": "사실 오늘 돈카츠 먹으려고 정해놨었는데",
          "original_form": "사실 오늘 돈카츠 먹으려고 정해놨었는데",
          "speaker_id": "2",
          "time": "20210908 04:02"
        },
        {
          "id": "MDRW2100006630.1.10",
          "form": "급하게 피자로 틀었어요ㅋㅋㅋㅋ",
          "original_form": "급하게 피자로 틀었어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:02"
        },
        {
          "id": "MDRW2100006630.1.11",
          "form": "앜ㅋㅋㅋㅋ 이게 실은 피자 바이럴이었다면....?!",
          "original_form": "앜ㅋㅋㅋㅋ 이게 실은 피자 바이럴이었다면....?!",
          "speaker_id": "1",
          "time": "20210908 04:02"
        },
        {
          "id": "MDRW2100006630.1.12",
          "form": "일식 돈카츠 vs 경양식 왕돈까스",
          "original_form": "일식 돈카츠 vs 경양식 왕돈까스",
          "speaker_id": "1",
          "time": "20210908 04:03"
        },
        {
          "id": "MDRW2100006630.1.13",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ이렇게 또 한명 낚입니다..",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ이렇게 또 한명 낚입니다..",
          "speaker_id": "2",
          "time": "20210908 04:03"
        },
        {
          "id": "MDRW2100006630.1.14",
          "form": "전 일식",
          "original_form": "전 일식",
          "speaker_id": "2",
          "time": "20210908 04:03"
        },
        {
          "id": "MDRW2100006630.1.15",
          "form": "자주 가는 동네에 인생 돈카츠집이 있거든요",
          "original_form": "자주 가는 동네에 인생 돈카츠집이 있거든요",
          "speaker_id": "2",
          "time": "20210908 04:03"
        },
        {
          "id": "MDRW2100006630.1.16",
          "form": "정말 미칩니다",
          "original_form": "정말 미칩니다",
          "speaker_id": "2",
          "time": "20210908 04:03"
        },
        {
          "id": "MDRW2100006630.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 04:03"
        },
        {
          "id": "MDRW2100006630.1.18",
          "form": "오오오오 얼만큼이길래 미칠 정도인거죠?!",
          "original_form": "오오오오 얼만큼이길래 미칠 정도인거죠?!",
          "speaker_id": "1",
          "time": "20210908 04:04"
        },
        {
          "id": "MDRW2100006630.1.19",
          "form": "인생 카츠 입니다 진짜",
          "original_form": "인생 카츠 입니다 진짜",
          "speaker_id": "2",
          "time": "20210908 04:04"
        },
        {
          "id": "MDRW2100006630.1.20",
          "form": "사람 없었는데 도대체 누가 소문을 내고 다니는건지",
          "original_form": "사람 없었는데 도대체 누가 소문을 내고 다니는건지",
          "speaker_id": "2",
          "time": "20210908 04:05"
        },
        {
          "id": "MDRW2100006630.1.21",
          "form": "요즘은 시간 잘 못 맞추면 한시간씩 기다려야하더라고요",
          "original_form": "요즘은 시간 잘 못 맞추면 한시간씩 기다려야하더라고요",
          "speaker_id": "2",
          "time": "20210908 04:05"
        },
        {
          "id": "MDRW2100006630.1.22",
          "form": "와 찐 라이징 맛집인가봐요",
          "original_form": "와 찐 라이징 맛집인가봐요",
          "speaker_id": "1",
          "time": "20210908 04:05"
        },
        {
          "id": "MDRW2100006630.1.23",
          "form": "웨이팅까지 있다니",
          "original_form": "웨이팅까지 있다니",
          "speaker_id": "1",
          "time": "20210908 04:05"
        },
        {
          "id": "MDRW2100006630.1.24",
          "form": "찐 라이징 맛집",
          "original_form": "찐 라이징 맛집",
          "speaker_id": "2",
          "time": "20210908 04:06"
        },
        {
          "id": "MDRW2100006630.1.25",
          "form": "그...저... 선생님 정보 귀띔 좀....",
          "original_form": "그...저... 선생님 정보 귀띔 좀....",
          "speaker_id": "1",
          "time": "20210908 04:06"
        },
        {
          "id": "MDRW2100006630.1.26",
          "form": "진짜 손님 없을 때 편하게 먹을 때가 그립습니다",
          "original_form": "진짜 손님 없을 때 편하게 먹을 때가 그립습니다",
          "speaker_id": "2",
          "time": "20210908 04:06"
        },
        {
          "id": "MDRW2100006630.1.27",
          "form": "어떻게 안되겠슴까...?",
          "original_form": "어떻게 안되겠슴까...?",
          "speaker_id": "1",
          "time": "20210908 04:06"
        },
        {
          "id": "MDRW2100006630.1.28",
          "form": "아 서울 신대방 쪽에 카츠오도라고 있습니다",
          "original_form": "아 서울 신대방 쪽에 카츠오도라고 있습니다",
          "speaker_id": "2",
          "time": "20210908 04:07"
        },
        {
          "id": "MDRW2100006630.1.29",
          "form": "진짜 찐이에요 거기 언젠가 반드시...",
          "original_form": "진짜 찐이에요 거기 언젠가 반드시...",
          "speaker_id": "2",
          "time": "20210908 04:07"
        },
        {
          "id": "MDRW2100006630.1.30",
          "form": "네이버지도 소개글부터가 최근 뜨고있는 핫한 맛집이네요ㅋㅋㅋㅋ",
          "original_form": "네이버지도 소개글부터가 최근 뜨고있는 핫한 맛집이네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 04:08"
        },
        {
          "id": "MDRW2100006630.1.31",
          "form": "저장했습니다 감사합니다 두근두근",
          "original_form": "저장했습니다 감사합니다 두근두근",
          "speaker_id": "1",
          "time": "20210908 04:08"
        },
        {
          "id": "MDRW2100006630.1.32",
          "form": "언젠가 꼭 가보세요. 미칩니다 정말",
          "original_form": "언젠가 꼭 가보세요. 미칩니다 정말",
          "speaker_id": "2",
          "time": "20210908 04:10"
        }
      ]
