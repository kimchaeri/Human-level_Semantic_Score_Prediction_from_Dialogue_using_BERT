      "utterance": [
        {
          "id": "MDRW2100020179.1.1",
          "form": "아아아!!!",
          "original_form": "아아아!!!",
          "speaker_id": "2",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020179.1.2",
          "form": "그러면이제",
          "original_form": "그러면이제",
          "speaker_id": "2",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020179.1.3",
          "form": "거기는 길냥이들 천굳,,,",
          "original_form": "거기는 길냥이들 천굳,,,",
          "speaker_id": "2",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020179.1.4",
          "form": "여러마리 있는데 다들 순하고 애교가 많아서 신기했어요ㅋㅋ",
          "original_form": "여러마리 있는데 다들 순하고 애교가 많아서 신기했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:18"
        },
        {
          "id": "MDRW2100020179.1.5",
          "form": "ㅋㅋㅋㅋㅋ고양이진짜좋아하시는군요",
          "original_form": "ㅋㅋㅋㅋㅋ고양이진짜좋아하시는군요",
          "speaker_id": "2",
          "time": "20210828 00:18"
        },
        {
          "id": "MDRW2100020179.1.6",
          "form": "사실 동물 무서워해요ㅋㅋㅋㅋ",
          "original_form": "사실 동물 무서워해요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100020179.1.7",
          "form": "그래서 잘 다가가진 못하는데 친구네 동물들 덕분에 그나마 괜찮아졌어요ㅋㅋㅋ",
          "original_form": "그래서 잘 다가가진 못하는데 친구네 동물들 덕분에 그나마 괜찮아졌어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100020179.1.8",
          "form": "아하!!!",
          "original_form": "아하!!!",
          "speaker_id": "2",
          "time": "20210828 00:25"
        },
        {
          "id": "MDRW2100020179.1.9",
          "form": "저는 큰동물 싫어해요,,,",
          "original_form": "저는 큰동물 싫어해요,,,",
          "speaker_id": "2",
          "time": "20210828 00:25"
        },
        {
          "id": "MDRW2100020179.1.10",
          "form": "이유는없지만",
          "original_form": "이유는없지만",
          "speaker_id": "2",
          "time": "20210828 00:25"
        },
        {
          "id": "MDRW2100020179.1.11",
          "form": "사자 코끼리 이런 동물요??",
          "original_form": "사자 코끼리 이런 동물요??",
          "speaker_id": "1",
          "time": "20210828 10:21"
        },
        {
          "id": "MDRW2100020179.1.12",
          "form": "네네!!!!",
          "original_form": "네네!!!!",
          "speaker_id": "2",
          "time": "20210828 11:16"
        },
        {
          "id": "MDRW2100020179.1.13",
          "form": "근데 얘네들도 덩치에 안맞게 애교도 있더라구요ㅋㅋㅋ",
          "original_form": "근데 얘네들도 덩치에 안맞게 애교도 있더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:57"
        },
        {
          "id": "MDRW2100020179.1.14",
          "form": "우아아아아앙 애교쟁이들",
          "original_form": "우아아아아앙 애교쟁이들",
          "speaker_id": "2",
          "time": "20210828 22:58"
        },
        {
          "id": "MDRW2100020179.1.15",
          "form": "너무 의외라서 더 귀여워요ㅋㅋㅋ",
          "original_form": "너무 의외라서 더 귀여워요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:03"
        },
        {
          "id": "MDRW2100020179.1.16",
          "form": "아잌ㅋㅋㅋㅋㅋ그죠그죠",
          "original_form": "아잌ㅋㅋㅋㅋㅋ그죠그죠",
          "speaker_id": "2",
          "time": "20210829 00:19"
        },
        {
          "id": "MDRW2100020179.1.17",
          "form": "동물 키워보신적은 있으세여??",
          "original_form": "동물 키워보신적은 있으세여??",
          "speaker_id": "1",
          "time": "20210829 14:19"
        },
        {
          "id": "MDRW2100020179.1.18",
          "form": "음..저는 없어요!!!",
          "original_form": "음..저는 없어요!!!",
          "speaker_id": "2",
          "time": "20210829 14:24"
        },
        {
          "id": "MDRW2100020179.1.19",
          "form": "아 햄스터 같은 소동물도요?!",
          "original_form": "아 햄스터 같은 소동물도요?!",
          "speaker_id": "1",
          "time": "20210829 14:27"
        },
        {
