      "utterance": [
        {
          "id": "MDRW2100040058.1.1",
          "form": "일단",
          "original_form": "일단",
          "speaker_id": "2",
          "time": "20210904 02:26"
        },
        {
          "id": "MDRW2100040058.1.2",
          "form": "맘에 안드는게",
          "original_form": "맘에 안드는게",
          "speaker_id": "2",
          "time": "20210904 02:27"
        },
        {
          "id": "MDRW2100040058.1.3",
          "form": "왜 내가 야에 뜻에 따라",
          "original_form": "왜 내가 야에 뜻에 따라",
          "speaker_id": "2",
          "time": "20210904 02:27"
        },
        {
          "id": "MDRW2100040058.1.4",
          "form": "차분해졋군",
          "original_form": "차분해졋군",
          "speaker_id": "1",
          "time": "20210904 02:27"
        },
        {
          "id": "MDRW2100040058.1.5",
          "form": "움직여야됨?",
          "original_form": "움직여야됨?",
          "speaker_id": "2",
          "time": "20210904 02:27"
        },
        {
          "id": "MDRW2100040058.1.6",
          "form": "모락스때는 걍 상황이 안좋아서",
          "original_form": "모락스때는 걍 상황이 안좋아서",
          "speaker_id": "2",
          "time": "20210904 02:27"
        },
        {
          "id": "MDRW2100040058.1.7",
          "form": "그렇게 된거지",
          "original_form": "그렇게 된거지",
          "speaker_id": "2",
          "time": "20210904 02:27"
        },
        {
          "id": "MDRW2100040058.1.8",
          "form": "분명 안수령에 대항하겠다고 결심한것도 사안공장 때려 부스겠다고 결심한것도",
          "original_form": "분명 안수령에 대항하겠다고 결심한것도 사안공장 때려 부스겠다고 결심한것도",
          "speaker_id": "2",
          "time": "20210904 02:28"
        },
        {
          "id": "MDRW2100040058.1.9",
          "form": "주인공인데",
          "original_form": "주인공인데",
          "speaker_id": "2",
          "time": "20210904 02:28"
        },
        {
          "id": "MDRW2100040058.1.10",
          "form": "왜 갑자기 야에 뜻에 따라서",
          "original_form": "왜 갑자기 야에 뜻에 따라서",
          "speaker_id": "2",
          "time": "20210904 02:28"
        },
        {
          "id": "MDRW2100040058.1.11",
          "form": "ㅇㅇ 그치",
          "original_form": "ㅇㅇ 그치",
          "speaker_id": "1",
          "time": "20210904 02:28"
        },
        {
          "id": "MDRW2100040058.1.12",
          "form": "이리갔다 저리갔다 하는거안",
          "original_form": "이리갔다 저리갔다 하는거안",
          "speaker_id": "2",
          "time": "20210904 02:28"
        },
        {
          "id": "MDRW2100040058.1.13",
          "form": "야",
          "original_form": "야",
          "speaker_id": "2",
          "time": "20210904 02:28"
        },
        {
          "id": "MDRW2100040058.1.14",
          "form": "심지어 야에랑 어떤 유대관계가 있는 것도 아닌데",
          "original_form": "심지어 야에랑 어떤 유대관계가 있는 것도 아닌데",
          "speaker_id": "1",
          "time": "20210904 02:28"
        },
        {
          "id": "MDRW2100040058.1.15",
          "form": "내가 대체 왜...?",
          "original_form": "내가 대체 왜...?",
          "speaker_id": "1",
          "time": "20210904 02:28"
        },
        {
          "id": "MDRW2100040058.1.16",
          "form": "그리고 번역 상태 정말 맘에 안들어",
          "original_form": "그리고 번역 상태 정말 맘에 안들어",
          "speaker_id": "2",
          "time": "20210904 02:28"
        },
        {
          "id": "MDRW2100040058.1.17",
          "form": "아무리 와풍이라지만",
          "original_form": "아무리 와풍이라지만",
          "speaker_id": "2",
          "time": "20210904 02:28"
        },
        {
          "id": "MDRW2100040058.1.18",
          "form": "직업을 일본어 발음 그대로 써놓는건",
          "original_form": "직업을 일본어 발음 그대로 써놓는건",
          "speaker_id": "2",
          "time": "20210904 02:29"
        },
        {
          "id": "MDRW2100040058.1.19",
          "form": "왜그러는거임",
          "original_form": "왜그러는거임",
          "speaker_id": "2",
          "time": "20210904 02:29"
        },
        {
          "id": "MDRW2100040058.1.20",
          "form": "산고노미야 걍 산호궁주라고 번역하고",
          "original_form": "산고노미야 걍 산호궁주라고 번역하고",
          "speaker_id": "2",
          "time": "20210904 02:29"
        },
        {
          "id": "MDRW2100040058.1.21",
          "form": "야에 미코 야에 무녀라고 번역해",
          "original_form": "야에 미코 야에 무녀라고 번역해",
          "speaker_id": "2",
          "time": "20210904 02:29"
        },
        {
          "id": "MDRW2100040058.1.22",
          "form": "왜 갑자기 그걸 일본어 그대로 읽는거임",
          "original_form": "왜 갑자기 그걸 일본어 그대로 읽는거임",
          "speaker_id": "2",
          "time": "20210904 02:29"
        },
        {
          "id": "MDRW2100040058.1.23",
          "form": "아 미코가 무녀야?",
          "original_form": "아 미코가 무녀야?",
          "speaker_id": "1",
          "time": "20210904 02:30"
        },
        {
          "id": "MDRW2100040058.1.24",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210904 02:30"
        },
        {
          "id": "MDRW2100040058.1.25",
          "form": "난 이름인줄 ㅋㅋㅋㅋㅋ",
          "original_form": "난 이름인줄 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 02:30"
        },
        {
          "id": "MDRW2100040058.1.26",
          "form": "ㅇㅇ꼭",
          "original_form": "ㅇㅇ꼭",
          "speaker_id": "2",
          "time": "20210904 02:30"
        },
        {
          "id": "MDRW2100040058.1.27",
          "form": "이름인것처럼",
          "original_form": "이름인것처럼",
          "speaker_id": "2",
          "time": "20210904 02:30"
        },
        {
          "id": "MDRW2100040058.1.28",
          "form": "산고노미야 코코미 야에 미코",
          "original_form": "산고노미야 코코미 야에 미코",
          "speaker_id": "2",
          "time": "20210904 02:30"
        },
        {
          "id": "MDRW2100040058.1.29",
          "form": "이러고 써놓잖아",
          "original_form": "이러고 써놓잖아",
          "speaker_id": "2",
          "time": "20210904 02:30"
        },
        {
          "id": "MDRW2100040058.1.30",
          "form": "근데 이번 시즌이",
          "original_form": "근데 이번 시즌이",
          "speaker_id": "1",
          "time": "20210904 02:30"
        },
        {
          "id": "MDRW2100040058.1.31",
          "form": "번역 제일 별로임",
          "original_form": "번역 제일 별로임",
          "speaker_id": "1",
          "time": "20210904 02:30"
        },
        {
          "id": "MDRW2100040058.1.32",
          "form": "일본풍이라 정신을 못차리는건가?",
          "original_form": "일본풍이라 정신을 못차리는건가?",
          "speaker_id": "1",
          "time": "20210904 02:30"
        },
        {
          "id": "MDRW2100040058.1.33",
          "form": "ㄹㅇ번역",
          "original_form": "ㄹㅇ번역",
          "speaker_id": "2",
          "time": "20210904 02:30"
        },
        {
          "id": "MDRW2100040058.1.34",
          "form": "최악이야",
          "original_form": "최악이야",
          "speaker_id": "2",
          "time": "20210904 02:30"
        }
      ]
    }
