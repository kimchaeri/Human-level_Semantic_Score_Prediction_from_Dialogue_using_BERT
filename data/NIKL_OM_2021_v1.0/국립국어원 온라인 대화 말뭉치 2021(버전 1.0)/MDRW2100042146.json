      "utterance": [
        {
          "id": "MDRW2100042146.1.1",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210906 20:06"
        },
        {
          "id": "MDRW2100042146.1.2",
          "form": "네 먹었어요!",
          "original_form": "네 먹었어요!",
          "speaker_id": "2",
          "time": "20210906 20:07"
        },
        {
          "id": "MDRW2100042146.1.3",
          "form": "어떤 음식 드셨어요? 저는 김치찜 먹었어요ㅎㅎ",
          "original_form": "어떤 음식 드셨어요? 저는 김치찜 먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 20:09"
        },
        {
          "id": "MDRW2100042146.1.4",
          "form": "우왕 김치찜 맛있겠어요!",
          "original_form": "우왕 김치찜 맛있겠어요!",
          "speaker_id": "2",
          "time": "20210906 20:10"
        },
        {
          "id": "MDRW2100042146.1.5",
          "form": "전 마라탕 시켜먹었습니다!ㅎㅎ",
          "original_form": "전 마라탕 시켜먹었습니다!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 20:10"
        },
        {
          "id": "MDRW2100042146.1.6",
          "form": "헉 저 마라탕 진짜 좋아해요ㅠㅠ",
          "original_form": "헉 저 마라탕 진짜 좋아해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 20:11"
        },
        {
          "id": "MDRW2100042146.1.7",
          "form": "마라탕 진짜 맛있죠 ㅠㅠㅠ오랜만에 시켜먹았는데 진짜 맛있더라구요",
          "original_form": "마라탕 진짜 맛있죠 ㅠㅠㅠ오랜만에 시켜먹았는데 진짜 맛있더라구요",
          "speaker_id": "2",
          "time": "20210906 20:15"
        },
        {
          "id": "MDRW2100042146.1.8",
          "form": "저는 샹궈도 사랑합니다,,",
          "original_form": "저는 샹궈도 사랑합니다,,",
          "speaker_id": "2",
          "time": "20210906 20:15"
        },
        {
          "id": "MDRW2100042146.1.9",
          "form": "오랜만에 먹으면 더 맛있어요ㅠㅠ",
          "original_form": "오랜만에 먹으면 더 맛있어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 20:16"
        },
        {
          "id": "MDRW2100042146.1.10",
          "form": "와 저 샹궈는 안 먹어봤어요!",
          "original_form": "와 저 샹궈는 안 먹어봤어요!",
          "speaker_id": "1",
          "time": "20210906 20:17"
        },
        {
          "id": "MDRW2100042146.1.11",
          "form": "많이 다른가요?",
          "original_form": "많이 다른가요?",
          "speaker_id": "1",
          "time": "20210906 20:17"
        },
        {
          "id": "MDRW2100042146.1.12",
          "form": "그렇게 많이 다르진 않아여!!",
          "original_form": "그렇게 많이 다르진 않아여!!",
          "speaker_id": "2",
          "time": "20210906 20:17"
        },
        {
          "id": "MDRW2100042146.1.13",
          "form": "그렇게 많이 다르지 않아요!",
          "original_form": "그렇게 많이 다르지 않아요!",
          "speaker_id": "2",
          "time": "20210906 20:17"
        },
        {
          "id": "MDRW2100042146.1.14",
          "form": "마라소스에 마라탕 재료들 넣고 뽂은 느낌이라 하면 될까요..여튼 맛나눀ㅋㅋ",
          "original_form": "마라소스에 마라탕 재료들 넣고 뽂은 느낌이라 하면 될까요..여튼 맛나눀ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:18"
        },
        {
          "id": "MDRW2100042146.1.15",
          "form": "담에 먹어봐야겠어요ㅋㅋㅋㅋ",
          "original_form": "담에 먹어봐야겠어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100042146.1.16",
          "form": "샹궈가 약간 그나마 몸에 덜 나쁠 것 같은 느낌이에요",
          "original_form": "샹궈가 약간 그나마 몸에 덜 나쁠 것 같은 느낌이에요",
          "speaker_id": "1",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100042146.1.17",
          "form": "맞아요 마라탕은 왠지 그 국물때문에!",
          "original_form": "맞아요 마라탕은 왠지 그 국물때문에!",
          "speaker_id": "2",
          "time": "20210906 20:21"
        },
        {
          "id": "MDRW2100042146.1.18",
          "form": "샹궈랑 밥이랑 먹으면...참 맛있습니당",
          "original_form": "샹궈랑 밥이랑 먹으면...참 맛있습니당",
          "speaker_id": "2",
          "time": "20210906 20:21"
        },
        {
          "id": "MDRW2100042146.1.19",
          "form": "마라탕은 보통 몇단계로 드셔요??",
          "original_form": "마라탕은 보통 몇단계로 드셔요??",
          "speaker_id": "2",
          "time": "20210906 20:21"
        },
        {
          "id": "MDRW2100042146.1.20",
          "form": "저는 진짜 매운 집은 중간단계 정도요!",
          "original_form": "저는 진짜 매운 집은 중간단계 정도요!",
          "speaker_id": "1",
          "time": "20210906 20:22"
        },
        {
          "id": "MDRW2100042146.1.21",
          "form": "다음날을 위해서...",
          "original_form": "다음날을 위해서...",
          "speaker_id": "1",
          "time": "20210906 20:22"
        },
        {
          "id": "MDRW2100042146.1.22",
          "form": "헉 제기준 매운거 잘 드시는 분이세여!ㅋㅋㅋㅋ 전 1단계가 최대임니다..",
          "original_form": "헉 제기준 매운거 잘 드시는 분이세여!ㅋㅋㅋㅋ 전 1단계가 최대임니다..",
          "speaker_id": "2",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100042146.1.23",
          "form": "전에 0단계 시키고 백탕나와서 그뒤론 아무리 못먹어도 1단계시켜욬ㅋㅋ",
          "original_form": "전에 0단계 시키고 백탕나와서 그뒤론 아무리 못먹어도 1단계시켜욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:25"
        },
        {
          "id": "MDRW2100042146.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ저도 친구가 그런 적 있어서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ저도 친구가 그런 적 있어서",
          "speaker_id": "1",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042146.1.25",
          "form": "백탕 본 적 있어요ㅋㅋㅋㅋ",
          "original_form": "백탕 본 적 있어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042146.1.26",
          "form": "너무 웃겨서 다들 사진 찍고 그랬어요",
          "original_form": "너무 웃겨서 다들 사진 찍고 그랬어요",
          "speaker_id": "1",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042146.1.27",
          "form": "진짜 .. 아무리 맵찔이를 무시해도 그렇지..!",
          "original_form": "진짜 .. 아무리 맵찔이를 무시해도 그렇지..!",
          "speaker_id": "2",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100042146.1.28",
          "form": "그게 마라맛이 조금이라도 나야죠 ㅠㅠ 사골탕도아니고!ㅋㅋㅋㅋㅋ",
          "original_form": "그게 마라맛이 조금이라도 나야죠 ㅠㅠ 사골탕도아니고!ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042146.1.29",
          "form": "저도 받고 그뒤부턴 0단계 절대 안시킴니다..",
          "original_form": "저도 받고 그뒤부턴 0단계 절대 안시킴니다..",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042146.1.30",
          "form": "그쵸ㅋㅋㅋ저희도 진짜 예상하지 못한 비주얼이어서",
          "original_form": "그쵸ㅋㅋㅋ저희도 진짜 예상하지 못한 비주얼이어서",
          "speaker_id": "1",
          "time": "20210907 18:11"
        },
        {
          "id": "MDRW2100042146.1.31",
