      "utterance": [
        {
          "id": "MDRW2100032557.1.1",
          "form": "맞추기 힘든어",
          "original_form": "맞추기 힘든어",
          "speaker_id": "2",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100032557.1.2",
          "form": "아니 일주일 내내 일하는거 힘든건 알겠는데",
          "original_form": "아니 일주일 내내 일하는거 힘든건 알겠는데",
          "speaker_id": "2",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100032557.1.3",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100032557.1.4",
          "form": "아침에는 엄청 기분 좋다가",
          "original_form": "아침에는 엄청 기분 좋다가",
          "speaker_id": "2",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100032557.1.5",
          "form": "오후에는 기분 안좋고",
          "original_form": "오후에는 기분 안좋고",
          "speaker_id": "2",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100032557.1.6",
          "form": "힘든티가 너무 나서",
          "original_form": "힘든티가 너무 나서",
          "speaker_id": "2",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100032557.1.7",
          "form": "계속 눈치보고",
          "original_form": "계속 눈치보고",
          "speaker_id": "2",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100032557.1.8",
          "form": "그러는게 불편함",
          "original_form": "그러는게 불편함",
          "speaker_id": "2",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100032557.1.9",
          "form": "아 눈치보게되는 스타일?",
          "original_form": "아 눈치보게되는 스타일?",
          "speaker_id": "1",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100032557.1.10",
          "form": "응응..",
          "original_form": "응응..",
          "speaker_id": "2",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100032557.1.11",
          "form": "솔직히 우리 1학년 초반에도",
          "original_form": "솔직히 우리 1학년 초반에도",
