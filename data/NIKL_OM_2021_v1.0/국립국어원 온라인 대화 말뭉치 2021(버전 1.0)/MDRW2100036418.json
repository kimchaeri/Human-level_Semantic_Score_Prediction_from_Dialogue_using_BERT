      "utterance": [
        {
          "id": "MDRW2100036418.1.1",
          "form": "아 청춘불패 재밌었는데",
          "original_form": "아 청춘불패 재밌었는데",
          "speaker_id": "2",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100036418.1.2",
          "form": "영웅호갈이랑 두 개 재밌게 봣어요!",
          "original_form": "영웅호갈이랑 두 개 재밌게 봣어요!",
          "speaker_id": "2",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100036418.1.3",
          "form": "영웅호걸도 추억이네요!",
          "original_form": "영웅호걸도 추억이네요!",
          "speaker_id": "1",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100036418.1.4",
          "form": "아이유랑 지연ㄴㅏ오고 ㅋㅋ 했죠?",
          "original_form": "아이유랑 지연ㄴㅏ오고 ㅋㅋ 했죠?",
          "speaker_id": "1",
          "time": "20210830 22:47"
        },
        {
          "id": "MDRW2100036418.1.5",
          "form": "맞아요!! 지금 보면 완전 대박 캐스팅이었던!!!@",
          "original_form": "맞아요!! 지금 보면 완전 대박 캐스팅이었던!!!@",
          "speaker_id": "2",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100036418.1.6",
          "form": "유인나도 나왔을 걸요!",
          "original_form": "유인나도 나왔을 걸요!",
          "speaker_id": "2",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100036418.1.7",
          "form": "아 ! 맞네요",
          "original_form": "아 ! 맞네요",
          "speaker_id": "1",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100036418.1.8",
          "form": "완전 아이유 친구들 ㅋㅋ",
          "original_form": "완전 아이유 친구들 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:49"
        },
        {
          "id": "MDRW2100036418.1.9",
          "form": "맞아요!! 그래서 아이유 라디오에서",
          "original_form": "맞아요!! 그래서 아이유 라디오에서",
          "speaker_id": "2",
          "time": "20210830 22:50"
        },
        {
          "id": "MDRW2100036418.1.10",
          "form": "거기서 친구 다 사겼다고 하더라고요 ㅎㅎ",
          "original_form": "거기서 친구 다 사겼다고 하더라고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 22:50"
        },
        {
          "id": "MDRW2100036418.1.11",
          "form": "아 진짜요 ??!",
          "original_form": "아 진짜요 ??!",
          "speaker_id": "1",
          "time": "20210830 22:51"
        },
        {
          "id": "MDRW2100036418.1.12",
          "form": "와 근데 뭔가 아이유도",
          "original_form": "와 근데 뭔가 아이유도",
          "speaker_id": "1",
          "time": "20210830 22:51"
        },
        {
          "id": "MDRW2100036418.1.13",
          "form": "인맥이 진짜 오래가는거같아요!",
          "original_form": "인맥이 진짜 오래가는거같아요!",
          "speaker_id": "1",
          "time": "20210830 22:51"
        },
        {
          "id": "MDRW2100036418.1.14",
          "form": "그니까요!! 좋은 사람들끼리 만나서 그른가..! 좀 부럽더라고요 ㅎ.ㅎ",
          "original_form": "그니까요!! 좋은 사람들끼리 만나서 그른가..! 좀 부럽더라고요 ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210830 22:52"
        },
        {
          "id": "MDRW2100036418.1.15",
          "form": "앗 저 티아라도 엄청 좋아했었어요 ㅎㅎ",
          "original_form": "앗 저 티아라도 엄청 좋아했었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 22:52"
        },
        {
          "id": "MDRW2100036418.1.16",
          "form": "티아라 안 좋아하는 사람 없었죠 !!",
          "original_form": "티아라 안 좋아하는 사람 없었죠 !!",
          "speaker_id": "1",
          "time": "20210830 22:53"
        },
        {
          "id": "MDRW2100036418.1.17",
          "form": "저 진짜 공부의신보고",
          "original_form": "저 진짜 공부의신보고",
          "speaker_id": "1",
          "time": "20210830 22:53"
        },
        {
          "id": "MDRW2100036418.1.18",
          "form": "지연 엄청 좋아했는뎈ㅋㅋ",
          "original_form": "지연 엄청 좋아했는뎈ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:53"
        },
        {
          "id": "MDRW2100036418.1.19",
          "form": "그니까요!!!! 완전 규ㅣ여웠잔ㄹ아요",
          "original_form": "그니까요!!!! 완전 규ㅣ여웠잔ㄹ아요",
          "speaker_id": "2",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100036418.1.20",
          "form": "전 드림하이2 속 지연도 좋아했어요",
          "original_form": "전 드림하이2 속 지연도 좋아했어요",
          "speaker_id": "2",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100036418.1.21",
          "form": "아 맞아요ㅋㄱㄲㄱ",
          "original_form": "아 맞아요ㅋㄱㄲㄱ",
          "speaker_id": "1",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100036418.1.22",
          "form": "오랜만이네요 드림하이 !",
          "original_form": "오랜만이네요 드림하이 !",
          "speaker_id": "1",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100036418.1.23",
          "form": "시즌1은 재밌게봤는데 ㅋㅋㅋ",
          "original_form": "시즌1은 재밌게봤는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100036418.1.24",
          "form": "맞어요!!! 거기서 처음 미스에이 알고 한창 빠져있었죠",
          "original_form": "맞어요!!! 거기서 처음 미스에이 알고 한창 빠져있었죠",
          "speaker_id": "2",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100036418.1.25",
          "form": "미스에이 터치가 제 최야곡이었어요!!",
          "original_form": "미스에이 터치가 제 최야곡이었어요!!",
          "speaker_id": "2",
          "time": "20210830 23:21"
