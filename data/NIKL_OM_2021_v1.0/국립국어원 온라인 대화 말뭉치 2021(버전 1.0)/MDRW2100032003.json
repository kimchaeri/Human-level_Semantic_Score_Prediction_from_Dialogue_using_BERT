      "utterance": [
        {
          "id": "MDRW2100032003.1.1",
          "form": "분명... 자발적 아싸였눙데...",
          "original_form": "분명... 자발적 아싸였눙데...",
          "speaker_id": "1",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100032003.1.2",
          "form": "죄 착각이었던..것...ㅋㅋㅋㅋ",
          "original_form": "죄 착각이었던..것...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:37"
        },
        {
          "id": "MDRW2100032003.1.3",
          "form": "진짴ㅋㅋㅋ 나한테 왜그래요",
          "original_form": "진짴ㅋㅋㅋ 나한테 왜그래요",
          "speaker_id": "1",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100032003.1.4",
          "form": "애초에 1학기때부터 성공했다고 생각했는데",
          "original_form": "애초에 1학기때부터 성공했다고 생각했는데",
          "speaker_id": "1",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100032003.1.5",
          "form": "성적 역추적부터 똥망했어",
          "original_form": "성적 역추적부터 똥망했어",
          "speaker_id": "1",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100032003.1.6",
          "form": "이런 비밀이 없는 학교 ㅋㅋㅋ",
          "original_form": "이런 비밀이 없는 학교 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 10:44"
        },
        {
          "id": "MDRW2100032003.1.7",
          "form": "거의 학년 중반쯤에는",
          "original_form": "거의 학년 중반쯤에는",
          "speaker_id": "1",
          "time": "20210905 10:45"
        },
        {
          "id": "MDRW2100032003.1.8",
          "form": "아무짓도 안해도",
          "original_form": "아무짓도 안해도",
          "speaker_id": "1",
          "time": "20210905 10:45"
        },
        {
          "id": "MDRW2100032003.1.9",
          "form": "아니.. 가 ㅇ 제 공개 됐다고",
          "original_form": "아니.. 가 ㅇ 제 공개 됐다고",
          "speaker_id": "1",
          "time": "20210905 10:45"
        },
        {
          "id": "MDRW2100032003.1.10",
          "form": "이 필터링 진짜..",
          "original_form": "이 필터링 진짜..",
          "speaker_id": "1",
          "time": "20210905 10:45"
        },
        {
          "id": "MDRW2100032003.1.11",
          "form": "문맥도 안 읽네",
          "original_form": "문맥도 안 읽네",
          "speaker_id": "1",
          "time": "20210905 10:45"
        },
        {
          "id": "MDRW2100032003.1.12",
          "form": "비공개로 준 의미가 없음...",
          "original_form": "비공개로 준 의미가 없음...",
          "speaker_id": "1",
          "time": "20210905 10:46"
        },
        {
          "id": "MDRW2100032003.1.13",
          "form": "등수만 알려지냐.. 학점도 다 공개구만",
          "original_form": "등수만 알려지냐.. 학점도 다 공개구만",
          "speaker_id": "1",
          "time": "20210905 10:46"
        },
        {
          "id": "MDRW2100032003.1.14",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:04"
        },
        {
          "id": "MDRW2100032003.1.15",
          "form": "그치 다들",
          "original_form": "그치 다들",
          "speaker_id": "2",
          "time": "20210905 11:04"
        },
        {
          "id": "MDRW2100032003.1.16",
          "form": "순위나온거 다 찾아다가 줄줄이 세워놓자넠",
          "original_form": "순위나온거 다 찾아다가 줄줄이 세워놓자넠",
          "speaker_id": "2",
          "time": "20210905 11:05"
        },
        {
          "id": "MDRW2100032003.1.17",
          "form": "ㅇㅇ...구리고 왜 난 매번 예외가 없는거니...",
          "original_form": "ㅇㅇ...구리고 왜 난 매번 예외가 없는거니...",
          "speaker_id": "1",
          "time": "20210905 11:09"
        },
        {
          "id": "MDRW2100032003.1.18",
          "form": "내가 아니라 다른애가 위 일 수도 있잖니...",
          "original_form": "내가 아니라 다른애가 위 일 수도 있잖니...",
          "speaker_id": "1",
          "time": "20210905 11:09"
        },
        {
          "id": "MDRW2100032003.1.19",
          "form": "애초에 1학기 초부터",
          "original_form": "애초에 1학기 초부터",
          "speaker_id": "1",
          "time": "20210905 11:09"
        },
        {
          "id": "MDRW2100032003.1.20",
          "form": "상위쪽에 랭크로 염두하고 다들 추측했단게",
          "original_form": "상위쪽에 랭크로 염두하고 다들 추측했단게",
          "speaker_id": "1",
          "time": "20210905 11:09"
        },
        {
          "id": "MDRW2100032003.1.21",
          "form": "얘들아.. 나 머리 나빠... 공부 못해요...",
          "original_form": "얘들아.. 나 머리 나빠... 공부 못해요...",
          "speaker_id": "1",
          "time": "20210905 11:09"
        },
        {
          "id": "MDRW2100032003.1.22",
          "form": "그치만 성적표는 거짓말 하지 않아요 ㅋㅋㅋ",
          "original_form": "그치만 성적표는 거짓말 하지 않아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:28"
        },
        {
          "id": "MDRW2100032003.1.23",
          "form": "아니에요 그짓말이야 그거",
          "original_form": "아니에요 그짓말이야 그거",
          "speaker_id": "1",
          "time": "20210905 11:38"
        },
        {
          "id": "MDRW2100032003.1.24",
          "form": "성적표가 줏대가 없어서 그래",
          "original_form": "성적표가 줏대가 없어서 그래",
          "speaker_id": "1",
          "time": "20210905 12:04"
        },
        {
          "id": "MDRW2100032003.1.25",
          "form": "줏대가 없다니 ㅋㅋㅋ",
          "original_form": "줏대가 없다니 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 12:38"
        },
        {
          "id": "MDRW2100032003.1.26",
          "form": "내 성적표는 줏대가 없었는걸",
          "original_form": "내 성적표는 줏대가 없었는걸",
          "speaker_id": "1",
          "time": "20210905 12:47"
        },
        {
          "id": "MDRW2100032003.1.27",
          "form": "에이 그래도 나만했을까 ㅋㅋㅋ",
          "original_form": "에이 그래도 나만했을까 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 12:56"
        },
        {
          "id": "MDRW2100032003.1.28",
          "form": "제일 널뛰기 많이 했을건데 ㅋㅋ",
          "original_form": "제일 널뛰기 많이 했을건데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 12:56"
        },
        {
          "id": "MDRW2100032003.1.29",
          "form": "만만치 않아 ㅋㅋㅋ",
          "original_form": "만만치 않아 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:02"
        },
        {
          "id": "MDRW2100032003.1.30",
          "form": "전체적으로 보면 올라가는 추세여도",
          "original_form": "전체적으로 보면 올라가는 추세여도",
          "speaker_id": "1",
          "time": "20210905 13:03"
        },
        {
          "id": "MDRW2100032003.1.31",
          "form": "엄청 오르락 내리락했어 ㅋㅋ",
          "original_form": "엄청 오르락 내리락했어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:03"
        },
        {
          "id": "MDRW2100032003.1.32",
          "form": "오르락~ 내리락~ 해요~",
          "original_form": "오르락~ 내리락~ 해요~",
          "speaker_id": "2",
          "time": "20210905 13:12"
        },
        {
          "id": "MDRW2100032003.1.33",
          "form": "엘리베이터지 ㅋㅋㅋ",
          "original_form": "엘리베이터지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:12"
        },
        {
          "id": "MDRW2100032003.1.34",
          "form": "그리고 이때 미나의 성적표가 탐나는데",
          "original_form": "그리고 이때 미나의 성적표가 탐나는데",
          "speaker_id": "2",
          "time": "20210905 13:12"
        },
        {
          "id": "MDRW2100032003.1.35",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:12"
        },
        {
          "id": "MDRW2100032003.1.36",
          "form": "성적은 에스칼레이터야하는데",
          "original_form": "성적은 에스칼레이터야하는데",
          "speaker_id": "1",
          "time": "20210905 13:12"
        },
        {
          "id": "MDRW2100032003.1.37",
          "form": "마저....",
          "original_form": "마저....",
          "speaker_id": "1",
          "time": "20210905 13:12"
        },
        {
          "id": "MDRW2100032003.1.38",
          "form": "진짜 최종학점 4.5",
          "original_form": "진짜 최종학점 4.5",
          "speaker_id": "1",
          "time": "20210905 13:12"
        },
        {
          "id": "MDRW2100032003.1.39",
          "form": "너무나 이상적이야..",
          "original_form": "너무나 이상적이야..",
          "speaker_id": "2",
          "time": "20210905 13:12"
        },
        {
          "id": "MDRW2100032003.1.40",
          "form": "시작점의.차이도 있긴했겠지만",
          "original_form": "시작점의.차이도 있긴했겠지만",
          "speaker_id": "1",
          "time": "20210905 13:13"
        },
        {
          "id": "MDRW2100032003.1.41",
          "form": "아님 처음부터 끝까지 4.5 여야했는데",
          "original_form": "아님 처음부터 끝까지 4.5 여야했는데",
          "speaker_id": "1",
          "time": "20210905 13:13"
        },
        {
          "id": "MDRW2100032003.1.42",
          "form": "그거야말로 꿈의 성적표다 ㅋㅋㅋ",
          "original_form": "그거야말로 꿈의 성적표다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:27"
        },
        {
          "id": "MDRW2100032003.1.43",
          "form": "내가 졸업전까지 4.5를 못찍고 졸업한게 한이다 진짜",
          "original_form": "내가 졸업전까지 4.5를 못찍고 졸업한게 한이다 진짜",
          "speaker_id": "1",
          "time": "20210905 13:28"
        },
        {
          "id": "MDRW2100032003.1.44",
          "form": "몹쓸 인강 ㅠ",
          "original_form": "몹쓸 인강 ㅠ",
          "speaker_id": "1",
          "time": "20210905 13:28"
        },
        {
          "id": "MDRW2100032003.1.45",
          "form": "고ㅑㄴ히 들어가지고 ㅋㅋㅋㅋ",
          "original_form": "고ㅑㄴ히 들어가지고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:32"
        },
        {
          "id": "MDRW2100032003.1.46",
          "form": "진짜 이건 내가 잘못한게지.. 엫혀..",
          "original_form": "진짜 이건 내가 잘못한게지.. 엫혀..",
          "speaker_id": "1",
          "time": "20210905 13:34"
        },
        {
          "id": "MDRW2100032003.1.47",
          "form": "4.5 할수 있었는데",
          "original_form": "4.5 할수 있었는데",
          "speaker_id": "1",
          "time": "20210905 13:34"
        },
        {
          "id": "MDRW2100032003.1.48",
          "form": "아쉽아쉽",
          "original_form": "아쉽아쉽",
          "speaker_id": "2",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100032003.1.49",
