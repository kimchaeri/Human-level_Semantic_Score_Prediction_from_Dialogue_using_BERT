      "utterance": [
        {
          "id": "MDRW2100010042.1.1",
          "form": "저희 고3아들도 중등되서야",
          "original_form": "저희 고3아들도 중등되서야",
          "speaker_id": "2",
          "time": "20210823 13:29"
        },
        {
          "id": "MDRW2100010042.1.2",
          "form": "사발면 끓여먹게 시켰어요",
          "original_form": "사발면 끓여먹게 시켰어요",
          "speaker_id": "2",
          "time": "20210823 13:29"
        },
        {
          "id": "MDRW2100010042.1.3",
          "form": "저도 겁이 많아요",
          "original_form": "저도 겁이 많아요",
          "speaker_id": "2",
          "time": "20210823 13:29"
        },
        {
          "id": "MDRW2100010042.1.4",
          "form": "요즘애들 빠르긴해요",
          "original_form": "요즘애들 빠르긴해요",
          "speaker_id": "1",
          "time": "20210823 13:29"
        },
        {
          "id": "MDRW2100010042.1.5",
          "form": "친구들이 편의점에서 컵라면 사먹는다더라구요",
          "original_form": "친구들이 편의점에서 컵라면 사먹는다더라구요",
          "speaker_id": "1",
          "time": "20210823 13:30"
        },
        {
          "id": "MDRW2100010042.1.6",
          "form": "뜨거운물부어서요",
          "original_form": "뜨거운물부어서요",
          "speaker_id": "1",
          "time": "20210823 13:30"
        },
        {
          "id": "MDRW2100010042.1.7",
          "form": "그렇더라구요",
          "original_form": "그렇더라구요",
          "speaker_id": "2",
          "time": "20210823 13:30"
        },
        {
          "id": "MDRW2100010042.1.8",
          "form": "ㅡㅡ전 절대 못하게해요",
          "original_form": "ㅡㅡ전 절대 못하게해요",
          "speaker_id": "2",
          "time": "20210823 13:30"
        },
        {
          "id": "MDRW2100010042.1.9",
          "form": "저 없을때 다치면 누가 병원 데려가겠어요",
          "original_form": "저 없을때 다치면 누가 병원 데려가겠어요",
          "speaker_id": "2",
          "time": "20210823 13:30"
        },
        {
          "id": "MDRW2100010042.1.10",
          "form": "근데 아이들도 또 진짜 그때 한참 배고플때니",
          "original_form": "근데 아이들도 또 진짜 그때 한참 배고플때니",
          "speaker_id": "2",
          "time": "20210823 13:31"
        },
        {
          "id": "MDRW2100010042.1.11",
          "form": "편의점 직원이 안바쁘면 좀 도와주면 좋겠다 생각도 해보고",
          "original_form": "편의점 직원이 안바쁘면 좀 도와주면 좋겠다 생각도 해보고",
          "speaker_id": "2",
          "time": "20210823 13:31"
        },
        {
          "id": "MDRW2100010042.1.12",
          "form": "별생각 다해봤어요 ㅋㅋ",
          "original_form": "별생각 다해봤어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:31"
        },
        {
          "id": "MDRW2100010042.1.13",
          "form": "그니깐요 ㅋ",
          "original_form": "그니깐요 ㅋ",
          "speaker_id": "1",
          "time": "20210823 13:32"
        },
        {
          "id": "MDRW2100010042.1.14",
          "form": "친구들이 부어줬대요ㅜㅜㅋ",
          "original_form": "친구들이 부어줬대요ㅜㅜㅋ",
          "speaker_id": "1",
          "time": "20210823 13:32"
        },
        {
          "id": "MDRW2100010042.1.15",
          "form": "잘하는 친구들이 있더라고요",
          "original_form": "잘하는 친구들이 있더라고요",
          "speaker_id": "2",
          "time": "20210823 13:38"
        },
        {
          "id": "MDRW2100010042.1.16",
          "form": "넘 고맙네요",
          "original_form": "넘 고맙네요",
          "speaker_id": "2",
          "time": "20210823 13:39"
        },
        {
          "id": "MDRW2100010042.1.17",
          "form": "아직어려서 혼자 부어먹기 힘들죠",
          "original_form": "아직어려서 혼자 부어먹기 힘들죠",
          "speaker_id": "2",
          "time": "20210823 13:39"
        },
        {
          "id": "MDRW2100010042.1.18",
          "form": "4학년이 고학년이라 이제 이것저것 경험하죠",
          "original_form": "4학년이 고학년이라 이제 이것저것 경험하죠",
          "speaker_id": "2",
          "time": "20210823 13:39"
        },
        {
          "id": "MDRW2100010042.1.19",
          "form": "사발면도 먹어보고 사춘기 처럼 반항도 하고요",
          "original_form": "사발면도 먹어보고 사춘기 처럼 반항도 하고요",
          "speaker_id": "2",
          "time": "20210823 13:40"
        },
        {
          "id": "MDRW2100010042.1.20",
          "form": "3학년이예요ㅜ\r\n한창 ..반항기",
          "original_form": "3학년이예요ㅜ\r\n한창 ..반항기",
          "speaker_id": "1",
          "time": "20210823 13:46"
        },
        {
          "id": "MDRW2100010042.1.21",
          "form": "한창친구랑 노느는게좋아서",
          "original_form": "한창친구랑 노느는게좋아서",
          "speaker_id": "1",
          "time": "20210823 13:48"
        },
        {
          "id": "MDRW2100010042.1.22",
          "form": "8시 15분에 들어오는거보고",
          "original_form": "8시 15분에 들어오는거보고",
          "speaker_id": "1",
          "time": "20210823 13:48"
        },
        {
          "id": "MDRW2100010042.1.23",
          "form": "엄청머라고 했어요",
          "original_form": "엄청머라고 했어요",
          "speaker_id": "1",
          "time": "20210823 13:48"
        },
        {
          "id": "MDRW2100010042.1.24",
          "form": "8시 15분 넘 걱정하셨겠어요",
          "original_form": "8시 15분 넘 걱정하셨겠어요",
          "speaker_id": "2",
          "time": "20210823 13:56"
        },
        {
          "id": "MDRW2100010042.1.25",
          "form": "저희애도 그때부터 사춘기 시작이라 반항했어요",
          "original_form": "저희애도 그때부터 사춘기 시작이라 반항했어요",
          "speaker_id": "2",
          "time": "20210823 13:56"
        },
        {
          "id": "MDRW2100010042.1.26",
          "form": "근데 화 내실만해요",
          "original_form": "근데 화 내실만해요",
          "speaker_id": "2",
          "time": "20210823 13:56"
        },
        {
          "id": "MDRW2100010042.1.27",
          "form": "남자애들이 놀면 자제를 못해요",
          "original_form": "남자애들이 놀면 자제를 못해요",
          "speaker_id": "2",
          "time": "20210823 13:56"
        },
        {
          "id": "MDRW2100010042.1.28",
          "form": "저희 큰애도 그랬어요",
          "original_form": "저희 큰애도 그랬어요",
          "speaker_id": "2",
          "time": "20210823 13:56"
        },
        {
          "id": "MDRW2100010042.1.29",
          "form": "나중에 축제때 새벽에도 와요 아무리 아파트 축제지만 저 그때 진짜 스팀나왔어요",
          "original_form": "나중에 축제때 새벽에도 와요 아무리 아파트 축제지만 저 그때 진짜 스팀나왔어요",
          "speaker_id": "2",
          "time": "20210823 14:05"
        },
        {
          "id": "MDRW2100010042.1.30",
          "form": "친구형성 기간이라고 그렇다곤 하는데..",
          "original_form": "친구형성 기간이라고 그렇다곤 하는데..",
          "speaker_id": "1",
          "time": "20210823 14:21"
        },
        {
          "id": "MDRW2100010042.1.31",
          "form": "너무 놀게할수도 없고..",
          "original_form": "너무 놀게할수도 없고..",
          "speaker_id": "1",
          "time": "20210823 14:22"
        },
        {
          "id": "MDRW2100010042.1.32",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210823 14:22"
        },
        {
          "id": "MDRW2100010042.1.33",
          "form": "근데 친구를 안만나게 하면 것도 아이가 넘 힘들어요",
          "original_form": "근데 친구를 안만나게 하면 것도 아이가 넘 힘들어요",
          "speaker_id": "2",
          "time": "20210823 14:22"
        },
        {
          "id": "MDRW2100010042.1.34",
          "form": "점점 부모보다 친구가 가까우니",
          "original_form": "점점 부모보다 친구가 가까우니",
          "speaker_id": "2",
          "time": "20210823 14:23"
        },
        {
          "id": "MDRW2100010042.1.35",
          "form": "막못놀게 할수가 없어요",
          "original_form": "막못놀게 할수가 없어요",
          "speaker_id": "2",
          "time": "20210823 14:23"
        },
        {
          "id": "MDRW2100010042.1.36",
          "form": "그래서 제한을 두고 할일안하면 못나가게 한다던지 제시간에 안들어오면 핸드폰을 뺏는다던지\r\n제지하니 조금 말듣더라구요",
          "original_form": "그래서 제한을 두고 할일안하면 못나가게 한다던지 제시간에 안들어오면 핸드폰을 뺏는다던지\r\n제지하니 조금 말듣더라구요",
          "speaker_id": "1",
          "time": "20210823 14:38"
        },
        {
          "id": "MDRW2100010042.1.37",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210823 14:39"
        },
        {
          "id": "MDRW2100010042.1.38",
          "form": "제지 제한은 해야해요",
          "original_form": "제지 제한은 해야해요",
          "speaker_id": "2",
          "time": "20210823 14:39"
        },
        {
          "id": "MDRW2100010042.1.39",
          "form": "애들이 크나 작으나  어차피 육아는 매번 고민이고 매번 힘들어요",
          "original_form": "애들이 크나 작으나  어차피 육아는 매번 고민이고 매번 힘들어요",
          "speaker_id": "2",
          "time": "20210823 14:39"
        },
        {
          "id": "MDRW2100010042.1.40",
          "form": "일도하시고 진짜 대단하세요 드라마 정주행 할시간도 없으실듯해요",
          "original_form": "일도하시고 진짜 대단하세요 드라마 정주행 할시간도 없으실듯해요",
          "speaker_id": "2",
          "time": "20210823 14:40"
        },
        {
          "id": "MDRW2100010042.1.41",
          "form": "ㅠㅠ 그래서 말씀드리게 다른거 하면서 드라마 보는거라 자막있는걸 못보는 거예요 ㅋㅋㅋ",
          "original_form": "ㅠㅠ 그래서 말씀드리게 다른거 하면서 드라마 보는거라 자막있는걸 못보는 거예요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:19"
        },
        {
          "id": "MDRW2100010042.1.42",
          "form": "집안일 해감서 그래도 드라마는 보고 싶으니깐... ㅎㅎㅎ",
          "original_form": "집안일 해감서 그래도 드라마는 보고 싶으니깐... ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 17:19"
        },
        {
          "id": "MDRW2100010042.1.43",
          "form": "그래두 풀근무는 아니라서 일찍 퇴근하니깐요",
          "original_form": "그래두 풀근무는 아니라서 일찍 퇴근하니깐요",
          "speaker_id": "1",
          "time": "20210823 17:19"
        },
        {
          "id": "MDRW2100010042.1.44",
          "form": "완전 이해해요 ㅎㅎ",
          "original_form": "완전 이해해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 17:38"
        },
        {
          "id": "MDRW2100010042.1.45",
          "form": "풀근무 아니라 다행이예요",
          "original_form": "풀근무 아니라 다행이예요",
          "speaker_id": "2",
          "time": "20210823 17:38"
        },
        {
          "id": "MDRW2100010042.1.46",
          "form": "집안일에 활력소 드라마죠 ㅎㅎ",
          "original_form": "집안일에 활력소 드라마죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 17:38"
        },
        {
          "id": "MDRW2100010042.1.47",
          "form": "일찍 퇴근해야지 집안일에 아이 케어하지 안그럼 밥할 시간도 없을것 같아요",
          "original_form": "일찍 퇴근해야지 집안일에 아이 케어하지 안그럼 밥할 시간도 없을것 같아요",
          "speaker_id": "2",
          "time": "20210823 17:39"
        }
      ]
