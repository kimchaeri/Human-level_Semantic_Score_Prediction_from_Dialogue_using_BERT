      "utterance": [
        {
          "id": "MDRW2100028486.1.1",
          "form": "엄마 요즘 뭐봐?",
          "original_form": "엄마 요즘 뭐봐?",
          "speaker_id": "2",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028486.1.2",
          "form": "난 모르는게 없고 안보는거 없이 다 본다 재밌으면",
          "original_form": "난 모르는게 없고 안보는거 없이 다 본다 재밌으면",
          "speaker_id": "1",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028486.1.3",
          "form": "아 그러니까 그 아랍? 드라마까지 보는 거 대박",
          "original_form": "아 그러니까 그 아랍? 드라마까지 보는 거 대박",
          "speaker_id": "2",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028486.1.4",
          "form": "ㅋㅋㅋㅋ왜 재밌던데",
          "original_form": "ㅋㅋㅋㅋ왜 재밌던데",
          "speaker_id": "1",
          "time": "20210902 20:21"
        },
        {
          "id": "MDRW2100028486.1.5",
          "form": "근데 그쪽 애들은 좀 삭아보이더라",
          "original_form": "근데 그쪽 애들은 좀 삭아보이더라",
          "speaker_id": "1",
          "time": "20210902 20:21"
        },
        {
          "id": "MDRW2100028486.1.6",
          "form": "학생인데",
          "original_form": "학생인데",
          "speaker_id": "1",
          "time": "20210902 20:21"
        },
        {
          "id": "MDRW2100028486.1.7",
          "form": "좀 우리나라보다 애들이 성숙해 보이긴하더라",
          "original_form": "좀 우리나라보다 애들이 성숙해 보이긴하더라",
          "speaker_id": "2",
          "time": "20210902 20:21"
        },
        {
          "id": "MDRW2100028486.1.8",
          "form": "체구 때문인가?",
          "original_form": "체구 때문인가?",
          "speaker_id": "2",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100028486.1.9",
          "form": "영화도 보고 싶은데",
          "original_form": "영화도 보고 싶은데",
          "speaker_id": "1",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100028486.1.10",
          "form": "극장가기가 그래서 못보고 있다",
          "original_form": "극장가기가 그래서 못보고 있다",
          "speaker_id": "1",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100028486.1.11",
          "form": "맞아 나 모가디슈랑 싱크홀 극장가서 보고 싶은데 코로나 짜응",
          "original_form": "맞아 나 모가디슈랑 싱크홀 극장가서 보고 싶은데 코로나 짜응",
          "speaker_id": "2",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028486.1.12",
          "form": "이제 극장 갈 수 있을지 모르겠다 코로나 때매",
          "original_form": "이제 극장 갈 수 있을지 모르겠다 코로나 때매",
          "speaker_id": "1",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028486.1.13",
          "form": "아ㅠㅜ진짜 싫어 코로나",
          "original_form": "아ㅠㅜ진짜 싫어 코로나",
          "speaker_id": "2",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100028486.1.14",
          "form": "경찰수업 재밌던데 그거나 봐라",
          "original_form": "경찰수업 재밌던데 그거나 봐라",
          "speaker_id": "1",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100028486.1.15",
          "form": "그거 재밌음?",
          "original_form": "그거 재밌음?",
          "speaker_id": "2",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100028486.1.16",
          "form": "볼만해 요즘 우리나라 드라마 볼 거 별로 없는데 그나마나음",
          "original_form": "볼만해 요즘 우리나라 드라마 볼 거 별로 없는데 그나마나음",
          "speaker_id": "1",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028486.1.17",
          "form": "아 넷플 오징어게임 한다는데 그거 재밌겠더라",
          "original_form": "아 넷플 오징어게임 한다는데 그거 재밌겠더라",
          "speaker_id": "2",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028486.1.18",
          "form": "드라마 이름이야?",
          "original_form": "드라마 이름이야?",
          "speaker_id": "1",
          "time": "20210902 20:25"
        },
