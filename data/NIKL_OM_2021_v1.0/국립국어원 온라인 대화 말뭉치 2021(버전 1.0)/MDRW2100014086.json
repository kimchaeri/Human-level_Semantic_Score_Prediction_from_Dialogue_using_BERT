      "utterance": [
        {
          "id": "MDRW2100014086.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 17:12"
        },
        {
          "id": "MDRW2100014086.1.2",
          "form": "혹시 종교 있으신가요 전 무교라서",
          "original_form": "혹시 종교 있으신가요 전 무교라서",
          "speaker_id": "1",
          "time": "20210825 17:13"
        },
        {
          "id": "MDRW2100014086.1.3",
          "form": "저도 무교이긴한데 초등학생때는 교회에 다녔었어요",
          "original_form": "저도 무교이긴한데 초등학생때는 교회에 다녔었어요",
          "speaker_id": "2",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014086.1.4",
          "form": "종교를 가지면 소속감도 들고 심리적안정감 같은게 드니까",
          "original_form": "종교를 가지면 소속감도 들고 심리적안정감 같은게 드니까",
          "speaker_id": "1",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014086.1.5",
          "form": "맞아요 저도 초등학생땐 친구들 따라서 다니기시작했었어요",
          "original_form": "맞아요 저도 초등학생땐 친구들 따라서 다니기시작했었어요",
          "speaker_id": "2",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014086.1.6",
          "form": "같은 종교인 친구들이랑 친해지면 활동이 재밌을꺼같아요",
          "original_form": "같은 종교인 친구들이랑 친해지면 활동이 재밌을꺼같아요",
          "speaker_id": "1",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014086.1.7",
          "form": "저도 교회다니는 친구 있었는데 이것저것 재밌는 활동같은거 많이하더라구요",
          "original_form": "저도 교회다니는 친구 있었는데 이것저것 재밌는 활동같은거 많이하더라구요",
          "speaker_id": "1",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014086.1.8",
          "form": "ㅎㅎㅎㅎ막 수련회도 있고 확실히 결집력이 좋은것같아요",
          "original_form": "ㅎㅎㅎㅎ막 수련회도 있고 확실히 결집력이 좋은것같아요",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014086.1.9",
          "form": "코로나이후엔 종교행사도 힘들어보이더라구오ㅡ",
          "original_form": "코로나이후엔 종교행사도 힘들어보이더라구오ㅡ",
          "speaker_id": "2",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014086.1.10",
          "form": "음 요즘은 코시국이니까 사람들을 안만나는게좋죠",
          "original_form": "음 요즘은 코시국이니까 사람들을 안만나는게좋죠",
          "speaker_id": "1",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100014086.1.11",
          "form": "맞아요  전에 지나가다 절행사를 봤는데 사람이 너무많아 걱정되더라구요",
          "original_form": "맞아요  전에 지나가다 절행사를 봤는데 사람이 너무많아 걱정되더라구요",
          "speaker_id": "2",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100014086.1.12",
          "form": "이시국에 행사하는건 좀 아닌거같아요",
          "original_form": "이시국에 행사하는건 좀 아닌거같아요",
          "speaker_id": "1",
          "time": "20210825 17:37"
        },
        {
          "id": "MDRW2100014086.1.13",
          "form": "요즘은 델타바이러스도 있어서",
          "original_form": "요즘은 델타바이러스도 있어서",
          "speaker_id": "1",
          "time": "20210825 17:38"
        },
        {
          "id": "MDRW2100014086.1.14",
          "form": "내년에 코로나 끝나는 기념으로 행사같은거 성대하게 하면좋을꺼같은데",
          "original_form": "내년에 코로나 끝나는 기념으로 행사같은거 성대하게 하면좋을꺼같은데",
          "speaker_id": "1",
          "time": "20210825 17:38"
        },
        {
          "id": "MDRW2100014086.1.15",
          "form": "맞아요 진짜 그랬음좋겠는데 사실 이해가 잘안되기도해요",
          "original_form": "맞아요 진짜 그랬음좋겠는데 사실 이해가 잘안되기도해요",
          "speaker_id": "2",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100014086.1.16",
          "form": "행사를 꼭 지금해야하는지..",
          "original_form": "행사를 꼭 지금해야하는지..",
          "speaker_id": "2",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100014086.1.17",
          "form": "내년이면 대충 정리될꺼같아서",
          "original_form": "내년이면 대충 정리될꺼같아서",
          "speaker_id": "1",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100014086.1.18",
          "form": "궂이 이번년도에는 행사든 뭐든 여러명 안모이는게 맞는거같아요",
          "original_form": "궂이 이번년도에는 행사든 뭐든 여러명 안모이는게 맞는거같아요",
          "speaker_id": "1",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100014086.1.19",
          "form": "맞아요!  딱 정확한 말이네요",
          "original_form": "맞아요!  딱 정확한 말이네요",
          "speaker_id": "2",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100014086.1.20",
          "form": "제발 코로나가 끝났으면좋겠어요",
          "original_form": "제발 코로나가 끝났으면좋겠어요",
          "speaker_id": "2",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100014086.1.21",
          "form": "정말 코로나가 빨리 끝나면 좋을것같아요!",
          "original_form": "정말 코로나가 빨리 끝나면 좋을것같아요!",
          "speaker_id": "1",
          "time": "20210825 17:44"
        }
      ]
    }
