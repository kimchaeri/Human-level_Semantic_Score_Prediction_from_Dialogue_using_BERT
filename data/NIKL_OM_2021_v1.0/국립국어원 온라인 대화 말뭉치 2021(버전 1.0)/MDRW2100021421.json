        },
        {
          "id": "MDRW2100021421.1.7",
          "form": "청소하고",
          "original_form": "청소하고",
          "speaker_id": "2",
          "time": "20210829 17:14"
        },
        {
          "id": "MDRW2100021421.1.8",
          "form": "애기들 봤지",
