      "utterance": [
        {
          "id": "MDRW2100011121.1.1",
          "form": "근데 리디 처음엔 혜택많았는데",
          "original_form": "근데 리디 처음엔 혜택많았는데",
          "speaker_id": "2",
          "time": "20210824 01:15"
        },
        {
          "id": "MDRW2100011121.1.2",
          "form": "누가 리디를 자꾸 신고해서...",
          "original_form": "누가 리디를 자꾸 신고해서...",
          "speaker_id": "2",
          "time": "20210824 01:15"
        },
        {
          "id": "MDRW2100011121.1.3",
          "form": "암튼 리디 이번에 도서정가제때문에 가격 올랐잖아요",
          "original_form": "암튼 리디 이번에 도서정가제때문에 가격 올랐잖아요",
          "speaker_id": "1",
          "time": "20210824 01:15"
        },
        {
          "id": "MDRW2100011121.1.4",
          "form": "다 날라간거잖아요",
          "original_form": "다 날라간거잖아요",
          "speaker_id": "2",
          "time": "20210824 01:15"
        },
        {
          "id": "MDRW2100011121.1.5",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210824 01:15"
        },
        {
          "id": "MDRW2100011121.1.6",
          "form": "갑자기 또 화나네 하...",
          "original_form": "갑자기 또 화나네 하...",
          "speaker_id": "2",
          "time": "20210824 01:15"
        },
        {
          "id": "MDRW2100011121.1.7",
          "form": "그거 근데 웹소 작가분들이",
          "original_form": "그거 근데 웹소 작가분들이",
          "speaker_id": "1",
          "time": "20210824 01:16"
        },
        {
          "id": "MDRW2100011121.1.8",
          "form": "신고한거잖아요",
          "original_form": "신고한거잖아요",
          "speaker_id": "1",
          "time": "20210824 01:16"
        },
        {
          "id": "MDRW2100011121.1.9",
          "form": "엥?",
          "original_form": "엥?",
          "speaker_id": "2",
          "time": "20210824 01:16"
        },
        {
          "id": "MDRW2100011121.1.10",
          "form": "아니왜요?",
          "original_form": "아니왜요?",
          "speaker_id": "2",
          "time": "20210824 01:16"
        },
        {
          "id": "MDRW2100011121.1.11",
          "form": "아...씁...이유가 뭐지...",
          "original_form": "아...씁...이유가 뭐지...",
          "speaker_id": "2",
          "time": "20210824 01:16"
        },
        {
          "id": "MDRW2100011121.1.12",
          "form": "그 인기작품 말고 작품 프로모션 안들어가는 작가님들이",
          "original_form": "그 인기작품 말고 작품 프로모션 안들어가는 작가님들이",
          "speaker_id": "1",
          "time": "20210824 01:16"
        },
        {
          "id": "MDRW2100011121.1.13",
          "form": "리디 도서정가제 위반했다고 신고하셨어요",
          "original_form": "리디 도서정가제 위반했다고 신고하셨어요",
          "speaker_id": "1",
          "time": "20210824 01:16"
        },
        {
          "id": "MDRW2100011121.1.14",
          "form": "프로모션 인기작가만 해주고 그러니까",
          "original_form": "프로모션 인기작가만 해주고 그러니까",
          "speaker_id": "1",
          "time": "20210824 01:16"
        },
        {
          "id": "MDRW2100011121.1.15",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "2",
          "time": "20210824 01:17"
        },
        {
          "id": "MDRW2100011121.1.16",
          "form": "그럼 뭐라 할 수가 없어요",
          "original_form": "그럼 뭐라 할 수가 없어요",
          "speaker_id": "2",
          "time": "20210824 01:17"
        },
        {
          "id": "MDRW2100011121.1.17",
          "form": "근데 도서정가제로 혜택줄어들고",
          "original_form": "근데 도서정가제로 혜택줄어들고",
          "speaker_id": "1",
          "time": "20210824 01:17"
        },
        {
          "id": "MDRW2100011121.1.18",
          "form": "아...씁...........",
          "original_form": "아...씁...........",
          "speaker_id": "2",
          "time": "20210824 01:17"
        },
        {
          "id": "MDRW2100011121.1.19",
          "form": "신인 작가들은 더 힘들어졌잖아요",
          "original_form": "신인 작가들은 더 힘들어졌잖아요",
          "speaker_id": "1",
          "time": "20210824 01:17"
        },
        {
          "id": "MDRW2100011121.1.20",
          "form": "사실 그렇긴해서 이게 약간..",
          "original_form": "사실 그렇긴해서 이게 약간..",
          "speaker_id": "2",
          "time": "20210824 01:17"
        },
        {
          "id": "MDRW2100011121.1.21",
          "form": "약간 자기 살 파먹기였죠",
          "original_form": "약간 자기 살 파먹기였죠",
          "speaker_id": "1",
          "time": "20210824 01:17"
        },
        {
          "id": "MDRW2100011121.1.22",
          "form": "글고 독자들은 도정제가 뭔지도 몰랐어요 작가분들만 알았지",
          "original_form": "글고 독자들은 도정제가 뭔지도 몰랐어요 작가분들만 알았지",
          "speaker_id": "1",
          "time": "20210824 01:17"
        },
        {
          "id": "MDRW2100011121.1.23",
          "form": "신기한게 저는 같은 업계사람이 신거할거란 생각을 못했어요",
          "original_form": "신기한게 저는 같은 업계사람이 신거할거란 생각을 못했어요",
          "speaker_id": "2",
          "time": "20210824 01:18"
        },
        {
          "id": "MDRW2100011121.1.24",
          "form": "헉 진짜요?!!",
          "original_form": "헉 진짜요?!!",
          "speaker_id": "1",
          "time": "20210824 01:18"
        },
        {
          "id": "MDRW2100011121.1.25",
          "form": "걍 어떤 독자가 그런줄 알았네 네!",
          "original_form": "걍 어떤 독자가 그런줄 알았네 네!",
          "speaker_id": "2",
          "time": "20210824 01:18"
        },
        {
          "id": "MDRW2100011121.1.26",
          "form": "자세하게 안찾아봐서",
          "original_form": "자세하게 안찾아봐서",
          "speaker_id": "2",
          "time": "20210824 01:18"
        },
        {
          "id": "MDRW2100011121.1.27",
          "form": "아니에요 독자들 중에 글케 도정제 잘아는 사람 많이 없었어요",
          "original_form": "아니에요 독자들 중에 글케 도정제 잘아는 사람 많이 없었어요",
          "speaker_id": "1",
          "time": "20210824 01:18"
        },
        {
          "id": "MDRW2100011121.1.28",
          "form": "하 씁 혜택 줄었다고? 어쩔수 없지 그냥보자!<",
          "original_form": "하 씁 혜택 줄었다고? 어쩔수 없지 그냥보자!<",
          "speaker_id": "2",
          "time": "20210824 01:18"
        },
        {
          "id": "MDRW2100011121.1.29",
          "form": "아 씁...",
          "original_form": "아 씁...",
          "speaker_id": "2",
          "time": "20210824 01:18"
        },
        {
          "id": "MDRW2100011121.1.30",
          "form": "그 첨에 도정제 글 신고하자고 올린 커뮤니티가 있는데",
          "original_form": "그 첨에 도정제 글 신고하자고 올린 커뮤니티가 있는데",
          "speaker_id": "1",
          "time": "20210824 01:18"
        },
        {
          "id": "MDRW2100011121.1.31",
          "form": "거기가 웹소 작가님들 카페였고 그 카페글 퍼가서",
          "original_form": "거기가 웹소 작가님들 카페였고 그 카페글 퍼가서",
          "speaker_id": "1",
          "time": "20210824 01:18"
        },
        {
          "id": "MDRW2100011121.1.32",
          "form": "독자중 몇몇도 신고하기 시작한거에요",
          "original_form": "독자중 몇몇도 신고하기 시작한거에요",
          "speaker_id": "1",
          "time": "20210824 01:19"
        },
        {
          "id": "MDRW2100011121.1.33",
          "form": "저는 책을 하도 사서...도정제 때문에 매일 눈물만...흘렸다고요 안돼 하지마 이러면서 아놔 진짜요?",
          "original_form": "저는 책을 하도 사서...도정제 때문에 매일 눈물만...흘렸다고요 안돼 하지마 이러면서 아놔 진짜요?",
          "speaker_id": "2",
          "time": "20210824 01:19"
        },
        {
          "id": "MDRW2100011121.1.34",
          "form": "왜 왜...왜 그런짓을",
          "original_form": "왜 왜...왜 그런짓을",
          "speaker_id": "2",
          "time": "20210824 01:19"
        },
        {
          "id": "MDRW2100011121.1.35",
          "form": "긍까요",
          "original_form": "긍까요",
          "speaker_id": "1",
          "time": "20210824 01:19"
        },
        {
          "id": "MDRW2100011121.1.36",
          "form": "진짜 도정데는 누굴위한건지 모르겠어요",
          "original_form": "진짜 도정데는 누굴위한건지 모르겠어요",
          "speaker_id": "2",
          "time": "20210824 01:19"
        },
        {
          "id": "MDRW2100011121.1.37",
          "form": "지역 서점 활성화라하는데",
          "original_form": "지역 서점 활성화라하는데",
          "speaker_id": "1",
          "time": "20210824 01:19"
        },
        {
          "id": "MDRW2100011121.1.38",
          "form": "동네서점위한건가 했는데 그렇다고 누가 동네서점을 가냐고요ㅠ 애시당초 웹툰 웹소설은 해당도 안되는데",
          "original_form": "동네서점위한건가 했는데 그렇다고 누가 동네서점을 가냐고요ㅠ 애시당초 웹툰 웹소설은 해당도 안되는데",
          "speaker_id": "2",
          "time": "20210824 01:19"
        },
        {
          "id": "MDRW2100011121.1.39",
          "form": "저는 지역서점을 위해 만든 법 안에 왜 웹툰 웹소설이 묶이는지 잘",
          "original_form": "저는 지역서점을 위해 만든 법 안에 왜 웹툰 웹소설이 묶이는지 잘",
          "speaker_id": "1",
          "time": "20210824 01:20"
        },
        {
          "id": "MDRW2100011121.1.40",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210824 01:20"
        },
        {
          "id": "MDRW2100011121.1.41",
          "form": "심지어 이제 오디오북까지 묶으려 하잖아요",
          "original_form": "심지어 이제 오디오북까지 묶으려 하잖아요",
          "speaker_id": "1",
          "time": "20210824 01:20"
        },
        {
          "id": "MDRW2100011121.1.42",
          "form": "엥?",
          "original_form": "엥?",
          "speaker_id": "2",
          "time": "20210824 01:20"
        },
        {
          "id": "MDRW2100011121.1.43",
          "form": "오디오북을 서점에서 팔지도 못하는데",
          "original_form": "오디오북을 서점에서 팔지도 못하는데",
          "speaker_id": "1",
          "time": "20210824 01:20"
        },
        {
          "id": "MDRW2100011121.1.44",
          "form": "지역서점가면 누가 오디오북해주나요?",
          "original_form": "지역서점가면 누가 오디오북해주나요?",
          "speaker_id": "2",
          "time": "20210824 01:20"
        },
        {
          "id": "MDRW2100011121.1.45",
          "form": "그니까요!!!",
          "original_form": "그니까요!!!",
          "speaker_id": "1",
          "time": "20210824 01:20"
        },
        {
          "id": "MDRW2100011121.1.46",
          "form": "근데 출판사에서 일하는 친구들은 도정제가",
          "original_form": "근데 출판사에서 일하는 친구들은 도정제가",
          "speaker_id": "1",
          "time": "20210824 01:20"
        },
        {
          "id": "MDRW2100011121.1.47",
          "form": "출판업계 그나마 유지시켜주는 제도라해서..",
          "original_form": "출판업계 그나마 유지시켜주는 제도라해서..",
          "speaker_id": "1",
          "time": "20210824 01:20"
        },
        {
          "id": "MDRW2100011121.1.48",
          "form": "어이없네 애시당초 오디오북 만든 취지가 그거아닌가요? 시각장애인들 위한 점자책 같은 의미로... 씁",
          "original_form": "어이없네 애시당초 오디오북 만든 취지가 그거아닌가요? 시각장애인들 위한 점자책 같은 의미로... 씁",
          "speaker_id": "2",
          "time": "20210824 01:21"
        },
        {
          "id": "MDRW2100011121.1.49",
          "form": "아....?",
          "original_form": "아....?",
          "speaker_id": "2",
          "time": "20210824 01:21"
        },
        {
          "id": "MDRW2100011121.1.50",
          "form": "뭐라 크게 욕은 못하겠어요",
          "original_form": "뭐라 크게 욕은 못하겠어요",
          "speaker_id": "1",
          "time": "20210824 01:21"
        },
        {
          "id": "MDRW2100011121.1.51",
          "form": "아.....",
          "original_form": "아.....",
          "speaker_id": "2",
          "time": "20210824 01:21"
        },
        {
          "id": "MDRW2100011121.1.52",
          "form": "근데 도정제로 돈 번거는",
          "original_form": "근데 도정제로 돈 번거는",
          "speaker_id": "1",
          "time": "20210824 01:21"
        },
        {
          "id": "MDRW2100011121.1.53",
          "form": "아놔 진짜",
          "original_form": "아놔 진짜",
          "speaker_id": "2",
          "time": "20210824 01:21"
        },
        {
          "id": "MDRW2100011121.1.54",
          "form": "학습지쪽 아닌가요",
          "original_form": "학습지쪽 아닌가요",
          "speaker_id": "1",
          "time": "20210824 01:21"
        },
        {
          "id": "MDRW2100011121.1.55",
          "form": "그 문제집 만드는 회사만 돈벌었죠 뭐",
          "original_form": "그 문제집 만드는 회사만 돈벌었죠 뭐",
          "speaker_id": "1",
          "time": "20210824 01:21"
        },
        {
          "id": "MDRW2100011121.1.56",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210824 01:21"
        },
        {
          "id": "MDRW2100011121.1.57",
          "form": "맞아요....",
          "original_form": "맞아요....",
          "speaker_id": "2",
          "time": "20210824 01:21"
        },
        {
          "id": "MDRW2100011121.1.58",
          "form": "문제집 지이이인짜 비싸요",
          "original_form": "문제집 지이이인짜 비싸요",
          "speaker_id": "1",
          "time": "20210824 01:21"
        },
        {
          "id": "MDRW2100011121.1.59",
          "form": "저 문제집에만 지금 30만원 쓴거같은데",
          "original_form": "저 문제집에만 지금 30만원 쓴거같은데",
          "speaker_id": "2",
          "time": "20210824 01:21"
        },
