      "utterance": [
        {
          "id": "MDRW2100025059.1.1",
          "form": "훠궈면.. 순한맛에 빠지신건 아니네여....",
          "original_form": "훠궈면.. 순한맛에 빠지신건 아니네여....",
          "speaker_id": "1",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100025059.1.2",
          "form": "ㅋㅋㄱㅋㅋname1님은 뭐많이드세용?",
          "original_form": "ㅋㅋㄱㅋㅋ&name1&님은 뭐많이드세용?",
          "speaker_id": "2",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100025059.1.3",
          "form": "전 국물파",
          "original_form": "전 국물파",
          "speaker_id": "2",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100025059.1.4",
          "form": "저는....",
          "original_form": "저는....",
          "speaker_id": "1",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100025059.1.5",
          "form": "그냥 제 입맛에 맛있는 건 다 잘먹어서....",
          "original_form": "그냥 제 입맛에 맛있는 건 다 잘먹어서....",
          "speaker_id": "1",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100025059.1.6",
          "form": "요새는",
          "original_form": "요새는",
          "speaker_id": "1",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100025059.1.7",
          "form": "국밥이 그렇게 좋더라구요",
          "original_form": "국밥이 그렇게 좋더라구요",
          "speaker_id": "1",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100025059.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100025059.1.9",
          "form": "아 혹시 강남쪽오실일있으면",
          "original_form": "아 혹시 강남쪽오실일있으면",
          "speaker_id": "2",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100025059.1.10",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100025059.1.11",
          "form": "선릉쪽에 농민백암순대 가보세요",
          "original_form": "선릉쪽에 농민백암순대 가보세요",
          "speaker_id": "2",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100025059.1.12",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100025059.1.13",
          "form": "jmt",
          "original_form": "jmt",
          "speaker_id": "2",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100025059.1.14",
          "form": "순대만 들어잇나요?",
          "original_form": "순대만 들어잇나요?",
          "speaker_id": "1",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100025059.1.15",
          "form": "요청하면그렇게 주고 일반적ㅇ르ㅗ는",
          "original_form": "요청하면그렇게 주고 일반적ㅇ르ㅗ는",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100025059.1.16",
          "form": "고기도들어있어요",
          "original_form": "고기도들어있어요",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100025059.1.17",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100025059.1.18",
          "form": "막",
          "original_form": "막",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100025059.1.19",
          "form": "귀랑 내장 이런 건 없어요?",
          "original_form": "귀랑 내장 이런 건 없어요?",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100025059.1.20",
          "form": "내장은 아니고 그냥일반 머릿고기 들어있긴한데",
          "original_form": "내장은 아니고 그냥일반 머릿고기 들어있긴한데",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100025059.1.21",
          "form": "그런 부속부위를 별로 안 좋아해서ㅠㅠ",
          "original_form": "그런 부속부위를 별로 안 좋아해서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100025059.1.22",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100025059.1.23",
          "form": "귀는없는거같아요",
          "original_form": "귀는없는거같아요",
          "speaker_id": "2",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100025059.1.24",
          "form": "그거면 아주 좋아여!",
          "original_form": "그거면 아주 좋아여!",
          "speaker_id": "1",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100025059.1.25",
          "form": "근데깔끔하고 맛있어요",
          "original_form": "근데깔끔하고 맛있어요",
          "speaker_id": "2",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100025059.1.26",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100025059.1.27",
          "form": "군침이 싹도네여",
          "original_form": "군침이 싹도네여",
          "speaker_id": "1",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100025059.1.28",
          "form": "그리고 여긴 머실거같지만",
          "original_form": "그리고 여긴 머실거같지만",
          "speaker_id": "2",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100025059.1.29",
          "form": "홍대근처오실일이있다면",
          "original_form": "홍대근처오실일이있다면",
          "speaker_id": "2",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100025059.1.30",
          "form": "옥동식이라고",
          "original_form": "옥동식이라고",
          "speaker_id": "2",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100025059.1.31",
          "form": "돼지국밥집",
          "original_form": "돼지국밥집",
          "speaker_id": "2",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100025059.1.32",
          "form": "정말맛있어요",
          "original_form": "정말맛있어요",
          "speaker_id": "2",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100025059.1.33",
          "form": "사람이름같아여",
          "original_form": "사람이름같아여",
