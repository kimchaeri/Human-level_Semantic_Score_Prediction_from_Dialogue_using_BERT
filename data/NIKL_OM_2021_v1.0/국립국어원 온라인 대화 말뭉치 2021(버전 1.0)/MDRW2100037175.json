      "utterance": [
        {
          "id": "MDRW2100037175.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100037175.1.2",
          "form": "안녕하세요~ 점심드셧나요?",
          "original_form": "안녕하세요~ 점심드셧나요?",
          "speaker_id": "2",
          "time": "20210901 14:30"
        },
        {
          "id": "MDRW2100037175.1.3",
          "form": "네 name2님님도 드셨나요?",
          "original_form": "네 &name2&님님도 드셨나요?",
          "speaker_id": "1",
          "time": "20210901 14:49"
        },
        {
          "id": "MDRW2100037175.1.4",
          "form": "네~ 참치찌개 끓여먹었어요 !",
          "original_form": "네~ 참치찌개 끓여먹었어요 !",
          "speaker_id": "2",
          "time": "20210901 15:07"
        },
        {
          "id": "MDRW2100037175.1.5",
          "form": "직접 만드신건가요?",
          "original_form": "직접 만드신건가요?",
          "speaker_id": "1",
          "time": "20210901 15:22"
        },
        {
          "id": "MDRW2100037175.1.6",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210901 15:22"
        },
        {
          "id": "MDRW2100037175.1.7",
          "form": "우와 레시피는 어디서 참고하신건가요?",
          "original_form": "우와 레시피는 어디서 참고하신건가요?",
          "speaker_id": "1",
          "time": "20210901 15:23"
        },
        {
          "id": "MDRW2100037175.1.8",
          "form": "그냥...김치볶고 참치캔넣고 시판사골국물 물",
          "original_form": "그냥...김치볶고 참치캔넣고 시판사골국물 물",
          "speaker_id": "2",
          "time": "20210901 15:24"
        },
        {
          "id": "MDRW2100037175.1.9",
          "form": "대충끓여요 ㅎㅎ",
          "original_form": "대충끓여요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 15:28"
        },
        {
          "id": "MDRW2100037175.1.10",
          "form": "요리를 잘하시나봐요",
          "original_form": "요리를 잘하시나봐요",
          "speaker_id": "1",
          "time": "20210901 15:36"
        },
        {
          "id": "MDRW2100037175.1.11",
          "form": "아니에용 ㅠㅠㅋㅋㅋ 그냥 제입에 먹을정도로만",
          "original_form": "아니에용 ㅠㅠㅋㅋㅋ 그냥 제입에 먹을정도로만",
          "speaker_id": "2",
          "time": "20210901 15:38"
        },
        {
          "id": "MDRW2100037175.1.12",
          "form": "국만 몇가지하고",
          "original_form": "국만 몇가지하고",
          "speaker_id": "2",
          "time": "20210901 15:38"
        },
        {
          "id": "MDRW2100037175.1.13",
          "form": "잘안해용 딴건 ㅋㅋㅋ 연두와 육수팩에 힘이죠!",
          "original_form": "잘안해용 딴건 ㅋㅋㅋ 연두와 육수팩에 힘이죠!",
          "speaker_id": "2",
          "time": "20210901 15:39"
        },
        {
          "id": "MDRW2100037175.1.14",
          "form": "그래도 잘하시는 요리가 있으신가요?",
          "original_form": "그래도 잘하시는 요리가 있으신가요?",
          "speaker_id": "1",
          "time": "20210901 15:48"
        },
        {
          "id": "MDRW2100037175.1.15",
          "form": "뭐 뽑낼 요리는 없네요ㅠㅠ",
          "original_form": "뭐 뽑낼 요리는 없네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 15:52"
        },
        {
          "id": "MDRW2100037175.1.16",
          "form": "그냥 국이나 찌개만 있으면 먹어서 ㅋㅋ",
          "original_form": "그냥 국이나 찌개만 있으면 먹어서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:52"
        },
        {
          "id": "MDRW2100037175.1.17",
          "form": "그렇군요 그래도 요리잘하시는건 정말 부럽네요",
          "original_form": "그렇군요 그래도 요리잘하시는건 정말 부럽네요",
          "speaker_id": "1",
