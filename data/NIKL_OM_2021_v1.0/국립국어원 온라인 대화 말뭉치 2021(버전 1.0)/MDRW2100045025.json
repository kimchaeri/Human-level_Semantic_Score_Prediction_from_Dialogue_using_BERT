      "utterance": [
        {
          "id": "MDRW2100045025.1.1",
          "form": "수박도 몇번 못먹고 올해는 ㅠㅠ뭔가 그렇네여ㅠㅠ",
          "original_form": "수박도 몇번 못먹고 올해는 ㅠㅠ뭔가 그렇네여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045025.1.2",
          "form": "아 저는 체리 산거 다 실패했어요",
          "original_form": "아 저는 체리 산거 다 실패했어요",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045025.1.3",
          "form": "맛이 읎었어요",
          "original_form": "맛이 읎었어요",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045025.1.4",
          "form": "맛이없었나여ㅠㅠ",
          "original_form": "맛이없었나여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045025.1.5",
          "form": "네 비싸게 주고 산것도 그냥 그랬어요",
          "original_form": "네 비싸게 주고 산것도 그냥 그랬어요",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045025.1.6",
          "form": "ㅠㅠ 속상하네여ㅠㅠ",
          "original_form": "ㅠㅠ 속상하네여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045025.1.7",
          "form": "레이니어 체린가 그런것도 시켜봤거든요",
          "original_form": "레이니어 체린가 그런것도 시켜봤거든요",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045025.1.8",
          "form": "근데 실패였어용",
          "original_form": "근데 실패였어용",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045025.1.9",
          "form": "제가 체리를 안좋아해서ㅠㅠㅠ",
          "original_form": "제가 체리를 안좋아해서ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045025.1.10",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045025.1.11",
          "form": "저 복숭아도 좋아해요!!",
          "original_form": "저 복숭아도 좋아해요!!",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045025.1.12",
          "form": "복숭아 완전 좋아하져 \n딱입니까 물입니까",
          "original_form": "복숭아 완전 좋아하져 \n딱입니까 물입니까",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045025.1.13",
          "form": "저는 물이여!!",
          "original_form": "저는 물이여!!",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045025.1.14",
          "form": "앗!! 전딱!",
          "original_form": "앗!! 전딱!",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045025.1.15",
          "form": "아삭아삭하니",
          "original_form": "아삭아삭하니",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045025.1.16",
          "form": "근데 애들은 물을 좋아하더라구요",
          "original_form": "근데 애들은 물을 좋아하더라구요",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045025.1.17",
          "form": "ㅎㅎ 딱도 좋아해요",
          "original_form": "ㅎㅎ 딱도 좋아해요",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045025.1.18",
          "form": "복숭아도 그렇고 비싸서원 ㅠㅠㅠ",
          "original_form": "복숭아도 그렇고 비싸서원 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045025.1.19",
          "form": "좀만 더 내리면 와구와구 살라구요",
          "original_form": "좀만 더 내리면 와구와구 살라구요",
          "speaker_id": "1",
          "time": "20210908 00:42"
        }
      ]
