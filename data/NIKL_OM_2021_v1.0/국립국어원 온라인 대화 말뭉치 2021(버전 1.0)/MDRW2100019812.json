      "utterance": [
        {
          "id": "MDRW2100019812.1.1",
          "form": "작게나마 환상이 있었고 그 환상은 현대사회 미디어가 만들어낸",
          "original_form": "작게나마 환상이 있었고 그 환상은 현대사회 미디어가 만들어낸",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019812.1.2",
          "form": "아 미디어!!",
          "original_form": "아 미디어!!",
          "speaker_id": "2",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019812.1.3",
          "form": "일정한 형식같다고 느껴지더라구요",
          "original_form": "일정한 형식같다고 느껴지더라구요",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019812.1.4",
          "form": "막 연인되면 말씀하셨듯이",
          "original_form": "막 연인되면 말씀하셨듯이",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019812.1.5",
          "form": "24시간 연락해야하고",
          "original_form": "24시간 연락해야하고",
          "speaker_id": "1",
          "time": "20210827 23:06"
        },
        {
          "id": "MDRW2100019812.1.6",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019812.1.7",
          "form": "안 되면 화낼 자격이 생기고",
          "original_form": "안 되면 화낼 자격이 생기고",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019812.1.8",
          "form": "내 어떤 욕구들을 채워줘야하고",
          "original_form": "내 어떤 욕구들을 채워줘야하고",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019812.1.9",
          "form": "그러는디",
          "original_form": "그러는디",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019812.1.10",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019812.1.11",
          "form": "외로움같은것들..",
          "original_form": "외로움같은것들..",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019812.1.12",
          "form": "근데 그게 우정이란 관계였을 땐",
          "original_form": "근데 그게 우정이란 관계였을 땐",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019812.1.13",
          "form": "그런 기대가 발생하지 않잖아유",
          "original_form": "그런 기대가 발생하지 않잖아유",
          "speaker_id": "1",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019812.1.14",
          "form": "오 그렇네요",
          "original_form": "오 그렇네요",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019812.1.15",
          "form": "강요하지 않고",
          "original_form": "강요하지 않고",
          "speaker_id": "2",
          "time": "20210827 23:07"
        },
        {
          "id": "MDRW2100019812.1.16",
          "form": "기대를 알아서 적절히 조절하게 되네요",
          "original_form": "기대를 알아서 적절히 조절하게 되네요",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019812.1.17",
          "form": "근데 사귀자는 말로 갑자기 뿅 하고 권리가 생긴다이 되게 이상하다(곰곰)",
          "original_form": "근데 사귀자는 말로 갑자기 뿅 하고 권리가 생긴다이 되게 이상하다(곰곰)",
          "speaker_id": "1",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019812.1.18",
          "form": "싶었어요ㅋㅋㅋㅋ",
          "original_form": "싶었어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019812.1.19",
          "form": "근데 그게 나자신에게만 그치는 게 아니라",
          "original_form": "근데 그게 나자신에게만 그치는 게 아니라",
          "speaker_id": "1",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019812.1.20",
          "form": "상대가 로맨스의 형식에 맞추지 않으면",
          "original_form": "상대가 로맨스의 형식에 맞추지 않으면",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019812.1.21",
          "form": "화가 나고 서운하고 그러게 되더라구요",
          "original_form": "화가 나고 서운하고 그러게 되더라구요",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019812.1.22",
          "form": "만들어진 환상을 잣대로 세워서",
          "original_form": "만들어진 환상을 잣대로 세워서",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019812.1.23",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019812.1.24",
          "form": "제 기대를 충족시켜주지않으면 화가 ㄴ요",
          "original_form": "제 기대를 충족시켜주지않으면 화가 ㄴ요",
          "speaker_id": "2",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019812.1.25",
          "form": "상대방을 재단하게 되는..",
          "original_form": "상대방을 재단하게 되는..",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019812.1.26",
          "form": "맞어요",
          "original_form": "맞어요",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019812.1.27",
          "form": "그 모든 게 현타가 와서 토론을 했고 미디어에서 나오는 사랑 형식을",
          "original_form": "그 모든 게 현타가 와서 토론을 했고 미디어에서 나오는 사랑 형식을",
          "speaker_id": "1",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019812.1.28",
          "form": "깨게 되었습니다",
          "original_form": "깨게 되었습니다",
          "speaker_id": "1",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019812.1.29",
          "form": "상대방을 있는 그대로 받아들이기가 어려운거같아요",
          "original_form": "상대방을 있는 그대로 받아들이기가 어려운거같아요",
          "speaker_id": "2",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019812.1.30",
          "form": "와 두분이 그래도 마음이 잘 맞으셨네요 토론 결과대로 지내깈ㅋㅋㅋ",
          "original_form": "와 두분이 그래도 마음이 잘 맞으셨네요 토론 결과대로 지내깈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:10"
        }
      ]
