      "utterance": [
        {
          "id": "MDRW2100040845.1.1",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210905 09:44"
        },
        {
          "id": "MDRW2100040845.1.2",
          "form": "네 여행 좋아해요",
          "original_form": "네 여행 좋아해요",
          "speaker_id": "1",
          "time": "20210905 09:45"
        },
        {
          "id": "MDRW2100040845.1.3",
          "form": "요즘은 통 못 다니고 있지만ㅠ",
          "original_form": "요즘은 통 못 다니고 있지만ㅠ",
          "speaker_id": "1",
          "time": "20210905 09:45"
        },
        {
          "id": "MDRW2100040845.1.4",
          "form": "여행 주로 혼자 하세요 같이 다니세요?",
          "original_form": "여행 주로 혼자 하세요 같이 다니세요?",
          "speaker_id": "2",
          "time": "20210905 09:45"
        },
        {
          "id": "MDRW2100040845.1.5",
          "form": "혼자 여행은 거의 안다닌 것 같아요",
          "original_form": "혼자 여행은 거의 안다닌 것 같아요",
          "speaker_id": "1",
          "time": "20210905 09:46"
        },
        {
          "id": "MDRW2100040845.1.6",
          "form": "같이 가서 잠깐씩 혼자 산책한 적은 있어도",
          "original_form": "같이 가서 잠깐씩 혼자 산책한 적은 있어도",
          "speaker_id": "1",
          "time": "20210905 09:47"
        },
        {
          "id": "MDRW2100040845.1.7",
          "form": "저도 거의 혼자 다녔어요",
          "original_form": "저도 거의 혼자 다녔어요",
          "speaker_id": "2",
          "time": "20210905 09:49"
        },
        {
          "id": "MDRW2100040845.1.8",
          "form": "아 혼자 여행 다니는 걸 좋아하시는군요",
          "original_form": "아 혼자 여행 다니는 걸 좋아하시는군요",
          "speaker_id": "1",
          "time": "20210905 09:51"
        },
        {
          "id": "MDRW2100040845.1.9",
          "form": "주로 어디 여행 하는 걸 좋아하세요?",
          "original_form": "주로 어디 여행 하는 걸 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 09:51"
        },
        {
          "id": "MDRW2100040845.1.10",
          "form": "저는 그냥 에어비앤비로 돌아다니는 거 좋아해요",
          "original_form": "저는 그냥 에어비앤비로 돌아다니는 거 좋아해요",
          "speaker_id": "2",
          "time": "20210905 09:52"
        },
        {
          "id": "MDRW2100040845.1.11",
          "form": "아 저도 해외갈 때는 에어비앤비 애용해요",
          "original_form": "아 저도 해외갈 때는 에어비앤비 애용해요",
          "speaker_id": "1",
          "time": "20210905 09:53"
        },
        {
          "id": "MDRW2100040845.1.12",
          "form": "주로 가족들이랑 가서 집 통째로 빌려서 지내는데",
          "original_form": "주로 가족들이랑 가서 집 통째로 빌려서 지내는데",
          "speaker_id": "1",
          "time": "20210905 09:53"
        },
        {
          "id": "MDRW2100040845.1.13",
          "form": "국내에서는 안가 보셨어요?",
          "original_form": "국내에서는 안가 보셨어요?",
          "speaker_id": "2",
          "time": "20210905 09:53"
        },
        {
          "id": "MDRW2100040845.1.14",
          "form": "국내 에어비앤비는 아직이요",
          "original_form": "국내 에어비앤비는 아직이요",
          "speaker_id": "1",
          "time": "20210905 09:53"
        },
        {
          "id": "MDRW2100040845.1.15",
          "form": "국내도 좋은가요?",
          "original_form": "국내도 좋은가요?",
          "speaker_id": "1",
          "time": "20210905 09:53"
        },
        {
          "id": "MDRW2100040845.1.16",
          "form": "네 잘 찾으면 괜찮아요",
          "original_form": "네 잘 찾으면 괜찮아요",
          "speaker_id": "2",
          "time": "20210905 09:54"
        },
        {
          "id": "MDRW2100040845.1.17",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210905 10:00"
        },
        {
          "id": "MDRW2100040845.1.18",
          "form": "다음에 국내여행할 때 알아봐야겠어요",
