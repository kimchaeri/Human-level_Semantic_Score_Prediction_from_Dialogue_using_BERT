        },
        {
          "id": "MDRW2100020435.1.2",
          "form": "흑임자라떼?",
          "original_form": "흑임자라떼?",
          "speaker_id": "1",
          "time": "20210828 05:35"
        },
        {
          "id": "MDRW2100020435.1.3",
          "form": "먹었거든요",
          "original_form": "먹었거든요",
          "speaker_id": "1",
          "time": "20210828 05:35"
        },
        {
          "id": "MDRW2100020435.1.4",
          "form": "흑임자라떼",
          "original_form": "흑임자라떼",
          "speaker_id": "1",
          "time": "20210828 05:36"
        },
        {
          "id": "MDRW2100020435.1.5",
          "form": "맛있어요?",
          "original_form": "맛있어요?",
          "speaker_id": "2",
          "time": "20210828 05:36"
        },
        {
          "id": "MDRW2100020435.1.6",
          "form": "이거 카페인이",
          "original_form": "이거 카페인이",
          "speaker_id": "1",
          "time": "20210828 05:36"
        },
        {
          "id": "MDRW2100020435.1.7",
          "form": "흑임자폼이 진짜 기가막히다던데",
          "original_form": "흑임자폼이 진짜 기가막히다던데",
          "speaker_id": "2",
          "time": "20210828 05:36"
        },
        {
          "id": "MDRW2100020435.1.8",
          "form": "그 스누피 커피우유 아세용?",
          "original_form": "그 스누피 커피우유 아세용?",
          "speaker_id": "1",
          "time": "20210828 05:36"
        },
        {
          "id": "MDRW2100020435.1.9",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210828 05:36"
        },
        {
          "id": "MDRW2100020435.1.10",
          "form": "그거보다 더 높아서ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그거보다 더 높아서ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 05:36"
        },
        {
          "id": "MDRW2100020435.1.11",
          "form": "하루종일 잠 못잤어요",
          "original_form": "하루종일 잠 못잤어요",
          "speaker_id": "1",
          "time": "20210828 05:36"
        },
        {
          "id": "MDRW2100020435.1.12",
          "form": "앜 진짜요",
          "original_form": "앜 진짜요",
          "speaker_id": "2",
          "time": "20210828 05:36"
        },
        {
          "id": "MDRW2100020435.1.13",
          "form": "흑임자폼은 진짜 맛있었어요!",
          "original_form": "흑임자폼은 진짜 맛있었어요!",
          "speaker_id": "1",
          "time": "20210828 05:36"
        },
        {
          "id": "MDRW2100020435.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ커피우유 먹고 진짜 너무 고생햐서",
          "original_form": "ㅋㅋㅋㅋㅋㅋ커피우유 먹고 진짜 너무 고생햐서",
          "speaker_id": "2",
          "time": "20210828 05:36"
        },
        {
          "id": "MDRW2100020435.1.15",
          "form": "다시는 쳐다도 못봐요",
          "original_form": "다시는 쳐다도 못봐요",
          "speaker_id": "2",
          "time": "20210828 05:36"
        },
        {
          "id": "MDRW2100020435.1.16",
          "form": "네네ㅠㅠ 피곤한데 잠 안오면 돌아버려요ㅋㅋㅋㅋ",
          "original_form": "네네ㅠㅠ 피곤한데 잠 안오면 돌아버려요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 05:36"
        },
        {
          "id": "MDRW2100020435.1.17",
          "form": "이틀을 내리 못잤던 기억이",
          "original_form": "이틀을 내리 못잤던 기억이",
          "speaker_id": "2",
          "time": "20210828 05:36"
        },
        {
          "id": "MDRW2100020435.1.18",
          "form": "아휴ㅠㅠ 카페인에 민감하신가봐요",
          "original_form": "아휴ㅠㅠ 카페인에 민감하신가봐요",
