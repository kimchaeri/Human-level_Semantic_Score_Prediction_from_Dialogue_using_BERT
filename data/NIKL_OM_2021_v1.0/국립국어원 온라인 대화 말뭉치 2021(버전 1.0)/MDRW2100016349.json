      "utterance": [
        {
          "id": "MDRW2100016349.1.1",
          "form": "좋아하는 연예인 있으세요?",
          "original_form": "좋아하는 연예인 있으세요?",
          "speaker_id": "1",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016349.1.2",
          "form": "혹시 데이식스라고 아세요?",
          "original_form": "혹시 데이식스라고 아세요?",
          "speaker_id": "2",
          "time": "20210826 12:11"
        },
        {
          "id": "MDRW2100016349.1.3",
          "form": "예뻤어 부른 그룹인데 들어보셨나용...?",
          "original_form": "예뻤어 부른 그룹인데 들어보셨나용...?",
          "speaker_id": "2",
          "time": "20210826 12:11"
        },
        {
          "id": "MDRW2100016349.1.4",
          "form": "데이식스 알죠!",
          "original_form": "데이식스 알죠!",
          "speaker_id": "1",
          "time": "20210826 12:11"
        },
        {
          "id": "MDRW2100016349.1.5",
          "form": "밴드아닌가요?",
          "original_form": "밴드아닌가요?",
          "speaker_id": "1",
          "time": "20210826 12:11"
        },
        {
          "id": "MDRW2100016349.1.6",
          "form": "오 아시네요 맞아요 밴드에요",
          "original_form": "오 아시네요 맞아요 밴드에요",
          "speaker_id": "2",
          "time": "20210826 12:12"
        },
        {
          "id": "MDRW2100016349.1.7",
          "form": "좋은 노래가 엄청 많아요",
          "original_form": "좋은 노래가 엄청 많아요",
          "speaker_id": "2",
          "time": "20210826 12:12"
        },
        {
          "id": "MDRW2100016349.1.8",
          "form": "오 노래는 안 들어봤는데 참고해야겠어요",
          "original_form": "오 노래는 안 들어봤는데 참고해야겠어요",
          "speaker_id": "1",
          "time": "20210826 12:12"
        },
        {
          "id": "MDRW2100016349.1.9",
          "form": "요즘 즐겨 보는 TV 프로그램 있으세요",
          "original_form": "요즘 즐겨 보는 TV 프로그램 있으세요",
          "speaker_id": "2",
          "time": "20210826 12:12"
        },
        {
          "id": "MDRW2100016349.1.10",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210826 12:13"
        },
        {
          "id": "MDRW2100016349.1.11",
          "form": "오케이광자매라고 주말드라마 아세요?",
          "original_form": "오케이광자매라고 주말드라마 아세요?",
          "speaker_id": "1",
          "time": "20210826 12:13"
        },
        {
          "id": "MDRW2100016349.1.12",
          "form": "이 프로그램 이게 엄청 많더라구요!",
          "original_form": "이 프로그램 이게 엄청 많더라구요!",
          "speaker_id": "2",
          "time": "20210826 12:13"
        },
        {
          "id": "MDRW2100016349.1.13",
          "form": "앗 인기요ㅠㅠㅋㅋ",
          "original_form": "앗 인기요ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:13"
        },
        {
          "id": "MDRW2100016349.1.14",
          "form": "재밌어요 ㅋㅋㅋ",
          "original_form": "재밌어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:13"
        },
        {
          "id": "MDRW2100016349.1.15",
          "form": "코로나라고 마스크 쓰고 나와요",
          "original_form": "코로나라고 마스크 쓰고 나와요",
          "speaker_id": "1",
          "time": "20210826 12:13"
        },
        {
          "id": "MDRW2100016349.1.16",
          "form": "우와 현대 상황을 잘 반영 했네요 보통 마스크 벗고 나오던데ㅋㅋㅋ",
          "original_form": "우와 현대 상황을 잘 반영 했네요 보통 마스크 벗고 나오던데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:14"
        },
        {
          "id": "MDRW2100016349.1.17",
          "form": "그쵸 신기해요",
          "original_form": "그쵸 신기해요",
          "speaker_id": "1",
          "time": "20210826 12:14"
        },
        {
          "id": "MDRW2100016349.1.18",
          "form": "저는 대탈출 봐요 방탈출 하는 프로그램인데 너무 재밌어요",
          "original_form": "저는 대탈출 봐요 방탈출 하는 프로그램인데 너무 재밌어요",
          "speaker_id": "2",
          "time": "20210826 12:15"
        },
        {
          "id": "MDRW2100016349.1.19",
          "form": "오 그런것도있군요",
          "original_form": "오 그런것도있군요",
          "speaker_id": "1",
          "time": "20210826 12:15"
        }
      ]
