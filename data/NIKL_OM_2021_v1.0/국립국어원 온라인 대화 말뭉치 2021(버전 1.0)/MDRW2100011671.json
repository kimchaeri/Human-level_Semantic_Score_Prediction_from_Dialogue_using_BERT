      "utterance": [
        {
          "id": "MDRW2100011671.1.1",
          "form": "안녕하세요 쇼핑에 관심 있으신가요",
          "original_form": "안녕하세요 쇼핑에 관심 있으신가요",
          "speaker_id": "1",
          "time": "20210824 14:59"
        },
        {
          "id": "MDRW2100011671.1.2",
          "form": "많아요 이젠 가을옷 구경하죠",
          "original_form": "많아요 이젠 가을옷 구경하죠",
          "speaker_id": "2",
          "time": "20210824 15:00"
        },
        {
          "id": "MDRW2100011671.1.3",
          "form": "그쵸 요새 저는 트렌치코트를 사고싶어요",
          "original_form": "그쵸 요새 저는 트렌치코트를 사고싶어요",
          "speaker_id": "1",
          "time": "20210824 15:01"
        },
        {
          "id": "MDRW2100011671.1.4",
          "form": "매번 입을옷이 없을까요",
          "original_form": "매번 입을옷이 없을까요",
          "speaker_id": "2",
          "time": "20210824 15:02"
        },
        {
          "id": "MDRW2100011671.1.5",
          "form": "맞아요 싼건 싸서 금방 못입고 비싼건 비싸서 많이 못사고",
          "original_form": "맞아요 싼건 싸서 금방 못입고 비싼건 비싸서 많이 못사고",
          "speaker_id": "1",
          "time": "20210824 15:03"
        },
        {
          "id": "MDRW2100011671.1.6",
          "form": "비싼건 또 팔때 아깝고",
          "original_form": "비싼건 또 팔때 아깝고",
          "speaker_id": "2",
          "time": "20210824 15:04"
        },
        {
          "id": "MDRW2100011671.1.7",
          "form": "코트만 비싼거사고 티는 싼거 한해 입는게 최고같아요",
          "original_form": "코트만 비싼거사고 티는 싼거 한해 입는게 최고같아요",
          "speaker_id": "2",
          "time": "20210824 15:05"
        },
        {
          "id": "MDRW2100011671.1.8",
          "form": "그러게요 큰 맘먹고 비싼티 사도 생각보다ㅡ오래 입진 못하더라고요",
          "original_form": "그러게요 큰 맘먹고 비싼티 사도 생각보다ㅡ오래 입진 못하더라고요",
          "speaker_id": "1",
          "time": "20210824 15:05"
        },
        {
          "id": "MDRW2100011671.1.9",
          "form": "기본으로 사야하나봐요",
          "original_form": "기본으로 사야하나봐요",
          "speaker_id": "2",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100011671.1.10",
          "form": "당근도 자주보시나요?",
          "original_form": "당근도 자주보시나요?",
          "speaker_id": "2",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100011671.1.11",
          "form": "당근에서 옷을 사본 적은 없고 중고물품 판매만 해봤어요",
          "original_form": "당근에서 옷을 사본 적은 없고 중고물품 판매만 해봤어요",
          "speaker_id": "1",
          "time": "20210824 15:10"
        },
        {
          "id": "MDRW2100011671.1.12",
          "form": "네 저도 구경하는데 재밌더라고요",
          "original_form": "네 저도 구경하는데 재밌더라고요",
          "speaker_id": "2",
          "time": "20210824 15:10"
        },
        {
          "id": "MDRW2100011671.1.13",
          "form": "중고거래 하면서 재미있는 일화도 많더라고요",
          "original_form": "중고거래 하면서 재미있는 일화도 많더라고요",
          "speaker_id": "1",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011671.1.14",
          "form": "대부분 남편이 나가고요 ㅎ",
          "original_form": "대부분 남편이 나가고요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 15:12"
        },
        {
          "id": "MDRW2100011671.1.15",
          "form": "당근 거래에서 의자 구매자가 앉아봐도 되겠냐며 길에서 의자에 5분이상 앉아있어서 뻘쭘했다더라고요",
          "original_form": "당근 거래에서 의자 구매자가 앉아봐도 되겠냐며 길에서 의자에 5분이상 앉아있어서 뻘쭘했다더라고요",
          "speaker_id": "1",
          "time": "20210824 15:12"
        },
        {
          "id": "MDRW2100011671.1.16",
          "form": "ㅋㅋㅋ대박이네요",
          "original_form": "ㅋㅋㅋ대박이네요",
