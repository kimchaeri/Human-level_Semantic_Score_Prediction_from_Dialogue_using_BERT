      "utterance": [
        {
          "id": "MDRW2100041931.1.1",
          "form": "그나저나 affiliation 도비들도",
          "original_form": "그나저나 &affiliation& 도비들도",
          "speaker_id": "1",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100041931.1.2",
          "form": "힘든 삶을 살고 계시는군요.....",
          "original_form": "힘든 삶을 살고 계시는군요.....",
          "speaker_id": "1",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100041931.1.3",
          "form": "네 교수님들이 하실 국고사업을 세상에",
          "original_form": "네 교수님들이 하실 국고사업을 세상에",
          "speaker_id": "2",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100041931.1.4",
          "form": "도비분들이 하시더라고요",
          "original_form": "도비분들이 하시더라고요",
          "speaker_id": "2",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100041931.1.5",
          "form": "차라리 전 돈이라도받지,,",
          "original_form": "차라리 전 돈이라도받지,,",
          "speaker_id": "2",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100041931.1.6",
          "form": "와 그러면",
          "original_form": "와 그러면",
          "speaker_id": "1",
          "time": "20210906 17:45"
        },
        {
          "id": "MDRW2100041931.1.7",
          "form": "재주는 곰이 부리고",
          "original_form": "재주는 곰이 부리고",
          "speaker_id": "1",
          "time": "20210906 17:45"
        },
        {
          "id": "MDRW2100041931.1.8",
          "form": "돈은 되놈이 받는 그 상황이네요..!",
          "original_form": "돈은 되놈이 받는 그 상황이네요..!",
          "speaker_id": "1",
          "time": "20210906 17:45"
        },
        {
          "id": "MDRW2100041931.1.9",
          "form": "그쵸!!!",
          "original_form": "그쵸!!!",
          "speaker_id": "2",
          "time": "20210906 17:45"
        },
        {
          "id": "MDRW2100041931.1.10",
          "form": "우리는 이름도안남는 사업,,.",
          "original_form": "우리는 이름도안남는 사업,,.",
          "speaker_id": "2",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100041931.1.11",
          "form": "교수님들 이름만 남는사업,.",
          "original_form": "교수님들 이름만 남는사업,.",
          "speaker_id": "2",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100041931.1.12",
          "form": "세상에.....",
          "original_form": "세상에.....",
          "speaker_id": "1",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100041931.1.13",
          "form": "정말 제가 다 서럽네요....",
          "original_form": "정말 제가 다 서럽네요....",
          "speaker_id": "1",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100041931.1.14",
          "form": "그러면 보통... 교직원은",
          "original_form": "그러면 보통... 교직원은",
          "speaker_id": "1",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100041931.1.15",
          "form": "어떤 기준으로 채용되나요...?!?!?",
          "original_form": "어떤 기준으로 채용되나요...?!?!?",
          "speaker_id": "1",
          "time": "20210906 17:48"
        },
        {
          "id": "MDRW2100041931.1.16",
          "form": "자기학교사람 뽑으려고하는것도있고",
          "original_form": "자기학교사람 뽑으려고하는것도있고",
          "speaker_id": "2",
          "time": "20210906 17:48"
        },
        {
          "id": "MDRW2100041931.1.17",
          "form": "사람인같은데에 올라와요!",
          "original_form": "사람인같은데에 올라와요!",
          "speaker_id": "2",
          "time": "20210906 17:48"
        },
        {
          "id": "MDRW2100041931.1.18",
          "form": "학교홈페이지랑",
          "original_form": "학교홈페이지랑",
          "speaker_id": "2",
          "time": "20210906 17:48"
        },
        {
          "id": "MDRW2100041931.1.19",
          "form": "오오오오 네네네",
          "original_form": "오오오오 네네네",
          "speaker_id": "1",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100041931.1.20",
          "form": "정직원도 종종 뽑아서 계약직하시는분들이 지원하기도해요!",
          "original_form": "정직원도 종종 뽑아서 계약직하시는분들이 지원하기도해요!",
          "speaker_id": "2",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100041931.1.21",
          "form": "오 그러면 학교에서도",
          "original_form": "오 그러면 학교에서도",
          "speaker_id": "1",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100041931.1.22",
          "form": "새로 가르치는 것보다 나으니까",
          "original_form": "새로 가르치는 것보다 나으니까",
          "speaker_id": "1",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100041931.1.23",
          "form": "원래 하던 사람 뽑으려고도 하겠네요!!!",
          "original_form": "원래 하던 사람 뽑으려고도 하겠네요!!!",
          "speaker_id": "1",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100041931.1.24",
          "form": "그렇지도않더라고요?!!",
          "original_form": "그렇지도않더라고요?!!",
          "speaker_id": "2",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100041931.1.25",
          "form": "제옆자리샘이 이번에 지원했었는데",
          "original_form": "제옆자리샘이 이번에 지원했었는데",
          "speaker_id": "2",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100041931.1.26",
          "form": "2차에서 탈락ㅠ",
          "original_form": "2차에서 탈락ㅠ",
          "speaker_id": "2",
          "time": "20210906 17:50"
        },
        {
          "id": "MDRW2100041931.1.27",
          "form": "아 진짜요?!!",
          "original_form": "아 진짜요?!!",
          "speaker_id": "1",
          "time": "20210906 17:51"
        },
        {
          "id": "MDRW2100041931.1.28",
          "form": "와 그럼 무슨 기준이려나....",
          "original_form": "와 그럼 무슨 기준이려나....",
          "speaker_id": "1",
          "time": "20210906 17:51"
        },
        {
          "id": "MDRW2100041931.1.29",
          "form": "저희 연구실 선배도",
          "original_form": "저희 연구실 선배도",
          "speaker_id": "1",
          "time": "20210906 17:51"
        },
        {
          "id": "MDRW2100041931.1.30",
          "form": "지금... 특목고에",
          "original_form": "지금... 특목고에",
          "speaker_id": "1",
          "time": "20210906 17:51"
        },
        {
          "id": "MDRW2100041931.1.31",
          "form": "어학이랑 자격증같은거 많이 요구하더라고요",
          "original_form": "어학이랑 자격증같은거 많이 요구하더라고요",
          "speaker_id": "2",
          "time": "20210906 17:51"
        },
        {
          "id": "MDRW2100041931.1.32",
          "form": "계약직 강사로 있는데",
          "original_form": "계약직 강사로 있는데",
          "speaker_id": "1",
          "time": "20210906 17:51"
        },
        {
          "id": "MDRW2100041931.1.33",
          "form": "아 헐 역시 그럴줄 알았어요ㅠㅠ",
          "original_form": "아 헐 역시 그럴줄 알았어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100041931.1.34",
          "form": "근데 거기도",
          "original_form": "근데 거기도",
          "speaker_id": "1",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100041931.1.35",
          "form": "정규직으로 지원 많이 하는데 될지 안될지 모르겠다 하더라구요",
          "original_form": "정규직으로 지원 많이 하는데 될지 안될지 모르겠다 하더라구요",
          "speaker_id": "1",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100041931.1.36",
          "form": "요즘진짜 정규직을 너무 안뽑아요 애초에ㅠ",
          "original_form": "요즘진짜 정규직을 너무 안뽑아요 애초에ㅠ",
          "speaker_id": "2",
          "time": "20210906 17:52"
        },
        {
          "id": "MDRW2100041931.1.37",
          "form": "그니까요 너무해요",
          "original_form": "그니까요 너무해요",
          "speaker_id": "1",
          "time": "20210906 17:52"
        }
      ]
    }
