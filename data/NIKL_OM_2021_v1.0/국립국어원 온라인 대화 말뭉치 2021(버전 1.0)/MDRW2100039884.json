      "utterance": [
        {
          "id": "MDRW2100039884.1.1",
          "form": "야식 드시나요?",
          "original_form": "야식 드시나요?",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100039884.1.2",
          "form": "아니요. 뭐 드시나요?",
          "original_form": "아니요. 뭐 드시나요?",
          "speaker_id": "1",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100039884.1.3",
          "form": "전 닭갈비 먹었어요ㅋㅋㅋㅋㅋㅎ",
          "original_form": "전 닭갈비 먹었어요ㅋㅋㅋㅋㅋㅎ",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100039884.1.4",
          "form": "와 맛있겠네요ㅋㅋ",
          "original_form": "와 맛있겠네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:16"
        },
        {
          "id": "MDRW2100039884.1.5",
          "form": "전 닭갈비에 우동사리 넣는거 좋아해요",
          "original_form": "전 닭갈비에 우동사리 넣는거 좋아해요",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100039884.1.6",
          "form": "우동사리도 찰떡인가요?",
          "original_form": "우동사리도 찰떡인가요?",
          "speaker_id": "2",
          "time": "20210904 00:26"
        },
        {
          "id": "MDRW2100039884.1.7",
          "form": "전 치즈가 베스트라고 ㅎㅎ ㅎ",
          "original_form": "전 치즈가 베스트라고 ㅎㅎ ㅎ",
          "speaker_id": "2",
          "time": "20210904 00:26"
        },
        {
          "id": "MDRW2100039884.1.8",
          "form": "치즈도 맛있죠ㅋㅋ 닭갈비에 다른 면보단 우동사리가 딱 맞더라구요",
          "original_form": "치즈도 맛있죠ㅋㅋ 닭갈비에 다른 면보단 우동사리가 딱 맞더라구요",
          "speaker_id": "1",
          "time": "20210904 09:22"
        },
        {
          "id": "MDRW2100039884.1.9",
          "form": "ㅋㅋㅋㅋ하 맛있겠네요 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ하 맛있겠네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 13:13"
        },
        {
          "id": "MDRW2100039884.1.10",
          "form": "남은양념에 볶음밥 해서 먹는것도 맛있잖아요ㅋㅋ",
          "original_form": "남은양념에 볶음밥 해서 먹는것도 맛있잖아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:17"
        },
        {
          "id": "MDRW2100039884.1.11",
          "form": "ㅠㅠ 맞아요",
          "original_form": "ㅠㅠ 맞아요",
          "speaker_id": "2",
          "time": "20210904 14:56"
        },
        {
          "id": "MDRW2100039884.1.12",
          "form": "마지막이 짱이져",
          "original_form": "마지막이 짱이져",
          "speaker_id": "2",
          "time": "20210904 14:56"
        },
        {
          "id": "MDRW2100039884.1.13",
          "form": "살짝 눌린게 맛있어요. 잘못하면 타겠지만ㅋㅋㅋ",
          "original_form": "살짝 눌린게 맛있어요. 잘못하면 타겠지만ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100039884.1.14",
          "form": "ㅋㅋㅋㅋㅋ 맞아요 짐짜 적절하게",
          "original_form": "ㅋㅋㅋㅋㅋ 맞아요 짐짜 적절하게",
          "speaker_id": "2",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100039884.1.15",
          "form": "잘 눌린게 최고맛 ㅜㅜ",
          "original_form": "잘 눌린게 최고맛 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 15:17"
        },
        {
          "id": "MDRW2100039884.1.16",
          "form": "고기먹으면 꼭 마지막엔 볶음밥이랑 냉면 먹는거 같아요ㅋㅋㅋ",
          "original_form": "고기먹으면 꼭 마지막엔 볶음밥이랑 냉면 먹는거 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100039884.1.17",
          "form": "그쳐ㅋㅋ",
          "original_form": "그쳐ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:21"
        },
        {
          "id": "MDRW2100039884.1.18",
          "form": "전 누룽지도 좋아행6",
          "original_form": "전 누룽지도 좋아행6",
