      "utterance": [
        {
          "id": "MDRW2100039156.1.1",
          "form": "둘째 바지하나사야되는데ㅋㅋ",
          "original_form": "둘째 바지하나사야되는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:48"
        },
        {
          "id": "MDRW2100039156.1.2",
          "form": "탑텐 가격어땠어요?",
          "original_form": "탑텐 가격어땠어요?",
          "speaker_id": "1",
          "time": "20210903 11:48"
        },
        {
          "id": "MDRW2100039156.1.3",
          "form": "한개 만오천원정도?",
          "original_form": "한개 만오천원정도?",
          "speaker_id": "2",
          "time": "20210903 12:54"
        },
        {
          "id": "MDRW2100039156.1.4",
          "form": "무난한가격인듯",
          "original_form": "무난한가격인듯",
          "speaker_id": "2",
          "time": "20210903 12:54"
        },
        {
          "id": "MDRW2100039156.1.5",
          "form": "오 가격은 무난하네요",
          "original_form": "오 가격은 무난하네요",
          "speaker_id": "1",
          "time": "20210903 14:16"
        },
        {
          "id": "MDRW2100039156.1.6",
          "form": "스판끼 좋나요?",
          "original_form": "스판끼 좋나요?",
          "speaker_id": "1",
          "time": "20210903 14:16"
        },
        {
          "id": "MDRW2100039156.1.7",
          "form": "배기샀다",
          "original_form": "배기샀다",
          "speaker_id": "2",
          "time": "20210903 14:28"
        },
        {
          "id": "MDRW2100039156.1.8",
          "form": "난 스키니못산다",
          "original_form": "난 스키니못산다",
          "speaker_id": "2",
          "time": "20210903 14:28"
        },
        {
          "id": "MDRW2100039156.1.9",
          "form": "스키니는 저도 안사요 ㅋ",
          "original_form": "스키니는 저도 안사요 ㅋ",
          "speaker_id": "1",
          "time": "20210903 14:29"
        },
        {
          "id": "MDRW2100039156.1.10",
          "form": "애들 입기 힘들어함 ㅋㅋ",
          "original_form": "애들 입기 힘들어함 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:29"
        },
        {
          "id": "MDRW2100039156.1.11",
          "form": "그래ㅋㅋ",
          "original_form": "그래ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:37"
        },
        {
          "id": "MDRW2100039156.1.12",
          "form": "난 잘산듯 이번쇼핑 만족해ㅋㅋ",
          "original_form": "난 잘산듯 이번쇼핑 만족해ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:37"
        },
        {
          "id": "MDRW2100039156.1.13",
          "form": "오오 ㅋㅋㅋ 좋네유 ㅋㅋ 한번 가보든지 해야되겟어유 ㅋㅋ",
          "original_form": "오오 ㅋㅋㅋ 좋네유 ㅋㅋ 한번 가보든지 해야되겟어유 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:37"
        },
        {
          "id": "MDRW2100039156.1.14",
          "form": "오늘도 둘째 바지 찾는데 ㅋㅋ 아....사야되...사야된다..ㅋ이랬어유 ㅋㅋ",
          "original_form": "오늘도 둘째 바지 찾는데 ㅋㅋ 아....사야되...사야된다..ㅋ이랬어유 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:38"
        },
        {
          "id": "MDRW2100039156.1.15",
          "form": "억지로 한벌 찾음 ㅋㅋ 바지가 없어서 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ내복입힐뻔했네욬ㅋ",
          "original_form": "억지로 한벌 찾음 ㅋㅋ 바지가 없어서 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ내복입힐뻔했네욬ㅋ",
          "speaker_id": "1",
          "time": "20210903 14:38"
        },
        {
          "id": "MDRW2100039156.1.16",
          "form": "나도 둘째옷이ㅡㅡ 멘붕이다",
          "original_form": "나도 둘째옷이ㅡㅡ 멘붕이다",
          "speaker_id": "2",
          "time": "20210903 14:46"
        },
        {
          "id": "MDRW2100039156.1.17",
          "form": "이번에 샀다면서요 ㅋㅋ",
          "original_form": "이번에 샀다면서요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:46"
        },
        {
          "id": "MDRW2100039156.1.18",
          "form": "흰바지라못입힘ㅋ",
          "original_form": "흰바지라못입힘ㅋ",
          "speaker_id": "2",
          "time": "20210903 14:46"
        },
        {
          "id": "MDRW2100039156.1.19",
          "form": "비와서ㅋ",
          "original_form": "비와서ㅋ",
          "speaker_id": "2",
          "time": "20210903 14:46"
        },
        {
          "id": "MDRW2100039156.1.20",
          "form": "아하 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그게 좀 그렇네요 ㅋㅋ",
          "original_form": "아하 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그게 좀 그렇네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:47"
        },
        {
          "id": "MDRW2100039156.1.21",
          "form": "샀는옷이 새하얗다ㅋ",
          "original_form": "샀는옷이 새하얗다ㅋ",
          "speaker_id": "2",
          "time": "20210903 14:47"
        },
        {
          "id": "MDRW2100039156.1.22",
          "form": "맑은날입어야함ㅋㅋ",
          "original_form": "맑은날입어야함ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 14:47"
        },
        {
          "id": "MDRW2100039156.1.23",
          "form": "ㅋㅋ그렇네요 ㅋㅋ 맑은날에 ㅋㅋ",
          "original_form": "ㅋㅋ그렇네요 ㅋㅋ 맑은날에 ㅋㅋ",
