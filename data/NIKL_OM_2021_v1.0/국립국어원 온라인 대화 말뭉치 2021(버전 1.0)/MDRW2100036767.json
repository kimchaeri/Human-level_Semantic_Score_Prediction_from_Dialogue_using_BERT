      "utterance": [
        {
          "id": "MDRW2100036767.1.1",
          "form": "아이 말리다 제가 해요ㅠ",
          "original_form": "아이 말리다 제가 해요ㅠ",
          "speaker_id": "2",
          "time": "20210831 12:30"
        },
        {
          "id": "MDRW2100036767.1.2",
          "form": "이론ㅠㅠ",
          "original_form": "이론ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 12:30"
        },
        {
          "id": "MDRW2100036767.1.3",
          "form": "게임 하세요?",
          "original_form": "게임 하세요?",
          "speaker_id": "2",
          "time": "20210831 12:30"
        },
        {
          "id": "MDRW2100036767.1.4",
          "form": "저는 게임 안해요 ㅠ 남편두요..아이도..드물죠.? ㅠㅠ",
          "original_form": "저는 게임 안해요 ㅠ 남편두요..아이도..드물죠.? ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 14:11"
        },
        {
          "id": "MDRW2100036767.1.5",
          "form": "좋은거네요~~  게임 중독성이 커서ㅠ",
          "original_form": "좋은거네요~~  게임 중독성이 커서ㅠ",
          "speaker_id": "2",
          "time": "20210831 14:20"
        },
        {
          "id": "MDRW2100036767.1.6",
          "form": "name2님은 온 가족이 다 즐기시나요?",
          "original_form": "&name2&님은 온 가족이 다 즐기시나요?",
          "speaker_id": "1",
          "time": "20210831 14:23"
        },
        {
          "id": "MDRW2100036767.1.7",
          "form": "저와 아들만ㅎ",
          "original_form": "저와 아들만ㅎ",
          "speaker_id": "2",
          "time": "20210831 14:23"
        },
        {
          "id": "MDRW2100036767.1.8",
          "form": "좋아하는 프로게이머있으세요?",
          "original_form": "좋아하는 프로게이머있으세요?",
          "speaker_id": "2",
          "time": "20210831 14:23"
        },
        {
          "id": "MDRW2100036767.1.9",
          "form": "잘 몰라요.ㅜ.ㅜ 전에 라스나온 어떤 게스트가 엄청난 프로게이머라고 하던데!!!",
          "original_form": "잘 몰라요.ㅜ.ㅜ 전에 라스나온 어떤 게스트가 엄청난 프로게이머라고 하던데!!!",
          "speaker_id": "1",
          "time": "20210831 14:25"
        },
        {
          "id": "MDRW2100036767.1.10",
          "form": "아 이름이",
          "original_form": "아 이름이",
          "speaker_id": "2",
          "time": "20210831 20:57"
        },
        {
          "id": "MDRW2100036767.1.11",
          "form": "얼굴은 생각나는데",
          "original_form": "얼굴은 생각나는데",
          "speaker_id": "2",
          "time": "20210831 20:57"
        },
        {
          "id": "MDRW2100036767.1.12",
          "form": "그분 대단한 분이던데",
          "original_form": "그분 대단한 분이던데",
          "speaker_id": "2",
          "time": "20210831 20:57"
        },
        {
          "id": "MDRW2100036767.1.13",
          "form": "첨 봤어요ㅠ",
          "original_form": "첨 봤어요ㅠ",
          "speaker_id": "2",
          "time": "20210831 20:57"
        },
        {
          "id": "MDRW2100036767.1.14",
          "form": "오 누군지 아실것 같나봐요 ㅎ 남편이 연봉이 엄청 높다고 1인자라고 하더라구요",
          "original_form": "오 누군지 아실것 같나봐요 ㅎ 남편이 연봉이 엄청 높다고 1인자라고 하더라구요",
          "speaker_id": "1",
          "time": "20210831 21:05"
        },
        {
          "id": "MDRW2100036767.1.15",
          "form": "세계 1위던데",
          "original_form": "세계 1위던데",
          "speaker_id": "2",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100036767.1.16",
          "form": "게임을 잘모르니",
          "original_form": "게임을 잘모르니",
          "speaker_id": "2",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100036767.1.17",
          "form": "그분도 모르공ㅋ",
          "original_form": "그분도 모르공ㅋ",
          "speaker_id": "2",
          "time": "20210831 21:07"
        },
        {
          "id": "MDRW2100036767.1.18",
          "form": "아이는 게임 채널이나 프로게이머 좋아하는 나이는 아직아닌가요?",
          "original_form": "아이는 게임 채널이나 프로게이머 좋아하는 나이는 아직아닌가요?",
          "speaker_id": "1",
          "time": "20210831 21:10"
        },
        {
          "id": "MDRW2100036767.1.19",
          "form": "유투브 게임채널 좋아하더라구요",
          "original_form": "유투브 게임채널 좋아하더라구요",
          "speaker_id": "2",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100036767.1.20",
          "form": "이스포츠도 하시나요?",
          "original_form": "이스포츠도 하시나요?",
          "speaker_id": "2",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100036767.1.21",
