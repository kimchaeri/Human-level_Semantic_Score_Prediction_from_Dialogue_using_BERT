      "utterance": [
        {
          "id": "MDRW2100040056.1.1",
          "form": "그리고 토템은 15초래",
          "original_form": "그리고 토템은 15초래",
          "speaker_id": "2",
          "time": "20210904 01:47"
        },
        {
          "id": "MDRW2100040056.1.2",
          "form": "ㅇㅇ터지지도 않고 걍",
          "original_form": "ㅇㅇ터지지도 않고 걍",
          "speaker_id": "2",
          "time": "20210904 01:47"
        },
        {
          "id": "MDRW2100040056.1.3",
          "form": "토끼춤추다 끝났어",
          "original_form": "토끼춤추다 끝났어",
          "speaker_id": "2",
          "time": "20210904 01:47"
        },
        {
          "id": "MDRW2100040056.1.4",
          "form": "15초에 생긴다고??",
          "original_form": "15초에 생긴다고??",
          "speaker_id": "1",
          "time": "20210904 01:47"
        },
        {
          "id": "MDRW2100040056.1.5",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210904 01:47"
        },
        {
          "id": "MDRW2100040056.1.6",
          "form": "아 근데 그사람이 한건",
          "original_form": "아 근데 그사람이 한건",
          "speaker_id": "2",
          "time": "20210904 01:47"
        },
        {
          "id": "MDRW2100040056.1.7",
          "form": "하드 난이도긴 하더라",
          "original_form": "하드 난이도긴 하더라",
          "speaker_id": "2",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100040056.1.8",
          "form": "5초 안에 부숴야하네???",
          "original_form": "5초 안에 부숴야하네???",
          "speaker_id": "1",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100040056.1.9",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100040056.1.10",
          "form": "그리고 케이아로 한건",
          "original_form": "그리고 케이아로 한건",
          "speaker_id": "2",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100040056.1.11",
          "form": "장신 남자가 대시쓰기 편하대",
          "original_form": "장신 남자가 대시쓰기 편하대",
          "speaker_id": "2",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100040056.1.12",
          "form": "그 아래단계는 20초일수도....",
          "original_form": "그 아래단계는 20초일수도....",
          "speaker_id": "1",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100040056.1.13",
          "form": "ㅎㅏ",
          "original_form": "ㅎㅏ",
          "speaker_id": "2",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100040056.1.14",
          "form": "아 ㅇㅇ 이거 키따라 대시 차이 있대",
          "original_form": "아 ㅇㅇ 이거 키따라 대시 차이 있대",
          "speaker_id": "1",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100040056.1.15",
          "form": "이러고도 성공 못하면",
          "original_form": "이러고도 성공 못하면",
          "speaker_id": "2",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100040056.1.16",
          "form": "나 오늘멘스 진행은",
          "original_form": "나 오늘멘스 진행은",
          "speaker_id": "2",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100040056.1.17",
          "form": "자고 일어나서 낼 해",
          "original_form": "자고 일어나서 낼 해",
          "speaker_id": "1",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100040056.1.18",
          "form": "포기할지도.....",
          "original_form": "포기할지도.....",
          "speaker_id": "2",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100040056.1.19",
          "form": "오늘 밀려고",
          "original_form": "오늘 밀려고",
          "speaker_id": "2",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100040056.1.20",
          "form": "커피도 마셨는데",
          "original_form": "커피도 마셨는데",
          "speaker_id": "2",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100040056.1.21",
          "form": "이게 무슨꼴이야",
          "original_form": "이게 무슨꼴이야",
          "speaker_id": "2",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100040056.1.22",
          "form": "나도 어제 그래서 하다 하다 열받아서 자버림",
          "original_form": "나도 어제 그래서 하다 하다 열받아서 자버림",
          "speaker_id": "1",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100040056.1.23",
          "form": "ㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ큐ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100040056.1.24",
          "form": "덱을 억케 짤까",
          "original_form": "덱을 억케 짤까",
          "speaker_id": "2",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100040056.1.25",
          "form": "엠버는 넣기 싫고",
          "original_form": "엠버는 넣기 싫고",
          "speaker_id": "2",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100040056.1.26",
          "form": "중운은 없어",
          "original_form": "중운은 없어",
          "speaker_id": "2",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100040056.1.27",
          "form": "원소 상성 생각하면",
          "original_form": "원소 상성 생각하면",
          "speaker_id": "1",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100040056.1.28",
          "form": "설탕이랑 케이아는 일단 고정",
          "original_form": "설탕이랑 케이아는 일단 고정",
          "speaker_id": "1",
          "time": "20210904 01:50"
        }
      ]
    }
  ]
