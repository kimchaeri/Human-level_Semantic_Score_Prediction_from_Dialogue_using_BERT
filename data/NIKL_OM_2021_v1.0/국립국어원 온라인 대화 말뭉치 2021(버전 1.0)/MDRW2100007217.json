      "utterance": [
        {
          "id": "MDRW2100007217.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007217.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007217.1.3",
          "form": "구직활동 중이신가요?",
          "original_form": "구직활동 중이신가요?",
          "speaker_id": "2",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007217.1.4",
          "form": "직장인이에요",
          "original_form": "직장인이에요",
          "speaker_id": "1",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007217.1.5",
          "form": "어떤 일을 하세요?",
          "original_form": "어떤 일을 하세요?",
          "speaker_id": "1",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007217.1.6",
          "form": "아 저는 주부에요. 애들이 어느정도 커서 다시 일하고싶은데 쉽지 않네요",
          "original_form": "아 저는 주부에요. 애들이 어느정도 커서 다시 일하고싶은데 쉽지 않네요",
          "speaker_id": "2",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007217.1.7",
          "form": "어떤일 하고 계세요?",
          "original_form": "어떤일 하고 계세요?",
          "speaker_id": "2",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007217.1.8",
          "form": "사무직이에요",
          "original_form": "사무직이에요",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007217.1.9",
          "form": "주로 예산과 회계 업무를 해요",
          "original_form": "주로 예산과 회계 업무를 해요",
          "speaker_id": "1",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007217.1.10",
          "form": "아 오래 일하실수 있는 직업이시네요",
          "original_form": "아 오래 일하실수 있는 직업이시네요",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007217.1.11",
          "form": "저는 웹디자인했었는데 오래쉬다보니 다시 일하기 어렵더라구요",
          "original_form": "저는 웹디자인했었는데 오래쉬다보니 다시 일하기 어렵더라구요",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007217.1.12",
          "form": "그래서 다른일을 찾아야하는데 고민이 많아요ㅎㅎ",
          "original_form": "그래서 다른일을 찾아야하는데 고민이 많아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007217.1.13",
          "form": "힘드시겠어요",
          "original_form": "힘드시겠어요",
          "speaker_id": "1",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007217.1.14",
          "form": "해오셨던 일을 다시 하고 싶으신 마음은 있으신건가요",
          "original_form": "해오셨던 일을 다시 하고 싶으신 마음은 있으신건가요",
          "speaker_id": "1",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007217.1.15",
          "form": "하고싶은데 아무래도 야근이랑 철야가 많아서 복직이 힘들더라구요",
          "original_form": "하고싶은데 아무래도 야근이랑 철야가 많아서 복직이 힘들더라구요",
          "speaker_id": "2",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007217.1.16",
          "form": "육아랑 병행이 쉽지않아서 아예다른걸 배워볼까해요",
          "original_form": "육아랑 병행이 쉽지않아서 아예다른걸 배워볼까해요",
          "speaker_id": "2",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007217.1.17",
          "form": "저는 직장 다니는 걸로만도 지치는데 너무 대단하셔요",
          "original_form": "저는 직장 다니는 걸로만도 지치는데 너무 대단하셔요",
          "speaker_id": "1",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007217.1.18",
          "form": "아 생각만 그렇고 실천이 쉽지가 않네요ㅎㅎ",
          "original_form": "아 생각만 그렇고 실천이 쉽지가 않네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007217.1.19",
          "form": "요즘은 투잡하는 사람들도 많더라구요",
          "original_form": "요즘은 투잡하는 사람들도 많더라구요",
          "speaker_id": "2",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007217.1.20",
          "form": "저도 본업에는 그닥 흥미가 없어서 좋아하는 일을 하나 더 해보고 싶기도 한데 겸업이 안되는 직업이라서요..",
          "original_form": "저도 본업에는 그닥 흥미가 없어서 좋아하는 일을 하나 더 해보고 싶기도 한데 겸업이 안되는 직업이라서요..",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007217.1.21",
          "form": "아 그러시구나 글고 회사만 다녀도 너덜너덜해지죠ㅠㅠ",
          "original_form": "아 그러시구나 글고 회사만 다녀도 너덜너덜해지죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007217.1.22",
          "form": "저는 회사다니면서 사람들 대하는게 어렵더라구요",
          "original_form": "저는 회사다니면서 사람들 대하는게 어렵더라구요",
          "speaker_id": "2",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007217.1.23",
          "form": "맞아요 일보다 힘든 게 사람이죠ㅜㅜ",
          "original_form": "맞아요 일보다 힘든 게 사람이죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007217.1.24",
          "form": "네 세상에 또라이는 많고  하필 그게사수면 더 힘들고...",
          "original_form": "네 세상에 또라이는 많고  하필 그게사수면 더 힘들고...",
          "speaker_id": "2",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007217.1.25",
          "form": "제 첫 사수가 그랬어서 혼자서 울고 그랬었는데 그땐 또 더 어릴 때라 억울해도 말도 못했던 게 후회되고 그러네요..ㅜㅜ",
          "original_form": "제 첫 사수가 그랬어서 혼자서 울고 그랬었는데 그땐 또 더 어릴 때라 억울해도 말도 못했던 게 후회되고 그러네요..ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007217.1.26",
          "form": "저도 첫직장다닐땐 화장실에서 정말 많이 울었던거같아요",
          "original_form": "저도 첫직장다닐땐 화장실에서 정말 많이 울었던거같아요",
          "speaker_id": "2",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007217.1.27",
          "form": "저도 화장실에서요!",
          "original_form": "저도 화장실에서요!",
          "speaker_id": "1",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007217.1.28",
          "form": "직장그만두고 육아만 하다보니 기억이 미화되서 즐거운기억만 남더라구요",
          "original_form": "직장그만두고 육아만 하다보니 기억이 미화되서 즐거운기억만 남더라구요",
          "speaker_id": "2",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007217.1.29",
          "form": "그래서 후배들한테는 정말 잘하려고 하는 편인데 또 후배들 입장에선 어떨지 모르겠네요",
          "original_form": "그래서 후배들한테는 정말 잘하려고 하는 편인데 또 후배들 입장에선 어떨지 모르겠네요",
          "speaker_id": "1",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007217.1.30",
          "form": "아무래도 육아가 배는 힘드시죠",
          "original_form": "아무래도 육아가 배는 힘드시죠",
          "speaker_id": "1",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007217.1.31",
          "form": "후배들은 이래도 저래도 불편할거에요ㅎㅎ",
          "original_form": "후배들은 이래도 저래도 불편할거에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007217.1.32",
          "form": "일도 힘든데 사람들 관계가 더 힘든거같아요",
          "original_form": "일도 힘든데 사람들 관계가 더 힘든거같아요",
          "speaker_id": "2",
          "time": "20210822 01:12"
        }
      ]
    }
