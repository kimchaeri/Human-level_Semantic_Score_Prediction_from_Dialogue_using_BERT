      "utterance": [
        {
          "id": "MDRW2100003871.1.1",
          "form": "ㅋㅋㅋㅋ 웃겨요",
          "original_form": "ㅋㅋㅋㅋ 웃겨요",
          "speaker_id": "2",
          "time": "20210821 13:08"
        },
        {
          "id": "MDRW2100003871.1.2",
          "form": "나이차이 11살차이인데",
          "original_form": "나이차이 11살차이인데",
          "speaker_id": "1",
          "time": "20210821 13:08"
        },
        {
          "id": "MDRW2100003871.1.3",
          "form": "11살 어린애한테 저런장난 치는거보면",
          "original_form": "11살 어린애한테 저런장난 치는거보면",
          "speaker_id": "1",
          "time": "20210821 13:08"
        },
        {
          "id": "MDRW2100003871.1.4",
          "form": "걍 어이없어요ㅋㅋㅋㅋ",
          "original_form": "걍 어이없어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 13:08"
        },
        {
          "id": "MDRW2100003871.1.5",
          "form": "나이차이가 많네요",
          "original_form": "나이차이가 많네요",
          "speaker_id": "2",
          "time": "20210821 13:09"
        },
        {
          "id": "MDRW2100003871.1.6",
          "form": "바람이 나면 더 어린애를 만나겠지",
          "original_form": "바람이 나면 더 어린애를 만나겠지",
          "speaker_id": "1",
          "time": "20210821 13:09"
        },
        {
          "id": "MDRW2100003871.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210821 13:09"
        },
        {
          "id": "MDRW2100003871.1.8",
          "form": "저는 1살 차이인데",
          "original_form": "저는 1살 차이인데",
          "speaker_id": "2",
          "time": "20210821 13:09"
        },
        {
          "id": "MDRW2100003871.1.9",
          "form": "그래서 안맞는게 많아도 웬만하면 대화로 해결돼요",
          "original_form": "그래서 안맞는게 많아도 웬만하면 대화로 해결돼요",
          "speaker_id": "1",
          "time": "20210821 13:09"
        },
        {
          "id": "MDRW2100003871.1.10",
          "form": "나이 들어도 그러는구나",
          "original_form": "나이 들어도 그러는구나",
          "speaker_id": "2",
          "time": "20210821 13:09"
        },
        {
          "id": "MDRW2100003871.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ그냥 남자들 다 똑같은가봐요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그냥 남자들 다 똑같은가봐요",
          "speaker_id": "1",
          "time": "20210821 13:09"
        },
        {
          "id": "MDRW2100003871.1.12",
          "form": "다음 연애때도 분명히 겪을거같아요",
          "original_form": "다음 연애때도 분명히 겪을거같아요",
          "speaker_id": "1",
          "time": "20210821 13:09"
        },
        {
          "id": "MDRW2100003871.1.13",
          "form": "그럴수도 있겠어요",
          "original_form": "그럴수도 있겠어요",
          "speaker_id": "2",
          "time": "20210821 13:10"
        },
        {
          "id": "MDRW2100003871.1.14",
          "form": "그냥 이상형인사람 만나면서 연애만 쭉 할까봐요",
          "original_form": "그냥 이상형인사람 만나면서 연애만 쭉 할까봐요",
          "speaker_id": "1",
          "time": "20210821 13:10"
        },
        {
          "id": "MDRW2100003871.1.15",
          "form": "어릴때 좀 더 많이 만나둘걸",
          "original_form": "어릴때 좀 더 많이 만나둘걸",
          "speaker_id": "1",
          "time": "20210821 13:10"
        },
        {
          "id": "MDRW2100003871.1.16",
          "form": "이상형은 어떻게되세요?",
          "original_form": "이상형은 어떻게되세요?",
          "speaker_id": "2",
          "time": "20210821 13:10"
        },
        {
          "id": "MDRW2100003871.1.17",
          "form": "말 잘통하는 사람이요",
          "original_form": "말 잘통하는 사람이요",
          "speaker_id": "1",
          "time": "20210821 13:11"
        },
        {
          "id": "MDRW2100003871.1.18",
          "form": "무조건 이게 1순위요",
          "original_form": "무조건 이게 1순위요",
          "speaker_id": "1",
          "time": "20210821 13:11"
        },
        {
          "id": "MDRW2100003871.1.19",
          "form": "얼굴은 잘 안보는것같아요...전남친이나 현남친이나 훈남정도인듯...잘생긴건아니고",
          "original_form": "얼굴은 잘 안보는것같아요...전남친이나 현남친이나 훈남정도인듯...잘생긴건아니고",
          "speaker_id": "1",
          "time": "20210821 13:12"
        },
        {
          "id": "MDRW2100003871.1.20",
          "form": "말통하는게 진짜 중요해요",
          "original_form": "말통하는게 진짜 중요해요",
          "speaker_id": "2",
          "time": "20210821 13:12"
        },
        {
          "id": "MDRW2100003871.1.21",
          "form": "1순위 잘 정하셨네요~!",
          "original_form": "1순위 잘 정하셨네요~!",
          "speaker_id": "2",
