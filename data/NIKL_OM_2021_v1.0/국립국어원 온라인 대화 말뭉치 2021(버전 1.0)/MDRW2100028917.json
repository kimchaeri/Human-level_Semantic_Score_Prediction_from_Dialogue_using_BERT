        {
          "id": "MDRW2100028917.1.2",
          "form": "전 동물 안 키워봣어요",
          "original_form": "전 동물 안 키워봣어요",
          "speaker_id": "1",
          "time": "20210902 22:51"
        },
        {
          "id": "MDRW2100028917.1.3",
          "form": "전 토끼도 잠시요",
          "original_form": "전 토끼도 잠시요",
          "speaker_id": "2",
          "time": "20210902 22:56"
        },
        {
          "id": "MDRW2100028917.1.4",
          "form": "토끼좋아하시나봐요",
          "original_form": "토끼좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100028917.1.5",
          "form": "병아리도 있었던거 같아요",
          "original_form": "병아리도 있었던거 같아요",
          "speaker_id": "2",
          "time": "20210902 23:04"
        },
        {
          "id": "MDRW2100028917.1.6",
          "form": "병아리 키우셧나여",
          "original_form": "병아리 키우셧나여",
          "speaker_id": "1",
          "time": "20210902 23:04"
        },
        {
          "id": "MDRW2100028917.1.7",
          "form": "네 금방 없어졌지만요",
          "original_form": "네 금방 없어졌지만요",
          "speaker_id": "2",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100028917.1.8",
          "form": "동물 모 좋아하세요",
          "original_form": "동물 모 좋아하세요",
          "speaker_id": "2",
          "time": "20210902 23:09"
        },
        {
          "id": "MDRW2100028917.1.9",
          "form": "음.....딱히",
