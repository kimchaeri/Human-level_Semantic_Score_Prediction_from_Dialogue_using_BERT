      "utterance": [
        {
          "id": "MDRW2100034713.1.1",
          "form": "팀버튼의 배트맨은",
          "original_form": "팀버튼의 배트맨은",
          "speaker_id": "1",
          "time": "20210828 18:05"
        },
        {
          "id": "MDRW2100034713.1.2",
          "form": "88 90년인가 그쯤인데",
          "original_form": "88 90년인가 그쯤인데",
          "speaker_id": "1",
          "time": "20210828 18:05"
        },
        {
          "id": "MDRW2100034713.1.3",
          "form": "거기 나온 조커는요 -_- 미술관서 현대미술을 부수고요",
          "original_form": "거기 나온 조커는요 -_- 미술관서 현대미술을 부수고요",
          "speaker_id": "1",
          "time": "20210828 18:05"
        },
        {
          "id": "MDRW2100034713.1.4",
          "form": "그담에 뭘하게요? ㅋㅋ",
          "original_form": "그담에 뭘하게요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:05"
        },
        {
          "id": "MDRW2100034713.1.5",
          "form": "현대미술 다음에요?",
          "original_form": "현대미술 다음에요?",
          "speaker_id": "2",
          "time": "20210828 18:06"
        },
        {
          "id": "MDRW2100034713.1.6",
          "form": "경비를 죽이나요?",
          "original_form": "경비를 죽이나요?",
          "speaker_id": "2",
          "time": "20210828 18:06"
        },
        {
          "id": "MDRW2100034713.1.7",
          "form": "여자얼굴에 염산뿌려요 ㅋ",
          "original_form": "여자얼굴에 염산뿌려요 ㅋ",
          "speaker_id": "1",
          "time": "20210828 18:06"
        },
        {
          "id": "MDRW2100034713.1.8",
          "form": "여자친구인데",
          "original_form": "여자친구인데",
          "speaker_id": "1",
          "time": "20210828 18:06"
        },
        {
          "id": "MDRW2100034713.1.9",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210828 18:07"
        },
        {
          "id": "MDRW2100034713.1.10",
          "form": "참 유구한 역사가 있는 행위군요",
          "original_form": "참 유구한 역사가 있는 행위군요",
          "speaker_id": "2",
          "time": "20210828 18:07"
        },
        {
          "id": "MDRW2100034713.1.11",
          "form": "어느 나라의 누구와 다르지 않은ㅋㅋ",
          "original_form": "어느 나라의 누구와 다르지 않은ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:07"
        },
        {
          "id": "MDRW2100034713.1.12",
          "form": "아 미술관 가기 전인가 훈가",
          "original_form": "아 미술관 가기 전인가 훈가",
          "speaker_id": "1",
          "time": "20210828 18:07"
        },
        {
          "id": "MDRW2100034713.1.13",
          "form": "기억이 안나는데 여자얼굴에 염산 뿌려놓고 자기 마스터피스라고 해요",
          "original_form": "기억이 안나는데 여자얼굴에 염산 뿌려놓고 자기 마스터피스라고 해요",
          "speaker_id": "1",
          "time": "20210828 18:07"
        },
        {
          "id": "MDRW2100034713.1.14",
          "form": "아 찾아보니 염산은 미술관 테러전이구나",
          "original_form": "아 찾아보니 염산은 미술관 테러전이구나",
          "speaker_id": "1",
          "time": "20210828 18:08"
        },
        {
          "id": "MDRW2100034713.1.15",
          "form": "ㅋㅋㅋ 여자는 부모님이 만들어주셨지 자기가 뭔데 마스터피스에요?",
          "original_form": "ㅋㅋㅋ 여자는 부모님이 만들어주셨지 자기가 뭔데 마스터피스에요?",
          "speaker_id": "2",
          "time": "20210828 18:09"
        },
        {
          "id": "MDRW2100034713.1.16",
          "form": "애초에 본인이 마스터라고 하는 그 자의식과잉ㅋㅋ",
          "original_form": "애초에 본인이 마스터라고 하는 그 자의식과잉ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:09"
        },
        {
          "id": "MDRW2100034713.1.17",
          "form": "그리고 그 염산테로 보여주는 장면도 ㅡㅡ",
          "original_form": "그리고 그 염산테로 보여주는 장면도 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210828 18:09"
        },
        {
          "id": "MDRW2100034713.1.18",
          "form": "배트맨 여자친구에게 만나자고 불러서 꼬시곤",
          "original_form": "배트맨 여자친구에게 만나자고 불러서 꼬시곤",
          "speaker_id": "1",
          "time": "20210828 18:09"
        },
        {
          "id": "MDRW2100034713.1.19",
          "form": "배트맨여친이 기자였나 그랬을거에요 그래서 나간건데",
          "original_form": "배트맨여친이 기자였나 그랬을거에요 그래서 나간건데",
          "speaker_id": "1",
          "time": "20210828 18:10"
        },
        {
          "id": "MDRW2100034713.1.20",
          "form": "그때 배트맨 여친에게 자기 여친얼구루 - 염산으로 녹음-을 보여줘요",
          "original_form": "그때 배트맨 여친에게 자기 여친얼구루 - 염산으로 녹음-을 보여줘요",
          "speaker_id": "1",
          "time": "20210828 18:10"
        },
        {
          "id": "MDRW2100034713.1.21",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210828 18:11"
        },
        {
          "id": "MDRW2100034713.1.22",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "2",
          "time": "20210828 18:11"
        },
        {
          "id": "MDRW2100034713.1.23",
          "form": "조커와 유투브 그사람들 다를게 없네요",
          "original_form": "조커와 유투브 그사람들 다를게 없네요",
          "speaker_id": "2",
          "time": "20210828 18:12"
        },
        {
          "id": "MDRW2100034713.1.24",
          "form": "결국 조커에게 염산테러당한 조커여친은",
          "original_form": "결국 조커에게 염산테러당한 조커여친은",
          "speaker_id": "1",
          "time": "20210828 18:12"
        },
        {
          "id": "MDRW2100034713.1.25",
          "form": "suicide 해요",
          "original_form": "suicide 해요",
          "speaker_id": "1",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100034713.1.26",
          "form": "필터링때문에",
          "original_form": "필터링때문에",
          "speaker_id": "1",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100034713.1.27",
          "form": "허류",
          "original_form": "허류",
          "speaker_id": "2",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100034713.1.28",
          "form": "그 얼마전 나온 영화도",
          "original_form": "그 얼마전 나온 영화도",
          "speaker_id": "2",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100034713.1.29",
          "form": "조커여친이 얼굴을 가리고 다니는 가면이 있는데 ㅡㅡ",
          "original_form": "조커여친이 얼굴을 가리고 다니는 가면이 있는데 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100034713.1.30",
          "form": "여친이 추락사해서 가면이 뽀개진게 나와요",
          "original_form": "여친이 추락사해서 가면이 뽀개진게 나와요",
          "speaker_id": "1",
          "time": "20210828 18:14"
        },
        {
          "id": "MDRW2100034713.1.31",
          "form": "진짜 잔인하네요",
          "original_form": "진짜 잔인하네요",
          "speaker_id": "2",
          "time": "20210828 18:14"
        },
        {
          "id": "MDRW2100034713.1.32",
          "form": "할리퀸도 화학물질 통에 떨어지도록 조장하잖아요",
          "original_form": "할리퀸도 화학물질 통에 떨어지도록 조장하잖아요",
          "speaker_id": "2",
          "time": "20210828 18:14"
        },
        {
          "id": "MDRW2100034713.1.33",
          "form": "글서 조커는 안 지질하단 사람들 생각에 동의안해요",
          "original_form": "글서 조커는 안 지질하단 사람들 생각에 동의안해요",
          "speaker_id": "1",
          "time": "20210828 18:15"
        },
        {
          "id": "MDRW2100034713.1.34",
          "form": "맞아요 그게 염산테러가 그쪽으로 바귄걸거에요 배트맨 캐릭터 설정도 시간지나며 바귀자나여",
          "original_form": "맞아요 그게 염산테러가 그쪽으로 바귄걸거에요 배트맨 캐릭터 설정도 시간지나며 바귀자나여",
          "speaker_id": "1",
          "time": "20210828 18:15"
        },
        {
          "id": "MDRW2100034713.1.35",
          "form": "어떤 만화책선 여친이 살자하고 어떤 만화책선 할리퀸되고 이런 변주?",
          "original_form": "어떤 만화책선 여친이 살자하고 어떤 만화책선 할리퀸되고 이런 변주?",
          "speaker_id": "1",
          "time": "20210828 18:15"
        },
        {
          "id": "MDRW2100034713.1.36",
          "form": "글쿠나",
          "original_form": "글쿠나",
          "speaker_id": "2",
          "time": "20210828 18:16"
        }
      ]
    }
