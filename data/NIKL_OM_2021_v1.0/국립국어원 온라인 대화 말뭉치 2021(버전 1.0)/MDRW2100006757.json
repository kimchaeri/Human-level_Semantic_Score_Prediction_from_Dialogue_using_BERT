      "utterance": [
        {
          "id": "MDRW2100006757.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100006757.1.2",
          "form": "기억에 남는 여행지 있으신가요??",
          "original_form": "기억에 남는 여행지 있으신가요??",
          "speaker_id": "1",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100006757.1.3",
          "form": "네 저는 파리요",
          "original_form": "네 저는 파리요",
          "speaker_id": "2",
          "time": "20210908 08:37"
        },
        {
          "id": "MDRW2100006757.1.4",
          "form": "우와 파리 가보셨구나",
          "original_form": "우와 파리 가보셨구나",
          "speaker_id": "1",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100006757.1.5",
          "form": "저도 한 번 가보고 싶네요 ㅠㅠ",
          "original_form": "저도 한 번 가보고 싶네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100006757.1.6",
          "form": "어디 기억에 남으세요?",
          "original_form": "어디 기억에 남으세요?",
          "speaker_id": "2",
          "time": "20210908 08:42"
        },
        {
          "id": "MDRW2100006757.1.7",
          "form": "저 아일랜드 좋았어요",
          "original_form": "저 아일랜드 좋았어요",
          "speaker_id": "1",
          "time": "20210908 08:42"
        },
        {
          "id": "MDRW2100006757.1.8",
          "form": "딱히 관광지로 유명한 곳은 아닌데",
          "original_form": "딱히 관광지로 유명한 곳은 아닌데",
          "speaker_id": "1",
          "time": "20210908 08:43"
        },
        {
          "id": "MDRW2100006757.1.9",
          "form": "그래서 조용하고 사람들도 친절하고 좋았던거 같아요",
          "original_form": "그래서 조용하고 사람들도 친절하고 좋았던거 같아요",
          "speaker_id": "1",
          "time": "20210908 08:43"
        },
        {
          "id": "MDRW2100006757.1.10",
          "form": "와 진짜 특이하네요",
          "original_form": "와 진짜 특이하네요",
          "speaker_id": "2",
          "time": "20210908 08:43"
        },
        {
          "id": "MDRW2100006757.1.11",
          "form": "영국 거처 가나요?",
          "original_form": "영국 거처 가나요?",
          "speaker_id": "2",
          "time": "20210908 08:43"
        },
        {
          "id": "MDRW2100006757.1.12",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210908 08:44"
        },
        {
          "id": "MDRW2100006757.1.13",
          "form": "영국 아일랜드 이탈리아 이렇게 갔어요",
          "original_form": "영국 아일랜드 이탈리아 이렇게 갔어요",
          "speaker_id": "1",
          "time": "20210908 08:44"
        },
        {
          "id": "MDRW2100006757.1.14",
          "form": "사실 저는 프랑스도 가보고 싶었는데 일정이 안 맞아서...",
          "original_form": "사실 저는 프랑스도 가보고 싶었는데 일정이 안 맞아서...",
          "speaker_id": "1",
          "time": "20210908 08:44"
        },
        {
          "id": "MDRW2100006757.1.15",
          "form": "이탈리아까지 횡단코스",
          "original_form": "이탈리아까지 횡단코스",
          "speaker_id": "2",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100006757.1.16",
          "form": "멋지시네요",
          "original_form": "멋지시네요",
          "speaker_id": "2",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100006757.1.17",
          "form": "파리는 어떻게 가시게 된거에요?",
          "original_form": "파리는 어떻게 가시게 된거에요?",
          "speaker_id": "1",
          "time": "20210908 08:55"
        },
        {
          "id": "MDRW2100006757.1.18",
          "form": "그냥 가고싶어서요ㅋ 에펠탑이 궁금",
          "original_form": "그냥 가고싶어서요ㅋ 에펠탑이 궁금",
          "speaker_id": "2",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100006757.1.19",
          "form": "아일랜드도 정통 영어인가요?",
          "original_form": "아일랜드도 정통 영어인가요?",
          "speaker_id": "2",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100006757.1.20",
          "form": "에펠탑 실제로 보면 어때요??",
          "original_form": "에펠탑 실제로 보면 어때요??",
          "speaker_id": "1",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100006757.1.21",
          "form": "생각보다 엄청 크다는데..",
          "original_form": "생각보다 엄청 크다는데..",
          "speaker_id": "1",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100006757.1.22",
          "form": "아 아일랜드어도 따로 있는데",
          "original_form": "아 아일랜드어도 따로 있는데",
          "speaker_id": "1",
          "time": "20210908 08:56"
        },
        {
          "id": "MDRW2100006757.1.23",
          "form": "대부분 그냥 영어 쓰시더라고요",
          "original_form": "대부분 그냥 영어 쓰시더라고요",
          "speaker_id": "1",
          "time": "20210908 08:57"
        },
        {
          "id": "MDRW2100006757.1.24",
          "form": "에펠탑 진짜 커요",
          "original_form": "에펠탑 진짜 커요",
          "speaker_id": "2",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100006757.1.25",
          "form": "진짜진짜ㅋㅋ",
          "original_form": "진짜진짜ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100006757.1.26",
          "form": "파리도 나중에 꼭 가봐야겠어요",
          "original_form": "파리도 나중에 꼭 가봐야겠어요",
          "speaker_id": "1",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100006757.1.27",
          "form": "일단 코로나 끝나면..",
          "original_form": "일단 코로나 끝나면..",
          "speaker_id": "1",
          "time": "20210908 08:59"
        },
        {
          "id": "MDRW2100006757.1.28",
          "form": "혹시 다른 유럽 국가들도 가보셨나요??",
          "original_form": "혹시 다른 유럽 국가들도 가보셨나요??",
          "speaker_id": "1",
          "time": "20210908 08:59"
        },
        {
          "id": "MDRW2100006757.1.29",
          "form": "네 체코요",
          "original_form": "네 체코요",
          "speaker_id": "2",
          "time": "20210908 09:02"
        },
        {
          "id": "MDRW2100006757.1.30",
          "form": "오 동유럽쪽 가신 분은 많이 못 본거 같네요",
          "original_form": "오 동유럽쪽 가신 분은 많이 못 본거 같네요",
          "speaker_id": "1",
          "time": "20210908 09:04"
        },
        {
          "id": "MDRW2100006757.1.31",
          "form": "분위기가 어떻던가요??",
          "original_form": "분위기가 어떻던가요??",
          "speaker_id": "1",
          "time": "20210908 09:04"
        }
      ]
