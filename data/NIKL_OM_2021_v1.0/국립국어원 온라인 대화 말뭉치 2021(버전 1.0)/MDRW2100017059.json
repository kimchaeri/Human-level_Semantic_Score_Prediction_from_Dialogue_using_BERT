        },
        {
          "id": "MDRW2100017059.1.2",
          "form": "ㅋㅋ 미모가 갈수록 살아나는 느낌?",
          "original_form": "ㅋㅋ 미모가 갈수록 살아나는 느낌?",
          "speaker_id": "1",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100017059.1.3",
          "form": "그러게 관리를 잘 해줘서 그런가",
          "original_form": "그러게 관리를 잘 해줘서 그런가",
          "speaker_id": "2",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100017059.1.4",
          "form": "ㅋㅋㅋ그럴듯 다 내가 관리를 잘 해줘서임~",
          "original_form": "ㅋㅋㅋ그럴듯 다 내가 관리를 잘 해줘서임~",
          "speaker_id": "1",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100017059.1.5",
          "form": "네가 뭘 했다고",
          "original_form": "네가 뭘 했다고",
          "speaker_id": "2",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100017059.1.6",
          "form": "내가 사랑을 듬뿍 나눠주잖아",
          "original_form": "내가 사랑을 듬뿍 나눠주잖아",
          "speaker_id": "1",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100017059.1.7",
          "form": "내가 더 잘 챙겨준다",
          "original_form": "내가 더 잘 챙겨준다",
          "speaker_id": "2",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100017059.1.8",
          "form": "그래용?ㅋㅋ",
          "original_form": "그래용?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100017059.1.9",
          "form": "그래 날 더 좋아하잖아",
          "original_form": "그래 날 더 좋아하잖아",
          "speaker_id": "2",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100017059.1.10",
          "form": "아무튼 ㅋㅋ 넘 귀여움 ㅠㅠ",
          "original_form": "아무튼 ㅋㅋ 넘 귀여움 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100017059.1.11",
