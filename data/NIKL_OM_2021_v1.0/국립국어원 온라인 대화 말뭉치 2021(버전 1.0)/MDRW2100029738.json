      "utterance": [
        {
          "id": "MDRW2100029738.1.1",
          "form": "점심 시간이 다가왔습니다 두구두구",
          "original_form": "점심 시간이 다가왔습니다 두구두구",
          "speaker_id": "1",
          "time": "20210903 12:41"
        },
        {
          "id": "MDRW2100029738.1.2",
          "form": "머드세요",
          "original_form": "머드세요",
          "speaker_id": "2",
          "time": "20210903 12:48"
        },
        {
          "id": "MDRW2100029738.1.3",
          "form": "저는 간만에 본가와서 집밥먹게요ㅎㅎ",
          "original_form": "저는 간만에 본가와서 집밥먹게요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 12:54"
        },
        {
          "id": "MDRW2100029738.1.4",
          "form": "오 집밥 최고죠",
          "original_form": "오 집밥 최고죠",
          "speaker_id": "2",
          "time": "20210903 12:54"
        },
        {
          "id": "MDRW2100029738.1.5",
          "form": "메뉴는 ~?~?",
          "original_form": "메뉴는 ~?~?",
          "speaker_id": "2",
          "time": "20210903 12:54"
        },
        {
          "id": "MDRW2100029738.1.6",
          "form": "그건 엄마가 뭘 준비했는지를 모릅니다",
          "original_form": "그건 엄마가 뭘 준비했는지를 모릅니다",
          "speaker_id": "1",
          "time": "20210903 12:55"
        },
        {
          "id": "MDRW2100029738.1.7",
          "form": "마싯는거해두셧겟다....부럽",
          "original_form": "마싯는거해두셧겟다....부럽",
          "speaker_id": "2",
          "time": "20210903 12:56"
        },
        {
          "id": "MDRW2100029738.1.8",
          "form": "ㅋㅋ 그냥 돼지고기였어요",
          "original_form": "ㅋㅋ 그냥 돼지고기였어요",
          "speaker_id": "1",
          "time": "20210903 14:14"
        },
        {
          "id": "MDRW2100029738.1.9",
          "form": "오홍 볶음 ???",
          "original_form": "오홍 볶음 ???",
          "speaker_id": "2",
          "time": "20210903 16:51"
        },
        {
          "id": "MDRW2100029738.1.10",
          "form": "네 그런건가보다!!!",
          "original_form": "네 그런건가보다!!!",
          "speaker_id": "1",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100029738.1.11",
          "form": "산적 느낌의 ㅎㅎ",
          "original_form": "산적 느낌의 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100029738.1.12",
          "form": "오오... 집에선 멀먹어도 맛잇고 건강한느낌이에요",
          "original_form": "오오... 집에선 멀먹어도 맛잇고 건강한느낌이에요",
          "speaker_id": "2",
          "time": "20210903 17:41"
        },
        {
          "id": "MDRW2100029738.1.13",
          "form": "ㅋㅋ 있을때는 몰랐는데",
          "original_form": "ㅋㅋ 있을때는 몰랐는데",
          "speaker_id": "1",
          "time": "20210903 18:57"
        },
        {
          "id": "MDRW2100029738.1.14",
          "form": "얼마나자주먹어요 집밥",
          "original_form": "얼마나자주먹어요 집밥",
          "speaker_id": "2",
          "time": "20210903 19:04"
        },
        {
          "id": "MDRW2100029738.1.15",
          "form": "본가는 한달에 두번",
          "original_form": "본가는 한달에 두번",
          "speaker_id": "1",
          "time": "20210903 19:54"
        },
        {
          "id": "MDRW2100029738.1.16",
          "form": "가는듯하고",
          "original_form": "가는듯하고",
          "speaker_id": "1",
          "time": "20210903 19:54"
        },
        {
          "id": "MDRW2100029738.1.17",
          "form": "집밥은 이틀에 한번정도?",
          "original_form": "집밥은 이틀에 한번정도?",
          "speaker_id": "1",
          "time": "20210903 19:54"
        },
        {
          "id": "MDRW2100029738.1.18",
          "form": "아하  마니 해드시나요ㅡ",
          "original_form": "아하  마니 해드시나요ㅡ",
          "speaker_id": "2",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100029738.1.19",
          "form": "ㅋㅋ노력은해요 ㅎㅎ",
          "original_form": "ㅋㅋ노력은해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 00:33"
        },
        {
          "id": "MDRW2100029738.1.20",
          "form": "님은?",
          "original_form": "님은?",
          "speaker_id": "1",
          "time": "20210904 00:33"
        },
        {
          "id": "MDRW2100029738.1.21",
          "form": "ㅋㅋ 저도 ...",
          "original_form": "ㅋㅋ 저도 ...",
          "speaker_id": "2",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100029738.1.22",
          "form": "근데뭐 밀키트 그런거..",
          "original_form": "근데뭐 밀키트 그런거..",
