      "utterance": [
        {
          "id": "MDRW2100032599.1.1",
          "form": "여행가고 싶은데 있어?",
          "original_form": "여행가고 싶은데 있어?",
          "speaker_id": "2",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100032599.1.2",
          "form": "움 코로나만 아니면",
          "original_form": "움 코로나만 아니면",
          "speaker_id": "1",
          "time": "20210905 21:10"
        },
        {
          "id": "MDRW2100032599.1.3",
          "form": "터키 이스탄불로 연차를 가고싶었찌ㅎㅎ",
          "original_form": "터키 이스탄불로 연차를 가고싶었찌ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100032599.1.4",
          "form": "오호ㅠㅜ나도 연차쓰고 노르웨이 갈랬는데",
          "original_form": "오호ㅠㅜ나도 연차쓰고 노르웨이 갈랬는데",
          "speaker_id": "2",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100032599.1.5",
          "form": "노르웨이!",
          "original_form": "노르웨이!",
          "speaker_id": "1",
          "time": "20210905 21:24"
        },
        {
          "id": "MDRW2100032599.1.6",
          "form": "거긴쫌춥지?",
          "original_form": "거긴쫌춥지?",
          "speaker_id": "1",
          "time": "20210905 21:24"
        },
        {
          "id": "MDRW2100032599.1.7",
          "form": "북유럽멋있다왠지",
          "original_form": "북유럽멋있다왠지",
          "speaker_id": "1",
          "time": "20210905 21:24"
        },
        {
          "id": "MDRW2100032599.1.8",
          "form": "응응 맞아 눈 되게.마니와",
          "original_form": "응응 맞아 눈 되게.마니와",
          "speaker_id": "2",
          "time": "20210905 21:36"
        },
        {
          "id": "MDRW2100032599.1.9",
          "form": "오로라 보고싶어",
          "original_form": "오로라 보고싶어",
          "speaker_id": "2",
          "time": "20210905 21:36"
        },
        {
          "id": "MDRW2100032599.1.10",
          "form": "오로라 와 멋지겠다",
          "original_form": "오로라 와 멋지겠다",
          "speaker_id": "1",
          "time": "20210905 21:37"
        },
        {
          "id": "MDRW2100032599.1.11",
          "form": "눈도 이쁠것같아",
          "original_form": "눈도 이쁠것같아",
          "speaker_id": "1",
          "time": "20210905 21:37"
        },
        {
          "id": "MDRW2100032599.1.12",
          "form": "코로나끝나면갈수있을꺼야ㅠ",
          "original_form": "코로나끝나면갈수있을꺼야ㅠ",
          "speaker_id": "1",
          "time": "20210905 21:37"
        },
        {
          "id": "MDRW2100032599.1.13",
          "form": "그치ㅠㅠ 나 무조건 가야해 거기.남친 있어ㅠㅠ",
          "original_form": "그치ㅠㅠ 나 무조건 가야해 거기.남친 있어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100032599.1.14",
          "form": "헐!!",
          "original_form": "헐!!",
          "speaker_id": "1",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100032599.1.15",
          "form": "진짜가야겠다",
          "original_form": "진짜가야겠다",
          "speaker_id": "1",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100032599.1.16",
          "form": "오로라는덤이네!",
          "original_form": "오로라는덤이네!",
          "speaker_id": "1",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100032599.1.17",
          "form": "그치 사실 그래 ㅋㅋ",
          "original_form": "그치 사실 그래 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:03"
        },
        {
          "id": "MDRW2100032599.1.18",
          "form": "남자친구보고싶겠다ㅠㅜ",
          "original_form": "남자친구보고싶겠다ㅠㅜ",
          "speaker_id": "1",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100032599.1.19",
          "form": "응 너무",
          "original_form": "응 너무",
          "speaker_id": "2",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100032599.1.20",
          "form": "코로나 터지고 못봐서 이제 지긋지긋해 ㅋㅋ",
          "original_form": "코로나 터지고 못봐서 이제 지긋지긋해 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:10"
        },
        {
          "id": "MDRW2100032599.1.21",
          "form": "하유ㅠㅜ코로나ㅠ",
          "original_form": "하유ㅠㅜ코로나ㅠ",
          "speaker_id": "1",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100032599.1.22",
          "form": "파리 가고파요",
          "original_form": "파리 가고파요",
