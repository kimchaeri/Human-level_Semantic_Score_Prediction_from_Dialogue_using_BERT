      "utterance": [
        {
          "id": "MDRW2100044214.1.1",
          "form": "ㅜ저녁 머드심 ?",
          "original_form": "ㅜ저녁 머드심 ?",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044214.1.2",
          "form": "지금 국에 밥말아 먹고있어요 시레기국",
          "original_form": "지금 국에 밥말아 먹고있어요 시레기국",
          "speaker_id": "2",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044214.1.3",
          "form": "직접해드심 ?",
          "original_form": "직접해드심 ?",
          "speaker_id": "1",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044214.1.4",
          "form": "그럴리가요 엄마표 국입니다",
          "original_form": "그럴리가요 엄마표 국입니다",
          "speaker_id": "2",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044214.1.5",
          "form": "부러워요 마싯어요?",
          "original_form": "부러워요 마싯어요?",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044214.1.6",
          "form": "네 매콤하니 맛있네요",
          "original_form": "네 매콤하니 맛있네요",
          "speaker_id": "2",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044214.1.7",
          "form": "name1님은 저녁 뭐먹?",
          "original_form": "&name1&님은 저녁 뭐먹?",
          "speaker_id": "2",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044214.1.8",
          "form": "제육이요",
          "original_form": "제육이요",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044214.1.9",
          "form": "저도 시래기먹고싶어요",
          "original_form": "저도 시래기먹고싶어요",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044214.1.10",
          "form": "디저트 드심 ?",
          "original_form": "디저트 드심 ?",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044214.1.11",
          "form": "아 대박 저 점심에 제육먹음요",
          "original_form": "아 대박 저 점심에 제육먹음요",
          "speaker_id": "2",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100044214.1.12",
          "form": "디저트 없는데...ㅜㅜ",
          "original_form": "디저트 없는데...ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100044214.1.13",
          "form": "먹고싶어요",
          "original_form": "먹고싶어요",
          "speaker_id": "2",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100044214.1.14",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "1",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044214.1.15",
          "form": "머드시고싶으심 ?",
          "original_form": "머드시고싶으심 ?",
          "speaker_id": "1",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044214.1.16",
          "form": "초코케이크요 달달한거땡겨요",
          "original_form": "초코케이크요 달달한거땡겨요",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044214.1.17",
          "form": "오 꾸덕이요?",
          "original_form": "오 꾸덕이요?",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044214.1.18",
          "form": "꾸덕도 좋죠 당근케이크도 먹고싶어요",
          "original_form": "꾸덕도 좋죠 당근케이크도 먹고싶어요",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044214.1.19",
          "form": "크 당케 어디꺼 조아하세요",
          "original_form": "크 당케 어디꺼 조아하세요",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044214.1.20",
          "form": "저 원래 좋아하던 케이크 집이 있었는데ㅠㅜ 사라졌거든요 ㅠㅠ",
          "original_form": "저 원래 좋아하던 케이크 집이 있었는데ㅠㅜ 사라졌거든요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044214.1.21",
          "form": "지금 딱 떠오르는데는 없네요",
          "original_form": "지금 딱 떠오르는데는 없네요",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044214.1.22",
          "form": "해크니 아세요?",
          "original_form": "해크니 아세요?",
          "speaker_id": "1",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044214.1.23",
          "form": "헐 알아요!! 해방촌에 있는거죠??",
          "original_form": "헐 알아요!! 해방촌에 있는거죠??",
          "speaker_id": "2",
